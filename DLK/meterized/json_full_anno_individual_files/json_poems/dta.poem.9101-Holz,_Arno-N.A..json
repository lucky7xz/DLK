{"dta.poem.9101": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem alten Park ein Schl\u00f6sschen.", "tokens": ["In", "ei\u00b7nem", "al\u00b7ten", "Park", "ein", "Schl\u00f6s\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ueber seinem bemoosten Dach gl\u00e4nzt ein Sommerhimmel,", "tokens": ["Ue\u00b7ber", "sei\u00b7nem", "be\u00b7moos\u00b7ten", "Dach", "gl\u00e4nzt", "ein", "Som\u00b7mer\u00b7him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "sieben verwilderte Taxusalleeen", "tokens": ["sie\u00b7ben", "ver\u00b7wil\u00b7der\u00b7te", "Ta\u00b7xu\u00b7sal\u00b7le\u00b7e\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "treffen sich vor seiner Th\u00fcr.", "tokens": ["tref\u00b7fen", "sich", "vor", "sei\u00b7ner", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ich halte die Hand vor und sehe in ein Fenster.", "tokens": ["Ich", "hal\u00b7te", "die", "Hand", "vor", "und", "se\u00b7he", "in", "ein", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Nichts.", "tokens": ["Nichts", "."], "token_info": ["word", "punct"], "pos": ["PIS", "$."], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Dann,", "tokens": ["Dann", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "blinkend,", "tokens": ["blin\u00b7kend", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "ein Goldrahmen,", "tokens": ["ein", "Gold\u00b7rah\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "verschwimmende Farben,", "tokens": ["ver\u00b7schwim\u00b7men\u00b7de", "Far\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "jetzt,", "tokens": ["jetzt", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "deutlich:", "tokens": ["deut\u00b7lich", ":"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.6": {"line.1": {"text": "Eine rosen\u00fcberstreute Tapete,", "tokens": ["Ei\u00b7ne", "ro\u00b7sen\u00b7\u00fc\u00b7bers\u00b7treu\u00b7te", "Ta\u00b7pe\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "ein blauer Divan,", "tokens": ["ein", "blau\u00b7er", "Di\u00b7van", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "eine nackte Dame f\u00fcttert einen Kakadu!", "tokens": ["ei\u00b7ne", "nack\u00b7te", "Da\u00b7me", "f\u00fct\u00b7tert", "ei\u00b7nen", "Ka\u00b7ka\u00b7du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}