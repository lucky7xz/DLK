{"textgrid.poem.39062": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Freiheit", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freiheit l\u00e4\u00dft sich nicht gewinnen,", "tokens": ["Die", "Frei\u00b7heit", "l\u00e4\u00dft", "sich", "nicht", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sie wird von au\u00dfen nicht erstrebt,", "tokens": ["sie", "wird", "von", "au\u00b7\u00dfen", "nicht", "er\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "wenn nicht zuerst sie selbst tief innen,", "tokens": ["wenn", "nicht", "zu\u00b7erst", "sie", "selbst", "tief", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADV", "PPER", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "im eignen Busen dich belebt.", "tokens": ["im", "eig\u00b7nen", "Bu\u00b7sen", "dich", "be\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Willst du den Kampf, den gro\u00dfen, wagen,", "tokens": ["Willst", "du", "den", "Kampf", ",", "den", "gro\u00b7\u00dfen", ",", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "so setz zuerst dich selber ein:", "tokens": ["so", "setz", "zu\u00b7erst", "dich", "sel\u00b7ber", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer fremde Fesseln will zerschlagen,", "tokens": ["Wer", "frem\u00b7de", "Fes\u00b7seln", "will", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "darf nicht sein eigner Sklave sein.", "tokens": ["darf", "nicht", "sein", "eig\u00b7ner", "Skla\u00b7ve", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nur reinen Herzen, reinen H\u00e4nden,", "tokens": ["Nur", "rei\u00b7nen", "Her\u00b7zen", ",", "rei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "geb\u00fchrt der Dienst im Heiligtum;", "tokens": ["ge\u00b7b\u00fchrt", "der", "Dienst", "im", "Hei\u00b7lig\u00b7tum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "der Freiheit Werk rein zu vollenden,", "tokens": ["der", "Frei\u00b7heit", "Werk", "rein", "zu", "voll\u00b7en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "dies, deutsches Volk, dies sei dein Ruhm.", "tokens": ["dies", ",", "deut\u00b7sches", "Volk", ",", "dies", "sei", "dein", "Ruhm", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ADJA", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die L\u00fcge winkt, die Schmeichler locken,", "tokens": ["Die", "L\u00fc\u00b7ge", "winkt", ",", "die", "Schmeich\u00b7ler", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "mit seiner Kette spielt der Knecht:", "tokens": ["mit", "sei\u00b7ner", "Ket\u00b7te", "spielt", "der", "Knecht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Du aber wandle unerschrocken,", "tokens": ["Du", "a\u00b7ber", "wand\u00b7le", "un\u00b7er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "und deine Waffe sei das Recht!", "tokens": ["und", "dei\u00b7ne", "Waf\u00b7fe", "sei", "das", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Freiheit l\u00e4\u00dft sich nicht gewinnen,", "tokens": ["Die", "Frei\u00b7heit", "l\u00e4\u00dft", "sich", "nicht", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter"