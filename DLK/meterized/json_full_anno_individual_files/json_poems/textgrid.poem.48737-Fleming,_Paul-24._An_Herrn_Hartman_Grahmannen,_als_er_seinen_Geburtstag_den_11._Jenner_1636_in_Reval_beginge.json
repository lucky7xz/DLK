{"textgrid.poem.48737": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "24. An Herrn Hartman Grahmannen, als er seinen Geburtstag den 11. Jenner 1636 in Reval beginge", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dank erstlich deinem Gott mit zwiefach frohem Mute,", "tokens": ["Dank", "erst\u00b7lich", "dei\u00b7nem", "Gott", "mit", "zwie\u00b7fach", "fro\u00b7hem", "Mu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPOSAT", "NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weil er dich zwiefach liebt, da\u00df er die h\u00f6chste Not,", "tokens": ["weil", "er", "dich", "zwie\u00b7fach", "liebt", ",", "da\u00df", "er", "die", "h\u00f6chs\u00b7te", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die dich auf diesen Tag nicht l\u00e4ngst wolt haben tot,", "tokens": ["die", "dich", "auf", "die\u00b7sen", "Tag", "nicht", "l\u00e4ngst", "wolt", "ha\u00b7ben", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PDAT", "NN", "PTKNEG", "ADV", "VMFIN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "auf lauter Leben doch zu schlagen aus geruhte:", "tokens": ["auf", "lau\u00b7ter", "Le\u00b7ben", "doch", "zu", "schla\u00b7gen", "aus", "ge\u00b7ruh\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "wo