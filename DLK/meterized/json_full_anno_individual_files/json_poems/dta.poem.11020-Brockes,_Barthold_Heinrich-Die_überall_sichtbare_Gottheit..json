{"dta.poem.11020": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die \u00fcberall  \n sichtbare Gottheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe meines K\u00f6rpers Seele, in meinem eignen K\u00f6rper,\nnicht,", "tokens": ["Ich", "se\u00b7he", "mei\u00b7nes", "K\u00f6r\u00b7pers", "See\u00b7le", ",", "in", "mei\u00b7nem", "eig\u00b7nen", "K\u00f6r\u00b7per", ",", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PTKNEG", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "So seh ich auch der Sonnen Sonne, die Gottheit, nicht im", "tokens": ["So", "seh", "ich", "auch", "der", "Son\u00b7nen", "Son\u00b7ne", ",", "die", "Got\u00b7theit", ",", "nicht", "im"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,", "ART", "NN", "$,", "PTKNEG", "APPRART"], "meter": "-+-+-+-+--++-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Da man die letzte nun so wenig, als wie die erste, leugnen kann:", "tokens": ["Da", "man", "die", "letz\u00b7te", "nun", "so", "we\u00b7nig", ",", "als", "wie", "die", "ers\u00b7te", ",", "leug\u00b7nen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "ADV", "ADV", "PIS", "$,", "KOUS", "KOKOM", "ART", "ADJA", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "So beth ich aller Sonnen Sonne, in unsrer Sonnen Stra-", "tokens": ["So", "be\u00b7th", "ich", "al\u00b7ler", "Son\u00b7nen", "Son\u00b7ne", ",", "in", "uns\u00b7rer", "Son\u00b7nen", "Stra"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "NN", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Mich deucht, in ihrem sichtbarn Glanz, den unsichtbaren Stral", "tokens": ["Mich", "deucht", ",", "in", "ih\u00b7rem", "sicht\u00b7barn", "Glanz", ",", "den", "un\u00b7sicht\u00b7ba\u00b7ren", "Stral"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Der Seelen der Natur, der Gottheit, belebend Wesen zu erh\u00f6hn.", "tokens": ["Der", "See\u00b7len", "der", "Na\u00b7tur", ",", "der", "Got\u00b7theit", ",", "be\u00b7le\u00b7bend", "We\u00b7sen", "zu", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "VVPP", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Doch mu\u00df man sich, die Gottheit nicht, als eine Seele", "tokens": ["Doch", "mu\u00df", "man", "sich", ",", "die", "Got\u00b7theit", "nicht", ",", "als", "ei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PIS", "PRF", "$,", "ART", "NN", "PTKNEG", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie sie in unserm K\u00f6rper ist, (so grob gedacht w\u00e4r) unterstehn;", "tokens": ["Wie", "sie", "in", "un\u00b7serm", "K\u00f6r\u00b7per", "ist", ",", "(", "so", "grob", "ge\u00b7dacht", "w\u00e4r", ")", "un\u00b7ter\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VAFIN", "$,", "$(", "ADV", "ADJD", "VVPP", "VAFIN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.9": {"text": "Nein, da\u00df sie \u00fcberall zu gegen und allenthalben wirke, denken.", "tokens": ["Nein", ",", "da\u00df", "sie", "\u00fc\u00b7be\u00b7rall", "zu", "ge\u00b7gen", "und", "al\u00b7len\u00b7thal\u00b7ben", "wir\u00b7ke", ",", "den\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "ADV", "APPR", "APPR", "KON", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "Die Absicht ist nur die\u00df: Die Kr\u00e4ste, auf welche wir fast gar", "tokens": ["Die", "Ab\u00b7sicht", "ist", "nur", "die\u00df", ":", "Die", "Kr\u00e4s\u00b7te", ",", "auf", "wel\u00b7che", "wir", "fast", "gar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PDS", "$.", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "Mit desto mehr Aufmerksamkeit, und Gott, in ihnen, zu be-", "tokens": ["Mit", "des\u00b7to", "mehr", "Auf\u00b7merk\u00b7sam\u00b7keit", ",", "und", "Gott", ",", "in", "ih\u00b7nen", ",", "zu", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,", "KON", "NN", "$,", "APPR", "PPER", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}