{"dta.poem.23588": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gott sei der armen Seele gn\u00e4dig!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Herr von Leib regieret jetzt,", "tokens": ["Der", "Herr", "von", "Leib", "re\u00b7gie\u00b7ret", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ganz gewaltiger Mann,", "tokens": ["Ein", "ganz", "ge\u00b7wal\u00b7ti\u00b7ger", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er ist gar werth und hochgesch\u00e4tzt,", "tokens": ["Er", "ist", "gar", "werth", "und", "hoch\u00b7ge\u00b7sch\u00e4tzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bleibt es auch fortan,", "tokens": ["Und", "bleibt", "es", "auch", "for\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn viele Millionen sind", "tokens": ["Denn", "vie\u00b7le", "Mil\u00b7lion\u00b7en", "sind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ihm unterthan mit Weib und Kind.", "tokens": ["Ihm", "un\u00b7ter\u00b7than", "mit", "Weib", "und", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Frau Seele schaffet Tag und Nacht,", "tokens": ["Frau", "See\u00b7le", "schaf\u00b7fet", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das arme gute Weib,", "tokens": ["Das", "ar\u00b7me", "gu\u00b7te", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gr\u00e4bt edles Erz aus manchem Schacht", "tokens": ["Gr\u00e4bt", "ed\u00b7les", "Erz", "aus", "man\u00b7chem", "Schacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nur f\u00fcr Herrn von Leib,", "tokens": ["Und", "nur", "f\u00fcr", "Herrn", "von", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn Herr von Leib das ist der Staat,", "tokens": ["Denn", "Herr", "von", "Leib", "das", "ist", "der", "Staat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr wisst schon, was ", "tokens": ["Ihr", "wisst", "schon", ",", "was"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "So wird in Kriegs- und Friedenszeit", "tokens": ["So", "wird", "in", "Kriegs", "und", "Frie\u00b7dens\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein theures Haupt bewacht,", "tokens": ["Sein", "theu\u00b7res", "Haupt", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zwar in Glanz und Herrlichkeit,", "tokens": ["Und", "zwar", "in", "Glanz", "und", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil's ihm Vergn\u00fcgen macht;", "tokens": ["Weil's", "ihm", "Ver\u00b7gn\u00fc\u00b7gen", "macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Und dies Vergn\u00fcgen kennt kein Ziel", "tokens": ["Und", "dies", "Ver\u00b7gn\u00fc\u00b7gen", "kennt", "kein", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kostet viel, ja viel viel viel.", "tokens": ["Und", "kos\u00b7tet", "viel", ",", "ja", "viel", "viel", "viel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Manch junger Held erh\u00e4lt viel Geld,", "tokens": ["Manch", "jun\u00b7ger", "Held", "er\u00b7h\u00e4lt", "viel", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blo\u00df weil er Wache steht", "tokens": ["Blo\u00df", "weil", "er", "Wa\u00b7che", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sorglos durch die Friedenswelt", "tokens": ["Und", "sorg\u00b7los", "durch", "die", "Frie\u00b7dens\u00b7welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Uniformen geht.", "tokens": ["In", "U\u00b7nif\u00b7or\u00b7men", "geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum zieh den bunten Rock auch an,", "tokens": ["Drum", "zieh", "den", "bun\u00b7ten", "Rock", "auch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du Seel', und werd' ein Kriegesmann!", "tokens": ["Du", "Seel'", ",", "und", "werd'", "ein", "Krie\u00b7ges\u00b7mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "KON", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}