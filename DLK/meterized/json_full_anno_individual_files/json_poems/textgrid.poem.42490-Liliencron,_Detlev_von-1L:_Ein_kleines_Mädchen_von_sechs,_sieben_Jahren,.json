{"textgrid.poem.42490": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein kleines M\u00e4dchen von sechs, sieben Jahren,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleines M\u00e4dchen von sechs, sieben Jahren,", "tokens": ["Ein", "klei\u00b7nes", "M\u00e4d\u00b7chen", "von", "sechs", ",", "sie\u00b7ben", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "CARD", "$,", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Kornblumenaugen und strohgelben Haaren,", "tokens": ["Mit", "Korn\u00b7blu\u00b7men\u00b7au\u00b7gen", "und", "stroh\u00b7gel\u00b7ben", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-++-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kommt mit einem Apfel gesprungen,", "tokens": ["Kommt", "mit", "ei\u00b7nem", "Ap\u00b7fel", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Hat ihn wie einen Ball geschwungen,", "tokens": ["Hat", "ihn", "wie", "ei\u00b7nen", "Ball", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von einer Hand ihn in die andre geflitzt,", "tokens": ["Von", "ei\u00b7ner", "Hand", "ihn", "in", "die", "and\u00b7re", "ge\u00b7flitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "APPR", "ART", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Da\u00df er blendend im grellen Sonnenlicht blitzt.", "tokens": ["Da\u00df", "er", "blen\u00b7dend", "im", "grel\u00b7len", "Son\u00b7nen\u00b7licht", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Sie sieht im Hofe hochaufget\u00fcrmt", "tokens": ["Sie", "sieht", "im", "Ho\u00b7fe", "hoch\u00b7auf\u00b7ge\u00b7t\u00fcrmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Einen Holzsto\u00df, und ist gleich hingest\u00fcrmt.", "tokens": ["Ei\u00b7nen", "Holz\u00b7sto\u00df", ",", "und", "ist", "gleich", "hin\u00b7ge\u00b7st\u00fcrmt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Und wie ein K\u00e4tzchen, katzenleicht,", "tokens": ["Und", "wie", "ein", "K\u00e4tz\u00b7chen", ",", "kat\u00b7zen\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Hat sie schnell die Spitze erreicht,", "tokens": ["Hat", "sie", "schnell", "die", "Spit\u00b7ze", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Und hockt nun dort, und will mit Begehren", "tokens": ["Und", "hockt", "nun", "dort", ",", "und", "will", "mit", "Be\u00b7geh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "KON", "VMFIN", "APPR", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Den gl\u00e4nzenden, goldgelben Apfel verzehren.", "tokens": ["Den", "gl\u00e4n\u00b7zen\u00b7den", ",", "gold\u00b7gel\u00b7ben", "Ap\u00b7fel", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.13": {"text": "Da, holterdipolter! pardauz! pardau!", "tokens": ["Da", ",", "hol\u00b7ter\u00b7di\u00b7pol\u00b7ter", "!", "par\u00b7dauz", "!", "par\u00b7dau", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$.", "NN", "$.", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Bricht zusammen der k\u00fcnstliche Bau.", "tokens": ["Bricht", "zu\u00b7sam\u00b7men", "der", "k\u00fcnst\u00b7li\u00b7che", "Bau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Wie bei Bergrutsch und Felsenbeben", "tokens": ["Wie", "bei", "Berg\u00b7rutsch", "und", "Fel\u00b7sen\u00b7be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "KON", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Haben Bretter und Scheite nachgegeben;", "tokens": ["Ha\u00b7ben", "Bret\u00b7ter", "und", "Schei\u00b7te", "nach\u00b7ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Wie alle Neun im Kegelspiel,", "tokens": ["Wie", "al\u00b7le", "Neun", "im", "Ke\u00b7gel\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "CARD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "So alles \u00fcber einander fiel.", "tokens": ["So", "al\u00b7les", "\u00fc\u00b7ber", "ein\u00b7an\u00b7der", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Die Leute im Hofe habens geh\u00f6rt", "tokens": ["Die", "Leu\u00b7te", "im", "Ho\u00b7fe", "ha\u00b7bens", "ge\u00b7h\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "NE", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Und laufen hin entsetzt und verst\u00f6rt;", "tokens": ["Und", "lau\u00b7fen", "hin", "ent\u00b7setzt", "und", "ver\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Die Mutter liegt ohnm\u00e4chtig, Gott erbarm,", "tokens": ["Die", "Mut\u00b7ter", "liegt", "ohn\u00b7m\u00e4ch\u00b7tig", ",", "Gott", "er\u00b7barm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Einem raschen Nachbarn im hilfreichen Arm.", "tokens": ["Ei\u00b7nem", "ra\u00b7schen", "Nach\u00b7barn", "im", "hilf\u00b7rei\u00b7chen", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.23": {"text": "Nun gehts ans R\u00e4umen der Tr\u00fcmmer von oben,", "tokens": ["Nun", "gehts", "ans", "R\u00e4u\u00b7men", "der", "Tr\u00fcm\u00b7mer", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "Vorsichtig wird St\u00fcck f\u00fcr St\u00fcck gehoben,", "tokens": ["Vor\u00b7sich\u00b7tig", "wird", "St\u00fcck", "f\u00fcr", "St\u00fcck", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.25": {"text": "Vorsichtig gehts weiter in dumpfem Schweigen,", "tokens": ["Vor\u00b7sich\u00b7tig", "gehts", "wei\u00b7ter", "in", "dum\u00b7pfem", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Der Atem stockt: was wird sich zeigen?", "tokens": ["Der", "A\u00b7tem", "stockt", ":", "was", "wird", "sich", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "VAFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Da \u2013 sitzt in einer gew\u00f6lbten Halle", "tokens": ["Da", "\u2013", "sitzt", "in", "ei\u00b7ner", "ge\u00b7w\u00f6lb\u00b7ten", "Hal\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$(", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"t