, "pos": ["ADJD", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.26": {"text": "Der Atem stockt: was wird sich zeigen?", "tokens": ["Der", "A\u00b7tem", "stockt", ":", "was", "wird", "sich", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "VAFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Da \u2013 sitzt in einer gew\u00f6lbten Halle", "tokens": ["Da", "\u2013", "sitzt", "in", "ei\u00b7ner", "ge\u00b7w\u00f6lb\u00b7ten", "Hal\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$(", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Das l\u00e4chelnde Kind wie die Maus in der Falle,", "tokens": ["Das", "l\u00e4\u00b7cheln\u00b7de", "Kind", "wie", "die", "Maus", "in", "der", "Fal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.29": {"text": "Hat schon vergessen den Purzelschrecken,", "tokens": ["Hat", "schon", "ver\u00b7ges\u00b7sen", "den", "Pur\u00b7zel\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Und bei\u00dft in den Apfel und l\u00e4\u00dft sichs schmecken.", "tokens": ["Und", "bei\u00dft", "in", "den", "Ap\u00b7fel", "und", "l\u00e4\u00dft", "sichs", "schme\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "PIS", "VVINF", "$.