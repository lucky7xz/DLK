{"dta.poem.8199": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Eine Fr\u00fchlingsnacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kranke liegt auf dem hei\u00dfen Pf\u00fchl.", "tokens": ["Der", "Kran\u00b7ke", "liegt", "auf", "dem", "hei\u00b7\u00dfen", "Pf\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Fieber hat er die Nacht verbracht;", "tokens": ["Im", "Fie\u00b7ber", "hat", "er", "die", "Nacht", "ver\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sein Herz ist m\u00fcde, sein Auge verwacht.", "tokens": ["Sein", "Herz", "ist", "m\u00fc\u00b7de", ",", "sein", "Au\u00b7ge", "ver\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Er lauscht auf der Stunden rinnenden Sand;", "tokens": ["Er", "lauscht", "auf", "der", "Stun\u00b7den", "rin\u00b7nen\u00b7den", "Sand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er h\u00e4lt die Uhr in der wei\u00dfen Hand.", "tokens": ["Er", "h\u00e4lt", "die", "Uhr", "in", "der", "wei\u00b7\u00dfen", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Er z\u00e4hlt die Schl\u00e4ge, die sie pickt,", "tokens": ["Er", "z\u00e4hlt", "die", "Schl\u00e4\u00b7ge", ",", "die", "sie", "pickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er forschet, wie der Weiser r\u00fcckt;", "tokens": ["Er", "for\u00b7schet", ",", "wie", "der", "Wei\u00b7ser", "r\u00fcckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es fragt ihn, ob er noch leb' vielleicht,", "tokens": ["Es", "fragt", "ihn", ",", "ob", "er", "noch", "leb'", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn der Weiser die schwarze Drei erreicht.", "tokens": ["Wenn", "der", "Wei\u00b7ser", "die", "schwar\u00b7ze", "Drei", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Die Wartfrau sitzt geduldig dabei,", "tokens": ["Die", "Wart\u00b7frau", "sitzt", "ge\u00b7dul\u00b7dig", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Harrend bis Alles vor\u00fcber sei. \u2014", "tokens": ["Har\u00b7rend", "bis", "Al\u00b7les", "vor\u00b7\u00fc\u00b7ber", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "PIS", "ADV", "VAFIN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Schon auf dem Herzen dr\u00fcckt ihn der Tod", "tokens": ["Schon", "auf", "dem", "Her\u00b7zen", "dr\u00fcckt", "ihn", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und drau\u00dfen d\u00e4mmert das Morgenroth;", "tokens": ["Und", "drau\u00b7\u00dfen", "d\u00e4m\u00b7mert", "das", "Mor\u00b7gen\u00b7roth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "An die Fenster klettert der Fr\u00fchlingstag,", "tokens": ["An", "die", "Fens\u00b7ter", "klet\u00b7tert", "der", "Fr\u00fch\u00b7lings\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "M\u00e4dchen und V\u00f6gel werden wach.", "tokens": ["M\u00e4d\u00b7chen", "und", "V\u00f6\u00b7gel", "wer\u00b7den", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Die Erde lacht in Liebesschein,", "tokens": ["Die", "Er\u00b7de", "lacht", "in", "Lie\u00b7bes\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Pfingstglocken l\u00e4uten das Brautfest ein;", "tokens": ["Pfingst\u00b7glo\u00b7cken", "l\u00e4u\u00b7ten", "das", "Braut\u00b7fest", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Singende Bursche ziehn \u00fcber's Feld", "tokens": ["Sin\u00b7gen\u00b7de", "Bur\u00b7sche", "ziehn", "\u00fc\u00b7ber's", "Feld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hinein in die bl\u00fchende, klingende Welt. \u2014", "tokens": ["Hin\u00b7ein", "in", "die", "bl\u00fc\u00b7hen\u00b7de", ",", "klin\u00b7gen\u00b7de", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Und immer stiller wird es drin;", "tokens": ["Und", "im\u00b7mer", "stil\u00b7ler", "wird", "es", "drin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Alte tritt zum Kranken hin.", "tokens": ["Die", "Al\u00b7te", "tritt", "zum", "Kran\u00b7ken", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der hat die H\u00e4nde gefaltet dicht;", "tokens": ["Der", "hat", "die", "H\u00e4n\u00b7de", "ge\u00b7fal\u00b7tet", "dicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie zieht ihm das Laken \u00fcber's Gesicht.", "tokens": ["Sie", "zieht", "ihm", "das", "La\u00b7ken", "\u00fc\u00b7ber's", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Dann geht sie fort. Stumm wird's und leer;", "tokens": ["Dann", "geht", "sie", "fort", ".", "Stumm", "wird's", "und", "leer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "VAFIN", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und drinnen wacht kein Auge mehr.", "tokens": ["Und", "drin\u00b7nen", "wacht", "kein", "Au\u00b7ge", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}