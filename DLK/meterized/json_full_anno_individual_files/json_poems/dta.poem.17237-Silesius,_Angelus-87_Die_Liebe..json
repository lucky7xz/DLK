{"dta.poem.17237": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "87  Die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe dieser Welt wil alls f\u00fcr sich allein.", "tokens": ["Die", "Lie\u00b7be", "die\u00b7ser", "Welt", "wil", "alls", "f\u00fcr", "sich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VMFIN", "ADV", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe GOttes macht dem N\u00e4chsten alls gemein:", "tokens": ["Die", "Lie\u00b7be", "Got\u00b7tes", "macht", "dem", "N\u00e4chs\u00b7ten", "alls", "ge\u00b7mein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die wird ein jeder Mensch f\u00fcr Liebe wol erkennen/", "tokens": ["Die", "wird", "ein", "je\u00b7der", "Mensch", "f\u00fcr", "Lie\u00b7be", "wol", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "PIAT", "NN", "APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jen\u2019 aber sol man Neid/ und keine Liebe nennen.", "tokens": ["Jen'", "a\u00b7ber", "sol", "man", "Neid", "/", "und", "kei\u00b7ne", "Lie\u00b7be", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "p