{"textgrid.poem.36682": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: W\u00e4r's wahr, o Nymphe? h\u00e4tte den Dichter wohl", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r's wahr, o Nymphe? h\u00e4tte den Dichter wohl", "tokens": ["W\u00e4r's", "wahr", ",", "o", "Nym\u00b7phe", "?", "h\u00e4t\u00b7te", "den", "Dich\u00b7ter", "wohl"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "FM", "NN", "$.", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vielleicht des Felsquells Lieblichkeit nicht get\u00e4uscht,", "tokens": ["Viel\u00b7leicht", "des", "Fels\u00b7quells", "Lieb\u00b7lich\u00b7keit", "nicht", "ge\u00b7t\u00e4uscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Du w\u00e4rst es, ewig flie\u00dfend Wesen,", "tokens": ["Du", "w\u00e4rst", "es", ",", "e\u00b7wig", "flie\u00b7\u00dfend", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das hier den Verghang hinuntermurmelt,", "tokens": ["Das", "hier", "den", "Ver\u00b7ghang", "hin\u00b7un\u00b7ter\u00b7mur\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du w\u00e4rst, als Numa, deinen Pompil, der Tod", "tokens": ["Du", "w\u00e4rst", ",", "als", "Nu\u00b7ma", ",", "dei\u00b7nen", "Pom\u00b7pil", ",", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "NE", "$,", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur Schattenwelt entf\u00fchrte, vor Schmerz und Weh", "tokens": ["Zur", "Schat\u00b7ten\u00b7welt", "ent\u00b7f\u00fchr\u00b7te", ",", "vor", "Schmerz", "und", "Weh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "An dieses H\u00fcgels Felsenwurzel", "tokens": ["An", "die\u00b7ses", "H\u00fc\u00b7gels", "Fel\u00b7sen\u00b7wur\u00b7zel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4rest vergangen in Thr\u00e4nenstr\u00f6men?", "tokens": ["W\u00e4\u00b7rest", "ver\u00b7gan\u00b7gen", "in", "Thr\u00e4\u00b7nen\u00b7str\u00f6\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dein h\u00e4tte sich die taurische Artemis", "tokens": ["Dein", "h\u00e4t\u00b7te", "sich", "die", "tau\u00b7ri\u00b7sche", "Ar\u00b7te\u00b7mis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VAFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erbarmt, dein jammernd Flehen geendet dir?", "tokens": ["Er\u00b7barmt", ",", "dein", "jam\u00b7mernd", "Fle\u00b7hen", "ge\u00b7en\u00b7det", "dir", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "ADJD", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O dann, du Bergstrom, k\u00fcss' erfrischend,", "tokens": ["O", "dann", ",", "du", "Berg\u00b7strom", ",", "k\u00fcss'", "er\u00b7fri\u00b7schend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "PPER", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00fcsse mir, Nymphe, die hei\u00dfen Lippen.", "tokens": ["K\u00fcs\u00b7se", "mir", ",", "Nym\u00b7phe", ",", "die", "hei\u00b7\u00dfen", "Lip\u00b7pen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Aus Treue sterben! Sch\u00f6nster Gedanke du,", "tokens": ["Aus", "Treu\u00b7e", "ster\u00b7ben", "!", "Sch\u00f6ns\u00b7ter", "Ge\u00b7dan\u00b7ke", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$.", "NN", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus unsern Tagen lange hinweggeflohn", "tokens": ["Aus", "un\u00b7sern", "Ta\u00b7gen", "lan\u00b7ge", "hin\u00b7weg\u00b7ge\u00b7flohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ins Reich der Dichtung, in die Zeiten,", "tokens": ["Ins", "Reich", "der", "Dich\u00b7tung", ",", "in", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ihn die Menschen von G\u00f6ttern lernten.", "tokens": ["Da", "ihn", "die", "Men\u00b7schen", "von", "G\u00f6t\u00b7tern", "lern\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Aus Treue sterben! Seliger Knabentraum,", "tokens": ["Aus", "Treu\u00b7e", "ster\u00b7ben", "!", "Se\u00b7li\u00b7ger", "Kna\u00b7ben\u00b7traum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du Stolz des thatengl\u00fchenden J\u00fcnglinges,", "tokens": ["Du", "Stolz", "des", "tha\u00b7ten\u00b7gl\u00fc\u00b7hen\u00b7den", "J\u00fcng\u00b7lin\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du \u00fcberschw\u00e4nglich Wort der Liebe,", "tokens": ["Du", "\u00fc\u00b7bersc\u00b7hw\u00e4ng\u00b7lich", "Wort", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Grausamer Spott des entt\u00e4uschten Pilgers!", "tokens": ["Grau\u00b7sa\u00b7mer", "Spott", "des", "ent\u00b7t\u00e4uschten", "Pil\u00b7gers", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Aus Treue sterben! K\u00f6nigsgeliebte du,", "tokens": ["Aus", "Treu\u00b7e", "ster\u00b7ben", "!", "K\u00f6\u00b7nigs\u00b7ge\u00b7lieb\u00b7te", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$.", "VMFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Trauer deinem ewig lebend'gen Grab", "tokens": ["Mit", "Trau\u00b7er", "dei\u00b7nem", "e\u00b7wig", "le\u00b7ben\u00b7d'\u00b7gen", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nah' ich, dir eine Schuld bekennend:", "tokens": ["Nah'", "ich", ",", "dir", "ei\u00b7ne", "Schuld", "be\u00b7ken\u00b7nend", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6re mich, Sterbende! Nimmer glaubt' ich", "tokens": ["H\u00f6\u00b7re", "mich", ",", "Ster\u00b7ben\u00b7de", "!", "Nim\u00b7mer", "glaubt'", "ich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "ADV", "VVFIN", "PPER"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "An Menschentreue. Wie es so kam, es sei", "tokens": ["An", "Men\u00b7schen\u00b7treu\u00b7e", ".", "Wie", "es", "so", "kam", ",", "es", "sei"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$.", "PWAV", "PPER", "ADV", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vergessen \u2013 aber Nymphe, wenn wahr, da\u00df du", "tokens": ["Ver\u00b7ges\u00b7sen", "\u2013", "a\u00b7ber", "Nym\u00b7phe", ",", "wenn", "wahr", ",", "da\u00df", "du"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "$(", "KON", "NN", "$,", "KOUS", "ADJD", "$,", "KOUS", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gestorben f\u00fcr Pompil, so la\u00df mich", "tokens": ["Ge\u00b7stor\u00b7ben", "f\u00fcr", "Pom\u00b7pil", ",", "so", "la\u00df", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "$,", "ADV", "VVIMP", "PPER"], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Artemis hier f\u00fcr den Frevel b\u00fc\u00dfen.", "tokens": ["Ar\u00b7te\u00b7mis", "hier", "f\u00fcr", "den", "Fre\u00b7vel", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Ich will ja glauben, G\u00f6ttliche, da\u00df du treu", "tokens": ["Ich", "will", "ja", "glau\u00b7ben", ",", "G\u00f6tt\u00b7li\u00b7che", ",", "da\u00df", "du", "treu"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Freund geblieben; denn von olympischem", "tokens": ["Dem", "Freund", "ge\u00b7blie\u00b7ben", ";", "denn", "von", "o\u00b7lym\u00b7pi\u00b7schem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "KON", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ursprung ist ja dein Herz: der Erde", "tokens": ["Ur\u00b7sprung", "ist", "ja", "dein", "Herz", ":", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kinder nur hab' ich nicht treu gefunden.", "tokens": ["Kin\u00b7der", "nur", "hab'", "ich", "nicht", "treu", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "An deinem Felsen, einsamer alter Hain,", "tokens": ["An", "dei\u00b7nem", "Fel\u00b7sen", ",", "ein\u00b7sa\u00b7mer", "al\u00b7ter", "Hain", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hier, wo Orest einst mit Iphigenien", "tokens": ["Hier", ",", "wo", "O\u00b7rest", "einst", "mit", "Ip\u00b7hi\u00b7ge\u00b7ni\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "NE", "ADV", "APPR", "NE"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der taur'schen G\u00f6ttin Bild gefl\u00fcchtet,", "tokens": ["Der", "taur'\u00b7schen", "G\u00f6t\u00b7tin", "Bild", "ge\u00b7fl\u00fcch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schau' ich hinab zum Dianenspiegel,", "tokens": ["Schau'", "ich", "hin\u00b7ab", "zum", "Di\u00b7a\u00b7nen\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Und schau' und fleh' und weine, bis mich die Huld", "tokens": ["Und", "schau'", "und", "fleh'", "und", "wei\u00b7ne", ",", "bis", "mich", "die", "Huld"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der G\u00f6ttin einmal pl\u00f6tzlich zerflie\u00dfen l\u00e4\u00dft,", "tokens": ["Der", "G\u00f6t\u00b7tin", "ein\u00b7mal", "pl\u00f6tz\u00b7lich", "zer\u00b7flie\u00b7\u00dfen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ich f\u00fcr meinen Glauben sterbe: \u2013", "tokens": ["Und", "ich", "f\u00fcr", "mei\u00b7nen", "Glau\u00b7ben", "ster\u00b7be", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Treu sind die Himmlischen, nicht die Menschen.", "tokens": ["Treu", "sind", "die", "Himm\u00b7li\u00b7schen", ",", "nicht", "die", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "W\u00e4r's wahr, o Nymphe? h\u00e4tte den Dichter wohl", "tokens": ["W\u00e4r's", "wahr", ",", "o", "Nym\u00b7phe", "?", "h\u00e4t\u00b7te", "den", "Dich\u00b7ter", "wohl"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "$,", "FM", "NN", "$.", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vielleicht des Felsquells Lieblichkeit nicht get\u00e4uscht,", "tokens": ["Viel\u00b7leicht", "des", "Fels\u00b7quells", "Lieb\u00b7lich\u00b7keit", "nicht", "ge\u00b7t\u00e4uscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Du w\u00e4rst es, ewig flie\u00dfend Wesen,", "tokens": ["Du", "w\u00e4rst", "es", ",", "e\u00b7wig", "flie\u00b7\u00dfend", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das hier den Verghang hinuntermurmelt,", "tokens": ["Das", "hier", "den", "Ver\u00b7ghang", "hin\u00b7un\u00b7ter\u00b7mur\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Du w\u00e4rst, als Numa, deinen Pompil, der Tod", "tokens": ["Du", "w\u00e4rst", ",", "als", "Nu\u00b7ma", ",", "dei\u00b7nen", "Pom\u00b7pil", ",", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "NE", "$,", "PPOSAT", "NN", "$,", "ART", "NN"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zur Schattenwelt entf\u00fchrte, vor Schmerz und Weh", "tokens": ["Zur", "Schat\u00b7ten\u00b7welt", "ent\u00b7f\u00fchr\u00b7te", ",", "vor", "Schmerz", "und", "Weh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "An dieses H\u00fcgels Felsenwurzel", "tokens": ["An", "die\u00b7ses", "H\u00fc\u00b7gels", "Fel\u00b7sen\u00b7wur\u00b7zel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "