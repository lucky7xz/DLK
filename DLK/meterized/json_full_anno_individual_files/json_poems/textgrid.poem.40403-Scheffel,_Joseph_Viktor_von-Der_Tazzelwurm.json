{"textgrid.poem.40403": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der Tazzelwurm", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als noch ein Bergsee klar und gro\u00df", "tokens": ["Als", "noch", "ein", "Berg\u00b7see", "klar", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser T\u00e4ler Tiefen flo\u00df,", "tokens": ["In", "die\u00b7ser", "T\u00e4\u00b7ler", "Tie\u00b7fen", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hab' ich allhier in grober Pracht", "tokens": ["Hab'", "ich", "all\u00b7hier", "in", "gro\u00b7ber", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gelebt, geliebt und auch gedracht", "tokens": ["Ge\u00b7lebt", ",", "ge\u00b7liebt", "und", "auch", "ge\u00b7dracht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVPP", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als Tazzelwurm.", "tokens": ["Als", "Taz\u00b7zel\u00b7wurm", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Vom Pentling bis zum Wendelstein", "tokens": ["Vom", "Pent\u00b7ling", "bis", "zum", "Wen\u00b7del\u00b7stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War Fels und Luft und Wasser mein,", "tokens": ["War", "Fels", "und", "Luft", "und", "Was\u00b7ser", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "KON", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich flog und ging und lag gerollt,", "tokens": ["Ich", "flog", "und", "ging", "und", "lag", "ge\u00b7rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und statt auf Heu schlief ich auf Gold", "tokens": ["Und", "statt", "auf", "Heu", "schlief", "ich", "auf", "Gold"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als Tazzelwurm.", "tokens": ["Als", "Taz\u00b7zel\u00b7wurm", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Hornhautig war mein Schuppenleib", "tokens": ["Horn\u00b7hau\u00b7tig", "war", "mein", "Schup\u00b7pen\u00b7leib"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Feuerspei'n mein Zeitvertreib,", "tokens": ["Und", "Feu\u00b7er\u00b7spei'n", "mein", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was da kroch den Berg herauf,", "tokens": ["Und", "was", "da", "kroch", "den", "Berg", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das blies ich um und fra\u00df es auf", "tokens": ["Das", "blies", "ich", "um", "und", "fra\u00df", "es", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als Tazzelwurm.", "tokens": ["Als", "Taz\u00b7zel\u00b7wurm", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Doch als ich mich so weit verga\u00df", "tokens": ["Doch", "als", "ich", "mich", "so", "weit", "ver\u00b7ga\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Sennerinnen roh auffra\u00df,", "tokens": ["Und", "Sen\u00b7ne\u00b7rin\u00b7nen", "roh", "auf\u00b7fra\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kam die S\u00fcndflut grausenhaft", "tokens": ["Da", "kam", "die", "S\u00fcnd\u00b7flut", "grau\u00b7sen\u00b7haft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tilgte meine Bergwirtschaft", "tokens": ["Und", "tilg\u00b7te", "mei\u00b7ne", "Berg\u00b7wirt\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Tazzelwurm.", "tokens": ["Zum", "Taz\u00b7zel\u00b7wurm", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Jetzt zier' ich nur gemalt im Bild", "tokens": ["Jetzt", "zier'", "ich", "nur", "ge\u00b7malt", "im", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Schweinesteigers neuen Schild,", "tokens": ["Des", "Schwei\u00b7nes\u00b7tei\u00b7gers", "neu\u00b7en", "Schild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Senn'rin h\u00f6rt man jauchzend schrei'n,", "tokens": ["Die", "Senn'\u00b7rin", "h\u00f6rt", "man", "jauch\u00b7zend", "schrei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und keine f\u00fcrcht't das Feuerspei'n", "tokens": ["Und", "kei\u00b7ne", "f\u00fcrcht't", "das", "Feu\u00b7er\u00b7spei'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Tazzelwurms.", "tokens": ["Des", "Taz\u00b7zel\u00b7wurms", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und kommt so ein gelahrtes Haus,", "tokens": ["Und", "kommt", "so", "ein", "ge\u00b7lahr\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So h\u00f6hnt's und spricht: \u00bbMit ", "tokens": ["So", "h\u00f6hnt's", "und", "spricht", ":", "\u00bb", "Mit"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word"], "pos": ["ADV", "NE", "KON", "VVFIN", "$.", "$(", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Der war ein vors\u00fcndflutlich Vieh,", "tokens": ["Der", "war", "ein", "vor\u00b7s\u00fcnd\u00b7flut\u00b7lich", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch weise M\u00e4nner sahn noch nie", "tokens": ["Doch", "wei\u00b7se", "M\u00e4n\u00b7ner", "sahn", "noch", "nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Tazzelwurm.\u00ab", "tokens": ["Den", "Taz\u00b7zel\u00b7wurm", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Kleingl\u00e4ub'ge Zweifler! kehrt nur ein", "tokens": ["Klein\u00b7gl\u00e4ub'\u00b7ge", "Zweif\u00b7ler", "!", "kehrt", "nur", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und setzt auf Bier Tiroler Wein ...", "tokens": ["Und", "setzt", "auf", "Bier", "Ti\u00b7ro\u00b7ler", "Wein", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob ihr dann bis nach Kufstein fleucht,", "tokens": ["Ob", "ihr", "dann", "bis", "nach", "Kufs\u00b7tein", "fleucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sp\u00fcrt, da\u00df ich euch angekeucht", "tokens": ["Ihr", "sp\u00fcrt", ",", "da\u00df", "ich", "euch", "an\u00b7ge\u00b7keucht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als Tazzelwurm.", "tokens": ["Als", "Taz\u00b7zel\u00b7wurm", "."], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Und ernsthaft spricht der Klausenwirt:", "tokens": ["Und", "ernst\u00b7haft", "spricht", "der", "Klau\u00b7sen\u00b7wirt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbschwernot! woher sind ", "tokens": ["\u00bb", "schwer\u00b7not", "!", "wo\u00b7her", "sind"], "token_info": ["punct", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "$.", "PWAV", "VAFIN"], "meter": "++-+-", "measure": "iambic.di"}, "line.3": {"text": "Das Fu\u00dfwerk schwankt ... im Kopf ist Sturm ...", "tokens": ["Das", "Fu\u00df\u00b7werk", "schwankt", "...", "im", "Kopf", "ist", "Sturm", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "APPRART", "NN", "VAFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sahen all' den Tazzelwurm!", "tokens": ["Die", "sa\u00b7hen", "all'", "den", "Taz\u00b7zel\u00b7wurm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Tazzelwurm!\u00ab", "tokens": ["Den", "Taz\u00b7zel\u00b7wurm", "!", "\u00ab"], "token_info": ["word", "word"