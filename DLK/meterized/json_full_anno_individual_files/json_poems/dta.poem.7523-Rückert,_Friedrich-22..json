{"dta.poem.7523": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Echo, das du weckst, reizt dich, o Nachtigall,", "tokens": ["Das", "E\u00b7cho", ",", "das", "du", "weckst", ",", "reizt", "dich", ",", "o", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie einen Dichter spornt des Beifalls Widerhall.", "tokens": ["Wie", "ei\u00b7nen", "Dich\u00b7ter", "spornt", "des", "Bei\u00b7falls", "Wi\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ist der Widerhall? Bist du es nicht allein?", "tokens": ["Was", "ist", "der", "Wi\u00b7der\u00b7hall", "?", "Bist", "du", "es", "nicht", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "VAFIN", "PPER", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gib dir den Beifall selbst, und la\u00df den tauben Stein.", "tokens": ["Gib", "dir", "den", "Bei\u00b7fall", "selbst", ",", "und", "la\u00df", "den", "tau\u00b7ben", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADV", "$,", "KON", "VVIMP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was hilfts! Es w\u00e4chst die Kraft des Worts und seine Lust,", "tokens": ["Was", "hilfts", "!", "Es", "w\u00e4chst", "die", "Kraft", "des", "Worts", "und", "sei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn statt aus deiner du es sprichst aus Aller Brust.", "tokens": ["Wenn", "statt", "aus", "dei\u00b7ner", "du", "es", "sprichst", "aus", "Al\u00b7ler", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "PPER", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}