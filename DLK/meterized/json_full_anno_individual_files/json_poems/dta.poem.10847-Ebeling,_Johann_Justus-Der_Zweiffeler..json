{"dta.poem.10847": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der Zweiffeler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Zweifler ist ein Mensch, der in der\nWirbel Schl\u00fcnden,", "tokens": ["Ein", "Zweif\u00b7ler", "ist", "ein", "Mensch", ",", "der", "in", "der", "Wir\u00b7bel", "Schl\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sich stets kr\u00e4uselnd drehn, wil", "tokens": ["Die", "sich", "stets", "kr\u00e4u\u00b7selnd", "drehn", ",", "wil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PRF", "ADV", "ADJD", "VVINF", "$,", "VMFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er zweiffelt immerfort an dem was offen-", "tokens": ["Er", "zweif\u00b7felt", "im\u00b7mer\u00b7fort", "an", "dem", "was", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "PWS", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was solche Gr\u00fcnde zeigt, die sicher und ganz", "tokens": ["Was", "sol\u00b7che", "Gr\u00fcn\u00b7de", "zeigt", ",", "die", "si\u00b7cher", "und", "ganz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "VVFIN", "$,", "PRELS", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Hingegen glaubet er, wenn er doch sichtbahr feh-", "tokens": ["Hin\u00b7ge\u00b7gen", "glau\u00b7bet", "er", ",", "wenn", "er", "doch", "sicht\u00b7bahr", "feh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Er habe nach Vernunfft das Beste auserw\u00e4h-", "tokens": ["Er", "ha\u00b7be", "nach", "Ver\u00b7nunfft", "das", "Bes\u00b7te", "aus\u00b7er\u00b7w\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mir deucht ein Zweiffler nimmt was wahr, darum", "tokens": ["Mir", "deucht", "ein", "Zweiff\u00b7ler", "nimmt", "was", "wahr", ",", "da\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "PWS", "ADJD", "$,", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Weil er es nicht allein, als Warheit glauben", "tokens": ["Weil", "er", "es", "nicht", "al\u00b7lein", ",", "als", "War\u00b7heit", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADV", "$,", "KOUS", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Sein angebohrner Stolz wil was vor andern", "tokens": ["Sein", "an\u00b7ge\u00b7bohr\u00b7ner", "Stolz", "wil", "was", "vor", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PIS", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Drum sezt er, wie man sagt, die Warheit stets auf", "tokens": ["Drum", "sezt", "er", ",", "wie", "man", "sagt", ",", "die", "War\u00b7heit", "stets", "auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VVFIN", "$,", "ART", "NN", "ADV", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Wo dunkle L\u00f6cher sind entsteht ein Wirbel", "tokens": ["Wo", "dunk\u00b7le", "L\u00f6\u00b7cher", "sind", "ent\u00b7steht", "ein", "Wir\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Mir deucht ein dunkel Hirn macht da\u00df viel Zweif-", "tokens": ["Mir", "deucht", "ein", "dun\u00b7kel", "Hirn", "macht", "da\u00df", "viel", "Zweif"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "KOUS", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}