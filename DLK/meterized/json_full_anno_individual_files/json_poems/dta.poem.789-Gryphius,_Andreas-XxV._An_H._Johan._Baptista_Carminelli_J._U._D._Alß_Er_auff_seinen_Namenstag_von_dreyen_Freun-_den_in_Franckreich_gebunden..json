{"dta.poem.789": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxV.  \n An H.  Johan. Baptista Carminelli J. U. D.  Al\u00df  \n Er auff seinen Namenstag von dreyen Freun-  \n den in Franckreich gebunden.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Drey Seelen binden dich/ die ein in einem Mund. ", "tokens": ["Drey", "See\u00b7len", "bin\u00b7den", "dich", "/", "die", "ein", "in", "ei\u00b7nem", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "$(", "PRELS", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drey Geister/ doch ein Hertz/ drey haben di\u00df geschrieben/", "tokens": ["Drey", "Geis\u00b7ter", "/", "doch", "ein", "Hertz", "/", "drey", "ha\u00b7ben", "di\u00df", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "ADV", "ART", "NN", "$(", "CARD", "VAFIN", "PDS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch nur durch eine Faust. Dir/ den sie einig lieben", "tokens": ["Doch", "nur", "durch", "ei\u00b7ne", "Faust", ".", "Dir", "/", "den", "sie", "ei\u00b7nig", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$.", "PPER", "$(", "ART", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verkn\u00fcpfft durch eine trew\u2019 vnd ewig-festen bund.", "tokens": ["Ver\u00b7kn\u00fcpfft", "durch", "ei\u00b7ne", "tre\u00b7w'", "vnd", "e\u00b7wig\u00b7fes\u00b7ten", "bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Drey binden mit sich selbst/ drey w\u00fcndschen dich gesund/", "tokens": ["Drey", "bin\u00b7den", "mit", "sich", "selbst", "/", "drey", "w\u00fcnd\u00b7schen", "dich", "ge\u00b7sund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "APPR", "PRF", "ADV", "$(", "CARD", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drey geben/ was bi\u00dfher nach leichen \u00fcberblieben/", "tokens": ["Drey", "ge\u00b7ben", "/", "was", "bi\u00df\u00b7her", "nach", "lei\u00b7chen", "\u00fc\u00b7berb\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVINF", "$(", "PWS", "ADV", "APPR", "ADJA", "VVINF", "$("], "meter": "+---+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Was keiner zeitten grimm/ was keine Pest vertrieben:", "tokens": ["Was", "kei\u00b7ner", "zeit\u00b7ten", "grimm", "/", "was", "kei\u00b7ne", "Pest", "ver\u00b7trie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "ADJD", "$(", "PWS", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Freundschafft/ die \u00fcber Todt/ vnd Leben/ zeit vnd Stund.", "tokens": ["Freund\u00b7schafft", "/", "die", "\u00fc\u00b7ber", "Todt", "/", "vnd", "Le\u00b7ben", "/", "zeit", "vnd", "Stund", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "APPR", "NN", "$(", "KON", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Di\u00df was der Himmel kennt/ was in den L\u00fcfften schwebt/", "tokens": ["Di\u00df", "was", "der", "Him\u00b7mel", "kennt", "/", "was", "in", "den", "L\u00fcff\u00b7ten", "schwebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "ART", "NN", "VVFIN", "$(", "PWS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was durch die Wasser geht/ was auff der Erden lebt", "tokens": ["Was", "durch", "die", "Was\u00b7ser", "geht", "/", "was", "auff", "der", "Er\u00b7den", "lebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "PWS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df alles was du sihst/ Herr Bruder/ mu\u00df verschwinden.", "tokens": ["Di\u00df", "al\u00b7les", "was", "du", "sihst", "/", "Herr", "Bru\u00b7der", "/", "mu\u00df", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PIS", "PWS", "PPER", "VVFIN", "$(", "NN", "NN", "$(", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch! Freundschafft pocht den Todt/ vnd trotzt die Ewigkeit.", "tokens": ["Doch", "!", "Freund\u00b7schafft", "pocht", "den", "Todt", "/", "vnd", "trotzt", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "NN", "VVFIN", "ART", "NN", "$(", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ist das st\u00e4rckste band/ Sie lacht/ in h\u00f6chstem leidt.", "tokens": ["Sie", "ist", "das", "st\u00e4rcks\u00b7te", "band", "/", "Sie", "lacht", "/", "in", "h\u00f6chs\u00b7tem", "leidt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd zwingt dlch selbst mein Freundt/ den drey nur k\u00f6nnen", "tokens": ["Vnd", "zwingt", "dlch", "selbst", "mein", "Freundt", "/", "den", "drey", "nur", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$(", "ART", "CARD", "ADV", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "binden.", "tokens": ["bin\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}