{"textgrid.poem.44403": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf Kresna-Hora, h\u00fctend seine K\u00fche,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Kresna-Hora, h\u00fctend seine K\u00fche,", "tokens": ["Auf", "Kres\u00b7na\u00b7Ho\u00b7ra", ",", "h\u00fc\u00b7tend", "sei\u00b7ne", "K\u00fc\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stand jener Hirt, da wollt es ihn bed\u00fcnken,", "tokens": ["Stand", "je\u00b7ner", "Hirt", ",", "da", "wollt", "es", "ihn", "be\u00b7d\u00fcn\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$,", "KOUS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er s\u00e4h es aus dem Erdreich guldig blinken,", "tokens": ["Er", "s\u00e4h", "es", "aus", "dem", "Er\u00b7dreich", "gul\u00b7dig", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im D\u00e4mmerlicht von Tages erster Fr\u00fche.", "tokens": ["Im", "D\u00e4m\u00b7mer\u00b7licht", "von", "Ta\u00b7ges", "ers\u00b7ter", "Fr\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit kurzem Atem eilt er hin und, siehe!", "tokens": ["Mit", "kur\u00b7zem", "A\u00b7tem", 