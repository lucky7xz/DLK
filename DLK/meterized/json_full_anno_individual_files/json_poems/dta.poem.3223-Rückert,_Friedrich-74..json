{"dta.poem.3223": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht minder als verstehn, will man verstanden seyn;", "tokens": ["Nicht", "min\u00b7der", "als", "ver\u00b7stehn", ",", "will", "man", "ver\u00b7stan\u00b7den", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "VVINF", "$,", "VMFIN", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie selten aber ist von beiden der Verein!", "tokens": ["Wie", "sel\u00b7ten", "a\u00b7ber", "ist", "von", "bei\u00b7den", "der", "Ver\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "VAFIN", "APPR", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch k\u00f6nnen zwei sich schon vertragen, die sich fanden,", "tokens": ["Doch", "k\u00f6n\u00b7nen", "zwei", "sich", "schon", "ver\u00b7tra\u00b7gen", ",", "die", "sich", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "CARD", "PRF", "ADV", "VVFIN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn von dem einen nur der andre wird verstanden.", "tokens": ["Wenn", "von", "dem", "ei\u00b7nen", "nur", "der", "and\u00b7re", "wird", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ART", "ADV", "ART", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der eine f\u00fchlt sich klug, den andern zu verstehn;", "tokens": ["Der", "ei\u00b7ne", "f\u00fchlt", "sich", "klug", ",", "den", "an\u00b7dern", "zu", "ver\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "ADJD", "$,", "PRELS", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem andern ist's genug, verstanden sich zu sehn.", "tokens": ["Dem", "an\u00b7dern", "ist's", "ge\u00b7nug", ",", "ver\u00b7stan\u00b7den", "sich", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "$,", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}