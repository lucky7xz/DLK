{"dta.poem.21098": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das Schicksal und das Ich.  \n ( Nach Jean Paul .)", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Schicksal.               ", "tokens": ["Das", "Schick\u00b7sal", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sohn des Staubes, vernimm des schwererweich-               ", "tokens": ["Sohn", "des", "Stau\u00b7bes", ",", "ver\u00b7nimm", "des", "schwe\u00b7rer\u00b7weich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "VVIMP", "ART", "TRUNC"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "lichen Schicksals", "tokens": ["li\u00b7chen", "Schick\u00b7sals"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Freundlichen Antrag: Dir ist eine der Bit-", "tokens": ["Freund\u00b7li\u00b7chen", "An\u00b7trag", ":", "Dir", "ist", "ei\u00b7ne", "der", "Bit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "ART", "TRUNC"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.5": {"text": "ten gew\u00e4hrt.", "tokens": ["ten", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "punct"], "pos": ["FM", "VVPP", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.6": {"text": "Mancherley Freuden vergolden der Menschen neb-", "tokens": ["Man\u00b7cher\u00b7ley", "Freu\u00b7den", "ver\u00b7gol\u00b7den", "der", "Men\u00b7schen", "ne\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "lichtes Leben.", "tokens": ["lich\u00b7tes", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "W\u00e4hl', und welche du w\u00e4hlst, sey dir vom", "tokens": ["W\u00e4hl'", ",", "und", "wel\u00b7che", "du", "w\u00e4hlst", ",", "sey", "dir", "vom"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KON", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "APPRART"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Schicksal gew\u00e4hrt!", "tokens": ["Schick\u00b7sal", "ge\u00b7w\u00e4hrt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}