{"dta.poem.5584": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und den die Jahre nicht bedeckten.", "tokens": ["Und", "den", "die", "Jah\u00b7re", "nicht", "be\u00b7deck\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will der Wahrheit S\u00fc\u00dfigkeit,", "tokens": ["Ich", "will", "der", "Wahr\u00b7heit", "S\u00fc\u00b7\u00dfig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jm holden Arm der Welt-Weisheit,", "tokens": ["Jm", "hol\u00b7den", "Arm", "der", "Welt\u00b7Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Beym lieblichen Gesang der klugen Schwestern schmecken.", "tokens": ["Beym", "lieb\u00b7li\u00b7chen", "Ge\u00b7sang", "der", "klu\u00b7gen", "Schwes\u00b7tern", "schme\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}