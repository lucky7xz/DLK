{"dta.poem.712": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "2. Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ach/ Erwecke meine Seele:", "tokens": ["Ach", "/", "Er\u00b7we\u00b7cke", "mei\u00b7ne", "See\u00b7le", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wende meinen vnverstand", "tokens": ["Wen\u00b7de", "mei\u00b7nen", "vn\u00b7ver\u00b7stand"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeige den/ den GOtt gesand.", "tokens": ["Zei\u00b7ge", "den", "/", "den", "Gott", "ge\u00b7sand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$(", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rei\u00df mich au\u00df der Jammer h\u00f6le/", "tokens": ["Rei\u00df", "mich", "au\u00df", "der", "Jam\u00b7mer", "h\u00f6\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In welcher mein gem\u00fctt verschlossen vnd verh\u00fcttet", "tokens": ["In", "wel\u00b7cher", "mein", "ge\u00b7m\u00fctt", "ver\u00b7schlos\u00b7sen", "vnd", "ver\u00b7h\u00fct\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PPOSAT", "ADJD", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd sonder ende zagt.", "tokens": ["Vnd", "son\u00b7der", "en\u00b7de", "zagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In der de\u00df ", "tokens": ["In", "der", "de\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Vnd mein ", "tokens": ["Vnd", "mein"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Ich zitter: Hilff mir den erbitten.", "tokens": ["Ich", "zit\u00b7ter", ":", "Hilff", "mir", "den", "er\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$.", "NN", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der seine Donner au\u00df wil sch\u00fctten:", "tokens": ["Der", "sei\u00b7ne", "Don\u00b7ner", "au\u00df", "wil", "sch\u00fct\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Ich k\u00e4mpffe: Hilff mir vberwinden:", "tokens": ["Ich", "k\u00e4mpf\u00b7fe", ":", "Hilff", "mir", "vber\u00b7win\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Ich jrre la\u00df den weg michfinden. ", "tokens": ["Ich", "jr\u00b7re", "la\u00df", "den", "weg", "mich\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVIMP", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}