{"dta.poem.13728": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Klage .  \n 1809.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "en:0.28"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da wird Aurora tagen", "tokens": ["Da", "wird", "Au\u00b7ro\u00b7ra", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hoch \u00fcber den Wald hinauf,", "tokens": ["Hoch", "\u00fc\u00b7ber", "den", "Wald", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da giebt's was zu singen und schlagen,", "tokens": ["Da", "giebt's", "was", "zu", "sin\u00b7gen", "und", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da wacht, ihr Getreuen, auf.", "tokens": ["Da", "wacht", ",", "ihr", "Ge\u00b7treu\u00b7en", ",", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}