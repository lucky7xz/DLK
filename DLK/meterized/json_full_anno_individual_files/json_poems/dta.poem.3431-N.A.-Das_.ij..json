{"dta.poem.3431": {"metadata": {"author": {"name": "N.A.", "birth": "N.A.", "death": "N.A."}, "title": "Das .ij.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1497", "urn": "urn:nbn:de:kobv:b4-30121-8", "language": ["de:0.99"], "booktitle": "Des Edlen Ritter Morgeners Walfart. Erfurt, 1497."}, "poem": {"stanza.1": {"line.1": {"text": "Er sprach hertz aller liebste fraw.", "tokens": ["Er", "sprach", "hertz", "al\u00b7ler", "liebs\u00b7te", "fraw", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "vernembt die rede mein f\u00fcr war", "tokens": ["ver\u00b7nembt", "die", "re\u00b7de", "mein", "f\u00fcr", "war"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "VVFIN", "PPOSAT", "APPR", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "aller eren ich e\u00fcch getraw", "tokens": ["al\u00b7ler", "e\u00b7ren", "ich", "e\u00b7\u00fcch", "ge\u00b7traw"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPER", "ADJD", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "w\u00f6lt ir mein peiten syben iar", "tokens": ["w\u00f6lt", "ir", "mein", "pei\u00b7ten", "sy\u00b7ben", "i\u00b7ar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "mir ist abentewr bekant", "tokens": ["mir", "ist", "a\u00b7ben\u00b7tewr", "be\u00b7kant"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "nun gebt mir vrlaub zarte fraw", "tokens": ["nun", "gebt", "mir", "vr\u00b7laub", "zar\u00b7te", "fraw"], "token_info": ["word", "word", "word", "word", "word