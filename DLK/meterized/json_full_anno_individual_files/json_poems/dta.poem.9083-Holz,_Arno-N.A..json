{"dta.poem.9083": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor meinem Fenster", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "singt ein Vogel.", "tokens": ["singt", "ein", "Vo\u00b7gel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Still h\u00f6r ich zu; mein Herz vergeht.", "tokens": ["Still", "h\u00f6r", "ich", "zu", ";", "mein", "Herz", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er singt,", "tokens": ["Er", "singt", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "was ich als Kind besass", "tokens": ["was", "ich", "als", "Kind", "be\u00b7sass"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "KOUS", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "und dann \u2014 vergessen.", "tokens": ["und", "dann", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$(", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}