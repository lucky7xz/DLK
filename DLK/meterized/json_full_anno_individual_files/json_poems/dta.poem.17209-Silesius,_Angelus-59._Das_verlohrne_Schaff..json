{"dta.poem.17209": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "59.  Das verlohrne Schaff.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin das arme Schaaff das sich verjrr et hat/", "tokens": ["Ich", "bin", "das", "ar\u00b7me", "Schaaff", "das", "sich", "ver\u00b7jrr", "et", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "PRF", "NE", "NE", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nunmehr von sich selbst nicht kennt den rechten", "tokens": ["Und", "nun\u00b7mehr", "von", "sich", "selbst", "nicht", "kennt", "den", "rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PRF", "ADV", "PTKNEG", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Pfad.", "tokens": ["Pfad", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Wer zeugt mir dann den Weg/ da\u00df ich nicht gantz erliege?", "tokens": ["Wer", "zeugt", "mir", "dann", "den", "Weg", "/", "da\u00df", "ich", "nicht", "gantz", "er\u00b7lie\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O da\u00df doch JEsus k\u00e4m\u2019/ und mich nach Hause tr\u00fcge!", "tokens": ["O", "da\u00df", "doch", "Je\u00b7sus", "k\u00e4m'", "/", "und", "mich", "nach", "Hau\u00b7se", "tr\u00fc\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ADV", "NE", "VVFIN", "$(", "KON", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}