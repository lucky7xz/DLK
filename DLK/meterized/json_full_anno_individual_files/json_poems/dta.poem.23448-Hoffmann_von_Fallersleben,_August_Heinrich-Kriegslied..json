{"dta.poem.23448": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kriegslied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt wie die Trommel schl\u00e4gt!", "tokens": ["H\u00f6rt", "wie", "die", "Trom\u00b7mel", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Seht wie das Volk sich regt!", "tokens": ["Seht", "wie", "das", "Volk", "sich", "regt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Fahne voran!", "tokens": ["Die", "Fah\u00b7ne", "vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wir folgen Mann f\u00fcr Mann.", "tokens": ["Wir", "fol\u00b7gen", "Mann", "f\u00fcr", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Kampf und Streit", "tokens": ["Im", "Kampf", "und", "Streit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ist keine Zeit", "tokens": ["Ist", "kei\u00b7ne", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zu fragen warum? warum? warum?", "tokens": ["Zu", "fra\u00b7gen", "wa\u00b7rum", "?", "wa\u00b7rum", "?", "wa\u00b7rum", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "PWAV", "$.", "PWAV", "$.", "PWAV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die Trommel die ruft wiederum pum pum pum pum", "tokens": ["Die", "Trom\u00b7mel", "die", "ruft", "wie\u00b7de\u00b7rum", "pum", "pum", "pum", "pum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit Gott f\u00fcr K\u00f6nig und Vaterland.", "tokens": ["Mit", "Gott", "f\u00fcr", "K\u00f6\u00b7nig", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}