{"textgrid.poem.52044": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die hinterm Pater knyn und neue Creutze schreiben,", "tokens": ["Die", "hin\u00b7term", "Pa\u00b7ter", "knyn", "und", "neu\u00b7e", "Creut\u00b7ze", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die hat nicht eine Sach erleuchtet und bekehrt,", "tokens": ["Die", "hat", "nicht", "ei\u00b7ne", "Sach", "er\u00b7leuch\u00b7tet", "und", "be\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der rufft ein Landgut an, und der es ihm gew\u00e4hrt,", "tokens": ["Der", "rufft", "ein", "Land\u00b7gut", "an", ",", "und", "der", "es", "ihm", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den mu\u00df er vor heraus mit frommen Finten treiben:", "tokens": ["Den", "mu\u00df", "er", "vor", "he\u00b7raus", "mit", "from\u00b7men", "Fin\u00b7ten", "trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "APPR", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+