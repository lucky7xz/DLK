{"dta.poem.18241": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Umspielt das schwanke Ried,", "tokens": ["Um\u00b7spielt", "das", "schwan\u00b7ke", "Ried", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Th\u00fcrmer bl\u00e4st sein Lied", "tokens": ["Der", "Th\u00fcr\u00b7mer", "bl\u00e4st", "sein", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ins Abendroth hinein.", "tokens": ["Ins", "A\u00b7ben\u00b7droth", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von fernher weht ein Duft", "tokens": ["Von", "fern\u00b7her", "weht", "ein", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Berauschend mir ums Haar,", "tokens": ["Be\u00b7rau\u00b7schend", "mir", "ums", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein wei\u00dfes Taubenpaar", "tokens": ["Ein", "wei\u00b7\u00dfes", "Tau\u00b7ben\u00b7paar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durchflattert noch die Luft.", "tokens": ["Durch\u00b7flat\u00b7tert", "noch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun taucht mein Geist ins Bad", "tokens": ["Nun", "taucht", "mein", "Geist", "ins", "Bad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und st\u00e4rkt sich im Gebet,", "tokens": ["Und", "st\u00e4rkt", "sich", "im", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ein Engel Gottes geht", "tokens": ["Ein", "En\u00b7gel", "Got\u00b7tes", "geht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Stillsegnend durch die Stadt.", "tokens": ["Still\u00b7seg\u00b7nend", "durch", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "F\u00fcr Jeden, der ihn sieht,", "tokens": ["F\u00fcr", "Je\u00b7den", ",", "der", "ihn", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hat er im Herzen Raum:", "tokens": ["Hat", "er", "im", "Her\u00b7zen", "Raum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir gab er einen Traum,", "tokens": ["Dir", "gab", "er", "ei\u00b7nen", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mir gab er dies Lied!", "tokens": ["Und", "mir", "gab", "er", "dies", "Lied", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PDS", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}