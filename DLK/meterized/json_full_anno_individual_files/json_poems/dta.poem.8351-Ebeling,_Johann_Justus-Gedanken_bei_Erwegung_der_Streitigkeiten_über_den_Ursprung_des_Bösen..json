{"dta.poem.8351": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken bei Erwegung der  \n Streitigkeiten \u00fcber den Ursprung  \n des B\u00f6sen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das B\u00f6se ist ein Gifft, dadurch\nder Mensch verderbt", "tokens": ["Das", "B\u00f6\u00b7se", "ist", "ein", "Gifft", ",", "da\u00b7durch", "der", "Mensch", "ver\u00b7derbt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einer Seuche gleich, die uns", "tokens": ["Und", "ei\u00b7ner", "Seu\u00b7che", "gleich", ",", "die", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Woher das Uebel sich in Geist", "tokens": ["Wo\u00b7her", "das", "Ue\u00b7bel", "sich", "in", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus welcher Quelle es, in un-", "tokens": ["Aus", "wel\u00b7cher", "Quel\u00b7le", "es", ",", "in", "un"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PWAT", "NN", "PPER", "$,", "APPR", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dar\u00fcber streitet man in der gelehrten Welt,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "strei\u00b7tet", "man", "in", "der", "ge\u00b7lehr\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die sich als wie in Krieg, Partheienweis gestellt;", "tokens": ["Die", "sich", "als", "wie", "in", "Krieg", ",", "Part\u00b7hei\u00b7en\u00b7weis", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PRF", "KOUS", "KOKOM", "APPR", "NN", "$,", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und dieser Federkrieg von Ursprung alles B\u00f6sen,", "tokens": ["Und", "die\u00b7ser", "Fe\u00b7der\u00b7krieg", "von", "Ur\u00b7sprung", "al\u00b7les", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wird darum nur gef\u00fchrt, die Knoten aufzul\u00f6sen,", "tokens": ["Wird", "da\u00b7rum", "nur", "ge\u00b7f\u00fchrt", ",", "die", "Kno\u00b7ten", "auf\u00b7zu\u00b7l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "ADV", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die scharffer Aberwiz aus Sp\u00f6tterei gekn\u00fcpft,", "tokens": ["Die", "scharf\u00b7fer", "A\u00b7ber\u00b7wiz", "aus", "Sp\u00f6t\u00b7te\u00b7rei", "ge\u00b7kn\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der wie ein Diestelfink von dies auf jenes h\u00fcpft.", "tokens": ["Der", "wie", "ein", "Dies\u00b7tel\u00b7fink", "von", "dies", "auf", "je\u00b7nes", "h\u00fcpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "APPR", "PDS", "APPR", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein wie th\u00f6rigt ists um solche Dinge streiten,", "tokens": ["Al\u00b7lein", "wie", "th\u00f6\u00b7rigt", "ists", "um", "sol\u00b7che", "Din\u00b7ge", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADJD", "VAFIN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die nicht aus der Vernunfft alleine herzuleiten?", "tokens": ["Die", "nicht", "aus", "der", "Ver\u00b7nunfft", "al\u00b7lei\u00b7ne", "her\u00b7zu\u00b7lei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir sind dem Kranken gleich, die matt und elend", "tokens": ["Wir", "sind", "dem", "Kran\u00b7ken", "gleich", ",", "die", "matt", "und", "e\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der Wille ist verkehrt, und der Verstand ist blind,", "tokens": ["Der", "Wil\u00b7le", "ist", "ver\u00b7kehrt", ",", "und", "der", "Ver\u00b7stand", "ist", "blind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das b\u00f6se S\u00fcnden-Gifft hat unser Fleisch durch-", "tokens": ["Das", "b\u00f6\u00b7se", "S\u00fcn\u00b7den\u00b7Gifft", "hat", "un\u00b7ser", "Fleisch", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und der Begierden Heer hat den Verstand bezwun-", "tokens": ["Und", "der", "Be\u00b7gier\u00b7den", "Heer", "hat", "den", "Ver\u00b7stand", "be\u00b7zwun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dies mu\u00df ein jeglicher, wer sich wird selbst besehn,", "tokens": ["Dies", "mu\u00df", "ein", "jeg\u00b7li\u00b7cher", ",", "wer", "sich", "wird", "selbst", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "PIAT", "$,", "PWS", "PRF", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als einen Saz der klar, unleugbar, eingestehn:", "tokens": ["Als", "ei\u00b7nen", "Saz", "der", "klar", ",", "un\u00b7leug\u00b7bar", ",", "ein\u00b7ge\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "ADJD", "$,", "ADJD", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es ist den Aerzten gleich, sie brauchens nicht zu", "tokens": ["Es", "ist", "den", "A\u00b7erz\u00b7ten", "gleich", ",", "sie", "brau\u00b7chens", "nicht", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "PTKZU"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Wenn einer der da krank, von einer Schlang ge-", "tokens": ["Wenn", "ei\u00b7ner", "der", "da", "krank", ",", "von", "ei\u00b7ner", "Schlang", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADV", "ADJD", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ob diese Schlagen-Art die jenes Leib entflammt,", "tokens": ["Ob", "die\u00b7se", "Schla\u00b7gen\u00b7Art", "die", "je\u00b7nes", "Leib", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ART", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Etwan aus Asien, aus Africa herstammt:", "tokens": ["Et\u00b7wan", "aus", "A\u00b7sien", ",", "aus", "A\u00b7fri\u00b7ca", "her\u00b7stammt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$,", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Wenn sie im Streit erhizt; so wird im Disputiren,", "tokens": ["Wenn", "sie", "im", "Streit", "er\u00b7hizt", ";", "so", "wird", "im", "Dis\u00b7pu\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$.", "ADV", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Kranke dr\u00fcber nur das Leben gar verliehren:", "tokens": ["Der", "Kran\u00b7ke", "dr\u00fc\u00b7ber", "nur", "das", "Le\u00b7ben", "gar", "ver\u00b7lieh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Viel besser aber ist, da\u00df sie darauf bedacht,", "tokens": ["Viel", "bes\u00b7ser", "a\u00b7ber", "ist", ",", "da\u00df", "sie", "da\u00b7rauf", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VAFIN", "$,", "KOUS", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wie dieses heisse Gifft werd aus dem Leib gebracht;", "tokens": ["Wie", "die\u00b7ses", "heis\u00b7se", "Gifft", "werd", "aus", "dem", "Leib", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und was der Kranke mu\u00df bei der Entz\u00fcndung", "tokens": ["Und", "was", "der", "Kran\u00b7ke", "mu\u00df", "bei", "der", "Ent\u00b7z\u00fcn\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "VMFIN", "APPR", "ART", "NN"], "meter": "++-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "Damit der gifftge Dunst ohn Schaden k\u00f6nn ver-", "tokens": ["Da\u00b7mit", "der", "gifft\u00b7ge", "Dunst", "ohn", "Scha\u00b7den", "k\u00f6nn", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "APPR", "NN", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Die Weisen dieser Welt sind denen Aerzten gleich,", "tokens": ["Die", "Wei\u00b7sen", "die\u00b7ser", "Welt", "sind", "de\u00b7nen", "A\u00b7erz\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "PDS", "NN", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "Aus eitler Wisbegier an vielen Fragen reich:", "tokens": ["Aus", "eit\u00b7ler", "Wis\u00b7be\u00b7gier", "an", "vie\u00b7len", "Fra\u00b7gen", "reich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "O! m\u00f6chten sie sich nur mit allen Ernst bem\u00fchen,", "tokens": ["O", "!", "m\u00f6ch\u00b7ten", "sie", "sich", "nur", "mit", "al\u00b7len", "Ernst", "be\u00b7m\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "PRF", "ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Um Mittel, dadurch wir der S\u00fcnden Qual ent-", "tokens": ["Um", "Mit\u00b7tel", ",", "da\u00b7durch", "wir", "der", "S\u00fcn\u00b7den", "Qual", "ent"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "$,", "PAV", "PPER", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "O! w\u00e4ren sie nur drauf mit allen Ernst bedacht,", "tokens": ["O", "!", "w\u00e4\u00b7ren", "sie", "nur", "drauf", "mit", "al\u00b7len", "Ernst", "be\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "PPER", "ADV", "PAV", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie was verdorben ist, w\u00fcrd wieder gut gemacht;", "tokens": ["Wie", "was", "ver\u00b7dor\u00b7ben", "ist", ",", "w\u00fcrd", "wie\u00b7der", "gut", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "VAFIN", "$,", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was hilft es nur allein die Dinge wissen wollen,", "tokens": ["Was", "hilft", "es", "nur", "al\u00b7lein", "die", "Din\u00b7ge", "wis\u00b7sen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die wir nach ", "tokens": ["Die", "wir", "nach"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.37": {"text": "So th\u00f6rigt ist der Wiz der armen Menschligkeit,", "tokens": ["So", "th\u00f6\u00b7rigt", "ist", "der", "Wiz", "der", "ar\u00b7men", "Mensc\u00b7hlig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die eitle Wisbegier erreget manchen Streit:", "tokens": ["Die", "eit\u00b7le", "Wis\u00b7be\u00b7gier", "er\u00b7re\u00b7get", "man\u00b7chen", "Streit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die sich ums Wissen blos, nicht um das Thun", "tokens": ["Die", "sich", "ums", "Wis\u00b7sen", "blos", ",", "nicht", "um", "das", "Thun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPRART", "NN", "ADV", "$,", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Die bessern warlich nicht, sie wollen nur verschlim-", "tokens": ["Die", "bes\u00b7sern", "war\u00b7lich", "nicht", ",", "sie", "wol\u00b7len", "nur", "ver\u00b7schlim"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "PTKNEG", "$,", "PPER", "VMFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}