{"dta.poem.19579": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das Joch am Leman.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edie Einen liegen todt mit ihren Wunden,", "tokens": ["\u201e", "die", "Ei\u00b7nen", "lie\u00b7gen", "todt", "mit", "ih\u00b7ren", "Wun\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Andern treiben wir daher gebunden \u2014", "tokens": ["Die", "An\u00b7dern", "trei\u00b7ben", "wir", "da\u00b7her", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PAV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den R\u00f6meraar der Zwillingslegion,", "tokens": ["Den", "R\u00f6\u00b7me\u00b7raar", "der", "Zwil\u00b7lings\u00b7le\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der eingegarnten W\u00f6lfin scharfen Bissen", "tokens": ["Der", "ein\u00b7ge\u00b7garn\u00b7ten", "W\u00f6l\u00b7fin", "schar\u00b7fen", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im M\u00e4nnerkampf, im Ro\u00dfgestampf entrissen,", "tokens": ["Im", "M\u00e4n\u00b7ner\u00b7kampf", ",", "im", "Ro\u00df\u00b7ge\u00b7stampf", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schwingt Divico, der Berge Sohn!\u201c", "tokens": ["Schwingt", "Di\u00b7vi\u00b7co", ",", "der", "Ber\u00b7ge", "Sohn", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NE", "$,", "ART", "NN", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Weit blaut die Seeflut. Scheltend jagen Treiber", "tokens": ["Weit", "blaut", "die", "See\u00b7flut", ".", "Schel\u00b7tend", "ja\u00b7gen", "Trei\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Ufer einen Haufen Menschenleiber,", "tokens": ["Am", "U\u00b7fer", "ei\u00b7nen", "Hau\u00b7fen", "Men\u00b7schen\u00b7lei\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die nackte Schmach umjauchzt Triumphgesang,", "tokens": ["Die", "nack\u00b7te", "Schmach", "um\u00b7jauchzt", "Tri\u00b7umph\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein J\u00fcngling kreist auf einem falben Pferde", "tokens": ["Ein", "J\u00fcng\u00b7ling", "kreist", "auf", "ei\u00b7nem", "fal\u00b7ben", "Pfer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um die zu Zwei'n gepaarte R\u00f6merheerde", "tokens": ["Um", "die", "zu", "Zwei'n", "ge\u00b7paar\u00b7te", "R\u00f6\u00b7mer\u00b7heer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Kr\u00fcmmen des Gestads entlang.", "tokens": ["Die", "Kr\u00fcm\u00b7men", "des", "Ge\u00b7stads", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er knickt den Aar mit einem stolzen Schreie,", "tokens": ["Er", "knickt", "den", "Aar", "mit", "ei\u00b7nem", "stol\u00b7zen", "Schrei\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er schickt den Ruf zur nahen Firnenreihe \u2014", "tokens": ["Er", "schickt", "den", "Ruf", "zur", "na\u00b7hen", "Fir\u00b7nen\u00b7rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gr\u00e4t' und W\u00e4nde blicken gro\u00df und bleich \u2014", "tokens": ["Die", "Gr\u00e4t'", "und", "W\u00e4n\u00b7de", "bli\u00b7cken", "gro\u00df", "und", "bleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u201ehebt, Ahnen, euch vom Silbersitz, zu schauen", "tokens": ["\u201e", "hebt", ",", "Ah\u00b7nen", ",", "euch", "vom", "Sil\u00b7ber\u00b7sitz", ",", "zu", "schau\u00b7en"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "PPER", "APPRART", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Pforte, die wir f\u00fcr den R\u00e4uber bauen,", "tokens": ["Die", "Pfor\u00b7te", ",", "die", "wir", "f\u00fcr", "den", "R\u00e4u\u00b7ber", "bau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der sich verstieg in euer Reich!", "tokens": ["Der", "sich", "ver\u00b7stieg", "in", "eu\u00b7er", "Reich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir bauen nicht mit M\u00f6rtel noch mit Steinen,", "tokens": ["Wir", "bau\u00b7en", "nicht", "mit", "M\u00f6r\u00b7tel", "noch", "mit", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zwei Speere pflanzt! Quer\u00fcber bindet einen!", "tokens": ["Zwei", "Spee\u00b7re", "pflanzt", "!", "Quer\u00b7\u00fc\u00b7ber", "bin\u00b7det", "ei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$.", "NN", "VVFIN", "ART", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zwei R\u00f6merk\u00f6pfe drauf! Es ist gethan!\u201c \u2014", "tokens": ["Zwei", "R\u00f6\u00b7mer\u00b7k\u00f6p\u00b7fe", "drauf", "!", "Es", "ist", "ge\u00b7than", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["CARD", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Joch umstehn verwogne Kriegsgesellen", "tokens": ["Das", "Joch", "um\u00b7stehn", "ver\u00b7wog\u00b7ne", "Kriegs\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit Auerh\u00f6rnern und mit B\u00e4renfellen", "tokens": ["Mit", "Au\u00b7er\u00b7h\u00f6r\u00b7nern", "und", "mit", "B\u00e4\u00b7ren\u00b7fel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schauen sich das Bauwerk an!", "tokens": ["Und", "schau\u00b7en", "sich", "das", "Bau\u00b7werk", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die H\u00f6rner dr\u00f6hnen. Zu der blut'gen Pforte", "tokens": ["Die", "H\u00f6r\u00b7ner", "dr\u00f6h\u00b7nen", ".", "Zu", "der", "blut'\u00b7gen", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Str\u00f6mt her das Volk aus jedem Thal und Orte,", "tokens": ["Str\u00f6mt", "her", "das", "Volk", "aus", "je\u00b7dem", "Thal", "und", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "ART", "NN", "APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gro\u00df wundert sich am Joch die Kinderschaar,", "tokens": ["Gro\u00df", "wun\u00b7dert", "sich", "am", "Joch", "die", "Kin\u00b7der\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein M\u00e4delreigen springt in heller Freude", "tokens": ["Ein", "M\u00e4\u00b7del\u00b7rei\u00b7gen", "springt", "in", "hel\u00b7ler", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Um das von Schande triefende Geb\u00e4ude,", "tokens": ["Um", "das", "von", "Schan\u00b7de", "trie\u00b7fen\u00b7de", "Ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Den bl\u00fch'nden Veilchenkranz im Haar.", "tokens": ["Den", "bl\u00fch'n\u00b7den", "Veil\u00b7chen\u00b7kranz", "im", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Manlierstirn verzogne Brauen grollen,", "tokens": ["Der", "Man\u00b7lier\u00b7stirn", "ver\u00b7zog\u00b7ne", "Brau\u00b7en", "grol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Claudierkopfs erhitzte Augen rollen \u2014", "tokens": ["Des", "Clau\u00b7di\u00b7er\u00b7kopfs", "er\u00b7hitz\u00b7te", "Au\u00b7gen", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Hirtenknabe gei\u00dfelt wie ein Rind", "tokens": ["Der", "Hir\u00b7ten\u00b7kna\u00b7be", "gei\u00b7\u00dfelt", "wie", "ein", "Rind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Brutusenkel. Sich durchs Joch zu b\u00fccken,", "tokens": ["Den", "Bru\u00b7tu\u00b7sen\u00b7kel", ".", "Sich", "durchs", "Joch", "zu", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kr\u00fcmmt jetzt das erste R\u00f6merpaar den R\u00fccken", "tokens": ["Kr\u00fcmmt", "jetzt", "das", "ers\u00b7te", "R\u00f6\u00b7mer\u00b7paar", "den", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und gellend lacht das Alpenkind.", "tokens": ["Und", "gel\u00b7lend", "lacht", "das", "Al\u00b7pen\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit starren Z\u00fcgen blickt, als ob er spotte,", "tokens": ["Mit", "star\u00b7ren", "Z\u00fc\u00b7gen", "blickt", ",", "als", "ob", "er", "spot\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Felsenblock, der eigen ist dem Gotte,", "tokens": ["Ein", "Fel\u00b7sen\u00b7block", ",", "der", "ei\u00b7gen", "ist", "dem", "Got\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drauf hoch des Landes Priesterinnen stehn:", "tokens": ["Drauf", "hoch", "des", "Lan\u00b7des", "Pries\u00b7te\u00b7rin\u00b7nen", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein hell Gesch\u00f6pf in sonnenlichten Flechten", "tokens": ["Ein", "hell", "Ge\u00b7sch\u00f6pf", "in", "son\u00b7nen\u00b7lich\u00b7ten", "Flech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und eine Drude mit geballter Rechten", "tokens": ["Und", "ei\u00b7ne", "Dru\u00b7de", "mit", "ge\u00b7ball\u00b7ter", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und rabenschwarzer Haare Wehn.", "tokens": ["Und", "ra\u00b7ben\u00b7schwar\u00b7zer", "Haa\u00b7re", "Wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Dunkle h\u00f6hnt: \u201eGeht, R\u00f6mer! Schneidet Stecken!", "tokens": ["Die", "Dunk\u00b7le", "h\u00f6hnt", ":", "\u201e", "Geht", ",", "R\u00f6\u00b7mer", "!", "Schnei\u00b7det", "Ste\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "VVFIN", "$,", "NN", "$.", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir r\u00fcsten euch zur Fahrt mit Bettels\u00e4cken!", "tokens": ["Wir", "r\u00fcs\u00b7ten", "euch", "zur", "Fahrt", "mit", "Bet\u00b7tel\u00b7s\u00e4\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Euch peitsch' ein wildes Wetter durch die Schlucht!", "tokens": ["Euch", "peit\u00b7sch'", "ein", "wil\u00b7des", "Wet\u00b7ter", "durch", "die", "Schlucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Verflucht der Steg, dar\u00fcber ihr gekommen,", "tokens": ["Ver\u00b7flucht", "der", "Steg", ",", "da\u00b7r\u00fc\u00b7ber", "ihr", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wen ihr euch zum F\u00fchrer habt genommen,", "tokens": ["Und", "wen", "ihr", "euch", "zum", "F\u00fch\u00b7rer", "habt", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PRF", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er sei am ganzen Leib verflucht!\u201c", "tokens": ["Er", "sei", "am", "gan\u00b7zen", "Leib", "ver\u00b7flucht", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}