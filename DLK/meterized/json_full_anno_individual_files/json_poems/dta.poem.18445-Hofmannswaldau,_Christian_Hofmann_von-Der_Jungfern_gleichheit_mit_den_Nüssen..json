{"dta.poem.18445": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Jungfern gleichheit mit den  \n N\u00fcssen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die jungfern sind bey uns den N\u00fcssen zu vergleichen/", "tokens": ["Die", "jung\u00b7fern", "sind", "bey", "uns", "den", "N\u00fcs\u00b7sen", "zu", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Man kennt sie/ sind sie reiff/ an p\u00fcschen und gestr\u00e4uchen.", "tokens": ["Man", "kennt", "sie", "/", "sind", "sie", "reiff", "/", "an", "p\u00fc\u00b7schen", "und", "ge\u00b7str\u00e4u\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$(", "VAFIN", "PPER", "VVFIN", "$(", "APPR", "ADJA", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann sich die schale f\u00e4rbt/ so ist sie rnnd und voll/", "tokens": ["Wann", "sich", "die", "scha\u00b7le", "f\u00e4rbt", "/", "so", "ist", "sie", "rnnd", "und", "voll", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "ADJA", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ist mit beyden zeit/ da\u00df man sie brechen soll.", "tokens": ["Und", "ist", "mit", "bey\u00b7den", "zeit", "/", "da\u00df", "man", "sie", "bre\u00b7chen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "$(", "KOUS", "PIS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}