{"textgrid.poem.52607": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun hab ichs endlich \u00fcberstanden,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun hab ichs endlich \u00fcberstanden,", "tokens": ["Nun", "hab", "ichs", "end\u00b7lich", "\u00fc\u00b7bers\u00b7tan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich f\u00fchle mich so frei und froh,", "tokens": ["Ich", "f\u00fch\u00b7le", "mich", "so", "frei", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil ich aus den verruchten Banden", "tokens": ["Weil", "ich", "aus", "den", "ver\u00b7ruch\u00b7ten", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einer k\u00fchnen That entfloh;", "tokens": ["Mit", "ei\u00b7ner", "k\u00fch\u00b7nen", "That", "ent\u00b7floh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hab auf ewig sie verschworen", "tokens": ["Ich", "hab", "auf", "e\u00b7wig", "sie", "ver\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJD", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die schm\u00e4hliche Vergangenheit.", "tokens": ["Die", "schm\u00e4h\u