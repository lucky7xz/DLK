{"dta.poem.9193": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Xi.  \n Er nimmt Abschied.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Du liebes kind/ indem ich von dir reise/", "tokens": ["Du", "lie\u00b7bes", "kind", "/", "in\u00b7dem", "ich", "von", "dir", "rei\u00b7se", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und deiner gunst zu guter letzt erweise/", "tokens": ["Und", "dei\u00b7ner", "gunst", "zu", "gu\u00b7ter", "letzt", "er\u00b7wei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ADJA", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ich bi\u00dfher mit grosser lust gethan;", "tokens": ["Was", "ich", "bi\u00df\u00b7her", "mit", "gros\u00b7ser", "lust", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So nimm di\u00df lied mit lieben h\u00e4nden an.", "tokens": ["So", "nimm", "di\u00df", "lied", "mit", "lie\u00b7ben", "h\u00e4n\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PDS", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "2. Mein kind/ es ist vielleicht mein besser gl\u00fccke/", "tokens": ["Mein", "kind", "/", "es", "ist", "viel\u00b7leicht", "mein", "bes\u00b7ser", "gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VAFIN", "ADV", "PPOSAT", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wofern ich bald zu dieser reise schicke/", "tokens": ["Wo\u00b7fern", "ich", "bald", "zu", "die\u00b7ser", "rei\u00b7se", "schi\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Absonderlich/ weil hier in deiner stadt", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "/", "weil", "hier", "in", "dei\u00b7ner", "stadt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein armer sinn kein ewig bleiben hat.", "tokens": ["Mein", "ar\u00b7mer", "sinn", "kein", "e\u00b7wig", "blei\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PIAT", "ADJD", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "3. Indessen werd ich offt zur\u00fccke dencken/", "tokens": ["In\u00b7des\u00b7sen", "werd", "ich", "offt", "zu\u00b7r\u00fc\u00b7cke", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mich damit in meinem hertzen kr\u00e4ncken/", "tokens": ["Und", "mich", "da\u00b7mit", "in", "mei\u00b7nem", "hert\u00b7zen", "kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PAV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich nunmehr den s\u00fcssen \u00fcberflu\u00df", "tokens": ["Da\u00df", "ich", "nun\u00b7mehr", "den", "s\u00fcs\u00b7sen", "\u00fc\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der sch\u00f6nsten lust mit dir entrathen mu\u00df.", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "lust", "mit", "dir", "ent\u00b7ra\u00b7then", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "4. Du bist allein mein augen-trost/", "tokens": ["Du", "bist", "al\u00b7lein", "mein", "au\u00b7gen\u00b7trost", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nachdem ich dich bedachtsam auserlesen/", "tokens": ["Nach\u00b7dem", "ich", "dich", "be\u00b7dacht\u00b7sam", "au\u00b7ser\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00e4tt ich nicht so offt auff dich gesehn/", "tokens": ["Und", "h\u00e4tt", "ich", "nicht", "so", "offt", "auff", "dich", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So w\u00e4r auch wol mein abschied eh geschehn.", "tokens": ["So", "w\u00e4r", "auch", "wol", "mein", "ab\u00b7schied", "eh", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PPOSAT", "VVFIN", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "5. Ich habe zwar die liebe stadt gepriesen/", "tokens": ["Ich", "ha\u00b7be", "zwar", "die", "lie\u00b7be", "stadt", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch hab ich dir das lob allein erwiesen/", "tokens": ["Doch", "hab", "ich", "dir", "das", "lob", "al\u00b7lein", "er\u00b7wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du warst die lust/ die kurtzweil und das spiel/", "tokens": ["Du", "warst", "die", "lust", "/", "die", "kurt\u00b7zweil", "und", "das", "spiel", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "NN", "KON", "PDS", "VVFIN", "$("], "meter": "-+-+--+-++", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das mir so wohl bey dieser stadt gefiel.", "tokens": ["Das", "mir", "so", "wohl", "bey", "die\u00b7ser", "stadt", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "6. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "So kan ich nichts/ als da\u00df ich mich bedancke/", "tokens": ["So", "kan", "ich", "nichts", "/", "als", "da\u00df", "ich", "mich", "be\u00b7dan\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "$(", "KOKOM", "KOUS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor deine gunst und alle freundligkeit/", "tokens": ["Vor", "dei\u00b7ne", "gunst", "und", "al\u00b7le", "freund\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Damit dein haus mich itzo noch erfreut.", "tokens": ["Da\u00b7mit", "dein", "haus", "mich", "it\u00b7zo", "noch", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "7. Nun gute nacht/ mein kind/ zu tausend mahlen/", "tokens": ["Nun", "gu\u00b7te", "nacht", "/", "mein", "kind", "/", "zu", "tau\u00b7send", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$(", "PPOSAT", "NN", "$(", "PTKA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich kan nicht viel mit grossen worten pralen/", "tokens": ["Ich", "kan", "nicht", "viel", "mit", "gros\u00b7sen", "wor\u00b7ten", "pra\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gefall ich dir/ nun so gedencke mein/", "tokens": ["Ge\u00b7fall", "ich", "dir", "/", "nun", "so", "ge\u00b7den\u00b7cke", "mein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$(", "ADV", "ADV", "VVFIN", "PPOSAT", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo nicht/ so la\u00df mich bald vergessen seyn.", "tokens": ["Wo", "nicht", "/", "so", "la\u00df", "mich", "bald", "ver\u00b7ges\u00b7sen", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$(", "ADV", "VVIMP", "PPER", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "8. Der abschied ist vor dieses mahl genommen/", "tokens": ["Der", "ab\u00b7schied", "ist", "vor", "die\u00b7ses", "mahl", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PDAT", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beliebt es dir/ so will ich wiederkommen/", "tokens": ["Be\u00b7liebt", "es", "dir", "/", "so", "will", "ich", "wie\u00b7der\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So bald die zeit mir einen kleinen rest", "tokens": ["So", "bald", "die", "zeit", "mir", "ei\u00b7nen", "klei\u00b7nen", "rest"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An meine lust zu wenden \u00fcbrig l\u00e4st.", "tokens": ["An", "mei\u00b7ne", "lust", "zu", "wen\u00b7den", "\u00fcb\u00b7rig", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}