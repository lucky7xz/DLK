{"textgrid.poem.47035": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich dir k\u00f6nnte, wie ich m\u00f6chte, geben", "tokens": ["Wenn", "ich", "dir", "k\u00f6nn\u00b7te", ",", "wie", "ich", "m\u00f6ch\u00b7te", ",", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "$,", "PWAV", "PPER", "VMFIN", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sch\u00e4tz' aus meiner Liebe vollem Schreine,", "tokens": ["Die", "Sch\u00e4tz'", "aus", "mei\u00b7ner", "Lie\u00b7be", "vol\u00b7lem", "Schrei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So w\u00e4r' auf Erden und im Himmel keine", "tokens": ["So", "w\u00e4r'", "auf", "Er\u00b7den", "und", "im", "Him\u00b7mel", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "KON", "APPRART", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geschm\u00fcckt wie du, o du mein s\u00fc\u00dfes Leben!", "tokens": ["Ge\u00b7schm\u00fcckt", "wie", "du", ",", "o", "du", "mein", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "PPER", "$,", "FM", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbwie war das?\u00ab H\u00f6r' es recht, mein s\u00fc\u00dfes Leben!", "tokens": ["\u00bb", "wie", "war", "das", "?", "\u00ab", "H\u00f6r'", "es", "recht", ",", "mein", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VAFIN", "PDS", "$.", "$(", "VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschm\u00fcckt in Erd' und Himmel w\u00e4re keine", "tokens": ["Ge\u00b7schm\u00fcckt", "in", "Erd'", "und", "Him\u00b7mel", "w\u00e4\u00b7re", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "VAFIN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie du, wenn dir aus meiner Liebe Schreine", "tokens": ["Wie", "du", ",", "wenn", "dir", "aus", "mei\u00b7ner", "Lie\u00b7be", "Schrei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sch\u00e4tz' ich, wie ich m\u00f6chte, k\u00f6nnte geben.", "tokens": ["Die", "Sch\u00e4tz'", "ich", ",", "wie", "ich", "m\u00f6ch\u00b7te", ",", "k\u00f6nn\u00b7te", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geschm\u00fcckt w\u00e4rst du mit mehr als K\u00f6nigsglanze,", "tokens": ["Ge\u00b7schm\u00fcckt", "w\u00e4rst", "du", "mit", "mehr", "als", "K\u00f6\u00b7nigs\u00b7glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPR", "PIAT", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn du sch\u00f6ner dann zu prangen w\u00e4hntest,", "tokens": ["Und", "wenn", "du", "sch\u00f6\u00b7ner", "dann", "zu", "pran\u00b7gen", "w\u00e4hn\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "W\u00fcrdest du sch\u00f6ner doch als jetzt nicht prangen. \u2013", "tokens": ["W\u00fcr\u00b7dest", "du", "sch\u00f6\u00b7ner", "doch", "als", "jetzt", "nicht", "pran\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "ADV", "KOKOM", "ADV", "PTKNEG", "VVFIN", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Das ward gesprochen abends unterm Tanze,", "tokens": ["Das", "ward", "ge\u00b7spro\u00b7chen", "a\u00b7bends", "un\u00b7term", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als du, nicht tanzend, sanft dich an mich lehntest,", "tokens": ["Als", "du", ",", "nicht", "tan\u00b7zend", ",", "sanft", "dich", "an", "mich", "lehn\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PTKNEG", "ADJD", "$,", "VVFIN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und littest, da\u00df mein Arm dich hielt umfangen.", "tokens": ["Und", "lit\u00b7test", ",", "da\u00df", "mein", "Arm", "dich", "hielt", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "wor