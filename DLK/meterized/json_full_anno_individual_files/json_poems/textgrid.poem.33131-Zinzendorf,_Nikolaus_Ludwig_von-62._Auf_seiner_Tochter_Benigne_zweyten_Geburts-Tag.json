{"textgrid.poem.33131": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "62. Auf seiner Tochter Benigne zweyten Geburts-Tag", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jesulein, man hat gelesen", "tokens": ["Je\u00b7su\u00b7lein", ",", "man", "hat", "ge\u00b7le\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIS", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df Du auch ein Kind gewesen,", "tokens": ["Da\u00df", "Du", "auch", "ein", "Kind", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und da\u00df wir durch Dich genesen,", "tokens": ["Und", "da\u00df", "wir", "durch", "Dich", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weil wir gar verdorben sind.", "tokens": ["Weil", "wir", "gar", "ver\u00b7dor\u00b7ben", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und darnach so steht geschrieben,", "tokens": ["Und", "dar\u00b7nach", "so", "steht", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ADV", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df Du sollst die Kinder lieben,", "tokens": ["Da\u00df", "Du", "sollst", "die", "Kin\u00b7der", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es immer sehr getrieben,", "tokens": ["Und", "es", "im\u00b7mer", "sehr", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df man Dir sie bringen soll.", "tokens": ["Da\u00df", "man", "Dir", "sie", "brin\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Heute sind unschuldge Kindlein,", "tokens": ["Heu\u00b7te", "sind", "un\u00b7schuld\u00b7ge", "Kin\u00b7dlein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Gestern sah man dich in Windlein,", "tokens": ["Ge\u00b7stern", "sah", "man", "dich", "in", "Wind\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PRF", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jesu bind in dieses B\u00fcndlein", "tokens": ["Je\u00b7su", "bind", "in", "die\u00b7ses", "B\u00fcnd\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Benigne Seele ein.", "tokens": ["Der", "Be\u00b7nig\u00b7ne", "See\u00b7le", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Mache Du sie Dir zum Lamme,", "tokens": ["Ma\u00b7che", "Du", "sie", "Dir", "zum", "Lam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gew\u00f6hn zum Creutzes-Stamme", "tokens": ["Und", "ge\u00b7w\u00f6hn", "zum", "Creut\u00b7zes\u00b7Stam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr dem Seelen-Br\u00e4utigamme", "tokens": ["Ihr", "dem", "See\u00b7len\u00b7Br\u00e4u\u00b7ti\u00b7gam\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohnedem geweyhtes Herz.", "tokens": ["Oh\u00b7ne\u00b7dem", "ge\u00b7weyh\u00b7tes", "Herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Weil du ja die Eltern liebest,", "tokens": ["Weil", "du", "ja", "die", "El\u00b7tern", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auf ihr Gebet was giebest,", "tokens": ["Und", "auf", "ihr", "Ge\u00b7bet", "was", "gie\u00b7best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PWS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und sie nicht mit Lust betr\u00fcbest;", "tokens": ["Und", "sie", "nicht", "mit", "Lust", "be\u00b7tr\u00fc\u00b7best", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So beleb auch dieses Kind.", "tokens": ["So", "be\u00b7leb", "auch", "die\u00b7ses", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Diesem L\u00e4mmlein von der Heerde,", "tokens": ["Die\u00b7sem", "L\u00e4mm\u00b7lein", "von", "der", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Du weidest auf der Erde,", "tokens": ["Die", "Du", "wei\u00b7dest", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gib, da\u00df es gehorsam werde,", "tokens": ["Gib", ",", "da\u00df", "es", "ge\u00b7hor\u00b7sam", "wer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Dir v\u00f6llig angenehm.", "tokens": ["Und", "Dir", "v\u00f6l\u00b7lig", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lehre dieses Kindes Eltern,", "tokens": ["Leh\u00b7re", "die\u00b7ses", "Kin\u00b7des", "El\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Deines Creutzes Zeltern,", "tokens": ["Un\u00b7ter", "Dei\u00b7nes", "Creut\u00b7zes", "Zel\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihren Eigenwillen keltern", "tokens": ["Ih\u00b7ren", "Ei\u00b7gen\u00b7wil\u00b7len", "kel\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Tochter Eigensinn.", "tokens": ["Und", "der", "Toch\u00b7ter", "Ei\u00b7gen\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wasche sie in Deinem Blute,", "tokens": ["Wa\u00b7sche", "sie", "in", "Dei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halt ihr mancherley zu gute,", "tokens": ["Halt", "ihr", "man\u00b7cher\u00b7ley", "zu", "gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIS", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das aus einem schwachen Muthe", "tokens": ["Das", "aus", "ei\u00b7nem", "schwa\u00b7chen", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und aus keiner Bosheit k\u00f6mt.", "tokens": ["Und", "aus", "kei\u00b7ner", "Bos\u00b7heit", "k\u00f6mt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wo Du ihr wilst Arbeit geben,", "tokens": ["Wo", "Du", "ihr", "wilst", "Ar\u00b7beit", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jesulein, so la\u00df sie leben;", "tokens": ["Je\u00b7su\u00b7lein", ",", "so", "la\u00df", "sie", "le\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonst kanst Du sie bald erheben", "tokens": ["Sonst", "kanst", "Du", "sie", "bald", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In das Reich der Kinderlein.", "tokens": ["In", "das", "Reich", "der", "Kin\u00b7der\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "K\u00f6nig aller K\u00f6nigreiche,", "tokens": ["K\u00f6\u00b7nig", "al\u00b7ler", "K\u00f6\u00b7nig\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Du bist dem Vater gleiche,", "tokens": ["Der", "Du", "bist", "dem", "Va\u00b7ter", "glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gib, da\u00df dieses Kind erreiche", "tokens": ["Gib", ",", "da\u00df", "die\u00b7ses", "Kind", "er\u00b7rei\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PDAT", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die geliebte neue Stadt.", "tokens": ["Die", "ge\u00b7lieb\u00b7te", "neu\u00b7e", "Stadt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "La\u00df doch auch uns andre Kleine", "tokens": ["La\u00df", "doch", "auch", "uns", "and\u00b7re", "Klei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Lammes Blute reine,", "tokens": ["In", "des", "Lam\u00b7mes", "Blu\u00b7te", "rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bey Deines Lichtes Scheine,", "tokens": ["Und", "bey", "Dei\u00b7nes", "Lich\u00b7tes", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Eine Weile fr\u00f6lich seyn.", "tokens": ["Ei\u00b7ne", "Wei\u00b7le", "fr\u00f6\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Schenk uns lauter Kinder-Freuden,", "tokens": ["Schenk", "uns", "lau\u00b7ter", "Kin\u00b7der\u00b7Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df uns wie die Kinder leiden,", "tokens": ["La\u00df", "uns", "wie", "die"