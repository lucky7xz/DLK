{"textgrid.poem.59854": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00e4nger gibt es, die ewig flennen,", "tokens": ["S\u00e4n\u00b7ger", "gibt", "es", ",", "die", "e\u00b7wig", "flen\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "In erk\u00fcnsteltem Gram sich strecken,", "tokens": ["In", "er\u00b7k\u00fcns\u00b7tel\u00b7tem", "Gram", "sich", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wimmern, als ob sie st\u00fcrben vor Schmerzen,", "tokens": ["Wim\u00b7mern", ",", "als", "ob", "sie", "st\u00fcr\u00b7ben", "vor", "Schmer\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ewig in falschen Gef\u00fchlen entbrennen,", "tokens": ["E\u00b7wig", "in", "fal\u00b7schen", "Ge\u00b7f\u00fch\u00b7len", "ent\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Weil sie das rechte Gef\u00fchl nicht kennen", "tokens": ["Weil", "sie", "das", "rech\u00b7te", "Ge\u00b7f\u00fchl", "nicht", "ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und darum auch in anderer Herzen", "tokens": ["Und", "da\u00b7rum", "auch", "in", "an\u00b7de\u00b7rer", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Keine rechten Gef\u00fchle wecken.", "tokens": ["Kei\u00b7ne", "rech\u00b7ten", "Ge\u00b7f\u00fch\u00b7le", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "H\u00fcte dich vor solcher schwindelnden Richtung,", "tokens": ["H\u00fc\u00b7te", "dich", "vor", "sol\u00b7cher", "schwin\u00b7deln\u00b7den", "Rich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Vor des Geschmacks und Verstandes Vernichtung!", "tokens": ["Vor", "des", "Ge\u00b7schmacks", "und", "Ver\u00b7stan\u00b7des", "Ver\u00b7nich\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "Frisch und ureigen", "tokens": ["Frisch", "und", "ur\u00b7ei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Mu\u00dft du dich zeigen,", "tokens": ["Mu\u00dft", "du", "dich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Wie im Gef\u00fchle, so in 