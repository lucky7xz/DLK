{"dta.poem.2852": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum verehrst du den? Weil ihn soviel verehren.", "tokens": ["Wa\u00b7rum", "ver\u00b7ehrst", "du", "den", "?", "Weil", "ihn", "so\u00b7viel", "ver\u00b7eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Beispiel ists wodurch einander Thoren lehren.", "tokens": ["Das", "Bei\u00b7spiel", "ists", "wo\u00b7durch", "ein\u00b7an\u00b7der", "Tho\u00b7ren", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PWAV", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hier ehrt dich einer erst, und dort ein andrer dann,", "tokens": ["Hier", "ehrt", "dich", "ei\u00b7ner", "erst", ",", "und", "dort", "ein", "an\u00b7drer", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADV", "$,", "KON", "ADV", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und endlich bist du ein verehrungsw\u00fcrdiger Mann.", "tokens": ["Und", "end\u00b7lich", "bist", "du", "ein", "ver\u00b7eh\u00b7rungs\u00b7w\u00fcr\u00b7di\u00b7ger", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.3": {"line.1": {"text": "Warum? wei\u00df keiner zwar, doch jeder glaubt gewis,", "tokens": ["Wa\u00b7rum", "?", "wei\u00df", "kei\u00b7ner", "zwar", ",", "doch", "je\u00b7der", "glaubt", "ge\u00b7wis", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PIS", "ADV", "$,", "ADV", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der andre wiss' es schon, und ihm gen\u00fcge dis.", "tokens": ["Der", "and\u00b7re", "wiss'", "es", "schon", ",", "und", "ihm", "ge\u00b7n\u00fc\u00b7ge", "dis", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "PDS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}