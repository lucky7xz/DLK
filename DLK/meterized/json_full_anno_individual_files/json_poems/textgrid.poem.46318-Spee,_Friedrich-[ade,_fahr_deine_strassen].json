 "measure": "iambic.tri"}, "line.3": {"text": "Ist ja doch nichts auff erden,", "tokens": ["Ist", "ja", "doch", "nichts", "auff", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da drinn man friedlich ruh.", "tokens": ["Da", "drinn", "man", "fried\u00b7lich", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O wan, wan wird erscheinen", "tokens": ["O", "wan", ",", "wan", "wird", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PWAV", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der villgewunschte tag,", "tokens": ["Der", "vill\u00b7ge\u00b7wunschte", "tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wan ich von stetem weinen", "tokens": ["Wan", "ich", "von", "ste\u00b7tem", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Einmal auffh\u00f6ren mag?", "tokens": ["Ein\u00b7mal", "auff\u00b7h\u00f6\u00b7ren", "mag", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Trost wolt ich mir bald finden,", "tokens": ["Trost", "wolt", "ich", "mir", "bald", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wan ich ein