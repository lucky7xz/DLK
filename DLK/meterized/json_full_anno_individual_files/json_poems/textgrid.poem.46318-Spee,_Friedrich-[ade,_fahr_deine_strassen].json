{"textgrid.poem.46318": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ade, fahr deine strassen]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ade, fahr deine strassen,", "tokens": ["A\u00b7de", ",", "fahr", "dei\u00b7ne", "stras\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "VVFIN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Du schn\u00f6d vnd b\u00f6se welt:", "tokens": ["Du", "schn\u00f6d", "vnd", "b\u00f6\u00b7se", "welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ade, will dich verlassen.", "tokens": ["A\u00b7de", ",", "will", "dich", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Weg, weg mit gut, vnd gelt!", "tokens": ["Weg", ",", "weg", "mit", "gut", ",", "vnd", "gelt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "ADJD", "$,", "KON", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dein zeitlich lust, vnd frewden,", "tokens": ["Dein", "zeit\u00b7lich", "lust", ",", "vnd", "frew\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "$,", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Pracht, ehr, vnd herrlichkeit", "tokens": ["Pracht", ",", "ehr", ",", "vnd", "herr\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Will f\u00fcrhin g\u00e4ntzlich meiden;", "tokens": ["Will", "f\u00fcr\u00b7hin", "g\u00e4ntz\u00b7lich", "mei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Achts nur f\u00fcr eitelkeit.", "tokens": ["Achts", "nur", "f\u00fcr", "ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vor trawrigkeit de\u00df hertzen", "tokens": ["Vor", "traw\u00b7rig\u00b7keit", "de\u00df", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seufftzt ich au\u00df tieffen grund;", "tokens": ["Seufftzt", "ich", "au\u00df", "tief\u00b7fen", "grund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vor immerlichem schmertzen", "tokens": ["Vor", "im\u00b7mer\u00b7li\u00b7chem", "schmert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ruff ich all tag, vnd stund:", "tokens": ["Ruff", "ich", "all", "tag", ",", "vnd", "stund", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$,", "KON", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die z\u00e4hr mir allweg rinnen,", "tokens": ["Die", "z\u00e4hr", "mir", "all\u00b7weg", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie sanffte regengu\u00df;", "tokens": ["Wie", "sanff\u00b7te", "re\u00b7gen\u00b7gu\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd augen immer schwimmen,", "tokens": ["Vnd", "au\u00b7gen", "im\u00b7mer", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie st\u00e4te w\u00e4sser-flu\u00df.", "tokens": ["Wie", "st\u00e4\u00b7te", "w\u00e4s\u00b7ser\u00b7flu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als offt ich dein gedencke,", "tokens": ["Als", "offt", "ich", "dein", "ge\u00b7den\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Gott, vnd h\u00f6chstes gut,", "tokens": ["Mein", "Gott", ",", "vnd", "h\u00f6chs\u00b7tes", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "ADJA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu dir mich g\u00fctlich lencke,", "tokens": ["Zu", "dir", "mich", "g\u00fct\u00b7lich", "len\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das blut mir wallen thut.", "tokens": ["Das", "blut", "mir", "wal\u00b7len", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Begird sich thut erheben,", "tokens": ["Be\u00b7gird", "sich", "thut", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd wird mir lang die zeit;", "tokens": ["Vnd", "wird", "mir", "lang", "die", "zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In ruh kan ich nit leben,", "tokens": ["In", "ruh", "kan", "ich", "nit", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bi\u00df ich von hinnen scheid.", "tokens": ["Bi\u00df", "ich", "von", "hin\u00b7nen", "scheid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach wan, wan soll es werden,", "tokens": ["Ach", "wan", ",", "wan", "soll", "es", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "$,", "PWAV", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich mich scheiden thu?", "tokens": ["Da\u00df", "ich", "mich", "schei\u00b7den", "thu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist ja doch nichts auff erden,", "tokens": ["Ist", "ja", "doch", "nichts", "auff", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da drinn man friedlich ruh.", "tokens": ["Da", "drinn", "man", "fried\u00b7lich", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O wan, wan wird erscheinen", "tokens": ["O", "wan", ",", "wan", "wird", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PWAV", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der villgewunschte tag,", "tokens": ["Der", "vill\u00b7ge\u00b7wunschte", "tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wan ich von stetem weinen", "tokens": ["Wan", "ich", "von", "ste\u00b7tem", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "Einmal auffh\u00f6ren mag?", "tokens": ["Ein\u00b7mal", "auff\u00b7h\u00f6\u00b7ren", "mag", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Trost wolt ich mir bald finden,", "tokens": ["Trost", "wolt", "ich", "mir", "bald", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wan ich ein T\u00e4ublein wer;", "tokens": ["Wan", "ich", "ein", "T\u00e4ub\u00b7lein", "wer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PWS", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hinauff wolt ich mich schwingen,", "tokens": ["Hin\u00b7auff", "wolt", "ich", "mich", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wol in da\u00df himmlisch heer.", "tokens": ["Wol", "in", "da\u00df", "himm\u00b7lisch", "heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "KOUS", "ADJD", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Da wolt ich mich versencken", "tokens": ["Da", "wolt", "ich", "mich", "ver\u00b7sen\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wol in das h\u00f6chste gut:", "tokens": ["Wol", "in", "das", "h\u00f6chs\u00b7te", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "O Gott, wer wird mirs schencken,", "tokens": ["O", "Gott", ",", "wer", "wird", "mirs", "schen\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Was mich verlangen thut?", "tokens": ["Was", "mich", "ver\u00b7lan\u00b7gen", "thut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun will doch ich verbleiben", "tokens": ["Nun", "will", "doch", "ich", "ver\u00b7blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bestendig allezeit,", "tokens": ["Bes\u00b7ten\u00b7dig", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "In lust, vnd auch in leiden;", "tokens": ["In", "lust", ",", "vnd", "auch", "in", "lei\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In frewd, vnd trawrigkeit:", "tokens": ["In", "frewd", ",", "vnd", "traw\u00b7rig\u00b7keit", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nie soll die lieb erkalten,", "tokens": ["Nie", "soll", "die", "lieb", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nie soll sie nemen ab;", "tokens": ["Nie", "soll", "sie", "ne\u00b7men", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Zu Gott will ich mich halten,", "tokens": ["Zu", "Gott", "will", "ich", "mich", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So gar bi\u00df in das grab.", "tokens": ["So", "gar", "bi\u00df", "in", "das", "grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Vnd wan dan schon thut sausen", "tokens": ["Vnd", "wan", "dan", "schon", "thut", "sau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der wind auff disem meer;", "tokens": ["Der", "wind", "auff", "di\u00b7sem", "meer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wan schon die wellen brausen", "tokens": ["Wan", "schon", "die", "wel\u00b7len", "brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Rund vmb mein schifflein her;", "tokens": ["Rund", "vmb", "mein", "schif\u00b7flein", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJD", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Will ich doch nie verzagen,", "tokens": ["Will", "ich", "doch", "nie", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gott wird mein h\u00fclffer sein;", "tokens": ["Gott", "wird", "mein", "h\u00fclf\u00b7fer", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den ancker will ich schlagen", "tokens": ["Den", "an\u00b7cker", "will", "ich", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu seinem hertzen ein. Amen.", "tokens": ["Zu", "sei\u00b7nem", "hert\u00b7zen", "ein", ".", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Ade, fahr deine strassen,", "tokens": ["A\u00b7de", ",", "fahr", "dei\u00b7ne", "stras\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "VVFIN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Du schn\u00f6d vnd b\u00f6se welt:", "tokens": ["Du", "schn\u00f6d", "vnd", "b\u00f6\u00b7se", "welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ade, will dich verlassen.", "tokens": ["A\u00b7de", ",", "will", "dich", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PRF", "VVINF", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Weg, weg mit gut, vnd gelt!", "tokens": ["Weg", ",", "weg", "mit", "gut", ",", "vnd", "gelt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "ADJD", "$,", "KON", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Dein zeitlich lust, vnd frewden,", "tokens": ["Dein", "zeit\u00b7lich", "lust", ",", "vnd", "frew\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVFIN", "$,", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"