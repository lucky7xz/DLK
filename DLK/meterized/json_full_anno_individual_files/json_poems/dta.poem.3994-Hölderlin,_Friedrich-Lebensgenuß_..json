{"dta.poem.3994": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Lebensgenu\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Noch kehrt in mich der s\u00fc\u00dfe Fr\u00fchling wieder,", "tokens": ["Noch", "kehrt", "in", "mich", "der", "s\u00fc\u00b7\u00dfe", "Fr\u00fch\u00b7ling", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch altert nicht mein kindisch fr\u00f6hlich Herz,", "tokens": ["Noch", "al\u00b7tert", "nicht", "mein", "kin\u00b7disch", "fr\u00f6h\u00b7lich", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch rinnt vom Auge mir der Thau der Liebe nieder,", "tokens": ["Noch", "rinnt", "vom", "Au\u00b7ge", "mir", "der", "Thau", "der", "Lie\u00b7be", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch lebt in mir der Hoffnung Lust und Schmerz.", "tokens": ["Noch", "lebt", "in", "mir", "der", "Hoff\u00b7nung", "Lust", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch tr\u00f6stet mich mit s\u00fc\u00dfer Augenweide", "tokens": ["Noch", "tr\u00f6s\u00b7tet", "mich", "mit", "s\u00fc\u00b7\u00dfer", "Au\u00b7gen\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der blaue Himmel und die gr\u00fcne Flur,", "tokens": ["Der", "blau\u00b7e", "Him\u00b7mel", "und", "die", "gr\u00fc\u00b7ne", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch reicht die G\u00f6ttliche den Taumelkelch der Freude,", "tokens": ["Noch", "reicht", "die", "G\u00f6tt\u00b7li\u00b7che", "den", "Tau\u00b7mel\u00b7kelch", "der", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die jugendliche, freundliche Natur.", "tokens": ["Die", "ju\u00b7gend\u00b7li\u00b7che", ",", "freund\u00b7li\u00b7che", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Getrost! Es ist der Schmerzen werth dies Leben,", "tokens": ["Ge\u00b7trost", "!", "Es", "ist", "der", "Schmer\u00b7zen", "werth", "dies", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ART", "NN", "ADJD", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So lang uns Armen Gottes Sonne scheint", "tokens": ["So", "lang", "uns", "Ar\u00b7men", "Got\u00b7tes", "Son\u00b7ne", "scheint"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "NN", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Bilder be\u00dfrer Zeit um unsre Seele schweben,", "tokens": ["Und", "Bil\u00b7der", "be\u00df\u00b7rer", "Zeit", "um", "uns\u00b7re", "See\u00b7le", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ach! mit uns ein treues Auge weint.", "tokens": ["Und", "ach", "!", "mit", "uns", "ein", "treu\u00b7es", "Au\u00b7ge", "weint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}