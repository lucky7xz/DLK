{"textgrid.poem.47899": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war der Helicon der Teutschen d\u00fcsterm Land", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war der Helicon der Teutschen d\u00fcsterm Land", "tokens": ["Es", "war", "der", "He\u00b7li\u00b7con", "der", "Teut\u00b7schen", "d\u00fcs\u00b7term", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aller Musenkunst vor Jahren unbekant.", "tokens": ["Und", "al\u00b7ler", "Mu\u00b7sen\u00b7kunst", "vor", "Jah\u00b7ren", "un\u00b7be\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der finsterrauhe Wald hat nur das Wild geheget/", "tokens": ["Der", "fins\u00b7ter\u00b7rau\u00b7he", "Wald", "hat", "nur", "das", "Wild", "ge\u00b7he\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es hatte noch die Hand an keinen Pflug geleget", "tokens": ["Es", "hat\u00b7te", "noch", "die", "Hand", "an", "kei\u00b7nen", "Pflug", "ge\u00b7le\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das halbverwildte Volk/ das damals eingeh\u00fcllt/", "tokens": ["Das", "halb\u00b7ver\u00b7wild\u00b7te", "Volk", "/", "das", "da\u00b7mals", "ein\u00b7ge\u00b7h\u00fcllt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PDS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In Fell' und B\u00e4renhaut'/ ihr Hunger ward gestillt", "tokens": ["In", "Fell'", "und", "B\u00e4\u00b7ren\u00b7haut'", "/", "ihr", "Hun\u00b7ger", "ward", "ge\u00b7stillt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$(", "PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von Ceres roher Furcht/ die helle Quell' am rangen", "tokens": ["Von", "Ce\u00b7res", "ro\u00b7her", "Furcht", "/", "die", "hel\u00b7le", "Quell'", "am", "ran\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gab ihnen Trank genug/ sie hatten kein Verlangen/", "tokens": ["Gab", "ih\u00b7nen", "Trank", "ge\u00b7nug", "/", "sie", "hat\u00b7ten", "kein", "Ver\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ADV", "$(", "PPER", "VAFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als/ mit freyk\u00fchner Hand/ zu w\u00fcrgen ihren Feind.", "tokens": ["Als", "/", "mit", "frey\u00b7k\u00fch\u00b7ner", "Hand", "/", "zu", "w\u00fcr\u00b7gen", "ih\u00b7ren", "Feind", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "APPR", "ADJA", "NN", "$(", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie sind sie heut zu Tag der Wei\u00dfheit werthe Freund?", "tokens": ["Wie", "sind", "sie", "heut", "zu", "Tag", "der", "Wei\u00df\u00b7heit", "wert\u00b7he", "Freund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jetzt steht Parnassus hier/ ist doppelt aufgespitzet/", "tokens": ["Jetzt", "steht", "Par\u00b7nas\u00b7sus", "hier", "/", "ist", "dop\u00b7pelt", "auf\u00b7ge\u00b7spit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "$(", "VAFIN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Um welchen Ph\u00f6bus selbst mit seinen Schwestern sitzet/", "tokens": ["Um", "wel\u00b7chen", "Ph\u00f6\u00b7bus", "selbst", "mit", "sei\u00b7nen", "Schwes\u00b7tern", "sit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PWAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Redner Mutterschos/ der Dichter Vaterland/", "tokens": ["Der", "Red\u00b7ner", "Mut\u00b7ter\u00b7schos", "/", "der", "Dich\u00b7ter", "Va\u00b7ter\u00b7land", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da Kunst und Wissenschaft steigt in den h\u00f6chsten Stand/", "tokens": ["Da", "Kunst", "und", "Wis\u00b7sen\u00b7schaft", "steigt", "in", "den", "h\u00f6chs\u00b7ten", "Stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Die Lehrer Gottes Worts/ der Recht und Aertzte Scharen", "tokens": ["Die", "Leh\u00b7rer", "Got\u00b7tes", "Worts", "/", "der", "Recht", "und", "A\u00b7ertz\u00b7te", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "NN", "$(", "ART", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "So manche K\u00fcnstler Zunfft (da\u00df Schande zu befahren", "tokens": ["So", "man\u00b7che", "K\u00fcnst\u00b7ler", "Zunfft", "(", "da\u00df", "Schan\u00b7de", "zu", "be\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "NN", "$(", "KOUS", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Archytas/ D\u00e4dalus/ und auch Lysippus Ruhm/ )", "tokens": ["Ar\u00b7chy\u00b7tas", "/", "D\u00e4\u00b7da\u00b7lus", "/", "und", "auch", "Ly\u00b7sip\u00b7pus", "Ruhm", "/", ")"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$(", "NE", "$(", "KON", "ADV", "NE", "NN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Umzirket dieses Land/ der Tugend Eigenthum.", "tokens": ["Um\u00b7zir\u00b7ket", "die\u00b7ses", "Land", "/", "der", "Tu\u00b7gend", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "$(", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Rom sch\u00e4met sich und fragt/ aus altverfaulten St\u00fctzen/", "tokens": ["Rom", "sch\u00e4\u00b7met", "sich", "und", "fragt", "/", "aus", "alt\u00b7ver\u00b7faul\u00b7ten", "St\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "KON", "VVFIN", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ist di\u00df der Thiere H\u00f6l/ die w\u00fcste Wildni\u00dfpf\u00fctzen?", "tokens": ["Ist", "di\u00df", "der", "Thie\u00b7re", "H\u00f6l", "/", "die", "w\u00fcs\u00b7te", "Wild\u00b7ni\u00df\u00b7pf\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie prachtet ihr Gewand? so s\u00fcsse Schleckerkost", "tokens": ["Wie", "prach\u00b7tet", "ihr", "Ge\u00b7wand", "?", "so", "s\u00fcs\u00b7se", "Schle\u00b7cker\u00b7kost"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Erwehlt Apitius/ Philoxenus den Most.", "tokens": ["Er\u00b7wehlt", "A\u00b7pi\u00b7tius", "/", "Phi\u00b7lo\u00b7xe\u00b7nus", "den", "Most", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$(", "NE", "ART", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.23": {"text": "Das fehlet ihnen noch: Ihr Sprache wolt versiegen/", "tokens": ["Das", "feh\u00b7let", "ih\u00b7nen", "noch", ":", "Ihr", "Spra\u00b7che", "wolt", "ver\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$.", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vnd durch das Wortgemeng des Fremdlings fast erliegen.", "tokens": ["Vnd", "durch", "das", "Wort\u00b7ge\u00b7meng", "des", "Fremd\u00b7lings", "fast", "er\u00b7lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die wird empor gebracht zur letzten Threnenzeit/", "tokens": ["Die", "wird", "em\u00b7por", "ge\u00b7bracht", "zur", "letz\u00b7ten", "Thre\u00b7nen\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKVZ", "VVPP", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und unsre Helden Sprach wird Hofgem\u00e4\u00df bereit.", "tokens": ["Und", "uns\u00b7re", "Hel\u00b7den", "Sprach", "wird", "Hof\u00b7ge\u00b7m\u00e4\u00df", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie rein- und scheinlich prangt sie aus dem Grund gezieret/", "tokens": ["Wie", "rein", "und", "schein\u00b7lich", "prangt", "sie", "aus", "dem", "Grund", "ge\u00b7zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "TRUNC", "KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wie Majest\u00e4tisch klingt/ was unsre Zunge r\u00fchret?", "tokens": ["Wie", "Ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "klingt", "/", "was", "uns\u00b7re", "Zun\u00b7ge", "r\u00fch\u00b7ret", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$(", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der l\u00fcstrend R\u00f6mer weicht/ der Griech der Trunkenpold/", "tokens": ["Der", "l\u00fcst\u00b7rend", "R\u00f6\u00b7mer", "weicht", "/", "der", "Griech", "der", "Trun\u00b7ken\u00b7pold", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der grosse Spanier/ der Frantzmann Neurungshold/", "tokens": ["Der", "gros\u00b7se", "Spa\u00b7nier", "/", "der", "Frantz\u00b7mann", "Neu\u00b7rungs\u00b7hold", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "NE", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Erblasset neben uns. Wie sie nunmehr genesen/", "tokens": ["Er\u00b7blas\u00b7set", "ne\u00b7ben", "uns", ".", "Wie", "sie", "nun\u00b7mehr", "ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$.", "PWAV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.32": {"text": "Mit Wunderart-zart-pracht- und m\u00e4chtiglichem Wesen/", "tokens": ["Mit", "Wun\u00b7der\u00b7ar\u00b7tzar\u00b7tpracht", "und", "m\u00e4ch\u00b7tig\u00b7li\u00b7chem", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "li