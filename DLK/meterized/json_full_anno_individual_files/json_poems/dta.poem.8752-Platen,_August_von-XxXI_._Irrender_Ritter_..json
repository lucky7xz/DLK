{"dta.poem.8752": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxXI .  \n  Irrender Ritter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Durch Geheg und Au',", "tokens": ["Durch", "Ge\u00b7heg", "und", "Au'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Pl\u00f6tzlich ihm zur Seite", "tokens": ["Pl\u00f6tz\u00b7lich", "ihm", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wandelt sch\u00f6ne Frau.", "tokens": ["Wan\u00b7delt", "sch\u00f6\u00b7ne", "Frau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Keusch in Flor geh\u00fcllet", "tokens": ["Keusch", "in", "Flor", "ge\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "War sie, doch es hing", "tokens": ["War", "sie", ",", "doch", "es", "hing"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KON", "PPER", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Flasche wohl gef\u00fcllet", "tokens": ["Fla\u00b7sche", "wohl", "ge\u00b7f\u00fcl\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ihr am G\u00fcrtelring.", "tokens": ["Ihr", "am", "G\u00fcr\u00b7tel\u00b7ring", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ritter sah es blinken,", "tokens": ["Rit\u00b7ter", "sah", "es", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "L\u00fcstern machte Wein,", "tokens": ["L\u00fcs\u00b7tern", "mach\u00b7te", "Wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sagte: La\u00df mich trinken!", "tokens": ["Sag\u00b7te", ":", "La\u00df", "mich", "trin\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Doch sie sagte: Nein!", "tokens": ["Doch", "sie", "sag\u00b7te", ":", "Nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PTKANT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Grimmig schaute Ritter,", "tokens": ["Grim\u00b7mig", "schau\u00b7te", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Der es nicht ertrug:", "tokens": ["Der", "es", "nicht", "er\u00b7trug", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Frau verh\u00f6hnt er bitter,", "tokens": ["Frau", "ver\u00b7h\u00f6hnt", "er", "bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Raubet sch\u00f6nen Krug.", "tokens": ["Rau\u00b7bet", "sch\u00f6\u00b7nen", "Krug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Als er den geleeret", "tokens": ["Als", "er", "den", "ge\u00b7lee\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "F\u00fchlt er sich so krank;", "tokens": ["F\u00fchlt", "er", "sich", "so", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, f\u00fcr Wein bescheeret", "tokens": ["Ach", ",", "f\u00fcr", "Wein", "be\u00b7schee\u00b7ret"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ward ihm Liebestrank.", "tokens": ["Ward", "ihm", "Lie\u00b7bes\u00b7trank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Nun durchschweift er Gr\u00fcnde,", "tokens": ["Nun", "durch\u00b7schweift", "er", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Felder, Berge wild,", "tokens": ["Fel\u00b7der", ",", "Ber\u00b7ge", "wild", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Klaget alte S\u00fcnde,", "tokens": ["Kla\u00b7get", "al\u00b7te", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Suchet Frauenbild.", "tokens": ["Su\u00b7chet", "Frau\u00b7en\u00b7bild", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Stimme l\u00e4\u00dft er schallen,", "tokens": ["Stim\u00b7me", "l\u00e4\u00dft", "er", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Holt es nirgend ein:", "tokens": ["Holt", "es", "nir\u00b7gend", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Waldes Nachtigallen", "tokens": ["Wal\u00b7des", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "H\u00f6ren Ritters Pein.", "tokens": ["H\u00f6\u00b7ren", "Rit\u00b7ters", "Pein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}