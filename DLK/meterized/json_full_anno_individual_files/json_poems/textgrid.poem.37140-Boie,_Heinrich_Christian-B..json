{"textgrid.poem.37140": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "B.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grabe, wems behagt, der Rinde", "tokens": ["Gra\u00b7be", ",", "wems", "be\u00b7hagt", ",", "der", "Rin\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PWS", "VVFIN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Geliebten Namen ein;", "tokens": ["Der", "Ge\u00b7lieb\u00b7ten", "Na\u00b7men", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher Hirtin ich empfinde,", "tokens": ["Wel\u00b7cher", "Hir\u00b7tin", "ich", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcstert keine Buch' im Hain.", "tokens": ["Fl\u00fcs\u00b7tert", "kei\u00b7ne", "Buch'", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsers Bundes Knoten schlangen", "tokens": ["Un\u00b7sers", "Bun\u00b7des", "Kno\u00b7ten", "schlan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jahre fester, doch verrieth", "tokens": ["Jah\u00b7re", "fes\u00b7ter", ",", "doch", "ver\u00b7rieth"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch den Saiten, die ihr klangen,", "tokens": ["Auch", "den", "Sai\u00b7ten", ",", "die", "ihr", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Ihren Namen nie mein Lied.", "tokens": ["Ih\u00b7ren", "Na\u00b7men", "nie", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! die Blume des Genu\u00dfes", "tokens": ["Ach", "!", "die", "Blu\u00b7me", "des", "Ge\u00b7nu\u00b7\u00dfes"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welkt am offnen Sonnenlicht.", "tokens": ["Welkt", "am", "off\u00b7nen", "Son\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeugen unsers stillen Kusses", "tokens": ["Zeu\u00b7gen", "un\u00b7sers", "stil\u00b7len", "Kus\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gaben Neidern schnell Bericht.", "tokens": ["Ga\u00b7ben", "Nei\u00b7dern", "schnell", "Be\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Selbst den Schwur der Treue schwuren", "tokens": ["Selbst", "den", "Schwur", "der", "Treu\u00b7e", "schwu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wir im unbetretnen Wald.", "tokens": ["Wir", "im", "un\u00b7be\u00b7tret\u00b7nen", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Neugier sp\u00e4t die kleinsten Spuren,", "tokens": ["Neu\u00b7gier", "sp\u00e4t", "die", "kleins\u00b7ten", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eifersucht h\u00f6rt leis' und bald.", "tokens": ["Ei\u00b7fer\u00b7sucht", "h\u00f6rt", "leis'", "und", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "ADV", "$."], "meter": "+--++-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Wo das Lamm der Trauten h\u00fcpfet,", "tokens": ["Wo", "das", "Lamm", "der", "Trau\u00b7ten", "h\u00fcp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trift man meine Herde nie.", "tokens": ["Trift", "man", "mei\u00b7ne", "Her\u00b7de", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fcchtern und bed\u00e4chtlich schl\u00fcpfet", "tokens": ["Sch\u00fcch\u00b7tern", "und", "be\u00b7d\u00e4cht\u00b7lich", "schl\u00fcp\u00b7fet"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein verstolner Blick auf sie.", "tokens": ["Mein", "ver\u00b7stol\u00b7ner", "Blick", "auf", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unverf\u00e4rbt h\u00f6r' ich sie nennen,", "tokens": ["Un\u00b7ver\u00b7f\u00e4rbt", "h\u00f6r'", "ich", "sie", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Scherz' und lache, wenn sie singt,", "tokens": ["Scher\u00b7z'", "und", "la\u00b7che", ",", "wenn", "sie", "singt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und will nicht ihr H\u00fcndchen kennen,", "tokens": ["Und", "will", "nicht", "ihr", "H\u00fcnd\u00b7chen", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Das liebkosend mich umspringt.", "tokens": ["Das", "lieb\u00b7ko\u00b7send", "mich", "um\u00b7springt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hirten, lernt euch selbst bewahren!", "tokens": ["Hir\u00b7ten", ",", "lernt", "euch", "selbst", "be\u00b7wah\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Plauderei gebiert nur Leid.", "tokens": ["Plau\u00b7de\u00b7rei", "ge\u00b7biert", "nur", "Leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fc\u00dfens alle gleich erfahren,", "tokens": ["M\u00fc\u00b7\u00dfens", "al\u00b7le", "gleich", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ihr zu beneiden seid?", "tokens": ["Da\u00df", "ihr", "zu", "be\u00b7nei\u00b7den", "seid", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schweigend ist der Wonne F\u00fclle,", "tokens": ["Schwei\u00b7gend", "ist", "der", "Won\u00b7ne", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gern entweicht sie dem Verdacht,", "tokens": ["Gern", "ent\u00b7weicht", "sie", "dem", "Ver\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und erw\u00e4hlt des Schattens H\u00fclle,", "tokens": ["Und", "er\u00b7w\u00e4hlt", "des", "Schat\u00b7tens", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der sie doppelt reizend macht.", "tokens": ["Der", "sie", "dop\u00b7pelt", "rei\u00b7zend", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Leichtes Sinnes schwatzen Thoren", "tokens": ["Leich\u00b7tes", "Sin\u00b7nes", "schwat\u00b7zen", "Tho\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der Seele W\u00fcnschen laut;", "tokens": ["Von", "der", "See\u00b7le", "W\u00fcn\u00b7schen", "laut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was des Kl\u00fcgern Herz erkoren,", "tokens": ["Was", "des", "Kl\u00fc\u00b7gern", "Herz", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird auch Freunden nicht vertraut.", "tokens": ["Wird", "auch", "Freun\u00b7den", "nicht", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Andern kein Geheimnis, bliebe", "tokens": ["An\u00b7dern", "kein", "Ge\u00b7heim\u00b7nis", ",", "blie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJA", "PIAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir mein Gl\u00fcck nicht sch\u00f6n, nicht rein.", "tokens": ["Mir", "mein", "Gl\u00fcck", "nicht", "sch\u00f6n", ",", "nicht", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sag ich denn, da\u00df ich dich liebe,", "tokens": ["Sag", "ich", "denn", ",", "da\u00df", "ich", "dich", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sag ich, Theure, dirs allein!", "tokens": ["Sag", "ich", ",", "Theu\u00b7re", ",", "dirs", "al\u00b7lein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJA", "$,", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Grabe, wems behagt, der Rinde", "tokens": ["Gra\u00b7be", ",", "wems", "be\u00b7hagt", ",", "der", "Rin\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PWS", "VVFIN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Geliebten Namen ein;", "tokens": ["Der", "Ge\u00b7lieb\u00b7ten", "Na\u00b7men", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher Hirtin ich empfinde,", "tokens": ["Wel\u00b7cher", "Hir\u00b7tin", "ich", "emp\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcstert keine Buch' im Hain.", "tokens": ["Fl\u00fcs\u00b7tert", "kei\u00b7ne", "Buch'", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unsers Bundes Knoten schlangen", "tokens": ["Un\u00b7sers", "Bun\u00b7des", "Kno\u00b7ten", "schlan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "