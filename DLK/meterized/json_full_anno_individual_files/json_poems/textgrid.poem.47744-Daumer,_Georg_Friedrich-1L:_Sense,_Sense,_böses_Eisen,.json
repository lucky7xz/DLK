{"textgrid.poem.47744": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sense, Sense, b\u00f6ses Eisen,", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sense, Sense, b\u00f6ses Eisen,", "tokens": ["Sen\u00b7se", ",", "Sen\u00b7se", ",", "b\u00f6\u00b7ses", "Ei\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00fcckisches, verr\u00e4therisches!", "tokens": ["T\u00fc\u00b7cki\u00b7sches", ",", "ver\u00b7r\u00e4\u00b7the\u00b7ri\u00b7sches", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wehe, weh, wie kannst du glei\u00dfen,", "tokens": ["We\u00b7he", ",", "weh", ",", "wie", "kannst", "du", "glei\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wehe, weh, wie kannst du bei\u00dfen,", "tokens": ["We\u00b7he", ",", "weh", ",", "wie", "kannst", "du", "bei\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "PWAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kannst du grausam ohn' Erbarmen", "tokens": ["Kannst", "du", "grau\u00b7sam", "ohn'", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine feine Haut zerrei\u00dfen!", "tokens": ["Ei\u00b7ne", "fei\u00b7ne", "Haut", "zer\u00b7rei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, wie ward das zarte H\u00fchnchen,", "tokens": ["Ach", ",", "wie", "ward", "das", "zar\u00b7te", "H\u00fchn\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das geschonte dieses Hauses", "tokens": ["Das", "ge\u00b7schon\u00b7te", "die\u00b7ses", "Hau\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Um so viel Gebl\u00fct gebracht!", "tokens": ["Um", "so", "viel", "Ge\u00b7bl\u00fct", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ach, wie ward das saftgeschwellte,", "tokens": ["Ach", ",", "wie", "ward", "das", "saft\u00b7ge\u00b7schwell\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Laubgeborgne rothe Beerchen", "tokens": ["Laub\u00b7ge\u00b7borg\u00b7ne", "ro\u00b7the", "Beer\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Saftberaubt und bla\u00df gemacht!", "tokens": ["Saft\u00b7be\u00b7raubt", "und", "bla\u00df", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Tr\u00f6ste dich jedoch, mein P\u00fcppchen,", "tokens": ["Tr\u00f6s\u00b7te", "dich", "je\u00b7doch", ",", "mein", "P\u00fcpp\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Stille, stille deine Z\u00e4hrchen!", "tokens": ["Stil\u00b7le", ",", "stil\u00b7le", "dei\u00b7ne", "Z\u00e4hr\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Kommt der Kr\u00e4mer in das D\u00f6rfchen,", "tokens": ["Kommt", "der", "Kr\u00e4\u00b7mer", "in", "das", "D\u00f6rf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wend' ich ein erspartes Scherfchen,", "tokens": ["Wend'", "ich", "ein", "er\u00b7spar\u00b7tes", "Scherf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Wend' ich einen Dreier auf,", "tokens": ["Wend'", "ich", "ei\u00b7nen", "Drei\u00b7er", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Kaufe dir, betr\u00fcbtes Seelchen,", "tokens": ["Kau\u00b7fe", "dir", ",", "be\u00b7tr\u00fcb\u00b7tes", "Seel\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Reiche dir zum Labetr\u00e4nkchen", "tokens": ["Rei\u00b7che", "dir", "zum", "La\u00b7be\u00b7tr\u00e4nk\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Meth in einem Eiersch\u00e4lchen,", "tokens": ["Me\u00b7th", "in", "ei\u00b7nem", "Ei\u00b7er\u00b7sch\u00e4l\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Tische dir in einem N\u00fc\u00dfchen", "tokens": ["Ti\u00b7sche", "dir", "in", "ei\u00b7nem", "N\u00fc\u00df\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Butter auf, in einem halben,", "tokens": ["But\u00b7ter", "auf", ",", "in", "ei\u00b7nem", "hal\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "APPR", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Bringe dir auf einem Bl\u00e4ttchen,", "tokens": ["Brin\u00b7ge", "dir", "auf", "ei\u00b7nem", "Bl\u00e4tt\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Dem gebrochnen eines Baumes,", "tokens": ["Dem", "ge\u00b7broch\u00b7nen", "ei\u00b7nes", "Bau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Schweinefleisch ein ganzes Quentchen.", "tokens": ["Schwei\u00b7ne\u00b7fleisch", "ein", "gan\u00b7zes", "Quent\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "W\u00e4ssert dir das Leckerm\u00fcndchen", "tokens": ["W\u00e4s\u00b7sert", "dir", "das", "Le\u00b7cker\u00b7m\u00fcnd\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Nach so seltnem Wonneschmaus?", "tokens": ["Nach", "so", "selt\u00b7nem", "Won\u00b7ne\u00b7schmaus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Wohl dazu im Bette h\u00fct' ich,", "tokens": ["Wohl", "da\u00b7zu", "im", "Bet\u00b7te", "h\u00fct'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Wickele das arme Kindchen,", "tokens": ["Wi\u00b7cke\u00b7le", "das", "ar\u00b7me", "Kind\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Das so f\u00e4hrlich angebissen,", "tokens": ["Das", "so", "f\u00e4hr\u00b7lich", "an\u00b7ge\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Dem so viel Gebl\u00fct entrissen,", "tokens": ["Dem", "so", "viel", "Ge\u00b7bl\u00fct", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "S\u00e4nftiglich in weiche Kissen,", "tokens": ["S\u00e4nf\u00b7tig\u00b7lich", "in", "wei\u00b7che", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Lass' es aus Gemach und Hause", "tokens": ["Lass'", "es", "aus", "Ge\u00b7mach", "und", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Lange, lange nicht hinaus.", "tokens": ["Lan\u00b7ge", ",", "lan\u00b7ge", "nicht", "hin\u00b7aus", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "So gelangt das zarte H\u00fchnchen", "tokens": ["So", "ge\u00b7langt", "das", "zar\u00b7te", "H\u00fchn\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wiederum zu seiner Kraft,", "tokens": ["Wie\u00b7de\u00b7rum", "zu", "sei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "So gelangt das rothe Beerchen", "tokens": ["So", "ge\u00b7langt", "das", "ro\u00b7the", "Beer\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Wiederum zu seinem Saft.", "tokens": ["Wie\u00b7de\u00b7rum", "zu", "sei\u00b7nem", "Saft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sense, Sense, b\u00f6ses Eisen,", "tokens": ["Sen\u00b7se", ",", "Sen\u00b7se", ",", "b\u00f6\u00b7ses", "Ei