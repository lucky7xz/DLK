{"dta.poem.20042": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Im Walde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Tief durch den Wald Gesang erschallt,", "tokens": ["Tief", "durch", "den", "Wald", "Ge\u00b7sang", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die leichten V\u00f6glein scherzen,", "tokens": ["Die", "leich\u00b7ten", "V\u00f6\u00b7glein", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Mensch allein, der tr\u00e4gt die Pein", "tokens": ["Der", "Mensch", "al\u00b7lein", ",", "der", "tr\u00e4gt", "die", "Pein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Recht tief im kranken Herzen.", "tokens": ["Recht", "tief", "im", "kran\u00b7ken", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Leicht h\u00fcpft der Bach den Blumen nach,", "tokens": ["Leicht", "h\u00fcpft", "der", "Bach", "den", "Blu\u00b7men", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm ist so k\u00fchl und helle,", "tokens": ["Ihm", "ist", "so", "k\u00fchl", "und", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch's Menschenherz, da schleicht mit Schmerz", "tokens": ["Durch's", "Men\u00b7schen\u00b7herz", ",", "da", "schleicht", "mit", "Schmerz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des hei\u00dfen Blutes Welle.", "tokens": ["Des", "hei\u00b7\u00dfen", "Blu\u00b7tes", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gesang verhallt, Sturm wiegt den Wald", "tokens": ["Ge\u00b7sang", "ver\u00b7hallt", ",", "Sturm", "wiegt", "den", "Wald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dumpfen Melodieen;", "tokens": ["In", "dum\u00b7pfen", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einsam die Bahn mu\u00df Wandersmann", "tokens": ["Ein\u00b7sam", "die", "Bahn", "mu\u00df", "Wan\u00b7ders\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "VMFIN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Mit d\u00fcstrer Wolke ziehen.", "tokens": ["Mit", "d\u00fcst\u00b7rer", "Wol\u00b7ke", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Rinn' nieder, Thau! aus Wolken grau,", "tokens": ["Rinn'", "nie\u00b7der", ",", "Thau", "!", "aus", "Wol\u00b7ken", "grau", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "$.", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich saugt die Blum' in Liebe!", "tokens": ["Dich", "saugt", "die", "Blum'", "in", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Thr\u00e4n'! bleib zur\u00fcck im Menschenblick,", "tokens": ["Thr\u00e4n'", "!", "bleib", "zu\u00b7r\u00fcck", "im", "Men\u00b7schen\u00b7blick", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Machst Blumen welk und tr\u00fcbe!", "tokens": ["Machst", "Blu\u00b7men", "welk", "und", "tr\u00fc\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}