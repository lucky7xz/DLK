{"textgrid.poem.59594": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flammet Herzen! Wehet Fahnen!", "tokens": ["Flam\u00b7met", "Her\u00b7zen", "!", "We\u00b7het", "Fah\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flammet, wehet himmelan!", "tokens": ["Flam\u00b7met", ",", "we\u00b7het", "him\u00b7me\u00b7lan", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Wonne der Germanen,", "tokens": ["Denn", "die", "Won\u00b7ne", "der", "Ger\u00b7ma\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn die Schlacht, sie hebet an,", "tokens": ["Denn", "die", "Schlacht", ",", "sie", "he\u00b7bet", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es tilgt der scharfe Stahl", "tokens": ["Und", "es", "tilgt", "der", "schar\u00b7fe", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Lange Schande, lange Qual.", "tokens": ["Lan\u00b7ge", "Schan\u00b7de", ",", "lan\u00b7ge", "Qual", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Klingt, Trompeten! Klingt, Posaunen!", "tokens": ["Klingt", ",", "Trom\u00b7pe\u00b7ten", "!", "Klingt", ",", "Po\u00b7sau\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVFIN", "$,", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Trommel, schmettre wirbelnd drein!", "tokens": ["Trom\u00b7mel", ",", "schmett\u00b7re", "wir\u00b7belnd", "drein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schrecken fliege und Erstaunen", "tokens": ["Schre\u00b7cken", "flie\u00b7ge", "und", "Er\u00b7stau\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit uns furchtbar durch die Reihn!", "tokens": ["Mit", "uns", "furcht\u00b7bar", "durch", "die", "Reihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vorderst leuchtend schwebe, Sieg!", "tokens": ["Vor\u00b7derst", "leuch\u00b7tend", "schwe\u00b7be", ",", "Sieg", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn wir ziehn in heil'gen Krieg.", "tokens": ["Denn", "wir", "ziehn", "in", "heil'\u00b7gen", "Krieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Auch ist einer mitgezogen,", "tokens": ["Auch", "ist", "ei\u00b7ner", "mit\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der am besten streiten kann,", "tokens": ["Der", "am", "bes\u00b7ten", "strei\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKA", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat der Rache Schwert gezogen,", "tokens": ["Hat", "der", "Ra\u00b7che", "Schwert", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer, ein gewalt'ger Mann \u2013", "tokens": ["Ei\u00b7ner", ",", "ein", "ge\u00b7walt'\u00b7ger", "Mann", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6rt! Sein Name klinget Gott,", "tokens": ["H\u00f6rt", "!", "Sein", "Na\u00b7me", "klin\u00b7get", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Feinde Trotz wird Spott.", "tokens": ["Und", "der", "Fein\u00b7de", "Trotz", "wird", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn er ist der Freiheit Retter", "tokens": ["Denn", "er", "ist", "der", "Frei\u00b7heit", "Ret\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Treue sichrer Hort,", "tokens": ["Und", "der", "Treu\u00b7e", "sich\u00b7rer", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Weht wie Sturmwind d\u00fcrre Bl\u00e4tter", "tokens": ["Weht", "wie", "Sturm\u00b7wind", "d\u00fcr\u00b7re", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lug und Trug der Buben fort;", "tokens": ["Lug", "und", "Trug", "der", "Bu\u00b7ben", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott ist unser Heil und Sieg,", "tokens": ["Gott", "ist", "un\u00b7ser", "Heil", "und", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn wir ziehn in heil'gen Krieg.", "tokens": ["Denn", "wir", "ziehn", "in", "heil'\u00b7gen", "Krieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Deutsche Ehre zu erneuen,", "tokens": ["Deut\u00b7sche", "Eh\u00b7re", "zu", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu vertilgen welschen Tand,", "tokens": ["Zu", "ver\u00b7til\u00b7gen", "wel\u00b7schen", "Tand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PWAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ziehn wir aus mit echten Treuen", "tokens": ["Ziehn", "wir", "aus", "mit", "ech\u00b7ten", "Treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr das heil'ge Vaterland,", "tokens": ["F\u00fcr", "das", "heil'\u00b7ge", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit uns ziehen Pflicht und Recht:", "tokens": ["Mit", "uns", "zie\u00b7hen", "Pflicht", "und", "Recht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zittre drum, Tyrann und Knecht!", "tokens": ["Zitt\u00b7re", "drum", ",", "Ty\u00b7rann", "und", "Knecht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Zittre! Denn wir wollen sterben,", "tokens": ["Zitt\u00b7re", "!", "Denn", "wir", "wol\u00b7len", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterben all in einem Mut,", "tokens": ["Ster\u00b7ben", "all", "in", "ei\u00b7nem", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder auch das Land erwerben", "tokens": ["O\u00b7der", "auch", "das", "Land", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Eisen, mit dem Blut:", "tokens": ["Mit", "dem", "Ei\u00b7sen", ",", "mit", "dem", "Blut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deutschland ist's, der Freiheit Land,", "tokens": ["Deutschland", "ist's", ",", "der", "Frei\u00b7heit", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Tapfrer M\u00e4nner Vaterland.", "tokens": ["Tapf\u00b7rer", "M\u00e4n\u00b7ner", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Darum, Herzen, werdet Flammen!", "tokens": ["Da\u00b7rum", ",", "Her\u00b7zen", ",", "wer\u00b7det", "Flam\u00b7men", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darum, Fahnen himmelan!", "tokens": ["Da\u00b7rum", ",", "Fah\u00b7nen", "him\u00b7me\u00b7lan", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutschlands Kinder, frisch zusammen!", "tokens": ["Deutschlands", "Kin\u00b7der", ",", "frisch", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dem Freiheitskampf voran!", "tokens": ["In", "dem", "Frei\u00b7heits\u00b7kampf", "vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brecht der Knechtschaft schn\u00f6des Joch!", "tokens": ["Brecht", "der", "Knecht\u00b7schaft", "schn\u00f6\u00b7des", "Joch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott im Himmel lebet noch.", "tokens": ["Gott", "im", "Him\u00b7mel", "le\u00b7bet", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Hebt die H\u00e4nde! La\u00dft uns schw\u00f6ren!", "tokens": ["Hebt", "die", "H\u00e4n\u00b7de", "!", "La\u00dft", "uns", "schw\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr das heil'ge deutsche Land,", "tokens": ["F\u00fcr", "das", "heil'\u00b7ge", "deut\u00b7sche", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr der V\u00e4ter hohe Ehren", "tokens": ["F\u00fcr", "der", "V\u00e4\u00b7ter", "ho\u00b7he", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herz und Schwert zum Sieg