{"textgrid.poem.48757": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "44. Auf das verlorne Kleinot, Herrn M. Olearien \u00fcberschicket", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Latona sah mir zu, o du mein wahrer Freund,", "tokens": ["La\u00b7to\u00b7na", "sah", "mir", "zu", ",", "o", "du", "mein", "wah\u00b7rer", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "FM", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "da ich dir di\u00df Sonnet, das schlechte, heute machte,", "tokens": ["da", "ich", "dir", "di\u00df", "Son\u00b7net", ",", "das", "schlech\u00b7te", ",", "heu\u00b7te", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PDS", "NN", "$,", "PDS", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "als ich das edle Schiff mit unsrer Schaar bewachte,", "tokens": ["als", "ich", "das", "ed\u00b7le", "Schiff", "mit", "uns\u00b7rer", "Schaar", "be\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das Schiff, das Menschen hat und scheuet keinen Feind.", "tokens": ["das", "Schiff", ",", "das", "Men\u00b7schen", "hat", "und", "scheu\u00b7et", "kei\u00b7nen", "Feind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich neigte mich vor dem, was an ihr g\u00f6ttlich scheint.", "tokens": ["Ich", "neig\u00b7te", "mich", "vor", "dem", ",", "was", "an", "ihr", "g\u00f6tt\u00b7lich", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "$,", "PRELS", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie schien, ich schriebe fort, bis ichs zum Ende brachte;", "tokens": ["Sie", "schien", ",", "ich", "schrie\u00b7be", "fort", ",", "bis", "ichs", "zum", "En\u00b7de", "brach\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,", "KOUS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mit diesem trat herf\u00fcr der junge Tag und lachte", "tokens": ["mit", "die\u00b7sem", "trat", "her\u00b7f\u00fcr", "der", "jun\u00b7ge", "Tag", "und", "lach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "ADV", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mich aus den Rosen an, in welchen, wie man meint,", "tokens": ["mich", "aus", "den", "Ro\u00b7sen", "an", ",", "in", "wel\u00b7chen", ",", "wie", "man", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKVZ", "$,", "APPR", "PWAT", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "er Thejens Tochter k\u00fc\u00dft. Ich war, wie Freunde pflegen,", "tokens": ["er", "The\u00b7jens", "Toch\u00b7ter", "k\u00fc\u00dft", ".", "Ich", "war", ",", "wie", "Freun\u00b7de", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "NN", "VVFIN", "$.", "PPER", "VAFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von ganzem Herzen froh von deines Gl\u00fcckes wegen,", "tokens": ["von", "gan\u00b7zem", "Her\u00b7zen", "froh", "von", "dei\u00b7nes", "Gl\u00fc\u00b7ckes", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "APPR", "PPOSAT", "NN", "APPR", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das f\u00fcr dein Reichsein wacht und deinem Schaden flucht.", "tokens": ["das", "f\u00fcr", "dein", "Reich\u00b7sein", "wacht", "und", "dei\u00b7nem", "Scha\u00b7den", "flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was Mund- und Herzensfreund, was suchst du deine Sachon,", "tokens": ["Was", "Mun\u00b7d", "und", "Her\u00b7zens\u00b7freund", ",", "was", "suchst", "du", "dei\u00b7ne", "Sa\u00b7chon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "TRUNC", "KON", "NN", "$,", "PWS", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "f\u00fcr welche Nacht und Tag die muntern Sterne wachen,", "tokens": ["f\u00fcr", "wel\u00b7che", "Nacht", "und", "Tag", "die", "mun\u00b7tern", "Ster\u00b7ne", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df sie auch ihr Verlust selbselbsten wiedersucht?", "tokens": ["da\u00df", "sie", "auch", "ihr", "Ver\u00b7lust", "selb\u00b7selbs\u00b7ten", "wie\u00b7der\u00b7sucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Latona sah mir zu, o du mein wahrer Freund,", "tokens": ["La\u00b7to\u00b7na", "sah", "mir", "zu", ",", "o", "du", "mein", "wah\u00b7rer", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "FM", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "da ich dir di\u00df Sonnet, das schlechte, heute machte,", "tokens": ["da", "ich", "dir", "di\u00df", "Son\u00b7net", ",", "das", "schlech\u00b7te", ",", "heu\u00b7te", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PDS", "NN", "$,", "PDS", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "als ich das edle Schiff mit unsrer Schaar bewachte,", "tokens": ["als", "ich", "das", "ed\u00b7le", "Schiff", "mit", "uns\u00b7rer", "Schaar", "be\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "wor