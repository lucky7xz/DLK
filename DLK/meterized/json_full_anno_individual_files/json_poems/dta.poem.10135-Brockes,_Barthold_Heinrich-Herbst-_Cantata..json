{"dta.poem.10135": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herbst- Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit beruhigtem Gem\u00fcthe,", "tokens": ["Mit", "be\u00b7ru\u00b7hig\u00b7tem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Seh\u2019 ich meines Sch\u00f6pffers G\u00fcte", "tokens": ["Seh'", "ich", "mei\u00b7nes", "Sch\u00f6pf\u00b7fers", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In des Herbstes Frucht und Zier.", "tokens": ["In", "des", "Herbs\u00b7tes", "Frucht", "und", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey dem aufgekl\u00e4rten Wetter,", "tokens": ["Bey", "dem", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stellen mir die bunten Bl\u00e4tter", "tokens": ["Stel\u00b7len", "mir", "die", "bun\u00b7ten", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gar ein lieblich Schau-Spiel f\u00fcr.", "tokens": ["Gar", "ein", "lieb\u00b7lich", "Schau\u00b7Spiel", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df liebste Seele doch die Welt", "tokens": ["La\u00df", "liebs\u00b7te", "See\u00b7le", "doch", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJA", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die GOTT im Herbst so herrlich schm\u00fccket,", "tokens": ["Die", "GoTT", "im", "Herbst", "so", "herr\u00b7lich", "schm\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht, wie vorhin, unangeblicket!", "tokens": ["Nicht", ",", "wie", "vor\u00b7hin", ",", "un\u00b7an\u00b7ge\u00b7bli\u00b7cket", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "ADV", "$,", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Erwege das, so uns ietzt vorgestellt!", "tokens": ["Er\u00b7we\u00b7ge", "das", ",", "so", "uns", "ietzt", "vor\u00b7ge\u00b7stellt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der W\u00e4lder lieblich Bl\u00e4tter-Zelt,", "tokens": ["Der", "W\u00e4l\u00b7der", "lieb\u00b7lich", "Bl\u00e4t\u00b7ter\u00b7Zelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stand erst in gr\u00fcner Farb\u2019 allein:", "tokens": ["Stand", "erst", "in", "gr\u00fc\u00b7ner", "Fa\u00b7rb'", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Jetzt nimmt es einen neuen Schein,", "tokens": ["Jetzt", "nimmt", "es", "ei\u00b7nen", "neu\u00b7en", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Indem es, da es sich bepurpert und verg\u00fcldet,", "tokens": ["In\u00b7dem", "es", ",", "da", "es", "sich", "be\u00b7pur\u00b7pert", "und", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "PRF", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Recht w\u00fcrckliche Tapeten bildet.", "tokens": ["Recht", "w\u00fcrck\u00b7li\u00b7che", "Ta\u00b7pe\u00b7ten", "bil\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wem iemahl die so bunt\u2019 als pr\u00e4chtigen Gezelte,", "tokens": ["Wem", "ie\u00b7mahl", "die", "so", "bunt'", "als", "pr\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7zel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADV", "ADJD", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So zu verschiedner Zeit den T\u00fcrcken abgenommen,", "tokens": ["So", "zu", "ver\u00b7schied\u00b7ner", "Zeit", "den", "T\u00fcr\u00b7cken", "ab\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In Wien einst zu Gesicht gekommen;", "tokens": ["In", "Wi\u00b7en", "einst", "zu", "Ge\u00b7sicht", "ge\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Wird freilich, da\u00df sie Wunder-sch\u00f6n,", "tokens": ["Wird", "frei\u00b7lich", ",", "da\u00df", "sie", "Wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit Lust gestehn.", "tokens": ["Mit", "Lust", "ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Jedoch ist dieses wol der Zelten Schmuck zu gleichen,", "tokens": ["Je\u00b7doch", "ist", "die\u00b7ses", "wol", "der", "Zel\u00b7ten", "Schmuck", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "ADV", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Den uns im Herbst der B\u00e4ume Wipfel reichen?", "tokens": ["Den", "uns", "im", "Herbst", "der", "B\u00e4u\u00b7me", "Wip\u00b7fel", "rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Denn, weil das helle Sonnen-Licht", "tokens": ["Denn", ",", "weil", "das", "hel\u00b7le", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Durch die so bunt-als zarten Bl\u00e4tter bricht,", "tokens": ["Durch", "die", "so", "bunt\u00b7als", "zar\u00b7ten", "Bl\u00e4t\u00b7ter", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Erblicket man der Farben Harmonie", "tokens": ["Er\u00b7bli\u00b7cket", "man", "der", "Far\u00b7ben", "Har\u00b7mo\u00b7nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "In einem klaren Glantz, und nicht verdickt", "tokens": ["In", "ei\u00b7nem", "kla\u00b7ren", "Glantz", ",", "und", "nicht", "ver\u00b7dickt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Als jene, die", "tokens": ["Als", "je\u00b7ne", ",", "die"], "token_info": ["word", "word", "punct", "word"], "pos": ["KOUS", "PDS", "$,", "PRELS"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Von dick- und dichter Seid\u2019 entworffen und gestickt.", "tokens": ["Von", "dick", "und", "dich\u00b7ter", "Seid'", "ent\u00b7worf\u00b7fen", "und", "ge\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Allein:", "tokens": ["Al\u00b7lein", ":"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.20": {"text": "Wie bald vergeht der Bl\u00e4tter bunter Schein!", "tokens": ["Wie", "bald", "ver\u00b7geht", "der", "Bl\u00e4t\u00b7ter", "bun\u00b7ter", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Kaum seh\u2019 ich sie mit Lust in hohen L\u00fcfften stehn,", "tokens": ["Kaum", "seh'", "ich", "sie", "mit", "Lust", "in", "ho\u00b7hen", "L\u00fcff\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So taumeln sie herab, sie welcken, sie vergehn,", "tokens": ["So", "tau\u00b7meln", "sie", "her\u00b7ab", ",", "sie", "wel\u00b7cken", ",", "sie", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVINF", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch selbst ihr sch\u00f6ner Tod ist billig hoch zu sch\u00e4tzen,", "tokens": ["Doch", "selbst", "ihr", "sch\u00f6\u00b7ner", "Tod", "ist", "bil\u00b7lig", "hoch", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und giebt dem, ders erwegt, ein lehrendes Ergetzen.", "tokens": ["Und", "giebt", "dem", ",", "ders", "er\u00b7wegt", ",", "ein", "leh\u00b7ren\u00b7des", "Er\u00b7get\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "$,", "ADV", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}}}}}