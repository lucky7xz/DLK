{"dta.poem.23587": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Heute mir, morgen dir.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts will bei uns mehr gehen,", "tokens": ["Nichts", "will", "bei", "uns", "mehr", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil wir auf's Stehn nur sehen,", "tokens": ["Weil", "wir", "auf's", "Stehn", "nur", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum lassen wir auch unsre Heere stehen.", "tokens": ["Drum", "las\u00b7sen", "wir", "auch", "uns\u00b7re", "Hee\u00b7re", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur ihnen ist zu danken,", "tokens": ["Nur", "ih\u00b7nen", "ist", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df wir in unsern Schranken", "tokens": ["Da\u00df", "wir", "in", "un\u00b7sern", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht kommen in ein misslich Schwanken.", "tokens": ["Nicht", "kom\u00b7men", "in", "ein", "miss\u00b7lich", "Schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch steht vor diesen Heeren,", "tokens": ["Doch", "steht", "vor", "die\u00b7sen", "Hee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Leibwachen mit Gewehren,", "tokens": ["Leib\u00b7wa\u00b7chen", "mit", "Ge\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein gro\u00df Gedankenheer mit Schwert und Speeren.", "tokens": ["Ein", "gro\u00df", "Ge\u00b7dan\u00b7ken\u00b7heer", "mit", "Schwert", "und", "Spee\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn beide sich bekriegen,", "tokens": ["Wenn", "bei\u00b7de", "sich", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer wird von beiden siegen?", "tokens": ["Wer", "wird", "von", "bei\u00b7den", "sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Gedanken stehn, und unsre Heere fliegen.", "tokens": ["Die", "Ge\u00b7dan\u00b7ken", "stehn", ",", "und", "uns\u00b7re", "Hee\u00b7re", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}