{"dta.poem.19725": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Die Mordwirthin .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es waren drei Soldaten-S\u00f6hn,               ", "tokens": ["Es", "wa\u00b7ren", "drei", "Sol\u00b7da\u00b7ten\u00b7S\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie haben Lust im Krieg zu gehn,", "tokens": ["Sie", "ha\u00b7ben", "Lust", "im", "Krieg", "zu", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl ins Soldaten Leben.", "tokens": ["Wohl", "ins", "Sol\u00b7da\u00b7ten", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie bleiben aus eine kleine Weil,", "tokens": ["Sie", "blei\u00b7ben", "aus", "ei\u00b7ne", "klei\u00b7ne", "Weil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Sie machen sich Geld und Brod dabei,", "tokens": ["Sie", "ma\u00b7chen", "sich", "Geld", "und", "Brod", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Auch Ungrische Dukaten.", "tokens": ["Auch", "Un\u00b7gri\u00b7sche", "Du\u00b7ka\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}