{"dta.poem.2467": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Solius pietatis flos perpetuus:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Wer mit der Tugendkrafft sich selbst kan \u00fcberwinden", "tokens": ["Wer", "mit", "der", "Tu\u00b7gend\u00b7krafft", "sich", "selbst", "kan", "\u00fc\u00b7berw\u00b7in\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "PRF", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ist der erst\u2019 und auch der beste Siegesmann;", "tokens": ["Der", "ist", "der", "er\u00b7st'", "und", "auch", "der", "bes\u00b7te", "Sie\u00b7ges\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "VVFIN", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der aber/ welcher sich nicht selbst bezwingen kan/", "tokens": ["Der", "a\u00b7ber", "/", "wel\u00b7cher", "sich", "nicht", "selbst", "be\u00b7zwin\u00b7gen", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$(", "PRELS", "PRF", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird f\u00fcr sein Ehrenlob ein he\u00dflichs Schandflekk finden.", "tokens": ["Wird", "f\u00fcr", "sein", "Eh\u00b7ren\u00b7lob", "ein", "he\u00df\u00b7lichs", "Schand\u00b7flekk", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN",