{"dta.poem.137": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ein weisses Regen-Tuch um hatte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "O Anmuths voller Geist! den Schoo\u00df und Br\u00fcste zieren/", "tokens": ["O", "An\u00b7muths", "vol\u00b7ler", "Geist", "!", "den", "Schoo\u00df", "und", "Br\u00fcs\u00b7te", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$.", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du Geist/ den jedermann mit Lust und Freuden sieht/", "tokens": ["Du", "Geist", "/", "den", "je\u00b7der\u00b7mann", "mit", "Lust", "und", "Freu\u00b7den", "sieht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "ART", "PIS", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geist/ der von aussen wei\u00df und doch wie Rosen bl\u00fcht/", "tokens": ["Geist", "/", "der", "von", "aus\u00b7sen", "wei\u00df", "und", "doch", "wie", "Ro\u00b7sen", "bl\u00fcht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "APPR", "VVIZU", "VVFIN", "KON", "ADV", "KOKOM", "NN", "VVFIN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Geist/ dessen Reitzungen ins Paradies hinf\u00fchren.", "tokens": ["Geist", "/", "des\u00b7sen", "Reit\u00b7zun\u00b7gen", "ins", "Pa\u00b7