{"textgrid.poem.52203": {"metadata": {"author": {"name": "Gryphius, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wienerisches Sieges-Lied", "genre": "verse", "period": "N.A.", "pub_year": 1677, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Best\u00e4ndigkeit, tapferes Wien,", "tokens": ["Dei\u00b7ne", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", ",", "tap\u00b7fe\u00b7res", "Wi\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Sol gleichwie die Cedern st\u00e4ts wachsen und bl\u00fchn", "tokens": ["Sol", "gleich\u00b7wie", "die", "Ce\u00b7dern", "st\u00e4ts", "wach\u00b7sen", "und", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "KON", "ART", "NN", "ADV", "VVINF", "KON", "VVINF"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dein Helden-Mutt", "tokens": ["Dein", "Hel\u00b7den\u00b7Mutt"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Trozt Schwerd und Glutt,", "tokens": ["Trozt", "Schwerd", "und", "Glutt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und tritt mit frohem Fu\u00df auf Stambols Brutt.", "tokens": ["Und", "tritt", "mit", "fro\u00b7hem", "Fu\u00df", "auf", "Stam\u00b7bols", "Brutt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Krone von Oesterreich, herrliche Stadt,", "tokens": ["Kro\u00b7ne", "von", "O\u00b7es\u00b7ter\u00b7