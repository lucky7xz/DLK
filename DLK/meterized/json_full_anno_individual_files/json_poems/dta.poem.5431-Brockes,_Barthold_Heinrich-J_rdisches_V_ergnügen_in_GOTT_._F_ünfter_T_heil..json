{"dta.poem.5431": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die\u00df zeiget Stoltz und Eigen-Liebe. Aus diesen schwar-", "tokens": ["Die\u00df", "zei\u00b7get", "Stoltz", "und", "Ei\u00b7gen\u00b7Lie\u00b7be", ".", "Aus", "die\u00b7sen", "schwa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "KON", "NN", "$.", "APPR", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+---+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "tzen Qvellen fliessen", "tokens": ["tzen", "Qvel\u00b7len", "flies\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ein st\u00f6rrig, wiedersinnig Wesen, zum Guten Tr\u00e4gheit,", "tokens": ["Ein", "st\u00f6r\u00b7rig", ",", "wie\u00b7der\u00b7sin\u00b7nig", "We\u00b7sen", ",", "zum", "Gu\u00b7ten", "Tr\u00e4g\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADJD", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "keine Triebe", "tokens": ["kei\u00b7ne", "Trie\u00b7be"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Von einer holden, kindlichen, mit Freundlichkeit vermisch-", "tokens": ["Von", "ei\u00b7ner", "hol\u00b7den", ",", "kind\u00b7li\u00b7chen", ",", "mit", "Freund\u00b7lich\u00b7keit", "ver\u00b7misch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "$,", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "ten Liebe,", "tokens": ["ten", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Wodurch denn gegen unsern N\u00e4chsten auch Ha\u00df und Bit-", "tokens": ["Wo\u00b7durch", "denn", "ge\u00b7gen", "un\u00b7sern", "N\u00e4chs\u00b7ten", "auch", "Ha\u00df", "und", "Bit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PPOSAT", "NN", "ADV", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "terkeit entspriessen.", "tokens": ["ter\u00b7keit", "ent\u00b7spries\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Man weis von keiner Furcht f\u00fcr GOtt, als einer Knechti-", "tokens": ["Man", "weis", "von", "kei\u00b7ner", "Furcht", "f\u00fcr", "Gott", ",", "als", "ei\u00b7ner", "Knech\u00b7ti"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "PTKVZ", "APPR", "PIAT", "NN", "APPR", "NN", "$,", "KOUS", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "schen; man w\u00fchlt", "tokens": ["schen", ";", "man", "w\u00fchlt"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "PIS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Jm Neid\u2019 und Geitz best\u00e4ndig fort, weil man sonst kein", "tokens": ["Jm", "Neid'", "und", "Geitz", "be\u00b7st\u00e4n\u00b7dig", "fort", ",", "weil", "man", "sonst", "kein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "ADJD", "PTKVZ", "$,", "KOUS", "PIS", "ADV", "PIAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vergn\u00fcgen f\u00fchlt.", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "f\u00fchlt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Nun stelle dir zwo Seelen f\u00fcr, gantz von einander unter-", "tokens": ["Nun", "stel\u00b7le", "dir", "zwo", "See\u00b7len", "f\u00fcr", ",", "gantz", "von", "ein\u00b7an\u00b7der", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "CARD", "NN", "APPR", "$,", "ADV", "APPR", "PRF", "TRUNC"], "meter": "-+-+-+--++-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "schieden,", "tokens": ["schie\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Die eine fr\u00f6lich, freundlich, fromm; die andre nimmer", "tokens": ["Die", "ei\u00b7ne", "fr\u00f6\u00b7lich", ",", "freund\u00b7lich", ",", "fromm", ";", "die", "and\u00b7re", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "ADJD", "$,", "ADJD", "$,", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "recht zu frieden,", "tokens": ["recht", "zu", "frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Stets murrisch, st\u00f6rrig, gr\u00e4mlich, traurig. Die eine", "tokens": ["Stets", "mur\u00b7risch", ",", "st\u00f6r\u00b7rig", ",", "gr\u00e4m\u00b7lich", ",", "trau\u00b7rig", ".", "Die", "ei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$.", "ART", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Seele zeigt in allen", "tokens": ["See\u00b7le", "zeigt", "in", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PIAT"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Erk\u00e4nntlichkeit, Danck, Liebe, Sanftmuth, Vergn\u00fcgen", "tokens": ["Er\u00b7k\u00e4nnt\u00b7lich\u00b7keit", ",", "Danck", ",", "Lie\u00b7be", ",", "Sanft\u00b7muth", ",", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "und Gelassenheit;", "tokens": ["und", "Ge\u00b7las\u00b7sen\u00b7heit", ";"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "Die andere Misvergn\u00fcgen, Unmuth, Gram, Wieder-", "tokens": ["Die", "an\u00b7de\u00b7re", "Mis\u00b7ver\u00b7gn\u00fc\u00b7gen", ",", "Un\u00b7muth", ",", "Gram", ",", "Wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "willen, Bitterkeit;", "tokens": ["wil\u00b7len", ",", "Bit\u00b7ter\u00b7keit", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Sprich selber welche solte dem, der alle beid\u2019, in dieser", "tokens": ["Sprich", "sel\u00b7ber", "wel\u00b7che", "sol\u00b7te", "dem", ",", "der", "al\u00b7le", "beid'", ",", "in", "die\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "PIS", "VMFIN", "ART", "$,", "PRELS", "PIS", "PTKVZ", "$,", "APPR", "PDAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.24": {"text": "Zeit,", "tokens": ["Zeit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Zu seiner Ehr\u2019, in seinen Wercken vergn\u00fcgen wolte, doch", "tokens": ["Zu", "sei\u00b7ner", "Ehr'", ",", "in", "sei\u00b7nen", "Wer\u00b7cken", "ver\u00b7gn\u00fc\u00b7gen", "wol\u00b7te", ",", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "gefallen?", "tokens": ["ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Entsteht nun aus der Nicht-Erk\u00e4nntni\u00df so vieler uns ge-", "tokens": ["Ent\u00b7steht", "nun", "aus", "der", "Nicht\u00b7Er\u00b7k\u00e4nnt\u00b7ni\u00df", "so", "vie\u00b7ler", "uns", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ADV", "PIAT", "PRF", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "schenckten Gaben", "tokens": ["schenck\u00b7ten", "Ga\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Und aus den nicht erwognen Wundern, die wir hier zu", "tokens": ["Und", "aus", "den", "nicht", "er\u00b7wog\u00b7nen", "Wun\u00b7dern", ",", "die", "wir", "hier", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "PTKNEG", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "betrachten haben,", "tokens": ["be\u00b7trach\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}}}}}