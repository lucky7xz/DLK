{"textgrid.poem.39079": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Zeichen der Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah einen Knaben, der spielte Krieg", "tokens": ["Ich", "sah", "ei\u00b7nen", "Kna\u00b7ben", ",", "der", "spiel\u00b7te", "Krieg"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "mit zierlichen, zinnernen Truppen.", "tokens": ["mit", "zier\u00b7li\u00b7chen", ",", "zin\u00b7ner\u00b7nen", "Trup\u00b7pen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da h\u00f6rt' er 'ne Trommel! Fuhr auf und schwieg,", "tokens": ["Da", "h\u00f6rt'", "er", "'ne", "Trom\u00b7mel", "!", "Fuhr", "auf", "und", "schwieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$.", "VVFIN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "in den Ofen warf er die Puppen:", "tokens": ["in", "den", "O\u00b7fen", "warf", "er", "die", "Pup\u00b7pen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Und sah mit Augen k\u00fchn und stolz,", "tokens": ["Und", "sah", "mit", "Au\u00b7gen", "k\u00fchn", "und", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "V