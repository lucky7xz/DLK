{"dta.poem.5878": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und H\u00e4user machen will, zu ihrem Wercke nimmt,", "tokens": ["Und", "H\u00e4u\u00b7ser", "ma\u00b7chen", "will", ",", "zu", "ih\u00b7rem", "Wer\u00b7cke", "nimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so verschiedene Figur von Stein dazu bestimmt,", "tokens": ["Und", "so", "ver\u00b7schie\u00b7de\u00b7ne", "Fi\u00b7gur", "von", "Stein", "da\u00b7zu", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "APPR", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Die all\u2019 aus einem Stein gehauen.", "tokens": ["Die", "all'", "aus", "ei\u00b7nem", "Stein", "ge\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}