{"textgrid.poem.48466": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "40. F\u00fcrstl. Holstein. Rats und Abgesandten Namenstag", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir hofften g\u00e4nzlich zwar, di\u00df eingelegte Band", "tokens": ["Wir", "hoff\u00b7ten", "g\u00e4nz\u00b7lich", "zwar", ",", "di\u00df", "ein\u00b7ge\u00b7leg\u00b7te", "Band"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,", "PDS", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "euch selbst zu binden hier um euer' edel' Hand,", "tokens": ["euch", "selbst", "zu", "bin\u00b7den", "hier", "um", "eu\u00b7er'", "e\u00b7del'", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "o wertester Patron! Wir waren schon gesonnen,", "tokens": ["o", "wer\u00b7tes\u00b7ter", "Pat\u00b7ron", "!", "Wir", "wa\u00b7ren", "schon", "ge\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "was wir f\u00fcr Lustigkeit, f\u00fcr Freuden und f\u00fcr Wonnen", "tokens": ["was", "wir", "f\u00fcr", "Lus\u00b7tig\u00b7keit", ",", "f\u00fcr", "Freu\u00b7den", "und", "f\u00fcr", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "auf diesen sch\u00f6nen Tag uns wolten nehmen f\u00fcr.", "tokens": ["auf", "die\u00b7sen", "sch\u00f6\u00b7nen", "Tag", "uns", "wol\u00b7ten", "neh\u00b7men", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PPER", "VMFIN", "VVINF", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein Trauren, kein Verdru\u00df, der solte sein allhier.", "tokens": ["Kein", "Trau\u00b7ren", ",", "kein", "Ver\u00b7dru\u00df", ",", "der", "sol\u00b7te", "sein", "all\u00b7hier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PRELS", "VMFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die liebliche Musik, die war bei uns geflie\u00dfen,", "tokens": ["Die", "lieb\u00b7li\u00b7che", "Mu\u00b7sik", ",", "die", "war", "bei", "uns", "ge\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "euch und auch euren Tag mit Freuden zu begr\u00fc\u00dfen.", "tokens": ["euch", "und", "auch", "eu\u00b7ren", "Tag", "mit", "Freu\u00b7den", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ADV", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein ieder unter uns, der hatte sich bedacht,", "tokens": ["Ein", "ie\u00b7der", "un\u00b7ter", "uns", ",", "der", "hat\u00b7te", "sich", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPER", "$,", "PRELS", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "was er f\u00fcr W\u00fcndsche wolt' euch haben vorgebracht,", "tokens": ["was", "er", "f\u00fcr", "W\u00fcnd\u00b7sche", "wolt'", "euch", "ha\u00b7ben", "vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VMFIN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "im Fall' ihr w\u00e4ret da. Der wolt' euch Nestors Leben,", "tokens": ["im", "Fall'", "ihr", "w\u00e4\u00b7ret", "da", ".", "Der", "wolt'", "euch", "Nes\u00b7tors", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VAFIN", "ADV", "$.", "PDS", "VMFIN", "PPER", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "der Kr\u00f6sus Gold und Gut in seinem Wundsche geben,", "tokens": ["der", "Kr\u00f6\u00b7sus", "Gold", "und", "Gut", "in", "sei\u00b7nem", "Wund\u00b7sche", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "der bat euch C\u00e4sars Gl\u00fcck und Katons Weisheit aus,", "tokens": ["der", "bat", "euch", "C\u00e4\u00b7sars", "Gl\u00fcck", "und", "Ka\u00b7tons", "Weis\u00b7heit", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "NE", "NN", "KON", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "da\u00df ihr das hohe Werk wol m\u00f6chtet f\u00fchrn hinaus,", "tokens": ["da\u00df", "ihr", "das", "ho\u00b7he", "Werk", "wol", "m\u00f6ch\u00b7tet", "f\u00fchrn", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "VMFIN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "wie t\u00e4glich denn geschieht. Nun geht es ganz zur\u00fccke.", "tokens": ["wie", "t\u00e4g\u00b7lich", "denn", "ge\u00b7schieht", ".", "Nun", "geht", "es", "ganz", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wir haben mehr gehofft, als unsrer keines Gl\u00fccke", "tokens": ["Wir", "ha\u00b7ben", "mehr", "ge\u00b7hofft", ",", "als", "uns\u00b7rer", "kei\u00b7nes", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "PPOSAT", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "zu geben ist vermocht, so da\u00df noch keiner kan", "tokens": ["zu", "ge\u00b7ben", "ist", "ver\u00b7mocht", ",", "so", "da\u00df", "noch", "kei\u00b7ner", "kan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VAFIN", "VVPP", "$,", "ADV", "KOUS", "ADV", "PIS", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "auf euren lieben Tag euch m\u00fcndlich reden an.", "tokens": ["auf", "eu\u00b7ren", "lie\u00b7ben", "Tag", "euch", "m\u00fcnd\u00b7lich", "re\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Seid gleichwol tausentmal und tausentmal gegr\u00fc\u00dfet", "tokens": ["Seid", "gleich\u00b7wol", "tau\u00b7sent\u00b7mal", "und", "tau\u00b7sent\u00b7mal", "ge\u00b7gr\u00fc\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADV", "ADV", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "von euern Treuen, uns, ob ihr gleich ab sein m\u00fcsset!", "tokens": ["von", "eu\u00b7ern", "Treu\u00b7en", ",", "uns", ",", "ob", "ihr", "gleich", "ab", "sein", "m\u00fcs\u00b7set", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "$,", "KOUS", "PPER", "ADV", "PTKVZ", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.21": {"text": "Daher wir denn annoch nicht ohne Sorgen stehn,", "tokens": ["Da\u00b7her", "wir", "denn", "an\u00b7noch", "nicht", "oh\u00b7ne", "Sor\u00b7gen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADV", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "ob es euch anders auch als m\u00f6ge wol ergehn.", "tokens": ["ob", "es", "euch", "an\u00b7ders", "auch", "als", "m\u00f6\u00b7ge", "wol", "er\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "KOUS", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gott geb' euch, was ihr wollt und was ihr werdet wollen,", "tokens": ["Gott", "geb'", "euch", ",", "was", "ihr", "wollt", "und", "was", "ihr", "wer\u00b7det", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "KON", "PWS", "PPER", "VAFIN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Gott geb' euch, was auch wir euch s\u00e4mtlich w\u00fcndschen sollen", "tokens": ["Gott", "geb'", "euch", ",", "was", "auch", "wir", "euch", "s\u00e4mt\u00b7lich", "w\u00fcnd\u00b7schen", "sol\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "PRELS", "ADV", "PPER", "PPER", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "auf heut' und allezeit! Itzt aber w\u00fcndschen wir,", "tokens": ["auf", "heut'", "und", "al\u00b7le\u00b7zeit", "!", "Itzt", "a\u00b7ber", "w\u00fcnd\u00b7schen", "wir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "$.", "ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "da\u00df ihr auf diesen Tag noch m\u00f6get sein allhier.", "tokens": ["da\u00df", "ihr", "auf", "die\u00b7sen", "Tag", "noch", "m\u00f6\u00b7get", "sein", "all\u00b7hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "ADV", "VMFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Itzt k\u00f6nnen wir mehr nicht, als mit Gedanken binden:", "tokens": ["Itzt", "k\u00f6n\u00b7nen", "wir", "mehr", "nicht", ",", "als", "mit", "Ge\u00b7dan\u00b7ken", "bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "kommt ihr, so sollt ihr noch mehr W\u00fcndsche vor euch finden!", "tokens": ["kommt", "ihr", ",", "so", "sollt", "ihr", "noch", "mehr", "W\u00fcnd\u00b7sche", "vor", "euch", "fin\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VMFIN", "PPER", "ADV", "PIAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wir hofften g\u00e4nzlich zwar, di\u00df eingelegte Band", "tokens": ["Wir", "hoff\u00b7ten", "g\u00e4nz\u00b7lich", "zwar", ",", "di\u00df", "ein\u00b7ge\u00b7leg\u00b7te", "Band"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,", "PDS", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "euch selbst zu binden hier um euer' edel' Hand,", "tokens": ["euch", "selbst", "zu", "bin\u00b7den", "hier", "um", "eu\u00b7er'", "e\u00b7del'", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "o wertester Patron! Wir waren schon gesonnen,", "tokens": ["o", "wer\u00b7tes\u00b7ter", "Pat\u00b7ron", "!", "Wir", "wa\u00b7ren", "schon", "ge\u00b7son\u00b7nen", ","], "token_inf