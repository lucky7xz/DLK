{"textgrid.poem.41839": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00dft folgen mich der Sonne Winken,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft folgen mich der Sonne Winken,", "tokens": ["La\u00dft", "fol\u00b7gen", "mich", "der", "Son\u00b7ne", "Win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Pfingstenrufe der Natur,", "tokens": ["Dem", "Pfings\u00b7ten\u00b7ru\u00b7fe", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00dft mich des Daseins Wonne trinken,", "tokens": ["La\u00dft", "mich", "des", "Da\u00b7seins", "Won\u00b7ne", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Luft des himmlischen Azur.", "tokens": ["Die", "Luft", "des", "himm\u00b7li\u00b7schen", "A\u00b7zur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00dft mich dem hohen Ruf gehorchen,", "tokens": ["La\u00dft", "mich", "dem", "ho\u00b7hen", "Ruf", "ge\u00b7hor\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der noch aus jeder Bl\u00fcte sprach,", "tokens": ["Der", "noch", "aus", "je\u00b7der", "Bl\u00fc\u00b7te", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus Lerchenlied am Pfingstenmorgen", "tokens": ["Aus", "Ler\u00b7chen\u00b7lied", "am", "Pfings\u00b7ten\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einem Hauch: ", "tokens": ["In", "ei\u00b7nem", "Hauch", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der Sonne nach: \u2013 O wie ihr Walten", "tokens": ["Der", "Son\u00b7ne", "nach", ":", "\u2013", "O", "wie", "ihr", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "$(", "NE", "KOKOM", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Den dunklen Himmelsdom durchbricht,", "tokens": ["Den", "dunk\u00b7len", "Him\u00b7mels\u00b7dom", "durch\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein neues Leben zu entfalten", "tokens": ["Ein", "neu\u00b7es", "Le\u00b7ben", "zu", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Sch\u00f6pfungswort: es werde Licht!", "tokens": ["Im", "Sch\u00f6p\u00b7fungs\u00b7wort", ":", "es", "wer\u00b7de", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und alles bl\u00fct und alles singet", "tokens": ["Und", "al\u00b7les", "bl\u00fct", "und", "al\u00b7les", "sin\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gr\u00fc\u00dft den heil'gen Pfingstentag,", "tokens": ["Und", "gr\u00fc\u00dft", "den", "heil'\u00b7gen", "Pfings\u00b7ten\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der neues Licht und Leben bringet,", "tokens": ["Der", "neu\u00b7es", "Licht", "und", "Le\u00b7ben", "brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles dr\u00e4ngt der Sonne nach.", "tokens": ["Und", "al\u00b7les", "dr\u00e4ngt", "der", "Son\u00b7ne", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr strebt die Lerche froh entgegen,", "tokens": ["Ihr", "strebt", "die", "Ler\u00b7che", "froh", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die zwitschernd hebt ihr Schwingenpaar,", "tokens": ["Die", "zwit\u00b7schernd", "hebt", "ihr", "Schwin\u00b7gen\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu ihr mit k\u00fchnen Fl\u00fcgelschl\u00e4gen", "tokens": ["Zu", "ihr", "mit", "k\u00fch\u00b7nen", "Fl\u00fc\u00b7gel\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Steigt stolz empor der junge Aar.", "tokens": ["Steigt", "stolz", "em\u00b7por", "der", "jun\u00b7ge", "Aar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und rings vom Pfingstenruf durchgl\u00fchet", "tokens": ["Und", "rings", "vom", "Pfings\u00b7ten\u00b7ruf", "durch\u00b7gl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dr\u00e4ngt alles zu dem Licht hervor,", "tokens": ["Dr\u00e4ngt", "al\u00b7les", "zu", "dem", "Licht", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo nur ein Sonnenfunken spr\u00fchet", "tokens": ["Wo", "nur", "ein", "Son\u00b7nen\u00b7fun\u00b7ken", "spr\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Klingt auch der Ruf: Empor! empor!", "tokens": ["Klingt", "auch", "der", "Ruf", ":", "Em\u00b7por", "!", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "NN", "$.", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So wird des Geistes Ruf vernommen,", "tokens": ["So", "wird", "des", "Geis\u00b7tes", "Ruf", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der alle Wesen aufw\u00e4rts zieht,", "tokens": ["Der", "al\u00b7le", "We\u00b7sen", "auf\u00b7w\u00e4rts", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist auch mir, auch mir gekommen,", "tokens": ["Er", "ist", "auch", "mir", ",", "auch", "mir", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Empor mein Aug', empor mein Lied!", "tokens": ["Em\u00b7por", "mein", "Aug'", ",", "em\u00b7por", "mein", "Lied", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Empor mein Sinnen und mein Denken,", "tokens": ["Em\u00b7por", "mein", "Sin\u00b7nen", "und", "mein", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sonne nach, dem Lichte zu!", "tokens": ["Der", "Son\u00b7ne", "nach", ",", "dem", "Lich\u00b7te", "zu", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und will die Erde dich beschr\u00e4nken", "tokens": ["Und", "will", "die", "Er\u00b7de", "dich", "be\u00b7schr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wage h\u00f6h'ren Flug auch du!", "tokens": ["So", "wa\u00b7ge", "h\u00f6h'\u00b7ren", "Flug", "auch", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wag ihn und sink ans Herz der Sonnen,", "tokens": ["Wag", "ihn", "und", "sink", "ans", "Herz", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus dem die Gottheit zu dir sprach:", "tokens": ["Aus", "dem", "die", "Got\u00b7theit", "zu", "dir", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Menschheit Heil wird nur gewonnen,", "tokens": ["Der", "Menschheit", "Heil", "wird", "nur", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Strebt sie empor \u2013 der Sonne nach!", "tokens": ["Strebt", "sie", "em\u00b7por", "\u2013", "der", "Son\u00b7ne", "nach", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$(", "ART", "NN", "PTKVZ", "$."], "meter": "-+