], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es traurt von Weh durchbohrt,", "tokens": ["Es", "traurt", "von", "Weh", "durch\u00b7bohrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwarz, Rot und Golden senken", "tokens": ["Schwarz", ",", "Rot", "und", "Gol\u00b7den", "sen\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Fahnen, schwarz umflort:", "tokens": ["Die", "Fah\u00b7nen", ",", "schwarz", "um\u00b7flort", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn eines Helden Leiche", "tokens": ["Denn", "ei\u00b7nes", "Hel\u00b7den", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00e4hrt hin zu anderm Staub,", "tokens": ["F\u00e4hrt", "hin", "zu", "an\u00b7derm", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihm trug die deutsche Eiche", "tokens": ["Ihm", "trug", "die", "deut\u00b7sche", "Ei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vergebens Siegeslaub.", "tokens": ["Ver\u00b7ge\u00b7bens", "Sie\u00b7ges\u00b7laub", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vergebens? wie? vergebens?", "tokens": ["Ver\u00b7ge\u00b7bens", "?", "wie", "?", "ver\u00b7ge\u00b7bens", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$.", "PWAV", "$.", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O nein! und aber nein!", "tokens": ["O", "nein", "!", "und", "a\u00b7ber", "nein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "KON", "ADV", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verh\u00fct' es, Herr des Lebens!", "tokens": ["Ver\u00b7h\u00fct'", "es", ",", "Herr", "des", "Le\u00b7bens", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei Gott! das soll nicht sein!", "tokens": ["Bei", "Gott", "!", "das", "soll", "nicht", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "PDS", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er fiel im guten Streite,", "tokens": ["Er", "fiel", "im", "gu\u00b7ten", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Er fiel f\u00fcrs Vaterland,", "tokens": ["Er", "fiel", "f\u00fcrs", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Durchlebt der Zeiten Weite,", "tokens": ["Durch\u00b7lebt", "der", "Zei\u00b7ten", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein Name wird nicht Sand.", "tokens": ["Sein", "Na\u00b7me", "wird", "nicht", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sein Klang ist der der magern,", "tokens": ["Sein", "Klang", "ist", "der", "der", "ma\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der kahlen Namen nicht,", "tokens": ["Der", "kah\u00b7len", "Na\u00b7men", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Friedrich Balduin von Gagern", "tokens": ["Fried\u00b7rich", "Bal\u00b7du\u00b7in", "von", "Ga\u00b7gern"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Verf\u00e4llt dem Dunkel nicht:", "tokens": ["Ver\u00b7f\u00e4llt", "dem", "Dun\u00b7kel", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er wird im Liede klingen,", "tokens": ["Er", "wird", "im", "Lie\u00b7de", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo ja als Schwur erklingt,", "tokens": ["Wo", "ja", "als", "Schwur", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "KOKOM", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Solange deutschen Klingen", "tokens": ["So\u00b7lan\u00b7ge", "deut\u00b7schen", "Klin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein guter Streit gelingt.", "tokens": ["Ein", "gu\u00b7ter", "Streit", "ge\u00b7lingt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "So zieh denn, Heldenleiche,", "tokens": ["So", "zieh", "denn", ",", "Hel\u00b7den\u00b7lei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zieh hin zur dunkeln Gruft,", "tokens": ["Zieh", "hin", "zur", "dun\u00b7keln", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Ha\u00df und Zwietracht weiche", "tokens": ["Und", "Ha\u00df", "und", "Zwiet\u00b7racht", "wei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus reiner deutscher Luft!", "tokens": ["Aus", "rei\u00b7ner", "deut\u00b7scher", "Luft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es flieh' von deutschen Grenzen", "tokens": ["Es", "flieh'", "von", "deut\u00b7schen", "Gren\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verrat und Untreu' fern!", "tokens": ["Ver\u00b7rat", "und", "Un\u00b7treu'",