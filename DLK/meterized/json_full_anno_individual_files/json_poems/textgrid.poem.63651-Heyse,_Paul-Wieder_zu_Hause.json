{"textgrid.poem.63651": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Wieder zu Hause", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und weiter braust das Leben,", "tokens": ["Und", "wei\u00b7ter", "braust", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du aber liegst so still.", "tokens": ["Du", "a\u00b7ber", "liegst", "so", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Stimmen klingen munter;", "tokens": ["Viel", "Stim\u00b7men", "klin\u00b7gen", "mun\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu dir, zu dir hinunter", "tokens": ["Zu", "dir", ",", "zu", "dir", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht eine dringen will.", "tokens": ["Nicht", "ei\u00b7ne", "drin\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O helles Bl\u00fchn und Gr\u00fcnen!", "tokens": ["O", "hel\u00b7les", "Bl\u00fchn", "und", "Gr\u00fc\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Fr\u00fchlings\u00fcberschwang!", "tokens": ["O", "Fr\u00fch\u00b7lings\u00b7\u00fc\u00b7bersc\u00b7hwang", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Nur deine zarten Glieder", "tokens": ["Nur", "dei\u00b7ne", "zar\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "W\u00e4rmt keine Sonne wieder,", "tokens": ["W\u00e4rmt", "kei\u00b7ne", "Son\u00b7ne", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Belebt kein Vogelsang!", "tokens": ["Be\u00b7lebt", "kein", "Vo\u00b7gel\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Amseln, die dich fr\u00fche", "tokens": ["Die", "Am\u00b7seln", ",", "die", "dich", "fr\u00fc\u00b7he"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geweckt mit Zwitscherton,", "tokens": ["Ge\u00b7weckt", "mit", "Zwit\u00b7scher\u00b7ton", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie f\u00fcttern neue Kleinen;", "tokens": ["Sie", "f\u00fct\u00b7tern", "neu\u00b7e", "Klei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich schaue zu mit Weinen \u2013", "tokens": ["Ich", "schau\u00b7e", "zu", "mit", "Wei\u00b7nen", "\u2013"], "token_info": ["word", "word", "word