{"dta.poem.4435": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Ehe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und sich bem\u00fcht, sie zu vertragen.)", "tokens": ["Und", "sich", "be\u00b7m\u00fcht", ",", "sie", "zu", "ver\u00b7tra\u00b7gen", ".", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PRF", "VVPP", "$,", "PPER", "PTKZU", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gelegenheit, aufs neu zu zanken und zu klagen!", "tokens": ["Ge\u00b7le\u00b7gen\u00b7heit", ",", "aufs", "neu", "zu", "zan\u00b7ken", "und", "zu", "kla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Frau murr\u2019t allezeit", "tokens": ["Die", "Frau", "murr't", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit ihrem Mann\u2019, und schm\u00e4let,", "tokens": ["Mit", "ih\u00b7rem", "Mann'", ",", "und", "schm\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df er, ohn\u2019 alle Lieb\u2019 und Z\u00e4rtlichkeit", "tokens": ["Da\u00df", "er", ",", "ohn'", "al\u00b7le", "Lieb'", "und", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUI", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie nicht beklag\u2019t, wie sehr die Schwangerschaft sie qu\u00e4let,", "tokens": ["Sie", "nicht", "be\u00b7klag't", ",", "wie", "sehr", "die", "Schwan\u00b7ger\u00b7schaft", "sie", "qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die ihm gleichwol so Tag als Nacht", "tokens": ["Die", "ihm", "gleich\u00b7wol", "so", "Tag", "als", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "NN", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verdru\u00df und Eckel macht.", "tokens": ["Ver\u00b7dru\u00df", "und", "E\u00b7ckel", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wenn sie nun beide gar, voll Eifersucht zumal,", "tokens": ["Wenn", "sie", "nun", "bei\u00b7de", "gar", ",", "voll", "Ei\u00b7fer\u00b7sucht", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "ADV", "$,", "ADJD", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit Gall\u2019 und Bitterkeit sich n\u00e4hren;", "tokens": ["Mit", "Gall'", "und", "Bit\u00b7ter\u00b7keit", "sich", "n\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Was ist das f\u00fcr ein Creuz! welch eine H\u00f6llen-Qual!", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "Creuz", "!", "welch", "ei\u00b7ne", "H\u00f6l\u00b7len\u00b7Qual", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$.", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist nun der Ehestand, da er so sehr beschwehrt,", "tokens": ["Ist", "nun", "der", "E\u00b7hes\u00b7tand", ",", "da", "er", "so", "sehr", "be\u00b7schwehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wol, da\u00df man sich dazu so \u00e4ngstlich dr\u00e4nge, wehrt?", "tokens": ["Wol", ",", "da\u00df", "man", "sich", "da\u00b7zu", "so", "\u00e4ngst\u00b7lich", "dr\u00e4n\u00b7ge", ",", "wehrt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "PRF", "PAV", "ADV", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "Von dieser Ahrt jedoch sind, leider! wie wir sehen,", "tokens": ["Von", "die\u00b7ser", "Ahrt", "je\u00b7doch", "sind", ",", "lei\u00b7der", "!", "wie", "wir", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VAFIN", "$,", "ADV", "$.", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die meisten Ehen.", "tokens": ["Die", "meis\u00b7ten", "E\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}