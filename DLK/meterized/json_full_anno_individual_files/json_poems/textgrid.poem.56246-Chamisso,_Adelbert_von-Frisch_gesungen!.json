{"textgrid.poem.56246": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Frisch gesungen!", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab oft im Kreise der Lieben", "tokens": ["Hab", "oft", "im", "Krei\u00b7se", "der", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In duftigem Grase geruht,", "tokens": ["In", "duf\u00b7ti\u00b7gem", "Gra\u00b7se", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und mir ein Liedlein gesungen,", "tokens": ["Und", "mir", "ein", "Lied\u00b7lein", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und alles war h\u00fcbsch und gut.", "tokens": ["Und", "al\u00b7les", "war", "h\u00fcbsch", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Hab einsam auch mich geh\u00e4rmet", "tokens": ["Hab", "ein\u00b7sam", "auch", "mich", "ge\u00b7h\u00e4r\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADV", "PPER", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In bangem d\u00fcsterem Mut,", "tokens": ["In", "ban\u00b7gem", "d\u00fcs\u00b7te\u00b7rem", "Mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR"