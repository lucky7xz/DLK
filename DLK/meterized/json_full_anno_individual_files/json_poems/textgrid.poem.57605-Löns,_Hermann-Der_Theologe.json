{"textgrid.poem.57605": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Theologe", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sa\u00dfen stets auf einer Bank zusammen,", "tokens": ["Wir", "sa\u00b7\u00dfen", "stets", "auf", "ei\u00b7ner", "Bank", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schrieben treulich voneinander ab,", "tokens": ["Und", "schrie\u00b7ben", "treu\u00b7lich", "von\u00b7ein\u00b7an\u00b7der", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleichzeitig sengten uns die Liebesflammen,", "tokens": ["Gleich\u00b7zei\u00b7tig", "seng\u00b7ten", "uns", "die", "Lie\u00b7bes\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Gleichzeitig sank der Liebestraum ins Grab;", "tokens": ["Gleich\u00b7zei\u00b7tig", "sank", "der", "Lie\u00b7best\u00b7raum", "ins", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir teilten Bude, Geld und Taschent\u00fccher", "tokens": ["Wir", "teil\u00b7ten", "Bu\u00b7de", ",", "Geld", "und", "Ta\u00b7schen\u00b7t\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und logen f\u00fcreinander in der Not,", "tokens": ["Und", "lo\u00b7gen", "f\u00fc\u00b7rein\u00b7an\u00b7der", "in", "der", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn uns f\u00fcr Kneiperei und Ketzerb\u00fccher", "tokens": ["Wenn", "uns", "f\u00fcr", "Knei\u00b7pe\u00b7rei", "und", "Ket\u00b7zer\u00b7b\u00fc\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Karzer oder das Konsil gedroht \u2013", "tokens": ["Der", "Kar\u00b7zer", "o\u00b7der", "das", "Kon\u00b7sil", "ge\u00b7droht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Stets hab' ich deinen stolzen Geist verehrt,", "tokens": ["Stets", "hab'", "ich", "dei\u00b7nen", "stol\u00b7zen", "Geist", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auch heut noch bist du mir bewundernswert.", "tokens": ["Auch", "heut", "noch", "bist", "du", "mir", "be\u00b7wun\u00b7derns\u00b7wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbgymnasium, du Seelenfolterzelle!\u00ab", "tokens": ["\u00bb", "gym\u00b7na\u00b7si\u00b7um", ",", "du", "See\u00b7len\u00b7fol\u00b7ter\u00b7zel\u00b7le", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "$,", "PPER", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So fluchtest du. \u00bbNur noch ein halbes Jahr,", "tokens": ["So", "fluch\u00b7test", "du", ".", "\u00bb", "Nur", "noch", "ein", "hal\u00b7bes", "Jahr", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann sch\u00e4umt um mich die bunte Lebenswelle,", "tokens": ["Dann", "sch\u00e4umt", "um", "mich", "die", "bun\u00b7te", "Le\u00b7bens\u00b7wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "K\u00fchn tret' ich an der Freiheit Hochaltar,", "tokens": ["K\u00fchn", "tret'", "ich", "an", "der", "Frei\u00b7heit", "Hoc\u00b7hal\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und jauchzend werf ich in das Freudenfeuer", "tokens": ["Und", "jauch\u00b7zend", "werf", "ich", "in", "das", "Freu\u00b7den\u00b7feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Kette der erzwungnen Heuchelei,", "tokens": ["Die", "Ket\u00b7te", "der", "er\u00b7zwung\u00b7nen", "Heu\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann fest gepackt des Lebenskahnes Steuer,", "tokens": ["Dann", "fest", "ge\u00b7packt", "des", "Le\u00b7bens\u00b7kah\u00b7nes", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Segel los, und frei bin ich, bin frei!\u00ab \u2013", "tokens": ["Das", "Se\u00b7gel", "los", ",", "und", "frei", "bin", "ich", ",", "bin", "frei", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "ADJD", "VAFIN", "PPER", "$,", "VAFIN", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Begeistert hab' ich oft dir zugeh\u00f6rt,", "tokens": ["Be\u00b7geis\u00b7tert", "hab'", "ich", "oft", "dir", "zu\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auch heut noch bist du mir bewundernswert.", "tokens": ["Auch", "heut", "noch", "bist", "du", "mir", "be\u00b7wun\u00b7derns\u00b7wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie bleich warst du, wie knirschten deine Z\u00e4hne,", "tokens": ["Wie", "bleich", "warst", "du", ",", "wie", "knirschten", "dei\u00b7ne", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn vor den Beichtstuhl uns der Schulzwang stie\u00df,", "tokens": ["Wenn", "vor", "den", "Beicht\u00b7stuhl", "uns", "der", "Schulz\u00b7wang", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie schwoll am Hals dir Aderstrang und Sehne,", "tokens": ["Wie", "schwoll", "am", "Hals", "dir", "A\u00b7der\u00b7strang", "und", "Seh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn dich der Lehrer Lump und L\u00fcmmel hie\u00df \u2013", "tokens": ["Wenn", "dich", "der", "Leh\u00b7rer", "Lump", "und", "L\u00fcm\u00b7mel", "hie\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NE", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Kette ri\u00df, es kam die Freiheitsstunde,", "tokens": ["Die", "Ket\u00b7te", "ri\u00df", ",", "es", "kam", "die", "Frei\u00b7heits\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du sangst das Hohelied vom Mannesruhm,", "tokens": ["Du", "sangst", "das", "Ho\u00b7he\u00b7lied", "vom", "Man\u00b7nes\u00b7ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und feurig sprachst du in der Freunde Runde", "tokens": ["Und", "feu\u00b7rig", "sprachst", "du", "in", "der", "Freun\u00b7de", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Von Wahrheit, Licht und freiem Menschentum \u2013", "tokens": ["Von", "Wahr\u00b7heit", ",", "Licht", "und", "frei\u00b7em", "Men\u00b7schen\u00b7tum", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Begeistert habe ich dir zugeh\u00f6rt,", "tokens": ["Be\u00b7geis\u00b7tert", "ha\u00b7be", "ich", "dir", "zu\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Auch heut noch bist du mir bewundernswert.", "tokens": ["Auch", "heut", "noch", "bist", "du", "mir", "be\u00b7wun\u00b7derns\u00b7wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und gestern hast du dein Primiz gelesen,", "tokens": ["Und", "ge\u00b7stern", "hast", "du", "dein", "Pri\u00b7miz", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Woher kam dir so pl\u00f6tzlich der Beruf?", "tokens": ["Wo\u00b7her", "kam", "dir", "so", "pl\u00f6tz\u00b7lich", "der", "Be\u00b7ruf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Paulusruf von oben ist's gewesen,", "tokens": ["Kein", "Pau\u00b7lus\u00b7ruf", "von", "o\u00b7ben", "ist's", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADV", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der aus dem Sp\u00f6tter einen Priester schuf;", "tokens": ["Der", "aus", "dem", "Sp\u00f6t\u00b7ter", "ei\u00b7nen", "Pries\u00b7ter", "schuf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dein Ohm ist Bischof \u2013 eine fette Pfr\u00fcnde,", "tokens": ["Dein", "Ohm", "ist", "Bi\u00b7schof", "\u2013", "ei\u00b7ne", "fet\u00b7te", "Pfr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$(", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Arbeitsscheu und freies Studium,", "tokens": ["Die", "Ar\u00b7beits\u00b7scheu", "und", "frei\u00b7es", "Stu\u00b7di\u00b7um", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Daf\u00fcr vergi\u00dft man Kampf mit Lug und S\u00fcnde", "tokens": ["Da\u00b7f\u00fcr", "ver\u00b7gi\u00dft", "man", "Kampf", "mit", "Lug", "und", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIS", "NN", "APPR", "NE", "KON", "NN"], "meter": 