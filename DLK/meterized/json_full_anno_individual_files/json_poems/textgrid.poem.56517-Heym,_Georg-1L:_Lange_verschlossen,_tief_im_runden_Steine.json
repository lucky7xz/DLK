{"textgrid.poem.56517": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lange verschlossen, tief im runden Steine", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange verschlossen, tief im runden Steine", "tokens": ["Lan\u00b7ge", "ver\u00b7schlos\u00b7sen", ",", "tief", "im", "run\u00b7den", "Stei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mit einem Trauerbaum und wenig Zweigen,", "tokens": ["Mit", "ei\u00b7nem", "Trau\u00b7er\u00b7baum", "und", "we\u00b7nig", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch dreht sie um den Hals den sanften Schleier", "tokens": ["Noch", "dreht", "sie", "um", "den", "Hals", "den", "sanf\u00b7ten", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und geht in leisem Tanz in stiller Feier.", "tokens": ["Und", "geht", "in", "lei\u00b7sem", "Tanz", "in", "stil\u00b7ler", "Fei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Immer noch fort, wo schon die G\u00f6tter starben", "tokens": ["Im\u00b7mer", "noch", "fort", ",", "wo", "schon", "die", "G\u00f6t\u00b7ter", "star\u00b7ben"], "token_info"