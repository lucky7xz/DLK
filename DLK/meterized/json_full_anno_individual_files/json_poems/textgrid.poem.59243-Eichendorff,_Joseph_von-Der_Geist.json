{"textgrid.poem.59243": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Geist", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4chtlich dehnen sich die Stunden,", "tokens": ["N\u00e4cht\u00b7lich", "deh\u00b7nen", "sich", "die", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unschuld schl\u00e4ft in stiller Bucht,", "tokens": ["Un\u00b7schuld", "schl\u00e4ft", "in", "stil\u00b7ler", "Bucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fernab ist die Welt verschwunden,", "tokens": ["Fern\u00b7ab", "ist", "die", "Welt", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die das Herz in Tr\u00e4umen sucht.", "tokens": ["Die", "das", "Herz", "in", "Tr\u00e4u\u00b7men", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und der Geist tritt auf die Zinne,", "tokens": ["Und", "der", "Geist", "tritt", "auf", "die", "Zin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und noch stiller wird's umher,", "tokens": ["Und", "noch", "stil\u00b7ler", "wird's