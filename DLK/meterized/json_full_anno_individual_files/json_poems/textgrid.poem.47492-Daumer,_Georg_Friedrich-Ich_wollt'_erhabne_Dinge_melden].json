ra"}, "line.23": {"text": "Du in Suleima's Locke z\u00e4hltest,", "tokens": ["Du", "in", "Su\u00b7lei\u00b7ma's", "Lo\u00b7cke", "z\u00e4hl\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Beschreibe deines holden Schenken", "tokens": ["Be\u00b7schrei\u00b7be", "dei\u00b7nes", "hol\u00b7den", "Schen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Rubinenlipp' und Schelmenauge", "tokens": ["Ru\u00b7bi\u00b7nen\u00b7lipp'", "und", "Schel\u00b7men\u00b7au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Dem lieben, alten Herrn der Schenke,", "tokens": ["Dem", "lie\u00b7ben", ",", "al\u00b7ten", "Herrn", "der", "Schen\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Dem w\u00fcrdigen und vielerfahrnen \u2013", "tokens": ["Dem", "w\u00fcr\u00b7di\u00b7gen", "und", "vie\u00b7ler\u00b7fahr\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "KON", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Sein treuer Sklave bin auch ich \u2013", "tokens": ["Sein", "treu\u00b7er", "Skla\u00b7ve", "bin", "auch", "ich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ein Compliment, denn er verdient es;", "tokens": ["Ein", "Com\u00b7pli\u00b7ment", ",", "denn", "er", "ver\u00b7dient", "es", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und hast du das zu hundert Malen,", "tokens": ["Und", "hast", "du", "das", "zu", "hun\u00b7dert", "Ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PDS", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Zu tausenden, gar sch\u00f6n verrichtet,", "tokens": ["Zu", "tau\u00b7sen\u00b7den", ",", "gar", "sch\u00f6n", "ver\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Verricht' es immer, immer wieder,", "tokens": ["Ver\u00b7richt'", "es", "im\u00b7mer", ",", "im\u00b7mer", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Dergleichen h\u00f6rt man immer gerne;", "tokens": ["Derg\u00b7lei\u00b7chen", "h\u00f6rt", "man", "im\u00b7mer", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Des Weiteren entschlage dich!", "tokens": ["Des", "Wei\u00b7te\u00b7ren", "ent\u00b7schla\u00b7ge", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}