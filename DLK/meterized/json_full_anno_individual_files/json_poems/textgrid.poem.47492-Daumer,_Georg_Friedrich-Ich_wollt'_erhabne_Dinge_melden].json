{"textgrid.poem.47492": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ich wollt' erhabne Dinge melden]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt' erhabne Dinge melden;", "tokens": ["Ich", "wollt'", "er\u00b7hab\u00b7ne", "Din\u00b7ge", "mel\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00e4rtyrer des wahren Glaubens", "tokens": ["Die", "M\u00e4r\u00b7ty\u00b7rer", "des", "wah\u00b7ren", "Glau\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lobpreisen im Gesange wollt' ich;", "tokens": ["Lob\u00b7prei\u00b7sen", "im", "Ge\u00b7san\u00b7ge", "wollt'", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "All ihre Thaten, ihre Wunden,", "tokens": ["All", "ih\u00b7re", "Tha\u00b7ten", ",", "ih\u00b7re", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die heiligen und gloriosen,", "tokens": ["Die", "hei\u00b7li\u00b7gen", "und", "glo\u00b7ri\u00b7o\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wollt' ich besingen hell und hehr.", "tokens": ["Wollt'", "ich", "be\u00b7sin\u00b7gen", "hell", "und", "hehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu H\u00fclfe bei so gro\u00dfem Werke", "tokens": ["Zu", "H\u00fcl\u00b7fe", "bei", "so", "gro\u00b7\u00dfem", "Wer\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Rief ich den Ost, der um die Rosen", "tokens": ["Rief", "ich", "den", "Ost", ",", "der", "um", "die", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und Veilchen im Gefilde buhlte,", "tokens": ["Und", "Veil\u00b7chen", "im", "Ge\u00b7fil\u00b7de", "buhl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und bat ihn um die Wunderkunden,", "tokens": ["Und", "bat", "ihn", "um", "die", "Wun\u00b7der\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die im Betreffe der besagten", "tokens": ["Die", "im", "Be\u00b7tref\u00b7fe", "der", "be\u00b7sag\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Materien sein Geist bewahre;", "tokens": ["Ma\u00b7te\u00b7ri\u00b7en", "sein", "Geist", "be\u00b7wah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.13": {"text": "Doch \u00fcbel unterst\u00fctzte der.", "tokens": ["Doch", "\u00fc\u00b7bel", "un\u00b7ter\u00b7st\u00fctz\u00b7te", "der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Er sprach: \u00bbHafis, wir beide sind", "tokens": ["Er", "sprach", ":", "\u00bb", "Ha\u00b7fis", ",", "wir", "bei\u00b7de", "sind"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "NE", "$,", "PPER", "PIS", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "F\u00fcr solche Dinge nicht geschaffen;", "tokens": ["F\u00fcr", "sol\u00b7che", "Din\u00b7ge", "nicht", "ge\u00b7schaf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Dazu geh\u00f6ren andre Helden,", "tokens": ["Da\u00b7zu", "ge\u00b7h\u00f6\u00b7ren", "and\u00b7re", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Denn unser Wesen ist zu luftig", "tokens": ["Denn", "un\u00b7ser", "We\u00b7sen", "ist", "zu", "luf\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und jene Dinge sind zu schwer.", "tokens": ["Und", "je\u00b7ne", "Din\u00b7ge", "sind", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Erz\u00e4hle du die Heroismen,", "tokens": ["Er\u00b7z\u00e4h\u00b7le", "du", "die", "He\u00b7ro\u00b7is\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Die du gethan beim Weinpokale,", "tokens": ["Die", "du", "ge\u00b7than", "beim", "Wein\u00b7po\u00b7ka\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Besch\u00e4ftige dich mit s\u00fc\u00dfem Tande,", "tokens": ["Be\u00b7sch\u00e4f\u00b7ti\u00b7ge", "dich", "mit", "s\u00fc\u00b7\u00dfem", "Tan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Berichte, wie viel Seelen-Angeln", "tokens": ["Be\u00b7rich\u00b7te", ",", "wie", "viel", "See\u00b7len\u00b7An\u00b7geln"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Du in Suleima's Locke z\u00e4hltest,", "tokens": ["Du", "in", "Su\u00b7lei\u00b7ma's", "Lo\u00b7cke", "z\u00e4hl\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Beschreibe deines holden Schenken", "tokens": ["Be\u00b7schrei\u00b7be", "dei\u00b7nes", "hol\u00b7den", "Schen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Rubinenlipp' und Schelmenauge", "tokens": ["Ru\u00b7bi\u00b7nen\u00b7lipp'", "und", "Schel\u00b7men\u00b7au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Dem lieben, alten Herrn der Schenke,", "tokens": ["Dem", "lie\u00b7ben", ",", "al\u00b7ten", "Herrn", "der", "Schen\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Dem w\u00fcrdigen und vielerfahrnen \u2013", "tokens": ["Dem", "w\u00fcr\u00b7di\u00b7gen", "und", "vie\u00b7ler\u00b7fahr\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "KON", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Sein treuer Sklave bin auch ich \u2013", "tokens": ["Sein", "treu\u00b7er", "Skla\u00b7ve", "bin", "auch", "ich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ein Compliment, denn er verdient es;", "tokens": ["Ein", "Com\u00b7pli\u00b7ment", ",", "denn", "er", "ver\u00b7dient", "es", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Und hast du das zu hundert Malen,", "tokens": ["Und", "hast", "du", "das", "zu", "hun\u00b7dert", "Ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PDS", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Zu tausenden, gar sch\u00f6n verrichtet,", "tokens": ["Zu", "tau\u00b7sen\u00b7den", ",", "gar", "sch\u00f6n", "ver\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Verricht' es immer, immer wieder,", "tokens": ["Ver\u00b7richt'", "es", "im\u00b7mer", ",", "im\u00b7mer", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Dergleichen h\u00f6rt man immer gerne;", "tokens": ["Derg\u00b7lei\u00b7chen", "h\u00f6rt", "man", "im\u00b7mer", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Des Weiteren entschlage dich!", "tokens": ["Des", "Wei\u00b7te\u00b7ren", "ent\u00b7schla\u00b7ge", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich wollt' erhabne Dinge melden;", "tokens": ["Ich", "wollt'", "er\u00b7hab\u00b7ne", "Din\u00b7ge", "mel\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00e4rtyrer des wahren Glaubens", "tokens": ["Die", "M\u00e4r\u00b7ty\u00b7rer", "des", "wah\u00b7ren", "Glau\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Lobpreisen im Gesange wollt' ich;", "tokens": ["Lob\u00b7prei\u00b7sen", "im", "Ge\u00b7san\u00b7ge", "wollt'", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iam