{"dta.poem.19941": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Herbstgef\u00fchl .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie mit Gold die W\u00e4lder prangen,", "tokens": ["Wie", "mit", "Gold", "die", "W\u00e4l\u00b7der", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rosen gleich die B\u00e4um' erbl\u00fch'n!", "tokens": ["Ro\u00b7sen", "gleich", "die", "B\u00e4um'", "er\u00b7bl\u00fch'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erde will wie Himmel gl\u00fch'n,", "tokens": ["Er\u00b7de", "will", "wie", "Him\u00b7mel", "gl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh sie starr liegt und vergangen.", "tokens": ["Eh", "sie", "starr", "liegt", "und", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Goldne Himmelsburgen tragen", "tokens": ["Gold\u00b7ne", "Him\u00b7mels\u00b7bur\u00b7gen", "tra\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Gebirg' in stolzer Pracht,", "tokens": ["Die", "Ge\u00b7bir\u00b7g'", "in", "stol\u00b7zer", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Drinnen wandeln l\u00e4ngst erwacht", "tokens": ["Drin\u00b7nen", "wan\u00b7deln", "l\u00e4ngst", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ritter und Frau'n aus alten Tagen.", "tokens": ["Rit\u00b7ter", "und", "Frau'n", "aus", "al\u00b7ten", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Der verkl\u00e4rten Erde Wonne", "tokens": ["Der", "ver\u00b7kl\u00e4r\u00b7ten", "Er\u00b7de", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcllt mit Licht auch meine Brust,", "tokens": ["F\u00fcllt", "mit", "Licht", "auch", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Herz h\u00fcpft auf in Lust,", "tokens": ["Und", "das", "Herz", "h\u00fcpft", "auf", "in", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein V\u00f6glein in der Sonne.", "tokens": ["Wie", "ein", "V\u00f6\u00b7glein", "in", "der", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Solche Lust, \u2014 Herz! w\u00e4hrt nicht lange,", "tokens": ["Sol\u00b7che", "Lust", ",", "Herz", "!", "w\u00e4hrt", "nicht", "lan\u00b7ge", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "$(", "NN", "$.", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Herz! Das ist nur ein Ergl\u00fchn", "tokens": ["Herz", "!", "Das", "ist", "nur", "ein", "Er\u00b7gl\u00fchn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PDS", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dem g\u00e4nzlichen Verbl\u00fch'n", "tokens": ["Vor", "dem", "g\u00e4nz\u00b7li\u00b7chen", "Ver\u00b7bl\u00fch'n"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterm H\u00fcgel kalt und bange!", "tokens": ["Un\u00b7term", "H\u00fc\u00b7gel", "kalt", "und", "ban\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}