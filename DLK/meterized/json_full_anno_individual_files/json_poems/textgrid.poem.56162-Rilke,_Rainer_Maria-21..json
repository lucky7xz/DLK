{"textgrid.poem.56162": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singe die G\u00e4rten, mein Herz, die du nicht kennst; wie in Glas", "tokens": ["Sin\u00b7ge", "die", "G\u00e4r\u00b7ten", ",", "mein", "Herz", ",", "die", "du", "nicht", "kennst", ";", "wie", "in", "Glas"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$.", "KOKOM", "APPR", "NN"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "eingegossene G\u00e4rten, klar, unerreichbar.", "tokens": ["ein\u00b7ge\u00b7gos\u00b7se\u00b7ne", "G\u00e4r\u00b7ten", ",", "klar", ",", "un\u00b7er\u00b7reich\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wasser und Rosen von Ispahan oder Schiras,", "tokens": ["Was\u00b7ser", "und", "Ro\u00b7sen", "von", "Is\u00b7pa\u00b7han", "o\u00b7der", "Schi\u00b7ras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "singe sie selig, preise sie, keinem vergleichbar.", "tokens": ["sin\u00b7ge", "sie", "se\u00b7lig", ",", "prei\u00b7se", "sie", ",", "kei\u00b7nem", "ver\u00b7gleich\u00b7bar", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "$,", "PIS", "ADJD", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Zeige, mein Herz, da\u00df du sie niemals entbehrst.", "tokens": ["Zei\u00b7ge", ",", "mein", "Herz", ",", "da\u00df", "du", "sie", "nie\u00b7mals", "ent\u00b7behrst", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df sie dich meinen, ihre reifenden Feigen.", "tokens": ["Da\u00df", "sie", "dich", "mei\u00b7nen", ",", "ih\u00b7re", "rei\u00b7fen\u00b7den", "Fei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df du mit ihren, zwischen den bl\u00fchenden Zweigen", "tokens": ["Da\u00df", "du", "mit", "ih\u00b7ren", ",", "zwi\u00b7schen", "den", "bl\u00fc\u00b7hen\u00b7den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "wie zum Gesicht gesteigerten L\u00fcften verkehrst.", "tokens": ["wie", "zum", "Ge\u00b7sicht", "ge\u00b7stei\u00b7ger\u00b7ten", "L\u00fcf\u00b7ten", "ver\u00b7kehrst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Meide den Irrtum, da\u00df es Entbehrungen gebe", "tokens": ["Mei\u00b7de", "den", "Irr\u00b7tum", ",", "da\u00df", "es", "Ent\u00b7beh\u00b7run\u00b7gen", "ge\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "f\u00fcr den geschehnen Entschlu\u00df, diesen: zu sein!", "tokens": ["f\u00fcr", "den", "ge\u00b7scheh\u00b7nen", "Ent\u00b7schlu\u00df", ",", "die\u00b7sen", ":", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PDAT", "$.", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Seidener Faden, kamst du hinein ins Gewebe.", "tokens": ["Sei\u00b7de\u00b7ner", "Fa\u00b7den", ",", "kamst", "du", "hin\u00b7ein", "ins", "Ge\u00b7we\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Welchem der Bilder du auch im Innern geeint bist", "tokens": ["Wel\u00b7chem", "der", "Bil\u00b7der", "du", "auch", "im", "In\u00b7nern", "ge\u00b7eint", "bist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "PPER", "ADV", "APPRART", "NN", "VVPP", "VAFIN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "(sei es selbst ein Moment aus dem Leben der Pein),", "tokens": ["(", "sei", "es", "selbst", "ein", "Mo\u00b7ment", "aus", "dem", "Le\u00b7ben", "der", "Pein", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$(", "$,"], "meter": "+-+-----+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "f\u00fchl, da\u00df der ganze, der r\u00fchmliche