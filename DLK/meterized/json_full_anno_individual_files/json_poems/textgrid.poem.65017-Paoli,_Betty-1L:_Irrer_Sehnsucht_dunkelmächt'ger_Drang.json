{"textgrid.poem.65017": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Irrer Sehnsucht dunkelm\u00e4cht'ger Drang", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Irrer Sehnsucht dunkelm\u00e4cht'ger Drang", "tokens": ["Ir\u00b7rer", "Sehn\u00b7sucht", "dun\u00b7kel\u00b7m\u00e4cht'\u00b7ger", "Drang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Lockte dich vom heimatlichen Herde,", "tokens": ["Lock\u00b7te", "dich", "vom", "hei\u00b7mat\u00b7li\u00b7chen", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und du folgtest dem Syrengesang \u2013", "tokens": ["Und", "du", "folg\u00b7test", "dem", "Sy\u00b7ren\u00b7ge\u00b7sang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Ob ich dich je wiedersehen werde? \u2013", "tokens": ["Ob", "ich", "dich", "je", "wie\u00b7der\u00b7se\u00b7hen", "wer\u00b7de", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Viele Tage sind seitdem verrauscht,", "tokens": ["Vie\u00b7le", "Ta\u00b7ge", "sind", "seit\u00b7dem", "ver\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PAV", "VVPP", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Bald mit heit'rer, bald mit Gramgeberde,", "tokens": ["Bald", "mit", "heit'\u00b7rer", ",", "bald", "mit", "Gram\u00b7ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch mein Herz nur auf die Antwort lauscht,", "tokens": ["Doch", "mein", "Herz", "nur", "auf", "die", "Ant\u00b7wort", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ob ich dich je wiedersehen werde? \u2013", "tokens": ["Ob", "ich", "dich", "je", "wie\u00b7der\u00b7se\u00b7hen", "wer\u00b7de", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Heimlich lockend t\u00f6nt's in meine Brust:", "tokens": ["Heim\u00b7lich", "lo\u00b7ckend", "t\u00f6nt's", "in", "mei\u00b7ne", "Brust", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Reich an off'nen Gr\u00e4bern ist die Erde!", "tokens": ["Reich", "an", "off'\u00b7nen", "Gr\u00e4\u00b7bern", "ist", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und auch mich erfa\u00dft's wie Wanderlust \u2013", "tokens": ["Und", "auch", "mich", "er\u00b7fa\u00dft's", "wie", "Wan\u00b7der\u00b7lust", "\u2013"], "token_inf