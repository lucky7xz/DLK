{"textgrid.poem.56081": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Apfelgarten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm gleich nach dem Sonnenuntergange,", "tokens": ["Komm", "gleich", "nach", "dem", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "sieh das Abendgr\u00fcn des Rasengrunds;", "tokens": ["sieh", "das", "A\u00b7bend\u00b7gr\u00fcn", "des", "Ra\u00b7sen\u00b7grunds", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ist es nicht, als h\u00e4tten wir es lange", "tokens": ["ist", "es", "nicht", ",", "als", "h\u00e4t\u00b7ten", "wir", "es", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "KOKOM", "VAFIN", "PPER", "PPER", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "angesammelt und erspart in uns,", "tokens": ["an\u00b7ge\u00b7sam\u00b7melt", "und", "er\u00b7spart", "in", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "um es jetzt aus F\u00fchlen und Erinnern,", "tokens": ["um", "es", "jetzt", "aus", "F\u00fch\u00b7len", "und", "E\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "neuer Hoffnung, halbverge\u00dfnem Freun,", "tokens": ["neu\u00b7er", "Hoff\u00b7nung", ",", "halb\u00b7ver\u00b7ge\u00df\u00b7nem", "Freun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "noch vermischt mit Dunkel aus dem Innern,", "tokens": ["noch", "ver\u00b7mischt", "mit", "Dun\u00b7kel", "aus", "dem", "In\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "in Gedanken vor uns hinzustreun", "tokens": ["in", "Ge\u00b7dan\u00b7ken", "vor", "uns", "hin\u00b7zu\u00b7streun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "unter B\u00e4ume wie von D\u00fcrer, die", "tokens": ["un\u00b7ter", "B\u00e4u\u00b7me", "wie", "von", "D\u00fc\u00b7rer", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "KOKOM", "APPR", "NE", "$,", "PRELS"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "das Gewicht von hundert Arbeitstagen", "tokens": ["das", "Ge\u00b7wicht", "von", "hun\u00b7dert", "Ar\u00b7beits\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "in den \u00fcberf\u00fcllten Fr\u00fcchten tragen,", "tokens": ["in", "den", "\u00fc\u00b7berf\u00b7\u00fcll\u00b7ten", "Fr\u00fcch\u00b7ten", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "dienend, voll Geduld, versuchend, wie", "tokens": ["die\u00b7nend", ",", "voll", "Ge\u00b7duld", ",", "ver\u00b7su\u00b7chend", ",", "wie"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVPP", "$,", "ADJD", "NN", "$,", "VVPP", "$,", "PWAV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "das, was alle Ma\u00dfe \u00fcbersteigt,", "tokens": ["das", ",", "was", "al\u00b7le", "Ma\u00b7\u00dfe", "\u00fc\u00b7bers\u00b7teigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "noch zu heben ist und hinzugeben,", "tokens": ["noch", "zu", "he\u00b7ben", "ist", "und", "hin\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VAFIN", "KON", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "wenn man willig, durch ein langes Leben", "tokens": ["wenn", "man", "wil\u00b7lig", ",", "durch", "ein", "lan\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJD", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "nur das Eine will und w\u00e4chst und schweigt.", "tokens": ["nur", "das", "Ei\u00b7ne", "will", "und", "w\u00e4chst", "und", "schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PIS", "VMFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Komm gleich nach dem Sonnenuntergange,", "tokens": ["Komm", "gleich", "nach", "dem", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gan\u00b7ge"