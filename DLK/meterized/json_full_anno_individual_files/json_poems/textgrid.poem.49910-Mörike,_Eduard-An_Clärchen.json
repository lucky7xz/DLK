{"textgrid.poem.49910": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Cl\u00e4rchen", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freundin immer neu zu schm\u00fccken,", "tokens": ["Die", "Freun\u00b7din", "im\u00b7mer", "neu", "zu", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich seh es wohl, ist deine Lust;", "tokens": ["Ich", "seh", "es", "wohl", ",", "ist", "dei\u00b7ne", "Lust", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darfst du ins Haar den Kranz ", "tokens": ["Darfst", "du", "ins", "Haar", "den", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des eignen bist du kaum bewu\u00dft.", "tokens": ["Des", "eig\u00b7nen", "bist", "du", "kaum", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und deinen Augen zu gefallen", "tokens": ["Und", "dei\u00b7nen", "Au\u00b7gen", "zu", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erlaubt sie gern das m\u00fc\u00dfge Spiel.", "tokens": ["Er\u00b7laubt", "sie", "gern", "das", "m\u00fc\u00df\u00b7ge", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach t\u00e4glich mehr gef\u00e4llt sie allen,", "tokens": ["Ach", "t\u00e4g\u00b7lich", "mehr", "ge\u00b7f\u00e4llt", "sie", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "ADV", "VVFIN", "PPER", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die allen schon zu sehr gefiel!", "tokens": ["Die", "al\u00b7len", "schon", "zu", "sehr", "ge\u00b7fiel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADV", "PTKA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du machst sie, ", "tokens": ["Du", "machst", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Kaum lieblicher als je sie war,", "tokens": ["Kaum", "lieb\u00b7li\u00b7cher", "als", "je", "sie", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch jede dieser Neuerungen", "tokens": ["Doch", "je\u00b7de", "die\u00b7ser", "Neu\u00b7e\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bringt neue Sorge und Gefahr.", "tokens": ["Bringt", "neu\u00b7e", "Sor\u00b7ge", "und", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Heut ringeltest du Kinderlocken", "tokens": ["Heut", "rin\u00b7gel\u00b7test", "du", "Kin\u00b7der\u00b7lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sch\u00f6n um Hals und Nacken ihr!", "tokens": ["Wie", "sch\u00f6n", "um", "Hals", "und", "Na\u00b7cken", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "NN", "KON", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein M\u00e4dchen sieht das unerschrocken,", "tokens": ["Ein", "M\u00e4d\u00b7chen", "sieht", "das", "un\u00b7er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Allein bedenk, bedenke, ", "tokens": ["Al\u00b7lein", "be\u00b7denk", ",", "be\u00b7den\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zwar mu\u00df vom Reiz ein Dichter leben,", "tokens": ["Zwar", "mu\u00df", "vom", "Reiz", "ein", "Dich\u00b7ter", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er heischt zur\u00fcck was du versteckt,", "tokens": ["Er", "heischt", "zu\u00b7r\u00fcck", "was", "du", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm bleibt der Pfeil ins Herz gege