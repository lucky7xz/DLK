{"dta.poem.15928": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "133.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was steht auf diesem Ring? der Gastfreund sandt' ihn mir.", "tokens": ["Was", "steht", "auf", "die\u00b7sem", "Ring", "?", "der", "Gast\u00b7freund", "sandt'", "ihn", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PDAT", "NN", "$.", "ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hei\u00dft es ", "tokens": ["Hei\u00dft", "es"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "In Perserschrift ist nicht der fl\u00fcchtige Vokal", "tokens": ["In", "Per\u00b7ser\u00b7schrift", "ist", "nicht", "der", "fl\u00fcch\u00b7ti\u00b7ge", "Vo\u00b7kal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bezeichnet; hei\u00dfen kann es beides alzumal.", "tokens": ["Be\u00b7zeich\u00b7net", ";", "hei\u00b7\u00dfen", "kann", "es", "bei\u00b7des", "al\u00b7zu\u00b7mal", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VVINF", "VMFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was hei\u00dft ", "tokens": ["Was", "hei\u00dft"], "token_info": ["word", "word"], "pos": ["PWS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Der Liebe.\u201c Lieblich lacht die Lieb' aus diesem Spiegel.", "tokens": ["Der", "Lie\u00b7be", ".", "\u201c", "Lieb\u00b7lich", "lacht", "die", "Lieb'", "aus", "die\u00b7sem", "Spie\u00b7gel", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ADJD", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was hei\u00dft ", "tokens": ["Was", "hei\u00dft"], "token_info": ["word", "word"], "pos": ["PWS", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Der Liebe.\u201c Dieser Gru\u00df ist wonnigliche Wonne.", "tokens": ["Der", "Lie\u00b7be", ".", "\u201c", "Die\u00b7ser", "Gru\u00df", "ist", "won\u00b7nig\u00b7li\u00b7che", "Won\u00b7ne", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "PDAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den Brief des Himmels deckt des Sonnensiegels Glanz,", "tokens": ["Den", "Brief", "des", "Him\u00b7mels", "deckt", "des", "Son\u00b7nen\u00b7sie\u00b7gels", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das l\u00f6st die Nacht und liest die Sterngeheimschrift ganz.", "tokens": ["Das", "l\u00f6st", "die", "Nacht", "und", "liest", "die", "Stern\u00b7ge\u00b7heim\u00b7schrift", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das Siegel aber auf dem Brief, den Liebe schrieb,", "tokens": ["Das", "Sie\u00b7gel", "a\u00b7ber", "auf", "dem", "Brief", ",", "den", "Lie\u00b7be", "schrieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist eine Sonne, die des Zweifels Nacht vertrieb.", "tokens": ["Ist", "ei\u00b7ne", "Son\u00b7ne", ",", "die", "des", "Zwei\u00b7fels", "Nacht", "ver\u00b7trieb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Mit dieser Sonne siegl' ich hier das erste Blatt,", "tokens": ["Mit", "die\u00b7ser", "Son\u00b7ne", "siegl'", "ich", "hier", "das", "ers\u00b7te", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das jenem, der dis Bild mir gab, zu danken hat.", "tokens": ["Das", "je\u00b7nem", ",", "der", "dis", "Bild", "mir", "gab", ",", "zu", "dan\u00b7ken", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "$,", "PRELS", "PDS", "NN", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wer einen Gru\u00df von mir empf\u00e4ngt mit diesem Stempel,", "tokens": ["Wer", "ei\u00b7nen", "Gru\u00df", "von", "mir", "emp\u00b7f\u00e4ngt", "mit", "die\u00b7sem", "Stem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er kommt vom Herzen aus der Liebe Sonnentempel.", "tokens": ["Er", "kommt", "vom", "Her\u00b7zen", "aus", "der", "Lie\u00b7be", "Son\u00b7nen\u00b7tem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}