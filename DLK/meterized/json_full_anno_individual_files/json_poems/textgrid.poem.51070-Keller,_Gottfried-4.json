{"textgrid.poem.51070": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4g ich, wo es Hy\u00e4nen gibt, im Sand,", "tokens": ["L\u00e4g", "ich", ",", "wo", "es", "Hy\u00b7\u00e4\u00b7nen", "gibt", ",", "im", "Sand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie wollt ich hoffnungsvoll die Nacht erharren,", "tokens": ["Wie", "wollt", "ich", "hoff\u00b7nungs\u00b7voll", "die", "Nacht", "er\u00b7har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis hungrig eine k\u00e4me hergerannt,", "tokens": ["Bis", "hung\u00b7rig", "ei\u00b7ne", "k\u00e4\u00b7me", "her\u00b7ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mich heulend aus der lockern Gruft zu scharren!", "tokens": ["Mich", "heu\u00b7lend", "aus", "der", "lo\u00b7ckern", "Gruft", "zu", "schar\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie wollt ich freudig mit dem gier'gen Tier", "tokens": ["Wie", "wollt", "ich", "freu\u00b7dig", "mit", "dem", "gier'\u00b7gen", "Tier"], "token_info": ["