{"dta.poem.23620": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erl\u00e4uterung  \n  zum 13. Artikel der Bundesacte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Wirth, Herr Wirth, ein Gl\u00e4schen Wein! \u2014", "tokens": ["Herr", "Wirth", ",", "Herr", "Wirth", ",", "ein", "Gl\u00e4s\u00b7chen", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "NN", "NN", "$,", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Schenkt mir ein volles Viertel ein! \u2014", "tokens": ["Schenkt", "mir", "ein", "vol\u00b7les", "Vier\u00b7tel", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Der Wirth, er dreht sich um und um,", "tokens": ["Der", "Wirth", ",", "er", "dreht", "sich", "um", "und", "um", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er l\u00e4uft im ganzen Haus' herum,", "tokens": ["Er", "l\u00e4uft", "im", "gan\u00b7zen", "Haus'", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und rechtsum, linksum, ringsum, und \u2014 kurzum,", "tokens": ["Und", "recht\u00b7sum", ",", "link\u00b7sum", ",", "ring\u00b7sum", ",", "und", "kur\u00b7zum", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "$,", "ADV", "$,", "KON", "$(", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Er kann den Schl\u00fcssel nicht finden.", "tokens": ["Er", "kann", "den", "Schl\u00fcs\u00b7sel", "nicht", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und ach! die G\u00e4ste mehren sich:", "tokens": ["Und", "ach", "!", "die", "G\u00e4s\u00b7te", "meh\u00b7ren", "sich", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was z\u00f6gerst du? so sprich, so sprich!", "tokens": ["Was", "z\u00f6\u00b7gerst", "du", "?", "so", "sprich", ",", "so", "sprich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Wirth, o Wirth, erbarme dich!", "tokens": ["O", "Wirth", ",", "o", "Wirth", ",", "er\u00b7bar\u00b7me", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn unser Durst ist f\u00fcrchterlich.", "tokens": ["Denn", "un\u00b7ser", "Durst", "ist", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wirth, er aber bleibet stumm,", "tokens": ["Der", "Wirth", ",", "er", "a\u00b7ber", "blei\u00b7bet", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dreht sich wieder um und um,", "tokens": ["Und", "dreht", "sich", "wie\u00b7der", "um", "und", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00e4uft im ganzen Haus' herum,", "tokens": ["Und", "l\u00e4uft", "im", "gan\u00b7zen", "Haus'", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und rechtsum, linksum, ringsum, und \u2014 kurzum,", "tokens": ["Und", "recht\u00b7sum", ",", "link\u00b7sum", ",", "ring\u00b7sum", ",", "und", "kur\u00b7zum", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "$,", "ADV", "$,", "KON", "$(", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Er kann den Schl\u00fcssel nicht finden.", "tokens": ["Er", "kann", "den", "Schl\u00fcs\u00b7sel", "nicht", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und gr\u00f6\u00dfer wird die Cumpanei,", "tokens": ["Und", "gr\u00f6\u00b7\u00dfer", "wird", "die", "Cum\u00b7pa\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gr\u00f6\u00dfer nur die Z\u00f6gerei,", "tokens": ["Und", "gr\u00f6\u00b7\u00dfer", "nur", "die", "Z\u00f6\u00b7ge\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer lauter das Geschrei:", "tokens": ["Und", "im\u00b7mer", "lau\u00b7ter", "das", "Ge\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "He holla! Wirthschaft! Wein herbei!", "tokens": ["He", "hol\u00b7la", "!", "Wirth\u00b7schaft", "!", "Wein", "her\u00b7bei", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "NN", "$.", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wirth, der Wirth, er stellt sich dumm,", "tokens": ["Der", "Wirth", ",", "der", "Wirth", ",", "er", "stellt", "sich", "dumm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er h\u00f6rt, er sieht, er bleibet stumm,", "tokens": ["Er", "h\u00f6rt", ",", "er", "sieht", ",", "er", "blei\u00b7bet", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und dreht sich wieder um und um,", "tokens": ["Und", "dreht", "sich", "wie\u00b7der", "um", "und", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und l\u00e4uft im ganzen Haus' herum,", "tokens": ["Und", "l\u00e4uft", "im", "gan\u00b7zen", "Haus'", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und rechtsum, linksum, ringsum, und \u2014 kurzum,", "tokens": ["Und", "recht\u00b7sum", ",", "link\u00b7sum", ",", "ring\u00b7sum", ",", "und", "kur\u00b7zum", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "$,", "ADV", "$,", "KON", "$(", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Er kann den Schl\u00fcssel nicht finden.", "tokens": ["Er", "kann", "den", "Schl\u00fcs\u00b7sel", "nicht", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "O Wirth, was ist das f\u00fcr Manier?", "tokens": ["O", "Wirth", ",", "was", "ist", "das", "f\u00fcr", "Ma\u00b7nier", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PDS", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Wirth, o Wirth, wie zaudert Ihr!", "tokens": ["O", "Wirth", ",", "o", "Wirth", ",", "wie", "zau\u00b7dert", "Ihr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bringt Wein! denn Wein begehren wir.", "tokens": ["Bringt", "Wein", "!", "denn", "Wein", "be\u00b7geh\u00b7ren", "wir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$.", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Teufel denn, was ist das hier!", "tokens": ["Zum", "Teu\u00b7fel", "denn", ",", "was", "ist", "das", "hier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "PWS", "VAFIN", "PDS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Wirth, verneigt sich, steht ganz krumm,", "tokens": ["Der", "Wirth", ",", "ver\u00b7neigt", "sich", ",", "steht", "ganz", "krumm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PRF", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er l\u00e4chelt, schmunzelt, stellt sich dumm,", "tokens": ["Er", "l\u00e4\u00b7chelt", ",", "schmun\u00b7zelt", ",", "stellt", "sich", "dumm", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er h\u00f6rt, er sieht, er bleibet stumm,", "tokens": ["Er", "h\u00f6rt", ",", "er", "sieht", ",", "er", "blei\u00b7bet", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "wor