{"textgrid.poem.40208": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Die gr\u00f6szere Aufgabe", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwer, den zerst\u00f6renden Geist der Zeit in Gestalten zu bannen;", "tokens": ["Schwer", ",", "den", "zer\u00b7st\u00f6\u00b7ren\u00b7den", "Geist", "der", "Zeit", "in", "Ge\u00b7stal\u00b7ten", "zu", "ban\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJA", "NN", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "schwerer den kl\u00e4renden noch, weil er verborgener wirkt.", "tokens": ["schwe\u00b7rer", "den", "kl\u00e4\u00b7ren\u00b7den", "noch", ",", "weil", "er", "ver\u00b7bor\u00b7ge\u00b7ner", "wirkt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Aber wir f\u00fchlen: er wirkt! Ihm folgen war immer des Lebens", "tokens": ["A\u00b7ber", "wir", "f\u00fch\u00b7len", ":", "er", "wirkt", "!", "Ihm", "fol\u00b7gen", "war", "im\u00b7mer", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "PPER", "VVINF", "VAFIN", "ADV", "ART", "NN"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "h\u00f6chstes Ziel und Gebot, h\u00f6chstes darum auch der Kunst.", "tokens": ["h\u00f6chs\u00b7tes", "Ziel", "und", "Ge\u00b7bot", ",", "h\u00f6chs\u00b7tes", "da\u0