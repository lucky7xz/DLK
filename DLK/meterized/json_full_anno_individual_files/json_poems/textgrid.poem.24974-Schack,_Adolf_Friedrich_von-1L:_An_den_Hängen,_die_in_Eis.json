ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Still an meiner Seite schleicht", "tokens": ["Still", "an", "mei\u00b7ner", "Sei\u00b7te", "schleicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Er durch bl\u00fch'nde Beete.", "tokens": ["Er", "durch", "bl\u00fch'n\u00b7de", "Bee\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Seit ", "tokens": ["Seit"], "token_info": ["word"], "pos": ["NN"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "In den Schlaf gesungen,", "tokens": ["In", "den", "Schlaf", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schwester, hat in jedem Mai", "tokens": ["Schwes\u00b7ter", ",", "hat", "in", "je\u00b7dem", "Mai"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich dein Arm umschlungen,", "tokens": ["Mich", "dein", "Arm", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schrittst du hier mit mir am Bach", "tokens": ["Schrittst", "du", "hier", "mit", "mir", "am", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die blum'ge Wiese;", "tokens": ["Durch", "die", "blum'\u00b7ge", "Wie\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nun zum erstenmale, ach!", "tokens": ["Nun", "zum", "ers\u00b7ten\u00b7ma\u00b7le", ",", "ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "$,", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fehlst du mir, Elise!", "tokens": ["Fehlst", "du", "mir", ",", "E\u00b7li\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NE", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "In der dumpfen Stube lang,", "tokens": ["In", "der", "dum\u00b7pfen", "Stu\u00b7be", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Winterlich umnachtet,", "tokens": ["Win\u00b7ter\u00b7lich", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nach der ersten Lerche Sang", "tokens": ["Nach", "der", "ers\u00b7ten", "Ler\u00b7che", "Sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hattest du geschmachtet.", "tokens": ["Hat\u00b7test", "du", "ge\u00b7schmach\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Endlich hell durch mildre Luft", "tokens": ["End\u00b7lich", "hell", "durch", "mild\u00b7re", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR",