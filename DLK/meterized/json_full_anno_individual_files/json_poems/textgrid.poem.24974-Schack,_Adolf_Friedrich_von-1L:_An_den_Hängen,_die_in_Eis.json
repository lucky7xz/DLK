{"textgrid.poem.24974": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: An den H\u00e4ngen, die in Eis", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An den H\u00e4ngen, die in Eis", "tokens": ["An", "den", "H\u00e4n\u00b7gen", ",", "die", "in", "Eis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tiefbegraben starrten,", "tokens": ["Tief\u00b7be\u00b7gra\u00b7ben", "starr\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schm\u00fccken Krokus, gelb und wei\u00df,", "tokens": ["Schm\u00fc\u00b7cken", "Kro\u00b7kus", ",", "gelb", "und", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Veilchen schon den Garten;", "tokens": ["Veil\u00b7chen", "schon", "den", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bl\u00e4tter h\u00e4ngt das junge Jahr", "tokens": ["Bl\u00e4t\u00b7ter", "h\u00e4ngt", "das", "jun\u00b7ge", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An die kahlen Aeste,", "tokens": ["An", "die", "kah\u00b7len", "A\u00b7es\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es kehrt der Wanderstar", "tokens": ["Und", "es", "kehrt", "der", "Wan\u00b7der\u00b7star"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zum verlass'nen Neste.", "tokens": ["Zum", "ver\u00b7lass'\u00b7nen", "Nes\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ja, im Glanz, der \u00fcber Thal", "tokens": ["Ja", ",", "im", "Glanz", ",", "der", "\u00fc\u00b7ber", "Thal"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPRART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Gebirg ergossen,", "tokens": ["Und", "Ge\u00b7birg", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Allen als ein Freudensaal", "tokens": ["Al\u00b7len", "als", "ein", "Freu\u00b7den\u00b7saal"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward die Welt erschlossen;", "tokens": ["Ward", "die", "Welt", "er\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nur aus meinem Herzen weicht", "tokens": ["Nur", "aus", "mei\u00b7nem", "Her\u00b7zen", "weicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht der Gram, der stete,", "tokens": ["Nicht", "der", "Gram", ",", "der", "ste\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Still an meiner Seite schleicht", "tokens": ["Still", "an", "mei\u00b7ner", "Sei\u00b7te", "schleicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Er durch bl\u00fch'nde Beete.", "tokens": ["Er", "durch", "bl\u00fch'n\u00b7de", "Bee\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Seit ", "tokens": ["Seit"], "token_info": ["word"], "pos": ["NN"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "In den Schlaf gesungen,", "tokens": ["In", "den", "Schlaf", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schwester, hat in jedem Mai", "tokens": ["Schwes\u00b7ter", ",", "hat", "in", "je\u00b7dem", "Mai"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich dein Arm umschlungen,", "tokens": ["Mich", "dein", "Arm", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schrittst du hier mit mir am Bach", "tokens": ["Schrittst", "du", "hier", "mit", "mir", "am", "Bach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die blum'ge Wiese;", "tokens": ["Durch", "die", "blum'\u00b7ge", "Wie\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nun zum erstenmale, ach!", "tokens": ["Nun", "zum", "ers\u00b7ten\u00b7ma\u00b7le", ",", "ach", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "$,", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fehlst du mir, Elise!", "tokens": ["Fehlst", "du", "mir", ",", "E\u00b7li\u00b7se", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "NE", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "In der dumpfen Stube lang,", "tokens": ["In", "der", "dum\u00b7pfen", "Stu\u00b7be", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Winterlich umnachtet,", "tokens": ["Win\u00b7ter\u00b7lich", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nach der ersten Lerche Sang", "tokens": ["Nach", "der", "ers\u00b7ten", "Ler\u00b7che", "Sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hattest du geschmachtet.", "tokens": ["Hat\u00b7test", "du", "ge\u00b7schmach\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Endlich hell durch mildre Luft", "tokens": ["End\u00b7lich", "hell", "durch", "mild\u00b7re", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Scholl er dir entgegen \u2013", "tokens": ["Scholl", "er", "dir", "ent\u00b7ge\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PPER", "PTKVZ", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da, Geliebte, in die Gruft", "tokens": ["Da", ",", "Ge\u00b7lieb\u00b7te", ",", "in", "die", "Gruft"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mu\u00dftest du dich legen.", "tokens": ["Mu\u00df\u00b7test", "du", "dich", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht im jungen Sonnenlicht", "tokens": ["Nicht", "im", "jun\u00b7gen", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "All das Gr\u00fcnen, Bl\u00fchen,", "tokens": ["All", "das", "Gr\u00fc\u00b7nen", ",", "Bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und der Fichten Sprossen nicht,", "tokens": ["Und", "der", "Fich\u00b7ten", "Spros\u00b7sen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Die wie Fackeln gl\u00fchen,", "tokens": ["Die", "wie", "Fa\u00b7ckeln", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nicht, durchblitzt vom Morgenrot,", "tokens": ["Nicht", ",", "durch\u00b7blitzt", "vom", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die beperlten Auen", "tokens": ["Die", "be\u00b7perl\u00b7ten", "Au\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "G\u00f6nnte dir der M\u00f6rder Tod", "tokens": ["G\u00f6nn\u00b7te", "dir", "der", "M\u00f6r\u00b7der", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch einmal zu schauen.", "tokens": ["Noch", "ein\u00b7mal", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Wohl in einem Jenseits gern,", "tokens": ["Wohl", "in", "ei\u00b7nem", "Jen\u00b7seits", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie zu h\u00f6hern R\u00e4umen", "tokens": ["Wie", "zu", "h\u00f6\u00b7hern", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hin du schwebst von Stern zu Stern,", "tokens": ["Hin", "du", "schwebst", "von", "Stern", "zu", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6cht' ich dich mir tr\u00e4umen;", "tokens": ["M\u00f6cht'", "ich", "dich", "mir", "tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Doch umsonst! Mein Geist mu\u00df matt", "tokens": ["Doch", "um\u00b7sonst", "!", "Mein", "Geist", "mu\u00df", "matt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PPOSAT", "NN", "VMFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Schwingen senken;", "tokens": ["Sei\u00b7ne", "Schwin\u00b7gen", "sen\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "In der finstern Todesstatt", "tokens": ["In", "der", "fins\u00b7tern", "To\u00b7des\u00b7statt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Kann ich nur dich denken.", "tokens": ["Kann", "ich", "nur", "dich", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Dort zu dir hinunter nun", "tokens": ["Dort", "zu", "dir", "hin\u00b7un\u00b7ter", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dringt kein Hauch vom Lenze.", "tokens": ["Dringt", "kein", "Hauch", "vom", "Len\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bleich zu deinen H\u00e4upten ruhn", "tokens": ["Bleich", "zu", "dei\u00b7nen", "H\u00e4up\u00b7ten", "ruhn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die verwelkten Kr\u00e4nze,", "tokens": ["Die", "ver\u00b7welk\u00b7ten", "Kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und ein blasser Lichtstrahl streicht", "tokens": ["Und", "ein", "blas\u00b7ser", "Licht\u00b7strahl", "streicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur mit D\u00e4mmerhelle", "tokens": ["Nur", "mit", "D\u00e4m\u00b7mer\u00b7hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "L\u00e4ngs der W\u00e4nde, kalt und feucht,", "tokens": ["L\u00e4ngs", "der", "W\u00e4n\u00b7de", ",", "kalt", "und", "feucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch die Grabkapelle.", "tokens": ["Durch", "die", "Grab\u00b7ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Oft im Traume, grambet\u00e4ubt,", "tokens": ["Oft", "im", "Trau\u00b7me", ",", "gram\u00b7be\u00b7t\u00e4ubt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischen Steingebr\u00f6ckel", "tokens": ["Zwi\u00b7schen", "Stein\u00b7ge\u00b7br\u00f6\u00b7ckel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Heb' ich, moderduftumst\u00e4ubt,", "tokens": ["Heb'", "ich", ",", "mo\u00b7der\u00b7duf\u00b7tums\u00b7t\u00e4ubt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Sarges Deckel.", "tokens": ["Dei\u00b7nes", "Sar\u00b7ges", "De\u00b7ckel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sieh! Da schl\u00e4fst nach kurzem Sein", "tokens": ["Sieh", "!", "Da", "schl\u00e4fst", "nach", "kur\u00b7zem", "Sein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du den Schlaf, den langen,", "tokens": ["Du", "den", "Schlaf", ",", "den", "lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und ein matter, eis'ger Schein", "tokens": ["Und", "ein", "mat\u00b7ter", ",", "eis'\u00b7ger", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Spielt um deine Wangen.", "tokens": ["Spielt", "um", "dei\u00b7ne", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochai