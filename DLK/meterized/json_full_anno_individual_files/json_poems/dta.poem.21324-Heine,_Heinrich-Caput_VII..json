{"dta.poem.21324": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich ging nach Haus und schlief als ob", "tokens": ["Ich", "ging", "nach", "Haus", "und", "schlief", "als", "ob"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "KOKOM", "KOUS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Engel gewiegt mich h\u00e4tten.", "tokens": ["Die", "En\u00b7gel", "ge\u00b7wiegt", "mich", "h\u00e4t\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Man ruht in deutschen Betten so weich,", "tokens": ["Man", "ruht", "in", "deut\u00b7schen", "Bet\u00b7ten", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Denn das sind Federbetten.", "tokens": ["Denn", "das", "sind", "Fe\u00b7der\u00b7bet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}