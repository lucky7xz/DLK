{"textgrid.poem.55976": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als l\u00e4ge er in einem Krater-Kreise", "tokens": ["Als", "l\u00e4\u00b7ge", "er", "in", "ei\u00b7nem", "Kra\u00b7ter\u00b7Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf einem Mond: ist jeder Hof umd\u00e4mmt,", "tokens": ["auf", "ei\u00b7nem", "Mond", ":", "ist", "je\u00b7der", "Hof", "um\u00b7d\u00e4mmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "VAFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und drin die G\u00e4rten sind auf gleiche Weise", "tokens": ["und", "drin", "die", "G\u00e4r\u00b7ten", "sind", "auf", "glei\u00b7che", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "gekleidet und wie Waisen gleich gek\u00e4mmt", "tokens": ["ge\u00b7klei\u00b7det", "und", "wie", "Wai\u00b7sen", "gleich", "ge\u00b7k\u00e4mmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "PWAV", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "von jenem Sturm, der sie so rauh erzieht", "tokens": ["von", "je\u00b7nem", "Sturm", ",", "der", "sie", "so", "rauh", "er\u00b7zieht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und tagelang sie bange macht mit Toden.", "tokens": ["und", "ta\u00b7ge\u00b7lang", "sie", "ban\u00b7ge", "macht", "mit", "To\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann sitzt man in den H\u00e4usern drin und sieht", "tokens": ["Dann", "sitzt", "man", "in", "den", "H\u00e4u\u00b7sern", "drin", "und", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "in schiefen Spiegeln was auf den Kommoden", "tokens": ["in", "schie\u00b7fen", "Spie\u00b7geln", "was", "auf", "den", "Kom\u00b7mo\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Seltsames steht. Und einer von den S\u00f6hnen", "tokens": ["Selt\u00b7sa\u00b7mes", "steht", ".", "Und", "ei\u00b7ner", "von", "den", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "KON", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "tritt abends vor die T\u00fcr und zieht ein T\u00f6nen", "tokens": ["tritt", "a\u00b7bends", "vor", "die", "T\u00fcr", "und", "zieht", "ein", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "aus der Harmonika wie Weinen weich;", "tokens": ["aus", "der", "Har\u00b7mo\u00b7ni\u00b7ka", "wie", "Wei\u00b7nen", "weich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "NN", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "so h\u00f6rte ers in einem fremden Hafen \u2013.", "tokens": ["so", "h\u00f6r\u00b7te", "ers", "in", "ei\u00b7nem", "frem\u00b7den", "Ha\u00b7fen", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und drau\u00dfen formt sich eines von den Schafen", "tokens": ["Und", "drau\u00b7\u00dfen", "formt", "sich", "ei\u00b7nes", "von", "den", "Scha\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ganz gro\u00df, fast drohend, auf dem Au\u00dfendeich.", "tokens": ["ganz", "gro\u00df", ",", "fast", "dro\u00b7hend", ",", "auf", "dem", "Au\u00b7\u00dfen\u00b7deich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVPP", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als l\u00e4ge er in einem Krater-Kreise", "tokens": ["Als", "l\u00e4\u00b7ge", "er", "in", "ei\u00b7nem", "Kra\u00b7ter\u00b7Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "auf einem Mond: ist jeder Hof umd\u00e4mmt,", "tokens": ["auf", "ei\u00b7nem", "Mond", ":", "ist",