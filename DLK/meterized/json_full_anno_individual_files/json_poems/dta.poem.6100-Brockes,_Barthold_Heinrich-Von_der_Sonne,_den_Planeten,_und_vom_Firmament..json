{"dta.poem.6100": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, sieht man ferner noch ein undurchdringlich Thal", "tokens": ["Ja", ",", "sieht", "man", "fer\u00b7ner", "noch", "ein", "un\u00b7durch\u00b7dring\u00b7lich", "Thal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PIS", "ADV", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voll Sterne, sonder Maa\u00df und Zahl,", "tokens": ["Voll", "Ster\u00b7ne", ",", "son\u00b7der", "Maa\u00df", "und", "Zahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die unbeschreib- und unaussprechlich seyn;", "tokens": ["Die", "un\u00b7be\u00b7schreib", "und", "un\u00b7aus\u00b7sprech\u00b7lich", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In welchem Abgrund sinckt sodann der Geist hinein!", "tokens": ["In", "wel\u00b7chem", "Ab\u00b7grund", "sinckt", "so\u00b7dann", "der", "Geist", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}