{"dta.poem.8317": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Tadelsucht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die falsche Tadelsucht ist eine\nPest der Zeiten,", "tokens": ["Die", "fal\u00b7sche", "Ta\u00b7del\u00b7sucht", "ist", "ei\u00b7ne", "Pest", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sucht nach blinden Wahn", "tokens": ["Sie", "sucht", "nach", "blin\u00b7den", "Wahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und weil sie sich nicht gros um", "tokens": ["Und", "weil", "sie", "sich", "nicht", "gros", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "APPR"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Nur durch der Einbildung, be-", "tokens": ["Nur", "durch", "der", "Ein\u00b7bil\u00b7dung", ",", "be"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "TRUNC"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So mu\u00df ihr oft was sch\u00f6n, als", "tokens": ["So", "mu\u00df", "ihr", "oft", "was", "sch\u00f6n", ",", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PWS", "ADJD", "$,", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es ist ihr Grundgesez, ein ungegr\u00fcndet Meinen.", "tokens": ["Es", "ist", "ihr", "Grund\u00b7ge\u00b7sez", ",", "ein", "un\u00b7ge\u00b7gr\u00fcn\u00b7det", "Mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie geifert immer fort, und greift dasselbe an,", "tokens": ["Sie", "gei\u00b7fert", "im\u00b7mer", "fort", ",", "und", "greift", "das\u00b7sel\u00b7be", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PDAT", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was ihr drum misgef\u00e4llt, weil sie nicht leisten", "tokens": ["Was", "ihr", "drum", "mis\u00b7ge\u00b7f\u00e4llt", ",", "weil", "sie", "nicht", "leis\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PAV", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Was andre in der Welt, als Tugenden beweisen,", "tokens": ["Was", "and\u00b7re", "in", "der", "Welt", ",", "als", "Tu\u00b7gen\u00b7den", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "ART", "NN", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das was sie an sich hat ist nur allein zu preisen.", "tokens": ["Das", "was", "sie", "an", "sich", "hat", "ist", "nur", "al\u00b7lein", "zu", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "APPR", "PRF", "VAFIN", "VAFIN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch nur in diesem Fall sind ihre Thaten sch\u00f6n,", "tokens": ["Doch", "nur", "in", "die\u00b7sem", "Fall", "sind", "ih\u00b7re", "Tha\u00b7ten", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So lange sie an ihr, an andern nicht zu sehn.", "tokens": ["So", "lan\u00b7ge", "sie", "an", "ihr", ",", "an", "an\u00b7dern", "nicht", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "PPER", "$,", "APPR", "PIS", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Was bei ihr Tugend heist, das heist bei andern", "tokens": ["Was", "bei", "ihr", "Tu\u00b7gend", "heist", ",", "das", "heist", "bei", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "ADJD", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die Sch\u00f6nheits Muschen sind, bei andern Nar-", "tokens": ["Die", "Sch\u00f6n\u00b7heits", "Mu\u00b7schen", "sind", ",", "bei", "an\u00b7dern", "Na\u00b7r"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.15": {"text": "Was ihr Gesicht bemahlt, mit Artigkeit ausziert,", "tokens": ["Was", "ihr", "Ge\u00b7sicht", "be\u00b7mahlt", ",", "mit", "Ar\u00b7tig\u00b7keit", "aus\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sind Flekken dadurch sich bei anderen verliert,", "tokens": ["Sind", "Flek\u00b7ken", "da\u00b7durch", "sich", "bei", "an\u00b7de\u00b7ren", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PAV", "PRF", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was man annehmlich heist. Man kan daraus er-", "tokens": ["Was", "man", "an\u00b7nehm\u00b7lich", "heist", ".", "Man", "kan", "da\u00b7raus", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "ADJD", "VAFIN", "$.", "PIS", "VMFIN", "PAV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Da\u00df aus der Eigenlieb, Neid, Tadelsucht entste-", "tokens": ["Da\u00df", "aus", "der", "Ei\u00b7gen\u00b7lieb", ",", "Neid", ",", "Ta\u00b7del\u00b7sucht", "ents\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "NN", "$,", "NN", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Die stets den schielen Blik auf andre Menschen", "tokens": ["Die", "stets", "den", "schie\u00b7len", "Blik", "auf", "and\u00b7re", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Sie zu erniedrigen, sich selbsten zu erh\u00f6hn.", "tokens": ["Sie", "zu", "er\u00b7nied\u00b7ri\u00b7gen", ",", "sich", "selbs\u00b7ten", "zu", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Drum ist die Tadelsucht ein schn\u00f6des Kind der", "tokens": ["Drum", "ist", "die", "Ta\u00b7del\u00b7sucht", "ein", "schn\u00f6\u00b7des", "Kind", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die sonst nichts weis und kan, als andre anzubel-", "tokens": ["Die", "sonst", "nichts", "weis", "und", "kan", ",", "als", "and\u00b7re", "an\u00b7zu\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "PIS", "PTKVZ", "KON", "VMFIN", "$,", "KOUS", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jhr Lohn daf\u00fcr ist nur, das weil sie immer beist,", "tokens": ["Ihr", "Lohn", "da\u00b7f\u00fcr", "ist", "nur", ",", "das", "weil", "sie", "im\u00b7mer", "beist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "VAFIN", "ADV", "$,", "PRELS", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sie sich aus eignen Grimm, zulezt nur selbst zer-", "tokens": ["Sie", "sich", "aus", "eig\u00b7nen", "Grimm", ",", "zu\u00b7lezt", "nur", "selbst", "zer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "APPR", "ADJA", "NE", "$,", "ADV", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}