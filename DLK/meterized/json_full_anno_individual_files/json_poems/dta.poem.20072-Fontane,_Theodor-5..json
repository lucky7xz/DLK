{"dta.poem.20072": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat das Herz viel Todte zu bestatten!", "tokens": ["Es", "hat", "das", "Herz", "viel", "Tod\u00b7te", "zu", "be\u00b7stat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie, die gelebt drin und es ganz besessen,", "tokens": ["Sie", ",", "die", "ge\u00b7lebt", "drin", "und", "es", "ganz", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "VVPP", "ADV", "KON", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verriethen\u2019s oder lernten\u2019s doch vergessen,", "tokens": ["Ver\u00b7ri\u00b7e\u00b7then's", "o\u00b7der", "lern\u00b7ten's", "doch", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie wurden kalt, wie hei\u00df gegl\u00fcht sie hatten.", "tokens": ["Sie", "wur\u00b7den", "kalt", ",", "wie", "hei\u00df", "ge\u00b7gl\u00fcht", "sie", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PWAV", "ADJD", "VVPP", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Besten selbst, und ob einst ohn\u2019 Ermatten", "tokens": ["Die", "Bes\u00b7ten", "selbst", ",", "und", "ob", "einst", "ohn'", "Er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "KON", "KOUS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Lieben sie verschwendrisch zugemessen,", "tokens": ["Ihr", "Lie\u00b7ben", "sie", "ver\u00b7schwend\u00b7risch", "zu\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4ngst pflanzt mein Herz an ihrem Grab Cypressen,", "tokens": ["L\u00e4ngst", "pflanzt", "mein", "Herz", "an", "ih\u00b7rem", "Grab", "Cyp\u00b7res\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie leben noch, und wurden dennoch \u2014 Schatten.", "tokens": ["Sie", "le\u00b7ben", "noch", ",", "und", "wur\u00b7den", "den\u00b7noch", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "VAFIN", "ADV", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein jeder Tag sieht neue Kreuze ragen;", "tokens": ["Ein", "je\u00b7der", "Tag", "sieht", "neu\u00b7e", "Kreu\u00b7ze", "ra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohl weint das Herz, \u2014 doch Mannes-Kraft", "tokens": ["Wohl", "weint", "das", "Herz", ",", "doch", "Man\u00b7nes\u00b7Kraft"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "$(", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "und W\u00fcrde", "tokens": ["und", "W\u00fcr\u00b7de"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Lehrt immer neu ", "tokens": ["Lehrt", "im\u00b7mer", "neu"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nur sollt ich je als schwerste Lebensb\u00fcrde", "tokens": ["Nur", "sollt", "ich", "je", "als", "schwers\u00b7te", "Le\u00b7bens\u00b7b\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Mein Herze f\u00fchlt es, da\u00df es brechen w\u00fcrde.", "tokens": ["Mein", "Her\u00b7ze", "f\u00fchlt", "es", ",", "da\u00df", "es", "bre\u00b7chen", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}