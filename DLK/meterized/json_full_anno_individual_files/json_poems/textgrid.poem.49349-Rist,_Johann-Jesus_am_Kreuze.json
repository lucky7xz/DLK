{"textgrid.poem.49349": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Jesus am Kreuze", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich Christo wil vertrauen,", "tokens": ["Wer", "sich", "Chris\u00b7to", "wil", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "NE", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mu\u00df ihn am Kreuze schauen;", "tokens": ["Der", "mu\u00df", "ihn", "am", "Kreu\u00b7ze", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Viererlei sind hie zu sehn:", "tokens": ["Vie\u00b7rer\u00b7lei", "sind", "hie", "zu", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Erstlich merk' auf seine Wunden,", "tokens": ["Erst\u00b7lich", "merk'", "auf", "sei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Derer f\u00fcnfe sind gefunden,", "tokens": ["De\u00b7rer", "f\u00fcn\u00b7fe", "sind", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als sein Leiden ist geschehn,", "tokens": ["Als", "sein", "Lei\u00b7den", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch die Striemen ausgenommen,", "tokens": ["Doch", "die", "Strie\u00b7men", "aus\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welch' er in der Stadt bekommen.", "tokens": ["Welch'", "er", "in", "der", "Stadt", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sein Reden la\u00df vor allen", "tokens": ["Sein", "Re\u00b7den", "la\u00df", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Stets in deiner Seel' erschallen,", "tokens": ["Stets", "in", "dei\u00b7ner", "Seel'", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn sie sind von Troste reich;", "tokens": ["Denn", "sie", "sind", "von", "Tros\u00b7te", "reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Schaue ferner seine Thr\u00e4nen,", "tokens": ["Schau\u00b7e", "fer\u00b7ner", "sei\u00b7ne", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Die nach deinem Heil sich sehnen,", "tokens": ["Die", "nach", "dei\u00b7nem", "Heil", "sich", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ja dich Armen locken gleich,", "tokens": ["Ja", "dich", "Ar\u00b7men", "lo\u00b7cken", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df du bald in deinen S\u00fcnden", "tokens": ["Da\u00df", "du", "bald", "in", "dei\u00b7nen", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Rat und H\u00fclfe m\u00fcgest finden.", "tokens": ["Rat", "und", "H\u00fcl\u00b7fe", "m\u00fc\u00b7gest", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Schaue, wie sein Herz mu\u00df sterben,", "tokens": ["Schau\u00b7e", ",", "wie", "sein", "Herz", "mu\u00df", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Nur da\u00df er dir m\u00fccht' erwerben", "tokens": ["Nur", "da\u00df", "er", "dir", "m\u00fccht'", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Leben und die Seligkeit.", "tokens": ["Le\u00b7ben", "und", "die", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Merke, wie die sch\u00f6nen Glieder", "tokens": ["Mer\u00b7ke", ",", "wie", "die", "sch\u00f6\u00b7nen", "Glie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Voller Striemen hin und wieder", "tokens": ["Vol\u00b7ler", "Strie\u00b7men", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Sind zermartert in dem Streit,", "tokens": ["Sind", "zer\u00b7mar\u00b7tert", "in", "dem", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Als die Lieb ihn hat getrieben,", "tokens": ["Als", "die", "Lieb", "ihn", "hat", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Da\u00df er todt f\u00fcr dich geblieben.", "tokens": ["Da\u00df", "er", "todt", "f\u00fcr", "dich", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Seht, der Himmelk\u00f6nig schweiget,", "tokens": ["Seht", ",", "der", "Him\u00b7mel\u00b7k\u00f6\u00b7nig", "schwei\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Denn er hat sein H\u00e4ubt geneiget;", "tokens": ["Denn", "er", "hat", "sein", "H\u00e4ubt", "ge\u00b7nei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Meine Seel', hie halte stil,", "tokens": ["Mei\u00b7ne", "Seel'", ",", "hie", "hal\u00b7te", "stil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Fasse doch die Rosenwangen", "tokens": ["Fas\u00b7se", "doch", "die", "Ro\u00b7sen\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Deines Sch\u00f6pfers mit Verlangen,", "tokens": ["Dei\u00b7nes", "Sch\u00f6p\u00b7fers", "mit", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Weil der Herr dich k\u00fcssen wil:", "tokens": ["Weil", "der", "Herr", "dich", "k\u00fcs\u00b7sen", "wil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "K\u00fcsse nun von ganzem Herzen", "tokens": ["K\u00fcs\u00b7se", "nun", "von", "gan\u00b7zem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Christus H\u00e4ubt in Todes Schmerzen.", "tokens": ["Chris\u00b7tus", "H\u00e4ubt", "in", "To\u00b7des", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Schauet die gestochne Seiten,", "tokens": ["Schau\u00b7et", "die", "ge\u00b7stoch\u00b7ne", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Welch' uns mu\u00df den Weg bereiten,", "tokens": ["Welch'", "uns", "mu\u00df", "den", "Weg", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Der zu Gottes Wohnung geht.", "tokens": ["Der", "zu", "Got\u00b7tes", "Woh\u00b7nung", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Keiner sol es unterlassen,", "tokens": ["Kei\u00b7ner", "sol", "es", "un\u00b7ter\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Christus liebes Herz zu fassen,", "tokens": ["Chris\u00b7tus", "lie\u00b7bes", "Herz", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Weil es nun er\u00f6ffnet steht.", "tokens": ["Weil", "es", "nun", "er\u00b7\u00f6ff\u00b7net", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Greife zu mit beiden H\u00e4nden,", "tokens": ["Grei\u00b7fe", "zu", "mit", "bei\u00b7den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Jesus wil sich zu dir wenden.", "tokens": ["Je\u00b7sus", "wil", "sich", "zu", "dir", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Durch sein theures Blutvergie\u00dfen", "tokens": ["Durch", "sein", "theu\u00b7res", "Blut\u00b7ver\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Wil er endlich dich beschlie\u00dfen", "tokens": ["Wil", "er", "end\u00b7lich", "dich", "be\u00b7schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Freundlich in die Gnadenarm';", "tokens": ["Freund\u00b7lich", "in", "die", "Gna\u00b7den\u00b7arm'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.44": {"text": "Seufze nur in deinem Herzen,", "tokens": ["Seuf\u00b7ze", "nur", "in", "dei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Da\u00df er wegen seiner Schmerzen", "tokens": ["Da\u00df", "er", "we\u00b7gen", "sei\u00b7ner", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Deiner Seele sich erbarm'.", "tokens": ["Dei\u00b7ner", "See\u00b7le", "sich", "er\u00b7barm'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "F\u00fcrchte nicht der H\u00f6lle Rachen,", "tokens": ["F\u00fcrch\u00b7te", "nicht", "der", "H\u00f6l\u00b7le", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Jesus wil dich selig machen!", "tokens": ["Je\u00b7sus", "wil", "dich", "se\u00b7lig", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer sich Christo wil vertrauen,", "tokens": ["Wer", "sich", "Chris\u00b7to", "wil", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "NE", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der mu\u00df ihn am Kreuze schauen;", "tokens": ["Der", "mu\u00df", "ihn", "am", "Kreu\u00b7ze", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Viererlei sind hie zu sehn:", "tokens": ["Vie\u00b7rer\u00b7lei", "sind", "hie", "zu", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Erstlich merk' auf seine Wunden,", "tokens": ["Erst\u00b7lich", "merk'", "auf", "sei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Derer f\u00fcnfe sind gefunden,", "tokens": ["De\u00b7rer", "f\u00fcn\u00b7fe", "sind", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als sein Leiden ist geschehn,", "tokens": ["Als", "sein", "Lei\u00b7den", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch die Striemen ausgenommen,", "tokens": ["Doch", "die", "Strie\u00b7men", "aus\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welch' er in der Stadt bekommen.", "tokens": ["Welch'", "er", "in", "der", "Stadt", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sein Reden la\u00df vor allen", "tokens": ["Sein", "Re\u00b7den", "la\u00df", "vor", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Stets in deiner Seel' erschallen,", "tokens": ["Stets", "in", "dei\u00b7ner", "Seel'", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Denn sie sind von Troste reich;", "tokens": ["Denn", "sie", "sind",