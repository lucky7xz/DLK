{"textgrid.poem.57072": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da auf einmal, neunzehnhundert-", "tokens": ["Da", "auf", "ein\u00b7mal", ",", "neun\u00b7zehn\u00b7hun\u00b7der\u00b7t"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ADV", "$,", "TRUNC"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "f\u00fcnf, vernimmt die Welt verwundert,", "tokens": ["f\u00fcnf", ",", "ver\u00b7nimmt", "die", "Welt", "ver\u00b7wun\u00b7dert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "da\u00df die Kirche diesen Mann", "tokens": ["da\u00df", "die", "Kir\u00b7che", "die\u00b7sen", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00fcrder nicht mehr dulden kann.", "tokens": ["f\u00fcr\u00b7der", "nicht", "mehr", "dul\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Grausam schallt von Rom es her:", "tokens": ["Grau\u00b7sam", "schallt", "von", "Rom", "es", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NE", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Expeditus ist nicht mehr!", "tokens": ["Ex\u00b7pe\u00b7di\u00b7tus", "ist", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und da seine lieben Nonnen", "tokens": ["Und", "da", "sei\u00b7ne", "lie\u00b7ben", "Non\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "l\u00e4ngst dem Erdental entronnen,", "tokens": ["l\u00e4ngst", "dem", "Er\u00b7den\u00b7tal", "ent\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "steht er da und sieht sich um \u2013", "tokens": ["steht", "er", "da", "und", "sieht", "sich", "um", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KON", "VVFIN", "PRF", "APPR", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "und die ganze Welt bleibt stumm.", "tokens": ["und", "die", "gan\u00b7ze", "Welt", "bleibt", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich allein hier hoch im Norden", "tokens": ["Ich", "al\u00b7lein", "hier", "hoch", "im", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "f\u00fchle mich von seinem Orden,", "tokens": ["f\u00fch\u00b7le", "mich", "von", "sei\u00b7nem", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "und mein Ketzergriffel schreibt:", "tokens": ["und", "mein", "Ket\u00b7zer\u00b7grif\u00b7fel", "schreibt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sanctus Expeditus \u2013 bleibt.", "tokens": ["Sanc\u00b7tus", "Ex\u00b7pe\u00b7di\u00b7tus", "\u2013", "bleibt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["FM", "FM", "$(", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und weil jenes nichts mehr gilt,", "tokens": ["Und", "weil", "je\u00b7nes", "nichts", "mehr", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "male ich hier neu sein Bild: \u2013", "tokens": ["ma\u00b7le", "ich", "hier", "neu", "sein", "Bild", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Expeditum, den Gesandten,", "tokens": ["Ex\u00b7pe\u00b7di\u00b7tum", ",", "den", "Ge\u00b7sand\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["FM", "$,", "ART", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "gr\u00fc\u00df ich hier, des Unbekannten.", "tokens": ["gr\u00fc\u00df", "ich", "hier", ",", "des", "Un\u00b7be\u00b7kann\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Expeditum, ihn, den Heiligen,", "tokens": ["Ex\u00b7pe\u00b7di\u00b7tum", ",", "ihn", ",", "den", "Hei\u00b7li\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["FM", "$,", "PPER", "$,", "ART", "NN", "$,"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.2": {"text": "mit den F\u00fc\u00dfen, den viel eiligen,", "tokens": ["mit", "den", "F\u00fc\u00b7\u00dfen", ",", "den", "viel", "ei\u00b7li\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.11": {"line.1": {"text": "mit den milden, wei\u00dfen Haaren", "tokens": ["mit", "den", "mil\u00b7den", ",", "wei\u00b7\u00dfen", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und dem fr\u00f6hlichen Gebaren,", "tokens": ["und", "dem", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "mit den Augen braun, voll G\u00fcte,", "tokens": ["mit", "den", "Au\u00b7gen", "braun", ",", "voll", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und mit einer gro\u00dfen D\u00fcte,", "tokens": ["und", "mit", "ei\u00b7ner", "gro\u00b7\u00dfen", "D\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "die den \u00fcberraschten Kindern", "tokens": ["die", "den", "\u00fc\u00b7berr\u00b7aschten", "Kin\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "strebt ihr sp\u00e4rlich Los zu lindern.", "tokens": ["strebt", "ihr", "sp\u00e4r\u00b7lich", "Los", "zu", "lin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Einen g\u00fcldnen Heiligenschein", "tokens": ["Ei\u00b7nen", "g\u00fcld\u00b7nen", "Hei\u00b7li\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "geb ich ihm noch obendrein,", "tokens": ["geb", "ich", "ihm", "noch", "o\u00b7ben\u00b7drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "den sein L\u00e4cheln um ihn breitet,", "tokens": ["den", "sein", "L\u00e4\u00b7cheln", "um", "ihn", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "wenn er durch die Lande schreitet.", "tokens": ["wenn", "er", "durch", "die", "Lan\u00b7de", "schrei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und um ihn in Engelswonnen", "tokens": ["Und", "um", "ihn", "in", "En\u00b7gels\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUI", "PPER", "APPR", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "stell ich seine treuen Nonnen:", "tokens": ["stell", "ich", "sei\u00b7ne", "treu\u00b7en", "Non\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "M\u00e4gdlein aus Italiens Auen,", "tokens": ["M\u00e4gd\u00b7lein", "aus", "I\u00b7ta\u00b7li\u00b7ens", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "himmlisch lieblich anzuschauen.", "tokens": ["himm\u00b7lisch", "lieb\u00b7lich", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Eine aber macht, f\u00fcrwahr,", "tokens": ["Ei\u00b7ne", "a\u00b7ber", "macht", ",", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "eine lange Nase gar.", "tokens": ["ei\u00b7ne", "lan\u00b7ge", "Na\u00b7se", "gar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Just ins \u00bbBronzne Tor\u00ab hinein", "tokens": ["Just", "ins", "\u00bb", "Bronz\u00b7ne", "Tor", "\u00ab", "hin\u00b7ein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "APPRART", "$(", "ADJA", "NN", "$(", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "spannt sie ihr klein Fingerlein.", "tokens": ["spannt", "sie", "ihr", "klein", "Fin\u00b7ger\u00b7lein", "."], "token_info": [