{"textgrid.poem.42170": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Sommer 1898", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich, der alte ", "tokens": ["Ich", ",", "der", "al\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Habe gro\u00dfe Eile,", "tokens": ["Ha\u00b7be", "gro\u00b7\u00dfe", "Ei\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zu verscheuchen w\u00fcnscht ich sehr", "tokens": ["Zu", "ver\u00b7scheu\u00b7chen", "w\u00fcnscht", "ich", "sehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig lange Weile:", "tokens": ["E\u00b7wig", "lan\u00b7ge", "Wei\u00b7le", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lenke wieder meine Bahn,", "tokens": ["Len\u00b7ke", "wie\u00b7der", "mei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlos mir beschieden,", "tokens": ["End\u00b7los", "mir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nach dem alten Kanaan", "tokens": ["Nach", "dem", "al\u00b7ten", "Ka\u00b7naan"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NE"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Das ich lang gemieden.", "tokens": ["Das", "ich", "lang", "ge\u00b7mie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Mir ist in der Ferne die Kunde geworden,", "tokens": ["Mir", "ist", "in", "der", "Fer\u00b7ne", "die", "Kun\u00b7de", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VAPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es k\u00e4me gezogen ein Herrscher von Norden,", "tokens": ["Es", "k\u00e4\u00b7me", "ge\u00b7zo\u00b7gen", "ein", "Herr\u00b7scher", "von", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da setzt es vielleicht auch f\u00fcr mich einen Orden.", "tokens": ["Da", "setzt", "es", "viel\u00b7leicht", "auch", "f\u00fcr", "mich", "ei\u00b7nen", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "R\u00fcckw\u00e4rts schweift mein Auge matt,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "schweift", "mein", "Au\u00b7ge", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Reuevoll umdustert,", "tokens": ["Reu\u00b7e\u00b7voll", "um\u00b7dus\u00b7tert", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nach der alten Judenstadt,", "tokens": ["Nach", "der", "al\u00b7ten", "Ju\u00b7den\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin ich einst geschustert,", "tokens": ["Drin", "ich", "einst", "ge\u00b7schus\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Derart, da\u00df mich heute noch", "tokens": ["Der\u00b7art", ",", "da\u00df", "mich", "heu\u00b7te", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Gottes Welt verachtet,", "tokens": ["Got\u00b7tes", "Welt", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Weil ich nicht den Braten roch,", "tokens": ["Weil", "ich", "nicht", "den", "Bra\u00b7ten", "roch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eh das Lamm geschlachtet!", "tokens": ["Eh", "das", "Lamm", "ge\u00b7schlach\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "W\u00e4r jener gekommen, wie dieser kommt heute,", "tokens": ["W\u00e4r", "je\u00b7ner", "ge\u00b7kom\u00b7men", ",", "wie", "die\u00b7ser", "kommt", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVPP", "$,", "PWAV", "PDS", "VVFIN", "ADV", "$,"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit stolzem Gepr\u00e4nge und gro\u00dfem Geleite,", "tokens": ["Mit", "stol\u00b7zem", "Ge\u00b7pr\u00e4n\u00b7ge", "und", "gro\u00b7\u00dfem", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich w\u00e4re moralisch gegangen nicht Pleite!", "tokens": ["Ich", "w\u00e4\u00b7re", "mo\u00b7ra\u00b7lisch", "ge\u00b7gan\u00b7gen", "nicht", "Plei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Jener ritt die Eselin,", "tokens": ["Je\u00b7ner", "ritt", "die", "E\u00b7se\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser den Trakehner,", "tokens": ["Die\u00b7ser", "den", "Tra\u00b7keh\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Ehr und Gl\u00fcck tr\u00e4gt dieser hin", "tokens": ["Ehr", "und", "Gl\u00fcck", "tr\u00e4gt", "die\u00b7ser", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PDS", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Leben jener.", "tokens": ["Und", "sein", "Le\u00b7ben", "je\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PDAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Durch der Rede reiches Wort", "tokens": ["Durch", "der", "Re\u00b7de", "rei\u00b7ches", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einzig sind die beiden,", "tokens": ["Ein\u00b7zig", "sind", "die", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "PIAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und ihr Ziehn von Ort zu Ort", "tokens": ["Und", "ihr", "Ziehn", "von", "Ort", "zu", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht zu unterscheiden.", "tokens": ["Nicht", "zu", "un\u00b7ter\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Was aber hilft tief mir im Busen die Reue!", "tokens": ["Was", "a\u00b7ber", "hilft", "tief", "mir", "im", "Bu\u00b7sen", "die", "Reu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Versagt ich denn jemals dem Herrscher die Treue?! \u2013", "tokens": ["Ver\u00b7sagt", "ich", "denn", "je\u00b7mals", "dem", "Herr\u00b7scher", "die", "Treu\u00b7e", "?!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Am Ende ereilt mich mein Ungl\u00fcck aufs neue!", "tokens": ["Am", "En\u00b7de", "er\u00b7eilt", "mich", "mein", "Un\u00b7gl\u00fcck", "aufs", "neu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Kam doch auch zu jener Zeit", "tokens": ["Kam", "doch", "auch", "zu", "je\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Kriegerscharen", "tokens": ["Un\u00b7ter", "Krie\u00b7ger\u00b7scha\u00b7ren"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In verbr\u00e4mtem Purpurkleid", "tokens": ["In", "ver\u00b7br\u00e4m\u00b7tem", "Pur\u00b7pur\u00b7kleid"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer angefahren! \u2013 \u2013", "tokens": ["Ei\u00b7ner", "an\u00b7ge\u00b7fah\u00b7ren", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["PIS", "VVPP", "$.", "$(", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn der andre nun auch jetzt", "tokens": ["Wenn", "der", "and\u00b7re", "nun", "auch", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PIS", "ADV", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Beim Erl\u00f6serwerke", "tokens": ["Beim", "Er\u00b7l\u00f6\u00b7ser\u00b7wer\u00b7ke"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sich vor meine T\u00fcre setzt,", "tokens": ["Sich", "vor", "mei\u00b7ne", "T\u00fc\u00b7re", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne da\u00df ich's merke?!", "tokens": ["Oh\u00b7ne", "da\u00df", "ich's", "mer\u00b7ke", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Von ihm stand kein Wort in der Zeitung geschrieben.", "tokens": ["Von", "ihm", "stand", "kein", "Wort", "in", "der", "Zei\u00b7tung", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich h\u00e4tt ihn ja sonst von der Bank nicht vertrieben!", "tokens": ["Ich", "h\u00e4tt", "ihn", "ja", "sonst", "von", "der", "Bank", "nicht", "ver\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und darin ist alles beim alten geblieben. \u2013", "tokens": ["Und", "da\u00b7rin", "ist", "al\u00b7les", "beim", "al\u00b7ten", "ge\u00b7blie\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PAV", "VAFIN", "PIS", "APPRART", "ADJA", "VVPP", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.9": {"line.1": {"text": "Ja, wir Menschen stolpern blind", "tokens": ["Ja", ",", "wir", "Men\u00b7schen", "stol\u00b7pern", "blind"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch des Lebens Enge.", "tokens": ["Durch", "des", "Le\u00b7bens", "En\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Oft ist leer wie Schall und Wind", "tokens": ["Oft", "ist", "leer", "wie", "Schall", "und", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "KOKOM", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00f6\u00dftes Festgepr\u00e4nge.", "tokens": ["Gr\u00f6\u00df\u00b7tes", "Fest\u00b7ge\u00b7pr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Irrt man ehrfurchtsvollen Blicks,", "tokens": ["Irrt", "man", "ehr\u00b7furchts\u00b7vol\u00b7len", "Blicks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ehr und Macht zu suchen,", "tokens": ["Ehr", "und", "Macht", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Kommt der M\u00e4cht'ge hinterr\u00fccks,", "tokens": ["Kommt", "der", "M\u00e4cht'\u00b7ge", "hin\u00b7ter\u00b7r\u00fccks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen zu verfluchen! \u2013", "tokens": ["Ei\u00b7nen", "zu", "ver\u00b7flu\u00b7chen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Es wechseln nicht nur an der B\u00f6rse die Gr\u00f6\u00dfen! \u2013", "tokens": ["Es", "wech\u00b7seln", "nicht", "nur", "an", "der", "B\u00f6r\u00b7se", "die", "Gr\u00f6\u00b7\u00dfen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nichts bleibt uns, inmitten von P\u00fcffen und St\u00f6\u00dfen,", "tokens": ["Nichts", "bleibt", "uns", ",", "in\u00b7mit\u00b7ten", "von", "P\u00fcf\u00b7fen", "und", "St\u00f6\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Als ununterbrochen das Haupt zu entbl\u00f6\u00dfen.", "tokens": ["Als", "un\u00b7un\u00b7ter\u00b7bro\u00b7chen", "das", "Haupt", "zu", "ent\u00b7bl\u00f6\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Ich, der alte ", "tokens": ["Ich", ",", "der", "al\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Habe gro\u00dfe Eile,", "tokens": ["Ha\u00b7be", "gro\u00b7\u00dfe", "Ei\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Zu verscheuchen w\u00fcnscht ich sehr", "tokens": ["Zu", "ver\u00b7scheu\u00b7chen", "w\u00fcnscht", "ich", "sehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ewig lange Weile:", "tokens": ["E\u00b7wig", "lan\u00b7ge", "Wei\u00b7le", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lenke wieder meine Bahn,", "tokens": ["Len\u00b7ke", "wie\u00b7der", "mei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlos mir beschieden,", "tokens": ["End\u00b7los", "mir", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nach dem alten Kanaan", "tokens": ["Nach", "dem", "al\u00b7ten", "Ka\u00b7naan"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NE"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Das ich lang gemieden.", "tokens": ["Das", "ich", "lang", "ge\u00b7mie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Mir ist in der Ferne die Kunde geworden,", "tokens": ["Mir", "ist", "in", "der", "Fer\u00b7ne", "die", "Kun\u00b7de", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "VAPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es k\u00e4me gezogen ein Herrscher von Norden,", "tokens": ["Es", "k\u00e4\u00b7me", "ge\u00b7zo\u00b7gen", "ein", "Herr\u00b7scher", "von", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Da setzt es vielleicht auch f\u00fcr mich einen Orden.", "tokens": ["Da", "setzt", "es", "viel\u00b7leicht", "auch", "f\u00fcr", "mich", "ei\u00b7nen", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.13": {"line.1": {"text": "R\u00fcckw\u00e4rts schweift mein Auge matt,", "tokens": ["R\u00fcck\u00b7w\u00e4rts", "schweift", "mein", "Au\u00b7ge", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Reuevoll umdustert,", "tokens": ["Reu\u00b7e\u00b7voll", "um\u00b7dus\u00b7tert", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nach der alten Judenstadt,", "tokens": ["Nach", "der", "al\u00b7ten", "Ju\u00b7den\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin ich einst geschustert,", "tokens": ["Drin", "ich", "einst", "ge\u00b7schus\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Derart, da\u00df mich heute noch", "tokens": ["Der\u00b7art", ",", "da\u00df", "mich", "heu\u00b7te", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Gottes Welt verachtet,", "tokens": ["Got\u00b7tes", "Welt", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Weil ich nicht den Braten roch,", "tokens": ["Weil", "ich", "nicht", "den", "Bra\u00b7ten", "roch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eh das Lamm geschlachtet!", "tokens": ["Eh", "das", "Lamm", "ge\u00b7schlach\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "W\u00e4r jener gekommen, wie dieser kommt heute,", "tokens": ["W\u00e4r", "je\u00b7ner", "ge\u00b7kom\u00b7men", ",", "wie", "die\u00b7ser", "kommt", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "VVPP", "$,", "PWAV", "PDS", "VVFIN", "ADV", "$,"], "meter": "-+--+--+-+--", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit stolzem Gepr\u00e4nge und gro\u00dfem Geleite,", "tokens": ["Mit", "stol\u00b7zem", "Ge\u00b7pr\u00e4n\u00b7ge", "und", "gro\u00b7\u00dfem", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich w\u00e4re moralisch gegangen nicht Pleite!", "tokens": ["Ich", "w\u00e4\u00b7re", "mo\u00b7ra\u00b7lisch", "ge\u00b7gan\u00b7gen", "nicht", "Plei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.15": {"line.1": {"text": "Jener ritt die Eselin,", "tokens": ["Je\u00b7ner", "ritt", "die", "E\u00b7se\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser den Trakehner,", "tokens": ["Die\u00b7ser", "den", "Tra\u00b7keh\u00b7ner", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Ehr und Gl\u00fcck tr\u00e4gt dieser hin", "tokens": ["Ehr", "und", "Gl\u00fcck", "tr\u00e4gt", "die\u00b7ser", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "PDS", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Leben jener.", "tokens": ["Und", "sein", "Le\u00b7ben", "je\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PDAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Durch der Rede reiches Wort", "tokens": ["Durch", "der", "Re\u00b7de", "rei\u00b7ches", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einzig sind die beiden,", "tokens": ["Ein\u00b7zig", "sind", "die", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "PIAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und ihr Ziehn von Ort zu Ort", "tokens": ["Und", "ihr", "Ziehn", "von", "Ort", "zu", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht zu unterscheiden.", "tokens": ["Nicht", "zu", "un\u00b7ter\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Was aber hilft tief mir im Busen die Reue!", "tokens": ["Was", "a\u00b7ber", "hilft", "tief", "mir", "im", "Bu\u00b7sen", "die", "Reu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Versagt ich denn jemals dem Herrscher die Treue?! \u2013", "tokens": ["Ver\u00b7sagt", "ich", "denn", "je\u00b7mals", "dem", "Herr\u00b7scher", "die", "Treu\u00b7e", "?!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Am Ende ereilt mich mein Ungl\u00fcck aufs neue!", "tokens": ["Am", "En\u00b7de", "er\u00b7eilt", "mich", "mein", "Un\u00b7gl\u00fcck", "aufs", "neu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.17": {"line.1": {"text": "Kam doch auch zu jener Zeit", "tokens": ["Kam", "doch", "auch", "zu", "je\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Kriegerscharen", "tokens": ["Un\u00b7ter", "Krie\u00b7ger\u00b7scha\u00b7ren"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In verbr\u00e4mtem Purpurkleid", "tokens": ["In", "ver\u00b7br\u00e4m\u00b7tem", "Pur\u00b7pur\u00b7kleid"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer angefahren! \u2013 \u2013", "tokens": ["Ei\u00b7ner", "an\u00b7ge\u00b7fah\u00b7ren", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct"], "pos": ["PIS", "VVPP", "$.", "$(", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn der andre nun auch jetzt", "tokens": ["Wenn", "der", "and\u00b7re", "nun", "auch", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "PIS", "ADV", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Beim Erl\u00f6serwerke", "tokens": ["Beim", "Er\u00b7l\u00f6\u00b7ser\u00b7wer\u00b7ke"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sich vor meine T\u00fcre setzt,", "tokens": ["Sich", "vor", "mei\u00b7ne", "T\u00fc\u00b7re", "setzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ohne da\u00df ich's merke?!", "tokens": ["Oh\u00b7ne", "da\u00df", "ich's", "mer\u00b7ke", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.18": {"line.1": {"text": "Von ihm stand kein Wort in der Zeitung geschrieben.", "tokens": ["Von", "ihm", "stand", "kein", "Wort", "in", "der", "Zei\u00b7tung", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich h\u00e4tt ihn ja sonst von der Bank nicht vertrieben!", "tokens": ["Ich", "h\u00e4tt", "ihn", "ja", "sonst", "von", "der", "Bank", "nicht", "ver\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und darin ist alles beim alten geblieben. \u2013", "tokens": ["Und", "da\u00b7rin", "ist", "al\u00b7les", "beim", "al\u00b7ten", "ge\u00b7blie\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PAV", "VAFIN", "PIS", "APPRART", "ADJA", "VVPP", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.19": {"line.1": {"text": "Ja, wir Menschen stolpern blind", "tokens": ["Ja", ",", "wir", "Men\u00b7schen", "stol\u00b7pern", "blind"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch des Lebens Enge.", "tokens": ["Durch", "des", "Le\u00b7bens", "En\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Oft ist leer wie Schall und Wind", "tokens": ["Oft", "ist", "leer", "wie", "Schall", "und", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "KOKOM", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00f6\u00dftes Festgepr\u00e4nge.", "tokens": ["Gr\u00f6\u00df\u00b7tes", "Fest\u00b7ge\u00b7pr\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Irrt man ehrfurchtsvollen Blicks,", "tokens": ["Irrt", "man", "ehr\u00b7furchts\u00b7vol\u00b7len", "Blicks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ehr und Macht zu suchen,", "tokens": ["Ehr", "und", "Macht", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Kommt der M\u00e4cht'ge hinterr\u00fccks,", "tokens": ["Kommt", "der", "M\u00e4cht'\u00b7ge", "hin\u00b7ter\u00b7r\u00fccks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Einen zu verfluchen! \u2013", "tokens": ["Ei\u00b7nen", "zu", "ver\u00b7flu\u00b7chen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.20": {"line.1": {"text": "Es wechseln nicht nur an der B\u00f6rse die Gr\u00f6\u00dfen! \u2013", "tokens": ["Es", "wech\u00b7seln", "nicht", "nur", "an", "der", "B\u00f6r\u00b7se", "die", "Gr\u00f6\u00b7\u00dfen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nichts bleibt uns, inmitten von P\u00fcffen und St\u00f6\u00dfen,", "tokens": ["Nichts", "bleibt", "uns", ",", "in\u00b7mit\u00b7ten", "von", "P\u00fcf\u00b7fen", "und", "St\u00f6\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", 