{"dta.poem.7088": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ungez\u00e4hlte Meng\u2019 entz\u00fcckender Jdeen", "tokens": ["Welch", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Meng'", "ent\u00b7z\u00fc\u00b7cken\u00b7der", "Jdeen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wird nicht all\u2019 Augenblick in ihr entstehen,", "tokens": ["Wird", "nicht", "all'", "Au\u00b7gen\u00b7blick", "in", "ihr", "ent\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sich in allzeit neuem Schimmer,", "tokens": ["Wenn", "sich", "in", "all\u00b7zeit", "neu\u00b7em", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Voll Wunder und voll Licht, viel helle Schau-Pl\u00e4tz immer", "tokens": ["Voll", "Wun\u00b7der", "und", "voll", "Licht", ",", "viel", "hel\u00b7le", "Schau\u00b7Pl\u00e4tz", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$,", "PIAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dem seeligen Gesicht er\u00f6ffnen und entdecken,", "tokens": ["Dem", "see\u00b7li\u00b7gen", "Ge\u00b7sicht", "er\u00b7\u00f6ff\u00b7nen", "und", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ja sich zugleich auf jeden Sinn erstrecken!", "tokens": ["Ja", "sich", "zu\u00b7gleich", "auf", "je\u00b7den", "Sinn", "er\u00b7stre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PRF", "ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie wird sich ihre Lust und ihr\u2019 Erkenntni\u00df mehren,", "tokens": ["Wie", "wird", "sich", "ih\u00b7re", "Lust", "und", "ih\u00b7r'", "Er\u00b7kennt\u00b7ni\u00df", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wenn ihnen eine neue Welt", "tokens": ["Wenn", "ih\u00b7nen", "ei\u00b7ne", "neu\u00b7e", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wird nach der andern vorgestellt!", "tokens": ["Wird", "nach", "der", "an\u00b7dern", "vor\u00b7ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn auf die Mannigfaltigkeit", "tokens": ["Wenn", "auf", "die", "Man\u00b7nig\u00b7fal\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und die verschiedliche Beschaffenheit", "tokens": ["Und", "die", "ver\u00b7schied\u00b7li\u00b7che", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Der Wesen, die in ihnen leben,", "tokens": ["Der", "We\u00b7sen", ",", "die", "in", "ih\u00b7nen", "le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie, voll Verwunderung und Anmuth, Achtung geben.", "tokens": ["Sie", ",", "voll", "Ver\u00b7wun\u00b7de\u00b7rung", "und", "An\u00b7muth", ",", "Ach\u00b7tung", "ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "NN", "KON", "NN", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}