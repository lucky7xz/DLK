{"textgrid.poem.57970": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "8. Jesus zu der Seele", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sprichst, ich m\u00f6ge dich bewirken und bereiten;", "tokens": ["Du", "sprichst", ",", "ich", "m\u00f6\u00b7ge", "dich", "be\u00b7wir\u00b7ken", "und", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun, streck die H\u00e4nde aus und la\u00df mich machen dann!", "tokens": ["Nun", ",", "streck", "die", "H\u00e4n\u00b7de", "aus", "und", "la\u00df", "mich", "ma\u00b7chen", "dann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVIMP", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein eig'ner Will' und Sorg', dein Treiben und Arbeiten", "tokens": ["Dein", "eig'\u00b7ner", "Will'", "und", "Sor\u00b7g'", ",", "dein", "Trei\u00b7ben", "und", "Ar\u00b7bei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "St\u00f6rt deine Ruh und macht, da\u00df ich nicht wirken kann.", "tokens": ["St\u00f6rt", "dei\u00b7ne", "Ruh", "und", "macht", ",", "da\u00df", "ich", "nicht", "wir\u00b7ken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schau nur die Bl\u00fcmlein an bei heiterm Sommerwetter,", "tokens": ["Schau", "nur", "die", "Bl\u00fcm\u00b7lein", "an", "bei", "hei\u00b7term", "Som\u00b7mer\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie halten sich ganz still und \u00f6ffnen ihre Bl\u00e4tter,", "tokens": ["Sie", "hal\u00b7ten", "sich", "ganz", "still", "und", "\u00f6ff\u00b7nen", "ih\u00b7re", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So scheint die Sonne drein und wirket s\u00e4nftiglich;", "tokens": ["So", "scheint", "die", "Son\u00b7ne", "drein", "und", "wir\u00b7ket", "s\u00e4nf\u00b7tig\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So will ich's machen auch, halt dich nur leidentlich!", "tokens": ["So", "will", "ich's", "ma\u00b7chen", "auch", ",", "halt", "dich", "nur", "lei\u00b7dent\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "VVFIN", "ADV", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du 