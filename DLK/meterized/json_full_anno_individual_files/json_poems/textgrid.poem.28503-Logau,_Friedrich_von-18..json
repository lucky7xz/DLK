{"textgrid.poem.28503": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man sucht die Unterthanen, die bey der Krieges-Zeit", "tokens": ["Man", "sucht", "die", "Un\u00b7ter\u00b7tha\u00b7nen", ",", "die", "bey", "der", "Krie\u00b7ge\u00b7sZeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verkrochen und verlauffen sich haben weit und breit;", "tokens": ["Ver\u00b7kro\u00b7chen", "und", "ver\u00b7lauf\u00b7fen", "sich", "ha\u00b7ben", "weit", "und", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PRF", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die durch den Krieg getreten au\u00df Gottes Eyd und Pflicht,", "tokens": ["Die", "durch", "den", "Krieg", "ge\u00b7tre\u00b7ten", "au\u00df", "Got\u00b7tes", "Eyd", "und", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Solt er wol diese lassen und eifrig forschen nicht?", "tokens": ["Solt", "er", "wol", "die\u00b7se", "las\u00b7sen", "und", "eif\u00b7rig", "for\u00b7schen", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PDS", "VVINF", "KON", "ADJD", "VVFIN", "PTKNEG", "$."], "meter": "+-+--+--+-+-+", "measure