{"dta.poem.12742": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die   Themis.  \n F. E. W.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Juristen sagen sonst: da\u00df Themis jungfer sey,", "tokens": ["Ju\u00b7ris\u00b7ten", "sa\u00b7gen", "sonst", ":", "da\u00df", "The\u00b7mis", "jung\u00b7fer", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$.", "KOUS", "NE", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gleichwol hat sie schon offt nothzucht ausgestanden;", "tokens": ["Und", "gleich\u00b7wol", "hat", "sie", "schon", "offt", "noth\u00b7zucht", "aus\u00b7ge\u00b7stan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer pflichtet mir daher hierinn nicht v\u00f6llig bey:", "tokens": ["Wer", "pflich\u00b7tet", "mir", "da\u00b7her", "hie\u00b7rinn", "nicht", "v\u00f6l\u00b7lig", "bey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PAV", "ADV", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df keine jungfer fast auf erden mehr verhanden.", "tokens": ["Da\u00df", "kei\u00b7ne", "jung\u00b7fer", "fast", "auf", "er\u00b7den", "mehr", "ver\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "ADV", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}