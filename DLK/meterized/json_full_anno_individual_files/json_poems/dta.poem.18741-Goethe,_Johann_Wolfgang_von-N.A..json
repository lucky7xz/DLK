{"dta.poem.18741": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich einmal eine Spinne erschlagen,", "tokens": ["Als", "ich", "ein\u00b7mal", "ei\u00b7ne", "Spin\u00b7ne", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dacht ich ob ich das wohl gesollt?", "tokens": ["Dacht", "ich", "ob", "ich", "das", "wohl", "ge\u00b7sollt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "PPER", "PDS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat Gott ihr doch wie mir gewollt", "tokens": ["Hat", "Gott", "ihr", "doch", "wie", "mir", "ge\u00b7wollt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "PPER", "ADV", "KOKOM", "PPER", "VMPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einen Antheil an diesen Tagen!", "tokens": ["Ei\u00b7nen", "An\u00b7theil", "an", "die\u00b7sen", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}