{"dta.poem.15647": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer zweien Herren mu\u00df zugleich seyn unterthan,", "tokens": ["Wer", "zwei\u00b7en", "Her\u00b7ren", "mu\u00df", "zu\u00b7gleich", "seyn", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "CARD", "NN", "VMFIN", "ADV", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem geht es schief, alswie dem Mond auf seiner Bahn;", "tokens": ["Dem", "geht", "es", "schief", ",", "als\u00b7wie", "dem", "Mond", "auf", "sei\u00b7ner", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$,", "KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der, von der Erde hier, der Sonne dort gezogen,", "tokens": ["Der", ",", "von", "der", "Er\u00b7de", "hier", ",", "der", "Son\u00b7ne", "dort", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beschreibt am Himmelskreis so unstet seinen Bogen.", "tokens": ["Be\u00b7schreibt", "am", "Him\u00b7mels\u00b7kreis", "so", "un\u00b7stet", "sei\u00b7nen", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "m