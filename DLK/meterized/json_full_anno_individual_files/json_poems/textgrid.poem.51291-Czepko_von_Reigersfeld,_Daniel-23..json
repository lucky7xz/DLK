{"textgrid.poem.51291": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey dieser schweren Zeit was sol der Mensch beginnen?", "tokens": ["Bey", "die\u00b7ser", "schwe\u00b7ren", "Zeit", "was", "sol", "der", "Mensch", "be\u00b7gin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er lebt nicht, lebt er nicht und gibt sich in die Sinnen.", "tokens": ["Er", "lebt", "nicht", ",", "lebt", "er", "nicht", "und", "gibt", "sich", "in", "die", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PPER", "PTKNEG", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer richtet Aecker zu? Der Krieger erndtet ein.", "tokens": ["Wer", "rich\u00b7tet", "A\u00b7e\u00b7cker", "zu", "?", "Der", "Krie\u00b7ger", "ernd\u00b7tet", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wer baut? nicht du, der Feind wil Kayser drinnen seyn.", "tokens": ["Wer", "baut", "?", "nicht", "du", ",", "der", "Feind", "wil", "Kay\u00b7ser", "drin\u00b7nen", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PTKNEG", "PPER", "$,", "ART", "NN", "VMFIN", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer liest ein Buch? Du kanst draus keine Steuer haben.", "tokens": ["Wer", "liest", "ein", "Buch", "?", "Du", "kanst", "draus", "kei\u00b7ne", "Steu\u00b7er", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "PPER", "VMFIN", "PAV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer nihmt ein Weib? Man kan dich wol allein begraben.", "tokens": ["Wer", "nihmt", "ein", "Weib", "?", "Man", "kan", "dich", "wol", "al\u00b7lein", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "PIS", "VMFIN", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer ziehet Kinder auff? Die Mutter sitzt und weint,", "tokens": ["Wer", "zie\u00b7het", "Kin\u00b7der", "auff", "?", "Die", "Mut\u00b7ter", "sitzt", "und", "weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie gebohren hat, und f\u00fcrchtet stets den Feind.", "tokens": ["Da\u00df", "sie", "ge\u00b7boh\u00b7ren", "hat", ",", "und", "f\u00fcrch\u00b7tet", "stets", "den", "Feind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So \u00fcbel ists bestellt. Doch wilt du in dir leben,", "tokens": ["So", "\u00fc\u00b7bel", "ists", "be\u00b7stellt", ".", "Doch", "wilt", "du", "in", "dir", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "VVPP", "$.", "KON", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlierest du wol nichts, must du es gleich begeben.", "tokens": ["Ver\u00b7lie\u00b7rest", "du", "wol", "nichts", ",", "must", "du", "es", "gleich", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "$,", "VMFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Feind raub ihm Gutt und Haab, er giebt es willig hin,", "tokens": ["Feind", "raub", "ihm", "Gutt", "und", "Ha\u00b7ab", ",", "er", "giebt", "es", "wil\u00b7lig", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "KON", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+--+-++-+-+-+", "measure": "iambic.septa.invert"}, "line.4": {"text": "Verbrennt ihm Haus und Hoff, kein Feuer kommt in Sinn.", "tokens": ["Ver\u00b7brennt", "ihm", "Haus", "und", "Hoff", ",", "kein", "Feu\u00b7er", "kommt", "in", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "VVFIN", "$,", "PIAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "La\u00df ihm kein Buch? er kan aus der Natur viel lesen,", "tokens": ["La\u00df", "ihm", "kein", "Buch", "?", "er", "kan", "aus", "der", "Na\u00b7tur", "viel", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "$.", "PPER", "VMFIN", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "T\u00f6dt ihm sein Weib? er w\u00fcnscht ihm also zu genesen.", "tokens": ["T\u00f6dt", "ihm", "sein", "Weib", "?", "er", "w\u00fcnscht", "ihm", "al\u00b7so", "zu", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Steig in der Kinder Leib mit einer Partisan?", "tokens": ["Steig", "in", "der", "Kin\u00b7der", "Leib", "mit", "ei\u00b7ner", "Par\u00b7ti\u00b7san", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du triffst den Vater doch nicht weinend alldar an.", "tokens": ["Du", "triffst", "den", "Va\u00b7ter", "doch", "nicht", "wei\u00b7nend", "all\u00b7dar", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gesetzt, er sterbe gar. Viel l\u00e4nger kann er sterben,", "tokens": ["Ge\u00b7setzt", ",", "er", "ster\u00b7be", "gar", ".", "Viel", "l\u00e4n\u00b7ger", "kann", "er", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "ADV", "$.", "ADV", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als du ihn plagen wirst, als du ihn wirst verterben,", "tokens": ["Als", "du", "ihn", "pla\u00b7gen", "wirst", ",", "als", "du", "ihn", "wirst", "ver\u00b7ter\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn alles was du nihmst, ist ihm nur schlecht verpflicht,", "tokens": ["Denn", "al\u00b7les", "was", "du", "nihmst", ",", "ist", "ihm", "nur", "schlecht", "ver\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie sehr du haust und schl\u00e4gst, die Seele triffst du nicht.", "tokens": ["Wie", "sehr", "du", "haust", "und", "schl\u00e4gst", ",", "die", "See\u00b7le", "triffst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "La\u00df nur, was Gott verh\u00e4ngt, o Mensch, la\u00df es geschehen,", "tokens": ["La\u00df", "nur", ",", "was", "Gott", "ver\u00b7h\u00e4ngt", ",", "o", "Mensch", ",", "la\u00df", "es", "ge