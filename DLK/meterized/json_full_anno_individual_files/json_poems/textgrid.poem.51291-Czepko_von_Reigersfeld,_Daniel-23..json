 "den", "Va\u00b7ter", "doch", "nicht", "wei\u00b7nend", "all\u00b7dar", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Gesetzt, er sterbe gar. Viel l\u00e4nger kann er sterben,", "tokens": ["Ge\u00b7setzt", ",", "er", "ster\u00b7be", "gar", ".", "Viel", "l\u00e4n\u00b7ger", "kann", "er", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "ADV", "$.", "ADV", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als du ihn plagen wirst, als du ihn wirst verterben,", "tokens": ["Als", "du", "ihn", "pla\u00b7gen", "wirst", ",", "als", "du", "ihn", "wirst", "ver\u00b7ter\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "VAFIN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn alles was du nihmst, ist ihm nur schlecht verpflicht,", "tokens": ["Denn", "al\u00b7les", "was", "du", "nihmst", ",", "ist", "ihm", "nur", "schlecht", "ver\u00b7pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie sehr du haust und schl\u00e4gst, die Seele triffst du nicht.", "tokens": ["Wie", "sehr", "du", "haust", "und", "schl\u00e4gst", ",", "die", "See\u00b7le", "triffst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "La\u00df nur, was Gott verh\u00e4ngt, o Mensch, la\u00df es geschehen,", "tokens": ["La\u00df", "nur", ",", "was", "Gott", "ver\u00b7h\u00e4ngt", ",", "o", "Mensch", ",", "la\u00df", "es", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PRELS", "NN", "VVFIN", "$,", "FM", "NN", "$,", "VVIMP", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steh auff der Seiten du, du kanst befreyt zu sehen.", "tokens": ["Steh", "auff", "der", "Sei\u00b7ten", "du", ",", "du", "kanst", "be\u00b7freyt", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "$,", "PPER", "VMFIN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}