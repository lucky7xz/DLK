{"dta.poem.13559": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "nl:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ihm ists verlieh'n, aus den verworrnen Tagen,", "tokens": ["Ihm", "ists", "ver\u00b7lieh'n", ",", "aus", "den", "ver\u00b7worr\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die um die andern sich wie Kerker dichten,", "tokens": ["Die", "um", "die", "an\u00b7dern", "sich", "wie", "Ker\u00b7ker", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "PRF", "KOKOM", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum blauen Himmel sich emporzurichten,", "tokens": ["Zum", "blau\u00b7en", "Him\u00b7mel", "sich", "em\u00b7por\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Freudigkeit: Hie bin ich, Herr! zu sagen.", "tokens": ["In", "Freu\u00b7dig\u00b7keit", ":", "Hie", "bin", "ich", ",", "Herr", "!", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "ADV", "VAFIN", "PPER", "$,", "NN", "$.", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}