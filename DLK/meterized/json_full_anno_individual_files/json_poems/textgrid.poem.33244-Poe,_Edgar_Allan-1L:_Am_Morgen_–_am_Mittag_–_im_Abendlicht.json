{"textgrid.poem.33244": {"metadata": {"author": {"name": "Poe, Edgar Allan", "birth": "N.A.", "death": "N.A."}, "title": "1L: Am Morgen \u2013 am Mittag \u2013 im Abendlicht", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Morgen \u2013 am Mittag \u2013 im Abendlicht", "tokens": ["Am", "Mor\u00b7gen", "\u2013", "am", "Mit\u00b7tag", "\u2013", "im", "A\u00b7bend\u00b7licht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$(", "APPRART", "NN", "$(", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vernahmst Du, Maria, mein Lobgedicht.", "tokens": ["Ver\u00b7nahmst", "Du", ",", "Ma\u00b7ria", ",", "mein", "Lob\u00b7ge\u00b7dicht", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In Lust und Leid \u2013 in Wonne und Weh,", "tokens": ["In", "Lust", "und", "Leid", "\u2013", "in", "Won\u00b7ne", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Gott-Mutter, auch fernerhin mit mir geh!", "tokens": ["Got\u00b7tMut\u00b7ter", ",", "auch", "fer\u00b7ner\u00b7hin", "mit", "mir", "geh", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+----+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Als strahlende Stunden heiter entwichen", "tokens": ["Als", "strah\u00b7len\u00b7de", "Stun\u00b7den", "hei\u00b7ter", "ent\u00b7wi\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und keine Wolken den Himmel durchstrichen,", "tokens": ["Und", "kei\u00b7ne", "Wol\u00b7ken", "den", "Him\u00b7mel", "durch\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "F\u00fchrtest Du gn\u00e4dig die Seele mir", "tokens": ["F\u00fchr\u00b7test", "Du", "gn\u00e4\u00b7dig", "die", "See\u00b7le", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "PPER"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Hin zu den Deinen, hin zu Dir.", "tokens": ["Hin", "zu", "den", "Dei\u00b7nen", ",", "hin", "zu", "Dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nun, da Schicksalsst\u00fcrme schrecken,", "tokens": ["Nun", ",", "da", "Schick\u00b7sals\u00b7st\u00fcr\u00b7me", "schre\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dunkel mein Heute, mein Gestern bedecken,", "tokens": ["Dun\u00b7kel", "mein", "Heu\u00b7te", ",", "mein", "Ge\u00b7stern", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "La\u00df mein Morgen strahlend scheinen", "tokens": ["La\u00df",