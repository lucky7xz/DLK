{"dta.poem.8933": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  An K\u00f6nig Ludwig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Zu dir sich auf, mit Trauer und Stolz zugleich:", "tokens": ["Zu", "dir", "sich", "auf", ",", "mit", "Trau\u00b7er", "und", "Stolz", "zu\u00b7gleich", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "PTKVZ", "$,", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vertrau'n im Blick, im Munde Wahrheit,", "tokens": ["Ver\u00b7trau'n", "im", "Blick", ",", "im", "Mun\u00b7de", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schw\u00f6rt es dem Sohne der Wittelsbacher.", "tokens": ["Schw\u00f6rt", "es", "dem", "Soh\u00b7ne", "der", "Wit\u00b7tels\u00b7ba\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Des Thrones glatte Schwelle, wie selbstbewu\u00dft,", "tokens": ["Des", "Thro\u00b7nes", "glat\u00b7te", "Schwel\u00b7le", ",", "wie", "selbst\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie fest betrittst du sie, wie gereift im Geist!", "tokens": ["Wie", "fest", "be\u00b7trittst", "du", "sie", ",", "wie", "ge\u00b7reift", "im", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPER", "$,", "PWAV", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ja, leichter hebt dein freyes Haupt sich,", "tokens": ["Ja", ",", "leich\u00b7ter", "hebt", "dein", "frey\u00b7es", "Haupt", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "PRF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit die metallene Last ihm zufiel.", "tokens": ["Seit", "die", "me\u00b7tal\u00b7le\u00b7ne", "Last", "ihm", "zu\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Dir schwellt erhab'ne G\u00fcte das Herz, mit ihr,", "tokens": ["Dir", "schwellt", "er\u00b7hab'\u00b7ne", "G\u00fc\u00b7te", "das", "Herz", ",", "mit", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ART", "NN", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was mehr noch frommt als G\u00fcte \u2014 der tiefe Sinn:", "tokens": ["Was", "mehr", "noch", "frommt", "als", "G\u00fc\u00b7te", "der", "tie\u00b7fe", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "VVFIN", "KOUS", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo dieser Sch\u00f6pfer mangelt, seh'n wir", "tokens": ["Wo", "die\u00b7ser", "Sch\u00f6p\u00b7fer", "man\u00b7gelt", ",", "seh'n", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "VVFIN", "$,", "VVFIN", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Alles zerst\u00fcckelt und schnell verungl\u00fcckt.", "tokens": ["Al\u00b7les", "zer\u00b7st\u00fc\u00b7ckelt", "und", "schnell", "ver\u00b7un\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein Auge sp\u00e4hte durch die Vergangenheit,", "tokens": ["Dein", "Au\u00b7ge", "sp\u00e4h\u00b7te", "durch", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es lag das Buch der Zeiten auf deinem Knie,", "tokens": ["Es", "lag", "das", "Buch", "der", "Zei\u00b7ten", "auf", "dei\u00b7nem", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gedanken pfl\u00fccktest du, wie Blumen,", "tokens": ["Ge\u00b7dan\u00b7ken", "pfl\u00fcck\u00b7test", "du", ",", "wie", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber dem Grabe der deutschen Vorwelt.", "tokens": ["Ue\u00b7ber", "dem", "Gra\u00b7be", "der", "deut\u00b7schen", "Vor\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Dein Volk, du kennst es. Jeglichem Zeitgeschick,", "tokens": ["Dein", "Volk", ",", "du", "kennst", "es", ".", "Jeg\u00b7li\u00b7chem", "Zeit\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "$.", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das ihm zu Theil ward, f\u00fchltest und sannst du nach,", "tokens": ["Das", "ihm", "zu", "Theil", "ward", ",", "f\u00fchl\u00b7test", "und", "sannst", "du", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VAFIN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und still, in eigner Brust verheimlicht,", "tokens": ["Und", "still", ",", "in", "eig\u00b7ner", "Brust", "ver\u00b7heim\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trugst du den lachenden Lenz der Zukunft.", "tokens": ["Trugst", "du", "den", "la\u00b7chen\u00b7den", "Lenz", "der", "Zu\u00b7kunft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Du hast mit uns erlitten den Fluch des Kriegs,", "tokens": ["Du", "hast", "mit", "uns", "er\u00b7lit\u00b7ten", "den", "Fluch", "des", "Kriegs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gez\u00e4hlt die Todesnarben der J\u00fcnglinge,", "tokens": ["Ge\u00b7z\u00e4hlt", "die", "To\u00b7des\u00b7nar\u00b7ben", "der", "J\u00fcng\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die deiner Ahnherrn Strom, der Rhein, sah", "tokens": ["Die", "dei\u00b7ner", "Ahn\u00b7herrn", "Strom", ",", "der", "Rhein", ",", "sah"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "$,", "PRELS", "NE", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seelen verhauchen f\u00fcr deutsche Freyheit.", "tokens": ["See\u00b7len", "ver\u00b7hau\u00b7chen", "f\u00fcr", "deut\u00b7sche", "Frey\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Und nicht umsonst verhauchen, du f\u00fchlst es wohl!", "tokens": ["Und", "nicht", "um\u00b7sonst", "ver\u00b7hau\u00b7chen", ",", "du", "f\u00fchlst", "es", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVINF", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach jenes C\u00e4sars tragischem Untergang,", "tokens": ["Nach", "je\u00b7nes", "C\u00e4\u00b7sars", "tra\u00b7gi\u00b7schem", "Un\u00b7ter\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Was k\u00f6nnten klein're Scheindespoten", "tokens": ["Was", "k\u00f6nn\u00b7ten", "klein'\u00b7re", "Schein\u00b7des\u00b7po\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Anders erregen, als frost'ges Lachen?", "tokens": ["An\u00b7ders", "er\u00b7re\u00b7gen", ",", "als", "frost'\u00b7ges", "La\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Du aber theilst die heilige Gluth mit uns,", "tokens": ["Du", "a\u00b7ber", "theilst", "die", "hei\u00b7li\u00b7ge", "Gluth", "mit", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vor der in Staub sank jener gepr\u00fcfte Held,", "tokens": ["Vor", "der", "in", "Staub", "sank", "je\u00b7ner", "ge\u00b7pr\u00fcf\u00b7te", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NN", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und fallen lie\u00dfest du mit uns ihr", "tokens": ["Und", "fal\u00b7len", "lie\u00b7\u00dfest", "du", "mit", "uns", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "APPR", "PPER", "PPOSAT"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eine begeisterte, warme Thr\u00e4ne.", "tokens": ["Ei\u00b7ne", "be\u00b7geis\u00b7ter\u00b7te", ",", "war\u00b7me", "Thr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Dem Stein des Rechts, den edelgesinnt und treu", "tokens": ["Dem", "Stein", "des", "Rechts", ",", "den", "e\u00b7del\u00b7ge\u00b7sinnt", "und", "treu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Vater legte, bl\u00e4sest du Athem ein,", "tokens": ["Dein", "Va\u00b7ter", "leg\u00b7te", ",", "bl\u00e4\u00b7sest", "du", "A\u00b7them", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du siehst im Marmor keinen Marmor,", "tokens": ["Du", "siehst", "im", "Mar\u00b7mor", "kei\u00b7nen", "Mar\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber ein k\u00fcnftiges Jovisantlitz.", "tokens": ["A\u00b7ber", "ein", "k\u00fcnf\u00b7ti\u00b7ges", "Jo\u00b7vi\u00b7sant\u00b7litz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.10": {"line.1": {"text": "Allein wie sehr du W\u00fcnsche des Tags verstehst,", "tokens": ["Al\u00b7lein", "wie", "sehr", "du", "W\u00fcn\u00b7sche", "des", "Tags", "ver\u00b7stehst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "PPER", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht horchst du blindlings jedem Ger\u00e4usch, du nimmst", "tokens": ["Nicht", "horchst", "du", "blind\u00b7lings", "je\u00b7dem", "Ge\u00b7r\u00e4usch", ",", "du", "nimmst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Zepter, jenem Joseph ungleich,", "tokens": ["Den", "Zep\u00b7ter", ",", "je\u00b7nem", "Jo\u00b7se\u00b7ph", "un\u00b7gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDAT", "NE", "ADJD", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht in die weltliche Faust der Neu'rung.", "tokens": ["Nicht", "in", "die", "welt\u00b7li\u00b7che", "Faust", "der", "Neu'\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Ehrfurcht erweckt, was V\u00e4ter gethan, in dir,", "tokens": ["Ehr\u00b7furcht", "er\u00b7weckt", ",", "was", "V\u00e4\u00b7ter", "ge\u00b7than", ",", "in", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PWS", "NN", "VVPP", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du f\u00fchlst verj\u00e4hrter Zeiten Bedeutsamkeit,", "tokens": ["Du", "f\u00fchlst", "ver\u00b7j\u00e4hr\u00b7ter", "Zei\u00b7ten", "Be\u00b7deut\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In's Wappenschild uralter Sitte", "tokens": ["In's", "Wap\u00b7pen\u00b7schild", "ur\u00b7al\u00b7ter", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcgst du die Rosen der j\u00fcngsten Freiheit.", "tokens": ["F\u00fcgst", "du", "die", "Ro\u00b7sen", "der", "j\u00fcng\u00b7sten", "Frei\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Heil dir und Heil der Lieblichen neben dir,", "tokens": ["Heil", "dir", "und", "Heil", "der", "Lieb\u00b7li\u00b7chen", "ne\u00b7ben", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Heil jedem Spr\u00f6\u00dfling, welchen sie dir gebar!", "tokens": ["Heil", "je\u00b7dem", "Spr\u00f6\u00df\u00b7ling", ",", "wel\u00b7chen", "sie", "dir", "ge\u00b7bar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PWAT", "PPER", "PPER", "ADJD", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wenn Kinder dich und Volk umjubeln,", "tokens": ["Wenn", "Kin\u00b7der", "dich", "und", "Volk", "um\u00b7ju\u00b7beln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Leerst du, als Becher, des Segens F\u00fcllhorn!", "tokens": ["Leerst", "du", ",", "als", "Be\u00b7cher", ",", "des", "Se\