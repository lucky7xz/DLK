{"textgrid.poem.43031": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Der Linken", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weit in die Ferne m\u00f6gt Ihr fl\u00fcchten,", "tokens": ["Weit", "in", "die", "Fer\u00b7ne", "m\u00f6gt", "Ihr", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die einst das Volk hierher gesandt,", "tokens": ["Die", "einst", "das", "Volk", "hier\u00b7her", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein stattlich Wohnhaus zu errichten,", "tokens": ["Ein", "statt\u00b7lich", "Wohn\u00b7haus", "zu", "er\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo sonst der Willk\u00fcr Zwingburg stand.", "tokens": ["Wo", "sonst", "der", "Will\u00b7k\u00fcr", "Zwing\u00b7burg", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf freier Schweizer freien Bergen", "tokens": ["Auf", "frei\u00b7er", "Schwei\u00b7zer", "frei\u00b7en", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "M\u00f6gt vor Verrath Ihr sicher sein;", "tokens": ["M\u00f6gt", "vor", "Ver\u00b7rath", "Ihr", "si\u00b7cher", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn seht! sie suchen schon nach Schergen,", "tokens": ["Denn", "seht", "!", "sie", "su\u00b7chen", "schon", "nach", "Scher\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Die Tirannei von Gottes Gnaden", "tokens": ["Die", "Ti\u00b7ran\u00b7nei", "von", "Got\u00b7tes", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will nur ein demuthsvoll Gebet; \u2013", "tokens": ["Will", "nur", "ein", "de\u00b7muths\u00b7voll", "Ge\u00b7bet", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr spracht zu k\u00fchn von Barrikaden", "tokens": ["Ihr", "spracht", "zu", "k\u00fchn", "von", "Bar\u00b7ri\u00b7ka\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKA", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und von des ", "tokens": ["Und", "von", "des"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Ihr habt, nach Brandenburg entboten,", "tokens": ["Ihr", "habt", ",", "nach", "Bran\u00b7den\u00b7burg", "ent\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Ukas der Gewalt verlacht,", "tokens": ["Den", "U\u00b7kas", "der", "Ge\u00b7walt", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr habt, als Bajonette drohten,", "tokens": ["Ihr", "habt", ",", "als", "Ba\u00b7jo\u00b7net\u00b7te", "droh\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur an des ", "tokens": ["Nur", "an", "des"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "\u00bbfort mit dem eitlen Tand der Orden", "tokens": ["\u00bb", "fort", "mit", "dem", "eit\u00b7len", "Tand", "der", "Or\u00b7den"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mit dem Adelspergament,", "tokens": ["Und", "mit", "dem", "A\u00b7del\u00b7sper\u00b7ga\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das l\u00e4ngst der W\u00fcrmer Raub geworden!\u00ab", "tokens": ["Das", "l\u00e4ngst", "der", "W\u00fcr\u00b7mer", "Raub", "ge\u00b7wor\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "NN", "VAPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer ist, der den Beschlu\u00df nicht kennt?", "tokens": ["Wer", "ist", ",", "der", "den", "Be\u00b7schlu\u00df", "nicht", "kennt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Das war zuviel der guten Gabe;", "tokens": ["Das", "war", "zu\u00b7viel", "der", "gu\u00b7ten", "Ga\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man gibt Euch Fersengeld in Kauf, \u2013", "tokens": ["Man", "gibt", "Euch", "Fer\u00b7sen\u00b7geld", "in", "Kauf", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und grinsend steht aus seinem Grabe", "tokens": ["Und", "grin\u00b7send", "steht", "aus", "sei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was soll das Puppenspiel uns frommen,", "tokens": ["Was", "soll", "das", "Pup\u00b7pen\u00b7spiel", "uns", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bereitet von h\u00f6chsteigner Hand?", "tokens": ["Be\u00b7rei\u00b7tet", "von", "h\u00f6chs\u00b7teig\u00b7ner", "Hand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es ", "tokens": ["Es"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Die Sonne hat zu hei\u00df gebrannt;", "tokens": ["Die", "Son\u00b7ne", "hat", "zu", "hei\u00df", "ge\u00b7brannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sklaven bunte Weihrauchwolke", "tokens": ["Der", "Skla\u00b7ven", "bun\u00b7te", "Weih\u00b7rauch\u00b7wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sch\u00f6ne Worte helfen nicht \u2013", "tokens": ["Und", "sch\u00f6\u00b7ne", "Wor\u00b7te", "hel\u00b7fen", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Wie leicht ein Wort, ein Eid sich bricht.", "tokens": ["Wie", "leicht", "ein", "Wort", ",", "ein", "Eid", "sich", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Weit in die Ferne m\u00f6gt Ihr fl\u00fcchten,", "tokens": ["Weit", "in", "die", "Fer\u00b7ne", "m\u00f6gt", "Ihr", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "li