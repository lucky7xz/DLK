{"textgrid.poem.41396": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schwimmer", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wagte sich einst in den Rhein", "tokens": ["Es", "wag\u00b7te", "sich", "einst", "in", "den", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "ART", "NE"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Ein Baccalaureus, der nie zuvor geschwommen.", "tokens": ["Ein", "Bac\u00b7ca\u00b7lau\u00b7reus", ",", "der", "nie", "zu\u00b7vor", "ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ADV", "ADV", "VVPP", "$."], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Vom Ufer mocht' er kaum f\u00fcnf ganzer Schritte sein,", "tokens": ["Vom", "U\u00b7fer", "mocht'", "er", "kaum", "f\u00fcnf", "gan\u00b7zer", "Schrit\u00b7te", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "CARD", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So steckt' er schon im Schilf, fing zappelnd an zu schrein,", "tokens": ["So", "steckt'", "er", "schon", "im", "Schilf", ",", "fing", "zap\u00b7pelnd", "an", "zu", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ward, auf sein Geschrei, von Fischern aufgenommen.", "tokens": ["Und", "ward", ",", "auf", "sein", "Ge\u00b7schrei", ",", "von", "Fi\u00b7schern", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die brachten ihn ans Land; der Dienst war ungemein.", "tokens": ["Die", "brach\u00b7ten", "ihn", "ans", "Land", ";", "der", "Dienst", "war", "un\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "$.", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er dankt daf\u00fcr, und spricht: da schwimm' ein andrer hin!", "tokens": ["Er", "dankt", "da\u00b7f\u00fcr", ",", "und", "spricht", ":", "da", "schwimm'", "ein", "an\u00b7drer", "hin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KON", "VVFIN", "$.", "ADV", "VVFIN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich will, das schw\u00f6r' ich euch, nicht eh' ins Wasser kommen,", "tokens": ["Ich", "will", ",", "das", "schw\u00f6r'", "ich", "euch", ",", "nicht", "eh'", "ins", "Was\u00b7ser", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PDS", "VVFIN", "PPER", "PPER", "$,", "PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als bis ich ganz und gar im Schwimmen Meister bin.", "tokens": ["Als", "bis", 