{"dta.poem.1274": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die junge Frau und der alte Mann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die gantze Nacht sitzt mir der Floh in Ohren:", "tokens": ["Die", "gant\u00b7ze", "Nacht", "sitzt", "mir", "der", "Floh", "in", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Alter schnarcht/ wenn ich die Ruh verlohren.", "tokens": ["Mein", "Al\u00b7ter", "schnarcht", "/", "wenn", "ich", "die", "Ruh", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er kehret mir den kalten R\u00fccken zu/", "tokens": ["Er", "keh\u00b7ret", "mir", "den", "kal\u00b7ten", "R\u00fc\u00b7cken", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ich mit ihm am allersch\u00f6nsten thu.", "tokens": ["Wenn", "ich", "mit", "ihm", "am", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "thu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Hitze Macht kan Ei\u00df und Steine zwingen/", "tokens": ["Der", "Hit\u00b7ze", "Macht", "kan", "Ei\u00df", "und", "Stei\u00b7ne", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und keinen Safft aus diesem Felsen bringen.", "tokens": ["Und", "kei\u00b7nen", "Safft", "aus", "die\u00b7sem", "Fel\u00b7sen", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er f\u00fchlt vor mich zu wenig/ ich zu viel/", "tokens": ["Er", "f\u00fchlt", "vor", "mich", "zu", "we\u00b7nig", "/", "ich", "zu", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKA", "PIS", "$(", "PPER", "PTKA", "PIS", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Karth entf\u00e4llt ihm/ wenn ich spielen will.", "tokens": ["Die", "Karth", "ent\u00b7f\u00e4llt", "ihm", "/", "wenn", "ich", "spie\u00b7len", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mein Mund/ gewohnt den Marmor zu entz\u00fcnden/", "tokens": ["Mein", "Mund", "/", "ge\u00b7wohnt", "den", "Mar\u00b7mor", "zu", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVPP", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kan keine Glutt in seiner Asche finden.", "tokens": ["Kan", "kei\u00b7ne", "Glutt", "in", "sei\u00b7ner", "A\u00b7sche", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein s\u00fcsser Ku\u00df/ mein Zug der linden Hand", "tokens": ["Mein", "s\u00fcs\u00b7ser", "Ku\u00df", "/", "mein", "Zug", "der", "lin\u00b7den", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wird nur bey ihm vergebens angewandt.", "tokens": ["Wird", "nur", "bey", "ihm", "ver\u00b7ge\u00b7bens", "an\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Komm/ Liebe/ komm/ mir Aermsten Recht zu sprechen/", "tokens": ["Komm", "/", "Lie\u00b7be", "/", "komm", "/", "mir", "A\u00b7erms\u00b7ten", "Recht", "zu", "spre\u00b7chen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NN", "$(", "VVFIN", "$(", "PPER", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Komm meine Glutt/ wo nicht sein Ei\u00df/ zu brechen/", "tokens": ["Komm", "mei\u00b7ne", "Glutt", "/", "wo", "nicht", "sein", "Ei\u00df", "/", "zu", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "PWAV", "PTKNEG", "PPOSAT", "NN", "$(", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich habe gnug bey seinem Schnee geschwizt/", "tokens": ["Ich", "ha\u00b7be", "gnug", "bey", "sei\u00b7nem", "Schnee", "ge\u00b7schwizt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Ei\u00df ist gnug bey meinem Brand erhizt.", "tokens": ["Sein", "Ei\u00df", "ist", "gnug", "bey", "mei\u00b7nem", "Brand", "er\u00b7hizt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}