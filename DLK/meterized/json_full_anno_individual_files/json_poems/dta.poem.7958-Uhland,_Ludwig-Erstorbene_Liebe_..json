{"dta.poem.7958": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Erstorbene Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wir waren neugeboren, himmlisch helle", "tokens": ["Wir", "wa\u00b7ren", "neu\u00b7ge\u00b7bo\u00b7ren", ",", "himm\u00b7lisch", "hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "$,", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War uns der Liebe Morgen aufgegangen.", "tokens": ["War", "uns", "der", "Lie\u00b7be", "Mor\u00b7gen", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie gl\u00fchten, Laura, Lippen dir und Wangen!", "tokens": ["Wie", "gl\u00fch\u00b7ten", ",", "Lau\u00b7ra", ",", "Lip\u00b7pen", "dir", "und", "Wan\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "NE", "$,", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Auge brannt\u2019, es schlug des Busens Welle.", "tokens": ["Dein", "Au\u00b7ge", "brannt'", ",", "es", "schlug", "des", "Bu\u00b7sens", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie wallt\u2019 in mir des neuen Lebens Quelle!", "tokens": ["Wie", "wallt'", "in", "mir", "des", "neu\u00b7en", "Le\u00b7bens", "Quel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie hohe Kr\u00e4fte rastlos mich durchdrangen!", "tokens": ["Wie", "ho\u00b7he", "Kr\u00e4f\u00b7te", "rast\u00b7los", "mich", "durch\u00b7dran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie lie\u00dfen nicht des Schlafes mich verlangen,", "tokens": ["Sie", "lie\u00b7\u00dfen", "nicht", "des", "Schla\u00b7fes", "mich", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lebendig kurzer Traum vertrat die Stelle.", "tokens": ["Le\u00b7ben\u00b7dig", "kur\u00b7zer", "Traum", "ver\u00b7trat", "die", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Ja! Lieb\u2019 ist h\u00f6her Leben im gemeinen;", "tokens": ["Ja", "!", "Lieb'", "ist", "h\u00f6\u00b7her", "Le\u00b7ben", "im", "ge\u00b7mei\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NN", "VAFIN", "ADJD", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das waren ihre regen Lebenszeichen:", "tokens": ["Das", "wa\u00b7ren", "ih\u00b7re", "re\u00b7gen", "Le\u00b7bens\u00b7zei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun such\u2019 ich sie an dir, in mir vergebens.", "tokens": ["Nun", "such'", "ich", "sie", "an", "dir", ",", "in", "mir", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPER", "$,", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Drum mu\u00df ich, Laura! dich und mich beweinen:", "tokens": ["Drum", "mu\u00df", "ich", ",", "Lau\u00b7ra", "!", "dich", "und", "mich", "be\u00b7wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$,", "NE", "$.", "PPER", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir beide sind erloschner Liebe Leichen,", "tokens": ["Wir", "bei\u00b7de", "sind", "er\u00b7lo\u00b7schner", "Lie\u00b7be", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Uns traf der Tod des liebelosen Lebens.", "tokens": ["Uns", "traf", "der", "Tod", "des", "lie\u00b7be\u00b7lo\u00b7sen", "Le\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}