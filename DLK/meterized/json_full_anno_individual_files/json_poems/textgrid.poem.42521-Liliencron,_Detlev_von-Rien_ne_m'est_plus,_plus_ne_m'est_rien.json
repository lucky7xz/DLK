{"textgrid.poem.42521": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Rien ne m'est plus, plus ne m'est rien", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da ich verloren habe, was mein war,", "tokens": ["Da", "ich", "ver\u00b7lo\u00b7ren", "ha\u00b7be", ",", "was", "mein", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "PRELS", "PPOSAT", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verschm\u00e4h ich alles nun, was mir geboten.", "tokens": ["Ver\u00b7schm\u00e4h", "ich", "al\u00b7les", "nun", ",", "was", "mir", "ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wandre mit dem Bettelstab, ein Narr,", "tokens": ["Ich", "wand\u00b7re", "mit", "dem", "Bet\u00b7tel\u00b7stab", ",", "ein", "Narr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schlafe auf dem d\u00fcrren Feld der Toten,", "tokens": ["Und", "schla\u00b7fe", "auf", "dem", "d\u00fcr\u00b7ren", "Feld", "der", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und bin ein Einsiedler und trostesbar,", "tokens": ["Und", "bin", "ein", "Ein\u00b7sied\u00b7ler", "und", "tros\u00b7tes\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und bin geringer noch als die Heloten.", "tokens": ["Und", "bin", "ge\u00b7rin\u00b7ger", "noch", "als", "die", "He\u00b7lo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADV", "KOUS", "ART", "NN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ich bin ein Elender, so ganz und gar,", "tokens": ["Ich", "bin", "ein", "E\u00b7len\u00b7der", ",", "so", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df mir die Hoffnung, Freud und Leid verlohten.", "tokens": ["Da\u00df", "mir", "die", "Hoff\u00b7nung", ",", "Freud", "und", "Leid", "ver\u00b7loh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da ich verloren habe, was mein war,", "tokens": ["Da", "ich", "ver\u00b7lo\u00b7ren", "ha\u00b7be", ",", "was", "mein", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "PRELS", "PPOSAT", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verschm\u00e4h ich alles nun, was mir geboten.", "tokens": ["Ver\u00b7schm\u00e4h", "ich", "al\u00b7les", "nun", ",", "was", "mir", "ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "pun