{"dta.poem.6258": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Gantze, so sich dr\u00fcckt und presst,", "tokens": ["Das", "Gant\u00b7ze", ",", "so", "sich", "dr\u00fcckt", "und", "presst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das widersteht zugleich der Trennung, da es fest.", "tokens": ["Das", "wi\u00b7der\u00b7steht", "zu\u00b7gleich", "der", "Tren\u00b7nung", ",", "da", "es", "fest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies sind die C\u00f6rper nun,", "tokens": ["Dies", "sind", "die", "C\u00f6r\u00b7per", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die hart, und die sich selbst begrentzend, immer ruhu.", "tokens": ["Die", "hart", ",", "und", "die", "sich", "selbst", "be\u00b7grent\u00b7zend", ",", "im\u00b7mer", "ru\u00b7hu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "KON", "PRELS", "PRF", "ADV", "VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}