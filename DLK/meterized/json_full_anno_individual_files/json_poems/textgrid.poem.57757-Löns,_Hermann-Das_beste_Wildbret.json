{"textgrid.poem.57757": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das beste Wildbret", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun aber will ich ziehen", "tokens": ["Nun", "a\u00b7ber", "will", "ich", "zie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinaus zum gr\u00fcnen Wald,", "tokens": ["Hin\u00b7aus", "zum", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Wildbret zu erjagen", "tokens": ["Ein", "Wild\u00b7bret", "zu", "er\u00b7ja\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von edeler Gestalt.", "tokens": ["Von", "e\u00b7de\u00b7ler", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es hat nicht lange H\u00f6rner", "tokens": ["Es", "hat", "nicht", "lan\u00b7ge", "H\u00f6r\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und auch kein stolz Geweih,", "tokens": ["Und", "auch", "kein", "stolz", "Ge\u00b7weih", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fri\u00dft nicht Gras noch Bl\u00e4tter", "tokens": ["Es", "fri\u00dft", "nicht", "Gras", "noch", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tritt kein Holz entzwei.", "tokens": ["Und", "tritt", "kein", "Holz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist nicht Hirsch, noch Hase,", "tokens": ["Es", "ist", "nicht", "Hirsch", ",", "noch", "Ha\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und auch kein wildes Schwein,", "tokens": ["Und", "auch", "kein", "wil\u00b7des", "Schwein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ist mir doch viel lieber", "tokens": ["Und", "ist", "mir", "doch", "viel", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als eins von diesen drein.", "tokens": ["Als", "eins", "von", "die\u00b7sen", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PDAT", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich fang's mit keinem Netze,", "tokens": ["Ich", "fang's", "mit", "kei\u00b7nem", "Net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich fang's mit keinem Hund,", "tokens": ["Ich", "fang's", "mit", "kei\u00b7nem", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schie\u00df es nicht mit Hagel,", "tokens": ["Ich", "schie\u00df", "es", "nicht", "mit", "Ha\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch mit der Kugel rund.", "tokens": ["Noch", "mit", "der", "Ku\u00b7gel", "rund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denn was ich geh zu jagen,", "tokens": ["Denn", "was", "ich", "geh", "zu", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das ist ein schlankes Reh,", "tokens": ["Das", "ist", "ein", "schlan\u00b7kes", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn ich es erlege,", "tokens": ["Und", "wenn", "ich", "es", "er\u00b7le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das tut ihm gar nicht weh.", "tokens": ["Das", "tut", "ihm", "gar", "nicht", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"t