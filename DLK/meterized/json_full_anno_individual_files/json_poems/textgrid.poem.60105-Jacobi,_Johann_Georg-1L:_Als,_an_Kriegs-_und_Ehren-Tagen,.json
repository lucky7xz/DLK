{"textgrid.poem.60105": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als, an Kriegs- und Ehren-Tagen,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als, an Kriegs- und Ehren-Tagen,", "tokens": ["Als", ",", "an", "Kriegs", "und", "Eh\u00b7ren\u00b7Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch ein deutscher Rund-Gesang", "tokens": ["Noch", "ein", "deut\u00b7scher", "Run\u00b7dGe\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Laut, bey fr\u00f6hlichen Gelagen,", "tokens": ["Laut", ",", "bey", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7la\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey der V\u00e4ter Halle klang,", "tokens": ["Bey", "der", "V\u00e4\u00b7ter", "Hal\u00b7le", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lie\u00dfen sie das Lob verstorbner Helden", "tokens": ["Lie\u00b7\u00dfen", "sie", "das", "Lob", "ver\u00b7storb\u00b7ner", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ihren ersten Becher melden.", "tokens": ["Ih\u00b7ren", "ers\u00b7ten", "Be\u00b7cher", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Freund! nach alter Weise schenken", "tokens": ["Freund", "!", "nach", "al\u00b7ter", "Wei\u00b7se", "schen\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Becher wir voll Wein;", "tokens": ["Die\u00b7sen", "Be\u00b7cher", "wir", "voll", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er soll dem Angedenken", "tokens": ["Und", "er", "soll", "dem", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines ", "tokens": ["Dei\u00b7nes"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der, wie ", "tokens": ["Der", ",", "wie"], "token_info": ["word", "punct", "word"], "pos": ["ART", "$,", "PWAV"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Dein bis in sein Grab geblieben.", "tokens": ["Dein", "bis", "in", "sein", "Grab", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch die St\u00e4tte des Erbla\u00dften,", "tokens": ["Doch", "die", "St\u00e4t\u00b7te", "des", "Er\u00b7bla\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wo mit ihm, vom s\u00fc\u00dfen Licht", "tokens": ["Wo", "mit", "ihm", ",", "vom", "s\u00fc\u00b7\u00dfen", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPER", "$,", "APPRART", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ach so fern! die Todten rasten,", "tokens": ["Ach", "so", "fern", "!", "die", "Tod\u00b7ten", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADJD", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nenne mein Gesang dir nicht!", "tokens": ["Nen\u00b7ne", "mein", "Ge\u00b7sang", "dir", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df uns nur den vollen Becher weihen,", "tokens": ["La\u00df", "uns", "nur", "den", "vol\u00b7len", "Be\u00b7cher", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Seines Lebens uns zu freuen:", "tokens": ["Sei\u00b7nes", "Le\u00b7bens", "uns", "zu", "freu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df, vor Tausenden zu gl\u00e4nzen,", "tokens": ["Da\u00df", ",", "vor", "Tau\u00b7sen\u00b7den", "zu", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er den hohen Geist empfing;", "tokens": ["Er", "den", "ho\u00b7hen", "Geist", "emp\u00b7fing", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber zwischen Lorber-Kr\u00e4nzen", "tokens": ["A\u00b7ber", "zwi\u00b7schen", "Lor\u00b7ber\u00b7Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Demuthsvoll, in Zweifeln ging,", "tokens": ["De\u00b7muths\u00b7voll", ",", "in", "Zwei\u00b7feln", "ging", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob er nicht des gro\u00dfen Ziels verfehlte,", "tokens": ["Ob", "er", "nicht", "des", "gro\u00b7\u00dfen", "Ziels", "ver\u00b7fehl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nicht f\u00fcr Wahrheit Irrthum w\u00e4hlte;", "tokens": ["Nicht", "f\u00fcr", "Wahr\u00b7heit", "Irr\u00b7thum", "w\u00e4hl\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da\u00df er bey gepr\u00fcften Sch\u00f6tzen", "tokens": ["Da\u00df", "er", "bey", "ge\u00b7pr\u00fcf\u00b7ten", "Sch\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alter Kunst voll Einfalt sa\u00df,", "tokens": ["Al\u00b7ter", "Kunst", "voll", "Ein\u00b7falt", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach der Sch\u00f6nheit Urgesetzen", "tokens": ["Nach", "der", "Sch\u00f6n\u00b7heit", "Ur\u00b7ge\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes seiner Werke ma\u00df,", "tokens": ["Je\u00b7des", "sei\u00b7ner", "Wer\u00b7ke", "ma\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freyen Muth in Frevel nie verkehrte,", "tokens": ["Frey\u00b7en", "Muth", "in", "Fre\u00b7vel", "nie", "ver\u00b7kehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nie der Sprache Recht entehrte;", "tokens": ["Nie", "der", "Spra\u00b7che", "Recht", "ent\u00b7ehr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Da\u00df er gl\u00e4ubig die Gebote", "tokens": ["Da\u00df", "er", "gl\u00e4u\u00b7big", "die", "Ge\u00b7bo\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reiner Liebe nicht verlie\u00df,", "tokens": ["Rei\u00b7ner", "Lie\u00b7be", "nicht", "ver\u00b7lie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem Priester, der ihm drohte,", "tokens": ["Und", "dem", "Pries\u00b7ter", ",", "der", "ihm", "droh\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Lebens Unschuld wies;", "tokens": ["Sei\u00b7nes", "Le\u00b7bens", "Un\u00b7schuld", "wies", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df den Mann, den sie zur H\u00f6lle bannten,", "tokens": ["Da\u00df", "den", "Mann", ",", "den", "sie", "zur", "H\u00f6l\u00b7le", "bann\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Arme Wittwen selig nannten;", "tokens": ["Ar\u00b7me", "Witt\u00b7wen", "se\u00b7lig", "nann\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Da\u00df sein letzter Tag gekommen", "tokens": ["Da\u00df", "sein", "letz\u00b7ter", "Tag", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Schrecken, leis' und mild,", "tokens": ["Oh\u00b7ne", "Schre\u00b7cken", ",", "leis'", "und", "mild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das Wandlen eines frommen", "tokens": ["Wie", "das", "Wand\u00b7len", "ei\u00b7nes", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "J\u00fcnglings, wie das holde Bild,", "tokens": ["J\u00fcng\u00b7lings", ",", "wie", "das", "hol\u00b7de", "Bild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das er uns im ", "tokens": ["Das", "er", "uns", "im"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPER", "PRF", "APPRART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Welcher ", "tokens": ["Wel\u00b7cher"], "token_info": ["word"], "pos": ["PWAT"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.8": {"line.1": {"text": "Nimm, o Gleim, den Freuden-Becher,", "tokens": ["Nimm", ",", "o", "Gleim", ",", "den", "Freu\u00b7den\u00b7Be\u00b7cher", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcll' ihn lange noch mit Wein,", "tokens": ["F\u00fcll'", "ihn", "lan\u00b7ge", "noch", "mit", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um des Freundes Freund und R\u00e4cher", "tokens": ["Um", "des", "Freun\u00b7des", "Freund", "und", "R\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst, wenn Alles schweigt, zu seyn:", "tokens": ["Einst", ",", "wenn", "Al\u00b7les", "schweigt", ",", "zu", "seyn", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "VVFIN", "$,", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es rauscht des falschen Eifers Fl\u00fcgel", "tokens": ["Denn", "es", "rauscht", "des", "fal\u00b7schen", "Ei\u00b7fers", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Auch um stille Todes-H\u00fcgel.", "tokens": ["Auch", "um", "stil\u00b7le", "To\u00b7des\u00b7H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Als, an Kriegs- und Ehren-Tagen,", "tokens": ["Als", ",", "an", "Kriegs", "und", "Eh\u00b7ren\u00b7Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch ein deutscher Rund-Gesang", "tokens": ["Noch", "ein", "deut\u00b7scher", "Run\u00b7dGe\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Laut, bey fr\u00f6hlichen Gelagen,", "tokens": ["Laut", ",", "bey", "fr\u00f6h\u00b7li\u00b7chen", "Ge\u00b7la\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bey der V\u00e4ter Halle klang,", "tokens": ["Bey", "der", "V\u00e4\u00b7ter", "Hal\u00b7le", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lie\u00dfen sie das Lob verstorbner Helden", "tokens": ["Lie\u00b7\u00dfen", "sie", "das", "Lob", "ver\u00b7storb\u00b7ner", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ihren ersten Becher melden.", "tokens": ["Ih\u00b7ren", "ers\u00b7ten", "Be\u00b7cher", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Freund! nach alter Weise schenken", "tokens": ["Freund", "!", "nach", "al\u00b7ter", "Wei\u00b7se", "schen\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Becher wir voll Wein;", "tokens": ["Die\u00b7sen", "Be\u00b7cher", "wir", "voll", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er soll dem Angedenken", "tokens": ["Und", "er", "soll", "dem", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines ", "tokens": ["Dei\u00b7nes"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Der, wie ", "tokens": ["Der", ",", "wie"], "token_info": ["word", "punct", "word"], "pos": ["ART", "$,", "PWAV"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Dein bis in sein Grab geblieben.", "tokens": ["Dein", "bis", "in", "sein", "Grab", "ge\u00b7blie\u00b7ben", "."], "token