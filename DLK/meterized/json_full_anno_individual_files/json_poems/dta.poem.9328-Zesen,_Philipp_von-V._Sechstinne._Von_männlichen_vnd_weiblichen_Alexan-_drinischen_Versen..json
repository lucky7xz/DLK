{"dta.poem.9328": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n Sechstinne.  \n Von m\u00e4nnlichen vnd weiblichen Alexan-  \n drinischen Versen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wird mich sterblichen aufnehmen doch der", "tokens": ["Wenn", "wird", "mich", "sterb\u00b7li\u00b7chen", "auf\u00b7neh\u00b7men", "doch", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "VVINF", "VVINF", "KON", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Himmel?", "tokens": ["Him\u00b7mel", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wenn werd ich doch einmahl anschauen meinen Gott?", "tokens": ["Wenn", "werd", "ich", "doch", "ein\u00b7mahl", "an\u00b7schau\u00b7en", "mei\u00b7nen", "Gott", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wenn werd ich h\u00f6ren wohl die Engelischen Lieder?", "tokens": ["Wenn", "werd", "ich", "h\u00f6\u00b7ren", "wohl", "die", "En\u00b7ge\u00b7li\u00b7schen", "Lie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich irre traurens-voll in dieser schwartzen Nacht.", "tokens": ["Ich", "ir\u00b7re", "trau\u00b7rens\u00b7voll", "in", "die\u00b7ser", "schwart\u00b7zen", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn kom\u0303ich doch einmahl zur himmelischen freude?", "tokens": ["Wenn", "kom\u0303ich", "doch", "ein\u00b7mahl", "zur", "him\u00b7me\u00b7li\u00b7schen", "freu\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Die Erd ist mier zu klein/ zu klein ist mier die Welt.", "tokens": ["Die", "Erd", "ist", "mier", "zu", "klein", "/", "zu", "klein", "ist", "mier", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$(", "PTKA", "ADJD", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein Gott ich halte dich viel h\u00f6her als die Welt/", "tokens": ["Mein", "Gott", "ich", "hal\u00b7te", "dich", "viel", "h\u00f6\u00b7her", "als", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "mein Gott ich halte dich viel h\u00f6her als den Himmel/", "tokens": ["mein", "Gott", "ich", "hal\u00b7te", "dich", "viel", "h\u00f6\u00b7her", "als", "den", "Him\u00b7mel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "dr\u00fcmb hab ich gro\u00dfe lust bey dier zu seyn mein Gott;", "tokens": ["dr\u00fcmb", "hab", "ich", "gro\u00b7\u00dfe", "lust", "bey", "dier", "zu", "seyn", "mein", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJA", "NN", "APPR", "PPER", "PTKZU", "VAINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein Gott/ ich h\u00f6re dich vor seitenspiel und lieder/", "tokens": ["Mein", "Gott", "/", "ich", "h\u00f6\u00b7re", "dich", "vor", "sei\u00b7ten\u00b7spiel", "und", "lie\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "PRF", "APPR", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mein Gott/ ich liebe dich und hoffe tag und nacht", "tokens": ["Mein", "Gott", "/", "ich", "lie\u00b7be", "dich", "und", "hof\u00b7fe", "tag", "und", "nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "mein Gott/ bey dier zu seyn/ zu schauen dich mit frenden.", "tokens": ["mein", "Gott", "/", "bey", "dier", "zu", "seyn", "/", "zu", "schau\u00b7en", "dich", "mit", "fren\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "APPR", "PPER", "PTKZU", "VAINF", "$(", "PTKZU", "VVINF", "PRF", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es suchet seine Braut der Breutigam mit freuden/", "tokens": ["Es", "su\u00b7chet", "sei\u00b7ne", "Braut", "der", "Breu\u00b7ti\u00b7gam", "mit", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NE", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "es liebet geld und gold die arme schwache Welt/", "tokens": ["es", "lie\u00b7bet", "geld", "und", "gold", "die", "ar\u00b7me", "schwa\u00b7che", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "die Erde liebet auch und wieder Sie der Himmel/", "tokens": ["die", "Er\u00b7de", "lie\u00b7bet", "auch", "und", "wie\u00b7der", "Sie", "der", "Him\u00b7mel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "ADV", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "vor liebe gegen uns gab selbst vor uns sich Gott/", "tokens": ["vor", "lie\u00b7be", "ge\u00b7gen", "uns", "gab", "selbst", "vor", "uns", "sich", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "PPER", "VVFIN", "ADV", "APPR", "PPER", "PRF", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "und Du/ ", "tokens": ["und", "Du", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$("], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "un\u0303 nim\u0303st mich nicht zu dier aus dieser schwartzen nacht.", "tokens": ["u\u00f1", "nim\u0303st", "mich", "nicht", "zu", "dier", "aus", "die\u00b7ser", "schwart\u00b7zen", "nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PDAT", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wo du nicht bist/ mein Gott/ da ist es lauter nacht/", "tokens": ["Wo", "du", "nicht", "bist", "/", "mein", "Gott", "/", "da", "ist", "es", "lau\u00b7ter", "nacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VAFIN", "$(", "PPOSAT", "NN", "$(", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "wo du nicht bist/ mein Gott/ da seyn wir ohne freuden/", "tokens": ["wo", "du", "nicht", "bist", "/", "mein", "Gott", "/", "da", "seyn", "wir", "oh\u00b7ne", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VAFIN", "$(", "PPOSAT", "NN", "$(", "ADV", "VAFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "wo du nicht bist mein Gott/ ver geh ich in der Welt/", "tokens": ["wo", "du", "nicht", "bist", "mein", "Gott", "/", "ver", "geh", "ich", "in", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VAFIN", "PPOSAT", "NN", "$(", "NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "wo du nicht bist mein Gott/ was hilffet mir der Himmel!", "tokens": ["wo", "du", "nicht", "bist", "mein", "Gott", "/", "was", "hilf\u00b7fet", "mir", "der", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "VAFIN", "PPOSAT", "NN", "$(", "PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Es ist \u00fcmb mich geschehn/ wo du nicht bist mein Gott/", "tokens": ["Es", "ist", "\u00fcmb", "mich", "ge\u00b7schehn", "/", "wo", "du", "nicht", "bist", "mein", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$(", "PWAV", "PPER", "PTKNEG", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "mein bethen wer \u00fcmbsonst/ \u00fcmbsonst die steren Lieder.", "tokens": ["mein", "be\u00b7then", "wer", "\u00fcm\u00b7bsonst", "/", "\u00fcm\u00b7bsonst", "die", "ste\u00b7ren", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PWS", "ADV", "$(", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Doch fahr ich immer fort zu singen meine Lieder/", "tokens": ["Doch", "fahr", "ich", "im\u00b7mer", "fort", "zu", "sin\u00b7gen", "mei\u00b7ne", "Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "PTKZU", "VVINF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "und gleube festiglich/ da\u00df ich nach dieser nacht/", "tokens": ["und", "gleu\u00b7be", "fes\u00b7tig\u00b7lich", "/", "da\u00df", "ich", "nach", "die\u00b7ser", "nacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$(", "KOUS", "PPER", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "o gro\u00dfer Gott/ dein Licht soll schauen an mit freuden/", "tokens": ["o", "gro\u00b7\u00dfer", "Gott", "/", "dein", "Licht", "soll", "schau\u00b7en", "an", "mit", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$(", "PPOSAT", "NN", "VMFIN", "VVINF", "APPR", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich hoffe festiglich zu segnen bald die Welt/", "tokens": ["Ich", "hof\u00b7fe", "fes\u00b7tig\u00b7lich", "zu", "seg\u00b7nen", "bald", "die", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich hoffe festiglich zu nehmen ein den Himmel", "tokens": ["Ich", "hof\u00b7fe", "fes\u00b7tig\u00b7lich", "zu", "neh\u00b7men", "ein", "den", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "und traue festiglich zu schauen meinen Gott.", "tokens": ["und", "trau\u00b7e", "fes\u00b7tig\u00b7lich", "zu", "schau\u00b7en", "mei\u00b7nen", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da lob und lieb ich dich von hertzen/ h\u00f6chster Gott/", "tokens": ["Da", "lob", "und", "lieb", "ich", "dich", "von", "hert\u00b7zen", "/", "h\u00f6chs\u00b7ter", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJD", "PPER", "PRF", "APPR", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "da sollen erst angehn die rechten freuden-Lieder/", "tokens": ["da", "sol\u00b7len", "erst", "an\u00b7gehn", "die", "rech\u00b7ten", "freu\u00b7den\u00b7Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "VVPP", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "weil uns kein ungeth\u00fcmb kein schrecken/ keine Nacht/", "tokens": ["weil", "uns", "kein", "un\u00b7ge\u00b7th\u00fcmb", "kein", "schre\u00b7cken", "/", "kei\u00b7ne", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJD", "PIAT", "VVINF", "$(", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "kein Krieg abhalten wird allda von solchen freuden/", "tokens": ["kein", "Krieg", "ab\u00b7hal\u00b7ten", "wird", "all\u00b7da", "von", "sol\u00b7chen", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VAFIN", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "die Gott uns hat hereit: Ey nun du schn\u00f6de Welt/", "tokens": ["die", "Gott", "uns", "hat", "he\u00b7reit", ":", "Ey", "nun", "du", "schn\u00f6\u00b7de", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VAFIN", "NN", "$.", "NN", "ADV", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich la\u00dfe deine Lust und suche lust in Himmel!", "tokens": ["Ich", "la\u00b7\u00dfe", "dei\u00b7ne", "Lust", "und", "su\u00b7che", "lust", "in", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Erh\u00f6re liebster Gott/ mein bethen/ meine Lieder/", "tokens": ["Er\u00b7h\u00f6\u00b7re", "liebs\u00b7ter", "Gott", "/", "mein", "be\u00b7then", "/", "mei\u00b7ne", "Lie\u00b7der", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "und ze\u00fcch mich aus der Welt dir nach hinnauf gen", "tokens": ["und", "ze\u00fcch", "mich", "aus", "der", "Welt", "dir", "nach", "hin\u00b7nauf", "gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "PPER", "APPR", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Himmel/", "tokens": ["Him\u00b7mel", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "weil hier nur lauter nacht dort aber licht der freuden!", "tokens": ["weil", "hier", "nur", "lau\u00b7ter", "nacht", "dort", "a\u00b7ber", "licht", "der", "freu\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIAT", "NN", "ADV", "ADV", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}