{"textgrid.poem.62436": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es t\u00f6nt in allen Landen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es t\u00f6nt in allen Landen", "tokens": ["Es", "t\u00f6nt", "in", "al\u00b7len", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Ruf zum heil'gen Streit;", "tokens": ["Ein", "Ruf", "zum", "heil'\u00b7gen", "Streit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In ihrer Kraft erstanden", "tokens": ["In", "ih\u00b7rer", "Kraft", "er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist neu die Christenheit.", "tokens": ["Ist", "neu", "die", "Chris\u00b7ten\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die St\u00fcrme Gottes fahren", "tokens": ["Die", "St\u00fcr\u00b7me", "Got\u00b7tes", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und wecken jeden Mann,", "tokens": ["Und", "we\u00b7cken", "je\u00b7den", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie da vor grauen Jahren", "tokens": ["Wie", "da", "vor", "grau\u00b7en", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Kreuzeszug begann.", "tokens": ["Der", "Kreu\u00b7zes\u00b7zug", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Uralte Kr\u00e4fte regen", "tokens": ["Ur\u00b7al\u00b7te", "Kr\u00e4f\u00b7te", "re\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich sch\u00f6n und f\u00fcrchterlich,", "tokens": ["Sich", "sch\u00f6n", "und", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In ihrer Gruft bewegen", "tokens": ["In", "ih\u00b7rer", "Gruft", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Freiheithelden sich.", "tokens": ["Die", "Frei\u00b7hei\u00b7thel\u00b7den", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es tritt aus seiner H\u00f6hle", "tokens": ["Es", "tritt", "aus", "sei\u00b7ner", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Felsengreis, der Tell,", "tokens": ["Der", "Fel\u00b7sen\u00b7greis", ",", "der", "Tell", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und jauchzt aus voller Seele,", "tokens": ["Und", "jauchzt", "aus", "vol\u00b7ler", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und pr\u00fcft den Bolzen schnell.", "tokens": ["Und", "pr\u00fcft", "den", "Bol\u00b7zen", "schnell", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und du nur k\u00f6nntest wanken,", "tokens": ["Und", "du", "nur", "k\u00f6nn\u00b7test", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sonst hochgepries'ne Schweiz?", "tokens": ["Sonst", "hoch\u00b7ge\u00b7pries'\u00b7ne", "Schweiz", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ge\u00f6ffnet stehn die Schranken \u2013", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "stehn", "die", "Schran\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat Freiheittod nicht Reiz?", "tokens": ["Hat", "Frei\u00b7heit\u00b7tod", "nicht", "Reiz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du bliebest unentz\u00fcndet", "tokens": ["Du", "blie\u00b7best", "un\u00b7ent\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Gottes Wort und Strahl,", "tokens": ["Von", "Got\u00b7tes", "Wort", "und", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "W\u00e4rst nicht mit uns verb\u00fcndet,", "tokens": ["W\u00e4rst", "nicht", "mit", "uns", "ver\u00b7b\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und hie\u00dfest jetzt neutral?", "tokens": ["Und", "hie\u00b7\u00dfest", "jetzt", "neut\u00b7ral", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Schmach der feigen Seele,", "tokens": ["O", "Schmach", "der", "fei\u00b7gen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die solches Wort erdacht!", "tokens": ["Die", "sol\u00b7ches", "Wort", "er\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein freies Volk erw\u00e4hle", "tokens": ["Kein", "frei\u00b7es", "Volk", "er\u00b7w\u00e4h\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So schlechte Grenzenwacht!", "tokens": ["So", "schlech\u00b7te", "Gren\u00b7zen\u00b7wacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dazu gab Gott uns Eisen,", "tokens": ["Da\u00b7zu", "gab", "Gott", "uns", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Armen gab er Kraft,", "tokens": ["Den", "Ar\u00b7men", "gab", "er", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das m\u00e4nnlich zu beweisen,", "tokens": ["Das", "m\u00e4nn\u00b7lich", "zu", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wuchs mancher Lanzenschaft.", "tokens": ["Wuchs", "man\u00b7cher", "Lan\u00b7zen\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Wenn's euch nach Schlaf gel\u00fcstet,", "tokens": ["Wenn's", "euch", "nach", "Schlaf", "ge\u00b7l\u00fcs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir haben Tag gemeint!", "tokens": ["Wir", "ha\u00b7ben", "Tag", "ge\u00b7meint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir kommen an ger\u00fcstet:", "tokens": ["Wir", "kom\u00b7men", "an", "ge\u00b7r\u00fcs\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Freund, hei\u00dft es, oder Feind!", "tokens": ["Freund", ",", "hei\u00dft", "es", ",", "o\u00b7der", "Feind", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Euch rufen V\u00e4terheere:", "tokens": ["Euch", "ru\u00b7fen", "V\u00e4\u00b7ter\u00b7hee\u00b7re", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbihr Schweizer! werdet wach!", "tokens": ["\u00bb", "ihr", "Schwei\u00b7zer", "!", "wer\u00b7det", "wach", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der keuschen M\u00fctter Ehre", "tokens": ["Der", "keu\u00b7schen", "M\u00fct\u00b7ter", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Errettet von der Schmach!\u00ab", "tokens": ["Er\u00b7ret\u00b7tet", "von", "der", "Schmach", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbwir konnten nimmer zeugen", "tokens": ["\u00bb", "wir", "konn\u00b7ten", "nim\u00b7mer", "zeu\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein schw\u00e4chliches Geschlecht;", "tokens": ["Ein", "schw\u00e4ch\u00b7li\u00b7ches", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn die V\u00f6lker schweigen,", "tokens": ["Und", "wenn", "die", "V\u00f6l\u00b7ker", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Felsen schrei'n um Recht!", "tokens": ["Die", "Fel\u00b7sen", "schrei'n", "um", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O z\u00fcndet schnell die Feuer", "tokens": ["O", "z\u00fcn\u00b7det", "schnell", "die", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf hohen Alpen an;", "tokens": ["Auf", "ho\u00b7hen", "Al\u00b7pen", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vielleicht erwarmt ein neuer", "tokens": ["Viel\u00b7leicht", "er\u00b7warmt", "ein", "neu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Held Gottes sich daran.\u00ab", "tokens": ["Held", "Got\u00b7tes", "sich", "da\u00b7ran", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "PRF", "PAV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es gr\u00fcndeten die Dreie", "tokens": ["Es", "gr\u00fcn\u00b7de\u00b7ten", "die", "Drei\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im stillen Felsenthal", "tokens": ["Im", "stil\u00b7len", "Fel\u00b7sen\u00b7thal"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Freiheit und der Treue", "tokens": ["Der", "Frei\u00b7heit", "und", "der", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Reich nach Gottes Wahl.", "tokens": ["Ein", "Reich", "nach", "Got\u00b7tes", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht f\u00fcr ein kleines Streiten", "tokens": ["Nicht", "f\u00fcr", "ein", "klei\u00b7nes", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Entbrannte das Gem\u00fcth;", "tokens": ["Ent\u00b7brann\u00b7te", "das", "Ge\u00b7m\u00fcth", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Held f\u00fcr ", "tokens": ["Ein", "Held", "f\u00fcr"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "APPR"], "meter": "+--", "measure": "dactylic.init"}, "line.8": {"text": "Fiel Arnold Winkelried.", "tokens": ["Fiel", "Ar\u00b7nold", "Win\u00b7kel\u00b7ried", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Noch h\u00e4ngen Felsenmassen", "tokens": ["Noch", "h\u00e4n\u00b7gen", "Fel\u00b7sen\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An die Lawine sich;", "tokens": ["An", "die", "La\u00b7wi\u00b7ne", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Frommen ziehn und fassen", "tokens": ["Die", "From\u00b7men", "ziehn", "und", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einander kr\u00e4ftiglich.", "tokens": ["Ein\u00b7an\u00b7der", "kr\u00e4f\u00b7tig\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Oft mu\u00df aus kleinem Samen", "tokens": ["Oft", "mu\u00df", "aus", "klei\u00b7nem", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die gr\u00f6\u00dfte That gedeihn:", "tokens": ["Die", "gr\u00f6\u00df\u00b7te", "That", "ge\u00b7deihn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Darum, in Gottes Namen,", "tokens": ["Da\u00b7rum", ",", "in", "Got\u00b7tes", "Na\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ihr Schweizer! schlaget drein!", "tokens": ["Ihr", "Schwei\u00b7zer", "!", "schla\u00b7get", "drein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Es t\u00f6nt in allen Landen", "tokens": ["Es", "t\u00f6nt", "in", "al\u00b7len", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Ruf zum heil'gen Streit;", "tokens": ["Ein", "Ruf", "zum", "heil'\u00b7gen", "Streit", ";"], "token_info": ["word", "word", "word