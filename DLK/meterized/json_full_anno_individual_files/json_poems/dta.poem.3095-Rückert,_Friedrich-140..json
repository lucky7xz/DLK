{"dta.poem.3095": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "140.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein h\u00f6chstes Leben sei zu leben gottbewu\u00dft;", "tokens": ["Dein", "h\u00f6chs\u00b7tes", "Le\u00b7ben", "sei", "zu", "le\u00b7ben", "gott\u00b7be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darin ist zweierlei: gottwissend, gottgewu\u00dft:", "tokens": ["Da\u00b7rin", "ist", "zwei\u00b7er\u00b7lei", ":", "gott\u00b7wis\u00b7send", ",", "gott\u00b7ge\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "$.", "VVPP", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df du dich wissest stets von Gott gewu\u00dft, gekannt,", "tokens": ["Da\u00df", "du", "dich", "wis\u00b7sest", "stets", "von", "Gott", "ge\u00b7wu\u00dft", ",", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV", "APPR", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gemahnt, gestraft, gepr\u00fcft, geliebt und Kind genannt.", "tokens": ["Ge\u00b7mahnt", ",", "ge\u00b7straft", ",", "ge\u00b7pr\u00fcft", ",", "ge\u00b7liebt", "und", "Kind", "ge\u00b7nannt", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$,", "VVPP