{"textgrid.poem.26791": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbritter, treue Schwesterliebe", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbritter, treue Schwesterliebe", "tokens": ["\u00bb", "rit\u00b7ter", ",", "treu\u00b7e", "Schwes\u00b7ter\u00b7lie\u00b7be"], "token_info": ["punct", "word", "punct", "word", "word"], "pos": ["$(", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Widmet Euch dies Herz,", "tokens": ["Wid\u00b7met", "Euch", "dies", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Fodert keine andre Liebe,", "tokens": ["Fo\u00b7dert", "kei\u00b7ne", "and\u00b7re", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn es macht mir Schmerz.", "tokens": ["Denn", "es", "macht", "mir", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ruhig mag ich Euch erscheinen,", "tokens": ["Ru\u00b7hig", "mag", "ich", "Euch", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ruhig gehen sehn.", "tokens": ["Ru\u00b7hig", "ge\u00b7hen", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Eurer Augen stilles Weinen", "tokens": ["Eu\u00b7rer", "Au\u00b7gen", "stil\u00b7les", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kann ich nicht verstehn.\u00ab", "tokens": ["Kann", "ich", "nicht", "ver\u00b7stehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und er h\u00f6rts mit stummem Harme,", "tokens": ["Und", "er", "h\u00f6rts", "mit", "stum\u00b7mem", "Har\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rei\u00dft sich blutend los,", "tokens": ["Rei\u00dft", "sich", "blu\u00b7tend", "los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Pre\u00dft sie heftig in die Arme,", "tokens": ["Pre\u00dft", "sie", "hef\u00b7tig", "in", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwingt sich auf sein Ro\u00df,", "tokens": ["Schwingt", "sich", "auf", "sein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Schickt zu seinen Mannen allen", "tokens": ["Schickt", "zu", "sei\u00b7nen", "Man\u00b7nen", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Lande Schweiz,", "tokens": ["In", "dem", "Lan\u00b7de", "Schweiz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Nach dem Heilgen Grab sie wallen,", "tokens": ["Nach", "dem", "Heil\u00b7gen", "Grab", "sie", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf der Brust das Kreuz.", "tokens": ["Auf", "der", "Brust", "das", "Kreuz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Gro\u00dfe Taten dort geschehen", "tokens": ["Gro\u00b7\u00dfe", "Ta\u00b7ten", "dort", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Helden Arm,", "tokens": ["Durch", "der", "Hel\u00b7den", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ihres Helmes B\u00fcsche wehen", "tokens": ["Ih\u00b7res", "Hel\u00b7mes", "B\u00fc\u00b7sche", "we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Feinde Schwarm,", "tokens": ["In", "der", "Fein\u00b7de", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Und des Toggenburgers Name", "tokens": ["Und", "des", "Tog\u00b7gen\u00b7bur\u00b7gers", "Na\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Schreckt den Muselmann,", "tokens": ["Schreckt", "den", "Mu\u00b7sel\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Doch das Herz von seinem Grame", "tokens": ["Doch", "das", "Herz", "von", "sei\u00b7nem", "Gra\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht genesen kann.", "tokens": ["Nicht", "ge\u00b7ne\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und ein Jahr hat ers getragen,", "tokens": ["Und", "ein", "Jahr", "hat", "ers", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4gts nicht l\u00e4nger mehr,", "tokens": ["Tr\u00e4gts", "nicht", "l\u00e4n\u00b7ger", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ruhe kann er nicht erjagen", "tokens": ["Ru\u00b7he", "kann", "er", "nicht", "er\u00b7ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verl\u00e4\u00dft das Heer,", "tokens": ["Und", "ver\u00b7l\u00e4\u00dft", "das", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.5": {"text": "Sieht ein Schiff an Joppes Strande,", "tokens": ["Sieht", "ein", "Schiff", "an", "Jop\u00b7pes", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das die Segel bl\u00e4ht,", "tokens": ["Das", "die", "Se\u00b7gel", "bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "Schiffet heim zum teuren Lande,", "tokens": ["Schif\u00b7fet", "heim", "zum", "teu\u00b7ren", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo ihr Atem weht.", "tokens": ["Wo", "ihr", "A\u00b7tem", "weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Und an ihres Schlosses Pforte", "tokens": ["Und", "an", "ih\u00b7res", "Schlos\u00b7ses", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klopft der Pilger an,", "tokens": ["Klopft", "der", "Pil\u00b7ger", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach! und mit dem Donnerworte", "tokens": ["Ach", "!", "und", "mit", "dem", "Don\u00b7ner\u00b7wor\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sie aufgetan:", "tokens": ["Wird", "sie", "auf\u00b7ge\u00b7tan", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "\u00bbdie Ihr suchet, tr\u00e4gt den Schleier,", "tokens": ["\u00bb", "die", "Ihr", "su\u00b7chet", ",", "tr\u00e4gt", "den", "Schlei\u00b7er", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist des Himmels Braut,", "tokens": ["Ist", "des", "Him\u00b7mels", "Braut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Gestern war des Tages Feier,", "tokens": ["Ge\u00b7stern", "war", "des", "Ta\u00b7ges", "Fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Der sie Gott getraut.\u00ab", "tokens": ["Der", "sie", "Gott", "ge\u00b7traut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "NN", "VVPP", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Da verl\u00e4sset er auf immer", "tokens": ["Da", "ver\u00b7l\u00e4s\u00b7set", "er", "auf", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Seiner V\u00e4ter Schlo\u00df,", "tokens": ["Sei\u00b7ner", "V\u00e4\u00b7ter", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seine Waffen sieht er nimmer,", "tokens": ["Sei\u00b7ne", "Waf\u00b7fen", "sieht", "er", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch sein treues Ro\u00df,", "tokens": ["Noch", "sein", "treu\u00b7es", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Von der Toggenburg hernieder", "tokens": ["Von", "der", "Tog\u00b7gen\u00b7burg", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steigt er unbekannt,", "tokens": ["Steigt", "er", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Denn es deckt die edeln Glieder", "tokens": ["Denn", "es", "deckt", "die", "e\u00b7deln", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4renes Gewand.", "tokens": ["H\u00e4\u00b7re\u00b7nes", "Ge\u00b7wand", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Und erbaut sich eine H\u00fctte", "tokens": ["Und", "er\u00b7baut", "sich", "ei\u00b7ne", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jener Gegend nah,", "tokens": ["Je\u00b7ner", "Ge\u00b7gend", "nah", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo das Kloster aus der Mitte", "tokens": ["Wo", "das", "Klos\u00b7ter", "aus", "der", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcstrer Linden sah;", "tokens": ["D\u00fcst\u00b7rer", "Lin\u00b7den", "sah", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Harrend von des Morgens Lichte", "tokens": ["Har\u00b7rend", "von", "des", "Mor\u00b7gens", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis zu Abends Schein,", "tokens": ["Bis", "zu", "A\u00b7bends", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Stille Hoffnung im Gesichte,", "tokens": ["Stil\u00b7le", "Hoff\u00b7nung", "im", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sa\u00df er da allein.", "tokens": ["Sa\u00df", "er", "da", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Blickte nach dem Kloster dr\u00fcben,", "tokens": ["Blick\u00b7te", "nach", "dem", "Klos\u00b7ter", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blickte stundenlang", "tokens": ["Blick\u00b7te", "stun\u00b7den\u00b7lang"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nach dem Fenster seiner Lieben,", "tokens": ["Nach", "dem", "Fens\u00b7ter", "sei\u00b7ner", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis das Fenster klang,", "tokens": ["Bis", "das", "Fens\u00b7ter", "klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Bis die Liebliche sich zeigte,", "tokens": ["Bis", "die", "Lieb\u00b7li\u00b7che", "sich", "zeig\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis das teure Bild", "tokens": ["Bis", "das", "teu\u00b7re", "Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sich ins Tal herunterneigte", "tokens": ["Sich", "ins", "Tal", "her\u00b7un\u00b7ter\u00b7neig\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ruhig, engelmild.", "tokens": ["Ru\u00b7hig", ",", "en\u00b7gel\u00b7mild", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Und dann legt' er froh sich nieder,", "tokens": ["Und", "dann", "legt'", "er", "froh", "sich", "nie\u00b7der", ","], "token_i