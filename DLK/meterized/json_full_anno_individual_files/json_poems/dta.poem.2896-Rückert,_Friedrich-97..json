{"dta.poem.2896": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Grund der Bildung ist dir an- und eingeboren,", "tokens": ["Ein", "Grund", "der", "Bil\u00b7dung", "ist", "dir", "an", "und", "ein\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "TRUNC", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu dem du nichts gewannst, von dem du nichts verloren;", "tokens": ["Zu", "dem", "du", "nichts", "ge\u00b7wannst", ",", "von", "dem", "du", "nichts", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "VVPP", "$,", "APPR", "PRELS", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den aus- und durch- und umzubilden du versucht,", "tokens": ["Den", "aus", "und", "durch", "und", "um\u00b7zu\u00b7bil\u00b7den", "du", "ver\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "TRUNC", "KON", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deines Anbaus Flei\u00df vermehrt des Grundes Frucht.", "tokens": ["Und", "dei\u00b7nes", "An\u00b7baus", "Flei\u00df", "ver\u00b7mehrt", "des", "Grun\u00b7des", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ausgehest du von ihm und kehrst zu ihm zur\u00fcck;", "tokens": ["Aus\u00b7ge\u00b7hest", "du", "von", "ihm", "und", "kehrst", "zu", "ihm", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und dis erkennen ist dein h\u00f6chstes letztes Gl\u00fcck.", "tokens": ["Und", "dis", "er\u00b7ken\u00b7nen", "ist", "dein", "h\u00f6chs\u00b7tes", "letz\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVINF", "VAFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}