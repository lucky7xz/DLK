{"dta.poem.12884": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Abri\u00df eines ehrerbietigen liebha-  \n bers. Aus dem   Ariosti.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein geist, der br\u00fcnstig liebt, und doch nichts wagen will,", "tokens": ["Ein", "geist", ",", "der", "br\u00fcns\u00b7tig", "liebt", ",", "und", "doch", "nichts", "wa\u00b7gen", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "KON", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hofft wenig, fordert nichts, w\u00fcnscht aber trefflich viel.", "tokens": ["Hofft", "we\u00b7nig", ",", "for\u00b7dert", "nichts", ",", "w\u00fcnscht", "a\u00b7ber", "treff\u00b7lich", "viel", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "PIS", "$,", "VVFIN", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}