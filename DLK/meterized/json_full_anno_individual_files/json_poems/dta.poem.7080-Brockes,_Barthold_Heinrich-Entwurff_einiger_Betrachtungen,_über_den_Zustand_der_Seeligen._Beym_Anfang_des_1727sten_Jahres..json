{"dta.poem.7080": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "cy:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann wir sodann, weit h\u00f6her noch erhoben,", "tokens": ["Wann", "wir", "so\u00b7dann", ",", "weit", "h\u00f6\u00b7her", "noch", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$,", "ADJD", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als der Planeten rege Heere,", "tokens": ["Als", "der", "Pla\u00b7ne\u00b7ten", "re\u00b7ge", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Mit einem scharffen Blick von oben,", "tokens": ["Mit", "ei\u00b7nem", "scharf\u00b7fen", "Blick", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Recht als im un\u00fcmschr\u00e4nckten Meere,", "tokens": ["Recht", "als", "im", "un\u00b7\u00fcm\u00b7schr\u00e4nck\u00b7ten", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Viel tausend C\u00f6rper, gleich der Erden,", "tokens": ["Viel", "tau\u00b7send", "C\u00f6r\u00b7per", ",", "gleich", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Voll Wunder schwimmen sehen werden,", "tokens": ["Voll", "Wun\u00b7der", "schwim\u00b7men", "se\u00b7hen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVINF", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und zwar, weil unsrer Seelen Augen", "tokens": ["Und", "zwar", ",", "weil", "uns\u00b7rer", "See\u00b7len", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Viel weiter, als allhier die C\u00f6rperlichen taugen", "tokens": ["Viel", "wei\u00b7ter", ",", "als", "all\u00b7hier", "die", "C\u00f6r\u00b7per\u00b7li\u00b7chen", "tau\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+--+--+---+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Durch hohl geschliffnes Gla\u00df zu sehn;", "tokens": ["Durch", "hohl", "ge\u00b7schliff\u00b7nes", "Gla\u00df", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In eine ungeme\u00dfne Ferne", "tokens": ["In", "ei\u00b7ne", "un\u00b7ge\u00b7me\u00df\u00b7ne", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sodann geschickt sich zu erstrecken!", "tokens": ["So\u00b7dann", "ge\u00b7schickt", "sich", "zu", "er\u00b7stre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Was werden wir f\u00fcr Herrlichkeit entdecken,", "tokens": ["Was", "wer\u00b7den", "wir", "f\u00fcr", "Herr\u00b7lich\u00b7keit", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Wenn so viel Sonnen, als wie Sterne,", "tokens": ["Wenn", "so", "viel", "Son\u00b7nen", ",", "als", "wie", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "$,", "KOUS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Jm un\u00fcmschrencktem Licht und Strahl,", "tokens": ["Jm", "un\u00b7\u00fcm\u00b7schrenck\u00b7tem", "Licht", "und", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die ohne Maa\u00df an Gr\u00f6\u00df\u2019 an Menge sonder Zahl,", "tokens": ["Die", "oh\u00b7ne", "Maa\u00df", "an", "Gr\u00f6\u00df'", "an", "Men\u00b7ge", "son\u00b7der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPR", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In eine Tieffe sonder Grentzen,", "tokens": ["In", "ei\u00b7ne", "Tief\u00b7fe", "son\u00b7der", "Grent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "In herrlich-reinen Flammen gl\u00e4ntzen.", "tokens": ["In", "herr\u00b7lich\u00b7rei\u00b7nen", "Flam\u00b7men", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}