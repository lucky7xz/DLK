{"textgrid.poem.36606": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn***", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, Freund, der Wein, der Wein giebt uns Verstand!", "tokens": ["Ja", ",", "Freund", ",", "der", "Wein", ",", "der", "Wein", "giebt", "uns", "Ver\u00b7stand", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das lehrt Hippokrates,", "tokens": ["Das", "lehrt", "Hip\u00b7po\u00b7kra\u00b7tes", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Konfucius und Aristoteles,", "tokens": ["Kon\u00b7fu\u00b7cius", "und", "A\u00b7ris\u00b7to\u00b7te\u00b7les", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und, der sich einen Gott erfand,", "tokens": ["Und", ",", "der", "sich", "ei\u00b7nen", "Gott", "er\u00b7fand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der gro\u00dfe Sokrates!", "tokens": ["Der", "gro\u00b7\u00dfe", "Sok\u00b7ra\u00b7tes", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Demokritus, der Rabner seiner Zeit,", "tokens": ["De\u00b7mo\u00b7kri\u00b7tus", ",", "der", "Rab\u00b7ner", "sei\u00b7ner", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der wei\u00dfe Lacher go\u00df", "tokens": ["Der", "wei\u00b7\u00dfe", "La\u00b7cher", "go\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Gl\u00e4schen Wein auf einer Lais Scho\u00df,", "tokens": ["Ein", "Gl\u00e4s\u00b7chen", "Wein", "auf", "ei\u00b7ner", "Lais", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und sahe Leer' und Nichtigkeit,", "tokens": ["Und", "sa\u00b7he", "Leer'", "und", "Nich\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lachte darauf los.", "tokens": ["Und", "lach\u00b7te", "da\u00b7rauf", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Heraklitus, der Meister ohne Zucht,", "tokens": ["Her\u00b7a\u00b7kli\u00b7tus", ",", "der", "Meis\u00b7ter", "oh\u00b7ne", "Zucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Der dunkle", "tokens": ["Der", "dunk\u00b7le"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Go\u00df keinen Wein in seinen kalten Kropf,", "tokens": ["Go\