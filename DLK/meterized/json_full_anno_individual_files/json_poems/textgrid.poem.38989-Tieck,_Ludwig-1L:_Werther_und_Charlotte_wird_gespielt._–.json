{"textgrid.poem.38989": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Werther und Charlotte wird gespielt. \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werther und Charlotte wird gespielt. \u2013", "tokens": ["Wert\u00b7her", "und", "Char\u00b7lot\u00b7te", "wird", "ge\u00b7spielt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Wie neugierig str\u00f6mt das Volk", "tokens": ["Wie", "neu\u00b7gie\u00b7rig", "str\u00f6mt", "das", "Volk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Lieblingsst\u00fcck zu sehn,", "tokens": ["Das", "Lieb\u00b7lings\u00b7st\u00fcck", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie ungeduldig sucht jeder Platz", "tokens": ["Wie", "un\u00b7ge\u00b7dul\u00b7dig", "sucht", "je\u00b7der", "Platz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PIAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Den Liebling als Werther zu vernehmen.", "tokens": ["Den", "Lieb\u00b7ling", "als", "Wert\u00b7her", "zu", "ver\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die kleine Bude", "tokens": ["Die", "klei\u00b7ne", "Bu\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Steht ohne Vorhang,", "tokens": ["Steht", "oh\u00b7ne", "Vor\u00b7hang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das volle Sonnenlicht scheint hinein.", "tokens": ["Das", "vol\u00b7le", "Son\u00b7nen\u00b7licht", "scheint", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Unten der gemeine Mann,", "tokens": ["Un\u00b7ten", "der", "ge\u00b7mei\u00b7ne", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In zweien Logen die Vornehmen und Kranken.", "tokens": ["In", "zwei\u00b7en", "Lo\u00b7gen", "die", "Vor\u00b7neh\u00b7men", "und", "Kran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wie sonderbar", "tokens": ["Wie", "son\u00b7der\u00b7bar"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Strecken sich die gro\u00dfen runden weiten Stufen", "tokens": ["Stre\u00b7cken", "sich", "die", "gro\u00b7\u00dfen", "run\u00b7den", "wei\u00b7ten", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "ART", "ADJA", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Der Steinzirkel aus.", "tokens": ["Der", "Stein\u00b7zir\u00b7kel", "aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Ein Sechstheil nur des gro\u00dfen Amphitheaters", "tokens": ["Ein", "Sechst\u00b7heil", "nur", "des", "gro\u00b7\u00dfen", "Am\u00b7phit\u00b7he\u00b7a\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ist eingehegt,", "tokens": ["Ist", "ein\u00b7ge\u00b7hegt", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Um auch von dort zu schaun.", "tokens": ["Um", "auch", "von", "dort", "zu", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Hierher ziehn die Frauen und M\u00e4gdlein,", "tokens": ["Hier\u00b7her", "ziehn", "die", "Frau\u00b7en", "und", "M\u00e4gd\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Mit Schmuck angethan,", "tokens": ["Mit", "Schmuck", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "In farbig seidenen Kleidern,", "tokens": ["In", "far\u00b7big", "sei\u00b7de\u00b7nen", "Klei\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Sie nehmen lachend die hohen Sitze ein,", "tokens": ["Sie", "neh\u00b7men", "la\u00b7chend", "die", "ho\u00b7hen", "Sit\u00b7ze", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Und spannen \u00fcber sich bunte Sonnenschirme.", "tokens": ["Und", "span\u00b7nen", "\u00fc\u00b7ber", "sich", "bun\u00b7te", "Son\u00b7nen\u00b7schir\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Wie ein Tulpenbret gl\u00e4nzt die Versammlung,", "tokens": ["Wie", "ein", "Tul\u00b7pen\u00b7bret", "gl\u00e4nzt", "die", "Ver\u00b7samm\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Wie leuchtende Edelsteine", "tokens": ["Wie", "leuch\u00b7ten\u00b7de", "E\u00b7del\u00b7stei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Bewegen sich die Farben im wechselnden Schimmer.", "tokens": ["Be\u00b7we\u00b7gen", "sich", "die", "Far\u00b7ben", "im", "wech\u00b7seln\u00b7den", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Alles ist aufmerksam,", "tokens": ["Al\u00b7les", "ist", "auf\u00b7merk\u00b7sam", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und wie das Leiden der Dichtung steigt,", "tokens": ["Und", "wie", "das", "Lei\u00b7den", "der", "Dich\u00b7tung", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Err\u00f6then die staunenden H\u00f6rer ger\u00fchrt.", "tokens": ["Er\u00b7r\u00f6\u00b7then", "die", "stau\u00b7nen\u00b7den", "H\u00f6\u00b7rer", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Im s\u00fc\u00dfesten Schmerze melodischen Lauts,", "tokens": ["Im", "s\u00fc\u00b7\u00dfes\u00b7ten", "Schmer\u00b7ze", "me\u00b7lo\u00b7di\u00b7schen", "Lauts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und alle H\u00e4nde, F\u00e4cher, T\u00fccher, Beine, St\u00f6cke", "tokens": ["Und", "al\u00b7le", "H\u00e4n\u00b7de", ",", "F\u00e4\u00b7cher", ",", "T\u00fc\u00b7cher", ",", "Bei\u00b7ne", ",", "St\u00f6\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PIAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erregen das lauteste Get\u00fcmmel freudigen Beifalls,", "tokens": ["Er\u00b7re\u00b7gen", "das", "lau\u00b7tes\u00b7te", "Ge\u00b7t\u00fcm\u00b7mel", "freu\u00b7di\u00b7gen", "Bei\u00b7falls", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Und tausend Thr\u00e4nen flie\u00dfen.", "tokens": ["Und", "tau\u00b7send", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gl\u00fcckseliger Dichter,", "tokens": ["Gl\u00fcck\u00b7se\u00b7li\u00b7ger", "Dich\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Der du nur die schwache Feder", "tokens": ["Der", "du", "nur", "die", "schwa\u00b7che", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Wohllaut der s\u00fc\u00dfesten Sprache", "tokens": ["In", "den", "Wohl\u00b7laut", "der", "s\u00fc\u00b7\u00dfes\u00b7ten", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Nachl\u00e4\u00dfig tauchen darfst!", "tokens": ["Nach\u00b7l\u00e4\u00b7\u00dfig", "tau\u00b7chen", "darfst", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wozu noch Bilder, Gedanken, Gef\u00fchle,", "tokens": ["Wo\u00b7zu", "noch", "Bil\u00b7der", ",", "Ge\u00b7dan\u00b7ken", ",", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Wenn dein Mutterton", "tokens": ["Wenn", "dein", "Mut\u00b7ter\u00b7ton"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Schon f\u00fcr dich dichtet und die Herzen bewegt?", "tokens": ["Schon", "f\u00fcr", "dich", "dich\u00b7tet", "und", "die", "Her\u00b7zen", "be\u00b7wegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Doch Heil dir, Werther,", "tokens": ["Doch", "Heil", "dir", ",", "Wert\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "PPER", "$,", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Denn nie vernahm ich wieder", "tokens": ["Denn", "nie", "ver\u00b7nahm", "ich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die zarten Worte also schmerzlich und s\u00fc\u00df erklingend.", "tokens": ["Die", "zar\u00b7ten", "Wor\u00b7te", "al\u00b7so", "schmerz\u00b7lich", "und", "s\u00fc\u00df", "er\u00b7klin\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Charlotte, das edelste Bild,", "tokens": ["Char\u00b7lot\u00b7te", ",", "das", "e\u00b7dels\u00b7te", "Bild", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.12": {"text": "Anmuth jede Geberde,", "tokens": ["An\u00b7muth", "je\u00b7de", "Ge\u00b7ber\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.13": {"text": "Kr\u00e4ftig und gro\u00df,", "tokens": ["Kr\u00e4f\u00b7tig", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Die Stimme zart und voll: \u2013", "tokens": ["Die", "Stim\u00b7me", "zart", "und", "voll", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O weh!", "tokens": ["O", "weh", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Was mischt sich in die Leiden der Liebenden?", "tokens": ["Was", "mischt", "sich", "in", "die", "Lei\u00b7den", "der", "Lie\u00b7ben\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein ferner Donner ert\u00f6nt vernehmlich,", "tokens": ["Ein", "fer\u00b7ner", "Don\u00b7ner", "er\u00b7t\u00f6nt", "ver\u00b7nehm\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die leuchtenden Farben bewegen sich unruhig,", "tokens": ["Die", "leuch\u00b7ten\u00b7den", "Far\u00b7ben", "be\u00b7we\u00b7gen", "sich", "un\u00b7ru\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.5": {"text": "Auch das Parterre murrt schon.", "tokens": ["Auch", "das", "Par\u00b7ter\u00b7re", "murrt", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wieder ein Schlag,", "tokens": ["Und", "wie\u00b7der", "ein", "Schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und der Regen str\u00f6mt schwer in gro\u00dfen Tropfen,", "tokens": ["Und", "der", "Re\u00b7gen", "str\u00f6mt", "schwer", "in", "gro\u00b7\u00dfen", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Da dr\u00e4ngen sich Weiber und M\u00e4dchen herbei,", "tokens": ["Da", "dr\u00e4n\u00b7gen", "sich", "Wei\u00b7ber", "und", "M\u00e4d\u00b7chen", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Sie springen die Stufen herab,", "tokens": ["Sie", "sprin\u00b7gen", "die", "Stu\u00b7fen", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Ein Flammenmeer bunter Farben,", "tokens": ["Ein", "Flam\u00b7men\u00b7meer", "bun\u00b7ter", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Sie suchen alle Schutz, wo keiner zu finden,", "tokens": ["Sie", "su\u00b7chen", "al\u00b7le", "Schutz", ",", "wo", "kei\u00b7ner", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PWAV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-