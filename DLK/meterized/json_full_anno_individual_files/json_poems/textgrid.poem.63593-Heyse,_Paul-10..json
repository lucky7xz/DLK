{"textgrid.poem.63593": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Betracht' ich unser schwankes Menschenlos,", "tokens": ["Be\u00b7tracht'", "ich", "un\u00b7ser", "schwan\u00b7kes", "Men\u00b7schen\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geringe Lust, von Unlust \u00fcberwogen,", "tokens": ["Ge\u00b7rin\u00b7ge", "Lust", ",", "von", "Un\u00b7lust", "\u00fc\u00b7ber\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Angst vorm Wechsel in des Gl\u00fcckes Scho\u00df,", "tokens": ["Die", "Angst", "vorm", "Wech\u00b7sel", "in", "des", "Gl\u00fc\u00b7ckes", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Jugend Hoffnungen, so schwer betrogen,", "tokens": ["Der", "Ju\u00b7gend", "Hoff\u00b7nun\u00b7gen", ",", "so", "schwer", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Alters bittre Weisheit: \u00bbAlles nichtig!\u00ab \u2013", "tokens": ["Des", "Al\u00b7ters", "bitt\u00b7re", "Weis\u00b7heit", ":", "\u00bb", "Al\u00b7les", "nich\u00b7tig", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "$(", "PIS", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Liebe G\u00f6tterrausch, so bald verflogen:", "tokens": ["Der", "Lie\u00b7be", "G\u00f6t\u00b7ter\u00b7rausch", ",", "so", "bald", "ver\u00b7flo\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dann preis' ich dich, mein Kind, da\u00df nur so fl\u00fcchtig", "tokens": ["Dann", "preis'", "ich", "dich", ",", "mein", "Kind", ",", "da\u00df", "nur", "so", "fl\u00fcch\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,", "KOUS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dein kleiner Fu\u00df des Lebens Bahn ber\u00fchrt,", "tokens": ["Dein", "klei\u00b7ner", "Fu\u00df", "des", "Le\u00b7bens", "Bahn", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und gern auf dich zu deinem Heil verzicht' ich.", "tokens": ["Und", "gern", "auf", "dich", "zu", "dei\u00b7nem", "Heil", "ver\u00b7zicht'", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie treu ich dich an meiner Hand gef\u00fchrt,", "tokens": ["Wie", "treu", "ich", "dich", "an", "mei\u00b7ner", "Hand", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Nicht h\u00e4tt' ich eine dir erspart der Plagen,", "tokens": ["Nicht", "h\u00e4tt'", "ich", "ei\u00b7ne", "dir", "er\u00b7spart", "der", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PPER", "ART", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die je ein Herz beklemmend eingeschn\u00fcrt.", "tokens": ["Die", "je", "ein", "Herz", "be\u00b7klem\u00b7mend", "ein\u00b7ge\u00b7schn\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Dein M\u00e4ppchen h\u00e4ttst zur Schule du getragen,", "tokens": ["Dein", "M\u00e4pp\u00b7chen", "h\u00e4ttst", "zur", "Schu\u00b7le", "du", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Zitternd in Furcht und Scham, wenn dir einmal", "tokens": ["Zit\u00b7ternd", "in", "Furcht", "und", "Scham", ",", "wenn", "dir", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.15": {"text": "Die Antwort stockte auf des Lehrers Fragen.", "tokens": ["Die", "Ant\u00b7wort", "stock\u00b7te", "auf", "des", "Leh\u00b7rers", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Dann Ehrgeiz, Reue, stumme Liebesqual,", "tokens": ["Dann", "Ehr\u00b7geiz", ",", "Reu\u00b7e", ",", "stum\u00b7me", "Lie\u00b7bes\u00b7qual", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Freundschaft an Stolz und Wankelmut verschwendet,", "tokens": ["Freund\u00b7schaft", "an", "Stolz", "und", "Wan\u00b7kel\u00b7mut", "ver\u00b7schwen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.18": {"text": "Und was die Jugend lockt und irrt zumal.", "tokens": ["Und", "was", "die", "Ju\u00b7gend", "lockt", "und", "irrt", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und h\u00e4tte sich das Herz dir zugewendet", "tokens": ["Und", "h\u00e4t\u00b7te", "sich", "das", "Herz", "dir", "zu\u00b7ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PRF", "ART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Des Einen, dem im Leben wie im Tod", "tokens": ["Des", "Ei\u00b7nen", ",", "dem", "im", "Le\u00b7ben", "wie", "im", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Du deine Pflicht und Treue gern verpf\u00e4ndet,", "tokens": ["Du", "dei\u00b7ne", "Pflicht", "und", "Treu\u00b7e", "gern", "ver\u00b7pf\u00e4n\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "O dann nach kurzem Gl\u00fcck die lange Not", "tokens": ["O", "dann", "nach", "kur\u00b7zem", "Gl\u00fcck", "die", "lan\u00b7ge", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Der Mutterschaft, erst Kinder ihm geb\u00e4ren", "tokens": ["Der", "Mut\u00b7ter\u00b7schaft", ",", "erst", "Kin\u00b7der", "ihm", "ge\u00b7b\u00e4\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und dann sie aufziehn, tausendfach bedroht;", "tokens": ["Und", "dann", "sie", "auf\u00b7ziehn", ",", "tau\u00b7send\u00b7fach", "be\u00b7droht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVINF", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Die Fiebern\u00e4chte, wo mit bangen Z\u00e4hren", "tokens": ["Die", "Fie\u00b7bern\u00b7\u00e4ch\u00b7te", ",", "wo", "mit", "ban\u00b7gen", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Am kleinen Bett du h\u00e4ttest wach gesessen,", "tokens": ["Am", "klei\u00b7nen", "Bett", "du", "h\u00e4t\u00b7test", "wach", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Den Feind des Lebens k\u00e4mpfend abzuwehren.", "tokens": ["Den", "Feind", "des", "Le\u00b7bens", "k\u00e4mp\u00b7fend", "ab\u00b7zu\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "O armes Frauenleben! Denk' ich dessen,", "tokens": ["O", "ar\u00b7mes", "Frau\u00b7en\u00b7le\u00b7ben", "!", "Denk'", "ich", "des\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "VVIMP", "PPER", "PDS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "So ist mir fast, es sei ein Hochgewinn,", "tokens": ["So", "ist", "mir", "fast", ",", "es", "sei", "ein", "Hoch\u00b7ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Da\u00df wir dich nur so kurze Frist besessen.", "tokens": ["Da\u00df", "wir", "dich", "nur", "so", "kur\u00b7ze", "Frist", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Dein Auge sah so ernsthaft vor sich hin,", "tokens": ["Dein", "Au\u00b7ge", "sah", "so", "ernst\u00b7haft", "vor", "sich", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Als h\u00e4ttest du, das Schwere leicht zu nehmen,", "tokens": ["Als", "h\u00e4t\u00b7test", "du", ",", "das", "Schwe\u00b7re", "leicht", "zu", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Schwermut zu viel, zu wenig leichten Sinn;", "tokens": ["Schwer\u00b7mut", "zu", "viel", ",", "zu", "we\u00b7nig", "leich\u00b7ten", "Sinn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIS", "$,", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.34": {"text": "Zu starken Willen, weich dich zu bequemen,", "tokens": ["Zu", "star\u00b7ken", "Wil\u00b7len", ",", "weich", "dich", "zu", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Zu treuen Sinn, um lachend zu verzichten,", "tokens": ["Zu", "treu\u00b7en", "Sinn", ",", "um", "la\u00b7chend", "zu", "ver\u00b7zich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUI", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Zu reines Herz, der Schuld dich nicht zu sch\u00e4men.", "tokens": ["Zu", "rei\u00b7nes", "Herz", ",", "der", "Schuld", "dich", "nicht", "zu", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und Solche wissen schwer sich einzurichten", "tokens": ["Und", "Sol\u00b7che", "wis\u00b7sen", "schwer", "sich", "ein\u00b7zu\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADJD", "PRF", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "In dieser argen Welt, wo man sich drehn", "tokens": ["In", "die\u00b7ser", "ar\u00b7gen", "Welt", ",", "wo", "man", "sich", "drehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "PWAV", "PIS", "PRF", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Und winden mu\u00df im Zwiespalt enger Pflichten. \u2013", "tokens": ["Und", "win\u00b7den", "mu\u00df", "im", "Zwies\u00b7palt", "en\u00b7ger", "Pflich\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPRART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Und doch, mein Kind: warst du auch ausersehn,", "tokens": ["Und", "doch", ",", "mein", "Kind", ":", "warst", "du", "auch", "au\u00b7ser\u00b7sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "NN", "$.", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Zu bluten aus den tiefsten Lebenswunden,", "tokens": ["Zu", "blu\u00b7ten", "aus", "den", "tiefs\u00b7ten", "Le\u00b7bens\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Doch h\u00e4tt' ich dir's geg\u00f6nnt, im Kampf zu stehn.", "tokens": ["Doch", "h\u00e4tt'", "ich", "dir's", "ge\u00b7g\u00f6nnt", ",", "im", "Kampf", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "VVPP", "$,", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Der Kr\u00e4fte frohes Spiel h\u00e4ttst du empfunden,", "tokens": ["Der", "Kr\u00e4f\u00b7te", "fro\u00b7hes", "Spiel", "h\u00e4ttst", "du", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Des Ringens Stolz, des Sieges hohe Lust,", "tokens": ["Des", "Rin\u00b7gens", "Stolz", ",", "des", "Sie\u00b7ges", "ho\u00b7he", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Die herbe Tage aufwiegt durch Sekunden.", "tokens": ["Die", "her\u00b7be", "Ta\u00b7ge", "auf\u00b7wiegt", "durch", "Se\u00b7kun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Du h\u00e4ttst an deine kleine Menschenbrust", "tokens": ["Du", "h\u00e4ttst", "an", "dei\u00b7ne", "klei\u00b7ne", "Men\u00b7schen\u00b7brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Die Welt gedr\u00fcckt und staunend zu den St