{"dta.poem.3721": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Kommt H\u00fcner bibi .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Reiter zu Pferd,               ", "tokens": ["Der", "Rei\u00b7ter", "zu", "Pferd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Die K\u00f6chin am Heerd,", "tokens": ["Die", "K\u00f6\u00b7chin", "am", "Heerd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die Nonne im Kloster,", "tokens": ["Die", "Non\u00b7ne", "im", "Klos\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Fischer im Wasser,", "tokens": ["Der", "Fi\u00b7scher", "im", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Mutter backt Kuchen,", "tokens": ["Die", "Mut\u00b7ter", "backt", "Ku\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sie l\u00e4\u00dft mich nicht gucken,", "tokens": ["Sie", "l\u00e4\u00dft", "mich", "nicht", "gu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sie giebt mir ein Brocken,", "tokens": ["Sie", "giebt", "mir", "ein", "Bro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Soll H\u00fcner mit locken,", "tokens": ["Soll", "H\u00fc\u00b7ner", "mit", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "K\u00f6mmt H\u00fcner bibi,", "tokens": ["K\u00f6mmt", "H\u00fc\u00b7ner", "bi\u00b7bi", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Die Knochen i\u00dft sie.", "tokens": ["Die", "Kno\u00b7chen", "i\u00dft", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}