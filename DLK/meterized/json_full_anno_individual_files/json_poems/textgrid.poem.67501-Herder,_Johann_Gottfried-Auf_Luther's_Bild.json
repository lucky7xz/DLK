{"textgrid.poem.67501": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Auf Luther's Bild", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guter schwarzer M\u00f6nch, mit starkem Arme begannst Du", "tokens": ["Gu\u00b7ter", "schwar\u00b7zer", "M\u00f6nch", ",", "mit", "star\u00b7kem", "Ar\u00b7me", "be\u00b7gannst", "Du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Auszufegen den Staub, der die Alt\u00e4re verbarg;", "tokens": ["Aus\u00b7zu\u00b7fe\u00b7gen", "den", "Staub", ",", "der", "die", "Al\u00b7t\u00e4\u00b7re", "ver\u00b7barg", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber schnell entrissen Dir Andre das s\u00e4ubernde Werkzeug,", "tokens": ["A\u00b7ber", "schnell", "ent\u00b7ris\u00b7sen", "Dir", "And\u00b7re", "das", "s\u00e4u\u00b7bern\u00b7de", "Werk\u00b7zeug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Lasen vom Staube das Gold, hingen den Besen sich auf.", "tokens": ["La\u00b7sen", "vom", "Stau\u00b7be", "das", "Gold", ",", "hin\u00b7gen", "den", "Be\u00b7sen", "sich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PRF", "PTKVZ"