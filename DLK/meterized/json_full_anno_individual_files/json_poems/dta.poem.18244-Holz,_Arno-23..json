{"dta.poem.18244": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Und grau in grau verschwimmt die Luft,", "tokens": ["Und", "grau", "in", "grau", "ver\u00b7schwimmt", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und um den Herd und um sein Feuer", "tokens": ["Und", "um", "den", "Herd", "und", "um", "sein", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Webt Winterduft.", "tokens": ["Webt", "Win\u00b7ter\u00b7duft", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das ist die Zeit, wo sich die Seele", "tokens": ["Das", "ist", "die", "Zeit", ",", "wo", "sich", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PWAV", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Stilleinsam auf sich selbst besinnt", "tokens": ["Stil\u00b7lein\u00b7sam", "auf", "sich", "selbst", "be\u00b7sinnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PRF", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie im Lenz einst Philomele", "tokens": ["Und", "wie", "im", "Lenz", "einst", "Phi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN", "ADV", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Lieder sinnt.", "tokens": ["Auf", "Lie\u00b7der", "sinnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Willkommen drum zur guten Stunde,", "tokens": ["Will\u00b7kom\u00b7men", "drum", "zur", "gu\u00b7ten", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O Muse, unter meinem Dach;", "tokens": ["O", "Mu\u00b7se", ",", "un\u00b7ter", "mei\u00b7nem", "Dach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist auch dies St\u00fcbchen hier im Grunde", "tokens": ["Ist", "auch", "dies", "St\u00fcb\u00b7chen", "hier", "im", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PDS", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Prunkgemach!", "tokens": ["Kein", "Prunk\u00b7ge\u00b7mach", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Vier W\u00e4nde nur und was darinnen,", "tokens": ["Vier", "W\u00e4n\u00b7de", "nur", "und", "was", "da\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "KON", "PWS", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Tisch, zwei St\u00fchle und ein Schrein;", "tokens": ["Ein", "Tisch", ",", "zwei", "St\u00fch\u00b7le", "und", "ein", "Schrein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "CARD", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sitzen wir vergn\u00fcgt und sinnen", "tokens": ["So", "sit\u00b7zen", "wir", "ver\u00b7gn\u00fcgt", "und", "sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beim Lampenschein.", "tokens": ["Beim", "Lam\u00b7pen\u00b7schein", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Doch drau\u00dfen, welch ein grauses Wetter", "tokens": ["Doch", "drau\u00b7\u00dfen", ",", "welch", "ein", "grau\u00b7ses", "Wet\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durchrast gespensterhaft die Nacht?", "tokens": ["Durc\u00b7hrast", "ge\u00b7spens\u00b7ter\u00b7haft", "die", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir d\u00e4ucht, so klingt das Horngeschmetter", "tokens": ["Mir", "d\u00e4ucht", ",", "so", "klingt", "das", "Horn\u00b7ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der wilden Jagd!", "tokens": ["Der", "wil\u00b7den", "Jagd", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Der Regen peitscht in j\u00e4hem Grimme", "tokens": ["Der", "Re\u00b7gen", "peitscht", "in", "j\u00e4\u00b7hem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ans Fenster, da\u00df der Laden wankt,", "tokens": ["Ans", "Fens\u00b7ter", ",", "da\u00df", "der", "La\u00b7den", "wankt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und durch die Luft heult eine Stimme", "tokens": ["Und", "durch", "die", "Luft", "heult", "ei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00e4chzt und bangt.", "tokens": ["Und", "\u00e4chzt", "und", "bangt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Ein Kreischen wie von Wetterh\u00e4hnen", "tokens": ["Ein", "Krei\u00b7schen", "wie", "von", "Wet\u00b7ter\u00b7h\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Umkreist der Kirche nahen Thurm,", "tokens": ["Um\u00b7kreist", "der", "Kir\u00b7che", "na\u00b7hen", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ihn bedr\u00e4ut mit giftgen Z\u00e4hnen", "tokens": ["Denn", "ihn", "be\u00b7dr\u00e4ut", "mit", "gift\u00b7gen", "Z\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Drache Sturm.", "tokens": ["Der", "Dra\u00b7che", "Sturm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Von Menschen scheint die Stadt verlassen,", "tokens": ["Von", "Men\u00b7schen", "scheint", "die", "Stadt", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Licht mehr, das nicht l\u00e4ngst verblich,", "tokens": ["Kein", "Licht", "mehr", ",", "das", "nicht", "l\u00e4ngst", "ver\u00b7blich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "PRELS", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wer hinabblickt auf die Gassen,", "tokens": ["Und", "wer", "hin\u00b7ab\u00b7blickt", "auf", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bekreuzigt sich.", "tokens": ["Be\u00b7kreu\u00b7zigt", "sich", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "F\u00fcrwahr, ist da nicht unsre Zelle", "tokens": ["F\u00fcr\u00b7wahr", ",", "ist", "da", "nicht", "uns\u00b7re", "Zel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "ADV", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein irdisch St\u00fccklein Seligkeit,", "tokens": ["Ein", "ir\u00b7disch", "St\u00fcck\u00b7lein", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und predigt nicht des L\u00e4mpchens Helle", "tokens": ["Und", "pre\u00b7digt", "nicht", "des", "L\u00e4mp\u00b7chens", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gem\u00fcthlichkeit?", "tokens": ["Ge\u00b7m\u00fcth\u00b7lich\u00b7keit", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Und n\u00e4her r\u00fccken wir zusammen", "tokens": ["Und", "n\u00e4\u00b7her", "r\u00fc\u00b7cken", "wir", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was ich frage, thust du kund;", "tokens": ["Und", "was", "ich", "fra\u00b7ge", ",", "thust", "du", "kund", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Auge spielt in blauen Flammen,", "tokens": ["Dein", "Au\u00b7ge", "spielt", "in", "blau\u00b7en", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es lacht dein Mund.", "tokens": ["Es", "lacht", "dein", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Aus Ost und Westen, S\u00fcd und Norden,", "tokens": ["Aus", "Ost", "und", "Wes\u00b7ten", ",", "S\u00fcd", "und", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Steinen, Blumen und Gethier,", "tokens": ["Von", "Stei\u00b7nen", ",", "Blu\u00b7men", "und", "Ge\u00b7thier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Warum und wie sie so geworden,", "tokens": ["Wa\u00b7rum", "und", "wie", "sie", "so", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "PWAV", "PPER", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erz\u00e4hlst du mir:", "tokens": ["Er\u00b7z\u00e4hlst", "du", "mir", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Und was einst vor so manchem J\u00e4hrchen", "tokens": ["Und", "was", "einst", "vor", "so", "man\u00b7chem", "J\u00e4hr\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt erlebt in Lust und Leid,", "tokens": ["Die", "Welt", "er\u00b7lebt", "in", "Lust", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich bitte, auch ein M\u00e4rchen", "tokens": ["Und", "wenn", "ich", "bit\u00b7te", ",", "auch", "ein", "M\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus alter Zeit.", "tokens": ["Aus", "al\u00b7ter", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Wie Siegfried einst die Maid Brunhilde", "tokens": ["Wie", "Sieg\u00b7fried", "einst", "die", "Maid", "Brun\u00b7hil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch seinen Ku\u00df vom Schlaf erweckt,", "tokens": ["Durch", "sei\u00b7nen", "Ku\u00df", "vom", "Schlaf", "er\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie sich hinter diesem Bilde", "tokens": ["Und", "wie", "sich", "hin\u00b7ter", "die\u00b7sem", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Sinn versteckt.", "tokens": ["Ein", "Sinn", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.14": {"line.1": {"text": "Wie j\u00e4hrlich noch die Mutter Erde", "tokens": ["Wie", "j\u00e4hr\u00b7lich", "noch", "die", "Mut\u00b7ter", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich einspinnt in die Winternacht,", "tokens": ["Sich", "ein\u00b7spinnt", "in", "die", "Win\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Bis sie im Lenz durch Gottes Werde", "tokens": ["Bis", "sie", "im", "Lenz", "durch", "Got\u00b7tes", "Wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aufs Neu erwacht.", "tokens": ["Aufs", "Neu", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.15": {"line.1": {"text": "Drum la\u00df den Tod nur drau\u00dfen dr\u00e4uen,", "tokens": ["Drum", "la\u00df", "den", "Tod", "nur", "drau\u00b7\u00dfen", "dr\u00e4u\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir zwei sind gegen ihn gefeit;", "tokens": ["Wir", "zwei", "sind", "ge\u00b7gen", "ihn", "ge\u00b7feit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Leben wird sich schon erneuen", "tokens": ["Das", "Le\u00b7ben", "wird", "sich", "schon", "er\u00b7neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seiner Zeit.", "tokens": ["Zu", "sei\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.16": {"line.1": {"text": "Als Lenz wird es uns Veilchen bringen,", "tokens": ["Als", "Lenz", "wird", "es", "uns", "Veil\u00b7chen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und t\u00e4ndeln wird's als Bl\u00fcthenfall,", "tokens": ["Und", "t\u00e4n\u00b7deln", "wird's", "als", "Bl\u00fc\u00b7then\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Nachts im Flieder wird es singen", "tokens": ["Und", "Nachts", "im", "Flie\u00b7der", "wird", "es", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als Nachtigall!", "tokens": ["Als", "Nach\u00b7ti\u00b7gall", "!"], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}