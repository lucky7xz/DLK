 "tokens": ["Was\u00b7ser", "und", "dem", "trock\u00b7nen", "See\u00b7sand", ",", "Mu\u00b7scheln", "lie\u00b7gen", "da", "ver\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$,", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "M\u00f6wen h\u00fcpfen auf und ab, reingefegt vom Wind sind die D\u00fcnen \u2013", "tokens": ["M\u00f6\u00b7wen", "h\u00fcp\u00b7fen", "auf", "und", "ab", ",", "rein\u00b7ge\u00b7fegt", "vom", "Wind", "sind", "die", "D\u00fc\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,", "VVFIN", "APPRART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "+-+-+-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wem geh\u00f6ren die?", "tokens": ["Wem", "ge\u00b7h\u00f6\u00b7ren", "die", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Die zitternde Blumenwiese mit den hohen saftigen Stengeln,", "tokens": ["Die", "zit\u00b7tern\u00b7de", "Blu\u00b7men\u00b7wie\u00b7se", "mit", "den", "ho\u00b7hen", "saf\u00b7ti\u00b7gen", "Sten\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "die K\u00e4fer, die summsen, die Berge, die her\u00fcberd\u00e4mmern,", "tokens": ["die", "K\u00e4\u00b7fer", ",", "die", "summ\u00b7sen", ",", "die", "Ber\u00b7ge", ",", "die", "her\u00b7\u00fc\u00b7berd\u00b7\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "punct", "word", "word", "pu