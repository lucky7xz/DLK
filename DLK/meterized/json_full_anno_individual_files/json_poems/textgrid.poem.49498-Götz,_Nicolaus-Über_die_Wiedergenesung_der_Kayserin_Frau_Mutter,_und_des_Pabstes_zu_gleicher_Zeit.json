{"textgrid.poem.49498": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcber die Wiedergenesung der Kayserin Frau Mutter, und des Pabstes zu gleicher Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der heilge Vater Pabst zu Rom,", "tokens": ["Der", "heil\u00b7ge", "Va\u00b7ter", "Pabst", "zu", "Rom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(des Allerh\u00f6chsten Vice-Dom)", "tokens": ["(", "des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Vi\u00b7ce\u00b7Dom", ")"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unsre Kayserin Frau Mutter:", "tokens": ["Und", "uns\u00b7re", "Kay\u00b7se\u00b7rin", "Frau", "Mut\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Erde H\u00e4upter, beede fromm,", "tokens": ["Der", "Er\u00b7de", "H\u00e4up\u00b7ter", ",", "bee\u00b7de", "fromm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sind durch der \u00c4rzte strengen Orden", "tokens": ["Sind", "durch", "der", "\u00c4rz\u00b7te", "stren\u00b7gen", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu gleicher Zeit verdammet worden,", "tokens": ["Zu", "g