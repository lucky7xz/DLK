{"dta.poem.8540": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Bezaubernd .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und wenn sie vor&#252;ber am Fenster geht,", "tokens": ["Und", "wenn", "sie", "vor", "&#252;", "ber", "am", "Fens\u00b7ter", "geht", ","], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "$(", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und f\u00e4llt ihr Schatten auf die Gasse,", "tokens": ["Und", "f\u00e4llt", "ihr", "Schat\u00b7ten", "auf", "die", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da steh\u2019n die J\u00fcnglinge sinnberaubt", "tokens": ["Da", "steh'n", "die", "J\u00fcng\u00b7lin\u00b7ge", "sinn\u00b7be\u00b7raubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wissen nicht, was sie beginnen;", "tokens": ["Und", "wis\u00b7sen", "nicht", ",", "was", "sie", "be\u00b7gin\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch in die Moschee die Derwische flieh\u2019n,", "tokens": ["Doch", "in", "die", "Mo\u00b7schee", "die", "Der\u00b7wi\u00b7sche", "flieh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Rufend: Allah! errett\u2019 uns!", "tokens": ["Ru\u00b7fend", ":", "Al\u00b7lah", "!", "er\u00b7rett'", "uns", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$.", "NN", "$.", "VVFIN", "PPER", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Denn dein Feuer vom Himmel fiel,", "tokens": ["Denn", "dein", "Feu\u00b7er", "vom", "Him\u00b7mel", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und m\u00f6gen ihm nimmer entrinnen.", "tokens": ["Und", "m\u00f6\u00b7gen", "ihm", "nim\u00b7mer", "ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}