{"dta.poem.13600": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Intermezzo .  \n  Chor der Schmiede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Bist zum k\u00fcnft'gen Holmgang", "tokens": ["Bist", "zum", "k\u00fcnft'\u00b7gen", "Holm\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Nun geh\u00e4mmert, Nordmann!", "tokens": ["Nun", "ge\u00b7h\u00e4m\u00b7mert", ",", "Nord\u00b7mann", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schl\u00e4ngelt sich im Todkampf", "tokens": ["Schl\u00e4n\u00b7gelt", "sich", "im", "Tod\u00b7kampf"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gluthroth einst dein Schwerdtblitz \u2014", "tokens": ["Gluth\u00b7roth", "einst", "dein", "Schwerdt\u00b7blitz"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Sehr weint da die Heldbraut \u2014", "tokens": ["Sehr", "weint", "da", "die", "Held\u00b7braut"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+++", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Denk! der Waffenmeister", "tokens": ["Denk", "!", "der", "Waf\u00b7fen\u00b7meis\u00b7ter"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVIMP", "$.", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Hammert, singet! Ist's auch", "tokens": ["Ham\u00b7mert", ",", "sin\u00b7get", "!", "Ist's", "auch"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$.", "NE", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Ungereimt, so klappt's doch!", "tokens": ["Un\u00b7ge\u00b7reimt", ",", "so", "klappt's", "doch", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "VVFIN", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}