{"textgrid.poem.35178": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Dein eigener Richterspruch", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du geliebt? Wei\u00dft du wohl, was das hei\u00dft?", "tokens": ["Hast", "du", "ge\u00b7liebt", "?", "Wei\u00dft", "du", "wohl", ",", "was", "das", "hei\u00dft", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "VVFIN", "PPER", "ADV", "$,", "PWS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denk nach, denk nach, wenn du es noch nicht wei\u00dft.", "tokens": ["Denk", "nach", ",", "denk", "nach", ",", "wenn", "du", "es", "noch", "nicht", "wei\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Frage wird dir jeden Tag gegeben;", "tokens": ["Die", "Fra\u00b7ge", "wird", "dir", "je\u00b7den", "Tag", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Antwort hast du jeden Tag zu leben.", "tokens": ["Die", "Ant\u00b7wort", "hast", "du", "je\u00b7den", "Tag", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hast du geliebt? Es wird ein Ja verlangt,", "tokens": ["Hast", "du", "ge\u00b7liebt", "?", "Es", "wird", "ein", "Ja", "ver\u00b7langt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weil Jeder so wie du nach Liebe bangt.", "tokens": ["Weil", "Je\u00b7der", "so", "wie", "du", "nach", "Lie\u00b7be", "bangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "KOKOM", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was du ihm giebst, sein Engel tr\u00e4gts nach oben,", "tokens": ["Was", "du", "ihm", "giebst", ",", "sein", "En\u00b7gel", "tr\u00e4gts", "nach", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dort, dort wird es f\u00fcr dich aufgehoben.", "tokens": ["Und", "dort", ",", "dort", "wird", "es", "f\u00fcr", "dich", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hast du geliebt? So wirst du einst gefragt,", "tokens": ["Hast", "du", "ge\u00b7liebt", "?", "So", "wirst", "du", "einst", "ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn das Gericht des Allerforschers tagt.", "tokens": ["Wenn", "das", "Ge\u00b7richt", "des", "Al\u00b7ler\u00b7for\u00b7schers", "tagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Urtheil hast du dir dann selbst zu geben;", "tokens": ["Das", "Ur\u00b7theil", "hast", "du", "dir", "dann", "selbst", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es liegt schon da: Es ist dein Erdenleben!", "tokens": ["Es", "liegt", "schon", "da", ":", "Es", "ist", "dein", "Er\u00b7den\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hast du geliebt? Wei\u00dft du wohl, was das hei\u00dft?", "tokens": ["Hast", "du", "ge\u00b7liebt", "?", "Wei\u00dft", "du", "wohl", ",", "was", "das", "hei\u00dft", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "VVFIN", "PPER", "ADV", "$,", "PWS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denk nach, denk nach, wenn du es noch nicht wei\u00dft.", "tokens": ["Denk", "nach", ",", "denk", "nach", ",", "wenn", "du", "es", "noch", "nicht", "wei\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "