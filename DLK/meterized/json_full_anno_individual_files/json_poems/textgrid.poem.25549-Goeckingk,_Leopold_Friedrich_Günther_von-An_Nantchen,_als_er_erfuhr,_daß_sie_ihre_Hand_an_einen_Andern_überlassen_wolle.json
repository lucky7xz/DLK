 ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRF", "APPR", "ADV", "ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "War f\u00fcr mich, mehr als das Weltgericht.", "tokens": ["War", "f\u00fcr", "mich", ",", "mehr", "als", "das", "Welt\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$,", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Lieber mir den Tod, als dich betr\u00fcbt:", "tokens": ["Lie\u00b7ber", "mir", "den", "Tod", ",", "als", "dich", "be\u00b7tr\u00fcbt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Sage, wer hat z\u00e4rtlicher geliebt?", "tokens": ["Sa\u00b7ge", ",", "wer", "hat", "z\u00e4rt\u00b7li\u00b7cher", "ge\u00b7liebt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Rede nun! wo bleiben deine Schw\u00fcre?", "tokens": ["Re\u00b7de", "nun", "!", "wo", "blei\u00b7ben", "dei\u00b7ne", "Schw\u00fc\u00b7re", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Schwurst du, sichrer zu betr\u00fcgen, sie?", "tokens": ["Schwurst", "du", ",", "sich\u00b7rer", "zu", "be\u00b7tr\u00fc\u00b7gen", ",", "sie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VVINF", "$,", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Nicht genug, da\u00df ich dein Herz verliere,", "tokens": ["Nicht", "ge\u00b7nug", ",", "da\u00df", "ich", "dein", "Herz", "ver\u00b7lie\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Sondern ", "tokens": ["Son\u00b7dern"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Gibst du dich nicht einem wilden Thiere?", "tokens": ["Gibst", "du", "dich", "nicht", "ei\u00b7nem", "wil\u00b7den", "Thie\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Denn was ist der sonst, der seine Knie", "tokens": ["Denn", "was", "ist", "der", "sonst", ",", "der", "sei\u00b7ne", "Knie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "ART", "ADV", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Mit Geschenken kriechend vor dir biegt,", "tokens": ["Mit", "Ge\u00b7schen\u00b7ken", "krie\u00b7chend", "vor", "dir", "biegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.32": {"text": "Und, mit Brunst im Auge, Liebe l\u00fcgt?", "tokens": ["Und", ",", "mit", "Brunst", "im", "Au\u00b7ge", ",", "Lie\u00b7be", "l\u00fcgt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "APPRART", "NN", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Geh denn! hole Amarantens Lieder,", "tokens": ["Geh", "denn", "!", "ho\u00b7le", "A\u00b7ma\u00b7ran\u00b7tens", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "$.", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Die er oft um Mitternacht ersann;", "tokens": ["Die", "er", "oft", "um", "Mit\u00b7ter\u00b7nacht", "er\u00b7sann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Geh und gib ihm seine Briefe wieder;", "tokens": ["Geh", "und", "gib", "ihm", "sei\u00b7ne", "Brie\u00b7fe", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVIMP", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Der sie schrieb war ein beth\u00f6rter Mann;", "tokens": ["Der", "sie", "schrieb", "war", "ein", "be\u00b7th\u00f6r\u00b7ter", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "Und ein jeder Tropfen, der hernieder", "tokens": ["Und", "ein", "je\u00b7der", "Trop\u00b7fen", ",", "der", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "PIAT", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Auf das Lob von einer Falschen rann,", "tokens": ["Auf", "das", "Lob", "von", "ei\u00b7ner", "Fal\u00b7schen", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.39": {"text": "Brenne nun in schlummerloser Nacht", "tokens": ["Bren\u00b7ne", "nun", "in", "schlum\u00b7mer\u00b7lo\u00b7ser", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "In dem Auge, das der Treue lacht!", "tokens": ["In", "dem", "Au\u00b7ge", ",", "das", "der", "Treu\u00b7e", "lacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.41": {"text": "O! ihr K\u00fcsse! die sie meinen Wangen,", "tokens": ["O", "!", "ihr", "K\u00fcs\u00b7se", "!", "die", "sie", "mei\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "$.", "PRELS", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "In der Rebenlaube aufgedr\u00fcckt,", "tokens": ["In", "der", "Re\u00b7ben\u00b7lau\u00b7be", "auf\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.43": {"text": "Werdet so viel Bisse falscher Schlangen,", "tokens": ["Wer\u00b7det", "so", "viel", "Bis\u00b7se", "fal\u00b7scher", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "Wenn sie in dem Garten Veilchen pfl\u00fcckt!", "tokens": ["Wenn", "sie", "in", "dem", "Gar\u00b7ten", "Veil\u00b7chen", "pfl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.45": {"text": "O! du Druck der Hand! womit vergangen", "tokens": ["O", "!", "du", "Druck", "der", "Hand", "!", "wo\u00b7mit", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "PPER", "NN", "ART", "NN", "$.", "PWAV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "Sie mich noch zum letzten mal ber\u00fcckt,", "tokens": ["Sie", "mich", "noch", "zum", "letz\u00b7ten", "mal", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "APPRART", "ADJA", "ADV", "VVPP", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.47": {"text": "Presse doch ihr schwurverge\u00dfnes Herz,", "tokens": ["Pres\u00b7se", "doch", "ihr", "schwur\u00b7ver\u00b7ge\u00df\u00b7nes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.48": {"text": "Wenn die Reu' erwacht, mit Folter-Schmerz!", "tokens": ["Wenn", "die", "Reu'", "er\u00b7wacht", ",", "mit", "Fol\u00b7ter\u00b7Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.49": {"text": "Und ihr Tropfen Schwei\u00df, die mir entfielen,", "tokens": ["Und", "ihr", "Trop\u00b7fen", "Schwei\u00df", ",", "die", "mir", "ent\u00b7fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.50": {"text": "Wenn ich zu ihr eilt' in fremder Tracht,", "tokens": ["Wenn", "ich", "zu", "ihr", "eilt'", "in", "frem\u00b7der", "Tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.51": {"text": "Drohende Gefahren mir zu Spielen,", "tokens": ["Dro\u00b7hen\u00b7de", "Ge\u00b7fah\u00b7ren", "mir", "zu", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "APPR", "NN", "$,"], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.52": {"text": "Nacht zum Tage, und den Tag zur Nacht,", "tokens": ["Nacht", "zum", "Ta\u00b7ge", ",", "und", "den", "Tag", "zur", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.53": {"text": "Oder, in dem hohen Schnee zu w\u00fchlen,", "tokens": ["O\u00b7der", ",", "in", "dem", "ho\u00b7hen", "Schnee", "zu", "w\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.54": {"text": "Zum Vergn\u00fcgen, (ach! f\u00fcr wen?) gemacht:", "tokens": ["Zum", "Ver\u00b7gn\u00fc\u00b7gen", ",", "(", "ach", "!", "f\u00fcr", "wen", "?", ")", "ge\u00b7macht", ":"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "$(", "ITJ", "$.", "APPR", "PWS", "$.", "$(", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.55": {"text": "O ihr Tropfen! badet feuerhei\u00df,", "tokens": ["O", "ihr", "Trop\u00b7fen", "!", "ba\u00b7det", "feu\u00b7er\u00b7hei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.56": {"text": "Ihre Stirn' dereinst im Todesschwei\u00df'!", "tokens": ["Ih\u00b7re", "Stirn'", "de\u00b7reinst", "im", "To\u00b7des\u00b7schwei\u00df'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.57": {"text": "Schrecklich macht sie dieser Lieb' ein Ende,", "tokens": ["Schreck\u00b7lich", "macht", "sie", "die\u00b7ser", "Lieb'", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.58": {"text": "Welcher keine gleich an Freude zwar,", "tokens": ["Wel\u00b7cher", "kei\u00b7ne", "gleich", "an", "Freu\u00b7de", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PIAT", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.59": {"text": "Aber auch, von einer Sonnenwende", "tokens": ["A\u00b7ber", "auch", ",", "von", "ei\u00b7ner", "Son\u00b7nen\u00b7wen\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.60": {"text": "Bis zur andern, gleich an Untreu' war.", "tokens": ["Bis", "zur", "an\u00b7dern", ",", "gleich", "an", "Un\u00b7treu'", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "$,", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.61": {"text": "Doch, was ist das? Himmel! ich verschwende", "tokens": ["Doch", ",", "was", "ist", "das", "?", "Him\u00b7mel", "!", "ich", "ver\u00b7schwen\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "$,", "PWS", "VAFIN", "PDS", "$.", "NN", "$.", "PPER", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.62": {"text": "Diese Thr\u00e4nen, Nante! noch sogar?", "tokens": ["Die\u00b7se", "Thr\u00e4\u00b7nen", ",", "Nan\u00b7te", "!", "noch", "so\u00b7gar", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NE", "$.", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.63": {"text": "Strafe dich der Himmel nicht daf\u00fcr!", "tokens": ["Stra\u00b7fe", "dich", "der", "Him\u00b7mel", "nicht", "da\u00b7f\u00fcr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKNEG", "PAV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.64": {"text": "Ja! dein eigen Herz vergebe dir!", "tokens": ["Ja", "!", "dein", "ei\u00b7gen", "Herz", "ver\u00b7ge\u00b7be", "dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}