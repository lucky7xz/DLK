{"textgrid.poem.25549": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "An Nantchen, als er erfuhr, da\u00df sie ihre Hand an einen Andern \u00fcberlassen wolle", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ha! nun kenn' ich endlich deine T\u00fccke!", "tokens": ["Ha", "!", "nun", "kenn'", "ich", "end\u00b7lich", "dei\u00b7ne", "T\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O du Falsche! o du Buhlerin!", "tokens": ["O", "du", "Fal\u00b7sche", "!", "o", "du", "Buh\u00b7le\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$.", "FM", "PPER", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sieh! zerrissen hab' ich deine Stricke!", "tokens": ["Sieh", "!", "zer\u00b7ris\u00b7sen", "hab'", "ich", "dei\u00b7ne", "Stri\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dich verlieren, ist f\u00fcr mich Gewinn!", "tokens": ["Dich", "ver\u00b7lie\u00b7ren", ",", "ist", "f\u00fcr", "mich", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "VAFIN", "APPR", "PPER", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Schande! Schande! da\u00df durch deine Blicke", "tokens": ["Schan\u00b7de", "!", "Schan\u00b7de", "!", "da\u00df", "durch", "dei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Jemals ich bezaubert worden bin!", "tokens": ["Je\u00b7mals", "ich", "be\u00b7zau\u00b7bert", "wor\u00b7den", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAPP", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aber, welches Aug' auf Erden ist", "tokens": ["A\u00b7ber", ",", "wel\u00b7ches", "Aug'", "auf", "Er\u00b7den", "ist"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAT", "NN", "APPR", "NN", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Scharf genug f\u00fcr deine Schlangenlist?", "tokens": ["Scharf", "ge\u00b7nug", "f\u00fcr", "dei\u00b7ne", "Schlan\u00b7gen\u00b7list", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Hast du nicht bei Mondlicht manche Stunde", "tokens": ["Hast", "du", "nicht", "bei", "Mond\u00b7licht", "man\u00b7che", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "NN", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Ach! so gern an meiner Brust geruht?", "tokens": ["Ach", "!", "so", "gern", "an", "mei\u00b7ner", "Brust", "ge\u00b7ruht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Weggek\u00fc\u00dft mit deinem Feuermunde", "tokens": ["Weg\u00b7ge\u00b7k\u00fc\u00dft", "mit", "dei\u00b7nem", "Feu\u00b7er\u00b7mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Meiner Augen milde Thr\u00e4nenfluth?", "tokens": ["Mei\u00b7ner", "Au\u00b7gen", "mil\u00b7de", "Thr\u00e4\u00b7nen\u00b7fluth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Und verlie\u00df, im Riesenh\u00f6hlengrunde,", "tokens": ["Und", "ver\u00b7lie\u00df", ",", "im", "Rie\u00b7sen\u00b7h\u00f6h\u00b7len\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Deine Tugend nicht zuerst der Muth?", "tokens": ["Dei\u00b7ne", "Tu\u00b7gend", "nicht", "zu\u00b7erst", "der", "Muth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Aber ich, du wei\u00dft es wohl, ich rang", "tokens": ["A\u00b7ber", "ich", ",", "du", "wei\u00dft", "es", "wohl", ",", "ich", "rang"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Mit der Wollust, bis ich sie bezwang.", "tokens": ["Mit", "der", "Wol\u00b7lust", ",", "bis", "ich", "sie", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Und warum die\u00df Ringen? Sieh! gestehen", "tokens": ["Und", "wa\u00b7rum", "die\u00df", "Rin\u00b7gen", "?", "Sieh", "!", "ge\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PWAV", "PDS", "NN", "$.", "NE", "$.", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Will ich's dir, aus Tugend rang ich nicht!", "tokens": ["Will", "ich's", "dir", ",", "aus", "Tu\u00b7gend", "rang", "ich", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "$,", "APPR", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Zwar ich h\u00f6re willig auf ihr Flehen,", "tokens": ["Zwar", "ich", "h\u00f6\u00b7re", "wil\u00b7lig", "auf", "ihr", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Aber in ", "tokens": ["A\u00b7ber", "in"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Nur, dich in voraus schon weinen sehen,", "tokens": ["Nur", ",", "dich", "in", "vo\u00b7raus", "schon", "wei\u00b7nen", "se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRF", "APPR", "ADV", "ADV", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "War f\u00fcr mich, mehr als das Weltgericht.", "tokens": ["War", "f\u00fcr", "mich", ",", "mehr", "als", "das", "Welt\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "$,", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Lieber mir den Tod, als dich betr\u00fcbt:", "tokens": ["Lie\u00b7ber", "mir", "den", "Tod", ",", "als", "dich", "be\u00b7tr\u00fcbt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Sage, wer hat z\u00e4rtlicher geliebt?", "tokens": ["Sa\u00b7ge", ",", "wer", "hat", "z\u00e4rt\u00b7li\u00b7cher", "ge\u00b7liebt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Rede nun! wo bleiben deine Schw\u00fcre?", "tokens": ["Re\u00b7de", "nun", "!", "wo", "blei\u00b7ben", "dei\u00b7ne", "Schw\u00fc\u00b7re", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Schwurst du, sichrer zu betr\u00fcgen, sie?", "tokens": ["Schwurst", "du", ",", "sich\u00b7rer", "zu", "be\u00b7tr\u00fc\u00b7gen", ",", "sie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VVINF", "$,", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Nicht genug, da\u00df ich dein Herz verliere,", "tokens": ["Nicht", "ge\u00b7nug", ",", "da\u00df", "ich", "dein", "Herz", "ver\u00b7lie\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.28": {"text": "Sondern ", "tokens": ["Son\u00b7dern"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Gibst du dich nicht einem wilden Thiere?", "tokens": ["Gibst", "du", "dich", "nicht", "ei\u00b7nem", "wil\u00b7den", "Thie\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.30": {"text": "Denn was ist der sonst, der seine Knie", "tokens": ["Denn", "was", "ist", "der", "sonst", ",", "der", "sei\u00b7ne", "Knie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VAFIN", "ART", "ADV", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Mit Geschenken kriechend vor dir biegt,", "tokens": ["Mit", "Ge\u00b7schen\u00b7ken", "krie\u00b7chend", "vor", "dir", "biegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.32": {"text": "Und, mit Brunst im Auge, Liebe l\u00fcgt?", "tokens": ["Und", ",", "mit", "Brunst", "im", "Au\u00b7ge", ",", "Lie\u00b7be", "l\u00fcgt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "APPRART", "NN", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Geh denn! hole Amarantens Lieder,", "tokens": ["Geh", "denn", "!", "ho\u00b7le", "A\u00b7ma\u00b7ran\u00b7tens", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "$.", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "Die er oft um Mitternacht ersann;", "tokens": ["Die", "er", "oft", "um", "Mit\u00b7ter\u00b7nacht", "er\u00b7sann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.35": {"text": "Geh und gib ihm seine Briefe wieder;", "tokens": ["Geh", "und", "gib", "ihm", "sei\u00b7ne", "Brie\u00b7fe", "wie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVIMP", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.36": {"text": "Der sie schrieb war ein beth\u00f6rter Mann;", "tokens": ["Der", "sie", "schrieb", "war", "ein", "be\u00b7th\u00f6r\u00b7ter", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.37": {"text": "Und ein jeder Tropfen, der hernieder", "tokens": ["Und", "ein", "je\u00b7der", "Trop\u00b7fen", ",", "der", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "PIAT", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Auf das Lob von einer Falschen rann,", "tokens": ["Auf", "das", "Lob", "von", "ei\u00b7ner", "Fal\u00b7schen", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.39": {"text": "Brenne nun in schlummerloser Nacht", "tokens": ["Bren\u00b7ne", "nun", "in", "schlum\u00b7mer\u00b7lo\u00b7ser", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "In dem Auge, das der Treue lacht!", "tokens": ["In", "dem", "Au\u00b7ge", ",", "das", "der", "Treu\u00b7e", "lacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.41": {"text": "O! ihr K\u00fcsse! die sie meinen Wangen,", "tokens": ["O", "!", "ihr", "K\u00fcs\u00b7se", "!", "die", "sie", "mei\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "$.", "PRELS", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "In der Rebenlaube aufgedr\u00fcckt,", "tokens": ["In", "der", "Re\u00b7ben\u00b7lau\u00b7be", "auf\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.43": {"text": "Werdet so viel Bisse falscher Schlangen,", "tokens": ["Wer\u00b7det", "so", "viel", "Bis\u00b7se", "fal\u00b7scher", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.44": {"text": "Wenn sie in dem Garten Veilchen pfl\u00fcckt!", "tokens": ["Wenn", "sie", "in", "dem", "Gar\u00b7ten", "Veil\u00b7chen", "pfl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.45": {"text": "O! du Druck der Hand! womit vergangen", "tokens": ["O", "!", "du", "Druck", "der", "Hand", "!", "wo\u00b7mit", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "PPER", "NN", "ART", "NN", "$.", "PWAV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.46": {"text": "Sie mich noch zum letzten mal ber\u00fcckt,", "tokens": ["Sie", "mich", "noch", "zum", "letz\u00b7ten", "mal", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "APPRART", "ADJA", "ADV", "VVPP", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.47": {"text": "Presse doch ihr schwurverge\u00dfnes Herz,", "tokens": ["Pres\u00b7se", "doch", "ihr", "schwur\u00b7ver\u00b7ge\u00df\u00b7nes", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.48": {"text": "Wenn die Reu' erwacht, mit Folter-Schmerz!", "tokens": ["Wenn", "die", "Reu'", "er\u00b7wacht", ",", "mit", "Fol\u00b7ter\u00b7Schmerz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.49": {"text": "Und ihr Tropfen Schwei\u00df, die mir entfielen,", "tokens": ["Und", "ihr", "Trop\u00b7fen", "Schwei\u00df", ",", "die", "mir", "ent\u00b7fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.50": {"text": "Wenn ich zu ihr eilt' in fremder Tracht,", "tokens": ["Wenn", "ich", "zu", "ihr", "eilt'", "in", "frem\u00b7der", "Tracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.51": {"text": "Drohende Gefahren mir zu Spielen,", "tokens": ["Dro\u00b7hen\u00b7de", "Ge\u00b7fah\u00b7ren", "mir", "zu", "Spie\