{"textgrid.poem.39633": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die ver\u00e4nderten Zeiten", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du zogst den Heeren nach durch manche L\u00e4nder,", "tokens": ["Du", "zogst", "den", "Hee\u00b7ren", "nach", "durch", "man\u00b7che", "L\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Deutschlands gro\u00dfem Kampf, als Autor-Marketender.", "tokens": ["In", "Deutschlands", "gro\u00b7\u00dfem", "Kampf", ",", "als", "Au\u00b7tor\u00b7Mar\u00b7ke\u00b7ten\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,", "KOUS", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Im Felde nimmt man's nicht genau:", "tokens": ["Im", "Fel\u00b7de", "nimmt", "man's", "nicht", "ge\u00b7nau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein patriot'scher Schnaps, zwar kratzend in der Kehle,", "tokens": ["Dein", "pa\u00b7tri\u00b7ot'\u00b7scher", "Schnaps", ",", "zwar", "krat\u00b7zend", "in", "der", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Fand damals Absatz doch bei mancher durst'gen Seele.", "tokens": ["Fand", "da\u00b7m