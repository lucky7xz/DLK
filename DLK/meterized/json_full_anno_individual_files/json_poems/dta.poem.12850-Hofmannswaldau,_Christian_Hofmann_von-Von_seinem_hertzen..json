{"dta.poem.12850": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von seinem hertzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Cupido schlug mein hertz Arminden in die hand.", "tokens": ["Cu\u00b7pi\u00b7do", "schlug", "mein", "hertz", "Ar\u00b7min\u00b7den", "in", "die", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach! rieff ich: Holdes Kind! verwahr dis zarte pfand,", "tokens": ["Ach", "!", "rieff", "ich", ":", "Hol\u00b7des", "Kind", "!", "ver\u00b7wahr", "dis", "zar\u00b7te", "pfand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$.", "ADJA", "NN", "$.", "VVFIN", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und la\u00df es weiter nicht ergimmte schl\u00e4ge f\u00fchlen:", "tokens": ["Und", "la\u00df", "es", "wei\u00b7ter", "nicht", "er\u00b7gimm\u00b7te", "schl\u00e4\u00b7ge", "f\u00fch\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man mu\u00df mit hertzen nicht, wie mit dem halle spielen.", "tokens": ["Man", "mu\u00df", "mit", "hert\u00b7zen", "nicht", ",", "wie", "mit", "dem", "hal\u00b7le", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos"