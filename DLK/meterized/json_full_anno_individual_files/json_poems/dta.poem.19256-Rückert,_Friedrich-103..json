{"dta.poem.19256": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "103.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh an den Edelstein, wie fest in sich geschlossen,", "tokens": ["Sieh", "an", "den", "E\u00b7del\u00b7stein", ",", "wie", "fest", "in", "sich", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "PWAV", "ADJD", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie undurchdringlich, ganz aus Einem St\u00fcck gegossen!", "tokens": ["Wie", "un\u00b7durch\u00b7dring\u00b7lich", ",", "ganz", "aus", "Ei\u00b7nem", "St\u00fcck", "ge\u00b7gos\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von fremdem Einflu\u00df doch erwehret er sich nicht,", "tokens": ["Von", "frem\u00b7dem", "Ein\u00b7flu\u00df", "doch", "er\u00b7weh\u00b7ret", "er", "sich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den undurchdringlichen durchdringet W\u00e4rm' und Licht.", "tokens": ["Den", "un\u00b7durch\u00b7dring\u00b7li\u00b7chen", "durch\u00b7drin\u00b7get", "W\u00e4rm'", "und", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und seine Farbe selbst, die er hat eingesogen", "tokens": ["Und", "sei\u00b7ne", "Far\u00b7be", "selbst", ",", "die", "er", "hat", "ein\u00b7ge\u00b7so\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$,", "PRELS", "PPER", "VAFIN", "VVPP"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit seiner Art, ist doch von Wechsel angeflogen.", "tokens": ["Mit", "sei\u00b7ner", "Art", ",", "ist", "doch", "von", "Wech\u00b7sel", "an\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bald blitzt er feuriger, wie er bald matter schmachtet,", "tokens": ["Bald", "blitzt", "er", "feu\u00b7ri\u00b7ger", ",", "wie", "er", "bald", "mat\u00b7ter", "schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schillert anders, wie man anders ihn betrachtet.", "tokens": ["Und", "schil\u00b7lert", "an\u00b7ders", ",", "wie", "man", "an\u00b7ders", "ihn", "be\u00b7trach\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWAV", "PIS", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein leichtes W\u00f6lkchen, das in seiner Helle schwimmt,", "tokens": ["Ein", "leich\u00b7tes", "W\u00f6lk\u00b7chen", ",", "das", "in", "sei\u00b7ner", "Hel\u00b7le", "schwimmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ver\u00e4ndert selbst den Platz, da\u00df es dich Wunder nimmt.", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "selbst", "den", "Platz", ",", "da\u00df", "es", "dich", "Wun\u00b7der", "nimmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nicht Wunder nehm' es dich, doch eine Lehre nim", "tokens": ["Nicht", "Wun\u00b7der", "nehm'", "es", "dich", ",", "doch", "ei\u00b7ne", "Leh\u00b7re", "nim"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "VVFIN", "PPER", "PRF", "$,", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom Edelstein, wenn du an Veste gleichest ihm.", "tokens": ["Vom", "E\u00b7del\u00b7stein", ",", "wenn", "du", "an", "Ves\u00b7te", "glei\u00b7chest", "ihm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Es kann kein Herz so starr sich in sich selber schlie\u00dfen,", "tokens": ["Es", "kann", "kein", "Herz", "so", "starr", "sich", "in", "sich", "sel\u00b7ber", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "ADJD", "PRF", "APPR", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nicht ein Mitgef\u00fchl der Welt wird doch durchflie\u00dfen.", "tokens": ["Das", "nicht", "ein", "Mit\u00b7ge\u00b7f\u00fchl", "der", "Welt", "wird", "doch", "durch\u00b7flie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Bist du so hart wie er, sei auch wie er so rein,", "tokens": ["Bist", "du", "so", "hart", "wie", "er", ",", "sei", "auch", "wie", "er", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "$,", "VAFIN", "ADV", "KOKOM", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und schm\u00fccke Gottes Welt nur auch als Edelstein.", "tokens": ["Und", "schm\u00fc\u00b7cke", "Got\u00b7tes", "Welt", "nur", "auch", "als", "E\u00b7del\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NN", "ADV", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}