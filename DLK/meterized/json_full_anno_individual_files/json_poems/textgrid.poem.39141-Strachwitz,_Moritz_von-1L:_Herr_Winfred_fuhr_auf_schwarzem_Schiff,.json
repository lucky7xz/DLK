{"textgrid.poem.39141": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr Winfred fuhr auf schwarzem Schiff,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Winfred fuhr auf schwarzem Schiff,", "tokens": ["Herr", "Win\u00b7fred", "fuhr", "auf", "schwar\u00b7zem", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wollte fahren nach Islands Riff,", "tokens": ["Er", "woll\u00b7te", "fah\u00b7ren", "nach", "Is\u00b7lands", "Riff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "NE", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er wollte holen die Braut zur See,", "tokens": ["Er", "woll\u00b7te", "ho\u00b7len", "die", "Braut", "zur", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das bracht' ihm gr\u00e4\u00dfliches Todesweh;", "tokens": ["Das", "bracht'", "ihm", "gr\u00e4\u00df\u00b7li\u00b7ches", "To\u00b7des\u00b7weh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hoch schlagen die Wogen am Borde.", "tokens": ["Hoch", "schla\u00b7gen", "die", "Wo\u00b7gen", "am", "Bor\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Herr Winfred hoch am Maste stand,", "tokens": ["Herr", "Win\u00b7fred", "hoch", "am", "Mas\u00b7te", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er trug ein funkelndes Stahlgewand,", "tokens": ["Er", "trug", "ein", "fun\u00b7keln\u00b7des", "Stahl\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das blitzte hinunter und strahlt' und glimmt';", "tokens": ["Das", "blitz\u00b7te", "hin\u00b7un\u00b7ter", "und", "strahlt'", "und", "glimmt'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Nixe auf brausender Welle schwimmt;", "tokens": ["Die", "Ni\u00b7xe", "auf", "brau\u00b7sen\u00b7der", "Wel\u00b7le", "schwimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Hoch schlagen die Wogen am Borde.", "tokens": ["Hoch", "schla\u00b7gen", "die", "Wo\u00b7gen", "am", "Bor\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Herr Winfred, komm in mein Schl\u00f6\u00dflein blau!", "tokens": ["Herr", "Win\u00b7fred", ",", "komm", "in", "mein", "Schl\u00f6\u00df\u00b7lein", "blau", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich will dich letzen mit Perlentau;", "tokens": ["Ich", "will", "dich", "let\u00b7zen", "mit", "Per\u00b7len\u00b7tau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "APPR", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du hast einen Helm von Golde klar,", "tokens": ["Du", "hast", "ei\u00b7nen", "Helm", "von", "Gol\u00b7de", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Viel goldner flutet dein Lockenhaar;", "tokens": ["Viel", "gold\u00b7ner", "flu\u00b7tet", "dein", "Lo\u00b7cken\u00b7haar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hoch schlagen die Wogen am Borde.", "tokens": ["Hoch", "schla\u00b7gen", "die", "Wo\u00b7gen", "am", "Bor\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Herr Winfred sprach: Du falsches Bild!", "tokens": ["Herr", "Win\u00b7fred", "sprach", ":", "Du", "fal\u00b7sches", "Bild", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mag nicht tauchen ins Meergefild,", "tokens": ["Ich", "mag", "nicht", "tau\u00b7chen", "ins", "Meer\u00b7ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du hast einen Leib halb Maid, halb Fisch,", "tokens": ["Du", "hast", "ei\u00b7nen", "Leib", "halb", "Maid", ",", "halb", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "NN", "$,", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und wohnst im kochenden Strudelgezisch;", "tokens": ["Und", "wohnst", "im", "ko\u00b7chen\u00b7den", "Stru\u00b7del\u00b7ge\u00b7zisch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hoch schlagen die Wogen am Borde.", "tokens": ["Hoch", "schla\u00b7gen", "die", "Wo\u00b7gen", "am", "Bor\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Da wurde die Fey zur Wog' in Hast", "tokens": ["Da", "wur\u00b7de", "die", "Fey", "zur", "Wo\u00b7g'", "in", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "APPR", "NE"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und leckte hinauf am schwarzen Mast,", "tokens": ["Und", "leck\u00b7te", "hin\u00b7auf", "am", "schwar\u00b7zen", "Mast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wollt' lecken hinab den Ritter gut;", "tokens": ["Wollt'", "le\u00b7cken", "hin\u00b7ab", "den", "Rit\u00b7ter", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der stand und lachte im trotzigen Mut;", "tokens": ["Der", "stand", "und", "lach\u00b7te", "im", "trot\u00b7zi\u00b7gen", "Mut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hoch schlagen die Wogen am Borde.", "tokens": ["Hoch", "schla\u00b7gen", "die", "Wo\u00b7gen", "am", "Bor\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Da wurde die Fey ein grimmer Nord,", "tokens": ["Da", "wur\u00b7de", "die", "Fey", "ein", "grim\u00b7mer", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schlug br\u00fcllend an Bug und Steuerbord,", "tokens": ["Schlug", "br\u00fcl\u00b7lend", "an", "Bug", "und", "Steu\u00b7er\u00b7bord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sie schlug den Mast in St\u00fccke drei;", "tokens": ["Sie", "schlug", "den", "Mast", "in", "St\u00fc\u00b7cke", "drei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herr Winfred stand und lachte dabei;", "tokens": ["Herr", "Win\u00b7fred", "stand", "und", "lach\u00b7te", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Hoch schlagen die Wogen am Borde.", "tokens": ["Hoch", "schla\u00b7gen", "die", "Wo\u00b7gen", "am", "Bor\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Da wurde zum Fische die sch\u00f6ne Fey", "tokens": ["Da", "wur\u00b7de", "zum", "Fi\u00b7sche", "die", "sch\u00f6\u00b7ne", "Fey"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und schwamm an dem Schiffe und war ein Hai,", "tokens": ["Und", "schwamm", "an", "dem", "Schif\u00b7fe", "und", "war", "ein", "Hai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie sah wohl hinauf mit dem Aug' voll Wut,", "tokens": ["Sie", "sah", "wohl", "hin\u00b7auf", "mit", "dem", "Aug'", "voll", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-----+-+", "measure": "dactylic.init"}, "line.4": {"text": "Herrn Winfred gerann sein Herzensblut;", "tokens": ["Herrn", "Win\u00b7fred", "ge\u00b7rann", "sein", "Her\u00b7zens\u00b7blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hoch schlagen die Wogen am Borde.", "tokens": ["Hoch", "schla\u00b7gen", "die", "Wo\u00b7gen", "am", "Bor\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Und er schwang den Speer um das Haupt im Flug,", "tokens": ["Und", "er", "schwang", "den", "Speer", "um", "das", "Haupt", "im", "Flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und er scho\u00df ihn im Zorn durch des Tieres Bug,", "tokens": ["Und", "er", "scho\u00df", "ihn", "im", "Zorn", "durch", "des", "Tie\u00b7res", "Bug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und als es zuckt' in des Todes Qual,", "tokens": ["Und", "als", "es", "zuckt'", "in", "des", "To\u00b7des", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da sah es hinauf zum letzten Mal;", "tokens": ["Da", "sah", "es", "hin\u00b7auf", "zum", "letz\u00b7ten", "Mal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV