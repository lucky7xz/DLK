{"textgrid.poem.58754": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "2. Unbegreiflich", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenne dich, mein Gott, doch nein,", "tokens": ["Ich", "ken\u00b7ne", "dich", ",", "mein", "Gott", ",", "doch", "nein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du kennst das, was du bist, allein.", "tokens": ["Du", "kennst", "das", ",", "was", "du", "bist", ",", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PWS", "PPER", "VAFIN", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wollt' in deinen Tempel treten,", "tokens": ["Ich", "wollt'", "in", "dei\u00b7nen", "Tem\u00b7pel", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch bleib' ich schon beim Eingang stumm,", "tokens": ["Doch", "bleib'", "ich", "schon", "beim", "Ein\u00b7gang", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sink' gar vorm dunkeln Heiligtum,", "tokens": ["Sink'", "gar", "vorm", "dun\u00b7keln", "Hei\u00b7lig\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will nichts mehr sehn, nur tief anbeten:", "tokens": ["Will", "nichts", "mehr", "sehn", ",", "nur", "tief", "an\u00b7be\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVINF", "$,", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dir ziemt die Unbegreiflichkeit,", "tokens": ["Dir", "ziemt", "die", "Un\u00b7be\u00b7greif\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O heil'ge, s\u00fc\u00dfe Dunkelheit,", "tokens": ["O", "heil'\u00b7ge", ",", "s\u00fc\u00b7\u00dfe", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Halleluja! Halleluja!", "tokens": ["Hal\u00b7le\u00b7lu\u00b7ja", "!", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ich kenne dich, mein Gott, doch nein,", "tokens": ["Ich", "ken\u00b7ne", "dich", ",", "mein", "Gott", ",", "doch", "nein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du kennst das, was du bist, allein.", "tokens": ["Du", "kennst", "das", ",", "was", "du", "bist", ",", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "$,", "PWS", "PPER", "VAFIN", "$