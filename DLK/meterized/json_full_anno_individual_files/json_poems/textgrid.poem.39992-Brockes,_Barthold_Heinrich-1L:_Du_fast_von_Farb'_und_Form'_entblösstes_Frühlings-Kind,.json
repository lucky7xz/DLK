{"textgrid.poem.39992": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du fast von Farb' und Form' entbl\u00f6sstes Fr\u00fchlings-Kind,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du fast von Farb' und Form' entbl\u00f6sstes Fr\u00fchlings-Kind,", "tokens": ["Du", "fast", "von", "Fa\u00b7rb'", "und", "Form'", "ent\u00b7bl\u00f6ss\u00b7tes", "Fr\u00fch\u00b7lings\u00b7Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "KON", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "An welchem ich nichts, als ein falbes Grau,", "tokens": ["An", "wel\u00b7chem", "ich", "nichts", ",", "als", "ein", "fal\u00b7bes", "Grau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIS", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ein schmutzig gr\u00fcnlichs Braun, ohn' allen Zierrath, schau,", "tokens": ["Ein", "schmut\u00b7zig", "gr\u00fcn\u00b7lichs", "Braun", ",", "ohn'", "al\u00b7len", "Zier\u00b7rath", ",", "schau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du unansehnliche Muscaten-Hyacinth!", "tokens": ["Du", "un\u00b7an\u00b7sehn\u00b7li\u00b7che", "Mus\u00b7ca\u00b7ten\u00b7Hya\u00b7cinth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Du siehst, im bunten Bluhmen-Reich,", "tokens": ["Du", "siehst", ",", "im", "bun\u00b7ten", "Bluh\u00b7men\u00b7Reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kaum einer Bluhme gleich,", "tokens": ["Kaum", "ei\u00b7ner", "Bluh\u00b7me", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und dennoch bricht aus dir", "tokens": ["Und", "den\u00b7noch", "bricht", "aus", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein recht balsamischer Geruch herf\u00fcr,", "tokens": ["Ein", "recht", "bal\u00b7sa\u00b7mi\u00b7scher", "Ge\u00b7ruch", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Der dem Ceylonischen Gew\u00fcrtze fast nicht weicht,", "tokens": ["Der", "dem", "Cey\u00b7lo\u00b7ni\u00b7schen", "Ge\u00b7w\u00fcrt\u00b7ze", "fast", "nicht", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Und holdem Ambra selbst an s\u00fcsser St\u00e4rcke gleicht.", "tokens": ["Und", "hol\u00b7dem", "A\u00b7mbra", "selbst", "an", "s\u00fcs\u00b7ser", "St\u00e4r\u00b7cke", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du dienest mir, zu ", "tokens": ["Du", "die\u00b7nest", "mir", ",", "zu"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Zum unumst\u00f6\u00dflichen Beweise", "tokens": ["Zum", "un\u00b7um\u00b7st\u00f6\u00df\u00b7li\u00b7chen", "Be\u00b7wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Der nicht zu z\u00e4hlenden Ver\u00e4nd'rung der Figuren", "tokens": ["Der", "nicht", "zu", "z\u00e4h\u00b7len\u00b7den", "Ver\u00b7\u00e4n\u00b7d'\u00b7rung", "der", "Fi\u00b7gu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "PTKZU", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "In seinen sch\u00f6nen Creaturen,", "tokens": ["In", "sei\u00b7nen", "sch\u00f6\u00b7nen", "Crea\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Und die\u00df vermehrt des Sch\u00f6pfers Ehre.", "tokens": ["Und", "die\u00df", "ver\u00b7mehrt", "des", "Sch\u00f6p\u00b7fers", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im weltlichen giebst du mir diese Lehre:", "tokens": ["Im", "welt\u00b7li\u00b7chen", "giebst", "du", "mir", "die\u00b7se", "Leh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "PPER", "PPER", "PDAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri