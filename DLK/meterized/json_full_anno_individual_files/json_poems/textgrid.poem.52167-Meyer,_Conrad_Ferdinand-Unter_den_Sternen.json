{"textgrid.poem.52167": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Unter den Sternen", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer in der Sonne k\u00e4mpft, ein Sohn der Erde,", "tokens": ["Wer", "in", "der", "Son\u00b7ne", "k\u00e4mpft", ",", "ein", "Sohn", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und feurig gei\u00dfelt das Gespann der Pferde,", "tokens": ["Und", "feu\u00b7rig", "gei\u00b7\u00dfelt", "das", "Ge\u00b7spann", "der", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer br\u00fcnstig ringt nach eines Zieles Ferne,", "tokens": ["Wer", "br\u00fcns\u00b7tig", "ringt", "nach", "ei\u00b7nes", "Zie\u00b7les", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von Staub umw\u00f6lkt \u2013 wie glaubte der die Sterne?", "tokens": ["Von", "Staub", "um\u00b7w\u00f6lkt", "\u2013", "wie", "glaub\u00b7te", "der", "die", "Ster\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "PWAV", "VVFIN", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch das Gespann erlahmt, die Pfade dunkeln,", "tokens": [