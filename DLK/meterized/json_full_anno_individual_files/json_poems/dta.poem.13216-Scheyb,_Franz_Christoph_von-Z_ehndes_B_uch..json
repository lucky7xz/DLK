{"dta.poem.13216": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gemach vernahmen wir den K\u00f6niglichen Willen,", "tokens": ["Ge\u00b7mach", "ver\u00b7nah\u00b7men", "wir", "den", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun mit Zufriedenheit den holden Streit zu stillen.", "tokens": ["Nun", "mit", "Zu\u00b7frie\u00b7den\u00b7heit", "den", "hol\u00b7den", "Streit", "zu", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Daher begaben sich die Tugenden zum Krei\u00df,", "tokens": ["Da\u00b7her", "be\u00b7ga\u00b7ben", "sich", "die", "Tu\u00b7gen\u00b7den", "zum", "Krei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Begn\u00fcgten sich in Ruh mit dem erhaltnen Prei\u00df;", "tokens": ["Be\u00b7gn\u00fcg\u00b7ten", "sich", "in", "Ruh", "mit", "dem", "er\u00b7halt\u00b7nen", "Prei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Indem ", "tokens": ["In\u00b7dem"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "400Mit Freuden-vollem Blick die Rede fortgef\u00fchret:", "tokens": ["Freu\u00b7den\u00b7vol\u00b7lem", "Blick", "die", "Re\u00b7de", "fort\u00b7ge\u00b7f\u00fch\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}