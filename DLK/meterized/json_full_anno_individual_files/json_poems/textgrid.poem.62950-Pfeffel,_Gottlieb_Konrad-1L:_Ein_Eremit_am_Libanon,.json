{"textgrid.poem.62950": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Eremit am Libanon,", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Eremit am Libanon,", "tokens": ["Ein", "E\u00b7re\u00b7mit", "am", "Li\u00b7ba\u00b7non", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den man als einen Heilgen ehrte,", "tokens": ["Den", "man", "als", "ei\u00b7nen", "Heil\u00b7gen", "ehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und welchen Gott zum \u00f6ftern schon", "tokens": ["Und", "wel\u00b7chen", "Gott", "zum", "\u00f6f\u00b7tern", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "NN", "APPRART", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch himmlische Gesichte lehrte,", "tokens": ["Durch", "himm\u00b7li\u00b7sche", "Ge\u00b7sich\u00b7te", "lehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Lag flehend einst vor seinem Thron.", "tokens": ["Lag", "fle\u00b7hend", "einst", "vor", "sei\u00b7nem", "Thron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da nahte sich in stiller Feyer", "tokens": ["Da", "nah\u00b7te", "sich", "in", "stil\u00b7ler", "Fe\u00b7yer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Eloah, F\u00fcrst der Seraphim,", "tokens": ["E\u00b7loah", ",", "F\u00fcrst", "der", "Se\u00b7ra\u00b7phim", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ber\u00fchrt sein Aug und spricht zu ihm:", "tokens": ["Be\u00b7r\u00fchrt", "sein", "Aug", "und", "spricht", "zu", "ihm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sieh jenes Weib im Nonnenschleyer", "tokens": ["Sieh", "je\u00b7nes", "Weib", "im", "Non\u00b7nen\u00b7schle\u00b7yer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PDAT", "NN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Und schwarzen, h\u00e4rnen Bu\u00dftalar;", "tokens": ["Und", "schwar\u00b7zen", ",", "h\u00e4r\u00b7nen", "Bu\u00df\u00b7ta\u00b7lar", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie kniet am ernsten S\u00fchnaltar", "tokens": ["Sie", "kniet", "am", "erns\u00b7ten", "S\u00fch\u00b7nal\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und ein Gebet des Isaiden", "tokens": ["Und", "ein", "Ge\u00b7bet", "des", "I\u00b7sai\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.13": {"tex