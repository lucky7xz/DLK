{"dta.poem.11301": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen alchymisten.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der alte Pantalon sucht stets der weisen stein,", "tokens": ["Der", "al\u00b7te", "Pan\u00b7ta\u00b7lon", "sucht", "stets", "der", "wei\u00b7sen", "stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und will vor aller welt ein haupt-gelehrter seyn.", "tokens": ["Und", "will", "vor", "al\u00b7ler", "welt", "ein", "haup\u00b7tge\u00b7lehr\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIAT", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein wo gleich und gleich sich auch bey ihm verbinden,", "tokens": ["Al\u00b7lein", "wo", "gleich", "und", "gleich", "sich", "auch", "bey", "ihm", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ADV", "KON", "ADV", "PRF", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wird man den narren-stein in seinem kopffe finden.", "tokens": ["Wird", "man", "den", "nar\u00b7ren\u00b7stein", "in", "sei\u00b7nem", "kopf\u00b7fe", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}