{"dta.poem.16129": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "183.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein altes Sprichwort sagt: Es h\u00e4ngt sich an den Frevel", "tokens": ["Ein", "al\u00b7tes", "Sprich\u00b7wort", "sagt", ":", "Es", "h\u00e4ngt", "sich", "an", "den", "Fre\u00b7vel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Strafe so geschwind, wie Feuer an den Schwefel.", "tokens": ["Die", "Stra\u00b7fe", "so", "ge\u00b7schwind", ",", "wie", "Feu\u00b7er", "an", "den", "Schwe\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Schwefel brennt, sobald ihm kommt ein Fl\u00e4mmchen nahe,", "tokens": ["Der", "Schwe\u00b7fel", "brennt", ",", "so\u00b7bald", "ihm", "kommt", "ein", "Fl\u00e4mm\u00b7chen", "na\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Frevel zittert stets, da\u00df er den Lohn empfahe.", "tokens": ["Und", "Fre\u00b7vel", "zit\u00b7tert", "stets", ",", "da\u00df", "er", "den", "Lohn", "em\u00b7pfa\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word