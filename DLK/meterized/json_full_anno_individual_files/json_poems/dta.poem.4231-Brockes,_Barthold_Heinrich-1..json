{"dta.poem.4231": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott Lob! die schnellen Blitze schwinden,", "tokens": ["Gott", "Lob", "!", "die", "schnel\u00b7len", "Blit\u00b7ze", "schwin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Donner rollt und knallt nicht mehr,", "tokens": ["Der", "Don\u00b7ner", "rollt", "und", "knallt", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wolken sind von wilden Winden,", "tokens": ["Die", "Wol\u00b7ken", "sind", "von", "wil\u00b7den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wie von schwehren Schlossen, leer,", "tokens": ["So", "wie", "von", "schweh\u00b7ren", "Schlos\u00b7sen", ",", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Luft-Kreis hat sich aufgekl\u00e4rt,", "tokens": ["Der", "Luft\u00b7Kreis", "hat", "sich", "auf\u00b7ge\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Ungewitter aufgeh\u00f6rt.", "tokens": ["Das", "Un\u00b7ge\u00b7wit\u00b7ter", "auf\u00b7ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie stark und heftig es gest\u00fcrmet,", "tokens": ["Wie", "stark", "und", "hef\u00b7tig", "es", "ge\u00b7st\u00fcr\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sehr der Wetter Grimm gekracht;", "tokens": ["Wie", "sehr", "der", "Wet\u00b7ter", "Grimm", "ge\u00b7kracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sind wir doch gn\u00e4diglich beschirmet", "tokens": ["Sind", "wir", "doch", "gn\u00e4\u00b7di\u00b7glich", "be\u00b7schir\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch GOttes weise Lieb\u2019 und Macht.", "tokens": ["Durch", "Got\u00b7tes", "wei\u00b7se", "Lieb'", "und", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat es gleich noch so sehr geblitzt,", "tokens": ["Hat", "es", "gleich", "noch", "so", "sehr", "ge\u00b7blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat Seine Huld uns doch besch\u00fctzt.", "tokens": ["Hat", "Sei\u00b7ne", "Huld", "uns", "doch", "be\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer sah im aufgeri\u00dfnen Himmel,", "tokens": ["Wer", "sah", "im", "auf\u00b7ge\u00b7ri\u00df\u00b7nen", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In einer allgemeinen Gluht,", "tokens": ["In", "ei\u00b7ner", "all\u00b7ge\u00b7mei\u00b7nen", "Gluht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey einem schmetternden Get\u00fcmmel,", "tokens": ["Bey", "ei\u00b7nem", "schmet\u00b7tern\u00b7den", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald Schwefel-blau, bald Roht wie Blut,", "tokens": ["Bald", "Schwe\u00b7fel\u00b7blau", ",", "bald", "Roht", "wie", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So manchen noch viel hellern Strahl,", "tokens": ["So", "man\u00b7chen", "noch", "viel", "hel\u00b7lern", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schnelle Blitze sonder Zahl.", "tokens": ["Und", "schnel\u00b7le", "Blit\u00b7ze", "son\u00b7der", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der nicht, im h\u00f6chst gerechten Schrecken,", "tokens": ["Der", "nicht", ",", "im", "h\u00f6chst", "ge\u00b7rech\u00b7ten", "Schre\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "$,", "APPRART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Elementen Kr\u00e4ft\u2019 empfand,", "tokens": ["Der", "E\u00b7le\u00b7men\u00b7ten", "Kr\u00e4ft'", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und, da sie sich so weit erstrecken,", "tokens": ["Und", ",", "da", "sie", "sich", "so", "weit", "er\u00b7stre\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht gleich sein armes Nichts erkannt;", "tokens": ["Nicht", "gleich", "sein", "ar\u00b7mes", "Nichts", "er\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der nicht, da ihm sein Fall so nah,", "tokens": ["Der", "nicht", ",", "da", "ihm", "sein", "Fall", "so", "nah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf seines Sch\u00f6pfers Gnade sah!", "tokens": ["Auf", "sei\u00b7nes", "Sch\u00f6p\u00b7fers", "Gna\u00b7de", "sah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dem, da so Luft als Erde sch\u00fcttert,", "tokens": ["Dem", ",", "da", "so", "Luft", "als", "Er\u00b7de", "sch\u00fct\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "ADV", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch der gepre\u00dften Wolken Grimm,", "tokens": ["Durch", "der", "ge\u00b7pre\u00df\u00b7ten", "Wol\u00b7ken", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht auch mit Recht sein Herz gezittert", "tokens": ["Nicht", "auch", "mit", "Recht", "sein", "Herz", "ge\u00b7zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Dem, Der in des Donners Stimm\u2019", "tokens": ["Vor", "Dem", ",", "Der", "in", "des", "Don\u00b7ners", "Stimm'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "ART", "APPR", "ART", "NN", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Solch einen furchtbarn Ton gepr\u00e4gt,", "tokens": ["Solch", "ei\u00b7nen", "furcht\u00b7barn", "Ton", "ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df jedes Herz f\u00fcr Schrecken schl\u00e4gt.", "tokens": ["Da\u00df", "je\u00b7des", "Herz", "f\u00fcr", "Schre\u00b7cken", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da aber itzt das schwehre Wetter", "tokens": ["Da", "a\u00b7ber", "itzt", "das", "schweh\u00b7re", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verzog, Blitz, Knall und Sturm vorbey;", "tokens": ["Ver\u00b7zog", ",", "Blitz", ",", "Knall", "und", "Sturm", "vor\u00b7bey", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So \u00fcberlegt, wer der Erretter", "tokens": ["So", "\u00fc\u00b7ber\u00b7legt", ",", "wer", "der", "Er\u00b7ret\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "PWS", "ART", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "In dieser Noht gewesen sey?", "tokens": ["In", "die\u00b7ser", "Noht", "ge\u00b7we\u00b7sen", "sey", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ohn\u2019 Den ihr arm, verbrannt, versehrt,", "tokens": ["Ohn'", "Den", "ihr", "arm", ",", "ver\u00b7brannt", ",", "ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "PPER", "ADJD", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gel\u00e4hmt, ja gar zerschmettert, w\u00e4rt.", "tokens": ["Ge\u00b7l\u00e4hmt", ",", "ja", "gar", "zer\u00b7schmet\u00b7tert", ",", "w\u00e4rt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ADV", "VVPP", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Unendlichs All! durch Dessen Willen", "tokens": ["Un\u00b7end\u00b7lichs", "All", "!", "durch", "Des\u00b7sen", "Wil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesch\u00f6pf\u2019 entstehen und vergehn,", "tokens": ["Ge\u00b7sch\u00f6pf'", "ent\u00b7ste\u00b7hen", "und", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Den allein die Donner br\u00fcllen,", "tokens": ["Durch", "Den", "al\u00b7lein", "die", "Don\u00b7ner", "br\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Den das Licht der Sonnen sch\u00f6n,", "tokens": ["Durch", "Den", "das", "Licht", "der", "Son\u00b7nen", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist blo\u00df Deine Gnad\u2019 allein,", "tokens": ["Es", "ist", "blo\u00df", "Dei\u00b7ne", "Gnad'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df wir erhalten worden seyn.", "tokens": ["Da\u00df", "wir", "er\u00b7hal\u00b7ten", "wor\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da\u00df unser Guht kein Blitz versehret,", "tokens": ["Da\u00df", "un\u00b7ser", "Guht", "kein", "Blitz", "ver\u00b7seh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Strahl durch unsre Glieder fuhr,", "tokens": ["Kein", "Strahl", "durch", "uns\u00b7re", "Glie\u00b7der", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ward blo\u00df allein durch Dich gewehret,", "tokens": ["Ward", "blo\u00df", "al\u00b7lein", "durch", "Dich", "ge\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Quell und Erhalter der Natur!", "tokens": ["Quell", "und", "Er\u00b7hal\u00b7ter", "der", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Dein Gnaden- reicher Wille wollt,", "tokens": ["Dein", "Gna\u00b7den", "rei\u00b7cher", "Wil\u00b7le", "wollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "ADJD", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df uns kein Unfall treffen sollt.", "tokens": ["Da\u00df", "uns", "kein", "Un\u00b7fall", "tref\u00b7fen", "sollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wie Du nun, HErr, durch die\u00df Erhalten,", "tokens": ["Wie", "Du", "nun", ",", "Herr", ",", "durch", "die\u00df", "Er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$,", "NN", "$,", "APPR", "PDS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Unsrig\u2019 uns aufs neu geschenkt;", "tokens": ["Das", "Uns\u00b7rig'", "uns", "aufs", "neu", "ge\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "ADJD", "VVPP", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Verdient die\u00df v\u00e4terliche Walten", "tokens": ["Ver\u00b7di\u00b7ent", "die\u00df", "v\u00e4\u00b7ter\u00b7li\u00b7che", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "PDS", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Denn nicht, da\u00df man, ger\u00fchret, denkt:", "tokens": ["Denn", "nicht", ",", "da\u00df", "man", ",", "ge\u00b7r\u00fch\u00b7ret", ",", "denkt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "KOUS", "PIS", "$,", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u201cerbarmung-voller GOtt, nur Dir", "tokens": ["\u201c", "er\u00b7bar\u00b7mung\u00b7vol\u00b7ler", "Gott", ",", "nur", "Dir"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJA", "NN", "$,", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\