.5": {"text": "Seh' ich sie, schnell verjaget", "tokens": ["Seh'", "ich", "sie", ",", "schnell", "ver\u00b7ja\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Harm die reinste Lust,", "tokens": ["Den", "Harm", "die", "reins\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Engelwonne taget,", "tokens": ["Und", "En\u00b7gel\u00b7won\u00b7ne", "ta\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In meiner trunknen Brust.", "tokens": ["In", "mei\u00b7ner", "trunk\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Von ihres L\u00e4chelns Schallen", "tokens": ["Von", "ih\u00b7res", "L\u00e4\u00b7chelns", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bebt jede Nerve mir;", "tokens": ["Bebt", "je\u00b7de", "Ner\u00b7ve", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von ihres Fu\u00dftritts Hallen", "tokens": ["Von", "ih\u00b7res", "Fu\u00df\u00b7tritts", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwillt Brust und Seele mir.", "tokens": ["Schwillt", "Brust", "und", "See\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist von meinen Kr\u00e4ften", "tokens": ["Sie", "ist", "von", "mei\u00b7nen", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Richtung, sie allein", "tokens": ["Die", "Rich\u00b7tung", ",", "sie", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist's bei des Tag's Gesch\u00e4ften", "tokens": ["Ist's", "bei", "des", "Tag's", "Ge\u00b7sch\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und sp\u00e4t bei Lampenschein.", "tokens": ["Und", "sp\u00e4t", "bei", "Lam\u00b7pen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich seh' ihr Bild am Tage,", "tokens": ["Ich", "seh'", "ihr", "Bild", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich seh's in dunkler Nacht;", "tokens": ["Ich", "seh's", "in", "dunk\u00b7ler", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei jedem Seigerschlage", "tokens": ["Bei", "je\u00b7dem", "Sei\u00b7ger\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verfolgt mich seine Pracht.", "tokens": ["Ver\u00b7folgt", "mich", "sei\u00b7ne", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im tiefsten, festen Schlummer", "tokens": ["Im", "tiefs\u00b7ten", ",", "fes\u00b7ten", "Schlum\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Seh' ich's so klar vor mir,", "tokens": ["Seh'", "ich's", "so", "klar", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und rufe ihr im Schlummer,", "tokens": ["Und", "ru\u00b7fe", "ihr", "im", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und streck' den Arm nach ihr.", "tokens": ["Und", "streck'", "den", "Arm", "nach", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ist's S\u00fcnd', in Lieb' entbrennen \u2013", "tokens": ["Ist's", "S\u00fcnd'", ",", "in", "Lieb'", "ent\u00b7bren\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weh' dir, mein s\u00fcndig Blut!", "tokens": ["Weh'", "dir", ",", "mein", "s\u00fcn\u00b7dig", "Blut", "!"], "token_info": ["word", "word", "punct", "word", "word