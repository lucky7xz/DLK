{"textgrid.poem.36128": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich hab' das M\u00e4dchen funden,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' das M\u00e4dchen funden,", "tokens": ["Ich", "hab'", "das", "M\u00e4d\u00b7chen", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das sich mein Herz erkor,", "tokens": ["Das", "sich", "mein", "Herz", "er\u00b7kor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jede dieser Stunden", "tokens": ["Und", "je\u00b7de", "die\u00b7ser", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kommt mir gefl\u00fcgelt vor.", "tokens": ["Kommt", "mir", "ge\u00b7fl\u00fc\u00b7gelt", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Aug' hat sie gesehen,", "tokens": ["Mein", "Aug'", "hat", "sie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Herz ist ihr ergl\u00fcht,", "tokens": ["Mein", "Herz", "ist", "ihr", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kaum mag ich ihr's gestehen,", "tokens": ["Kaum", "mag", "ich", "ih\u00b7r's", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Obgleich ihr Aug' es sieht.", "tokens": ["Ob\u00b7gleich", "ihr", "Aug'", "es", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kann ihr nicht enteilen,", "tokens": ["Ich", "kann", "ihr", "nicht", "ent\u00b7ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich mu\u00df ihr nahe seyn,", "tokens": ["Ich", "mu\u00df", "ihr", "na\u00b7he", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df auf der St\u00e4tte weilen,", "tokens": ["Mu\u00df", "auf", "der", "St\u00e4t\u00b7te", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ihre F\u00fc\u00dfe weihn.", "tokens": ["Die", "ih\u00b7re", "F\u00fc\u00b7\u00dfe", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich mu\u00df das L\u00fcftchen trinken,", "tokens": ["Ich", "mu\u00df", "das", "L\u00fcft\u00b7chen", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das die Geliebte trank,", "tokens": ["Das", "die", "Ge\u00b7lieb\u00b7te", "trank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mu\u00df jeden Raum durchdringen,", "tokens": ["Mu\u00df", "je\u00b7den", "Raum", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch den die Heil'ge drang.", "tokens": ["Durch", "den", "die", "Heil'\u00b7ge", "drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Seh' ich sie nicht, wie sehnet,", "tokens": ["Seh'", "ich", "sie", "nicht", ",", "wie", "seh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie schrei't mein Herz nach ihr,", "tokens": ["Wie", "schrei't", "mein", "Herz", "nach", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein dunkles Auge thr\u00e4net,", "tokens": ["Mein", "dunk\u00b7les", "Au\u00b7ge", "thr\u00e4\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz zerschmilzt Begier;", "tokens": ["Mein", "Herz", "zer\u00b7schmilzt", "Be\u00b7gier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Seh' ich sie, schnell verjaget", "tokens": ["Seh'", "ich", "sie", ",", "schnell", "ver\u00b7ja\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Harm die reinste Lust,", "tokens": ["Den", "Harm", "die", "reins\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Engelwonne taget,", "tokens": ["Und", "En\u00b7gel\u00b7won\u00b7ne", "ta\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In meiner trunknen Brust.", "tokens": ["In", "mei\u00b7ner", "trunk\u00b7nen", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von ihres L\u00e4chelns Schallen", "tokens": ["Von", "ih\u00b7res", "L\u00e4\u00b7chelns", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bebt jede Nerve mir;", "tokens": ["Bebt", "je\u00b7de", "Ner\u00b7ve", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von ihres Fu\u00dftritts Hallen", "tokens": ["Von", "ih\u00b7res", "Fu\u00df\u00b7tritts", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwillt Brust und Seele mir.", "tokens": ["Schwillt", "Brust", "und", "See\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist von meinen Kr\u00e4ften", "tokens": ["Sie", "ist", "von", "mei\u00b7nen", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Richtung, sie allein", "tokens": ["Die", "Rich\u00b7tung", ",", "sie", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist's bei des Tag's Gesch\u00e4ften", "tokens": ["Ist's", "bei", "des", "Tag's", "Ge\u00b7sch\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und sp\u00e4t bei Lampenschein.", "tokens": ["Und", "sp\u00e4t", "bei", "Lam\u00b7pen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich seh' ihr Bild am Tage,", "tokens": ["Ich", "seh'", "ihr", "Bild", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich seh's in dunkler Nacht;", "tokens": ["Ich", "seh's", "in", "dunk\u00b7ler", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei jedem Seigerschlage", "tokens": ["Bei", "je\u00b7dem", "Sei\u00b7ger\u00b7schla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verfolgt mich seine Pracht.", "tokens": ["Ver\u00b7folgt", "mich", "sei\u00b7ne", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im tiefsten, festen Schlummer", "tokens": ["Im", "tiefs\u00b7ten", ",", "fes\u00b7ten", "Schlum\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Seh' ich's so klar vor mir,", "tokens": ["Seh'", "ich's", "so", "klar", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und rufe ihr im Schlummer,", "tokens": ["Und", "ru\u00b7fe", "ihr", "im", "Schlum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und streck' den Arm nach ihr.", "tokens": ["Und", "streck'", "den", "Arm", "nach", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ist's S\u00fcnd', in Lieb' entbrennen \u2013", "tokens": ["Ist's", "S\u00fcnd'", ",", "in", "Lieb'", "ent\u00b7bren\u00b7nen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weh' dir, mein s\u00fcndig Blut!", "tokens": ["Weh'", "dir", ",", "mein", "s\u00fcn\u00b7dig", "Blut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir z\u00fcckt in Mark und Sehnen", "tokens": ["Mir", "z\u00fcckt", "in", "Mark", "und", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebe wilde Gluth.", "tokens": ["Der", "Lie\u00b7be", "wil\u00b7de", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist mir Lust und Wehe,", "tokens": ["Sie", "ist", "mir", "Lust", "und", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist Tod und Eden mir.", "tokens": ["Ist", "Tod", "und", "E\u00b7den", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich f\u00fchl, ich h\u00f6r', und sehe,", "tokens": ["Ich", "f\u00fchl", ",", "ich", "h\u00f6r'", ",", "und", "se\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und leb' und web' in ihr.", "tokens": ["Und", "leb'", "und", "web'", "in", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So ist die Ruh' verloren,", "tokens": ["So", "ist", "die", "Ruh'", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die vormals mich erfreu't;", "tokens": ["Die", "vor\u00b7mals", "mich", "er\u00b7freu't", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch, da\u00df ich sie verloren,", "tokens": ["Doch", ",", "da\u00df", "ich", "sie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat mich noch nie gereu't;", "tokens": ["Hat", "mich", "noch", "nie", "ge\u00b7reu't", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir kommt von meinen Wunden", "tokens": ["Mir", "kommt", "von", "mei\u00b7nen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die tiefste heilsam vor \u2013", "tokens": ["Die", "tiefs\u00b7te", "heil\u00b7sam", "vor", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hat sie in mir ihn funden,", "tokens": ["Hat", "sie", "in", "mir", "ihn", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den sich mein Herz erkor!", "tokens": ["Den", "sich", "mein", "Herz", "er\u00b7kor", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich hab' das M\u00e4dchen funden,", "tokens": ["Ich", "hab'", "das", "M\u00e4d\u00b7chen", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das sich mein Herz erkor,", "tokens": ["Das", "sich", "mein", "Herz", "er\u00b7kor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jede dieser Stunden", "tokens": ["Und", "je\u00b7de", "die\u00b7ser", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kommt mir gefl\u00fcgelt vor.", "tokens": ["Kommt", "mir", "ge\u00b7fl\u00fc\u00b7gelt", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Aug' hat sie gesehen,", "tokens": ["Mein", "Aug'", "hat", "sie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Herz ist ihr ergl\u00fcht,", "tokens": ["Mein", "Herz", "ist", "ihr", "er\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kaum mag ich ihr's gestehen,", "tokens": ["Kaum", "mag", "ich", "ih\u00b7r's", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Obgleich ihr Aug' es sieht.", "tokens": ["Ob\u00b7gleich", "ihr", "Aug'", "es", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ich kann ihr nicht enteilen,", "tokens": ["Ich", "kann", "ihr", "nicht", "ent\u00b7ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich mu\u00df ihr nahe seyn,", "tokens": ["Ich", "mu\u00df", "ihr", "na\u00b7he", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df auf der St\u00e4tte weilen,", "tokens": ["Mu\u00df", "auf", "der", "St\u00e4t\u00b7te", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ihre F\u00fc\u00dfe weihn.", "tokens": ["Die", "ih\u00b7re", "F\u00fc\u00b7\u00dfe", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich mu\u00df das L\u00fcftchen trinken,", "tokens": ["Ich", "mu\u00df", "das", "L\u00fcft\u00b7chen", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das die G