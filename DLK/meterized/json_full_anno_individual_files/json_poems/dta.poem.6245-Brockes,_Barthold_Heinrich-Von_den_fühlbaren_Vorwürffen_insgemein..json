{"dta.poem.6245": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den f\u00fchlbaren Vorw\u00fcrffen  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Regel, hat man sie recht innen,", "tokens": ["Die", "Re\u00b7gel", ",", "hat", "man", "sie", "recht", "in\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "PIS", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erstrecket sich auf alle Sinnen.", "tokens": ["Er\u00b7stre\u00b7cket", "sich", "auf", "al\u00b7le", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es zeigt sich der Geschmack, durch ein beweglich Zittern", "tokens": ["Es", "zeigt", "sich", "der", "Ge\u00b7schmack", ",", "durch", "ein", "be\u00b7weg\u00b7lich", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Nervgen, draus die Zung, als wie ein Netz, gewebt.", "tokens": ["Der", "Nerv\u00b7gen", ",", "draus", "die", "Zung", ",", "als", "wie", "ein", "Netz", ",", "ge\u00b7webt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PAV", "ART", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie der Geruch entsteht, wenn sich ein Dunst erhebt,", "tokens": ["Wie", "der", "Ge\u00b7ruch", "ent\u00b7steht", ",", "wenn", "sich", "ein", "Dunst", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und bis zur Nase steigt. Aus einem regen Sch\u00fcttern", "tokens": ["Und", "bis", "zur", "Na\u00b7se", "steigt", ".", "Aus", "ei\u00b7nem", "re\u00b7gen", "Sch\u00fct\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wird allezeit der Ton gebohren;", "tokens": ["Wird", "al\u00b7le\u00b7zeit", "der", "Ton", "ge\u00b7boh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die W\u00fcrckung in der Lufft empfangen unsre Ohren.", "tokens": ["Die", "W\u00fcr\u00b7ckung", "in", "der", "Lufft", "emp\u00b7fan\u00b7gen", "uns\u00b7re", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es ist ein schneller Druck der Nervgen in den Augen,", "tokens": ["Es", "ist", "ein", "schnel\u00b7ler", "Druck", "der", "Nerv\u00b7gen", "in", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wodurch dieselbigen so viel zu sehen taugen.", "tokens": ["Wo\u00b7durch", "die\u00b7sel\u00b7bi\u00b7gen", "so", "viel", "zu", "se\u00b7hen", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von jeder Handlung ist ein Ursprung nur allein", "tokens": ["Von", "je\u00b7der", "Hand\u00b7lung", "ist", "ein", "Ur\u00b7sprung", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "F\u00fcr alle Sinnen insgemein.", "tokens": ["F\u00fcr", "al\u00b7le", "Sin\u00b7nen", "ins\u00b7ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}