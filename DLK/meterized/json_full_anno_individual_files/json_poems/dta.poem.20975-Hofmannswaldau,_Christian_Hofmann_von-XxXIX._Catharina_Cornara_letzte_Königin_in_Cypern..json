{"dta.poem.20975": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIX.  \n Catharina Cornara  letzte K\u00f6nigin  \n in Cypern.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war zu meiner Zeit der ", "tokens": ["Ich", "war", "zu", "mei\u00b7ner", "Zeit", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich herrschte wo vor mir die ", "tokens": ["Ich", "herrschte", "wo", "vor", "mir", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWAV", "APPR", "PPER", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich hat Venedig selbst zur Tochter angenommen/", "tokens": ["Mich", "hat", "Ve\u00b7ne\u00b7dig", "selbst", "zur", "Toch\u00b7ter", "an\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Durch Lieb und Mahlerey bin ich zur Crone kom-", "tokens": ["Durch", "Lieb", "und", "Mah\u00b7le\u00b7rey", "bin", "ich", "zur", "Cro\u00b7ne", "kom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VAFIN", "PPER", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "men.", "tokens": ["men", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}}}}}