{"textgrid.poem.27884": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein fr\u00f6lich Ding um eines Menschen sterben;", "tokens": ["Es", "ist", "ein", "fr\u00f6\u00b7lich", "Ding", "um", "ei\u00b7nes", "Men\u00b7schen", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es freuen sich darauff die gerne-reichen Erben.", "tokens": ["Es", "freu\u00b7en", "sich", "dar\u00b7auff", "die", "ger\u00b7ne\u00b7rei\u00b7chen", "Er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Priester freuen sich, das Opffer zu genissen;", "tokens": ["Die", "Pries\u00b7ter", "freu\u00b7en", "sich", ",", "das", "Opf\u00b7fer", "zu", "ge\u00b7nis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die W\u00fcrme freuen sich an einem guten Bissen;", "tokens": ["Die", "W\u00fcr\u00b7me", "freu\u00b7en", "sich", "an", "ei\u00b7nem", "gu\u00b7ten", "Bis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5"