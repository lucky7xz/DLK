{"dta.poem.10279": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Abseits.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "In einer Riesenstadt durchschritt ich j&#252;ngst             ", "tokens": ["In", "ei\u00b7ner", "Rie\u00b7sen\u00b7stadt", "durch\u00b7schritt", "ich", "j", "&#252;", "ngst"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NE", "$(", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die volkbelebteste der gro\u00dfen Stra\u00dfen.", "tokens": ["Die", "volk\u00b7be\u00b7leb\u00b7tes\u00b7te", "der", "gro\u00b7\u00dfen", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und eine Stille kam, und, wunderbar,", "tokens": ["Und", "ei\u00b7ne", "Stil\u00b7le", "kam", ",", "und", ",", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KON", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In all\u2019 dem Schreien, Fluchen, Sto\u00dfen, Treiben,", "tokens": ["In", "all'", "dem", "Schrei\u00b7en", ",", "Flu\u00b7chen", ",", "Sto\u00b7\u00dfen", ",", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zog klar vor\u00fcber mir ein liebes Bild:", "tokens": ["Zog", "klar", "vor\u00b7\u00fc\u00b7ber", "mir", "ein", "lie\u00b7bes", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ganz wie versteckt in Wald und Feld und Heide,", "tokens": ["Ganz", "wie", "ver\u00b7steckt", "in", "Wald", "und", "Feld", "und", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VVPP", "APPR", "NN", "KON", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von gro\u00dfen und von kleinen St\u00e4dten fern,", "tokens": ["Von", "gro\u00b7\u00dfen", "und", "von", "klei\u00b7nen", "St\u00e4d\u00b7ten", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Liegt unser Haus, vereinsamt und verloren", "tokens": ["Liegt", "un\u00b7ser", "Haus", ",", "ver\u00b7ein\u00b7samt", "und", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVPP", "KON", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "In eines alten Gartens stiller Welt.", "tokens": ["In", "ei\u00b7nes", "al\u00b7ten", "Gar\u00b7tens", "stil\u00b7ler", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Die Sonne schien auf kiesbedeckte Wege,", "tokens": ["Die", "Son\u00b7ne", "schien", "auf", "kies\u00b7be\u00b7deck\u00b7te", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und in den B\u00e4umen war ein Maienleben.", "tokens": ["Und", "in", "den", "B\u00e4u\u00b7men", "war", "ein", "Mai\u00b7en\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Du gingst zur Seite mir, und Hand in Hand,", "tokens": ["Du", "gingst", "zur", "Sei\u00b7te", "mir", ",", "und", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "$,", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So standen endlich wir am lichten Rande", "tokens": ["So", "stan\u00b7den", "end\u00b7lich", "wir", "am", "lich\u00b7ten", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Der kleinen H\u00f6lzung. Vor uns schwieg die Landschaft.", "tokens": ["Der", "klei\u00b7nen", "H\u00f6l\u00b7zung", ".", "Vor", "uns", "schwieg", "die", "Land\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ein L\u00e4uten kam aus unsichtbarer Ferne.", "tokens": ["Ein", "L\u00e4u\u00b7ten", "kam", "aus", "un\u00b7sicht\u00b7ba\u00b7rer", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wie sch\u00f6n es war. Es zogen tiefe Schatten", "tokens": ["Wie", "sch\u00f6n", "es", "war", ".", "Es", "zo\u00b7gen", "tie\u00b7fe", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$.", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Um uns, und fr\u00f6hlich k\u00fc\u00dfte deine Augen", "tokens": ["Um", "uns", ",", "und", "fr\u00f6h\u00b7lich", "k\u00fc\u00df\u00b7te", "dei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "$,", "KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ein frischer Buchenzweig.", "tokens": ["Ein", "fri\u00b7scher", "Bu\u00b7chen\u00b7zweig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als Abends dann noch einmal wir durchschritten", "tokens": ["Als", "A\u00b7bends", "dann", "noch", "ein\u00b7mal", "wir", "durch\u00b7schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "ADV", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Parkes Grund, die Nachtigall zu finden,", "tokens": ["Des", "Par\u00b7kes", "Grund", ",", "die", "Nach\u00b7ti\u00b7gall", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u2014 Du wolltest ja durchaus sie singen ", "tokens": ["Du", "woll\u00b7test", "ja", "durc\u00b7haus", "sie", "sin\u00b7gen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VMFIN", "ADV", "ADV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie lehntest halb erschrocken du den Kopf", "tokens": ["Wie", "lehn\u00b7test", "halb", "er\u00b7schro\u00b7cken", "du", "den", "Kopf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An meine Schulter, als im Dickicht, pl\u00f6tzlich,", "tokens": ["An", "mei\u00b7ne", "Schul\u00b7ter", ",", "als", "im", "Di\u00b7ckicht", ",", "pl\u00f6tz\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Marmorfaun gespenstig auf uns sah.", "tokens": ["Der", "Mar\u00b7mor\u00b7faun", "ge\u00b7spens\u00b7tig", "auf", "uns", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und grade hier mit voller Inbrunst schlug,", "tokens": ["Und", "gra\u00b7de", "hier", "mit", "vol\u00b7ler", "In\u00b7brunst", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In einem kaum erbl\u00fchten Apfelbaum,", "tokens": ["In", "ei\u00b7nem", "kaum", "er\u00b7bl\u00fch\u00b7ten", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Liederk\u00f6nigin. Die sch\u00f6nsten Weisen", "tokens": ["Die", "Lie\u00b7der\u00b7k\u00f6\u00b7ni\u00b7gin", ".", "Die", "sch\u00f6ns\u00b7ten", "Wei\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sang klagend sie dem frechen Gotte vor.", "tokens": ["Sang", "kla\u00b7gend", "sie", "dem", "fre\u00b7chen", "Got\u00b7te", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das Gl\u00fcck, der Schnell\u00e4ufer, hielt Ruhetag", "tokens": ["Das", "Gl\u00fcck", ",", "der", "Schnel\u00b7l\u00e4u\u00b7fer", ",", "hielt", "Ru\u00b7he\u00b7tag"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In unsern Herzen, und es zog der Friede", "tokens": ["In", "un\u00b7sern", "Her\u00b7zen", ",", "und", "es", "zog", "der", "Frie\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Weit \u00fcber\u2019s Land. Hell leuchteten die Sterne,", "tokens": ["Weit", "\u00fc\u00b7ber's", "Land", ".", "Hell", "leuch\u00b7te\u00b7ten", "die", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$.", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Hell \u00fcber uns in stiller Fr\u00fchlingsnacht.", "tokens": ["Hell", "\u00fc\u00b7ber", "uns", "in", "stil\u00b7ler", "Fr\u00fch\u00b7lings\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}