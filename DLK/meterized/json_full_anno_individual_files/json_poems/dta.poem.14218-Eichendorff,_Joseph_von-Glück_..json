{"dta.poem.14218": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Rings Menschen sich drehen", "tokens": ["Rings", "Men\u00b7schen", "sich", "dre\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und sprechen gescheut,", "tokens": ["Und", "spre\u00b7chen", "ge\u00b7scheut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich kann nichts verstehen,", "tokens": ["Ich", "kann", "nichts", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So fr\u00f6hlich zerstreut. \u2014", "tokens": ["So", "fr\u00f6h\u00b7lich", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}