{"textgrid.poem.60514": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Frivolen", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit ist hin, wo vor den Banngewittern", "tokens": ["Die", "Zeit", "ist", "hin", ",", "wo", "vor", "den", "Bann\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Glaubens noch ein Bube mu\u00dfte zittern.", "tokens": ["Des", "Glau\u00b7bens", "noch", "ein", "Bu\u00b7be", "mu\u00df\u00b7te", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dahin sind auch die Tage, wo der Flug", "tokens": ["Da\u00b7hin", "sind", "auch", "die", "Ta\u00b7ge", ",", "wo", "der", "Flug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Meisterkraft die St\u00fcmper niederschlug.", "tokens": ["Der", "Meis\u00b7ter\u00b7kraft", "die", "St\u00fcm\u00b7per", "nie\u00b7der\u00b7schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Geist hat auch sein gutes Recht verloren,", "tokens": ["Der", "Geist", "hat", "auch", "sein", "gu\u00b7tes", "Recht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein altes Machtwort \u00fcbers Volk der Toren.", "tokens": ["Sein", "al\u00b7tes", "Macht\u00b7wort", "\u00fc\u00b7bers", "Volk", "der", "To\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie einen Lappen, aufgeh\u00e4ngt im Winde,", "tokens": ["Wie", "ei\u00b7nen", "Lap\u00b7pen", ",", "auf\u00b7ge\u00b7h\u00e4ngt", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchbohrt kein Kugelschu\u00df auch dies Gesinde.", "tokens": ["Durch\u00b7bohrt", "kein", "Ku\u00b7gel\u00b7schu\u00df", "auch", "dies", "Ge\u00b7sin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie fl\u00fcchten, wenn der Ernst sie je befiel,", "tokens": ["Sie", "fl\u00fcch\u00b7ten", ",", "wenn", "der", "Ernst", "sie", "je", "be\u00b7fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ins Fleisch, in ihr verwesliches Asyl.", "tokens": ["Ins", "Fleisch", ",", "in", "ihr", "ver\u00b7wes\u00b7li\u00b7ches", "A\u00b7syl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "So durch und durch verdorben ist die Bande,", "tokens": ["So", "durch", "und", "durch", "ver\u00b7dor\u00b7ben", "ist", "die", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "KON", "APPR", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sich der Blitz befleckt an ihrer Schande.", "tokens": ["Da\u00df", "sich", "der", "Blitz", "be\u00b7fleckt", "an", "ih\u00b7rer", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der Bube l\u00e4\u00dft aufg\u00e4ren mit Gekreische", "tokens": ["Der", "Bu\u00b7be", "l\u00e4\u00dft", "auf\u00b7g\u00e4\u00b7ren", "mit", "Ge\u00b7krei\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVIZU", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der niedern Leidenschaften tr\u00fcbe Maische;", "tokens": ["Der", "nie\u00b7dern", "Lei\u00b7den\u00b7schaf\u00b7ten", "tr\u00fc\u00b7be", "Mai\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Was als ihr Heiligstes die Menschheit kennt,", "tokens": ["Was", "als", "ihr", "Hei\u00b7ligs\u00b7tes", "die", "Menschheit", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOKOM", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er wirfts in seinen K\u00fcbel als Ferment;", "tokens": ["Er", "wirfts", "in", "sei\u00b7nen", "K\u00fc\u00b7bel", "als", "Fer\u00b7ment", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Wenn er die Blase schaut in seinem Schaume,", "tokens": ["Wenn", "er", "die", "Bla\u00b7se", "schaut", "in", "sei\u00b7nem", "Schau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scheint sie Weltkugel seinem D\u00fcnkeltraume.", "tokens": ["Scheint", "sie", "Welt\u00b7ku\u00b7gel", "sei\u00b7nem", "D\u00fcn\u00b7kel\u00b7trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}}, "stanza.10": {"line.1": {"text": "Die Kunst ist eine derbe Magd geworden,", "tokens": ["Die", "Kunst", "ist", "ei\u00b7ne", "der\u00b7be", "Magd", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verp\u00f6belt in der Frone schlechter Horden.", "tokens": ["Ver\u00b7p\u00f6\u00b7belt", "in", "der", "Fro\u00b7ne", "schlech\u00b7ter", "Hor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Sie schleppt das Holz, da\u00df z\u00fcndend sie bediene", "tokens": ["Sie", "schleppt", "das", "Holz", ",", "da\u00df", "z\u00fcn\u00b7dend", "sie", "be\u00b7die\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "ADJD", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der L\u00fcste lustig prasselnde Kamine.", "tokens": ["Der", "L\u00fcs\u00b7te", "lus\u00b7tig", "pras\u00b7seln\u00b7de", "Ka\u00b7mi\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Sie tr\u00e4gt den Eimer der verflachten Lumpen,", "tokens": ["Sie", "tr\u00e4gt", "den", "Ei\u00b7mer", "der", "ver\u00b7flach\u00b7ten", "Lum\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Beifallstr\u00e4nenflut ihn voll zu pumpen.", "tokens": ["Mit", "Bei\u00b7fall\u00b7str\u00e4\u00b7nen\u00b7flut", "ihn", "voll", "zu", "pum\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Im Stalle waltet sie, den Freudenfesten", "tokens": ["Im", "Stal\u00b7le", "wal\u00b7tet", "sie", ",", "den", "Freu\u00b7den\u00b7fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Taumelnden das Vieh heranzum\u00e4sten.", "tokens": ["Der", "Tau\u00b7meln\u00b7den", "das", "Vieh", "her\u00b7an\u00b7zu\u00b7m\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Sie schreitet ihnen vor, aus ihren Wegen", "tokens": ["Sie", "schrei\u00b7tet", "ih\u00b7nen", "vor", ",", "aus", "ih\u00b7ren", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie d\u00fcrres Laub die Sitte fortzufegen.", "tokens": ["Wie", "d\u00fcr\u00b7res", "Laub", "die", "Sit\u00b7te", "fort\u00b7zu\u00b7fe\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Ich las einmal in einem fr\u00e4nkschen Blatte,", "tokens": ["Ich", "las", "ein\u00b7mal", "in", "ei\u00b7nem", "fr\u00e4nk\u00b7schen", "Blat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df eine Metze einen Liebsten hatte.", "tokens": ["Da\u00df", "ei\u00b7ne", "Met\u00b7ze", "ei\u00b7nen", "Liebs\u00b7ten", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Der Liebste war ein armer, armer Ritter,", "tokens": ["Der", "Liebs\u00b7te", "war", "ein", "ar\u00b7mer", ",", "ar\u00b7mer", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dachlos, brotlos, kleidlos, es dr\u00fcckt' ihn bitter.", "tokens": ["Dach\u00b7los", ",", "brot\u00b7los", ",", "kleid\u00b7los", ",", "es", "dr\u00fcckt'", "ihn", "bit\u00b7ter", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.17": {"line.1": {"text": "Denn, ach! er hatte nicht um sich geschlagen", "tokens": ["Denn", ",", "ach", "!", "er", "hat\u00b7te", "nicht", "um", "sich", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ITJ", "$.", "PPER", "VAFIN", "PTKNEG", "APPR", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Bettlermantel, den die Schwaben tragen,", "tokens": ["Den", "Bett\u00b7ler\u00b7man\u00b7tel", ",", "den", "die", "Schwa\u00b7ben", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Das Notgew\u00e4ndlein, das im Neckartal", "tokens": ["Das", "Not\u00b7ge\u00b7w\u00e4nd\u00b7lein", ",", "das", "im", "Ne\u00b7ckar\u00b7tal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Patria, Religion, Moral,", "tokens": ["Die", "Pat\u00b7ria", ",", "Re\u00b7li\u00b7gi\u00b7on", ",", "Mo\u00b7ral", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.19": {"line.1": {"text": "Drei alte Schneiderjungfern, zubereiten", "tokens": ["Drei", "al\u00b7te", "Schnei\u00b7der\u00b7jung\u00b7fern", ",", "zu\u00b7be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["CARD", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und dort den Bettlern um die H\u00fcfte breiten.", "tokens": ["Und", "dort", "den", "Bett\u00b7lern", "um", "die", "H\u00fcf\u00b7te", "brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.20": {"line.1": {"text": "Schon war der Arme fast in Not verkommen,", "tokens": ["Schon", "war", "der", "Ar\u00b7me", "fast", "in", "Not", "ver\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da hat die Metze sein sich angenommen.", "tokens": ["Da", "hat", "die", "Met\u00b7ze", "sein", "sich", "an\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPOSAT", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.21": {"line.1": {"text": "So manchem J\u00fcngling war die Dirne sch\u00e4dlich,", "tokens": ["So", "man\u00b7chem", "J\u00fcng\u00b7ling", "war", "die", "Dir\u00b7ne", "sch\u00e4d\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur mit dem Einen meinte sie es redlich.", "tokens": ["Nur", "mit", "dem", "Ei\u00b7nen", "mein\u00b7te", "sie", "es", "red\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.22": {"line.1": {"text": "Was mit der S\u00fcnde sie gewann, der feilen,", "tokens": ["Was", "mit", "der", "S\u00fcn\u00b7de", "sie", "ge\u00b7wann", ",", "der", "fei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie bracht es heim, es treu mit ihm zu teilen;", "tokens": ["Sie", "bracht", "es", "heim", ",", "es", "treu", "mit", "ihm", "zu", "tei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "ADJD", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.23": {"line.1": {"text": "Behaglich nahm es an der faule Schuft,", "tokens": ["Be\u00b7hag\u00b7lich", "nahm", "es", "an", "der", "fau\u00b7le", "Schuft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie sie entehrt zueilte ihrer Gruft.", "tokens": ["Wie", "sie", "ent\u00b7ehrt", "zu\u00b7eil\u00b7te", "ih\u00b7rer", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.24": {"line.1": {"text": "Und als ich von der Dirne las die Kunde,", "tokens": ["Und", "als", "ich", "von", "der", "Dir\u00b7ne", "las", "die", "Kun\u00b7de", ","], "token_info