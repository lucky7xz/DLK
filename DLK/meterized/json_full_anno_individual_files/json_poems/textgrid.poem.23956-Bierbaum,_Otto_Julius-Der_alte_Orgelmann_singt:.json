{"textgrid.poem.23956": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Der alte Orgelmann singt:", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst in meinen Jugendjahren", "tokens": ["Einst", "in", "mei\u00b7nen", "Ju\u00b7gend\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich Liebe viel erfahren,", "tokens": ["Hab", "ich", "Lie\u00b7be", "viel", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Bel-Etage sowohl", "tokens": ["In", "der", "Bel\u00b7E\u00b7ta\u00b7ge", "so\u00b7wohl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Sout'rain und Entresol.", "tokens": ["Wie", "Sout'\u00b7rain", "und", "Ent\u00b7re\u00b7sol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Bin ein frecher Fuchs gewesen,", "tokens": ["Bin", "ein", "fre\u00b7cher", "Fuchs", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Machte nicht viel Federlesen,", "tokens": ["Mach\u00b7te", "nicht", "viel", "Fe\u00b7der\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rupfte hier und rupfte da,", "tokens": ["Rupf\u00b7te", "hier", "und", "rupf\u00b7te", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lina, Laura, Lucia.", "tokens": ["Li\u00b7na", ",", "Lau\u00b7ra", ",", "Lu\u00b7cia", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Als Student hat man es leichte,", "tokens": ["Als", "Stu\u00b7dent", "hat", "man", "es", "leich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Denn es hei\u00dft selbst in der Beichte:", "tokens": ["Denn", "es", "hei\u00dft", "selbst", "in", "der", "Beich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Studiosus fecit id?", "tokens": ["Stu\u00b7di\u00b7o\u00b7sus", "fe\u00b7cit", "id", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Macht ein Rosenkr\u00e4nzlein quitt.", "tokens": ["Macht", "ein", "Ro\u00b7sen\u00b7kr\u00e4nz\u00b7lein", "quitt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so lebt man wie die Finken,", "tokens": ["Und", "so", "lebt", "man", "wie", "die", "Fin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcckt auf rauh und glatte Klinken,", "tokens": ["Dr\u00fcckt", "auf", "rauh", "und", "glat\u00b7te", "Klin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "F\u00fchrn sie nur zum K\u00e4mmerlein,", "tokens": ["F\u00fchrn", "sie", "nur", "zum", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die lieben M\u00e4dchen sein.", "tokens": ["Wo", "die", "lie\u00b7ben", "M\u00e4d\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jetzo bin ich alt geworden", "tokens": ["Jet\u00b7zo", "bin", "ich", "alt", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im grauen Katerorden", "tokens": ["Und", "im", "grau\u00b7en", "Ka\u00b7te\u00b7ror\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller\u00e4ltster Senior;", "tokens": ["Al\u00b7le\u00b7r\u00e4lts\u00b7ter", "Se\u00b7ni\u00b7or", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm mir selber putzig vor.", "tokens": ["Komm", "mir", "sel\u00b7ber", "put\u00b7zig", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Von dem ganzen Lie-la-lieben", "tokens": ["Von", "dem", "gan\u00b7zen", "Lie\u00b7la\u00b7lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist kaum ein Gedicht geblieben,", "tokens": ["Ist", "kaum", "ein", "Ge\u00b7dicht", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das erb\u00e4rmlich klagt und klingt", "tokens": ["Das", "er\u00b7b\u00e4rm\u00b7lich", "klagt", "und", "klingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Erinnerungen singt.", "tokens": ["Und", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "singt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Traurig dreh ich meine Walze,", "tokens": ["Trau\u00b7rig", "dreh", "ich", "mei\u00b7ne", "Wal\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die, belaugt vom Thr\u00e4nensalze,", "tokens": ["Die", ",", "be\u00b7laugt", "vom", "Thr\u00e4\u00b7nen\u00b7sal\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00f6rmlich um Erbarmen steht,", "tokens": ["F\u00f6rm\u00b7lich", "um", "Er\u00b7bar\u00b7men", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil es mir so \u00fcbel geht.", "tokens": ["Weil", "es", "mir", "so", "\u00fc\u00b7bel", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00df ich meine Walze rasten,", "tokens": ["La\u00df", "ich", "mei\u00b7ne", "Wal\u00b7ze", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dreht da dr\u00fcben ihren Kasten", "tokens": ["Dreht", "da", "dr\u00fc\u00b7ben", "ih\u00b7ren", "Kas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Laura, einst die sch\u00f6nste Maid,", "tokens": ["Lau\u00b7ra", ",", "einst", "die", "sch\u00f6ns\u00b7te", "Maid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt ein Weib im Lumpenkleid.", "tokens": ["Jetzt", "ein", "Weib", "im", "Lum\u00b7pen\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie auch hat es toll getrieben", "tokens": ["Sie", "auch", "hat", "es", "toll", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem gottverfluchten Lieben,", "tokens": ["Mit", "dem", "gott\u00b7ver\u00b7fluch\u00b7ten", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum, h\u00f6r es, Publikum,", "tokens": ["Da\u00b7rum", ",", "h\u00f6r", "es", ",", "Pub\u00b7li\u00b7kum", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dreht sie das Harmonium.", "tokens": ["Dreht", "sie", "das", "Har\u00b7mo\u00b7ni\u00b7um", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.10": {"line.1": {"text": "Oh ihr netten jungen Leute,", "tokens": ["Oh", "ihr", "net\u00b7ten", "jun\u00b7gen", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["XY", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebt mit Ma\u00dfen und gescheute,", "tokens": ["Liebt", "mit", "Ma\u00b7\u00dfen", "und", "ge\u00b7scheu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ihr, tadellos gesund,", "tokens": ["Bis", "ihr", ",", "ta\u00b7del\u00b7los", "ge\u00b7sund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schlie\u00dfet einen Ehebund.", "tokens": ["Schlie\u00b7\u00dfet", "ei\u00b7nen", "E\u00b7he\u00b7bund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Denn die allerschlimmste Ehe", "tokens": ["Denn", "die", "al\u00b7ler\u00b7schlimms\u00b7te", "E\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thut noch immer nicht so wehe,", "tokens": ["Thut", "noch", "im\u00b7mer", "nicht", "so", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das Leierkastenspiel,", "tokens": ["Wie", "das", "Lei\u00b7er\u00b7kas\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn dies ist kein Lebensziel.", "tokens": ["Denn", "dies", "ist", "kein", "Le\u00b7bens\u00b7ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Kinderzeugen dahingegen", "tokens": ["Kin\u00b7der\u00b7zeu\u00b7gen", "da\u00b7hin\u00b7ge\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht Vergn\u00fcgen und bringt Segen,", "tokens": ["Macht", "Ver\u00b7gn\u00fc\u00b7gen", "und", "bringt", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenns geschieht im Ehebett", "tokens": ["Wenns", "ge\u00b7schieht", "im", "E\u00b7he\u00b7bett"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Standesamtlich und honett.", "tokens": ["Stan\u00b7des\u00b7amt\u00b7lich", "und", "ho\u00b7nett", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Einst in meinen Jugendjahren", "tokens": ["Einst", "in", "mei\u00b7nen", "Ju\u00b7gend\u00b7jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich Liebe viel erfahren,", "tokens": ["Hab", "ich", "Lie\u00b7be", "viel", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Bel-Etage sowohl", "tokens": ["In", "der", "Bel\u00b7E\u00b7ta\u00b7ge", "so\u00b7wohl"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Sout'rain und Entresol.", "tokens": ["Wie", "Sout'\u00b7rain", "und", "Ent\u00b7re\u00b7sol", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Bin ein frecher Fuchs gewesen,", "tokens": ["Bin", "ein", "fre\u00b7cher", "Fuchs", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Machte nicht viel Federlesen,", "tokens": ["Mach\u00b7te", "nicht", "viel", "Fe\u00b7der\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rupfte hier und rupfte da,", "tokens": ["Rupf\u00b7te", "hier", "und", "rupf\u00b7te", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lina, Laura, Lucia.", "tokens": ["Li\u00b7na", ",", "Lau\u00b7ra", ",", "Lu\u00b7cia", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.15": {"line.1": {"text": "Als Student hat man es leichte,", "tokens": ["Als", "Stu\u00b7dent", "hat", "man", "es", "leich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Denn es hei\u00dft selbst in der Beichte:", "tokens": ["Denn", "es", "hei\u00dft", "selbst", "in", "der", "Beich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Studiosus fecit id?", "tokens": ["Stu\u00b7di\u00b7o\u00b7sus", "fe\u00b7cit", "id", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Macht ein Rosenkr\u00e4nzlein quitt.", "tokens": ["Macht", "ein", "Ro\u00b7sen\u00b7kr\u00e4nz\u00b7lein", "quitt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und so lebt man wie die Finken,", "tokens": ["Und", "so", "lebt", "man", "wie", "die", "Fin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dr\u00fcckt auf rauh und glatte Klinken,", "tokens": ["Dr\u00fcckt", "auf", "rauh", "und", "glat\u00b7te", "Klin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "F\u00fchrn sie nur zum K\u00e4mmerlein,", "tokens": ["F\u00fchrn", "sie", "nur", "zum", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die lieben M\u00e4dchen sein.", "tokens": ["Wo", "die", "lie\u00b7ben", "M\u00e4d\u00b7chen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Jetzo bin ich alt geworden", "tokens": ["Jet\u00b7zo", "bin", "ich", "alt", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im grauen Katerorden", "tokens": ["Und", "im", "grau\u00b7en", "Ka\u00b7te\u00b7ror\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller\u00e4ltster Senior;", "tokens": ["Al\u00b7le\u00b7r\u00e4lts\u00b7ter", "Se\u00b7ni\u00b7or", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm mir selber putzig vor.", "tok