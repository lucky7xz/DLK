, "line.3": {"text": "F\u00f6rmlich um Erbarmen steht,", "tokens": ["F\u00f6rm\u00b7lich", "um", "Er\u00b7bar\u00b7men", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil es mir so \u00fcbel geht.", "tokens": ["Weil", "es", "mir", "so", "\u00fc\u00b7bel", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "La\u00df ich meine Walze rasten,", "tokens": ["La\u00df", "ich", "mei\u00b7ne", "Wal\u00b7ze", "ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dreht da dr\u00fcben ihren Kasten", "tokens": ["Dreht", "da", "dr\u00fc\u00b7ben", "ih\u00b7ren", "Kas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Laura, einst die sch\u00f6nste Maid,", "tokens": ["Lau\u00b7ra", ",", "einst", "die", "sch\u00f6ns\u00b7te", "Maid", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt ein Weib im Lumpenkleid.", "tokens": ["Jetzt", "ein", "Weib", "im", "Lum\u00b7pen\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Sie auch hat es toll getrieben", "tokens": ["Sie", "auch", "hat", "es", "toll", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word"