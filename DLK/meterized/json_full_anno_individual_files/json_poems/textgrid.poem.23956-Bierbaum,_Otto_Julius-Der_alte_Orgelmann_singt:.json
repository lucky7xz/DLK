 ["Schlie\u00b7\u00dfet", "ei\u00b7nen", "E\u00b7he\u00b7bund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Denn die allerschlimmste Ehe", "tokens": ["Denn", "die", "al\u00b7ler\u00b7schlimms\u00b7te", "E\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thut noch immer nicht so wehe,", "tokens": ["Thut", "noch", "im\u00b7mer", "nicht", "so", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das Leierkastenspiel,", "tokens": ["Wie", "das", "Lei\u00b7er\u00b7kas\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn dies ist kein Lebensziel.", "tokens": ["Denn", "dies", "ist", "kein", "Le\u00b7bens\u00b7ziel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Kinderzeugen dahingegen", "tokens": ["Kin\u00b7der\u00b7zeu\u00b7gen", "da\u00b7hin\u00b7ge\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Macht Vergn\u00fcgen und bringt Segen,", "tokens": ["Macht", "Ver\u00b7gn\u00fc\u00b7gen", "und", "bringt", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenns geschieht im Ehebett", "tokens": ["Wenns", "ge\u00b7schieht", "im", "E\u00b7he\u00b7bett"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Standesamtlich und honett.", "tokens": ["Stan\u00b7des\u00b7amt\u00b7lich", "und", "ho\u00b7nett", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}