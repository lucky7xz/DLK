{"dta.poem.19091": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "LxXIII.  Auf die selige Gnade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Grosse Gottheit! ich erstaune", "tokens": ["Gros\u00b7se", "Got\u00b7theit", "!", "ich", "er\u00b7stau\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uber deinen Liebes-Rath,", "tokens": ["U\u00b7ber", "dei\u00b7nen", "Lie\u00b7bes\u00b7Rath", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und worzu mich die Posaune", "tokens": ["Und", "wor\u00b7zu", "mich", "die", "Po\u00b7sau\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Deines Reichs geruffen hat.", "tokens": ["Dei\u00b7nes", "Reichs", "ge\u00b7ruf\u00b7fen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hochzeit wird dem grossen Sohne,", "tokens": ["Hoch\u00b7zeit", "wird", "dem", "gros\u00b7sen", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinem K\u00f6nige, gemacht,", "tokens": ["Mei\u00b7nem", "K\u00f6\u00b7ni\u00b7ge", ",", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Sitz in seinem Throne", "tokens": ["Und", "der", "Sitz", "in", "sei\u00b7nem", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist mir Armen zugedacht.", "tokens": ["Ist", "mir", "Ar\u00b7men", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unter denen Engel-Ch\u00f6ren", "tokens": ["Un\u00b7ter", "de\u00b7nen", "En\u00b7gel\u00b7Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PRELS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00f6hrte Vasthi Stoltz das Fest,", "tokens": ["St\u00f6hr\u00b7te", "Vas\u00b7thi", "Stoltz", "das", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bi\u00df du sie mit ihren Heeren", "tokens": ["Bi\u00df", "du", "sie", "mit", "ih\u00b7ren", "Hee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Abgrund schleudertest.", "tokens": ["In", "den", "Ab\u00b7grund", "schleu\u00b7der\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Damit bautest du den Tempel", "tokens": ["Da\u00b7mit", "bau\u00b7test", "du", "den", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Pracht von neuen auf,", "tokens": ["Dei\u00b7ner", "Pracht", "von", "neu\u00b7en", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das neue Liebs-Exempel", "tokens": ["Und", "das", "neu\u00b7e", "Liebs\u00b7Exem\u00b7pel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "Endlich gabst du dich, o Liebe!", "tokens": ["End\u00b7lich", "gabst", "du", "dich", ",", "o", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selber vor die Seelen dar,", "tokens": ["Sel\u00b7ber", "vor", "die", "See\u00b7len", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine tugendliche Triebe", "tokens": ["Dei\u00b7ne", "tu\u00b7gend\u00b7li\u00b7che", "Trie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wurden ihr nun all zu klar.", "tokens": ["Wur\u00b7den", "ihr", "nun", "all", "zu", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jener Herr von Oriente", "tokens": ["Je\u00b7ner", "Herr", "von", "O\u00b7ri\u00b7en\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach: ", "tokens": ["Sprach", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wenn ich dich vergn\u00fcgen k\u00f6nte!", "tokens": ["Wenn", "ich", "dich", "ver\u00b7gn\u00fc\u00b7gen", "k\u00f6n\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Statt der Antwort sanck sie hin.", "tokens": ["Statt", "der", "Ant\u00b7wort", "san\u00b7ck", "sie", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "O du ewiges Gesichte!", "tokens": ["O", "du", "e\u00b7wi\u00b7ges", "Ge\u00b7sich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O du Glantz der Herrlichkeit!", "tokens": ["O", "du", "Glantz", "der", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich versinck vor deinem Lichte,", "tokens": ["Ich", "ver\u00b7sinck", "vor", "dei\u00b7nem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn michs noch so sehr erfreut.", "tokens": ["Wenn", "michs", "noch", "so", "sehr", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.8": {"line.1": {"text": "K\u00fcsse mich, wenns Hertz in Wehmuth,", "tokens": ["K\u00fcs\u00b7se", "mich", ",", "wenns", "Hertz", "in", "Weh\u00b7muth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht mirs gut, so mach mich bl\u00f6d,", "tokens": ["Geht", "mirs", "gut", ",", "so", "mach", "mich", "bl\u00f6d", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So verbleib ich in der Demuth,", "tokens": ["So", "ver\u00b7bleib", "ich", "in", "der", "De\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "O du h\u00f6chste Majest\u00e4t!", "tokens": ["O", "du", "h\u00f6chs\u00b7te", "Ma\u00b7jes\u00b7t\u00e4t", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}