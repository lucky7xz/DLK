{"dta.poem.20354": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr augen flie\u00dft! beweint den nahen tod/", "tokens": ["Ihr", "au\u00b7gen", "flie\u00dft", "!", "be\u00b7weint", "den", "na\u00b7hen", "tod", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Flie\u00dft/ weil noch eure thr\u00e4nen w\u00e4hren/", "tokens": ["Flie\u00dft", "/", "weil", "noch", "eu\u00b7re", "thr\u00e4\u00b7nen", "w\u00e4h\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sparet nicht in meiner letzten noth", "tokens": ["Und", "spa\u00b7ret", "nicht", "in", "mei\u00b7ner", "letz\u00b7ten", "noth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die letzten tropffen heisser z\u00e4hren.", "tokens": ["Die", "letz\u00b7ten", "tropf\u00b7fen", "heis\u00b7ser", "z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr augen flie\u00dft! das \u00fcber grosse weh", "tokens": ["Ihr", "au\u00b7gen", "flie\u00dft", "!", "das", "\u00fc\u00b7ber", "gros\u00b7se", "weh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erfodert eine thr\u00e4nen-see.", "tokens": ["Er\u00b7fo\u00b7dert", "ei\u00b7ne", "thr\u00e4\u00b7nen\u00b7see", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein krancker geist/ der schmertzlich jenesmahl", "tokens": ["Mein", "kran\u00b7cker", "geist", "/", "der", "schmertz\u00b7lich", "je\u00b7nes\u00b7mahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den grimmen liebes-pfeil empfunden/", "tokens": ["Den", "grim\u00b7men", "lie\u00b7bes\u00b7pfeil", "emp\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der stirbt anitzt in unerh\u00f6rter qvaal/", "tokens": ["Der", "stirbt", "a\u00b7nitzt", "in", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "qvaal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erleget durch des todes-wunden.", "tokens": ["Er\u00b7le\u00b7get", "durch", "des", "to\u00b7des\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr augen flie\u00dft! das \u00fcbergrosse weh", "tokens": ["Ihr", "au\u00b7gen", "flie\u00dft", "!", "das", "\u00fc\u00b7berg\u00b7ros\u00b7se", "weh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erfodert eine thr\u00e4nen-see.", "tokens": ["Er\u00b7fo\u00b7dert", "ei\u00b7ne", "thr\u00e4\u00b7nen\u00b7see", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Eh\u2019 diese glut mich gantz zu asche macht/", "tokens": ["Eh'", "die\u00b7se", "glut", "mich", "gantz", "zu", "asc\u00b7he", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "PPER", "ADV", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Eh\u2019 angst und jammer mich aussaugen;", "tokens": ["Eh'", "angst", "und", "jam\u00b7mer", "mich", "aus\u00b7sau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eh\u2019 mich bef\u00e4llt des grossen todes nacht/", "tokens": ["Eh'", "mich", "be\u00b7f\u00e4llt", "des", "gros\u00b7sen", "to\u00b7des", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So weinet noch zuvor/ ihr augen.", "tokens": ["So", "wei\u00b7net", "noch", "zu\u00b7vor", "/", "ihr", "au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$(", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr augen flie\u00dft! das \u00fcber-grosse weh", "tokens": ["Ihr", "au\u00b7gen", "flie\u00dft", "!", "das", "\u00fc\u00b7ber\u00b7gros\u00b7se", "weh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erfodert eine thr\u00e4nen-see.", "tokens": ["Er\u00b7fo\u00b7dert", "ei\u00b7ne", "thr\u00e4\u00b7nen\u00b7see", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch m\u00fcssen es nicht schlechte thr\u00e4nen seyn/", "tokens": ["Doch", "m\u00fcs\u00b7sen", "es", "nicht", "schlech\u00b7te", "thr\u00e4\u00b7nen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die ihr/ ihr augen/ lasset fliessen;", "tokens": ["Die", "ihr", "/", "ihr", "au\u00b7gen", "/", "las\u00b7set", "flies\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "$(", "PPOSAT", "NN", "$(", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr m\u00fcsset euch in dieser todes-pein", "tokens": ["Ihr", "m\u00fcs\u00b7set", "euch", "in", "die\u00b7ser", "to\u00b7des\u00b7pein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In einen strohm von blut ergiessen.", "tokens": ["In", "ei\u00b7nen", "strohm", "von", "blut", "er\u00b7gies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr augen flie\u00dft! das \u00fcber-grosse weh", "tokens": ["Ihr", "au\u00b7gen", "flie\u00df