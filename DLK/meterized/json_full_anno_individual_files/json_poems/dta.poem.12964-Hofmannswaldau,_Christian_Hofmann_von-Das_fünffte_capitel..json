{"dta.poem.12964": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte capitel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer aus der bibel nichts als kluge reden lernen,", "tokens": ["Wer", "aus", "der", "bi\u00b7bel", "nichts", "als", "klu\u00b7ge", "re\u00b7den", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PIS", "KOKOM", "ADJA", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht vielmehr daselbst die wahrheit suchen will,", "tokens": ["Und", "nicht", "viel\u00b7mehr", "da\u00b7selbst", "die", "wahr\u00b7heit", "su\u00b7chen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "PAV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der trifft nicht auf den zweck; er folget falschen sternen", "tokens": ["Der", "trifft", "nicht", "auf", "den", "zweck", ";", "er", "fol\u00b7get", "fal\u00b7schen", "ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "VVINF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und macht des HErren wort zu einem blosen spiel.", "tokens": ["Und", "macht", "des", "Her\u00b7ren", "wort", "zu", "ei\u00b7nem", "blo\u00b7sen", "spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gott lehrt uns in der schrifft nur lauter wahrheit sinden:", "tokens": ["Gott", "lehrt", "uns", "in", "der", "schrifft", "nur", "lau\u00b7ter", "wahr\u00b7heit", "sin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "VVFIN", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wer die bibel nicht in diesem geiste liest,", "tokens": ["Und", "wer", "die", "bi\u00b7bel", "nicht", "in", "die\u00b7sem", "geis\u00b7te", "liest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PTKNEG", "APPR", "PDAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der sie geschrieben hat, der wird nicht viel ergr\u00fcnden,", "tokens": ["Der", "sie", "ge\u00b7schrie\u00b7ben", "hat", ",", "der", "wird", "nicht", "viel", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,", "PRELS", "VAFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dieweil sie der natur ein dunckles r\u00e4tzel ist.", "tokens": ["Die\u00b7weil", "sie", "der", "na\u00b7tur", "ein", "dunck\u00b7les", "r\u00e4t\u00b7zel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drum so versteig dich nicht in spitzigen gedancken,", "tokens": ["Drum", "so", "ver\u00b7steig", "dich", "nicht", "in", "spit\u00b7zi\u00b7gen", "ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und such\u2019 in GOttes wort blos, was die seel erbaut.", "tokens": ["Und", "such'", "in", "Got\u00b7tes", "wort", "blos", ",", "was", "die", "seel", "er\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "ADV", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Ein weises hertze mu\u00df nicht von der einfalt wancken,", "tokens": ["Ein", "wei\u00b7ses", "hert\u00b7ze", "mu\u00df", "nicht", "von", "der", "ein\u00b7falt", "wan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Denn der k\u00f6mmt hier nicht fort, dem schon vor dieser graut.", "tokens": ["Denn", "der", "k\u00f6mmt", "hier", "nicht", "fort", ",", "dem", "schon", "vor", "die\u00b7ser", "graut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$,", "PRELS", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein and acht-reiches buch, das nach der einfalt schmecket,", "tokens": ["Ein", "and", "acht\u00b7rei\u00b7ches", "buch", ",", "das", "nach", "der", "ein\u00b7falt", "schme\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mu\u00df uns so angenehm, als hohe reden seyn.", "tokens": ["Mu\u00df", "uns", "so", "an\u00b7ge\u00b7nehm", ",", "als", "ho\u00b7he", "re\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "ADJA", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn obgleich Seneca voll tiefer spr\u00fcche stecket,", "tokens": ["Denn", "ob\u00b7gleich", "Se\u00b7ne\u00b7ca", "voll", "tie\u00b7fer", "spr\u00fc\u00b7che", "ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NE", "ADJD", "ADJD", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So machen sie dich doch von keiner s\u00fcnde rein.", "tokens": ["So", "ma\u00b7chen", "sie", "dich", "doch", "von", "kei\u00b7ner", "s\u00fcn\u00b7de", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bekommest du ein buch, so lie\u00df es blos aus liebe", "tokens": ["Be\u00b7kom\u00b7mest", "du", "ein", "buch", ",", "so", "lie\u00df", "es", "blos", "aus", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der lautern wahrheit durch, obgleich die tolle welt", "tokens": ["Der", "lau\u00b7tern", "wahr\u00b7heit", "durch", ",", "ob\u00b7gleich", "die", "tol\u00b7le", "welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Erst auf den autor sieht, und aus verkehrtem triebe", "tokens": ["Erst", "auf", "den", "au\u00b7tor", "sieht", ",", "und", "aus", "ver\u00b7kehr\u00b7tem", "trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das sch\u00f6nst\u2019 und beste buch offt vor verwerfflich h\u00e4lt.", "tokens": ["Das", "sch\u00f6nst'", "und", "bes\u00b7te", "buch", "offt", "vor", "ver\u00b7werf\u00b7flich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADJA", "NN", "ADV", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein kluger merckt auf das, was er geschrieben siehet,", "tokens": ["Ein", "klu\u00b7ger", "merckt", "auf", "das", ",", "was", "er", "ge\u00b7schrie\u00b7ben", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und nicht auf die person, die es geschrieben hat.", "tokens": ["Und", "nicht", "auf", "die", "per\u00b7son", ",", "die", "es", "ge\u00b7schrie\u00b7ben", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ART", "NE", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bisweilen giebt ein maun, der schein und ehre fliehet,", "tokens": ["Bis\u00b7wei\u00b7len", "giebt", "ein", "maun", ",", "der", "schein", "und", "eh\u00b7re", "flie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "VVINF", "$,", "PRELS", "ADJD", "KON", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Uns in der tiefsten noth den allerkl\u00fcgsten rath,", "tokens": ["Uns", "in", "der", "tiefs\u00b7ten", "noth", "den", "al\u00b7ler\u00b7kl\u00fcgs\u00b7ten", "rath", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der menschen ansehn ist ein stern, der bald verschwindet,", "tokens": ["Der", "men\u00b7schen", "an\u00b7sehn", "ist", "ein", "stern", ",", "der", "bald", "ver\u00b7schwin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VAFIN", "ART", "ADJA", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die wahrheit aber bleibt, wenn alles untergeht.", "tokens": ["Die", "wahr\u00b7heit", "a\u00b7ber", "bleibt", ",", "wenn", "al\u00b7les", "un\u00b7ter\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die wahrheit, die man offt bey albern leuten findet,", "tokens": ["Die", "wahr\u00b7heit", ",", "die", "man", "offt", "bey", "al\u00b7bern", "leu\u00b7ten", "fin\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "APPR", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und die Carneades und Plato nicht versteht.", "tokens": ["Und", "die", "Car\u00b7ne\u00b7a\u00b7des", "und", "Pla\u00b7to", "nicht", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "NE", "PTKNEG", "VVFIN", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Gott offenbaret sich ohn absicht der personen:", "tokens": ["Gott", "of\u00b7fen\u00b7ba\u00b7ret", "sich", "ohn", "ab\u00b7sicht", "der", "per\u00b7so\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Er macht uns seinen rath durch arme fischer kund.", "tokens": ["Er", "macht", "uns", "sei\u00b7nen", "rath", "durch", "ar\u00b7me", "fi\u00b7scher", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Man sieht der wahrheit geist noch bey der einfalt wohnen,", "tokens": ["Man", "sieht", "der", "wahr\u00b7heit", "geist", "noch", "bey", "der", "ein\u00b7falt", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Gott legt sein hohes wort auch in der kinder mund.", "tokens": ["Gott", "legt", "sein", "ho\u00b7hes", "wort", "auch", "in", "der", "kin\u00b7der", "mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "La\u00df dich den vorwitz nicht in deinem lesen hindern:", "tokens": ["La\u00df", "dich", "den", "vor\u00b7witz", "nicht", "in", "dei\u00b7nem", "le\u00b7sen", "hin\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Was GOtt verbergen will, das la\u00df verborgen seyn.", "tokens": ["Was", "Gott", "ver\u00b7ber\u00b7gen", "will", ",", "das", "la\u00df", "ver\u00b7bor\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "$,", "PRELS", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die fackel der vernunfft wird diese nacht nicht mindern.", "tokens": ["Die", "fa\u00b7ckel", "der", "ver\u00b7nunfft", "wird", "die\u00b7se", "nacht", "nicht", "min\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PDAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sie blendet dich vielmehr durch ihren falschen schein.", "tokens": ["Sie", "blen\u00b7det", "dich", "viel\u00b7mehr", "durch", "ih\u00b7ren", "fal\u00b7schen", "schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Darum bem\u00fche dich nicht in die h\u00f6h zu klimmen,", "tokens": ["Da\u00b7rum", "be\u00b7m\u00fc\u00b7he", "dich", "nicht", "in", "die", "h\u00f6h", "zu", "klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wo man am sichersten auf ebner erden steht.", "tokens": ["Wo", "man", "am", "si\u00b7chers\u00b7ten", "auf", "eb\u00b7ner", "er\u00b7den", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "ADJA", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Die bibel ist ein meer, da elephanten schwimmen,", "tokens": ["Die", "bi\u00b7bel", "ist", "ein", "meer", ",", "da", "e\u00b7le\u00b7phan\u00b7ten", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "KOUS", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und da ein niedrig schaf mit trocknem fu\u00dfe geht.", "tokens": ["Und", "da", "ein", "nied\u00b7rig", "schaf", "mit", "trock\u00b7nem", "fu\u00b7\u00dfe", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wilst du die heilge schrifft mit nutzen durchstudieren,", "tokens": ["Wilst", "du", "die", "heil\u00b7ge", "schrifft", "mit", "nut\u00b7zen", "durch\u00b7stu\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So la\u00df des glaubens licht der seele leit-stern seyn,", "tokens": ["So", "la\u00df", "des", "glau\u00b7bens", "licht", "der", "see\u00b7le", "leit\u00b7stern", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "NN", "ART", "VVFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und in der einfalt dich den geist der demuth f\u00fchren,", "tokens": ["Und", "in", "der", "ein\u00b7falt", "dich", "den", "geist", "der", "de\u00b7muth", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.44": {"text": "So dringest du gewi\u00df in die geheimni\u00df\u2019 ein.", "tokens": ["So", "drin\u00b7gest", "du", "ge\u00b7wi\u00df", "in", "die", "ge\u00b7heim\u00b7ni\u00df'", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "In die geheimni\u00dfe, in die kein stoltzer meister,", "tokens": ["In", "die", "ge\u00b7heim\u00b7ni\u00b7\u00dfe", ",", "in", "die", "kein", "stolt\u00b7zer", "meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "APPR", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "So hoch er sich vermi\u00dft, mit seiner klugheit dringt.", "tokens": ["So", "hoch", "er", "sich", "ver\u00b7mi\u00dft", ",", "mit", "sei\u00b7ner", "klug\u00b7heit", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Denn GOttes wahrheit flieht die aufgebla\u00dfnen geister,", "tokens": ["Denn", "Got\u00b7tes", "wahr\u00b7heit", "flieht", "die", "auf\u00b7ge\u00b7bla\u00df\u00b7nen", "geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Die nun ein eitler zug zum bibel-lesen bringt.", "tokens": ["Die", "nun", "ein", "eit\u00b7ler", "zug", "zum", "bi\u00b7bel\u00b7le\u00b7sen", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Drum la\u00df dich iederzeit den sinn der demuth leiten,", "tokens": ["Drum", "la\u00df", "dich", "ie\u00b7der\u00b7zeit", "den", "sinn", "der", "de\u00b7muth", "lei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.50": {"text": "Und schlag der heiligen erkl\u00e4rung nicht in wind.", "tokens": ["Und", "schlag", "der", "hei\u00b7li\u00b7gen", "er\u00b7kl\u00e4\u00b7rung", "nicht", "in", "wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Verwirff die reden nicht, die von erfahrnen leuten", "tokens": ["Ver\u00b7wirff", "die", "re\u00b7den", "nicht", ",", "die", "von", "er\u00b7fahr\u00b7nen", "leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "PTKNEG", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Nach GOttes sinn und rath genau gepr\u00fcfet sind.", "tokens": ["Nach", "Got\u00b7tes", "sinn", "und", "rath", "ge\u00b7nau", "ge\u00b7pr\u00fc\u00b7fet", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "La\u00df dich die gleichnisse der alten nicht verdrie\u00dfen,", "tokens": ["La\u00df", "dich", "die", "gleich\u00b7nis\u00b7se", "der", "al\u00b7ten", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ART", "ADJA", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Ob sie gleich fleisch und blut vor schlecht und dunckel sch\u00e4tzt.", "tokens": ["Ob", "sie", "gleich", "fleisch", "und", "blut", "vor", "schlecht", "und", "dun\u00b7ckel", "sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "NN", "APPR", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Gott wei\u00df zu rechter zeit dir alles aufzuschlie\u00dfen;", "tokens": ["Gott", "wei\u00df", "zu", "rech\u00b7ter", "zeit", "dir", "al\u00b7les", "auf\u00b7zu\u00b7schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Denn ihr erleuchter geist hat nichts umsonst gesetzt.", "tokens": ["Denn", "ihr", "er\u00b7leuch\u00b7ter", "geist", "hat", "nichts", "um\u00b7sonst", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}