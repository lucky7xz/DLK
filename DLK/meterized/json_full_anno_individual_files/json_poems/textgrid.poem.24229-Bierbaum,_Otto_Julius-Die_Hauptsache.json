{"textgrid.poem.24229": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Die Hauptsache", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir sind, wer wei\u00df es von wem, auf die Welt,", "tokens": ["Wir", "sind", ",", "wer", "wei\u00df", "es", "von", "wem", ",", "auf", "die", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "VVFIN", "PPER", "APPR", "PIS", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wer wei\u00df es woher, wozu gestellt;", "tokens": ["Wer", "wei\u00df", "es", "wo\u00b7her", ",", "wo\u00b7zu", "ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es ist ein Gewirre.", "tokens": ["Es", "ist", "ein", "Ge\u00b7wir\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der eine geht seinen Weg gradaus,", "tokens": ["Der", "ei\u00b7ne", "geht", "sei\u00b7nen", "Weg", "gra\u00b7daus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der andre findet nie nach Haus,", "tokens": ["Der", "and\u00b7re", "fin\u00b7det", "nie", "nach", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jeder geht einmal irre.", "tokens": ["Je\u00b7der", "geht", "ein\u00b7mal", "ir\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Ich \u2013 wei\u00df nicht viel", "tokens": ["Ich", "\u2013", "wei\u00df", "nicht", "viel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$(", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Von End und Ziel,", "tokens": ["Von", "End", "und", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Geh meine Stra\u00dfe wie im Spiel", "tokens": ["Geh", "mei\u00b7ne", "Stra\u00b7\u00dfe", "wie", "im", "Spiel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und denke frei:", "tokens": ["Und", "den\u00b7ke", "frei", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Was es auch sei, \u2013", "tokens": ["Was", "es", "auch", "sei", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ich bin auf der Welt und du bist dabei.", "tokens": ["Ich", "bin", "auf", "der", "Welt", "und", "du", "bist", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "KON", "PPER", "VAFIN", "PAV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "W