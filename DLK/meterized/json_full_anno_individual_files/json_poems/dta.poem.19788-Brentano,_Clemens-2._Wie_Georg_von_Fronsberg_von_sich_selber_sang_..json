{"dta.poem.19788": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "2.  Wie Georg von Fronsberg von  \n sich selber sang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Flei\u00df und M\u00fch, ich nie hab gespart,                 ", "tokens": ["Mein", "Flei\u00df", "und", "M\u00fch", ",", "ich", "nie", "hab", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und allzeit gewahrt, dem Herren mein;", "tokens": ["Und", "all\u00b7zeit", "ge\u00b7wahrt", ",", "dem", "Her\u00b7ren", "mein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "ART", "NN", "PPOSAT", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Zum Besten sein schickt ich mich drein,", "tokens": ["Zum", "Bes\u00b7ten", "sein", "schickt", "ich", "mich", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAINF", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Gnad, Gunst verhofft, dochs Gem\u00fcth zu Hof", "tokens": ["Gnad", ",", "Gunst", "ver\u00b7hofft", ",", "dochs", "Ge\u00b7m\u00fcth", "zu", "Hof"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "VVFIN", "$,", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Verkehrt sich oft.", "tokens": ["Ver\u00b7kehrt", "sich", "oft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wer sich zukauft, der lauft weit vor,", "tokens": ["Wer", "sich", "zu\u00b7kauft", ",", "der", "lauft", "weit", "vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "$,", "PRELS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00f6mmt empor' doch wer lang Zeit", "tokens": ["Und", "k\u00f6mmt", "em\u00b7por'", "doch", "wer", "lang", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "PWS", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach Ehren streit, mu\u00df dannen weit,", "tokens": ["Nach", "Eh\u00b7ren", "streit", ",", "mu\u00df", "dan\u00b7nen", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das sehr mich kr\u00e4nkt, mein treuer Dienst", "tokens": ["Das", "sehr", "mich", "kr\u00e4nkt", ",", "mein", "treu\u00b7er", "Dienst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bleibt unerkennt.", "tokens": ["Bleibt", "un\u00b7er\u00b7kennt", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Kein Dank noch Lohn davon ich bring,", "tokens": ["Kein", "Dank", "noch", "Lohn", "da\u00b7von", "ich", "bring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man wiegt mich gring, und hat mein gar", "tokens": ["Man", "wiegt", "mich", "gring", ",", "und", "hat", "mein", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "VVFIN", "$,", "KON", "VAFIN", "PPOSAT", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergessen zwar, gro\u00df Noth, Gefahr", "tokens": ["Ver\u00b7ges\u00b7sen", "zwar", ",", "gro\u00df", "Noth", ",", "Ge\u00b7fahr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "ADV", "$,", "ADJD", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bestanden han, was Freude soll", "tokens": ["Ich", "be\u00b7stan\u00b7den", "han", ",", "was", "Freu\u00b7de", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVPP", "VAFIN", "$,", "PWS", "NN", "VMFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ich haben dran?", "tokens": ["Ich", "ha\u00b7ben", "dran", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}