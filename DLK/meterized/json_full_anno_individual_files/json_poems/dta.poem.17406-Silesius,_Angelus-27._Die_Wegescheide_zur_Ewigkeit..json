{"dta.poem.17406": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "27.  Die Wegescheide zur Ewigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wegescheid\u2019 ist hier: Wo lenkstu dich nu hin?", "tokens": ["Die", "Wege\u00b7scheid'", "ist", "hier", ":", "Wo", "lenks\u00b7tu", "dich", "nu", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Zur Lincken ist verlust/ zur Rechten ist gewien.", "tokens": ["Zur", "Lin\u00b7cken", "ist", "ver\u00b7lust", "/", "zur", "Rech\u00b7ten", "ist", "ge\u00b7wi\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$(", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}