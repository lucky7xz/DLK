{"textgrid.poem.59572": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Gott der Hirt", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Sch\u00e4fer fromm und gut,", "tokens": ["Es", "ist", "ein", "Sch\u00e4\u00b7fer", "fromm", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der treibet g\u00fcldne Schafe aus,", "tokens": ["Der", "trei\u00b7bet", "g\u00fcld\u00b7ne", "Scha\u00b7fe", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er h\u00e4lt sie wohl in sichrer Hut,", "tokens": ["Er", "h\u00e4lt", "sie", "wohl", "in", "sich\u00b7rer", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jedes kommt ihm froh zu Haus;", "tokens": ["Und", "je\u00b7des", "kommt", "ihm", "froh", "zu", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Blau ist die weite Himmelsweide,", "tokens": ["Blau", "ist", "die", "wei\u00b7te", "Him\u00b7mels\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Der Sch\u00e4fer sitzt auf h\u00f6chster H\u00f6h'", "tokens": ["Der", "Sch\u00e4\u00b7fer", "sitzt", "auf", "h\u00f6chs\u00b7ter", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schaut die Weltenenden beide,", "tokens": ["Und", "schaut", "die", "Wel\u00b7te\u00b7nen\u00b7den", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df seiner Schar kein Leid gescheh'.", "tokens": ["Da\u00df", "sei\u00b7ner", "Schar", "kein", "Leid", "ge\u00b7scheh'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So weidet sie in Ewigkeit,", "tokens": ["So", "wei\u00b7det", "sie", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und jedes kennet seine Flur", "tokens": ["Und", "je\u00b7des", "ken\u00b7net", "sei\u00b7ne", "Flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weichet keinen Finger breit", "tokens": ["Und", "wei\u00b7chet", "kei\u00b7nen", "Fin\u00b7ger", "breit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von seines Weges fester Spur;", "tokens": ["Von", "sei\u00b7nes", "We\u00b7ges", "fes\u00b7ter", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man sieht die einen um die andern", "tokens": ["Man", "sieht", "die", "ei\u00b7nen", "um", "die", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ART", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gar lustig ohne Ansto\u00df gehn,", "tokens": ["Gar", "lus\u00b7tig", "oh\u00b7ne", "An\u00b7sto\u00df", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn k\u00f6nnten sie in Irre wandern,", "tokens": ["Denn", "k\u00f6nn\u00b7ten", "sie", "in", "Ir\u00b7re", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So m\u00fc\u00dfte gleich die Welt vergehn.", "tokens": ["So", "m\u00fc\u00df\u00b7te", "gleich", "die", "Welt", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Herde F\u00fcrstin Sonne hei\u00dft,", "tokens": ["Der", "Her\u00b7de", "F\u00fcrs\u00b7tin", "Son\u00b7ne", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr folgt ihr Sohn, der helle Tag,", "tokens": ["Ihr", "folgt", "ihr", "Sohn", ",", "der", "hel\u00b7le", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Mond wird als ihr F\u00fcrst gepreist,", "tokens": ["Der", "Mond", "wird", "als", "ihr", "F\u00fcrst", "ge\u00b7preist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sich die Nacht erfreuen mag;", "tokens": ["Da\u00df", "sich", "die", "Nacht", "er\u00b7freu\u00b7en", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm folgen viele tausend Lichter", "tokens": ["Ihm", "fol\u00b7gen", "vie\u00b7le", "tau\u00b7send", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als schnelle Diener h\u00fcbsch und fein", "tokens": ["Als", "schnel\u00b7le", "Die\u00b7ner", "h\u00fcbsch", "und", "fein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ziehn die Menschenangesichter", "tokens": ["Und", "ziehn", "die", "Men\u00b7schen\u00b7an\u00b7ge\u00b7sich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Empor mit wunderbarem Schein.", "tokens": ["Em\u00b7por", "mit", "wun\u00b7der\u00b7ba\u00b7rem", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn wie mit Gottes Augen blickt", "tokens": ["Denn", "wie", "mit", "Got\u00b7tes", "Au\u00b7gen", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ganze Firmament herab,", "tokens": ["Das", "gan\u00b7ze", "Fir\u00b7ma\u00b7ment", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bis ins tiefste Herz entz\u00fcckt", "tokens": ["Und", "bis", "ins", "tiefs\u00b7te", "Herz", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verl\u00e4\u00dft der Mensch sein Erdengrab,", "tokens": ["Ver\u00b7l\u00e4\u00dft", "der", "Mensch", "sein", "Er\u00b7den\u00b7grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er schwingt sich \u00fcber Erdenqualen", "tokens": ["Er", "schwingt", "sich", "\u00fc\u00b7ber", "Er\u00b7den\u00b7qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hin, wo der Seraph selig fliegt,", "tokens": ["Hin", ",", "wo", "der", "Se\u00b7raph", "se\u00b7lig", "fliegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Wo aller Tand von Wort und Zahlen,", "tokens": ["Wo", "al\u00b7ler", "Tand", "von", "Wort", "und", "Zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo der Gedanke selbst versiegt.", "tokens": ["Wo", "der", "Ge\u00b7dan\u00b7ke", "selbst", "ver\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du, der die g\u00fcldnen Schafe treibt,", "tokens": ["Du", ",", "der", "die", "g\u00fcld\u00b7nen", "Scha\u00b7fe", "treibt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du guter, treuer, frommer Gott,", "tokens": ["Du", "gu\u00b7ter", ",", "treu\u00b7er", ",", "from\u00b7mer", "Gott", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was in die Brust so tief sich schreibt,", "tokens": ["Was", "in", "die", "Brust", "so", "tief", "sich", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist kein Wahn, das ist kein Spott:", "tokens": ["Das", "ist", "kein", "Wahn", ",", "das", "ist", "kein", "Spott", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "$,", "PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich werde mit der sel'gen Herde", "tokens": ["Ich", "wer\u00b7de", "mit", "der", "sel'\u00b7gen", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Einst droben lustig weiden gehn", "tokens": ["Einst", "dro\u00b7ben", "lus\u00b7tig", "wei\u00b7den", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VVINF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und als ein dunkles Bild die Erde", "tokens": ["Und", "als", "ein", "dunk\u00b7les", "Bild", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Tief unter mir sich w\u00e4lzen sehn.", "tokens": ["Tief", "un\u00b7ter", "mir", "sich", "w\u00e4l\u00b7zen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "La\u00df denn die g\u00fcldnen Schafe aus,", "tokens": ["La\u00df", "denn", "die", "g\u00fcld\u00b7nen", "Scha\u00b7fe", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Sch\u00e4fer, la\u00df sie wieder ein,", "tokens": ["O", "Sch\u00e4\u00b7fer", ",", "la\u00df", "sie", "wie\u00b7der", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVIMP", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schaue immer froh hinaus", "tokens": ["Ich", "schau\u00b7e", "im\u00b7mer", "froh", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "