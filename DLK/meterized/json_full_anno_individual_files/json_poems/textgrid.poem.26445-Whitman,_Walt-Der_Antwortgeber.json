{"textgrid.poem.26445": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "Der Antwortgeber", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun lauscht auf meine Morgen-Romanze \u2013 ich k\u00fcnde euch ", "tokens": ["Nun", "lauscht", "auf", "mei\u00b7ne", "Mor\u00b7gen\u00b7Ro\u00b7man\u00b7ze", "\u2013", "ich", "k\u00fcn\u00b7de", "euch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Den St\u00e4dten und Farmen sing' ich, wie sie im Sonnenschein vor mir sich breiten.", "tokens": ["Den", "St\u00e4d\u00b7ten", "und", "Far\u00b7men", "sing'", "ich", ",", "wie", "sie", "im", "Son\u00b7nen\u00b7schein", "vor", "mir", "sich", "brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "APPRART", "NN", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein J\u00fcngling kommt und bringt mir Botschaft von seinem Bruder;", "tokens": ["Ein", "J\u00fcng\u00b7ling", "kommt", "und", "bringt", "mir", "Bot\u00b7schaft", "von", "sei\u00b7nem", "Bru\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie mag er das Ob und Wann seines Bruders wissen?", "tokens": ["Wie", "mag", "er", "das", "Ob", "und", "Wann", "sei\u00b7nes", "Bru\u00b7ders", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "KON", "PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sag' ihm, er soll mir die Zeichen geben.", "tokens": ["Sag'", "ihm", ",", "er", "soll", "mir", "die", "Zei\u00b7chen", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Seiner harren wir alle! Ihm \u00fcberlassen sich alle. Sein Wort gilt und entscheidet.", "tokens": ["Sei\u00b7ner", "har\u00b7ren", "wir", "al\u00b7le", "!", "Ihm", "\u00fc\u00b7ber\u00b7las\u00b7sen", "sich", "al\u00b7le", ".", "Sein", "Wort", "gilt", "und", "ent\u00b7schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "PIS", "$.", "PPER", "VVFIN", "PRF", "PIS", "$.", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+--+--+-+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ihn erkennen sie, baden sich in ihm wie im Licht,", "tokens": ["Ihn", "er\u00b7ken\u00b7nen", "sie", ",", "ba\u00b7den", "sich", "in", "ihm", "wie", "im", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "PRF", "APPR", "PPER", "KOKOM", "APPRART", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Tauchen in ihm unter, und er in ihnen.", "tokens": ["Tau\u00b7chen", "in", "ihm", "un\u00b7ter", ",", "und", "er", "in", "ih\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "$,", "KON", "PPER", "APPR", "PPER", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}}, "stanza.4": {"line.1": {"text": "Die Engl\u00e4nder meinen, er sei von ihrem Stamme,", "tokens": ["Die", "En\u00b7gl\u00e4n\u00b7der", "mei\u00b7nen", ",", "er", "sei", "von", "ih\u00b7rem", "Stam\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dem Juden erscheint er als Jude, dem Russen ein Russe \u2013 vertraut und nah, und keinem fremd.", "tokens": ["Dem", "Ju\u00b7den", "er\u00b7scheint", "er", "als", "Ju\u00b7de", ",", "dem", "Rus\u00b7sen", "ein", "Rus\u00b7se", "\u2013", "ver\u00b7traut", "und", "nah", ",", "und", "kei\u00b7nem", "fremd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOUS", "NN", "$,", "ART", "NN", "ART", "NN", "$(", "ADJD", "KON", "ADJD", "$,", "KON", "PIS", "ADJD", "$."], "meter": "-+--+--+--+--+--+-+-+-+", "measure": "amphibrach.penta.plus"}}, "stanza.5": {"line.1": {"text": "Die S\u00e4nger sind willkommen, werden verstanden, erscheinen h\u00e4ufig genug;", "tokens": ["Die", "S\u00e4n\u00b7ger", "sind", "will\u00b7kom\u00b7men", ",", "wer\u00b7den", "ver\u00b7stan\u00b7den", ",", "er\u00b7schei\u00b7nen", "h\u00e4u\u00b7fig", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "VAFIN", "VVPP", "$,", "VVFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+--+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Doch selten der Tag wie der Ort, da der Dichtung Erzeuger geboren,", "tokens": ["Doch", "sel\u00b7ten", "der", "Tag", "wie", "der", "Ort", ",", "da", "der", "Dich\u00b7tung", "Er\u00b7zeu\u00b7ger", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "KOKOM", "ART", "NN", "$,", "KOUS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+--+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.3": {"text": "Nicht jedes Jahrhundert, nicht f\u00fcnf Jahrhunderte enthalten solch einen Tag, trotz all der Namen!", "tokens": ["Nicht", "je\u00b7des", "Jahr\u00b7hun\u00b7dert", ",", "nicht", "f\u00fcnf", "Jahr\u00b7hun\u00b7der\u00b7te", "ent\u00b7hal\u00b7ten", "solch", "ei\u00b7nen", "Tag", ",", "trotz", "all", "der", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "$,", "PTKNEG", "CARD", "NN", "VVFIN", "PIAT", "ART", "NN", "$,", "APPR", "PIAT", "ART", "NN", "$."], "meter": "-+--+--+-+-+-+--+-+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Die ganze Zeit und zu allen Zeiten warten die Worte echter Dichtung,", "tokens": ["Die", "gan\u00b7ze", "Zeit", "und", "zu", "al\u00b7len", "Zei\u00b7ten", "war\u00b7ten", "die", "Wor\u00b7te", "ech\u00b7ter", "Dich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Die Worte echter Dichtung gefallen nicht nur,", "tokens": ["Die", "Wor\u00b7te", "ech\u00b7ter", "Dich\u00b7tung", "ge\u00b7fal\u00b7len", "nicht", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die echten Dichter sind nicht nur Anh\u00e4nger der Sch\u00f6nheit,", "tokens": ["Die", "ech\u00b7ten", "Dich\u00b7ter", "sind", "nicht", "nur", "An\u00b7h\u00e4n\u00b7ger", "der", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "ADV", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie sind ihre erhabenen Beherrscher.", "tokens": ["Sie", "sind", "ih\u00b7re", "er\u00b7ha\u00b7be\u00b7nen", "Be\u00b7herr\u00b7scher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Gr\u00f6\u00dfe der S\u00f6hne ist der Ausflu\u00df der Gr\u00f6\u00dfe von M\u00fcttern und V\u00e4tern.", "tokens": ["Die", "Gr\u00f6\u00b7\u00dfe", "der", "S\u00f6h\u00b7ne", "ist", "der", "Aus\u00b7flu\u00df", "der", "Gr\u00f6\u00b7\u00dfe", "von", "M\u00fct\u00b7tern", "und", "V\u00e4\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+--+--+--+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Die Worte echter Dichtung sind die Krone und der endg\u00fcltige Triumph der Wissenschaft!", "tokens": ["Die", "Wor\u00b7te", "ech\u00b7ter", "Dich\u00b7tung", "sind", "die", "Kro\u00b7ne", "und", "der", "end\u00b7g\u00fcl\u00b7ti\u00b7ge", "Tri\u00b7umph", "der", "Wis\u00b7sen\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ART", "NN", "KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+--+--+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Seemann und Wanderer liegen dem Stoff der Dichtung zu Grunde;", "tokens": ["See\u00b7mann", "und", "Wan\u00b7de\u00b7rer", "lie\u00b7gen", "dem", "Stoff", "der", "Dich\u00b7tung", "zu", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Der Antwortgeber, Baumeister, Geometer, Chemiker, Anatom, Phrenologe, K\u00fcnstler:", "tokens": ["Der", "Ant\u00b7wort\u00b7ge\u00b7ber", ",", "Bau\u00b7meis\u00b7ter", ",", "Geo\u00b7me\u00b7ter", ",", "Che\u00b7mi\u00b7ker", ",", "A\u00b7na\u00b7tom", ",", "Phre\u00b7no\u00b7lo\u00b7ge", ",", "K\u00fcnst\u00b7ler", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NE", "$,", "NE", "$,", "NN", "$."], "meter": "-+-+-+--+--+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "Alle liegen dem Dichter, dem ", "tokens": ["Al\u00b7le", "lie\u00b7gen", "dem", "Dich\u00b7ter", ",", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,", "PRELS"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Nun lauscht auf meine Morgen-Romanze \u2013 ich k\u00fcnde euch ", "tokens": ["Nun", "lauscht", "auf", "mei\u00b7ne", "Mor\u00b7gen\u00b7Ro\u00b7man\u00b7ze", "\u2013", "ich", "k\u00fcn\u00b7de", "euch