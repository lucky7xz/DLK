{"textgrid.poem.56021": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Eine von den Alten", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abends manchmal (wei\u00dft du, wie das tut?)", "tokens": ["A\u00b7bends", "manch\u00b7mal", "(", "wei\u00dft", "du", ",", "wie", "das", "tut", "?", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$(", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wenn sie pl\u00f6tzlich stehn und r\u00fcckw\u00e4rts nicken", "tokens": ["wenn", "sie", "pl\u00f6tz\u00b7lich", "stehn", "und", "r\u00fcck\u00b7w\u00e4rts", "ni\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und ein L\u00e4cheln, wie aus lauter Flicken,", "tokens": ["und", "ein", "L\u00e4\u00b7cheln", ",", "wie", "aus", "lau\u00b7ter", "Fli\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "zeigen unter ihrem halben Hut.", "tokens": ["zei\u00b7gen", "un\u00b7ter", "ih\u00b7rem", "hal\u00b7ben", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Neben ihnen ist dann ein Geb\u00e4ude,", "tokens": ["Ne\u00b7ben", "ih\u00b7nen", "ist", "dann", "ein", "Ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "wo