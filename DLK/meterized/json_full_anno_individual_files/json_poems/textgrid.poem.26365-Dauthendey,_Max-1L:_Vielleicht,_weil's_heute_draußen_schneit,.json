], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Stuhl, Feder und die Tinte hier;", "tokens": ["Stuhl", ",", "Fe\u00b7der", "und", "die", "Tin\u00b7te", "hier", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und willst du dir nicht alles schreiben,", "tokens": ["Und", "willst", "du", "dir", "nicht", "al\u00b7les", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKNEG", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "So kann ja manches unterbleiben.", "tokens": ["So", "kann", "ja", "man\u00b7ches", "un\u00b7ter\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Du deutest es nur an von ferne,", "tokens": ["Du", "deu\u00b7test", "es", "nur", "an", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Geheimes denkt sich jeder gerne;", "tokens": ["Ge\u00b7hei\u00b7mes", "denkt", "sich", "je\u00b7der", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PIS", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Von allen die dich lesen werden,", "tokens": ["Von", "al\u00b7len", "die", "dich", "le\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PRELS", "PRF", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Macht's jeder, wie er mu\u00df, auf Erden.\u00ab", "tokens": ["Macht's", "je\u00b7der", ",", "wie", "er", "mu\u00df", ",", "auf", "Er\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "PIS", "$,", "PWAV", "PPER", "VMFIN", "$,", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Ich sprach zu meiner hohen Seele,", "tokens": ["Ich", "sprach", "zu", "mei\u00b7ner", "ho\u00b7hen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Da\u00df sie mir ganz die Ruhe stehle,", "tokens": ["Da\u00df", "sie", "mir", "ganz", "die", "Ru\u00b7he", "steh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Sie m\u00f6chte mir das Schreiben lassen;", "tokens": ["Sie", "m\u00f6ch\u00b7te", "mir", "das", "Schrei\u00b7ben", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Doch mit der Seel war nicht zu spa\u00dfen,", "tokens": ["Doch", "mit", "der", "Seel", "war", "nicht", "zu", "spa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Die hohe Seele sprach nur wieder:", "tokens": ["Die", "ho\u00b