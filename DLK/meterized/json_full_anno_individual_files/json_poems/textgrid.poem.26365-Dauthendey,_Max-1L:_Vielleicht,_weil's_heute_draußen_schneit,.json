{"textgrid.poem.26365": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vielleicht, weil's heute drau\u00dfen schneit,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vielleicht, weil's heute drau\u00dfen schneit,", "tokens": ["Viel\u00b7leicht", ",", "weil's", "heu\u00b7te", "drau\u00b7\u00dfen", "schneit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchl' ich mich so elegisch weit.", "tokens": ["F\u00fchl'", "ich", "mich", "so", "e\u00b7le\u00b7gisch", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der Winter ist's, der Seelen weckt", "tokens": ["Der", "Win\u00b7ter", "ist's", ",", "der", "See\u00b7len", "weckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht allein den Dreck bezweckt.", "tokens": ["Und", "nicht", "al\u00b7lein", "den", "Dreck", "be\u00b7zweckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Sommer war man drau\u00dfen heiter,", "tokens": ["Im", "Som\u00b7mer", "war", "man", "drau\u00b7\u00dfen", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und davon ward der K\u00f6rper breiter,", "tokens": ["Und", "da\u00b7von", "ward", "der", "K\u00f6r\u00b7per", "brei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Man sa\u00df im Garten bei dem Bier", "tokens": ["Man", "sa\u00df", "im", "Gar\u00b7ten", "bei", "dem", "Bier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und legte von sich alle vier;", "tokens": ["Und", "leg\u00b7te", "von", "sich", "al\u00b7le", "vier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "PIAT", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Im Winter aber sch\u00f6n bescheiden", "tokens": ["Im", "Win\u00b7ter", "a\u00b7ber", "sch\u00f6n", "be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "H\u00e4lt man sich zu den Eingeweiden;", "tokens": ["H\u00e4lt", "man", "sich", "zu", "den", "Ein\u00b7ge\u00b7wei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Seele, die im Leib verschlossen,", "tokens": ["Die", "See\u00b7le", ",", "die", "im", "Leib", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wird jetzt so seelenvoll genossen.", "tokens": ["Wird", "jetzt", "so", "see\u00b7len\u00b7voll", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Doch Menschen, die von heute sind,", "tokens": ["Doch", "Men\u00b7schen", ",", "die", "von", "heu\u00b7te", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Kennen die Seele nur als Kind.", "tokens": ["Ken\u00b7nen", "die", "See\u00b7le", "nur", "als", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "KOUS", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Wir sind ein seelenlos Geschlecht,", "tokens": ["Wir", "sind", "ein", "see\u00b7len\u00b7los", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und keine Gottheit macht's uns recht;", "tokens": ["Und", "kei\u00b7ne", "Got\u00b7theit", "macht's", "uns", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Doch immer hab' ich's so gefunden:", "tokens": ["Doch", "im\u00b7mer", "hab'", "ich's", "so", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Der Mensch hat seine schwachen Stunden,", "tokens": ["Der", "Mensch", "hat", "sei\u00b7ne", "schwa\u00b7chen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Sie lassen keinen ungeschoren,", "tokens": ["Sie", "las\u00b7sen", "kei\u00b7nen", "un\u00b7ge\u00b7scho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Sie sind uns einfach angeboren.", "tokens": ["Sie", "sind", "uns", "ein\u00b7fach", "an\u00b7ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die Seele l\u00e4\u00dft sich nicht verneinen,", "tokens": ["Die", "See\u00b7le", "l\u00e4\u00dft", "sich", "nicht", "ver\u00b7nei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und kommt sie, will sie bei dir weinen.", "tokens": ["Und", "kommt", "sie", ",", "will", "sie", "bei", "dir", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Doch uns Modernen heutzutage", "tokens": ["Doch", "uns", "Mo\u00b7der\u00b7nen", "heut\u00b7zu\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Uns ist die Seel die gr\u00f6\u00dfte Plage,", "tokens": ["Uns", "ist", "die", "Seel", "die", "gr\u00f6\u00df\u00b7te", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Wir haben f\u00fcr die schw\u00e4chsten Stunden", "tokens": ["Wir", "ha\u00b7ben", "f\u00fcr", "die", "schw\u00e4chs\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Als Wehr den \u00dcbermensch erfunden.", "tokens": ["Als", "Wehr", "den", "\u00dc\u00b7ber\u00b7mensch", "er\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wir sprechen, wenn wir seelisch sind,", "tokens": ["Wir", "spre\u00b7chen", ",", "wenn", "wir", "see\u00b7lisch", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Gar sehr bedeutend in den Wind", "tokens": ["Gar", "sehr", "be\u00b7deu\u00b7tend", "in", "den", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und fragen nicht, warum und wo,", "tokens": ["Und", "fra\u00b7gen", "nicht", ",", "wa\u00b7rum", "und", "wo", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "KON", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Denn es geh\u00f6rt sich einmal so.", "tokens": ["Denn", "es", "ge\u00b7h\u00f6rt", "sich", "ein\u00b7mal", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und anders w\u00e4r es gar zu schwer,", "tokens": ["Und", "an\u00b7ders", "w\u00e4r", "es", "gar", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Weil es ja dann nicht seelisch w\u00e4r;", "tokens": ["Weil", "es", "ja", "dann", "nicht", "see\u00b7lisch", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Meist ist der Schlu\u00df vom Seelischsein,", "tokens": ["Meist", "ist", "der", "Schlu\u00df", "vom", "See\u00b7lischsein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Man legt sich ab und f\u00fchlt sich rein.", "tokens": ["Man", "legt", "sich", "ab", "und", "f\u00fchlt", "sich", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und da ich heute mal so bin,", "tokens": ["Und", "da", "ich", "heu\u00b7te", "mal", "so", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Sagt meine Seele: \u00bbGehe hin,", "tokens": ["Sagt", "mei\u00b7ne", "See\u00b7le", ":", "\u00bb", "Ge\u00b7he", "hin", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "$(", "VVIMP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Bekenne, wie es dir gegangen;", "tokens": ["Be\u00b7ken\u00b7ne", ",", "wie", "es", "dir", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Ein gro\u00dfer Dieb wird nicht gehangen,", "tokens": ["Ein", "gro\u00b7\u00dfer", "Dieb", "wird", "nicht", "ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Es h\u00f6rt dich nur dein Schreibpapier,", "tokens": ["Es", "h\u00f6rt", "dich", "nur", "dein", "Schreib\u00b7pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Stuhl, Feder und die Tinte hier;", "tokens": ["Stuhl", ",", "Fe\u00b7der", "und", "die", "Tin\u00b7te", "hier", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und willst du dir nicht alles schreiben,", "tokens": ["Und", "willst", "du", "dir", "nicht", "al\u00b7les", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKNEG", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "So kann ja manches unterbleiben.", "tokens": ["So", "kann", "ja", "man\u00b7ches", "un\u00b7ter\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Du deutest es nur an von ferne,", "tokens": ["Du", "deu\u00b7test", "es", "nur", "an", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Geheimes denkt sich jeder gerne;", "tokens": ["Ge\u00b7hei\u00b7mes", "denkt", "sich", "je\u00b7der", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "PIS", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Von allen die dich lesen werden,", "tokens": ["Von", "al\u00b7len", "die", "dich", "le\u00b7sen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PRELS", "PRF", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Macht's jeder, wie er mu\u00df, auf Erden.\u00ab", "tokens": ["Macht's", "je\u00b7der", ",", "wie", "er", "mu\u00df", ",", "auf", "Er\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "PIS", "$,", "PWAV", "PPER", "VMFIN", "$,", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Ich sprach zu meiner hohen Seele,", "tokens": ["Ich", "sprach", "zu", "mei\u00b7ner", "ho\u00b7hen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Da\u00df sie mir ganz die Ruhe stehle,", "tokens": ["Da\u00df", "sie", "mir", "ganz", "die", "Ru\u00b7he", "steh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Sie m\u00f6chte mir das Schreiben lassen;", "tokens": ["Sie", "m\u00f6ch\u00b7te", "mir", "das", "Schrei\u00b7ben", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Doch mit der Seel war nicht zu spa\u00dfen,", "tokens": ["Doch", "mit", "der", "Seel", "war", "nicht", "zu", "spa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Die hohe Seele sprach nur wieder:", "tokens": ["Die", "ho\u00b7he", "See\u00b7le", "sprach", "nur", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "\u00bbschweig, Balthasar, und setz dich nieder!\u00ab", "tokens": ["\u00bb", "schweig", ",", "Balt\u00b7ha\u00b7sar", ",", "und", "setz", "dich", "nie\u00b7der", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "NE", "$,", "KON", "VVIMP", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Ich, Kaspar Melchior Balthasar,", "tokens": ["Ich", ",", "Kas\u00b7par", "Melc\u00b7hi\u00b7or", "Balt\u00b7ha\u00b7sar", ","], "token_info": ["word", "punct", "word", "w