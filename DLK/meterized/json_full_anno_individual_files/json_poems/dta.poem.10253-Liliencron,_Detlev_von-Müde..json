{"dta.poem.10253": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "M\u00fcde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Aauf dem Wege vom Tanzsaal nach Haus             ", "tokens": ["Aa\u00b7uf", "dem", "We\u00b7ge", "vom", "Tanz\u00b7saal", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "APPR", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Ruht sich auf dem Steine aus", "tokens": ["Ruht", "sich", "auf", "dem", "Stei\u00b7ne", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die h\u00fcbsche Margreth.", "tokens": ["Die", "h\u00fcb\u00b7sche", "Mar\u00b7greth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Sie \u00f6ffnet ein wenig das stramme Mieder,", "tokens": ["Sie", "\u00f6ff\u00b7net", "ein", "we\u00b7nig", "das", "stram\u00b7me", "Mie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Da\u00df k\u00fchl \u00fcber die wei\u00dfen Glieder", "tokens": ["Da\u00df", "k\u00fchl", "\u00fc\u00b7ber", "die", "wei\u00b7\u00dfen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Der Nachtwind weht.", "tokens": ["Der", "Nacht\u00b7wind", "weht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Desselben Weges kommt auch der Junker,", "tokens": ["Des\u00b7sel\u00b7ben", "We\u00b7ges", "kommt", "auch", "der", "Jun\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Mit Troddeln am Hut und vielem Geflunker,", "tokens": ["Mit", "Trod\u00b7deln", "am", "Hut", "und", "vie\u00b7lem", "Ge\u00b7flun\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "KON", "PIS", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sieht den Stein,", "tokens": ["Und", "sieht", "den", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und auf dem Steine das schmucke Kind,", "tokens": ["Und", "auf", "dem", "Stei\u00b7ne", "das", "schmu\u00b7cke", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wie der Blitz geschwind,", "tokens": ["Und", "wie", "der", "Blitz", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00e4llt ihm was ein.", "tokens": ["F\u00e4llt", "ihm", "was", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Das liebe M\u00e4dchen hatte geschlafen,", "tokens": ["Das", "lie\u00b7be", "M\u00e4d\u00b7chen", "hat\u00b7te", "ge\u00b7schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch wie sie des Junkers Augen trafen,", "tokens": ["Doch", "wie", "sie", "des", "Jun\u00b7kers", "Au\u00b7gen", "tra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ist sie erwacht.", "tokens": ["Ist", "sie", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Erst schreit sie auf und will feldein,", "tokens": ["Erst", "schreit", "sie", "auf", "und", "will", "feld\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VMFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich denke wir lassen die beiden allein", "tokens": ["Ich", "den\u00b7ke", "wir", "las\u00b7sen", "die", "bei\u00b7den", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "PIAT", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "In der Sommernacht.", "tokens": ["In", "der", "Som\u00b7mer\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}