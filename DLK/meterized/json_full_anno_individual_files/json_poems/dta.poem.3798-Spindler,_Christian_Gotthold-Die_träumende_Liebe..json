{"dta.poem.3798": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Die tr\u00e4umende Liebe.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Nechst hatte Titan kaum sich unserm Pol entrissen,", "tokens": ["Nechst", "hat\u00b7te", "Ti\u00b7tan", "kaum", "sich", "un\u00b7serm", "Pol", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kam des Lichtes Feind, ein Freund von", "tokens": ["So", "kam", "des", "Lich\u00b7tes", "Feind", ",", "ein", "Freund", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Finsternissen,", "tokens": ["Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Der schr\u00f6ckliche ", "tokens": ["Der", "schr\u00f6ck\u00b7li\u00b7che"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+--", "measure": "dactylic.init"}, "line.5": {"text": "Und hatte ihr zur Lust die Schlafsucht mitgebracht.", "tokens": ["Und", "hat\u00b7te", "ihr", "zur", "Lust", "die", "Schlaf\u00b7sucht", "mit\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie steht gemeiniglich derselben zu der Seiten,", "tokens": ["Sie", "steht", "ge\u00b7mei\u00b7nig\u00b7lich", "der\u00b7sel\u00b7ben", "zu", "der", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es steht in ihrer Pflicht, sie \u00f6ffters zu begleiten.", "tokens": ["Es", "steht", "in", "ih\u00b7rer", "Pflicht", ",", "sie", "\u00f6ff\u00b7ters", "zu", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Sterne schim\u0303ernd Heer, der sch\u00f6ne ", "tokens": ["Der", "Ster\u00b7ne", "schim\u0303ernd", "Heer", ",", "der", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Schien schon mit gr\u00f6stem Glantz; des itzgen Tages", "tokens": ["Schien", "schon", "mit", "gr\u00f6s\u00b7tem", "Glantz", ";", "des", "itz\u00b7gen", "Ta\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Schlu\u00df", "tokens": ["Schlu\u00df"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "That ihre starcke Macht den Sterblichen zuwissen,", "tokens": ["That", "ih\u00b7re", "star\u00b7cke", "Macht", "den", "Sterb\u00b7li\u00b7chen", "zu\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und diese solten nun so Ruh als Schlaf geniessen.", "tokens": ["Und", "die\u00b7se", "sol\u00b7ten", "nun", "so", "Ruh", "als", "Schlaf", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "ADV", "ADV", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich folgte ihrem Winck, und legte mich zur Ruh,", "tokens": ["Ich", "folg\u00b7te", "ih\u00b7rem", "Win\u00b7ck", ",", "und", "leg\u00b7te", "mich", "zur", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Mit einem Wort, ich schlief; Doch welch ein artig", "tokens": ["Mit", "ei\u00b7nem", "Wort", ",", "ich", "schlief", ";", "Doch", "welch", "ein", "ar\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VVFIN", "$.", "KON", "PWAT", "ART", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Tr\u00e4umen!", "tokens": ["Tr\u00e4u\u00b7men", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Dich, Sch\u00f6nste, sahe ich bey dick belaubten B\u00e4ume\u0303,", "tokens": ["Dich", ",", "Sch\u00f6ns\u00b7te", ",", "sa\u00b7he", "ich", "bey", "dick", "be\u00b7laub\u00b7ten", "B\u00e4um\u1ebd", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In einem furchtsamen und Ein\u00f6ds-vollen Hayn,", "tokens": ["In", "ei\u00b7nem", "furcht\u00b7sa\u00b7men", "und", "Ei\u00b7n\u00f6ds\u00b7vol\u00b7len", "Hayn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wist bist du, sprach ich, Kind! allhier so gar allein!", "tokens": ["Wist", "bist", "du", ",", "sprach", "ich", ",", "Kind", "!", "all\u00b7hier", "so", "gar", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "NN", "$.", "ADV", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Welch Schicksal treibt dich her? Wie? k\u00f6nnen", "tokens": ["Welch", "Schick\u00b7sal", "treibt", "dich", "her", "?", "Wie", "?", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "$.", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Wald und Hecken,", "tokens": ["Wald", "und", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Sand, W\u00fcste, Stein und Fels dich, Sch\u00f6nste, nicht", "tokens": ["Sand", ",", "W\u00fcs\u00b7te", ",", "Stein", "und", "Fels", "dich", ",", "Sch\u00f6ns\u00b7te", ",", "nicht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "KON", "NN", "PPER", "$,", "NN", "$,", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "erschrecken?", "tokens": ["er\u00b7schre\u00b7cken", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Komm, eile doch mit mir aus diesem bangen Wald,", "tokens": ["Komm", ",", "ei\u00b7le", "doch", "mit", "mir", "aus", "die\u00b7sem", "ban\u00b7gen", "Wald", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "APPR", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es ist ja selbiger ein schn\u00f6der Aufenthalt", "tokens": ["Es", "ist", "ja", "sel\u00b7bi\u00b7ger", "ein", "schn\u00f6\u00b7der", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Von R\u00e4ubern, Angst und Qual, o! komme, la\u00df", "tokens": ["Von", "R\u00e4u\u00b7bern", ",", "Angst", "und", "Qual", ",", "o", "!", "kom\u00b7me", ",", "la\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "FM", "$.", "VVFIN", "$,", "XY"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "uns fliehen!", "tokens": ["uns", "flie\u00b7hen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "La\u00df dich und mich, o Kind! der Todes Furcht ent-", "tokens": ["La\u00df", "dich", "und", "mich", ",", "o", "Kind", "!", "der", "To\u00b7des", "Furcht", "ent"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "KON", "PPER", "$,", "FM", "NN", "$.", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "ziehen.", "tokens": ["zie\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Allein du sprachst zu mir: Freund! eine h\u00f6hre Macht", "tokens": ["Al\u00b7lein", "du", "sprachst", "zu", "mir", ":", "Freund", "!", "ei\u00b7ne", "h\u00f6h\u00b7re", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PPER", "$.", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Hat mich mit gr\u00f6sten Zwang in diesen Busch ge-", "tokens": ["Hat", "mich", "mit", "gr\u00f6s\u00b7ten", "Zwang", "in", "die\u00b7sen", "Busch", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "APPR", "PDAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "bracht.", "tokens": ["bracht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.33": {"text": "Ich sage weiter nichts; jedoch mein banges Schweige\u0303", "tokens": ["Ich", "sa\u00b7ge", "wei\u00b7ter", "nichts", ";", "je\u00b7doch", "mein", "ban\u00b7ges", "Schweig\u1ebd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$.", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und da mein Auge thr\u00e4nt, die\u00df wird dir gnugsam", "tokens": ["Und", "da", "mein", "Au\u00b7ge", "thr\u00e4nt", ",", "die\u00df", "wird", "dir", "gnug\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "zeigen.", "tokens": ["zei\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Freund! ich bin nicht mehr dein; sechs Jahre ist", "tokens": ["Freund", "!", "ich", "bin", "nicht", "mehr", "dein", ";", "sechs", "Jah\u00b7re", "ist"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VAFIN", "PTKNEG", "ADV", "PPOSAT", "$.", "CARD", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "es schon,", "tokens": ["es", "schon", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.38": {"text": "Seit dem du mich geliebt, was hast du denn davon?", "tokens": ["Seit", "dem", "du", "mich", "ge\u00b7liebt", ",", "was", "hast", "du", "denn", "da\u00b7von", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVPP", "$,", "PWS", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Du must, erschrockner Freund! auf ewig dich ent-", "tokens": ["Du", "must", ",", "er\u00b7schrock\u00b7ner", "Freund", "!", "auf", "e\u00b7wig", "dich", "ent"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "ADJA", "NN", "$.", "APPR", "ADJD", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "schliessen,", "tokens": ["schlies\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "Mich, dein sonst treues Kind, vielleicht mit Schmertz", "tokens": ["Mich", ",", "dein", "sonst", "treu\u00b7es", "Kind", ",", "viel\u00b7leicht", "mit", "Schmertz"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "ADV", "ADJA", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "zu missen,", "tokens": ["zu", "mis\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.43": {"text": "So, wie ein Donnerschlag die Sterblichen erschreckt,", "tokens": ["So", ",", "wie", "ein", "Don\u00b7ner\u00b7schlag", "die", "Sterb\u00b7li\u00b7chen", "er\u00b7schreckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wenn Hagel, Blitz und Dampff ihr d\u00fcrres Erd-", "tokens": ["Wenn", "Ha\u00b7gel", ",", "Blitz", "und", "Dampff", "ihr", "d\u00fcr\u00b7res", "Erd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "reich deckt,", "tokens": ["reich", "deckt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.46": {"text": "So, hefftig war bey mir, in dem so treuen Hertzen,", "tokens": ["So", ",", "heff\u00b7tig", "war", "bey", "mir", ",", "in", "dem", "so", "treu\u00b7en", "Hert\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VAFIN", "APPR", "PPER", "$,", "APPR", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Die \u00fcberh\u00e4ufte Qual bey \u00fcberh\u00e4uften Schmertzen.", "tokens": ["Die", "\u00fc\u00b7berh\u00b7\u00e4uf\u00b7te", "Qual", "bey", "\u00fc\u00b7berh\u00b7\u00e4uf\u00b7ten", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Ich fiel wohl hundertmal dir, Liebste! um den Hals.", "tokens": ["Ich", "fiel", "wohl", "hun\u00b7dert\u00b7mal", "dir", ",", "Liebs\u00b7te", "!", "um", "den", "Hals", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPER", "$,", "NN", "$.", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Des Traurens \u00e4chte Frucht, der Z\u00e4hren bittres", "tokens": ["Des", "Trau\u00b7rens", "\u00e4ch\u00b7te", "Frucht", ",", "der", "Z\u00e4h\u00b7ren", "bitt\u00b7res"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.50": {"text": "Saltz,", "tokens": ["Saltz", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.51": {"text": "Die\u00df muste, um dir nur die Redligkeit zu zollen,", "tokens": ["Die\u00df", "mus\u00b7te", ",", "um", "dir", "nur", "die", "Red\u00b7lig\u00b7keit", "zu", "zol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "KOUI", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Mit gr\u00f6ster Hefftigkeit von meinen Wangen rollen.", "tokens": ["Mit", "gr\u00f6s\u00b7ter", "Heff\u00b7tig\u00b7keit", "von", "mei\u00b7nen", "Wan\u00b7gen", "rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Da nun die Raserey den armen Geist bezwang,", "tokens": ["Da", "nun", "die", "Ra\u00b7se\u00b7rey", "den", "ar\u00b7men", "Geist", "be\u00b7zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Da die Verzweifelung fast mit dem Tode rang,", "tokens": ["Da", "die", "Ver\u00b7zwei\u00b7fe\u00b7lung", "fast", "mit", "dem", "To\u00b7de", "rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.55": {"text": "So fing sie freylich an sehr hefftig auszubrechen.", "tokens": ["So", "fing", "sie", "frey\u00b7lich", "an", "sehr", "heff\u00b7tig", "aus\u00b7zu\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADV", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Verwegner! rief ich aus, ich will mich an dir r\u00e4chen.", "tokens": ["Ver\u00b7weg\u00b7ner", "!", "rief", "ich", "aus", ",", "ich", "will", "mich", "an", "dir", "r\u00e4\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Sieh hier den blancken Stahl, der dir mit st\u00e4rcksten", "tokens": ["Sieh", "hier", "den", "blan\u00b7cken", "Stahl", ",", "der", "dir", "mit", "st\u00e4rcks\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.58": {"text": "Muth,", "tokens": ["Muth", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.59": {"text": "So viel er nur vermag, gerechten Einhalt thut.", "tokens": ["So", "viel", "er", "nur", "ver\u00b7mag", ",", "ge\u00b7rech\u00b7ten", "Ein\u00b7halt", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Dir, Ehrvergessenen, will ich gar balde zeigen,", "tokens": ["Dir", ",", "Ehr\u00b7ver\u00b7ges\u00b7se\u00b7nen", ",", "will", "ich", "gar", "bal\u00b7de", "zei\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Die Tugend m\u00fcsse nicht verdamten R\u00e4ubern weiche\u0303.", "tokens": ["Die", "Tu\u00b7gend", "m\u00fcs\u00b7se", "nicht", "ver\u00b7dam\u00b7ten", "R\u00e4u\u00b7bern", "weich\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "Ja fordre was du wilt, nimm Reichthum, Gl\u00fcck", "tokens": ["Ja", "ford\u00b7re", "was", "du", "wilt", ",", "nimm", "Reicht\u00b7hum", ",", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PTKANT", "VVFIN", "PWS", "PPER", "VMFIN", "$,", "VVIMP", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.63": {"text": "und Gut,", "tokens": ["und", "Gut", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.64": {"text": "Ja nim\u0303, o stoltzer Feind! mein warm erhitztes Blut,", "tokens": ["Ja", "nim\u0303", ",", "o", "stolt\u00b7zer", "Feind", "!", "mein", "warm", "er\u00b7hitz\u00b7tes", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$,", "FM", "ADJA", "NN", "$.", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Nur la\u00df mir meinen Schatz, wo nicht, so soll mein", "tokens": ["Nur", "la\u00df", "mir", "mei\u00b7nen", "Schatz", ",", "wo", "nicht", ",", "so", "soll", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "$,", "PWAV", "PTKNEG", "$,", "ADV", "VMFIN", "PPOSAT"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.66": {"text": "Degen", "tokens": ["De\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.67": {"text": "Dich, R\u00e4uber meiner Lust, im Augenblick erlegen.", "tokens": ["Dich", ",", "R\u00e4u\u00b7ber", "mei\u00b7ner", "Lust", ",", "im", "Au\u00b7gen\u00b7blick", "er\u00b7le\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "PPOSAT", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Und-- o! aber ach! wie sehr erschrack ich nicht!", "tokens": ["Un\u00b7d", "o", "!", "a\u00b7ber", "ach", "!", "wie", "sehr", "er\u00b7schrack", "ich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "FM", "$.", "ADV", "ADV", "$.", "PWAV", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Ein hell, ein gl\u00e4ntzendes, ein angenehmes Licht", "tokens": ["Ein", "hell", ",", "ein", "gl\u00e4nt\u00b7zen\u00b7des", ",", "ein", "an\u00b7ge\u00b7neh\u00b7mes", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Erleuchtete den Wald; Da ich geredet habe,", "tokens": ["Er\u00b7leuch\u00b7te\u00b7te", "den", "Wald", ";", "Da", "ich", "ge\u00b7re\u00b7det", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "So kam ein Anmuthsvoll, ein z\u00e4rtlich junger Knabe.", "tokens": ["So", "kam", "ein", "An\u00b7muths\u00b7voll", ",", "ein", "z\u00e4rt\u00b7lich", "jun\u00b7ger", "Kna\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Sein Ansehn schreckte mich, ja ich erstaunte gantz,", "tokens": ["Sein", "An\u00b7sehn", "schreck\u00b7te", "mich", ",", "ja", "ich", "er\u00b7staun\u00b7te", "gantz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "ADV", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Es war ", "tokens": ["Es", "war"], "token_info": ["word", "word"], "pos": ["PPER", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.74": {"text": "War Schuld, da\u00df ich verst\u00fcrtzt und unbeweglich", "tokens": ["War", "Schuld", ",", "da\u00df", "ich", "ver\u00b7st\u00fcrtzt", "und", "un\u00b7be\u00b7weg\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.75": {"text": "bliebe.", "tokens": ["blie\u00b7be", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.76": {"text": "Wie? Frecher! rieff er aus; wo ist nun deine Liebe?", "tokens": ["Wie", "?", "Fre\u00b7cher", "!", "rieff", "er", "aus", ";", "wo", "ist", "nun", "dei\u00b7ne", "Lie\u00b7be", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "$.", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.77": {"text": "Dein Muth war ja vorhin, o Stoltzer! ungemein,", "tokens": ["Dein", "Muth", "war", "ja", "vor\u00b7hin", ",", "o", "Stolt\u00b7zer", "!", "un\u00b7ge\u0