{"textgrid.poem.49661": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Westf\u00e4lische Kaisertage", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wirbelt der Staub, es tutet das Horn;", "tokens": ["Es", "wir\u00b7belt", "der", "Staub", ",", "es", "tu\u00b7tet", "das", "Horn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was rasselt und knattert und brauset da vorn?", "tokens": ["Was", "ras\u00b7selt", "und", "knat\u00b7tert", "und", "brau\u00b7set", "da", "vorn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dort kommt mit hellen Fanfaren", "tokens": ["Dort", "kommt", "mit", "hel\u00b7len", "Fan\u00b7fa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Kaiser, der Kaiser gefahren.", "tokens": ["Der", "Kai\u00b7ser", ",", "der", "Kai\u00b7ser", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Frau Lehmann und Schulz, ihr zittert so sehr,", "tokens": ["Frau", "Leh\u00b7mann", "und", "Schulz", ",", "ihr", "zit\u00b7tert", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NE", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nun kommt euer Herrscher im Sturme daher.", "tokens": ["Nun", "kommt", "eu\u00b7er", "Herr\u00b7scher", "im", "Stur\u00b7me", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "PAV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ihr d\u00fcrft jetzt das Sch\u00f6nste genie\u00dfen,", "tokens": ["Ihr", "d\u00fcrft", "jetzt", "das", "Sch\u00f6ns\u00b7te", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Den Kaiser, den Kaiser zu gr\u00fc\u00dfen.", "tokens": ["Den", "Kai\u00b7ser", ",", "den", "Kai\u00b7ser", "zu", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und knax-ta ratax, da f\u00e4hrt Er vorbei ...", "tokens": ["Und", "kna\u00b7xta", "ra\u00b7tax", ",", "da", "f\u00e4hrt", "Er", "vor\u00b7bei", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Erhebt eure Herzen, erhebt ein Geschrei!", "tokens": ["Er\u00b7hebt", "eu\u00b7re", "Her\u00b7zen", ",", "er\u00b7hebt", "ein", "Ge\u00b7schrei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure":