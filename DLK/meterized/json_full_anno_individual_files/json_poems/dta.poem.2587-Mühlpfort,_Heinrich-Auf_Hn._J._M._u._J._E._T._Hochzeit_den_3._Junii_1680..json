{"dta.poem.2587": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. J. M. u. J. E. T. Hochzeit den  3.  \n Junii 1680.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach lebte Tscherning noch/ der Ruhm gelehrter Zeiten!", "tokens": ["Ach", "leb\u00b7te", "Tscher\u00b7ning", "noch", "/", "der", "Ruhm", "ge\u00b7lehr\u00b7ter", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NE", "ADV", "$(", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der himmlische Poet/ des Vaterlandes Prei\u00df/", "tokens": ["Der", "himm\u00b7li\u00b7sche", "Po\u00b7et", "/", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Prei\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von dessen Kunst und Witz der Belth zu sagen wei\u00df/", "tokens": ["Von", "des\u00b7sen", "Kunst", "und", "Witz", "der", "Belth", "zu", "sa\u00b7gen", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df er gewesen sey ein Meister deutscher Seiten.", "tokens": ["Da\u00df", "er", "ge\u00b7we\u00b7sen", "sey", "ein", "Meis\u00b7ter", "deut\u00b7scher", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAPP", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach lebte Tscherning noch/ wie ehrt\u2019 er dieses Fest!", "tokens": ["Ach", "leb\u00b7te", "Tscher\u00b7ning", "noch", "/", "wie", "ehrt'", "er", "die\u00b7ses", "Fest", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NE", "ADV", "$(", "PWAV", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das euch Verlobte Zwey zusammen treten l\u00e4st.", "tokens": ["Das", "euch", "Ver\u00b7lob\u00b7te", "Zwey", "zu\u00b7sam\u00b7men", "tre\u00b7ten", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "NN", "CARD", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er w\u00fcrde/ wie er pflag/ den Helicon bewegen/", "tokens": ["Er", "w\u00fcr\u00b7de", "/", "wie", "er", "pflag", "/", "den", "He\u00b7li\u00b7con", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PWAV", "PPER", "VVFIN", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es m\u00fcste Castalis mit vollen Str\u00f6men gehn/", "tokens": ["Es", "m\u00fcs\u00b7te", "Cas\u00b7ta\u00b7lis", "mit", "vol\u00b7len", "Str\u00f6\u00b7men", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und ihm der Lorber-Wald zu dienst und willen stehn/", "tokens": ["Und", "ihm", "der", "Lor\u00b7ber\u00b7Wald", "zu", "dienst", "und", "wil\u00b7len", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "PDAT", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Damit er seiner Muhm als ", "tokens": ["Da\u00b7mit", "er", "sei\u00b7ner", "Muhm", "als"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KOKOM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Erkl\u00e4rte durch ein Lied/ weissagte Heil und Ruh/", "tokens": ["Er\u00b7kl\u00e4r\u00b7te", "durch", "ein", "Lied", "/", "weis\u00b7sag\u00b7te", "Heil", "und", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und Ph\u00f6bus spreche selbst ein kr\u00e4fftig ja darzu.", "tokens": ["Und", "Ph\u00f6\u00b7bus", "spre\u00b7che", "selbst", "ein", "kr\u00e4ff\u00b7tig", "ja", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADV", "ART", "ADJD", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Umbsonst! sein edler Geist wohnt nunmehr bey den Sternen/", "tokens": ["Um\u00b7bsonst", "!", "sein", "ed\u00b7ler", "Geist", "wohnt", "nun\u00b7mehr", "bey", "den", "Ster\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er hat die Sterbligkeit schon l\u00e4ngsten abgelegt/", "tokens": ["Er", "hat", "die", "Ster\u00b7blig\u00b7keit", "schon", "l\u00e4ngs\u00b7ten", "ab\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mich/ den kein heisser Zug der Dichter mehr bewegt/", "tokens": ["Mich", "/", "den", "kein", "heis\u00b7ser", "Zug", "der", "Dich\u00b7ter", "mehr", "be\u00b7wegt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ART", "PIAT", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der ich den Kindern gleich die Reim-Kunst wil verlernen/", "tokens": ["Der", "ich", "den", "Kin\u00b7dern", "gleich", "die", "Reim\u00b7Kunst", "wil", "ver\u00b7ler\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Brennt itzt kein s\u00fcsses Feur/ das solche Funcken spr\u00fcht/", "tokens": ["Brennt", "itzt", "kein", "s\u00fcs\u00b7ses", "Feur", "/", "das", "sol\u00b7che", "Fun\u00b7cken", "spr\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "$(", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Worinnen man gebildt der Liebe Sch\u00f6nheit sieht.", "tokens": ["Wo\u00b7rin\u00b7nen", "man", "ge\u00b7bildt", "der", "Lie\u00b7be", "Sch\u00f6n\u00b7heit", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Jedoch die Freude wil die schwache Feder f\u00fchren/", "tokens": ["Je\u00b7doch", "die", "Freu\u00b7de", "wil", "die", "schwa\u00b7che", "Fe\u00b7der", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn ich die Eltern seh bey ihren Kindern stehn/", "tokens": ["Wenn", "ich", "die", "El\u00b7tern", "seh", "bey", "ih\u00b7ren", "Kin\u00b7dern", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und wie sie mit der ", "tokens": ["Und", "wie", "sie", "mit", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.23": {"text": "Und seufftzen da zu GOtt/ dem Stiffter keuscher Eh/", "tokens": ["Und", "seufft\u00b7zen", "da", "zu", "Gott", "/", "dem", "Stiff\u00b7ter", "keu\u00b7scher", "Eh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df es doch ihrem Blut und Saamen wolergeh.", "tokens": ["Da\u00df", "es", "doch", "ih\u00b7rem", "Blut", "und", "Saa\u00b7men", "wo\u00b7ler\u00b7geh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "KON", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es schmiltzt der ", "tokens": ["Es", "schmiltzt", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Nun sich ihr Ehren-Schnee mit Wolfahrts-Lilgen schm\u00fcckt/", "tokens": ["Nun", "sich", "ihr", "Eh\u00b7ren\u00b7Schnee", "mit", "Wolfahrts\u00b7Lil\u00b7gen", "schm\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Und es der grosse Rath des Himmels hat geschickt/", "tokens": ["Und", "es", "der", "gros\u00b7se", "Rath", "des", "Him\u00b7mels", "hat", "ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df sich in gleiche Treu zwey gleiche Seelen kleiden/", "tokens": ["Da\u00df", "sich", "in", "glei\u00b7che", "Treu", "zwey", "glei\u00b7che", "See\u00b7len", "klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN", "CARD", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und da\u00df ein B\u00fcrgers Kind dem andern sich verm\u00e4hlt/", "tokens": ["Und", "da\u00df", "ein", "B\u00fcr\u00b7gers", "Kind", "dem", "an\u00b7dern", "sich", "ver\u00b7m\u00e4hlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "ART", "ADJA", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und in der Gleichheit auch noch Stand noch Uhrsprung fehlt.", "tokens": ["Und", "in", "der", "Gleich\u00b7heit", "auch", "noch", "Stand", "noch", "Uhrs\u00b7prung", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So baut ihr ", "tokens": ["So", "baut", "ihr"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.32": {"text": "Und f\u00fchret Seulen auff aus eurem eignen Blut/", "tokens": ["Und", "f\u00fch\u00b7ret", "Seu\u00b7len", "auff", "aus", "eu\u00b7rem", "eig\u00b7nen", "Blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Denn fromme Kinder sind doch nur das h\u00f6chste Gut.", "tokens": ["Denn", "from\u00b7me", "Kin\u00b7der", "sind", "doch", "nur", "das", "h\u00f6chs\u00b7te", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wer die gezeuget hat/ den ehren gar die Rechte/", "tokens": ["Wer", "die", "ge\u00b7zeu\u00b7get", "hat", "/", "den", "eh\u00b7ren", "gar", "die", "Rech\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "VVPP", "VAFIN", "$(", "ART", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Jhr wist wem ihr den Schwei\u00df von euren H\u00e4nden last/", "tokens": ["Ihr", "wist", "wem", "ihr", "den", "Schwei\u00df", "von", "eu\u00b7ren", "H\u00e4n\u00b7den", "last", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und Erb und Eigemhum besitzt kein frembder Gast.", "tokens": ["Und", "Erb", "und", "Ei\u00b7gem\u00b7hum", "be\u00b7sitzt", "kein", "fremb\u00b7der", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Last ietzt die Sorgen seyn ihr hochbegl\u00fcckten ", "tokens": ["Last", "ietzt", "die", "Sor\u00b7gen", "seyn", "ihr", "hoch\u00b7be\u00b7gl\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VAINF", "PPOSAT", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.38": {"text": "Der m\u00fcden Kr\u00e4ffte Stab ist ein geliebter Sohn/", "tokens": ["Der", "m\u00fc\u00b7den", "Kr\u00e4ff\u00b7te", "Stab", "ist", "ein", "ge\u00b7lieb\u00b7ter", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und keusche T\u00f6chter sind der Eltern Schmuck und Cron.", "tokens": ["Und", "keu\u00b7sche", "T\u00f6ch\u00b7ter", "sind", "der", "El\u00b7tern", "Schmuck", "und", "Cron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die Gunst des Himmels wil ob eurem Namen walten/", "tokens": ["Die", "Gunst", "des", "Him\u00b7mels", "wil", "ob", "eu\u00b7rem", "Na\u00b7men", "wal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Jhr k\u00f6nt nicht untergehn/ ob euch das Grab schon h\u00e4lt/", "tokens": ["Ihr", "k\u00f6nt", "nicht", "un\u00b7ter\u00b7gehn", "/", "ob", "euch", "das", "Grab", "schon", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$(", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Dieweil ihr wieder lebt durch eurer Enckel Welt.", "tokens": ["Die\u00b7weil", "ihr", "wie\u00b7der", "lebt", "durch", "eu\u00b7rer", "En\u00b7ckel", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Nun zehlet diesen Tag vor einen von den besten.", "tokens": ["Nun", "zeh\u00b7let", "die\u00b7sen", "Tag", "vor", "ei\u00b7nen", "von", "den", "bes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "APPR", "ART", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Allein/ wo lassen wir denn unsre sch\u00f6ne ", "tokens": ["Al\u00b7lei