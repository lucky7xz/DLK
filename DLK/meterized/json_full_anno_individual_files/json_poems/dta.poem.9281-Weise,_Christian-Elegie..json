{"dta.poem.9281": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Elegie.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "So mu\u00df ich liebster freund ihn wieder irre machen/", "tokens": ["So", "mu\u00df", "ich", "liebs\u00b7ter", "freund", "ihn", "wie\u00b7der", "ir\u00b7re", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "PPER", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich sein nahmens-fest so zeitlich feyren wil?", "tokens": ["Da\u00df", "ich", "sein", "nah\u00b7mens\u00b7fest", "so", "zeit\u00b7lich", "fey\u00b7ren", "wil", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja freylich wenn bey mir die geister schon erwachen/", "tokens": ["Ja", "frey\u00b7lich", "wenn", "bey", "mir", "die", "geis\u00b7ter", "schon", "er\u00b7wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "KOUS", "APPR", "PPER", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So fodern sie die schuld/ das matte seitenspiel.", "tokens": ["So", "fo\u00b7dern", "sie", "die", "schuld", "/", "das", "mat\u00b7te", "sei\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wiewol es ist an dem/ ich bin nicht so vergessen/", "tokens": ["Wie\u00b7wol", "es", "ist", "an", "dem", "/", "ich", "bin", "nicht", "so", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "ART", "$(", "PPER", "VAFIN", "PTKNEG", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es ist ein halbes jahr und noch kaum gantz vorbey/", "tokens": ["Es", "ist", "ein", "hal\u00b7bes", "jahr", "und", "noch", "kaum", "gantz", "vor\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "ADV", "ADV", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da hab ich eben so bey meiner pflicht gesessen:", "tokens": ["Da", "hab", "ich", "e\u00b7ben", "so", "bey", "mei\u00b7ner", "pflicht", "ge\u00b7ses\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und itzo mach ich schon die freude wieder neu/", "tokens": ["Und", "it\u00b7zo", "mach", "ich", "schon", "die", "freu\u00b7de", "wie\u00b7der", "neu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es ist der liebe schuld/ die wil vor \u00e4ngsten sterben/", "tokens": ["Es", "ist", "der", "lie\u00b7be", "schuld", "/", "die", "wil", "vor", "\u00e4ngs\u00b7ten", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "VMFIN", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn sie ein liebes haupt nicht stets bedienen sol/", "tokens": ["Wenn", "sie", "ein", "lie\u00b7bes", "haupt", "nicht", "stets", "be\u00b7die\u00b7nen", "sol", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und kan sie durch den dienst nicht was sie w\u00fcnscht erwerbe\u0303/", "tokens": ["Und", "kan", "sie", "durch", "den", "dienst", "nicht", "was", "sie", "w\u00fcnscht", "er\u00b7wer\u00b7b\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "ART", "ADV", "PTKNEG", "PWS", "PPER", "VVFIN", "NE", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "Nun so gef\u00e4llt ihr doch die m\u00fch und arbeit wohl.", "tokens": ["Nun", "so", "ge\u00b7f\u00e4llt", "ihr", "doch", "die", "m\u00fch", "und", "ar\u00b7beit", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJD", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bey freunden kan ich nicht mein courtisiren lassen/", "tokens": ["Bey", "freun\u00b7den", "kan", "ich", "nicht", "mein", "cour\u00b7ti\u00b7si\u00b7ren", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie haben mein gem\u00fcth und alles in der hand:", "tokens": ["Sie", "ha\u00b7ben", "mein", "ge\u00b7m\u00fcth", "und", "al\u00b7les", "in", "der", "hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein ander geh die spur der eitlen liebes-strassen/", "tokens": ["Ein", "an\u00b7der", "geh", "die", "spur", "der", "eit\u00b7len", "lie\u00b7bes\u00b7stras\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mein hertz ist doch allein in einen freund entbrant.", "tokens": ["Mein", "hertz", "ist", "doch", "al\u00b7lein", "in", "ei\u00b7nen", "freund", "ent\u00b7brant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zwar wo ich bitten darff/ so seh er nicht die worte/", "tokens": ["Zwar", "wo", "ich", "bit\u00b7ten", "darff", "/", "so", "seh", "er", "nicht", "die", "wor\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPER", "VVINF", "VMFIN", "$(", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den ungeschickten reim die schlechten zeilen an/", "tokens": ["Den", "un\u00b7ge\u00b7schick\u00b7ten", "reim", "die", "schlech\u00b7ten", "zei\u00b7len", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Jtz an der messe liegt nichts an dem rechten orte:", "tokens": ["Jtz", "an", "der", "mes\u00b7se", "liegt", "nichts", "an", "dem", "rech\u00b7ten", "or\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Kaum da\u00df ich so viel zeit zum schreiben stehlen kan:", "tokens": ["Kaum", "da\u00df", "ich", "so", "viel", "zeit", "zum", "schrei\u00b7ben", "steh\u00b7len", "kan", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PIAT", "NN", "APPRART", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn es zu schmeicheln komt/ so bin ich sonst verdorben/", "tokens": ["Wenn", "es", "zu", "schmei\u00b7cheln", "komt", "/", "so", "bin", "ich", "sonst", "ver\u00b7dor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ich f\u00fchr in meiner haut den alten teutschen sinn/", "tokens": ["Ich", "f\u00fchr", "in", "mei\u00b7ner", "haut", "den", "al\u00b7ten", "teut\u00b7schen", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und w\u00e4r die redligkeit gleich \u00fcberall verdorben/", "tokens": ["Und", "w\u00e4r", "die", "red\u00b7lig\u00b7keit", "gleich", "\u00fc\u00b7be\u00b7rall", "ver\u00b7dor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So bleibt es noch darbey/ da\u00df ich noch ehrlich bin.", "tokens": ["So", "bleibt", "es", "noch", "dar\u00b7bey", "/", "da\u00df", "ich", "noch", "ehr\u00b7lich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "$(", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Doch wil er einen schertz von meiner feder fodern/", "tokens": ["Doch", "wil", "er", "ei\u00b7nen", "schertz", "von", "mei\u00b7ner", "fe\u00b7der", "fo\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich wei\u00df nicht ob die lust mich gantz verlassen hat/", "tokens": ["Ich", "wei\u00df", "nicht", "ob", "die", "lust", "mich", "gantz", "ver\u00b7las\u00b7sen", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "ART", "VVFIN", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der gr\u00fcne myrtenstrauch will allgemach verlodern/", "tokens": ["Der", "gr\u00fc\u00b7ne", "myr\u00b7ten\u00b7strauch", "will", "all\u00b7ge\u00b7mach", "ver\u00b7lo\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und mein betr\u00fcbter geist ist fast des lachens satt/", "tokens": ["Und", "mein", "be\u00b7tr\u00fcb\u00b7ter", "geist", "ist", "fast", "des", "la\u00b7chens", "satt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Er frage nicht warum? er schaue nur die sachen", "tokens": ["Er", "fra\u00b7ge", "nicht", "wa\u00b7rum", "?", "er", "schau\u00b7e", "nur", "die", "sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der jungen erden an/ wie itzo wald und feld/", "tokens": ["Der", "jun\u00b7gen", "er\u00b7den", "an", "/", "wie", "it\u00b7zo", "wald", "und", "feld", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$(", "KOKOM", "ADV", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Fisch/ v\u00f6gel/ thier und alls mit freuden hochzeit machen/", "tokens": ["Fisch", "/", "v\u00f6\u00b7gel", "/", "thier", "und", "alls", "mit", "freu\u00b7den", "hoch\u00b7zeit", "ma\u00b7chen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$(", "NN", "KON", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Nur ich bekomme nichts ich arme kleine welt.", "tokens": ["Nur", "ich", "be\u00b7kom\u00b7me", "nichts", "ich", "ar\u00b7me", "klei\u00b7ne", "welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PIS", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich soll mich um die frau noch gar zu tode gr\u00e4men.", "tokens": ["Ich", "soll", "mich", "um", "die", "frau", "noch", "gar", "zu", "to\u00b7de", "gr\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Denn wo ein guter freund zur hochzeit blasen l\u00e4\u00dft/", "tokens": ["Denn", "wo", "ein", "gu\u00b7ter", "freund", "zur", "hoch\u00b7zeit", "bla\u00b7sen", "l\u00e4\u00dft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So mu\u00df ich mich f\u00fcrwar vor meinem barte sch\u00e4men", "tokens": ["So", "mu\u00df", "ich", "mich", "f\u00fcr\u00b7war", "vor", "mei\u00b7nem", "bar\u00b7te", "sch\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der wol so stachlicht ist als manches schwalben-nest.", "tokens": ["Der", "wol", "so", "stach\u00b7licht", "ist", "als", "man\u00b7ches", "schwal\u00b7ben\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "KOKOM", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.37": {"text": "Der schwartzdorn bl\u00fcth nunmehr/ der k\u00fctzel sticht die leute/", "tokens": ["Der", "schwartz\u00b7dorn", "bl\u00fcth", "nun\u00b7mehr", "/", "der", "k\u00fct\u00b7zel", "sticht", "die", "leu\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$(", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das alte winterblut wil zu den adern au\u00df/", "tokens": ["Das", "al\u00b7te", "win\u00b7ter\u00b7blut", "wil", "zu", "den", "a\u00b7dern", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der k\u00fchle rosenthal tr\u00e4gt schon die fr\u00fchlings-beute/", "tokens": ["Der", "k\u00fch\u00b7le", "ro\u00b7sen\u00b7thal", "tr\u00e4gt", "schon", "die", "fr\u00fch\u00b7lings\u00b7beu\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und f\u00fchret zwey hinein und dreye wieder nau\u00df/", "tokens": ["Und", "f\u00fch\u00b7ret", "zwey", "hin\u00b7ein", "und", "drey\u00b7e", "wie\u00b7der", "nau\u00df", "/"], "toke