{"textgrid.poem.59315": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Nachtblume", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nacht ist wie ein stilles Meer,", "tokens": ["Nacht", "ist", "wie", "ein", "stil\u00b7les", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lust und Leid und Liebesklagen", "tokens": ["Lust", "und", "Leid", "und", "Lie\u00b7bes\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommen so verworren her", "tokens": ["Kom\u00b7men", "so", "ver\u00b7wor\u00b7ren", "her"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem linden Wellenschlagen.", "tokens": ["In", "dem", "lin\u00b7den", "Wel\u00b7len\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00fcnsche wie die Wolken sind,", "tokens": ["W\u00fcn\u00b7sche", "wie", "die", "Wol\u00b7ken", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schiffen durch die stillen R\u00e4ume,", "tokens": ["Schif\u00b7fen", "durch", "die", "stil\u00b7len", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer erkennt im lauen Wind,", "tokens": ["Wer", "er\u00b7kennt", "im", "lau\u00b7en", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob's Gedanken oder Tr\u00e4ume? \u2013", "tokens": ["Ob's", "Ge\u00b7dan\u00b7ken", "o\u00b7der", "Tr\u00e4u\u00b7me", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "KON", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schlie\u00df ich nun auch Herz und Mund,", "tokens": ["Schlie\u00df", "ich", "nun", "auch", "Herz", "und", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so gern den Sternen klagen:", "tokens": ["Die", "so", "gern", "den", "Ster\u00b7nen", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Leise doch im Herzensgrund", "tokens": ["Lei\u00b7se", "doch", "im", "Her\u00b7zens\u00b7grund"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bleibt das linde Wellenschlagen.", "tokens": ["Bleibt", "das", "lin\u00b7de", "Wel\u00b7len\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nacht ist wie ein stilles Meer,", "tokens": ["Nacht", "ist", "wie", "ein", "stil\u00b7les", "Meer