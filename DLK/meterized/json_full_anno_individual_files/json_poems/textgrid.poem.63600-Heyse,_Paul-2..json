{"textgrid.poem.63600": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchlt' in meinen hellsten Lebenstagen", "tokens": ["Ich", "f\u00fchlt'", "in", "mei\u00b7nen", "hells\u00b7ten", "Le\u00b7bens\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Mut, auf diese Sonne zu verzichten", "tokens": ["Den", "Mut", ",", "auf", "die\u00b7se", "Son\u00b7ne", "zu", "ver\u00b7zich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PDAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und der Vernichtung Schauer zu ertragen.", "tokens": ["Und", "der", "Ver\u00b7nich\u00b7tung", "Schau\u00b7er", "zu", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Jetzt und Hier lernt' ich mich einzurichten,", "tokens": ["Im", "Jetzt", "und", "Hier", "lernt'", "ich", "mich", "ein\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "KON", "ADV", "VVFIN", "PPER", "PRF", "VVIZU", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und selbst die Neugier nach dem letzten Wort", "tokens": ["Und", "selbst", "die", "Neu\u00b7gier", "nach", "dem", "letz\u00b7ten", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des bunten R\u00e4tsels lockte mich mit nichten.", "tokens": ["Des", "bun\u00b7ten", "R\u00e4t\u00b7sels", "lock\u00b7te", "mich", "mit", "nich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wohl ri\u00df die Brandung manchen vor mir fort,", "tokens": ["Wohl", "ri\u00df", "die", "Bran\u00b7dung", "man\u00b7chen", "vor", "mir", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der, w\u00e4hrend ich noch trieb auf hoher See,", "tokens": ["Der", ",", "w\u00e4h\u00b7rend", "ich", "noch", "trieb", "auf", "ho\u00b7her", "See", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Still Anker warf im ew'gen Ruheport.", "tokens": ["Still", "An\u00b7ker", "warf", "im", "ew'\u00b7gen", "Ruh\u00b7e\u00b7port", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nach Sturm und Not und namenlosem Weh,", "tokens": ["Nach", "Sturm", "und", "Not", "und", "na\u00b7men\u00b7lo\u00b7sem", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Oft an ein letztes morsches Brett gebunden", "tokens": ["Oft", "an", "ein", "letz\u00b7tes", "mor\u00b7sches", "Brett", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und f\u00fchlend, da\u00df auch das in Tr\u00fcmmer geh',", "tokens": ["Und", "f\u00fch\u00b7lend", ",", "da\u00df", "auch", "das", "in", "Tr\u00fcm\u00b7mer", "geh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "KOUS", "ADV", "ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie sahn sie gern der Irrfahrt sich entbunden,", "tokens": ["Wie", "sahn", "sie", "gern", "der", "Irr\u00b7fahrt", "sich", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die wenig Freude bot und viele Last;", "tokens": ["Die", "we\u00b7nig", "Freu\u00b7de", "bot", "und", "vie\u00b7le", "Last", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Wie g\u00f6nnt' ich's ihnen, da\u00df sie \u00bbLand\u00ab gefunden!", "tokens": ["Wie", "g\u00f6nnt'", "ich's", "ih\u00b7nen", ",", "da\u00df", "sie", "\u00bb", "Land", "\u00ab", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PPER", "$,", "KOUS", "PPER", "$(", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sie hofften nichts mehr, als zu ruhn, und fast", "tokens": ["Sie", "hoff\u00b7ten", "nichts", "mehr", ",", "als", "zu", "ruhn", ",", "und", "fast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$,", "KOUS", "PTKZU", "VVINF", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Mit Ingrimm h\u00f6rten sie die Tr\u00f6ster sagen,", "tokens": ["Mit", "In\u00b7grimm", "h\u00f6r\u00b7ten", "sie", "die", "Tr\u00f6s\u00b7ter", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Man wecke sie dereinst nach kurzer Rast.", "tokens": ["Man", "we\u00b7cke", "sie", "de\u00b7reinst", "nach", "kur\u00b7zer", "Rast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Doch du, mein Knabe, dem noch offen lagen", "tokens": ["Doch", "du", ",", "mein", "Kna\u00b7be", ",", "dem", "noch", "of\u00b7fen", "la\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die Meer' und L\u00e4nder, Mut die Segel schwellt',", "tokens": ["Die", "Meer'", "und", "L\u00e4n\u00b7der", ",", "Mut", "die", "Se\u00b7gel", "schwellt'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ein Inselland der Sel'gen zu erjagen,", "tokens": ["Ein", "In\u00b7sel\u00b7land", "der", "Sel'\u00b7gen", "zu", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wie ward dein leichter Nachen fr\u00fch zerschellt,", "tokens": ["Wie", "ward", "dein", "leich\u00b7ter", "Na\u00b7chen", "fr\u00fch", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und was die Sonne dir an Bl\u00fcten gab,", "tokens": ["Und", "was", "die", "Son\u00b7ne", "dir", "an", "Bl\u00fc\u00b7ten", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Verschlungen von der dunklen Wasserwelt!", "tokens": ["Ver\u00b7schlun\u00b7gen", "von", "der", "dunk\u00b7len", "Was\u00b7ser\u00b7welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und sollte nicht an deinem jungen Grab", "tokens": ["Und", "soll\u00b7te", "nicht", "an", "dei\u00b7nem", "jun\u00b7gen", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Das alte M\u00e4rchen sehnlich mich beschleichen:", "tokens": ["Das", "al\u00b7te", "M\u00e4r\u00b7chen", "sehn\u00b7lich", "mich", "be\u00b7schlei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Einst dring' ein Weckruf in die Nacht hinab,", "tokens": ["Einst", "dring'", "ein", "Weck\u00b7ruf", "in", "die", "Nacht", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Ein Hahnenschrei aus ew'gen Lebensreichen,", "tokens": ["Ein", "Hah\u00b7nen\u00b7schrei", "aus", "ew'\u00b7gen", "Le\u00b7bens\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Den Tag verk\u00fcndend, dem die Macht verliehn,", "tokens": ["Den", "Tag", "ver\u00b7k\u00fcn\u00b7dend", ",", "dem", "die", "Macht", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Des Erdentages Unbill auszugleichen?", "tokens": ["Des", "Er\u00b7den\u00b7ta\u00b7ges", "Un\u00b7bill", "aus\u00b7zu\u00b7glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Dann d\u00fcrft' ich wieder an mein Herz dich ziehn,", "tokens": ["Dann", "d\u00fcrft'", "ich", "wie\u00b7der", "an", "mein", "Herz", "dich", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Den frischen Mund dir, den geliebten, k\u00fcssen.", "tokens": ["Den", "fri\u00b7schen", "Mund", "dir", ",", "den", "ge\u00b7lieb\u00b7ten", ",", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "$,", "ART", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "(nun d\u00fcnkt mich's, ach, zu selten k\u00fc\u00dft' ich ihn!)", "tokens": ["(", "nun", "d\u00fcnkt", "mich's", ",", "ach", ",", "zu", "sel\u00b7ten", "k\u00fc\u00dft'", "ich", "ihn", "!", ")"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIS", "$,", "ITJ", "$,", "PTKA", "ADJD", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Die hier entbl\u00e4ttert sank zu meinen F\u00fc\u00dfen,", "tokens": ["Die", "hier", "ent\u00b7bl\u00e4t\u00b7tert", "sank", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Die Knospe s\u00e4h' ich dort sich rein entfalten,", "tokens": ["Die", "Knos\u00b7pe", "s\u00e4h'", "ich", "dort", "sich", "rein", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Mit Duft mein neues Leben zu vers\u00fc\u00dfen! \u2013", "tokens": ["Mit", "Duft", "mein", "neu\u00b7es", "Le\u00b7ben", "zu", "ver\u00b7s\u00fc\u00b7\u00dfen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Hinweg den Schleier, den ich fern gehalten", "tokens": ["Hin\u00b7weg", "den", "Schlei\u00b7er", ",", "den", "ich", "fern", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Vom ", "tokens": ["Vom"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}, "line.39": {"text": "Auch jetzt nicht trocknen mit den weichen Falten.", "tokens": ["Auch", "jetzt", "nicht", "trock\u00b7nen", "mit", "den", "wei\u00b7chen", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADJA", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Kein Einst und Dr\u00fcben, nur ein Jetzt und Hier.", "tokens": ["Kein", "Einst", "und", "Dr\u00fc\u00b7ben", ",", "nur", "ein", "Jetzt", "und", "Hier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,", "ADV", "ART", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Erbetteln will ich nicht vom Selbstbetrug", "tokens": ["Er\u00b7bet\u00b7teln", "will", "ich", "nicht", "vom", "Selbst\u00b7be\u00b7trug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Den feigen Trost. Das eine wissen wir:", "tokens": ["Den", "fei\u00b7gen", "Trost", ".", "Das", "ei\u00b7ne", "wis\u00b7sen", "wir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Auch wir vergehn; und das ist Trost genug.", "tokens": ["Auch", "wir", "ver\u00b7gehn", ";", "und", "das", "ist", "Trost", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "$.", "KON", "PDS", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich f\u00fchlt' in meinen hellsten Lebenstagen", "tokens": ["Ich", "f\u00fchlt'", "in", "mei\u00b7nen", "hells\u00b7ten", "Le\u00b7bens\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Mut, auf diese Sonne zu verzichten", "tokens": ["Den", "Mut", ",", "auf", "die\u00b7se", "Son\u00b7ne", "zu", "ver\u00b7zich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PDAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und der Vernichtung Schauer zu ertragen.", "tokens": ["Und", "der", "Ver\u00b7nich\u00b7tung", "Schau\u00b7er", "zu", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Jetzt und Hier lernt' ich mich einzurichten,", "tokens": ["Im", "Jetzt", "und", "Hier", "lernt'", "ich", "mich", "ein\u00b7zu\u00b7rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "KON", "ADV", "VVFIN", "PPER", "PRF", "VVIZU", "$,"], "meter": "-+--+--+-+-", "measure": "amphi