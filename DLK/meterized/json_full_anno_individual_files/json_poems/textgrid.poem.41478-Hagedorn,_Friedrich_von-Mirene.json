{"textgrid.poem.41478": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Mirene", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mirene stand an einer Quelle,", "tokens": ["Mi\u00b7re\u00b7ne", "stand", "an", "ei\u00b7ner", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei welcher sch\u00f6ne Veilchen bl\u00fchn,", "tokens": ["Bei", "wel\u00b7cher", "sch\u00f6\u00b7ne", "Veil\u00b7chen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sah um rasche Wasserf\u00e4lle", "tokens": ["Und", "sah", "um", "ra\u00b7sche", "Was\u00b7ser\u00b7f\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ungez\u00e4hlte Heerde ziehn.", "tokens": ["Die", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Heer\u00b7de", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die z\u00e4hlte sie mit wenig Freude,", "tokens": ["Die", "z\u00e4hl\u00b7te", "sie", "mit", "we\u00b7nig", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sprach: Kaum da\u00df ich's dulden kann;", "tokens": ["Und", "sprach", ":", "Kaum", "da\u00df", "ich's", "dul\u00b7den", "kann", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bei allen Weibchen, die ich weide,", "tokens": ["Bei", "al\u00b7len", "Weib\u00b7chen", ",", "die", "ich", "wei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Treff' ich nur einen Widder an.", "tokens": ["Treff'", "ich", "nur", "ei\u00b7nen", "Wid\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Will meine Mutter mich nur h\u00f6ren,", "tokens": ["Will", "mei\u00b7ne", "Mut\u00b7ter", "mich", "nur", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Schafe, so gelob' ich euch,", "tokens": ["Ihr", "Scha\u00b7fe", ",", "so", "ge\u00b7lob'", "ich", "euch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will bald euer Wohl vermehren,", "tokens": ["Ich", "will", "bald", "eu\u00b7er", "Wohl", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meines auch vielleicht zugleich.", "tokens": ["Und", "mei\u00b7nes", "auch", "viel\u00b7leicht", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich kenne schon aus eignem Triebe,", "tokens": ["Ich", "ken\u00b7ne", "schon", "aus", "eig\u00b7nem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ungerecht das Gl\u00fcck verf\u00e4hrt,", "tokens": ["Wie", "un\u00b7ge\u00b7recht", "das", "Gl\u00fcck", "ver\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wann es der Jugend und der Liebe", "tokens": ["Wann", "es", "der", "Ju\u00b7gend", "und", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Freiheit und die Wahl verwehrt.", "tokens": ["Die", "Frei\u00b7heit", "und", "die", "Wahl", "ver\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nichts auf der Welt ist fast verliebter,", "tokens": ["Nichts", "auf", "der", "Welt", "ist", "fast", "ver\u00b7lieb\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als Damon, der sich mir geweiht:", "tokens": ["Als", "Da\u00b7mon", ",", "der", "sich", "mir", "ge\u00b7weiht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "PRELS", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch auf der Welt ist nichts betr\u00fcbter,", "tokens": ["Doch", "auf", "der", "Welt", "ist", "nichts", "be\u00b7tr\u00fcb\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PIS", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als seine trockne Z\u00e4rtlichkeit.", "tokens": ["Als", "sei\u00b7ne", "trock\u00b7ne", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er folgt mir, wo ich geh' und stehe,", "tokens": ["Er", "folgt", "mir", ",", "wo", "ich", "geh'", "und", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und kennet noch nicht meine Brust.", "tokens": ["Und", "ken\u00b7net", "noch", "nicht", "mei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein solches Leben gleicht der Ehe:", "tokens": ["Ein", "sol\u00b7ches", "Le\u00b7ben", "gleicht", "der", "E\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Allein, ihm fehlt noch ihre Lust.", "tokens": ["Al\u00b7lein", ",", "ihm", "fehlt", "noch", "ih\u00b7re", "Lust", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er schneidet in die nahen Linden", "tokens": ["Er", "schnei\u00b7det", "in", "die", "na\u00b7hen", "Lin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wol zehnmal meines Namens Zug.", "tokens": ["Wol", "zehn\u00b7mal", "mei\u00b7nes", "Na\u00b7mens", "Zug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die M\u00fche kann mich zwar verbinden,", "tokens": ["Die", "M\u00fc\u00b7he", "kann", "mich", "zwar", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihm scheint auch mein Dank genug.", "tokens": ["Und", "ihm", "scheint", "auch", "mein", "Dank", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mein Lob erklingt auf seiner Leier;", "tokens": ["Mein", "Lob", "er\u00b7klingt", "auf", "sei\u00b7ner", "Lei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich wecket oft sein Saitenspiel:", "tokens": ["Mich", "we\u00b7cket", "oft", "sein", "Sai\u00b7ten\u00b7spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hingegen wird er nimmer freier,", "tokens": ["Hin\u00b7ge\u00b7gen", "wird", "er", "nim\u00b7mer", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und ehret mich vielleicht zu viel.", "tokens": ["Und", "eh\u00b7ret", "mich", "viel\u00b7leicht", "zu", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich ehrt' und liebt' ihn selbst vor Zeiten:", "tokens": ["Ich", "ehrt'", "und", "liebt'", "ihn", "selbst", "vor", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das aber that ich als ein Kind.", "tokens": ["Das", "a\u00b7ber", "that", "ich", "als", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun wachs' ich auf, und gleiche Leuten,", "tokens": ["Nun", "wachs'", "ich", "auf", ",", "und", "glei\u00b7che", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die kl\u00fcger und erfahrner sind.", "tokens": ["Die", "kl\u00fc\u00b7ger", "und", "er\u00b7fahr\u00b7ner", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wahr ist's: mir hat er sich verschrieben.", "tokens": ["Wahr", "ist's", ":", "mir", "hat", "er", "sich", "ver\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Soll ich daraus die Folge ziehn:", "tokens": ["Soll", "ich", "da\u00b7raus", "die", "Fol\u00b7ge", "ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich m\u00fcsse Damon ewig lieben,", "tokens": ["Ich", "m\u00fcs\u00b7se", "Da\u00b7mon", "e\u00b7wig", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und keinen lieben, als nur ihn?", "tokens": ["Und", "kei\u00b7nen", "lie\u00b7ben", ",", "als", "nur", "ihn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "$,", "KOUS", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Will hier ein Sch\u00e4fer sich erfreuen:", "tokens": ["Will", "hier", "ein", "Sch\u00e4\u00b7fer", "sich", "er\u00b7freu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "(mich d\u00e4ucht, ich merk' es ziemlich oft,)", "tokens": ["(", "mich", "d\u00e4ucht", ",", "ich", "merk'", "es", "ziem\u00b7lich", "oft", ",", ")"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So f\u00fchret er mich zu den Reihen,", "tokens": ["So", "f\u00fch\u00b7ret", "er", "mich", "zu", "den", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tanzt und k\u00fc\u00dft mich unverhofft.", "tokens": ["Und", "tanzt", "und", "k\u00fc\u00dft", "mich", "un\u00b7ver\u00b7hofft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein einz'ger scheint mir zu gefallen.", "tokens": ["Ein", "einz'\u00b7ger", "scheint", "mir", "zu", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verr\u00e4th mir Damon seinen Neid,", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "mir", "Da\u00b7mon", "sei\u00b7nen", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ihr Sch\u00e4fer: ja, so g\u00f6nn ich allen", "tokens": ["Ihr", "Sch\u00e4\u00b7fer", ":", "ja", ",", "so", "g\u00f6nn", "ich", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PTKANT", "$,", "ADV", "VVFIN", "PPER", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Ku\u00df, den Damon mir verbeut.", "tokens": ["Den", "Ku\u00df", ",", "den", "Da\u00b7mon", "mir", "ver\u00b7beut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mirene stand an einer Quelle,", "tokens": ["Mi\u00b7re\u00b7ne", "stand", "an", "ei\u00b7ner", "Quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bei welcher sch\u00f6ne Veilchen bl\u00fchn,", "tokens": ["Bei", "wel\u00b7cher", "sch\u00f6\u00b7ne", "Veil\u00b7chen", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sah um rasche Wasserf\u00e4lle", "tokens": ["Und", "sah", "um", "ra\u00b7sche", "Was\u00b7ser\u00b7f\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ungez\u00e4hlte Heerde ziehn.", "tokens": ["Die", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Heer\u00b7de", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die z\u00e4hlte sie mit wenig Freude,", "tokens": ["Die", "z\u00e4hl\u00b7te", "sie", "mit", "we\u00b7nig", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und sprach: K