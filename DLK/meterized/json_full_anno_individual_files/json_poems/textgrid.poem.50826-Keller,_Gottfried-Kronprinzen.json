s": ["Schlen\u00b7dern", "heim\u00b7w\u00e4rts", "wohl\u00b7ge\u00b7mut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch schon tropft es auf die Nase \u2013", "tokens": ["Doch", "schon", "tropft", "es", "auf", "die", "Na\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spute sich, wer laufen mag!", "tokens": ["Spu\u00b7te", "sich", ",", "wer", "lau\u00b7fen", "mag", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PWS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "'s kommt nach all dem frohen Spa\u00dfe", "tokens": ["'s", "kommt", "nach", "all", "dem", "fro\u00b7hen", "Spa\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gar ein langer Regentag.", "tokens": ["Gar", "ein", "lan\u00b7ger", "Re\u00b7gen\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}