{"dta.poem.7067": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das sonst in ihrer Brust gar offt verloschne Licht", "tokens": ["Das", "sonst", "in", "ih\u00b7rer", "Brust", "gar", "offt", "ver\u00b7lo\u00b7schne", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der frohen Zuversicht", "tokens": ["Der", "fro\u00b7hen", "Zu\u00b7ver\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu G\u00f6ttlicher Barmhertzigkeit und Liebe,", "tokens": ["Zu", "G\u00f6tt\u00b7li\u00b7cher", "Barm\u00b7hert\u00b7zig\u00b7keit", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Brennt ewig lichter Loh. Sie f\u00fchlet keine Triebe,", "tokens": ["Brennt", "e\u00b7wig", "lich\u00b7ter", "Loh", ".", "Sie", "f\u00fch\u00b7let", "kei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "NN", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als die unstr\u00e4fflich, heilig, rein,", "tokens": ["Als", "die", "uns\u00b7tr\u00e4ff\u00b7lich", ",", "hei\u00b7lig", ",", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihrem GOtt gef\u00e4llig seyn.", "tokens": ["Und", "ih\u00b7rem", "Gott", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}