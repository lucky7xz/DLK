{"textgrid.poem.26452": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst kam ich durch eine volkreiche Stadt,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst kam ich durch eine volkreiche Stadt,", "tokens": ["Einst", "kam", "ich", "durch", "ei\u00b7ne", "vol\u00b7krei\u00b7che", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "mit ihren bunten Bildern, Bauten, Sitten und Sagen,", "tokens": ["mit", "ih\u00b7ren", "bun\u00b7ten", "Bil\u00b7dern", ",", "Bau\u00b7ten", ",", "Sit\u00b7ten", "und", "Sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "um mir alles einzupr\u00e4gen, zu k\u00fcnftigem Nutzen.", "tokens": ["um", "mir", "al\u00b7les", "ein\u00b7zu\u00b7pr\u00e4\u00b7gen", ",", "zu", "k\u00fcnf\u00b7ti\u00b7gem", "Nut\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PIS", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Doch von der ganzen Stadt erinnere ich nichts mehr,", "tokens": ["Doch", "von", "der", "gan\u00b7zen", "Stadt", "e\u00b7rin\u00b7ne\u00b7re", "ich", "nichts", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "i