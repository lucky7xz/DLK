{"textgrid.poem.35655": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "18. An Goethe", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du edel eutbranntst, wo hochgelahrte", "tokens": ["Der", "du", "e\u00b7del", "eut\u00b7branntst", ",", "wo", "hoch\u00b7ge\u00b7lahr\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,", "PWAV", "VVFIN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Diener Justinians Banditen zogen,", "tokens": ["Die\u00b7ner", "Jus\u00b7ti\u00b7ni\u00b7ans", "Ban\u00b7di\u00b7ten", "zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die in Roms Labyrinthen", "tokens": ["Die", "in", "Roms", "La\u00b7by\u00b7rin\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "W\u00fcrgen das Recht der Vernunft;", "tokens": ["W\u00fcr\u00b7gen", "das", "Recht", "der", "Ver\u00b7nunft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Freier Goethe, du darfst die goldne Fessel,", "tokens": ["Frei\u00b7er", "Goe\u00b7the", ",", "du", "darfst", "die", "gold\u00b7ne", "Fes\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Aus des Griechen Gesang gschmiedet, h\u00f6hnen!", "tokens": ["Aus", "des", "Grie\u00b7chen", "Ge\u00b7sang", "gschmie\u00b7det", ",", "h\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Shakespeare durft' es und Klopstock,", "tokens": ["Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "durft'", "es", "und", "Klops\u00b7tock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "KON", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "S\u00f6hne, gleich ihm, der Natur!", "tokens": ["S\u00f6h\u00b7ne", ",", "gleich", "ihm", ",", "der", "Na\u00b7tur", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mag doch Heinrichs Homer, im tr\u00e4gen Mohnkranz,", "tokens": ["Mag", "doch", "Hein\u00b7richs", "Ho\u00b7mer", ",", "im", "tr\u00e4\u00b7gen", "Mohn\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NE", "NE", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mag der gro\u00dfe Corneill', am Aristarchen-", "tokens": ["Mag", "der", "gro\u00b7\u00dfe", "Cor\u00b7neill'", ",", "am", "A\u00b7ri\u00b7sta\u00b7rchen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$,", "APPRART", "TRUNC"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Throne knieend, das Klatschen", "tokens": ["Thro\u00b7ne", "kni\u00b7e\u00b7end", ",", "das", "Klat\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Staunender Leutlein erflehn!", "tokens": ["Stau\u00b7nen\u00b7der", "Leut\u00b7lein", "er\u00b7flehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Deutsch und eisern wie G\u00f6tz, sprich Hohn den Schurken \u2013", "tokens": ["Deutsch", "und", "ei\u00b7sern", "wie", "G\u00f6tz", ",", "sprich", "Hohn", "den", "Schur\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KOKOM", "NE", "$,", "ADJD", "NN", "ART", "NN", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mit der Fessel im Arm! Des Sumpfes Schreier", "tokens": ["Mit", "der", "Fes\u00b7sel", "im", "Arm", "!", "Des", "Sump\u00b7fes", "Schrei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$.", "ART", "NN", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schm\u00e4ht der Leu zu zerstampfen,", "tokens": ["Schm\u00e4ht", "der", "Leu", "zu", "zer\u00b7stamp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wandelt durch W\u00e4lder und herrscht!", "tokens": ["Wan\u00b7delt", "durch", "W\u00e4l\u00b7der", "und", "herrscht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Der du edel eutbranntst, wo hochgelahrte", "tokens": ["Der", "du", "e\u00b7del", "eut\u00b7branntst", ",", "wo", "hoch\u00b7ge\u00b7