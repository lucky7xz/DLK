{"textgrid.poem.33936": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Erleuchtung", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In unerme\u00dflich tiefen Stunden", "tokens": ["In", "un\u00b7er\u00b7me\u00df\u00b7lich", "tie\u00b7fen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hast du, in ahnungsvollem Schmerz,", "tokens": ["Hast", "du", ",", "in", "ah\u00b7nungs\u00b7vol\u00b7lem", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Geist des Weltalls nie empfunden,", "tokens": ["Den", "Geist", "des", "Welt\u00b7alls", "nie", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der niederflammte in dein Herz?", "tokens": ["Der", "nie\u00b7der\u00b7flamm\u00b7te", "in", "dein", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jedwedes Dasein zu erg\u00e4nzen", "tokens": ["Jed\u00b7we\u00b7des", "Da\u00b7sein", "zu", "er\u00b7g\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Durch ein Gef\u00fchl, das ihn umfa\u00dft,", "tokens": ["Durch", "ein", "Ge\u00b7f\u00fchl", ",", "das", "ihn", "um\u00b7fa\u00dft", ","], "token