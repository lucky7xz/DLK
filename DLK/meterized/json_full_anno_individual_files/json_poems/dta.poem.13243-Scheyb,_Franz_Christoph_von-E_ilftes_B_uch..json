{"dta.poem.13243": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201emit was vor Ha\u00df und Rach, Erbittrung, Zorn und Neid", "tokens": ["\u201e", "mit", "was", "vor", "Ha\u00df", "und", "Rach", ",", "Er\u00b7bit\u00b7trung", ",", "Zorn", "und", "Neid"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "PRELS", "APPR", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ezog nicht der Feinde Groll den Mord-Stahl aus der Scheid?", "tokens": ["\u201e", "zog", "nicht", "der", "Fein\u00b7de", "Groll", "den", "Mord\u00b7Stahl", "aus", "der", "Scheid", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKNEG", "ART", "NN", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "\u201edas Feur ward angesteckt; was hat man nicht gesehen?", "tokens": ["\u201e", "das", "Feur", "ward", "an\u00b7ge\u00b7steckt", ";", "was", "hat", "man", "nicht", "ge\u00b7se\u00b7hen", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$.", "PWS", "VAFIN", "PIS", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201enichts als der Himmel wei\u00df, was ferner k\u00f6nnt geschehen.", "tokens": ["\u201e", "nichts", "als", "der", "Him\u00b7mel", "wei\u00df", ",", "was", "fer\u00b7ner", "k\u00f6nnt", "ge\u00b7sche\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "KOKOM", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "165\u201dDer Stolz, die L\u00e4ndersucht, die Mi\u00dfgunst seynd die Gicht,", "tokens": ["\"", "Der", "Stolz", ",", "die", "L\u00e4n\u00b7der\u00b7sucht", ",", "die", "Mi\u00df\u00b7gunst", "seynd", "die", "Gicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edie still, doch mit Gewalt durch manche Glieder kriecht;", "tokens": ["\u201e", "die", "still", ",", "doch", "mit", "Ge\u00b7walt", "durch", "man\u00b7che", "Glie\u00b7der", "kriecht", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJD", "$,", "ADV", "APPR", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201esie schwellt, erhizt und brennt die Brust verschiedner F\u00fcrsten,", "tokens": ["\u201e", "sie", "schwellt", ",", "er\u00b7hizt", "und", "brennt", "die", "Brust", "ver\u00b7schied\u00b7ner", "F\u00fcrs\u00b7ten", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eda\u00df sie nur nach dem Saft derselben Quelle d\u00fcrsten.", "tokens": ["\u201e", "da\u00df", "sie", "nur", "nach", "dem", "Saft", "der\u00b7sel\u00b7ben", "Quel\u00b7le", "d\u00fcrs\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "PDAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ees wiegelte der Trieb in diesem Krieges-Lauf", "tokens": ["\u201e", "es", "wie\u00b7gel\u00b7te", "der", "Trieb", "in", "die\u00b7sem", "Krie\u00b7ge\u00b7sLauf"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "170\u201dSo Freunde wieder uns, als fremde V\u00f6lcker auf;", "tokens": ["\"", "So", "Freun\u00b7de", "wie\u00b7der", "uns", ",", "als", "frem\u00b7de", "V\u00f6l\u00b7cker", "auf", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "ADV", "PPER", "$,", "KOUS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201epflicht, Eigennuz und Recht war mit der Macht vermenget,", "tokens": ["\u201e", "pflicht", ",", "Ei\u00b7gen\u00b7nuz", "und", "Recht", "war", "mit", "der", "Macht", "ver\u00b7men\u00b7get", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "KON", "NN", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eauch selbst der Treueste durch die Gewalt verdr\u00e4nget.", "tokens": ["\u201e", "auch", "selbst", "der", "Treu\u00b7es\u00b7te", "durch", "die", "Ge\u00b7walt", "ver\u00b7dr\u00e4n\u00b7get", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}