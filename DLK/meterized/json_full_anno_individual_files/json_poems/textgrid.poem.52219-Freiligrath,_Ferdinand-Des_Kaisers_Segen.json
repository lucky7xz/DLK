{"textgrid.poem.52219": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Des Kaisers Segen", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin die ganze Nacht hindurch", "tokens": ["Ich", "bin", "die", "gan\u00b7ze", "Nacht", "hin\u00b7durch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Rhein hinaufgeschritten,", "tokens": ["Den", "Rhein", "hin\u00b7auf\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Drachenfels und Wolkenburg,", "tokens": ["Von", "Dra\u00b7chen\u00b7fels", "und", "Wol\u00b7ken\u00b7burg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis wo die Linzer schnitten.", "tokens": ["Bis", "wo", "die", "Lin\u00b7zer", "schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bei Rh\u00f6ndorf unterm Drachenloch", "tokens": ["Bei", "Rh\u00f6n\u00b7dorf", "un\u00b7term", "Dra\u00b7chen\u00b7loch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Anband sein Boot der Ferge;", "tokens": ["An\u00b7band", "sein", "Boot", "der", "Fer\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Zu Honnef sang ein M\u00e4dchen noch:", "tokens": ["Zu", "Hon\u00b7nef", "sang", "ein", "M\u00e4d\u00b7chen", "noch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbstand ich auf hohem Berge.\u00ab", "tokens": ["\u00bb", "stand", "ich", "auf", "ho\u00b7hem", "Ber\u00b7ge", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In Breitbach stellte mich die Wacht,", "tokens": ["In", "Breit\u00b7bach", "stell\u00b7te", "mich", "die", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Unkel trank man Neuen.", "tokens": ["In", "Un\u00b7kel", "trank", "man", "Neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Erpel schlug es Mitternacht.", "tokens": ["In", "Er\u00b7pel", "schlug", "es", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Erpel vor der Leyen.", "tokens": ["In", "Er\u00b7pel", "vor", "der", "Le\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Und hinter Erpel in dem Feld,", "tokens": ["Und", "hin\u00b7ter", "Er\u00b7pel", "in", "dem", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ist er mir begegnet,", "tokens": ["Da", "ist", "er", "mir", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der gro\u00dfe Karl, der Frankenheld,", "tokens": ["Der", "gro\u00b7\u00dfe", "Karl", ",", "der", "Fran\u00b7ken\u00b7held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der seine Trauben segnet.", "tokens": ["Der", "sei\u00b7ne", "Trau\u00b7ben", "seg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er ging mit ernstem Angesicht", "tokens": ["Er", "ging", "mit", "erns\u00b7tem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In seinen Grabgewanden;", "tokens": ["In", "sei\u00b7nen", "Grab\u00b7ge\u00b7wan\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er ging einher in Glanz und Licht,", "tokens": ["Er", "ging", "ein\u00b7her", "in", "Glanz", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Segnen auferstanden.", "tokens": ["Zum", "Seg\u00b7nen", "auf\u00b7er\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und um ihn sangen Reb' und Moos,", "tokens": ["Und", "um", "ihn", "san\u00b7gen", "Reb'", "und", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dazu die Felsenbl\u00f6cke:", "tokens": ["Da\u00b7zu", "die", "Fel\u00b7sen\u00b7bl\u00f6\u00b7cke", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bber segnet nicht im Rheingau blo\u00df", "tokens": ["\u00bb", "er", "seg\u00b7net", "nicht", "im", "Rhein\u00b7gau", "blo\u00df"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "APPRART", "NE", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die stolzen Herrenst\u00f6cke!", "tokens": ["Die", "stol\u00b7zen", "Her\u00b7ren\u00b7st\u00f6\u00b7cke", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er feit nicht blo\u00df am Oberrhein", "tokens": ["Er", "feit", "nicht", "blo\u00df", "am", "O\u00b7berr\u00b7hein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des F\u00fcrstenwinzers Messer;", "tokens": ["Des", "F\u00fcrs\u00b7ten\u00b7win\u00b7zers", "Mes\u00b7ser", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er macht den Gro\u00dfen nicht allein", "tokens": ["Er", "macht", "den", "Gro\u00b7\u00dfen", "nicht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Reichen volle F\u00e4sser!", "tokens": ["Und", "Rei\u00b7chen", "vol\u00b7le", "F\u00e4s\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er denkt auch an den irdnen Krug", "tokens": ["Er", "denkt", "auch", "an", "den", "ird\u00b7nen", "Krug"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In strohgedeckten H\u00fctten,", "tokens": ["In", "stroh\u00b7ge\u00b7deck\u00b7ten", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00fcttet Most und Wein genug", "tokens": ["Und", "sch\u00fct\u00b7tet", "Most", "und", "Wein", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In armer Halfen B\u00fctten.", "tokens": ["In", "ar\u00b7mer", "Hal\u00b7fen", "B\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er wei\u00df: der echte Feuertrank", "tokens": ["Er", "wei\u00df", ":", "der", "ech\u00b7te", "Feu\u00b7er\u00b7trank"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Springt leider nur den F\u00fcrsten,", "tokens": ["Springt", "lei\u00b7der", "nur", "den", "F\u00fcrs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und friert das Volk und liegt es krank,", "tokens": ["Und", "friert", "das", "Volk", "und", "liegt", "es", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00df es nach ihm d\u00fcrsten!", "tokens": ["So", "mu\u00df", "es", "nach", "ihm", "d\u00fcrs\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch labt und st\u00e4rkt es noch zur Frist", "tokens": ["Doch", "labt", "und", "st\u00e4rkt", "es", "noch", "zur", "Frist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Segen herbrer Reiser;", "tokens": ["Der", "Se\u00b7gen", "her\u00b7brer", "Rei\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und da\u00df an dem kein Mangel ist \u2013", "tokens": ["Und", "da\u00df", "an", "dem", "kein", "Man\u00b7gel", "ist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ART", "PIAT", "NN", "VAFIN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auch daf\u00fcr sorgt der Kaiser!", "tokens": ["Auch", "da\u00b7f\u00fcr", "sorgt", "der", "Kai\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und darum wallt er feierlich", "tokens": ["Und", "da\u00b7rum", "wallt", "er", "fei\u00b7er\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stromunter durch die St\u00e4be,", "tokens": ["Stro\u00b7mun\u00b7ter", "durch", "die", "St\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis wo am allerletzten sich", "tokens": ["Bis", "wo", "am", "al\u00b7ler\u00b7letz\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PWAV", "APPRART", "ADJA", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Festrankt die letzte Rebe!", "tokens": ["Fest\u00b7rankt", "die", "letz\u00b7te", "Re\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Kaiser wei\u00df, was allen frommt,", "tokens": ["Der", "Kai\u00b7ser", "wei\u00df", ",", "was", "al\u00b7len", "frommt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am ganzen gr\u00fcnen Strome!", "tokens": ["Am", "gan\u00b7zen", "gr\u00fc\u00b7nen", "Stro\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sanft ruh' er, bis er wiederkommt,", "tokens": ["Sanft", "ruh'", "er", ",", "bis", "er", "wie\u00b7der\u00b7kommt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Aachen in dem Dome!\u00ab", "tokens": ["Zu", "Aa\u00b7chen", "in", "dem", "Do\u00b7me", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "So raunt' es fl\u00fcsternd durch die Nacht \u2013", "tokens": ["So", "raunt'", "es", "fl\u00fcs\u00b7ternd", "durch", "die", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schemen war verschwunden.", "tokens": ["Der", "Sche\u00b7men", "war", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe durch die Ranken sacht", "tokens": ["Ich", "ha\u00b7be", "durch", "die", "Ran\u00b7ken", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Hause mich gefunden.", "tokens": ["Nach", "Hau\u00b7se", "mich", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ich bin die ganze Nacht hindurch", "tokens": ["Ich", "bin", "die", "gan\u00b7ze", "Nacht", "hin\u00b7durch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Rhein hinaufgeschritten,", "tokens": ["Den", "Rhein", "hin\u00b7auf\u00b7ge\u00b7schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Drachenfels und Wolkenburg,", "tokens": ["Von", "Dra\u00b7chen\u00b7fels", "und", "Wol\u00b7ken\u00b7burg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis wo die Linzer schnitten.", "tokens": ["Bis", "wo", "die", "Lin\u00b7zer", "schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Bei Rh\u00f6ndorf unterm Drachenloch", "tokens": ["Bei", "Rh\u00f6n\u00b7dorf", "un\u00b7term", "Dra\u00b7chen\u00b7loch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Anband sein Boot der Ferge;", "tokens": ["An\u00b7band", "sein", "Boot", "der", "Fer\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Zu Honnef sang ein M\u00e4dchen noch:", "tokens": ["Zu", "Hon\u00b7nef", "sang", "ein", "M\u00e4d\u00b7chen", "noch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure"