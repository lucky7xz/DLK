{"dta.poem.8807": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Verwirrung traf mein Antlitz, sobald der Schenke genaht;", "tokens": ["Ver\u00b7wir\u00b7rung", "traf", "mein", "Ant\u00b7litz", ",", "so\u00b7bald", "der", "Schen\u00b7ke", "ge\u00b7naht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Im Weine suche Heil nie, wen ach! die Liebe berauscht:", "tokens": ["Im", "Wei\u00b7ne", "su\u00b7che", "Heil", "nie", ",", "wen", "ach", "!", "die", "Lie\u00b7be", "be\u00b7rauscht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ADV", "$,", "PWS", "ITJ", "$.", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wer n\u00fcchtern nicht ihr ausweicht, der flieht im Rausche", "tokens": ["Wer", "n\u00fcch\u00b7tern", "nicht", "ihr", "aus\u00b7weicht", ",", "der", "flieht", "im", "Rau\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "PPER", "VVPP", "$,", "PRELS", "VVFIN", "APPRART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Um T\u00fccher aus Samarkand, um Perlenschmuck von Aden", "tokens": ["Um", "T\u00fc\u00b7cher", "aus", "Sa\u00b7mar\u00b7kand", ",", "um", "Per\u00b7len\u00b7schmuck", "von", "A\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "APPR", "NE", "$,", "KOUI", "NN", "APPR", "NN"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Verhandl\u2019 ich nicht das Staubkorn, das deine Ferse betrat:", "tokens": ["Ver\u00b7handl'", "ich", "nicht", "das", "Staub\u00b7korn", ",", "das", "dei\u00b7ne", "Fer\u00b7se", "be\u00b7trat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "O denk\u2019, ich w\u00e4re ", "tokens": ["O", "denk'", ",", "ich", "w\u00e4\u00b7re"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mit reiner Marmorhand mir, im bunten Glas von", "tokens": ["Mit", "rei\u00b7ner", "Mar\u00b7mor\u00b7hand", "mir", ",", "im", "bun\u00b7ten", "Glas", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "$,", "APPRART", "ADJA", "NN", "APPR"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}