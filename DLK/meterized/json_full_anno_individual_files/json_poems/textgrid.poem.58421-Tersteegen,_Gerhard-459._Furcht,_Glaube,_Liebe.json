{"textgrid.poem.58421": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "459. Furcht, Glaube, Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Gott f\u00fcrchtet, ha\u00dft die S\u00fcnden,", "tokens": ["Wer", "Gott", "f\u00fcrch\u00b7tet", ",", "ha\u00dft", "die", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wer Gott f\u00fcrchtet, h\u00e4lt sein Recht,", "tokens": ["Wer", "Gott", "f\u00fcrch\u00b7tet", ",", "h\u00e4lt", "sein", "Recht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht von ferne als ein Knecht.", "tokens": ["Steht", "von", "fer\u00b7ne", "als", "ein", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer Gott glaubt, wird Gnade finden,", "tokens": ["Wer", "Gott", "glaubt", ",", "wird", "Gna\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VAFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer Gott glaubt, wird fromm und rein", "tokens": ["Wer", "Gott", "glaubt", ",", "wird", "fromm", "und", "rein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "--+-+-+", "measur