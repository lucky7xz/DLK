{"dta.poem.18681": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Gewarnt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Auch in Locken hab\u2019 ich mich", "tokens": ["Auch", "in", "Lo\u00b7cken", "hab'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gar zu gern verfangen,", "tokens": ["Gar", "zu", "gern", "ver\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und so Hafis! w\u00e4r\u2019s wie dir", "tokens": ["Und", "so", "Ha\u00b7fis", "!", "w\u00e4r's", "wie", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "NE", "$.", "VAFIN", "KOKOM", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinem Freund ergangen.", "tokens": ["Dei\u00b7nem", "Freund", "er\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Aber Z\u00f6pfe flechten sie", "tokens": ["A\u00b7ber", "Z\u00f6p\u00b7fe", "flech\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun aus langen Haaren,", "tokens": ["Nun", "aus", "lan\u00b7gen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unterm Helme fechten sie,", "tokens": ["Un\u00b7term", "Hel\u00b7me", "fech\u00b7ten", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie wir wohl erfahren.", "tokens": ["Wie", "wir", "wohl", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wer sich aber wohl besann", "tokens": ["Wer", "sich", "a\u00b7ber", "wohl", "be\u00b7sann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4sst sich so nicht zwingen:", "tokens": ["L\u00e4sst", "sich", "so", "nicht", "zwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schwere Ketten f\u00fcrchtet man,", "tokens": ["Schwe\u00b7re", "Ket\u00b7ten", "f\u00fcrch\u00b7tet", "man", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rennt in leichte Schlingen.", "tokens": ["Rennt", "in", "leich\u00b7te", "Schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}