{"dta.poem.19551": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Unruhige Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Heut ward mir bis zum jungen Tag", "tokens": ["Heut", "ward", "mir", "bis", "zum", "jun\u00b7gen", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schlummer abgebrochen,", "tokens": ["Der", "Schlum\u00b7mer", "ab\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Herzen ging es Schlag auf Schlag", "tokens": ["Im", "Her\u00b7zen", "ging", "es", "Schlag", "auf", "Schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit H\u00e4mmern und mit Pochen,", "tokens": ["Mit", "H\u00e4m\u00b7mern", "und", "mit", "Po\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als trieb' sich eine Bubenschaar", "tokens": ["Als", "trieb'", "sich", "ei\u00b7ne", "Bu\u00b7ben\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wild um in beiden Kammern,", "tokens": ["Wild", "um", "in", "bei\u00b7den", "Kam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "PIAT", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Gew\u00e4hrt hat, bis es Morgen war,", "tokens": ["Ge\u00b7w\u00e4hrt", "hat", ",", "bis", "es", "Mor\u00b7gen", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Klopfen und das Hammern.", "tokens": ["Das", "Klop\u00b7fen", "und", "das", "Ham\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun weist es sich bei Tagesschein", "tokens": ["Nun", "weist", "es", "sich", "bei", "Ta\u00b7ges\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was drin geschafft die Rangen:", "tokens": ["Was", "drin", "ge\u00b7schafft", "die", "Ran\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie haben mir im Herzensschrein", "tokens": ["Sie", "ha\u00b7ben", "mir", "im", "Her\u00b7zens\u00b7schrein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}