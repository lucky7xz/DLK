{"textgrid.poem.62969": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Sch\u00e4fer stie\u00df auf einen Scorpion", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Sch\u00e4fer stie\u00df auf einen Scorpion", "tokens": ["Ein", "Sch\u00e4\u00b7fer", "stie\u00df", "auf", "ei\u00b7nen", "Scor\u00b7pi\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schwang schon seinen Fu\u00df ihn zu zerst\u00f6ren.", "tokens": ["Und", "schwang", "schon", "sei\u00b7nen", "Fu\u00df", "ihn", "zu", "zer\u00b7st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Halt ein! rief das Insect: ist die\u00df der Lohn", "tokens": ["Halt", "ein", "!", "rief", "das", "In\u00b7sect", ":", "ist", "die\u00df", "der", "Lohn"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$.", "VVFIN", "ART", "NN", "$.", "VAFIN", "PDS", "ART", "NN"], "meter": "+-+----+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "F\u00fcr meine N\u00fctzlichkeit? \u2013 Hoho, la\u00df h\u00f6ren,", "tokens": ["F\u00fcr", "mei\u00b7ne", "N\u00fctz\u00b7lich\u00b7keit", "?", "\u2013", "Ho\u00b7ho", ",", "la\u00df", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$(", "NE", "$,", "VVIMP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Versetzt der Hirt, was wohl ein Scorpion", "tokens": ["Ver\u00b7setzt", "der", "Hirt", ",", "was", "wohl", "ein", "Scor\u00b7pi\u00b7on"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In aller Welt f\u00fcr Nutzen stifte? \u2013", "tokens": ["In", "al\u00b7ler", "Welt", "f\u00fcr", "Nut\u00b7zen", "stif\u00b7te", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ey, sprach der Wurm im Operatorston,", "tokens": ["Ey", ",", "sprach", "der", "Wurm", "im", "O\u00b7pe\u00b7ra\u00b7tor\u00b7ston", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wer kennet nicht mein Oel? Es wehrt dem schnellen Gifte,", "tokens": ["Wer", "ken\u00b7net", "nicht", "mein", "O\u00b7el", "?", "Es", "wehrt", "dem", "schnel\u00b7len", "Gif\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Wenn euch mein Stachel ritzt. \u2013 So B\u00f6sewicht", "tokens": ["Wenn", "euch", "mein", "Sta\u00b7chel", "ritzt", ".", "\u2013", "So", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "$(", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und darum soll die Rache dich verschonen?", "tokens": ["Und", "da\u00b7rum", "soll", "die", "Ra\u00b7che", "dich", "ver\u00b7scho\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Rief Milon; g\u00e4b es keine Scorpionen,", "tokens": ["Rief", "Mi\u00b7lon", ";", "g\u00e4b", "es", "kei\u00b7ne", "Scor\u00b7pi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "So brauchte man ihr Oel auch nicht.", "tokens": ["So", "brauch\u00b7te", "man", "ihr", "O\u00b7el", "auch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "ADV", "PTKNEG", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein Sch\u00e4fer stie\u00df auf einen Scorpion", "tokens": ["Ein", "Sch\u00e4\u00b7fer", "stie\u00df", "auf", "ei\u00b7nen", "Scor\u00b7pi\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schwang schon seinen Fu\u00df ihn zu zerst\u00f6ren.", "tokens": ["Und", "schwang", "schon", "sei\u00b7nen", "Fu\u00df", "ihn", "zu", "zer\u00b7st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Halt ein! rief das Insect: ist die\u00df der Lohn", "tokens": ["Halt", "ein", "!", "rief", "das", "In\u00b7sect", ":", "ist", "die\u00df", "der", "Lohn"], "token_