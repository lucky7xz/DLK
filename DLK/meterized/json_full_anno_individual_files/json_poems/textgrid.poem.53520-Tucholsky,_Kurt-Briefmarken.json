{"textgrid.poem.53520": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Briefmarken", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Germania, die was auf den bunten Marken", "tokens": ["Ger\u00b7ma\u00b7nia", ",", "die", "was", "auf", "den", "bun\u00b7ten", "Mar\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Reichspost prangt, hat l\u00e4ngst die Nase voll.", "tokens": ["der", "Reichs\u00b7post", "prangt", ",", "hat", "l\u00e4ngst", "die", "Na\u00b7se", "voll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie ist ein Weib. Wir brauchen einen starken", "tokens": ["Sie", "ist", "ein", "Weib", ".", "Wir", "brau\u00b7chen", "ei\u00b7nen", "star\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und kr\u00e4ftigen Mann, der k\u00fcnftig prangen soll.", "tokens": ["und", "kr\u00e4f\u00b7ti\u00b7gen", "Mann", ",", "der", "k\u00fcnf\u00b7tig", "pran\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "So leg ich denn den Finger an die Nase", "tokens": ["So", "leg", "ich", "denn", "den", "Fin\u00b7ger", "an", "die", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und denke nach: Wer ist der Ehre wert?", "tokens": ["und", "den\u00b7ke", "nach", ":", "Wer", "ist", "der", "Eh\u00b7re", "wert", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$.", "PWS", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Herr Chamberlain? Herr Oldenburg? Herr Haase?", "tokens": ["Herr", "Cham\u00b7ber\u00b7lain", "?", "Herr", "Ol\u00b7den\u00b7burg", "?", "Herr", "Haa\u00b7se", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "NN", "NE", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "auf einem Hoppe-Hoppe-Reiter-Pferd?", "tokens": ["auf", "ei\u00b7nem", "Hop\u00b7pe\u00b7Hop\u00b7pe\u00b7Rei\u00b7ter\u00b7Pferd", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch nehmen wir die G\u00f6tter aus den Tempeln", "tokens": ["Doch", "neh\u00b7men", "wir", "die", "G\u00f6t\u00b7ter", "aus", "den", "Tem\u00b7peln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 zum Beispiel Herrn von Heydebrands Gesicht \u2013,", "tokens": ["\u2013", "zum", "Bei\u00b7spiel", "Herrn", "von", "Hey\u00b7de\u00b7brands", "Ge\u00b7sicht", "\u2013", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": 