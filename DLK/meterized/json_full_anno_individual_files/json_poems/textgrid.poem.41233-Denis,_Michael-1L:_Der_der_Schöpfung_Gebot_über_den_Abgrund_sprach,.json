{"textgrid.poem.41233": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der der Sch\u00f6pfung Gebot \u00fcber den Abgrund sprach,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der der Sch\u00f6pfung Gebot \u00fcber den Abgrund sprach,", "tokens": ["Der", "der", "Sch\u00f6p\u00b7fung", "Ge\u00b7bot", "\u00fc\u00b7ber", "den", "Ab\u00b7grund", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und aus tr\u00e4chtigem Nichts staunende Wesen rief,", "tokens": ["Und", "aus", "tr\u00e4ch\u00b7ti\u00b7gem", "Nichts", "stau\u00b7nen\u00b7de", "We\u00b7sen", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sprach zur werdenden Zeit, als sie vor ihm erschien:", "tokens": ["Sprach", "zur", "wer\u00b7den\u00b7den", "Zeit", ",", "als", "sie", "vor", "ihm", "er\u00b7schien", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "\u00bbdu nimm Fl\u00fcgel, und raste nie!\u00ab", "tokens": ["\u00bb", "du", "nimm", "Fl\u00fc\u00b7gel", ",", "und", "ras\u00b7te", "nie", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVIMP", "NN", "$,", "KON", "VVFIN", "ADV", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Sie nahm Fl\u00fcgel, und flog, und der geschwinde Pfeil,", "tokens": ["Sie", "nahm", "Fl\u00fc\u00b7gel", ",", "und", "flog", ",", "und", "der", "ge\u00b7schwin\u00b7de", "Pfeil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KON", "VVFIN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und der streifende Nord, und der gest\u00fcrzte Strom", "tokens": ["Und", "der", "strei\u00b7fen\u00b7de", "Nord", ",", "und", "der", "ge\u00b7st\u00fcrz\u00b7te", "Strom"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Blieben m\u00fcde zur\u00fcck. Selbst der Gedanken Flug", "tokens": ["Blie\u00b7ben", "m\u00fc\u00b7de", "zu\u00b7r\u00fcck", ".", "Selbst", "der", "Ge\u00b7dan\u00b7ken", "Flug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ", "$.", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Keichet arbeitsam hinter ihr.", "tokens": ["Kei\u00b7chet", "ar\u00b7beit\u00b7sam", "hin\u00b7ter", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Dennoch schilt sie der Thor, wenn er gesellschaftlos,", "tokens": ["Den\u00b7noch", "schilt", "sie", "der", "Thor", ",", "wenn", "er", "ge\u00b7sell\u00b7schaft\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ueberlassen sich selbst, lange Secunden z\u00e4hlt;", "tokens": ["Ue\u00b7ber\u00b7las\u00b7sen", "sich", "selbst", ",", "lan\u00b7ge", "Se\u00b7cun\u00b7den", "z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dennoch schilt er sie tr\u00e4g', wenn ihm dann auf sich selbst", "tokens": ["Den\u00b7noch", "schilt", "er", "sie", "tr\u00e4g'", ",", "wenn", "ihm", "dann", "auf", "sich", "selbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPR", "PRF", "ADV"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mancher schaudernde Blick entf\u00e4hrt.", "tokens": ["Man\u00b7cher", "schau\u00b7dern\u00b7de", "Blick", "ent\u00b7f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Wenn um's goldene Bett schwarze Phantomen stehn;", "tokens": ["Wenn", "um's", "gol\u00b7de\u00b7ne", "Bett", "schwar\u00b7ze", "Phan\u00b7to\u00b7men", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJA", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wenn sein zagender Geist D\u00f6rner auf Schwanen f\u00fchlt,", "tokens": ["Wenn", "sein", "za\u00b7gen\u00b7der", "Geist", "D\u00f6r\u00b7ner", "auf", "Schwa\u00b7nen", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und der lautere Ruf seines Gewissens itzt", "tokens": ["Und", "der", "lau\u00b7te\u00b7re", "Ruf", "sei\u00b7nes", "Ge\u00b7wis\u00b7sens", "itzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PPOSAT", "NN", "ADV"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Durch die n\u00e4chtliche Stille t\u00f6nt.", "tokens": ["Durch", "die", "n\u00e4cht\u00b7li\u00b7che", "Stil\u00b7le", "t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "O dann w\u00fcnscht er den Tag, welcher den Musenfreund", "tokens": ["O", "dann", "w\u00fcnscht", "er", "den", "Tag", ",", "wel\u00b7cher", "den", "Mu\u00b7sen\u00b7freund"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Schon vom Abendroth' her, seiner uneingedenk,", "tokens": ["Schon", "vom", "A\u00b7bend\u00b7ro\u00b7th'", "her", ",", "sei\u00b7ner", "un\u00b7ein\u00b7ge\u00b7denk", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Tief verloren ins Meer weiser Betrachtungen,", "tokens": ["Tief", "ver\u00b7lo\u00b7ren", "ins", "Meer", "wei\u00b7ser", "Be\u00b7trach\u00b7tun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bei der wachenden Lampe trifft. \u2013", "tokens": ["Bei", "der", "wa\u00b7chen\u00b7den", "Lam\u00b7pe", "trifft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Aber schilt er auch dann, fl\u00fcchtige Zeit! dich tr\u00e4g',", "tokens": ["A\u00b7ber", "schilt", "er", "auch", "dann", ",", "fl\u00fcch\u00b7ti\u00b7ge", "Zeit", "!", "dich", "tr\u00e4g'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wann im Thore des Tod's ihm die Verwesung winkt,", "tokens": ["Wann", "im", "Tho\u00b7re", "des", "To\u00b7d's", "ihm", "die", "Ver\u00b7we\u00b7sung", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--++-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Und vom Staube sein Geist wartender Ewigkeit", "tokens": ["Und", "vom", "Stau\u00b7be", "sein", "Geist", "war\u00b7ten\u00b7der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ahnungvoller entgegen bebt?", "tokens": ["Ah\u00b7nung\u00b7vol\u00b7ler", "ent\u00b7ge\u00b7gen", "bebt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Wann das, was er verlebt, klein wie ein Atomus,", "tokens": ["Wann", "das", ",", "was", "er", "ver\u00b7lebt", ",", "klein", "wie", "ein", "A\u00b7to\u00b7mus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "$,", "PWS", "PPER", "VVPP", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind's Jahrhunderte schon, dennoch ein Atomus,", "tokens": ["Sin\u00b7d's", "Jahr\u00b7hun\u00b7der\u00b7te", "schon", ",", "den\u00b7noch", "ein", "A\u00b7to\u00b7mus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Den im luftigen Raum' irrend ein Nord verhaucht,", "tokens": ["Den", "im", "luf\u00b7ti\u00b7gen", "Raum'", "ir\u00b7rend", "ein", "Nord", "ver\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVPP", "ART", "NN", "VVPP", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Vor der schwitzenden Stirne schwebt?", "tokens": ["Vor", "der", "schwit\u00b7zen\u00b7den", "Stir\u00b7ne", "schwebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "O dann ha\u00dft er den Wahn, der ihn so lang betrog,", "tokens": ["O", "dann", "ha\u00dft", "er", "den", "Wahn", ",", "der", "ihn", "so", "lang", "be\u00b7trog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der den flatternden Sinn Jahre vert\u00e4ndeln hie\u00df;", "tokens": ["Der", "den", "flat\u00b7tern\u00b7den", "Sinn", "Jah\u00b7re", "ver\u00b7t\u00e4n\u00b7deln", "hie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "NN", "VVINF", "VVFIN", "$."], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Dann erst sieht er den Werth eilender Stunden ein,", "tokens": ["Dann", "erst", "sieht", "er", "den", "Werth", "ei\u00b7len\u00b7der", "Stun\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "W\u00fcnscht sein Leben zur\u00fcck \u2013 und stirbt.", "tokens": ["W\u00fcnscht", "sein", "Le\u00b7ben", "zu\u00b7r\u00fcck", "\u2013", "und", "stirbt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$(", "KON", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Zeit! unsch\u00e4tzbares Gut! Weise nur kennen dich;", "tokens": ["Zeit", "!", "un\u00b7sch\u00e4tz\u00b7ba\u00b7res", "Gut", "!", "Wei\u00b7se", "nur", "ken\u00b7nen", "dich", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "$.", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sie nur geizen nach dir. Jeglicher Augenblick", "tokens": ["Sie", "nur", "gei\u00b7zen", "nach", "dir", ".", "Jeg\u00b7li\u00b7cher", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "NN", "APPR", "PPER", "$.", "PIAT", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Flie\u00dfet Weisen gebraucht. Weisen nur ist bewu\u00dft,", "tokens": ["Flie\u00b7\u00dfet", "Wei\u00b7sen", "ge\u00b7braucht", ".", "Wei\u00b7sen", "nur", "ist", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVPP", "$.", "NN", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Was oft eine Minute lehrt.", "tokens": ["Was", "oft", "ei\u00b7ne", "Mi\u00b7nu\u00b7te", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Freund! die l\u00e4ngere Zeit, die sich der Thor vertreibt,", "tokens": ["Freund", "!", "die", "l\u00e4n\u00b7ge\u00b7re", "Zeit", ",", "die", "sich", "der", "Thor", "ver\u00b7treibt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der ins f\u00fcnfzigste Jahr buhlet und schwelgt und spielt,", "tokens": ["Der", "ins", "f\u00fcnf\u00b7zigs\u00b7te", "Jahr", "buh\u00b7let", "und", "schwelgt", "und", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Freund! o sage, warum gab sie der Himmel nicht", "tokens": ["Freund", "!", "o", "sa\u00b7ge", ",", "wa\u00b7rum", "gab", "sie", "der", "Him\u00b7mel", "nicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": [