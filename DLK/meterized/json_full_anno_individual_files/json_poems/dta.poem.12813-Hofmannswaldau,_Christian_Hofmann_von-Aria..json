{"dta.poem.12813": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aria.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Steh, Lehrer der jugend!", "tokens": ["Steh", ",", "Leh\u00b7rer", "der", "ju\u00b7gend", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Du Leit-stern der tugend!", "tokens": ["Du", "Leit\u00b7stern", "der", "tu\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Steh ferner und bl\u00fch!", "tokens": ["Steh", "fer\u00b7ner", "und", "bl\u00fch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Gott kehre die feinde", "tokens": ["Gott", "keh\u00b7re", "die", "fein\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Noch selber in freunde!", "tokens": ["Noch", "sel\u00b7ber", "in", "freun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Gott segne die m\u00fch!", "tokens": ["Gott", "seg\u00b7ne", "die", "m\u00fch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Gott segne dein lehren!", "tokens": ["Gott", "seg\u00b7ne", "dein", "leh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "So bleibst du in ehren;", "tokens": ["So", "bleibst", "du", "in", "eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So lebst du vergn\u00fcgt.", "tokens": ["So", "lebst", "du", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "So cr\u00f6net die jugend", "tokens": ["So", "cr\u00f6\u00b7net", "die", "ju\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Recht, weisheit und tugend;", "tokens": ["Recht", ",", "weis\u00b7heit", "und", "tu\u00b7gend", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "So hast du gesiegt.", "tokens": ["So", "hast", "du", "ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}