{"dta.poem.15758": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch lange nicht genug geschrieben und gedichtet,", "tokens": ["Noch", "lan\u00b7ge", "nicht", "ge\u00b7nug", "ge\u00b7schrie\u00b7ben", "und", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch lange nicht genug gesichtet und gelichtet.", "tokens": ["Noch", "lan\u00b7ge", "nicht", "ge\u00b7nug", "ge\u00b7sich\u00b7tet", "und", "ge\u00b7lich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gebt nur die Ewigkeit von euerm Schreiben auf,", "tokens": ["Gebt", "nur", "die", "E\u00b7wig\u00b7keit", "von", "eu\u00b7erm", "Schrei\u00b7ben", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst schreckt die Fantasie ma\u00dfloser B\u00fccherhauf.", "tokens": ["Sonst", "schreckt", "die", "Fan\u00b7ta\u00b7sie", "ma\u00df\u00b7lo\u00b7ser", "B\u00fc\u00b7cher\u00b7hauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Denkt, da\u00df ihr schreibt nur statt zu denken und zu sprechen,", "tokens": ["Denkt", ",", "da\u00df", "ihr", "schreibt", "nur", "statt", "zu", "den\u00b7ken", "und", "zu", "spre\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "ADV", "ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so ist ohne Ma\u00df zu schreiben kein Verbrechen.", "tokens": ["Und", "so", "ist", "oh\u00b7ne", "Ma\u00df", "zu", "schrei\u00b7ben", "kein", "Ver\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "APPR", "NN", "PTKZU", "VVINF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Denn alles was ihr denkt und sprecht, verweht der Wind,", "tokens": ["Denn", "al\u00b7les", "was", "ihr", "denkt", "und", "sprecht", ",", "ver\u00b7weht", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer mu\u00df sich neu aussprechen Mann und Kind.", "tokens": ["Und", "im\u00b7mer", "mu\u00df", "sich", "neu", "aus\u00b7spre\u00b7chen", "Mann", "und", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PRF", "ADJD", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}