 "Ruh und Sicherheit verspricht!", "tokens": ["Ruh", "und", "Si\u00b7cher\u00b7heit", "ver\u00b7spricht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Darum an dem langen Tage", "tokens": ["Da\u00b7rum", "an", "dem", "lan\u00b7gen", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Merke dir es, liebe Brust:", "tokens": ["Mer\u00b7ke", "dir", "es", ",", "lie\u00b7be", "Brust", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Tag hat seine Plage,", "tokens": ["Je\u00b7der", "Tag", "hat", "sei\u00b7ne", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Nacht hat ihre Lust.", "tokens": ["Und", "die", "Nacht", "hat", "ih\u00b7re", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}