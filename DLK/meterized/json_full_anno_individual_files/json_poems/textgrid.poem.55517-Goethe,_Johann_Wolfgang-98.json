{"textgrid.poem.55517": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "98", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Worte sind der Seele Bild \u2013", "tokens": ["Wor\u00b7te", "sind", "der", "See\u00b7le", "Bild", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht ein Bild! sie sind ein Schatten!", "tokens": ["Nicht", "ein", "Bild", "!", "sie", "sind", "ein", "Schat\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagen herbe, deuten mild,", "tokens": ["Sa\u00b7gen", "her\u00b7be", ",", "deu\u00b7ten", "mild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was wir haben, was wir hatten. \u2013", "tokens": ["Was", "wir", "ha\u00b7ben", ",", "was", "wir", "hat\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "PRELS", "PPER", "VAFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was wir hatten, wo ist's hin?", "tokens": ["Was", "wir", "hat\u00b7ten", ",", "wo", "ist's", "hin", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "PWAV", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und was ist's denn, was wir haben? \u2013", "toke