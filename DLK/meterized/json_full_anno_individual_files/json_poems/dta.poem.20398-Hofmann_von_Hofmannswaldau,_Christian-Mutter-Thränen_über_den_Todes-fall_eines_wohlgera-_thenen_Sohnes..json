{"dta.poem.20398": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Mutter-Thr\u00e4nen  \n \u00fcber den Todes-fall eines wohlgera-  \n thenen Sohnes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die mutter/ deren hertz des sohnes tod ger\u00fchret/", "tokens": ["Die", "mut\u00b7ter", "/", "de\u00b7ren", "hertz", "des", "soh\u00b7nes", "tod", "ge\u00b7r\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELAT", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat die betr\u00fcbte hand auff seinen stein gef\u00fchret/", "tokens": ["Hat", "die", "be\u00b7tr\u00fcb\u00b7te", "hand", "auff", "sei\u00b7nen", "stein", "ge\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und gr\u00e4bt mit stiller angst die schmertzen selber ein/", "tokens": ["Und", "gr\u00e4bt", "mit", "stil\u00b7ler", "angst", "die", "schmert\u00b7zen", "sel\u00b7ber", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ART", "ADJA", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ihr ihn seel und geist stets reg und lebend seyn:", "tokens": ["Die", "ihr", "ihn", "seel", "und", "geist", "stets", "reg", "und", "le\u00b7bend", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADJD", "KON", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hieher hat GOttes schlu\u00df dasselbe kind geleget/", "tokens": ["Hie\u00b7her", "hat", "Got\u00b7tes", "schlu\u00df", "das\u00b7sel\u00b7be", "kind", "ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das seine mutter noch in ihrem hertzen tr\u00e4get/", "tokens": ["Das", "sei\u00b7ne", "mut\u00b7ter", "noch", "in", "ih\u00b7rem", "hert\u00b7zen", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sein alter nennt ihn kind/ die tugend einen mann/", "tokens": ["Sein", "al\u00b7ter", "nennt", "ihn", "kind", "/", "die", "tu\u00b7gend", "ei\u00b7nen", "mann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PPER", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die tugend/ die ihn itzt zum engel machen kan/", "tokens": ["Die", "tu\u00b7gend", "/", "die", "ihn", "itzt", "zum", "en\u00b7gel", "ma\u00b7chen", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und in die zahl versetzt/ die man nicht soll beklagen/", "tokens": ["Und", "in", "die", "zahl", "ver\u00b7setzt", "/", "die", "man", "nicht", "soll", "be\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$(", "PRELS", "PIS", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil man die seligen nicht kan zu grabe tragen/", "tokens": ["Weil", "man", "die", "se\u00b7li\u00b7gen", "nicht", "kan", "zu", "gra\u00b7be", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "PTKNEG", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein allzu langer schmertz vor todte narren ist/", "tokens": ["Ein", "all\u00b7zu", "lan\u00b7ger", "schmertz", "vor", "tod\u00b7te", "nar\u00b7ren", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKA", "ADJD", "ADJD", "APPR", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und nur ein kluger will/ da\u00df man ihn nicht vergist.", "tokens": ["Und", "nur", "ein", "klu\u00b7ger", "will", "/", "da\u00df", "man", "ihn", "nicht", "ver\u00b7gist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "VMFIN", "$(", "KOUS", "PIS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr thr\u00e4nen/ die ihr hier den alten jammer treibet/", "tokens": ["Ihr", "thr\u00e4\u00b7nen", "/", "die", "ihr", "hier", "den", "al\u00b7ten", "jam\u00b7mer", "trei\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Beweinet nicht den sohn/ beweint der mutter noth/", "tokens": ["Be\u00b7wei\u00b7net", "nicht", "den", "sohn", "/", "be\u00b7weint", "der", "mut\u00b7ter", "noth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und sprecht: Sein sterben sey auch endlich noch ihr tod.", "tokens": ["Und", "sprecht", ":", "Sein", "ster\u00b7ben", "sey", "auch", "end\u00b7lich", "noch", "ihr", "tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "VVINF", "VAFIN", "ADV", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Es schleu\u00dft sich freud und lust mit dieses kindes augen/", "tokens": ["Es", "schleu\u00dft", "sich", "freud", "und", "lust", "mit", "die\u00b7ses", "kin\u00b7des", "au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVFIN", "KON", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die in dem leben nicht/ im tode thr\u00e4nen saugen;", "tokens": ["Die", "in", "dem", "le\u00b7ben", "nicht", "/", "im", "to\u00b7de", "thr\u00e4\u00b7nen", "sau\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "VVFIN", "PTKNEG", "$(", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Denn er hielt ein gebot/ und wiese noch als kind/", "tokens": ["Denn", "er", "hielt", "ein", "ge\u00b7bot", "/", "und", "wie\u00b7se", "noch", "als", "kind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "VVFIN", "$(", "KON", "VVFIN", "ADV", "KOUS", "NN", "$("], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Da\u00df in dem vierdten auch die andern neune sind.", "tokens": ["Da\u00df", "in", "dem", "vierd\u00b7ten", "auch", "die", "an\u00b7dern", "neu\u00b7ne", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "ADV", "ART", "ADJA", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nun du ruhst allzuwohl/ ich mu\u00df vor mich was schreiben/", "tokens": ["Nun", "du", "ruhst", "all\u00b7zu\u00b7wohl", "/", "ich", "mu\u00df", "vor", "mich", "was", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$(", "PPER", "VMFIN", "APPR", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und nach gef\u00e4hrter angst auff diesem schlusse bleiben:", "tokens": ["Und", "nach", "ge\u00b7f\u00e4hr\u00b7ter", "angst", "auff", "die\u00b7sem", "schlus\u00b7se", "blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "O GOtt/ du reissest mir mein ander hertze hin/", "tokens": ["O", "Gott", "/", "du", "reis\u00b7sest", "mir", "mein", "an\u00b7der", "hert\u00b7ze", "hin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "PPER", "VVFIN", "PPER", "PPOSAT", "ADJD", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Zu zeigen/ da\u00df ich nur an dich gewiesen bin.", "tokens": ["Zu", "zei\u00b7gen", "/", "da\u00df", "ich", "nur", "an", "dich", "ge\u00b7wie\u00b7sen", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}