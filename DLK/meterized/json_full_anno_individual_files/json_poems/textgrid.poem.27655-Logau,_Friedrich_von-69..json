{"textgrid.poem.27655": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Momus wil ich nicht seyn, Momus, noch vernichten", "tokens": ["Dein", "Mo\u00b7mus", "wil", "ich", "nicht", "seyn", ",", "Mo\u00b7mus", ",", "noch", "ver\u00b7nich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VAINF", "$,", "NE", "$,", "ADV", "VVINF"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Urtheil, wenn du sprichst: Das Vers- und Reime-Tichten", "tokens": ["Dein", "Ur\u00b7theil", ",", "wenn", "du", "sprichst", ":", "Das", "Ver\u00b7s", "und", "Rei\u00b7me\u00b7Tich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$.", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sei Schulenf\u00fcchserei. Wie aber, da\u00df das lesen", "tokens": ["Sei", "Schu\u00b7len\u00b7f\u00fcch\u00b7se\u00b7rei", ".", "Wie", "a\u00b7ber", ",", "da\u00df", "das", "le\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "$.", "PWAV", "ADV", "$,", "KOUS", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Noch g\u00fcltig bei dir ist als Schulenf\u00fcchsisch Wesen?", "tokens": ["Noch", "g\u00fcl\u00b7tig", "bei", "dir", "ist", "als", "Schu\u00b7len\u00b7f\u00fcch\u00b7sisch", "We\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VAFIN", "KOKOM", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ey, Lieber, lies nicht mehr, sonst wirst du gar zum Kinde", "tokens": ["Ey", ",", "Lie\u00b7ber", ",", "lies", "nicht", "mehr", ",", "sonst", "wirst", "du", "gar", "zum", "Kin\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "VVFIN", "PTKNEG", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und darffst, da\u00df dir mein Reim noch eine Ruthe binde.", "tokens": ["Und", "darffst", ",", "da\u00df", "dir", "mein", "Reim", "noch", "ei\u00b7ne", "Ru\u00b7the", "bin\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein Momus wil ich nicht seyn, Momus, noch vernichten", "tokens": ["Dein", "Mo\u00b7mus", "wil", "ich", "nicht", "seyn", ",", "Mo\u00b7mus", ",", "noch", "ver\u00b7nich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VAINF", "$,", "NE", "$,", "ADV", "VVINF"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein Urtheil, wenn du sprichst: Das Vers- und Reime-Tichten", "tokens": ["Dein", "Ur\u00b7theil", ",", "wenn", "du", "sprichst", ":", "Das", "Ver\u00b7s", "und", "Rei\u00b7me\u00b7Tich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$.", "ART", "TRUNC", "KON", "NN