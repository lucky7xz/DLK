{"dta.poem.11577": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Maschinka.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Dieser schwellende Mund, den Reiz der Heimath noch", "tokens": ["Die\u00b7ser", "schwel\u00b7len\u00b7de", "Mund", ",", "den", "Reiz", "der", "Hei\u00b7math", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$,", "ART", "NN", "ART", "NN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Kennt die Sprache nicht mehr, die ihn so lieblich ge-", "tokens": ["Kennt", "die", "Spra\u00b7che", "nicht", "mehr", ",", "die", "ihn", "so", "lieb\u00b7lich", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,", "PRELS", "PPER", "ADV", "ADJD", "TRUNC"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Halb verdrie\u00dflich greifet die Sch\u00f6ne nach der Grammatik,", "tokens": ["Halb", "ver\u00b7drie\u00df\u00b7lich", "grei\u00b7fet", "die", "Sch\u00f6\u00b7ne", "nach", "der", "Gram\u00b7ma\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Stammelt russischen Laut, weil es der Vater befiehlt.", "tokens": ["Stam\u00b7melt", "rus\u00b7si\u00b7schen", "Laut", ",", "weil", "es", "der", "Va\u00b7ter", "be\u00b7fiehlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Euer Stammeln ist s\u00fc\u00df, doch pflegt ihr, trutzige Lippen,", "tokens": ["Eu\u00b7er", "Stam\u00b7meln", "ist", "s\u00fc\u00df", ",", "doch", "pflegt", "ihr", ",", "trut\u00b7zi\u00b7ge", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Heimlich ein ander Gesch\u00e4ft, das euch vor allem ver-", "tokens": ["Heim\u00b7lich", "ein", "an\u00b7der", "Ge\u00b7sch\u00e4ft", ",", "das", "euch", "vor", "al\u00b7lem", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "PIS", "TRUNC"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}}}}}