{"dta.poem.10428": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Li.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Sie sa\u00dfen und tranken am Theetisch,", "tokens": ["Sie", "sa\u00b7\u00dfen", "und", "tran\u00b7ken", "am", "Thee\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und sprachen von Liebe viel.", "tokens": ["Und", "spra\u00b7chen", "von", "Lie\u00b7be", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Herren, die waren \u00e4sthetisch,", "tokens": ["Die", "Her\u00b7ren", ",", "die", "wa\u00b7ren", "\u00e4s\u00b7the\u00b7tisch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Damen von zartem Gef\u00fchl.", "tokens": ["Die", "Da\u00b7men", "von", "zar\u00b7tem", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Die Liebe mu\u00df seyn platonisch,", "tokens": ["Die", "Lie\u00b7be", "mu\u00df", "seyn", "pla\u00b7to\u00b7nisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der d\u00fcrre Hofrath sprach.", "tokens": ["Der", "d\u00fcr\u00b7re", "Hof\u00b7rath", "sprach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Hofr\u00e4thin l\u00e4chelt ironisch,", "tokens": ["Die", "Hof\u00b7r\u00e4t\u00b7hin", "l\u00e4\u00b7chelt", "i\u00b7ron\u00b7isch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dennoch seufzet sie: Ach!", "tokens": ["Und", "den\u00b7noch", "seuf\u00b7zet", "sie", ":", "Ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "ITJ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Der Domherr \u00f6ffnet den Mund weit:", "tokens": ["Der", "Dom\u00b7herr", "\u00f6ff\u00b7net", "den", "Mund", "weit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Die Liebe sey nicht zu roh,", "tokens": ["Die", "Lie\u00b7be", "sey", "nicht", "zu", "roh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie schadet sonst der Gesundheit.", "tokens": ["Sie", "scha\u00b7det", "sonst", "der", "Ge\u00b7sund\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Fr\u00e4ulein lispelt: wie so?", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "lis\u00b7pelt", ":", "wie", "so", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Gr\u00e4fin spricht wehm\u00fcthig:", "tokens": ["Die", "Gr\u00e4\u00b7fin", "spricht", "weh\u00b7m\u00fct\u00b7hig", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Liebe ist eine Passion!", "tokens": ["Die", "Lie\u00b7be", "ist", "ei\u00b7ne", "Pas\u00b7si\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und pr\u00e4sentiret g\u00fctig", "tokens": ["Und", "pr\u00e4\u00b7sen\u00b7ti\u00b7ret", "g\u00fc\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Tasse dem Herren Baron.", "tokens": ["Die", "Tas\u00b7se", "dem", "Her\u00b7ren", "Ba\u00b7ron", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Am Tische war noch ein Pl\u00e4tzchen;", "tokens": ["Am", "Ti\u00b7sche", "war", "noch", "ein", "Pl\u00e4tz\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Liebchen, da hast du gefehlt.", "tokens": ["Mein", "Lieb\u00b7chen", ",", "da", "hast", "du", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Du h\u00e4ttest so h\u00fcbsch, mein Sch\u00e4tzchen,", "tokens": ["Du", "h\u00e4t\u00b7test", "so", "h\u00fcbsch", ",", "mein", "Sch\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von deiner Liebe erz\u00e4hlt.", "tokens": ["Von", "dei\u00b7ner", "Lie\u00b7be", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}