: {"line.1": {"text": "Trat Zweifel dir entgegen;", "tokens": ["Trat", "Zwei\u00b7fel", "dir", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nie standst du ihm getrost,", "tokens": ["Nie", "standst", "du", "ihm", "ge\u00b7trost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Anringend nach Verm\u00f6gen:", "tokens": ["An\u00b7rin\u00b7gend", "nach", "Ver\u00b7m\u00f6\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bebtest und entflohst.", "tokens": ["Du", "beb\u00b7test", "und", "ent\u00b7flohst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Kehr um, du sinkst noch tiefer,", "tokens": ["Kehr", "um", ",", "du", "sinkst", "noch", "tie\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kehr um, verlockter Freund,", "tokens": ["Kehr", "um", ",", "ver\u00b7lock\u00b7ter", "Freund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Forscher und als Pr\u00fcfer,", "tokens": ["Als", "For\u00b7scher", "und", "als", "Pr\u00fc\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "KOUS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu dem, der um dich weint!", "tokens": ["Zu", "dem", ",", "der", "um", "dich", "weint", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}