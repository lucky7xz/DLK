{"textgrid.poem.55663": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Hatem", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies zu deuten, bin erb\u00f6tig!", "tokens": ["Dies", "zu", "deu\u00b7ten", ",", "bin", "er\u00b7b\u00f6\u00b7tig", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich dir nicht oft erz\u00e4hlt,", "tokens": ["Hab", "ich", "dir", "nicht", "oft", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Doge von Venedig", "tokens": ["Wie", "der", "Do\u00b7ge", "von", "Ve\u00b7ne\u00b7dig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NE"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mit dem Meere sich verm\u00e4hlt?", "tokens": ["Mit", "dem", "Mee\u00b7re", "sich", "ver\u00b7m\u00e4hlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So von deinen Fingergliedern", "tokens": ["So", "von", "dei\u00b7nen", "Fin\u00b7ger\u00b7glie\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fiel der Ring dem Euphrat zu.", "tokens": ["Fiel", "der", "Ring", "dem", "Eu\u00b7ph\u00b7rat", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["