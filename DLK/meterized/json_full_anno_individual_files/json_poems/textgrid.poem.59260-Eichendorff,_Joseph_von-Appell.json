{"textgrid.poem.59260": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Appell", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00f6rt viel Dichter klagen", "tokens": ["Ich", "h\u00f6rt", "viel", "Dich\u00b7ter", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von alter Ehre rein,", "tokens": ["Von", "al\u00b7ter", "Eh\u00b7re", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wen'ge mochten's wagen", "tokens": ["Doch", "wen'\u00b7ge", "moch\u00b7ten's", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und selber schlagen drein.", "tokens": ["Und", "sel\u00b7ber", "schla\u00b7gen", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Herz wollt mir zerspringen,", "tokens": ["Mein", "Herz", "wollt", "mir", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sucht' mir ein ander Ziel,", "tokens": ["Sucht'", "mir", "ein", "an\u00b7der", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn anders sein und singen,", "tokens": ["Denn", "a