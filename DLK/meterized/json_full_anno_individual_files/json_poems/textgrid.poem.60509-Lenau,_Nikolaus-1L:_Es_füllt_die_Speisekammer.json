{"textgrid.poem.60509": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es f\u00fcllt die Speisekammer", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00fcllt die Speisekammer", "tokens": ["Es", "f\u00fcllt", "die", "Spei\u00b7se\u00b7kam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein bitterlicher Jammer,", "tokens": ["Ein", "bit\u00b7ter\u00b7li\u00b7cher", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wohl mit Fug und wohl mit Recht,", "tokens": ["Und", "wohl", "mit", "Fug", "und", "wohl", "mit", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn wie die Welt geworden schlecht,", "tokens": ["Denn", "wie", "die", "Welt", "ge\u00b7wor\u00b7den", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VAPP", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zeigt sich ein schn\u00f6d Exempel", "tokens": ["Zeigt", "sich", "ein", "schn\u00f6d", "Ex\u00b7em\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In diesem Magentempel.", "tokens": ["In", "die\u00b7sem", "Ma\u00b7gen\u00b7tem\u00b7pel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.7": {"text": "Die Mutter steht betroffen", "tokens": ["Die", "Mut\u00b7ter", "steht", "be\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An den beraubten Brettern", "tokens": ["An", "den", "be\u00b7raub\u00b7ten", "Bret\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und ruft in Zorneswettern:", "tokens": ["Und", "ruft", "in", "Zor\u00b7nes\u00b7wet\u00b7tern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "\u00bbwer lie\u00df das Fenster offen?\u00ab", "tokens": ["\u00bb", "wer", "lie\u00df", "das", "Fens\u00b7ter", "of\u00b7fen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wenn sie nicht Christin w\u00e4re", "tokens": ["Wenn", "sie", "nicht", "Chris\u00b7tin", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "NE", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und eingedenk der Lehre:", "tokens": ["Und", "ein\u00b7ge\u00b7denk", "der", "Leh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "\u203adu sollst dem Feind vergeben\u2039,", "tokens": ["\u203a", "du", "sollst", "dem", "Feind", "ver\u00b7ge\u00b7ben", "\u2039", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "VVPP", "$(", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Der Eingriff in ihr Leben,", "tokens": ["Der", "Ein\u00b7griff", "in", "ihr", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "In ihren Speisest\u00e4nder,", "tokens": ["In", "ih\u00b7ren", "Spei\u00b7se\u00b7st\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Er k\u00f6nnte sie versuchen,", "tokens": ["Er", "k\u00f6nn\u00b7te", "sie", "ver\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Den R\u00e4uber zu verfluchen,", "tokens": ["Den", "R\u00e4u\u00b7ber", "zu", "ver\u00b7flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Den Magentempelsch\u00e4nder.", "tokens": ["Den", "Ma\u00b7gen\u00b7tem\u00b7pel\u00b7sch\u00e4n\u00b7der", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Sie blickt nach ihren Sch\u00e4tzen,", "tokens": ["Sie", "blickt", "nach", "ih\u00b7ren", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Und ach! erblickt sie nicht,", "tokens": ["Und", "ach", "!", "er\u00b7blickt", "sie", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Da bleicht ihr Angesicht", "tokens": ["Da", "bleicht", "ihr", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Hausfrauliches Entsetzen.", "tokens": ["Haus\u00b7frau\u00b7li\u00b7ches", "Ent\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Sie forscht in ihrem Schrecke", "tokens": ["Sie", "forscht", "in", "ih\u00b7rem", "Schre\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Vergebens nach dem Specke,", "tokens": ["Ver\u00b7ge\u00b7bens", "nach", "dem", "Spe\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Er ist bei Nacht verschwunden,", "tokens": ["Er", "ist", "bei", "Nacht", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Trotz unseren drei Hunden.", "tokens": ["Trotz", "un\u00b7se\u00b7ren", "drei", "Hun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Sie sucht in ihrem Gram", "tokens": ["Sie", "sucht", "in", "ih\u00b7rem", "Gram"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Das Leibgericht der Wiener,", "tokens": ["Das", "Leib\u00b7ge\u00b7richt", "der", "Wie\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Das auch abhanden kam,", "tokens": ["Das", "auch", "ab\u00b7han\u00b7den", "kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Die braungebacknen H\u00fchner.", "tokens": ["Die", "braun\u00b7ge\u00b7back\u00b7nen", "H\u00fch\u00b7ner", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "H\u00fchnlein sind abgezogen,", "tokens": ["H\u00fchn\u00b7lein", "sind", "ab\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.32": {"text": "Dem Specke nachgeflogen,", "tokens": ["Dem", "Spe\u00b7cke", "nach\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Sie sind vorbeigeschwunden", "tokens": ["Sie", "sind", "vor\u00b7bei\u00b7ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "An drei verschlafnen Hunden.", "tokens": ["An", "drei", "ver\u00b7schlaf\u00b7nen", "Hun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Jetzt fa\u00dft ein t\u00f6dlich Grauen", "tokens": ["Jetzt", "fa\u00dft", "ein", "t\u00f6d\u00b7lich", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Die h\u00e4uslichste der Frauen,", "tokens": ["Die", "h\u00e4us\u00b7lichs\u00b7te", "der", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Sie ist ins Herz verletzt,", "tokens": ["Sie", "ist", "ins", "Herz", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Der Jammer packt sie jetzt", "tokens": ["Der", "Jam\u00b7mer", "packt", "sie", "jetzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Mit seiner ganzen St\u00e4rke,", "tokens": ["Mit", "sei\u00b7ner", "gan\u00b7zen", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Es ist ein Streich zum Weinen:", "tokens": ["Es", "ist", "ein", "Streich", "zum", "Wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Geraubt sind auch die feinen", "tokens": ["Ge\u00b7raubt", "sind", "auch", "die", "fei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Geburtstagszuckerwerke!", "tokens": ["Ge\u00b7burts\u00b7tags\u00b7zu\u00b7cker\u00b7wer\u00b7ke", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Nun steht sie da ergrimmt,", "tokens": ["Nun", "steht", "sie", "da", "er\u00b7grimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Ihr Auge gl\u00fcht und schwimmt", "tokens": ["Ihr", "Au\u00b7ge", "gl\u00fcht", "und", "schwimmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "In wirtschaftlichen Tr\u00e4nen,", "tokens": ["In", "wirt\u00b7schaft\u00b7li\u00b7chen", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Unchristlich, doch von Herzen", "tokens": ["Un\u00b7christ\u00b7lich", ",", "doch", "von", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "W\u00fcnscht sie drei Tage Schmerzen", "tokens": ["W\u00fcnscht", "sie", "drei", "Ta\u00b7ge", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "CARD", "NN", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.48": {"text": "Den frechen Diebesz\u00e4hnen.", "tokens": ["Den", "fre\u00b7chen", "Die\u00b7bes\u00b7z\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Jetzt sammeln sich die Kinder", "tokens": ["Jetzt", "sam\u00b7meln", "sich", "die", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Und klagen nicht gelinder,", "tokens": ["Und", "kla\u00b7gen", "nicht", "ge\u00b7lin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Und aus der bittern Klage", "tokens": ["Und", "aus", "der", "bit\u00b7tern", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Entspringt die gro\u00dfe Frage:", "tokens": ["Ent\u00b7springt", "die", "gro\u00b7\u00dfe", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "\u00bbhat sich ein Mensch vergessen?", "tokens": ["\u00bb", "hat", "sich", "ein", "Mensch", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.54": {"text": "Hat dies ein Tier gefressen?\u00ab", "tokens": ["Hat", "dies", "ein", "Tier", "ge\u00b7fres\u00b7sen", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Als eurer Zweifel Richter", "tokens": ["Als", "eu\u00b7rer", "Zwei\u00b7fel", "Rich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "La\u00dft gelten einen Dichter:", "tokens": ["La\u00dft", "gel\u00b7ten", "ei\u00b7nen", "Dich\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.57": {"text": "Was hier dem Dieb gefiel,", "tokens": ["Was", "hier", "dem", "Dieb", "ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.58": {"text": "Zu vielerlei und viel", "tokens": ["Zu", "vie\u00b7ler\u00b7lei", "und", "viel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "Wills meinem Sinne scheinen", "tokens": ["Wills", "mei\u00b7nem", "Sin\u00b7ne", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "F\u00fcr eines Tieres Fra\u00df;", "tokens": ["F\u00fcr", "ei\u00b7nes", "Tie\u00b7res", "Fra\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.61": {"text": "Drum soll ich lieber meinen,", "tokens": ["Drum", "soll", "ich", "lie\u00b7ber", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.62": {"text": "Da\u00df sich ein Mensch verga\u00df.", "tokens": ["Da\u00df", "sich", "ein", "Mensch", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.63": {"text": "Doch mu\u00df ich wieder glauben", "tokens": ["Doch", "mu\u00df", "ich", "wie\u00b7der", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "Trotz viel und vielerlei,", "tokens": ["Trotz", "viel", "und", "vie\u00b7ler\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "KON", "PIAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.65": {