{"dta.poem.18232": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Lied, das dir mein Alles klagt;", "tokens": ["Ein", "Lied", ",", "das", "dir", "mein", "Al\u00b7les", "klagt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann mag die Harfe mir zerspringen,", "tokens": ["Dann", "mag", "die", "Har\u00b7fe", "mir", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann hat mein Herz genug gesagt.", "tokens": ["Dann", "hat", "mein", "Herz", "ge\u00b7nug", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch woher nehme ich die Worte,", "tokens": ["Doch", "wo\u00b7her", "neh\u00b7me", "ich", "die", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie finde ich die Melodie?", "tokens": ["Wie", "fin\u00b7de", "ich", "die", "Me\u00b7lo\u00b7die", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Blume meiner Lust verdorrte", "tokens": ["Die", "Blu\u00b7me", "mei\u00b7ner", "Lust", "ver\u00b7dorr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit ihr starb die Poesie!", "tokens": ["Und", "mit", "ihr", "starb", "die", "Poe\u00b7sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich f\u00fchl's, eh mir der Tod begegnet,", "tokens": ["Ich", "f\u00fchl's", ",", "eh", "mir", "der", "Tod", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat sich mein Lied nicht ausgeweint \u2014", "tokens": ["Hat", "sich", "mein", "Lied", "nicht", "aus\u00b7ge\u00b7weint"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PPOSAT", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn was verflucht zugleich und segnet,", "tokens": ["Denn", "was", "ver\u00b7flucht", "zu\u00b7gleich", "und", "seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Wort noch hat's in sich vereint.", "tokens": ["Kein", "Wort", "noch", "hat's", "in", "sich", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VAFIN", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}