{"dta.poem.21753": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Erscheinung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lag auf gr&#252;nen Matten,", "tokens": ["Ich", "lag", "auf", "gr", "&#252;", "nen", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An klarer Quellen Rand.", "tokens": ["An", "kla\u00b7rer", "Quel\u00b7len", "Rand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir k\u00fchlten Erlenschatten", "tokens": ["Mir", "k\u00fchl\u00b7ten", "Er\u00b7len\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Wangen heissen Brand.", "tokens": ["Der", "Wan\u00b7gen", "heis\u00b7sen", "Brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich dachte diess und jenes,", "tokens": ["Ich", "dach\u00b7te", "diess", "und", "je\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "KON", "PDS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und tr\u00e4umte sanftbetr\u00fcbt", "tokens": ["Und", "tr\u00e4um\u00b7te", "sanft\u00b7be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Viel S\u00fcsses mir und Sch\u00f6nes,", "tokens": ["Viel", "S\u00fcs\u00b7ses", "mir", "und", "Sch\u00f6\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das diese Welt nicht giebt.", "tokens": ["Das", "die\u00b7se", "Welt", "nicht", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und sieh dem Hayn entschwebte", "tokens": ["Und", "sieh", "dem", "Hayn", "ent\u00b7schweb\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein M\u00e4gdlein sonnenklar.", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "son\u00b7nen\u00b7klar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein weisser Schleyer webte", "tokens": ["Ein", "weis\u00b7ser", "Schle\u00b7yer", "web\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um ihr nussbraunes Haar.", "tokens": ["Um", "ihr", "nuss\u00b7brau\u00b7nes", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ihr Auge feucht und schimmernd", "tokens": ["Ihr", "Au\u00b7ge", "feucht", "und", "schim\u00b7mernd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Umfloss \u00e4therisch Blau.", "tokens": ["Um\u00b7floss", "\u00e4t\u00b7he\u00b7risch", "Blau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die Wimpern n\u00e4sste flimmernd", "tokens": ["Die", "Wim\u00b7pern", "n\u00e4ss\u00b7te", "flim\u00b7mernd"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Wehmuth Perlenthau.", "tokens": ["Der", "Weh\u00b7muth", "Per\u00b7lent\u00b7hau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein traurend L\u00e4cheln schwebte", "tokens": ["Ein", "trau\u00b7rend", "L\u00e4\u00b7cheln", "schweb\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um ihren s\u00fcssen Mund.", "tokens": ["Um", "ih\u00b7ren", "s\u00fcs\u00b7sen", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schauerte, sie bebte.", "tokens": ["Sie", "schau\u00b7er\u00b7te", ",", "sie", "beb\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Auge thr\u00e4nenwund,", "tokens": ["Ihr", "Au\u00b7ge", "thr\u00e4\u00b7nen\u00b7wund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Hinschaun liebesehnend,", "tokens": ["Ihr", "Hin\u00b7schaun", "lie\u00b7be\u00b7seh\u00b7nend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So w\u00e4hnt' ich, suchte mich.", "tokens": ["So", "w\u00e4hnt'", "ich", ",", "such\u00b7te", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer war wie ich so w\u00e4hnend,", "tokens": ["Wer", "war", "wie", "ich", "so", "w\u00e4h\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "KOKOM", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So selig wer, wie ich!", "tokens": ["So", "se\u00b7lig", "wer", ",", "wie", "ich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PWS", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich auf sie zu umfassen \u2014", "tokens": ["Ich", "auf", "sie", "zu", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ach! sie trat zur\u00fcck.", "tokens": ["Und", "ach", "!", "sie", "trat", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah sie schnell erblassen,", "tokens": ["Ich", "sah", "sie", "schnell", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tr\u00fcber ward ihr Blick.", "tokens": ["Und", "tr\u00fc\u00b7ber", "ward", "ihr", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie sah mich an so innig,", "tokens": ["Sie", "sah", "mich", "an", "so", "in\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie wies mit ihrer Hand", "tokens": ["Sie", "wies", "mit", "ih\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erhaben und tiefsinnig", "tokens": ["Er\u00b7ha\u00b7ben", "und", "tief\u00b7sin\u00b7nig"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gen Himmel, und verschwand.", "tokens": ["Gen", "Him\u00b7mel", ",", "und", "ver\u00b7schwand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Fahr wohl, fahr wohl Erscheinung!", "tokens": ["Fahr", "wohl", ",", "fahr", "wohl", "Er\u00b7schei\u00b7nung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADV", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Fahr wohl! dich kenn' ich wohl!", "tokens": ["Fahr", "wohl", "!", "dich", "kenn'", "ich", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und deines Winkes Meinung", "tokens": ["Und", "dei\u00b7nes", "Win\u00b7kes", "Mei\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versteh' ich, wie ich soll! \u2014", "tokens": ["Ver\u00b7steh'", "ich", ",", "wie", "ich", "soll", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl f\u00fcr die Zeit geschieden", "tokens": ["Wohl", "f\u00fcr", "die", "Zeit", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Eint uns ein sch\u00f6nres Band.", "tokens": ["Eint", "uns", "ein", "sch\u00f6n\u00b7res", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hoch droben, nicht hienieden", "tokens": ["Hoch", "dro\u00b7ben", ",", "nicht", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADV", "$,", "PTKNEG", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hat Lieb' ihr Vaterland.", "tokens": ["Hat", "Lieb'", "ihr", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}