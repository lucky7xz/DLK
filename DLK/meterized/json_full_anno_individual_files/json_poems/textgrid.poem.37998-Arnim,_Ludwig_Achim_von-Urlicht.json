{"textgrid.poem.37998": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Urlicht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O R\u00f6schen roth,", "tokens": ["O", "R\u00f6\u00b7schen", "roth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Mensch liegt in gr\u00f6ster Noth,", "tokens": ["Der", "Mensch", "liegt", "in", "gr\u00f6s\u00b7ter", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Mensch liegt in gr\u00f6ster Pein,", "tokens": ["Der", "Mensch", "liegt", "in", "gr\u00f6s\u00b7ter", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Je lieber m\u00f6gt ich im Himmel seyn.", "tokens": ["Je", "lie\u00b7ber", "m\u00f6gt", "ich", "im", "Him\u00b7mel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da kam ich auf einen breiten Weg,", "tokens": ["Da", "kam", "ich", "auf", "ei\u00b7nen", "brei\u00b7ten", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da kam ein Engellein und wollt mich abweisen,", "tokens": ["Da", "kam", "ein", "En\u00b7gel\u00b7lein", "und", "wollt", "mich", "ab\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ach nein ich lie\u00df mich nicht abweisen.", "tokens": ["Ach", "nein", "ich", "lie\u00df", "mich", "nicht", "ab\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ich bin von Gott, ich will wieder zu Gott,", "tokens": ["Ich", "bin", "von", "Gott", ",", "ich", "will", "wie\u00b7der", "zu", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "PPER", "VMFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Der liebe Gott wird mir ein Lichtchen geben,", "tokens": ["Der", "lie\u00b7be", "Gott", "wird", "mir", "ein", "Licht\u00b7chen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wird leuchten mir bis in das ewig selig Leben.", "tokens": ["Wird", "leuch\u00b7ten", "mir", "bis", "in", "das", "e\u00b7wig", "se\u00b7lig", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "PPER", "ADV", "APPR", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O R\u00f6schen roth,", "tokens": ["O", "R\u00f6\u00b7schen", "roth", ","], "token_info": ["