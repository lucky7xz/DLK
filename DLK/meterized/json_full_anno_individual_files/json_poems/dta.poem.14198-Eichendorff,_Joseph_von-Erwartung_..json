{"dta.poem.14198": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Erwartung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was rauscht? \u2014 ", "tokens": ["Was", "rauscht", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Nun, Wald, rausch' von den H\u00f6h'n,", "tokens": ["Nun", ",", "Wald", ",", "rausch'", "von", "den", "H\u00f6h'n", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun la\u00df Mond, Nacht und Sterne", "tokens": ["Nun", "la\u00df", "Mond", ",", "Nacht", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur auf und untergeh'n!", "tokens": ["Nur", "auf", "und", "un\u00b7ter\u00b7geh'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}