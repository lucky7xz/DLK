{"dta.poem.2382": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Trochaische Ringelverse/  \n eines betr\u00fcbten S\u00fcnders.  \n Auff die Worte:  \n Mein Gott/ mein Gott/ war\u00fcm hastu  \n mich verlassen?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ach mein Gott mein treuer Gott war\u00fcm hastu mich", "tokens": ["Ach", "mein", "Gott", "mein", "treu\u00b7er", "Gott", "wa\u00b7r\u00fcm", "has\u00b7tu", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "NE", "VAFIN", "PPER"], "meter": "+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "verlassen/", "tokens": ["ver\u00b7las\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Mich verwirrt- verirrten Menschen/ mein sehr", "tokens": ["Mich", "ver\u00b7wirr\u00b7t", "ver\u00b7irr\u00b7ten", "Men\u00b7schen", "/", "mein", "sehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "TRUNC", "ADJA", "NN", "$(", "PPOSAT", "ADV"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "schwaches Fleisch und Blut", "tokens": ["schwa\u00b7ches", "Fleisch", "und", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Jrret kl\u00e4glich hin und her/ auff klippschl\u00fcpferige\u0303 Strassen/", "tokens": ["Jr\u00b7ret", "kl\u00e4g\u00b7lich", "hin", "und", "her", "/", "auff", "klipp\u00b7schl\u00fcp\u00b7fe\u00b7rig\u1ebd", "Stras\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$(", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Fehlet doch des rechten Weges der uns das gerechte Gut", "tokens": ["Feh\u00b7let", "doch", "des", "rech\u00b7ten", "We\u00b7ges", "der", "uns", "das", "ge\u00b7rech\u00b7te", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Zu dem Himmel zeigen soll; Diese Welt mit ihren G\u00fcnste\u0303", "tokens": ["Zu", "dem", "Him\u00b7mel", "zei\u00b7gen", "soll", ";", "Die\u00b7se", "Welt", "mit", "ih\u00b7ren", "G\u00fcnst\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "VMFIN", "$.", "PDAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Lauffet \u00fcm mich her mit Lachen da\u00df sie meinen leichten", "tokens": ["Lauf\u00b7fet", "\u00fcm", "mich", "her", "mit", "La\u00b7chen", "da\u00df", "sie", "mei\u00b7nen", "leich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APZR", "APPR", "NN", "KOUS", "PPER", "PPOSAT", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Sinn", "tokens": ["Sinn"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Ins Verderben f\u00fchren mag; mit den gro\u00df- gemachten", "tokens": ["Ins", "Ver\u00b7der\u00b7ben", "f\u00fch\u00b7ren", "mag", ";", "mit", "den", "gro\u00df", "ge\u00b7mach\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$.", "APPR", "ART", "TRUNC", "ADJA"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "D\u00fcnsten", "tokens": ["D\u00fcns\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Der beschminkten Eitelkeiten lokket mich die Falsche hin/", "tokens": ["Der", "be\u00b7schmink\u00b7ten", "Ei\u00b7tel\u00b7kei\u00b7ten", "lok\u00b7ket", "mich", "die", "Fal\u00b7sche", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Dort auff jenen breiten Weg. Solcher vorgesatzter massen/", "tokens": ["Dort", "auff", "je\u00b7nen", "brei\u00b7ten", "Weg", ".", "Sol\u00b7cher", "vor\u00b7ge\u00b7satz\u00b7ter", "mas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$.", "PIAT", "ADJA", "NN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Wo du nicht mein h\u00f6chster Helffer/ du O mein HErr", "tokens": ["Wo", "du", "nicht", "mein", "h\u00f6chs\u00b7ter", "Helf\u00b7fer", "/", "du", "O", "mein", "Herr"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN", "$(", "PPER", "NE", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Jesu Christ", "tokens": ["Je\u00b7su", "Christ"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Bester Trost/ mit deinem Geist\u2019/ hier auff dieser Lebens-", "tokens": ["Bes\u00b7ter", "Trost", "/", "mit", "dei\u00b7nem", "Geist'", "/", "hier", "auff", "die\u00b7ser", "Le\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "$(", "ADV", "APPR", "PDAT", "TRUNC"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.17": {"text": "strassen", "tokens": ["stras\u00b7sen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Mich Verf\u00fchreten begleitest/ und so mein Gefehrde bist", "tokens": ["Mich", "Ver\u00b7f\u00fch\u00b7re\u00b7ten", "be\u00b7glei\u00b7test", "/", "und", "so", "mein", "Ge\u00b7fehr\u00b7de", "bist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "$(", "KON", "ADV", "PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Ach so ist es mit mir au\u00df! Ich verbleib\u2019 im S\u00fcndenstege", "tokens": ["Ach", "so", "ist", "es", "mit", "mir", "au\u00df", "!", "Ich", "ver\u00b7bleib'", "im", "S\u00fcn\u00b7dens\u00b7te\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "APPR", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Mein betr\u00fcbtes Fleisch das irret bi\u00df zum hellschen Tar-", "tokens": ["Mein", "be\u00b7tr\u00fcb\u00b7tes", "Fleisch", "das", "ir\u00b7ret", "bi\u00df", "zum", "hell\u00b7schen", "Ta\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PDS", "VVFIN", "APPR", "APPRART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "tarus.", "tokens": ["ta\u00b7rus", "."], "token_info": ["word", "punct"], "pos": ["FM.la", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Dar\u00fcm f\u00fchre du mich nur au\u00df dem falschen Fehlerwege/", "tokens": ["Da\u00b7r\u00fcm", "f\u00fch\u00b7re", "du", "mich", "nur", "au\u00df", "dem", "fal\u00b7schen", "Feh\u00b7ler\u00b7we\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.23": {"text": "Da\u00df ich dein verirter Pilgram stets nach dir nicht seuff-", "tokens": ["Da\u00df", "ich", "dein", "ver\u00b7ir\u00b7ter", "Pilg\u00b7ram", "stets", "nach", "dir", "nicht", "seuff"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "ADV", "APPR", "PPER", "PTKNEG", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.24": {"text": "tze\u0303n mu\u00df:", "tokens": ["tz\u1ebdn", "mu\u00df", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Ach mein Gott/ mein treuer Gott war\u00fcm hastu mich ver-", "tokens": ["Ach", "mein", "Gott", "/", "mein", "treu\u00b7er", "Gott", "wa\u00b7r\u00fcm", "has\u00b7tu", "mich", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "NE", "VAFIN", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.26": {"text": "lassen?", "tokens": ["las\u00b7sen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}