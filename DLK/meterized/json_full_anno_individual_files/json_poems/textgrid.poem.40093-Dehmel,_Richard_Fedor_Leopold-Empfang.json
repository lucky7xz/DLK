{"textgrid.poem.40093": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Empfang", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In den Kreis der Zechgenossen", "tokens": ["In", "den", "Kreis", "der", "Zech\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "bin ich wieder eingekehrt,", "tokens": ["bin", "ich", "wie\u00b7der", "ein\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wo man mit den alten Possen", "tokens": ["wo", "man", "mit", "den", "al\u00b7ten", "Pos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bacchus und Gambrin noch ehrt.", "tokens": ["Bac\u00b7chus", "und", "Gam\u00b7brin", "noch", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "An Comment und Schl\u00e4gerhieben", "tokens": ["An", "Com\u00b7ment", "und", "Schl\u00e4\u00b7ge\u00b7rhie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "h\u00e4nget da der Freundschaft Band,", "tokens": ["h\u00e4n\u00b7get", "da", "der", "Freund\u00b7schaft", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "doch im Wappen steht geschrieben:", "tokens": ["doch", "im", "Wap\u00b7pen", "steht", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freiheit, Ehre, Vaterland!", "tokens": ["Frei\u00b7heit", ",", "Eh\u00b7re", ",", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar ert\u00f6nt bei ihren Festen", "tokens": ["Zwar", "er\u00b7t\u00f6nt", "bei", "ih\u00b7ren", "Fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "manches gro\u00dfe, volle Wort:", "tokens": ["man\u00b7ches", "gro\u00b7\u00dfe", ",", "vol\u00b7le", "Wort", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "zugeschnitten aus den Resten", "tokens": ["zu\u00b7ge\u00b7schnit\u00b7ten", "aus", "den", "Res\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "toten Ernstes \u2013 nun ein Sport!", "tokens": ["to\u00b7ten", "Erns\u00b7tes", "\u2013", "nun", "ein", "Sport", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und sie haben mich empfangen,", "tokens": ["Und", "sie", "ha\u00b7ben", "mich", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie man's Einst beim Willkomm hielt;", "tokens": ["wie", "man's", "Einst", "beim", "Will\u00b7komm", "hielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "doch aus ihrer Worte Prangen", "tokens": ["doch", "aus", "ih\u00b7rer", "Wor\u00b7te", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "blassen Augs die L\u00fcge schielt.", "tokens": ["blas\u00b7sen", "Augs", "die", "L\u00fc\u00b7ge", "schielt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ich sa\u00df und sa\u00df und suchte", "tokens": ["Und", "ich", "sa\u00df", "und", "sa\u00df", "und", "such\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "bis ich ihrem Anstand fluchte", "tokens": ["bis", "ich", "ih\u00b7rem", "An\u00b7stand", "fluch\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und mich still vondannen stahl.", "tokens": ["und", "mich", "still", "von\u00b7dan\u00b7nen", "stahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "I