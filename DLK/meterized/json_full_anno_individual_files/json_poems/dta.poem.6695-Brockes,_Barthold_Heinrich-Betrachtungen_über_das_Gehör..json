{"dta.poem.6695": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man wird am kr\u00e4fftigsten ger\u00fchret", "tokens": ["Man", "wird", "am", "kr\u00e4ff\u00b7tigs\u00b7ten", "ge\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Durch T\u00f6ne, so die Red-Kunst f\u00fchret;", "tokens": ["Durch", "T\u00f6\u00b7ne", ",", "so", "die", "Re\u00b7dKunst", "f\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als die uns lencken und entz\u00fcnden,", "tokens": ["Als", "die", "uns", "len\u00b7cken", "und", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bes\u00e4nftigen, verbinden.", "tokens": ["Be\u00b7s\u00e4nf\u00b7ti\u00b7gen", ",", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Vergewisserung, Furcht, Freud und Traurigkeit,", "tokens": ["Die", "Ver\u00b7ge\u00b7wis\u00b7se\u00b7rung", ",", "Furcht", ",", "Freud", "und", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Besitzen in sich eine Macht,", "tokens": ["Be\u00b7sit\u00b7zen", "in", "sich", "ei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch deren Biegung allezeit", "tokens": ["Durch", "de\u00b7ren", "Bie\u00b7gung", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Geist wird in Bewegung bracht.", "tokens": ["Der", "Geist", "wird", "in", "Be\u00b7we\u00b7gung", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ein recht ger\u00fchrter Geist wird andre Geister r\u00fchren:", "tokens": ["Ein", "recht", "ge\u00b7r\u00fchr\u00b7ter", "Geist", "wird", "and\u00b7re", "Geis\u00b7ter", "r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er macht ihn brennen und gefrieren,", "tokens": ["Er", "macht", "ihn", "bren\u00b7nen", "und", "ge\u00b7frie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Erregt ihm seine Lust und seiner Triebe Krafft,", "tokens": ["Er\u00b7regt", "ihm", "sei\u00b7ne", "Lust", "und", "sei\u00b7ner", "Trie\u00b7be", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und zeugt in ihm sein\u2019 eigne Leidenschafft.", "tokens": ["Und", "zeugt", "in", "ihm", "sein'", "eig\u00b7ne", "Lei\u00b7den\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}