{"dta.poem.9226": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  \n Die endliche Erkl\u00e4rung.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin betr\u00fcbt/", "tokens": ["Ich", "bin", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Weil mich mein kind nicht wieder liebt/", "tokens": ["Weil", "mich", "mein", "kind", "nicht", "wie\u00b7der", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn dieses feuer meiner pein", "tokens": ["Denn", "die\u00b7ses", "feu\u00b7er", "mei\u00b7ner", "pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df noch zur zeit verborgen seyn.", "tokens": ["Mu\u00df", "noch", "zur", "zeit", "ver\u00b7bor\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und die mir gef\u00e4llet/ die wei\u00df es noch nicht/", "tokens": ["Und", "die", "mir", "ge\u00b7f\u00e4l\u00b7let", "/", "die", "wei\u00df", "es", "noch", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "VVPP", "$(", "PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und wenn sie es w\u00fcste/ so glaubte sie es nicht/", "tokens": ["Und", "wenn", "sie", "es", "w\u00fcs\u00b7te", "/", "so", "glaub\u00b7te", "sie", "es", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und wenn sie es glaubte/ so sagte sie es nicht/", "tokens": ["Und", "wenn", "sie", "es", "glaub\u00b7te", "/", "so", "sag\u00b7te", "sie", "es", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und wenn sie es sagte/ so traut ich ihr nicht/", "tokens": ["Und", "wenn", "sie", "es", "sag\u00b7te", "/", "so", "traut", "ich", "ihr", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Und wenn ich ihr traute/ so h\u00fclffe michs nicht;", "tokens": ["Und", "wenn", "ich", "ihr", "trau\u00b7te", "/", "so", "h\u00fclf\u00b7fe", "michs", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Drum lieget mein hertze gefangen/ und spricht:", "tokens": ["Drum", "lie\u00b7get", "mein", "hert\u00b7ze", "ge\u00b7fan\u00b7gen", "/", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$(", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Es ist um mich geschehn/", "tokens": ["Es", "ist", "um", "mich", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ich bin zu lauter qual versehn.", "tokens": ["Ich", "bin", "zu", "lau\u00b7ter", "qual", "ver\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Ich mache zwar", "tokens": ["Ich", "ma\u00b7che", "zwar"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mein hertz bi\u00dfweilen offenbahr/", "tokens": ["Mein", "hertz", "bi\u00df\u00b7wei\u00b7len", "of\u00b7fen\u00b7bahr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und singe manch betr\u00fcbtes lied/", "tokens": ["Und", "sin\u00b7ge", "manch", "be\u00b7tr\u00fcb\u00b7tes", "lied", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das blos auf ihre sch\u00f6nheit sieht:", "tokens": ["Das", "blos", "auf", "ih\u00b7re", "sch\u00f6n\u00b7heit", "sieht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch/ wann ich es singe/ so h\u00f6rt sie es nicht/", "tokens": ["Doch", "/", "wann", "ich", "es", "sin\u00b7ge", "/", "so", "h\u00f6rt", "sie", "es", "nicht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWAV", "PPER", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und wenn sie es h\u00f6rte/ so merckte sies nicht/", "tokens": ["Und", "wenn", "sie", "es", "h\u00f6r\u00b7te", "/", "so", "merck\u00b7te", "sies", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und wenn sie es merckte/ so wolte sie nicht/", "tokens": ["Und", "wenn", "sie", "es", "merck\u00b7te", "/", "so", "wol\u00b7te", "sie", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$(", "ADV", "VMFIN", "PPER", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und wenn sie auch wolte/ so d\u00fcrffte sie nicht/", "tokens": ["Und", "wenn", "sie", "auch", "wol\u00b7te", "/", "so", "d\u00fcrff\u00b7te", "sie", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VMFIN", "$(", "ADV", "VMFIN", "PPER", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Und wann sie auch d\u00fcrffte/ so schickte sichs nicht/", "tokens": ["Und", "wann", "sie", "auch", "d\u00fcrff\u00b7te", "/", "so", "schick\u00b7te", "sichs", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VMFIN", "$(", "ADV", "VVFIN", "PIS", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Indessen so bin ich dem m\u00e4dgen verpicht/", "tokens": ["In\u00b7des\u00b7sen", "so", "bin", "ich", "dem", "m\u00e4d\u00b7gen", "ver\u00b7picht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Und denck in meiner noth/", "tokens": ["Und", "denck", "in", "mei\u00b7ner", "noth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ist doch die hoffnung noch nicht todt.", "tokens": ["Ist", "doch", "die", "hoff\u00b7nung", "noch", "nicht", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ich seh es wohl/", "tokens": ["Ich", "seh", "es", "wohl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wer im gedancken wuchern sol/", "tokens": ["Wer", "im", "ge\u00b7dan\u00b7cken", "wu\u00b7chern", "sol", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bi\u00df ihm das gute gl\u00fccke rufft/", "tokens": ["Bi\u00df", "ihm", "das", "gu\u00b7te", "gl\u00fc\u00b7cke", "rufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der baut nur schl\u00f6sser in die lufft;", "tokens": ["Der", "baut", "nur", "schl\u00f6s\u00b7ser", "in", "die", "lufft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch/ hilfft es mich wenig/ so schad mir es nicht/", "tokens": ["Doch", "/", "hilfft", "es", "mich", "we\u00b7nig", "/", "so", "schad", "mir", "es", "nicht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "PPER", "PRF", "ADV", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und wann es mir schade/ so schmertzte michs nicht/", "tokens": ["Und", "wann", "es", "mir", "scha\u00b7de", "/", "so", "schmertz\u00b7te", "michs", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVFIN", "$(", "ADV", "VVFIN", "PIS", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und wann es mich schmertzte/ so kr\u00e4nckte michs nicht/", "tokens": ["Und", "wann", "es", "mich", "schmertz\u00b7te", "/", "so", "kr\u00e4nck\u00b7te", "michs", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$(", "ADV", "VVFIN", "PIS", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und wann es mich kr\u00e4nckte/ so st\u00fcrb ich doch nicht/", "tokens": ["Und", "wann", "es", "mich", "kr\u00e4nck\u00b7te", "/", "so", "st\u00fcrb", "ich", "doch", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Und wann ich auch st\u00fcrbe/ so lie\u00df ich es nicht", "tokens": ["Und", "wann", "ich", "auch", "st\u00fcr\u00b7be", "/", "so", "lie\u00df", "ich", "es", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Sie bliebe mein leben/ sie bliebe mein liecht", "tokens": ["Sie", "blie\u00b7be", "mein", "le\u00b7ben", "/", "sie", "blie\u00b7be", "mein", "liecht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVINF", "$(", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Derhalben bleibts darbey/", "tokens": ["Der\u00b7hal\u00b7ben", "bleibts", "dar\u00b7bey", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ich bin ihr gut und ewig treu.", "tokens": ["Ich", "bin", "ihr", "gut", "und", "e\u00b7wig", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Ich ungewi\u00df in meinen sinn/", "tokens": ["Ich", "un\u00b7ge\u00b7wi\u00df", "in", "mei\u00b7nen", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was ich nicht erlangen kan/", "tokens": ["Und", "was", "ich", "nicht", "er\u00b7lan\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seh ich mit schwacher hoffnung an/", "tokens": ["Seh", "ich", "mit", "schwa\u00b7cher", "hoff\u00b7nung", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie kennet mich endlich/ und kennet mich nicht.", "tokens": ["Sie", "ken\u00b7net", "mich", "end\u00b7lich", "/", "und", "ken\u00b7net", "mich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$(", "KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Ich brenne vor liebe/ sie liebet mich nicht/", "tokens": ["Ich", "bren\u00b7ne", "vor", "lie\u00b7be", "/", "sie", "lie\u00b7bet", "mich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Doch/ weil sie nicht liebet/ so hasst sie auch nicht.", "tokens": ["Doch", "/", "weil", "sie", "nicht", "lie\u00b7bet", "/", "so", "hasst", "sie", "auch", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "PPER", "PTKNEG", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und weil sie nicht hasset/ so f\u00fcrcht ich mich nicht/", "tokens": ["Und", "weil", "sie", "nicht", "has\u00b7set", "/", "so", "f\u00fcrcht", "ich", "mich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Und wo ich mich f\u00fcrchte/ verzweiffel ich nicht", "tokens": ["Und", "wo", "ich", "mich", "f\u00fcrch\u00b7te", "/", "ver\u00b7zweif\u00b7fel", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN", "$(", "VVIMP", "PPER", "PTKNEG"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Drum tr\u00f6stet mein hertze sich selber und spricht:", "tokens": ["Drum", "tr\u00f6s\u00b7tet", "mein", "hert\u00b7ze", "sich", "sel\u00b7ber", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "VVFIN", "PRF", "ADV", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Es ist noch nicht geschehn/", "tokens": ["Es", "ist", "noch", "nicht", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Ich bin noch wohl darzu versehn.", "tokens": ["Ich", "bin", "noch", "wohl", "dar\u00b7zu", "ver\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}