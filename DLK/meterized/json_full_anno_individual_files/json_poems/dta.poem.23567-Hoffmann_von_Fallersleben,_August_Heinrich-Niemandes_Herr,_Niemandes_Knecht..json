{"dta.poem.23567": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Niemandes Herr, Niemandes Knecht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Ambo\u00df hielt ich mich zu schlecht,", "tokens": ["Zum", "Am\u00b7bo\u00df", "hielt", "ich", "mich", "zu", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Hammer war ich euch nicht recht.", "tokens": ["Zum", "Ham\u00b7mer", "war", "ich", "euch", "nicht", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So bin ich Ambo\u00df nicht noch Hammer", "tokens": ["So", "bin", "ich", "Am\u00b7bo\u00df", "nicht", "noch", "Ham\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "PTKNEG", "ADV", "NN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und rufe frei von Herzensjammer:", "tokens": ["Und", "ru\u00b7fe", "frei", "von", "Her\u00b7zens\u00b7jam\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So ist es gut, so ist es recht,", "tokens": ["So", "ist", "es", "gut", ",", "so", "ist", "es", "recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Niemandes Herr, Niemandes Knecht!", "tokens": ["Nie\u00b7man\u00b7des", "Herr", ",", "Nie\u00b7man\u00b7des", "Knecht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Fliegt frei der Vogel durch das Feld,", "tokens": ["Fliegt", "frei", "der", "Vo\u00b7gel", "durch", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ist noch sein die ganze Welt.", "tokens": ["So", "ist", "noch", "sein", "die", "gan\u00b7ze", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "M\u00fcsst' er im goldnen K\u00e4ficht hocken,", "tokens": ["M\u00fcsst'", "er", "im", "gold\u00b7nen", "K\u00e4\u00b7ficht", "ho\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er w\u00fcrde schwerlich dort frohlocken:", "tokens": ["Er", "w\u00fcr\u00b7de", "schwer\u00b7lich", "dort", "froh\u00b7lo\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So ist es gut, so ist es recht,", "tokens": ["So", "ist", "es", "gut", ",", "so", "ist", "es", "recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Niemandes Herr, Niemandes Knecht!", "tokens": ["Nie\u00b7man\u00b7des", "Herr", ",", "Nie\u00b7man\u00b7des", "Knecht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}