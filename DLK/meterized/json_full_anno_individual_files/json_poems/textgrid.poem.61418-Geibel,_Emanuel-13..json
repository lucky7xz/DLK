{"textgrid.poem.61418": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jegliches gleichet sich aus. Die Gl\u00fccklichen sind wie die Kinder,", "tokens": ["Jeg\u00b7li\u00b7ches", "glei\u00b7chet", "sich", "aus", ".", "Die", "Gl\u00fcck\u00b7li\u00b7chen", "sind", "wie", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKVZ", "$.", "ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Froh durchs sonnige Tal wandeln sie ohne Bedacht;", "tokens": ["Froh", "durchs", "son\u00b7ni\u00b7ge", "Tal", "wan\u00b7deln", "sie", "oh\u00b7ne", "Be\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und sie brechen die purpurne Frucht und singen im Schatten", "tokens": ["Und", "sie", "bre\u00b7chen", "die", "pur\u00b7pur\u00b7ne", "Frucht", "und", "sin\u00b7gen", "im", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "APPRART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "M\u00fchlos heiter, es deucht ihnen das Leben ein Traum.", "tokens": ["M\u00fch\u00b7los", "hei\u00b7ter", ",", "es", "deucht", "ih\u00b7nen", "das", "Le\u00b7ben", "ein", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Aber das Ungl\u00fcck reift die k\u00f6stliche Perle der Weisheit,", "tokens": ["A\u00b7ber", "das", "Un\u00b7gl\u00fcck", "reift", "die", "k\u00f6st\u00b7li\u00b7che", "Per\u00b7le", "der", "Weis\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Schmerzlich gefurcht ist die Stirn, drin der Gedanke sich zeugt.", "tokens": ["Schmerz\u00b7lich", "ge\u00b7furcht", "ist", "die", "Stirn", ",", "drin", "der", "Ge\u00b7dan\u00b7ke", "sich", "zeugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Was dir gelang, leicht nimmst du es hin und genie\u00dfest es achtlos,", "tokens": ["Was", "dir", "ge\u00b7lang", ",", "leicht", "nimmst", "du", "es", "hin", "und", "ge\u00b7nie\u00b7\u00dfest", "es", "acht\u00b7los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Was du verfehltest, es schlie\u00dft immer ein R\u00e4tsel dir auf.", "tokens": ["Was", "du", "ver\u00b7fehl\u00b7test", ",", "es", "schlie\u00dft", "im\u00b7mer", "ein", "R\u00e4t\u00b7sel", "dir", "auf", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Drum so du scheitertest, grolle du nie. Aus jeglichem Schiffbruch", "tokens": ["Drum", "so", "du", "schei\u00b7ter\u00b7test", ",", "grol\u00b7le", "du", "nie", ".", "Aus", "jeg\u00b7li\u00b7chem", "Schiff\u00b7bruch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Geht der erhabene Geist gr\u00f6\u00dfer und reicher hervor.", "tokens": ["Geht", "der", "er\u00b7ha\u00b7be\u00b7ne", "Geist", "gr\u00f6\u00b7\u00dfer", "und", "rei\u00b7cher", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Jegliches gleichet sich aus. Die Gl\u00fccklichen sind wie die Kinder,", "tokens": ["Jeg\u00b7li\u00b7ches", "glei\u00b7chet", "sich", "aus", ".", "Die", "Gl\u00fcck\u00b7li\u00b7chen", "sind", "wie", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKVZ", "$.", "ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Froh durchs sonnige Tal wandeln sie ohne Bedacht;", "tokens": ["Froh", "durchs", "son\u00b7ni\u00b7ge", "Tal", "wan\u00b7deln", "sie", "oh\u00b7ne", "Be\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVFIN", "PPER