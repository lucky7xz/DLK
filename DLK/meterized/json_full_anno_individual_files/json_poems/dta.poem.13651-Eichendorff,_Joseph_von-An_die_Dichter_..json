{"dta.poem.13651": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Dichter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Vor Eitelkeit soll er vor Allen", "tokens": ["Vor", "Ei\u00b7tel\u00b7keit", "soll", "er", "vor", "Al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "APPR", "NE"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Streng h\u00fcten sein unschuld'ges Herz,", "tokens": ["Streng", "h\u00fc\u00b7ten", "sein", "un\u00b7schuld'\u00b7ges", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Falschen nimmer sich gefallen,", "tokens": ["Im", "Fal\u00b7schen", "nim\u00b7mer", "sich", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um eitel Witz und blanken Scherz.", "tokens": ["Um", "ei\u00b7tel", "Witz", "und", "blan\u00b7ken", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}