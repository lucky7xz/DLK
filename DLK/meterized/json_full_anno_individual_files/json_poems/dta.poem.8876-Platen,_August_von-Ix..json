{"dta.poem.8876": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Ix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "So bist du mir der Gl\u00fcckliche hienieden;", "tokens": ["So", "bist", "du", "mir", "der", "Gl\u00fcck\u00b7li\u00b7che", "hien\u00b7ie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir ist ein leichter Lebenskampf beschieden,", "tokens": ["Dir", "ist", "ein", "leich\u00b7ter", "Le\u00b7bens\u00b7kampf", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn du verlierst, beginnst du neu zu wagen.", "tokens": ["Wenn", "du", "ver\u00b7lierst", ",", "be\u00b7ginnst", "du", "neu", "zu", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wenn du hast Treulosigkeit ertragen,", "tokens": ["Und", "wenn", "du", "hast", "Treu\u00b7lo\u00b7sig\u00b7keit", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als, die du liebtest, dich geha\u00dft, vermieden,", "tokens": ["Als", ",", "die", "du", "lieb\u00b7test", ",", "dich", "ge\u00b7ha\u00dft", ",", "ver\u00b7mie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und doch im Herzen nie verlorst den Frieden,", "tokens": ["Und", "doch", "im", "Her\u00b7zen", "nie", "ver\u00b7lorst", "den", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann ist die Zeit dir voll von sch\u00f6nen Tagen!", "tokens": ["Dann", "ist", "die", "Zeit", "dir", "voll", "von", "sch\u00f6\u00b7nen", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn jede Trennung du mit Muth verschmerzest,", "tokens": ["Wenn", "je\u00b7de", "Tren\u00b7nung", "du", "mit", "Muth", "ver\u00b7schmer\u00b7zest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn, da kaum ein Liebchen dich verlassen,", "tokens": ["Und", "wenn", ",", "da", "kaum", "ein", "Lieb\u00b7chen", "dich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du schon ein andres voll Verlangen herzest:", "tokens": ["Du", "schon", "ein", "and\u00b7res", "voll", "Ver\u00b7lan\u00b7gen", "her\u00b7zest", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "PIS", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann wei\u00dft du, traun! dich in der Welt zu fassen;", "tokens": ["Dann", "wei\u00dft", "du", ",", "traun", "!", "dich", "in", "der", "Welt", "zu", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVINF", "$.", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Leben st\u00fcrmt und w\u00fcthet, doch du scherzest,", "tokens": ["Das", "Le\u00b7ben", "st\u00fcrmt", "und", "w\u00fct\u00b7het", ",", "doch", "du", "scher\u00b7zest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit sanftem Hauch bewegend schwere Massen.", "tokens": ["Mit", "sanf\u00b7tem", "Hauch", "be\u00b7we\u00b7gend", "schwe\u00b7re", "Mas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}