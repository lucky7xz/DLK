{"dta.poem.16697": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "56.  Armut und Reichthumb.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der/ was er hat/ nicht hat/ und alles sch\u00e4tzet gleich/", "tokens": ["Der", "/", "was", "er", "hat", "/", "nicht", "hat", "/", "und", "al\u00b7les", "sch\u00e4t\u00b7zet", "gleich", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "VAFIN", "$(", "PTKNEG", "VAFIN", "$(", "KON", "PIS", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ist im Reichthum arm/ in Armuth ist er reich.", "tokens": ["Der", "ist", "im", "Reicht\u00b7hum", "arm", "/", "in", "Ar\u00b7muth", "ist", "er", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ADJD", "$(", "APPR", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}