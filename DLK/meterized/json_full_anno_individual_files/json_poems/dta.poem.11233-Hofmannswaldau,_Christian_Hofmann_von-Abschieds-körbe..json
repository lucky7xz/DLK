{"dta.poem.11233": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Abschieds-k\u00f6rbe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer auf die freyth will gehn,", "tokens": ["Wer", "auf", "die", "frey\u00b7th", "will", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mu\u00df stets in sorgen stehn,", "tokens": ["Mu\u00df", "stets", "in", "sor\u00b7gen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df man ihm nicht den tantz versage,", "tokens": ["Da\u00df", "man", "ihm", "nicht", "den", "tantz", "ver\u00b7sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit dem korbe weiter jage.", "tokens": ["Und", "mit", "dem", "kor\u00b7be", "wei\u00b7ter", "ja\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo einmahl sich die k\u00f6rbe hingew\u00f6hnen,", "tokens": ["Wo", "ein\u00b7mahl", "sich", "die", "k\u00f6r\u00b7be", "hin\u00b7ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PRF", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da pflegen sie vor mehr den weg zu b\u00e4hnen.", "tokens": ["Da", "pfle\u00b7gen", "sie", "vor", "mehr", "den", "weg", "zu", "b\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADV", "ART", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch ists noch gut, da\u00df sie nicht sichtbar seyn;", "tokens": ["Doch", "ists", "noch", "gut", ",", "da\u00df", "sie", "nicht", "sicht\u00b7bar", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sonst w\u00fcrden gar die m\u00e4gdgen", "tokens": ["Sonst", "w\u00fcr\u00b7den", "gar", "die", "m\u00e4gd\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Vor einem solchen korbe-tr\u00e4ger", "tokens": ["Vor", "ei\u00b7nem", "sol\u00b7chen", "kor\u00b7be\u00b7tr\u00e4\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie vor dem pop-hans schreyn.", "tokens": ["Wie", "vor", "dem", "pop\u00b7hans", "schreyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ach! k\u00e4men nur die moden auf,", "tokens": ["Ach", "!", "k\u00e4\u00b7men", "nur", "die", "mo\u00b7den", "auf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df selbst die m\u00e4gdgen m\u00fcsten gehn", "tokens": ["Da\u00df", "selbst", "die", "m\u00e4gd\u00b7gen", "m\u00fcs\u00b7ten", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "VVINF", "VMFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Auf diesen liebes-kauff!", "tokens": ["Auf", "die\u00b7sen", "lie\u00b7bes\u00b7kauff", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Was gilts? es w\u00fcrd\u2019 um sie vielleicht noch schlimmer", "tokens": ["Was", "gilts", "?", "es", "w\u00fcrd'", "um", "sie", "viel\u00b7leicht", "noch", "schlim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "stehn.", "tokens": ["stehn", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Viel m\u00e4gdgen w\u00fcrden sich gantz lahm an k\u00f6rben tragen:", "tokens": ["Viel", "m\u00e4gd\u00b7gen", "w\u00fcr\u00b7den", "sich", "gantz", "lahm", "an", "k\u00f6r\u00b7ben", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "PRF", "ADV", "PTKVZ", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nach angebotner gunst pflegt niemand viel zu fragen.", "tokens": ["Nach", "an\u00b7ge\u00b7bot\u00b7ner", "gunst", "pflegt", "nie\u00b7mand", "viel", "zu", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}