{"dta.poem.3227": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie kommt es, da du doch gern h\u00f6rst das Wasser rauschen,", "tokens": ["Wie", "kommt", "es", ",", "da", "du", "doch", "gern", "h\u00f6rst", "das", "Was\u00b7ser", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die L\u00fcfte fl\u00fcstern und die Zweige Gr\u00fc\u00dfe tauschen;", "tokens": ["Die", "L\u00fcf\u00b7te", "fl\u00fcs\u00b7tern", "und", "die", "Zwei\u00b7ge", "Gr\u00fc\u00b7\u00dfe", "tau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie kommt es, da du gern die unverstandnen Lieder", "tokens": ["Wie", "kommt", "es", ",", "da", "du", "gern", "die", "un\u00b7ver\u00b7stand\u00b7nen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Vogels h\u00f6rst, da\u00df dir ist dieses Lied zuwider?", "tokens": ["Des", "Vo\u00b7gels", "h\u00f6rst", ",", "da\u00df", "dir", "ist", "die\u00b7ses", "Lied", "zu\u00b7wi\u00b7der", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ist kein Verstand darin, betracht' als Klang es nur,", "tokens": ["Ist", "kein", "Ver\u00b7stand", "da\u00b7rin", ",", "be\u00b7tracht'", "als", "Klang", "es", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PAV", "$,", "VVFIN", "KOUS", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nimm es eben auch als Stimme der Natur. \u2014", "tokens": ["Und", "nimm", "es", "e\u00b7ben", "auch", "als", "Stim\u00b7me", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ADV", "KOUS", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gern h\u00f6r' ich die Natur in allen Stimmen reden,", "tokens": ["Gern", "h\u00f6r'", "ich", "die", "Na\u00b7tur", "in", "al\u00b7len", "Stim\u00b7men", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fchle jeden Ton, versteh' ich auch nicht jeden.", "tokens": ["Und", "f\u00fch\u00b7le", "je\u00b7den", "Ton", ",", "ver\u00b7steh'", "ich", "auch", "nicht", "je\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch das ist eine Pein, was klingt wie V\u00f6gelein,", "tokens": ["Doch", "das", "ist", "ei\u00b7ne", "Pein", ",", "was", "klingt", "wie", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "$,", "PWS", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Flut, Luft und Zweig, und will doch Menschensprache seyn.", "tokens": ["Flut", ",", "Luft", "und", "Zweig", ",", "und", "will", "doch", "Men\u00b7schen\u00b7spra\u00b7che", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "KON", "VMFIN", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}