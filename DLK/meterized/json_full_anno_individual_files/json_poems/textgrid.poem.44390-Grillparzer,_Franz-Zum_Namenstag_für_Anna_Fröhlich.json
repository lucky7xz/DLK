{"textgrid.poem.44390": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Zum Namenstag f\u00fcr Anna Fr\u00f6hlich", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch Nette zu heben,", "tokens": ["Hoch", "Net\u00b7te", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wir Feuerwerk geben,", "tokens": ["Wir", "Feu\u00b7er\u00b7werk", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Wenns das Wetter erlaubt.", "tokens": ["Wenns", "das", "Wet\u00b7ter", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Brillanten und Farben", "tokens": ["Bril\u00b7lan\u00b7ten", "und", "Far\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zu gro\u00dfem Applaus,", "tokens": ["Zu", "gro\u00b7\u00dfem", "Ap\u00b7plaus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.3": {"text": "Raketen in Garben", "tokens": ["Ra\u00b7ke\u00b7ten", "in", "Gar\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Kein Feurwerk f\u00fcrs Auge,", "tokens": ["Kein", "Feur\u00b7werk", "f\u00fcrs", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nein eines f\u00fcrs Ohr,", "tokens": ["Nein", "ei\u00b7nes", "f\u00fcrs", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ob das ", "tokens": ["Ob", "das"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Stellt das ", "tokens": ["Stellt", "das"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Die erste der Fronten", "tokens": ["Die", "ers\u00b7te", "der", "Fron\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zeigt Malerstafflei,", "tokens": ["Zeigt", "Ma\u00b7ler\u00b7staff\u00b7lei", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "So treu als wir konnten,", "tokens": ["So", "treu", "als", "wir", "konn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auch Pinsel dabei.", "tokens": ["Auch", "Pin\u00b7sel", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Angelika Kaufmann,", "tokens": ["An\u00b7ge\u00b7li\u00b7ka", "Kauf\u00b7mann", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "F\u00e4ngt hier ihren Lauf an,", "tokens": ["F\u00e4ngt", "hier", "ih\u00b7ren", "Lauf", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Bald weithin bekannt;", "tokens": ["Bald", "weit\u00b7hin", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Ihre Hand wird gepriesen,", "tokens": ["Ih\u00b7re", "Hand", "wird", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Die, ohn Pinsel und Stift,", "tokens": ["Die", ",", "ohn", "Pin\u00b7sel", "und", "Stift", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUI", "NN", "KON", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Oft als Kind schon bewiesen,", "tokens": ["Oft", "als", "Kind", "schon", "be\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "ADV", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Da\u00df Gesichter sie trifft.", "tokens": ["Da\u00df", "Ge\u00b7sich\u00b7ter", "sie", "trifft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Drauf zeigt sich ein ", "tokens": ["Drauf", "zeigt", "sich", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Von mohrischer Art,", "tokens": ["Von", "mo\u00b7hri\u00b7scher", "Art", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zwar l\u00e4nglicht und mager,", "tokens": ["Zwar", "l\u00e4ng\u00b7licht", "und", "ma\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Doch lieblich und zart;", "tokens": ["Doch", "lieb\u00b7lich", "und", "zart", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Sieh, Tr\u00e4nen entfallen,", "tokens": ["Sieh", ",", "Tr\u00e4\u00b7nen", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von Liebe verspritzt,", "tokens": ["Von", "Lie\u00b7be", "ver\u00b7spritzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Fr\u00f6mmsten von allen,", "tokens": ["Der", "Fr\u00f6mms\u00b7ten", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die D\u00f6bling besitzt.", "tokens": ["Die", "D\u00f6b\u00b7ling", "be\u00b7sitzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Dir Freude zu schaffen,", "tokens": ["Dir", "Freu\u00b7de", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verlie\u00df sie sogar", "tokens": ["Ver\u00b7lie\u00df", "sie", "so\u00b7gar"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die z\u00e4rtlichsten Affen,", "tokens": ["Die", "z\u00e4rt\u00b7lichs\u00b7ten", "Af\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ihr elterlich Paar;", "tokens": ["Ihr", "el\u00b7ter\u00b7lich", "Paar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Sie blecken die Z\u00e4hne,", "tokens": ["Sie", "ble\u00b7cken", "die", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verziehn das Gesicht,", "tokens": ["Ver\u00b7ziehn", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch fruchtlos, denn jene,", "tokens": ["Doch", "frucht\u00b7los", ",", "denn", "je\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KON", "PDS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie achtet es nicht.", "tokens": ["Sie", "ach\u00b7tet", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.11": {"line.1": {"text": "Dann breiter und stiller", "tokens": ["Dann", "brei\u00b7ter", "und", "stil\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Naht sich ", "tokens": ["Naht", "sich"], "token_info": ["word", "word"], "pos": ["NN", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sie schl\u00e4gt einen Triller", "tokens": ["Sie", "schl\u00e4gt", "ei\u00b7nen", "Tril\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und bleibet im Ton;", "tokens": ["Und", "blei\u00b7bet", "im", "Ton", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Wo L\u00fcge sonst schimmert,", "tokens": ["Wo", "L\u00fc\u00b7ge", "sonst", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Erh\u00e4lt sie sich wahr,", "tokens": ["Er\u00b7h\u00e4lt", "sie", "sich", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was andre verschlimmert,", "tokens": ["Was", "and\u00b7re", "ver\u00b7schlim\u00b7mert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Versch\u00f6nt sie wohl gar;", "tokens": ["Ver\u00b7sch\u00f6nt", "sie", "wohl", "gar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.13": {"line.1": {"text": "Und so wird sie bleiben", "tokens": ["Und", "so", "wird", "sie", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und so wird sie sein;", "tokens": ["Und", "so", "wird", "sie", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mag Rauch sie umst\u00e4uben,", "tokens": ["Mag", "Rauch", "sie", "um\u00b7st\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Flamme bleibt rein.", "tokens": ["Die", "Flam\u00b7me", "bleibt", "rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.14": {"line.1": {"text": "Sieh die Fronten sich mischen", "tokens": ["Sieh", "die", "Fron\u00b7ten", "sich", "mi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "PRF", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Mit Schw\u00e4rmern gar dicht,", "tokens": ["Mit", "Schw\u00e4r\u00b7mern", "gar", "dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie poltern, sie zischen,", "tokens": ["Sie", "pol\u00b7tern", ",", "sie", "zi\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Doch z\u00fcnden sie nicht.", "tokens": ["Doch", "z\u00fcn\u00b7den", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.15": {"line.1": {"text": "Ob auch das Gekrache", "tokens": ["Ob", "auch", "das", "Ge\u00b7kra\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gar feindlich erscheint,", "tokens": ["Gar", "feind\u00b7lich", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es zeigt bald die Sache,", "tokens": ["Es", "zeigt", "bald", "die", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "'s war bestens gemeint.", "tokens": ["'s", "war", "bes\u00b7tens", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Und alle zusammen", "tokens": ["Und", "al\u00b7le", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie bilden zuletzt", "tokens": ["Sie", "bil\u00b7den", "zu\u00b7letzt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein Vivat in Flammen", "tokens": ["Ein", "Vi\u00b7vat", "in", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "----+-", "measure": "unknown.measure.single"}, "line.4": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.17": {"line.1": {"text": "Auch stehn auf dem Anger", "tokens": ["Auch", "stehn", "auf", "dem", "An\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Musikanten noch drei,", "tokens": ["Mu\u00b7si\u00b7kan\u00b7ten", "noch", "drei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "CARD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein kurzer, ein langer,", "tokens": ["Ein", "kur\u00b7zer", ",", "ein", "lan\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein dicker dabei;", "tokens": ["Ein", "di\u00b7cker", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.18": {"line.1": {"text": "Es singet der eine,", "tokens": ["Es", "sin\u00b7get", "der", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ART", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der andere pfeift,", "tokens": ["Der", "an\u00b7de\u00b7re", "pfeift", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der dritt im Vereine", "tokens": ["Der", "dritt", "im", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nach beiderlei greift.", "tokens": ["Nach", "bei\u00b7der\u00b7lei", "greift", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.19": {"line.1": {"text": "Aus den Kl\u00e4ngen, den zweien", "tokens": ["Aus", "den", "Kl\u00e4n\u00b7gen", ",", "den", "zwei\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "CARD"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Er ein Feuerwerk gie\u00dft,", "tokens": ["Er", "ein", "Feu\u00b7er\u00b7werk", "gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gott m\u00f6g ihms verzeihen,", "tokens": ["Gott", "m\u00f6g", "ihms", "ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wenns ein Wasserwerk ist.", "tokens": ["Wenns", "ein", "Was\u00b7ser\u00b7werk", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.20": {"line.1": {"text": "Hoch Nette zu heben,", "tokens": ["Hoch", "Net\u00b7te", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Wir Feuerwerk geben,", "tokens": ["Wir", "Feu\u00b7er\u00b7werk", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Wenns das Wetter erlaubt.", "tokens": ["Wenns", "das", "Wet\u00b7ter", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.21": {"line.1": {"text": "Brillanten und Farben", "tokens": ["Bril\u00b7lan\u00b7ten", "und", "Far\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zu gro\u00dfem Applaus,", "tokens": ["Zu", "gro\u00b7\u00dfem", "Ap\u00b7plaus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.3": {"text": "Raketen in Garben", "tokens": ["Ra\u00b7ke\u00b7ten", "in", "Gar\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Im ", "tokens": ["Im"], "token_info": ["word"], "pos": ["APPRART"], "meter": "+", "measure": "single.up"}}, "stanza.22": {"line.1": {"text": "Kein Feurwerk f\u00fcrs Auge,", "tokens": ["Kein", "Feur\u00b7werk", "f\u00fcrs", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Nein eines f\u00fcrs Ohr,", "tokens": ["Nein", "ei\u00b7nes", "f\u00fcrs", "Ohr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ob das ", "tokens": ["Ob", "das"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Stellt das ", "tokens": ["Stellt", "das"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.23": {"line.1": {"text": "Die erste der Fronten", "tokens": ["Die", "ers\u00b7te", "der", "Fron\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": [