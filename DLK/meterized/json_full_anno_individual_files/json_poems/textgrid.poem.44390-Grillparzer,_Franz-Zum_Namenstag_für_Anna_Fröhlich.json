ure": "amphibrach.di.relaxed"}, "line.2": {"text": "Von Liebe verspritzt,", "tokens": ["Von", "Lie\u00b7be", "ver\u00b7spritzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der Fr\u00f6mmsten von allen,", "tokens": ["Der", "Fr\u00f6mms\u00b7ten", "von", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die D\u00f6bling besitzt.", "tokens": ["Die", "D\u00f6b\u00b7ling", "be\u00b7sitzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.28": {"line.1": {"text": "Dir Freude zu schaffen,", "tokens": ["Dir", "Freu\u00b7de", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verlie\u00df sie sogar", "tokens": ["Ver\u00b7lie\u00df", "sie", "so\u00b7gar"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die z\u00e4rtlichsten Affen,", "tokens": ["Die", "z\u00e4rt\u00b7lichs\u00b7ten", "Af\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ihr elterlich Paar;", "tokens": ["Ihr", "el\u00b7ter\u00b7lich", "Paar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.29": {"line.1": {"text": "Sie blecken die Z\u00e4hne,", "tokens": ["Sie", "ble\u00b7cken", "die", "Z\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verziehn das Gesicht,", "tokens": ["Ver\u00b7ziehn", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch fruchtlos, denn jene,", "tokens": ["Doch", "frucht\u00b7los", ",", "denn", "je\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KON", "PDS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Sie achtet es nicht.", "tokens": ["Sie", "ach\u00b7tet", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.30": {"line.1": {"text": "Dann breiter und stiller", "tokens": ["Dann", "brei\u00b7ter", "und", "stil\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Naht sich ", "tokens": ["Naht", "sich"], "token_info": ["word", "word"], "pos": ["NN", "PRF"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sie schl\u00e4gt einen Triller", "tokens": ["Sie", "schl\u00e4gt", "ei\u00b7nen", "Tril\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und bleibet im Ton;", "tokens": ["Und", "blei\u00b7bet", "im", "Ton", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.31": {"line.1": {"text": "Wo L\u00fcge sonst schimmert,", "tokens": ["Wo", "L\u00fc\u00b7ge", "sonst", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Erh\u00e4lt sie sich wahr,", "tokens": ["Er\u00b7h\u00e4lt", "sie", "sich", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Was andre verschlimmert,", "tokens": ["Was", "and\u00b7re", "ver\u00b7schlim\u00b7mert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Versch\u00f6nt sie wohl gar;", "tokens": ["Ver\u00b7sch\u00f6nt", "sie", "wohl", "gar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.32": {"line.1": {"text": "Und so wird sie bleiben", "tokens": ["Und", "so", "wird", "sie", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Und so wird sie sein;", "tokens": ["Und", "so", "wird", "sie", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mag Rauch sie umst\u00e4uben,", "tokens": ["Mag", "Rauch", "sie", "um\u00b7st\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Flamme bleibt rein.", "tokens": ["Die", "Flam\u00b7me", "bleibt", "rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.33": {"line.1": {"text": "Sieh die Fronten sich mischen", "tokens": ["Sieh", "die", "Fron\u00b7ten", "sich", "mi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "PRF", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Mit Schw\u00e4rmern gar dicht,", "tokens": ["Mit", "Schw\u00e4r\u00b7mern", "gar", "dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Sie poltern, sie zischen,", "tokens": ["Sie", "pol\u00b7tern", ",", "sie", "zi\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Doch z\u00fcnden sie nicht.", "tokens": ["Doch", "z\u00fcn\u00b7den", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.34": {"line.1": {"text": "Ob auch das Gekrache", "tokens": ["Ob", "auch", "das", "Ge\u00b7kra\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Gar feindlich erscheint,", "tokens": ["Gar", "feind\u00b7lich", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es zeigt bald die Sache,", "tokens": ["Es", "zeigt", "bald", "die", "Sa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "'s war bestens gemeint.", "tokens": ["'s", "war", "bes\u00b7tens", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.35": {"line.1": {"text": "Und alle zusammen", "tokens": ["Und", "al\u00b7le", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie bilden zuletzt", "tokens": ["Sie", "bil\u00b7den", "zu\u00b7letzt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ein Vivat in Flammen", "tokens": ["Ein", "Vi\u00b7vat", "in", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "----+-", "measure": "unknown.measure.single"}, "line.4": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.36": {"line.1": {"text": "Auch stehn auf dem Anger", "tokens": ["Auch", "stehn", "auf", "dem", "An\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Musikanten noch drei,", "tokens": ["Mu\u00b7si\u00b7kan\u00b7ten", "noch", "drei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "CARD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ein kurzer, ein langer,", "tokens": ["Ein", "kur\u00b7zer", ",", "ein", "lan\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein dicker dabei;", "tokens": ["Ein", "di\u00b7cker", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.37": {"line.1": {"text": "Es singet der eine,", "tokens": ["Es", "sin\u00b7get", "der", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ART", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der andere pfeift,", "tokens": ["Der", "an\u00b7de\u00b7re", "pfeift", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der dritt im Vereine", "tokens": ["Der", "dritt", "im", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nach beiderlei greift.", "tokens": ["Nach", "bei\u00b7der\u00b7lei", "greift", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.38": {"line.1": {"text": "Aus den Kl\u00e4ngen, den zweien", "tokens": ["Aus", "den", "Kl\u00e4n\u00b7gen", ",", "den", "zwei\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "CARD"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Er ein Feuerwerk gie\u00dft,", "tokens": ["Er", "ein", "Feu\u00b7er\u00b7werk", "gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Gott m\u00f6g ihms verzeihen,", "tokens": ["Gott", "m\u00f6g", "ihms", "ver\u00b7zei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wenns ein Wasserwerk ist.", "tokens": ["Wenns", "ein", "Was\u00b7ser\u00b7werk", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}