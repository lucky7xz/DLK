{"dta.poem.14374": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "In der Fremde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Aus der Heimath hinter den Blitzen roth", "tokens": ["Aus", "der", "Hei\u00b7math", "hin\u00b7ter", "den", "Blit\u00b7zen", "roth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "ADJD"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da kommen die Wolken her,", "tokens": ["Da", "kom\u00b7men", "die", "Wol\u00b7ken", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aber Vater und Mutter sind lange todt,", "tokens": ["A\u00b7ber", "Va\u00b7ter", "und", "Mut\u00b7ter", "sind", "lan\u00b7ge", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Es kennt mich dort keiner mehr.", "tokens": ["Es", "kennt", "mich", "dort", "kei\u00b7ner", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wie bald, wie bald kommt die stille Zeit,", "tokens": ["Wie", "bald", ",", "wie", "bald", "kommt", "die", "stil\u00b7le", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "PWAV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Da ruhe ich auch, und \u00fcber mir", "tokens": ["Da", "ru\u00b7he", "ich", "auch", ",", "und", "\u00fc\u00b7ber", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "APPR", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Rauschet die sch\u00f6ne Waldeinsamkeit", "tokens": ["Rau\u00b7schet", "die", "sch\u00f6\u00b7ne", "Wal\u00b7de\u00b7in\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Und keiner mehr kennt mich auch hier.", "tokens": ["Und", "kei\u00b7ner", "mehr", "kennt", "mich", "auch", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}