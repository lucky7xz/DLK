{"textgrid.poem.48875": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "100. Er redet ihre Tr\u00e4nen an", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Versieget doch einmal, ihr siedenheisse Tropfen,", "tokens": ["Ver\u00b7sie\u00b7get", "doch", "ein\u00b7mal", ",", "ihr", "sie\u00b7den\u00b7heis\u00b7se", "Trop\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die wie das fremde Pech mein Feuer stecket an,", "tokens": ["die", "wie", "das", "frem\u00b7de", "Pech", "mein", "Feu\u00b7er", "ste\u00b7cket", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das ohne das f\u00fcr sich kein Wasser l\u00f6schen kan,", "tokens": ["das", "oh\u00b7ne", "das", "f\u00fcr", "sich", "kein", "Was\u00b7ser", "l\u00f6\u00b7schen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PRELS", "APPR", "PRF", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "schlie\u00dft euer' Adern zu und la\u00dft sie sich verstopfen!", "tokens": ["schlie\u00dft", "eu\u00b7er'", "A\u00b7dern", "zu", "und", "la\u00dft", "sie", "sich", "ver\u00b7stop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "VVIMP", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr seid der herbe Saft aus Wermut, Gall' und Hopfen,", "tokens": ["Ihr", "seid", "der", "her\u00b7be", "Saft", "aus", "Wer\u00b7mut", ",", "Gall'", "und", "Hop\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "f\u00fcr meinen Durst erpre\u00dft. Was Labsal hab ich dran?", "tokens": ["f\u00fcr", "mei\u00b7nen", "Durst", "er\u00b7pre\u00dft", ".", "Was", "Lab\u00b7sal", "hab", "ich", "dran", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "PWS", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "H\u00f6rt auf, sonst wird mir noch von euch der Tod getan,", "tokens": ["H\u00f6rt", "auf", ",", "sonst", "wird", "mir", "noch", "von", "euch", "der", "Tod", "ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "in dem ihr mir erweckt ein solches Herzenklopfen.", "tokens": ["in", "dem", "ihr", "mir", "er\u00b7weckt", "ein", "sol\u00b7ches", "Her\u00b7zen\u00b7klop\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "VVFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was k\u00f6nnt ihr Anders tun, ihr Schmerzenkinder, ihr,", "tokens": ["Was", "k\u00f6nnt", "ihr", "An\u00b7ders", "tun", ",", "ihr", "Schmer\u00b7zen\u00b7kin\u00b7der", ",", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "VVINF", "$,", "PPOSAT", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als da\u00df ihr Schmerzen auch und Pein erweckt in mir,", "tokens": ["als", "da\u00df", "ihr", "Schmer\u00b7zen", "auch", "und", "Pein", "er\u00b7weckt", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPOSAT", "NN", "ADV", "KON", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die wieder von mir aus in eure Brunnen quellen?", "tokens": ["die", "wie\u00b7der", "von", "mir", "aus", "in", "eu\u00b7re", "Brun\u00b7nen", "quel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "APPR", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O Angsttau, der mein Herz' hat matt und welk gemacht,", "tokens": ["O", "Angst\u00b7tau", ",", "der", "mein", "Her\u00b7z'", "hat", "matt", "und", "welk", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "ists noch nicht gnung, da\u00df du bisher dich um hast bracht,", "tokens": ["ists", "noch", "nicht", "gnung", ",", "da\u00df", "du", "bis\u00b7her", "dich", "um", "hast", "bracht", ","], "token_info"