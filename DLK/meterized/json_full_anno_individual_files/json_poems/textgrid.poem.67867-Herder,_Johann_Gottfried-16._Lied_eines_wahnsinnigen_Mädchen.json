{"textgrid.poem.67867": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "16. Lied eines wahnsinnigen M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchmorgens, als ich gestern", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", ",", "als", "ich", "ge\u00b7stern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Felde ging entlang,", "tokens": ["Im", "Fel\u00b7de", "ging", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da h\u00f6rt' ich, wie im Thurme", "tokens": ["Da", "h\u00f6rt'", "ich", ",", "wie", "im", "Thur\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein M\u00e4dchen lieblich sang;", "tokens": ["Ein", "M\u00e4d\u00b7chen", "lieb\u00b7lich", "sang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Ketten rasselnd an der Hand,", "tokens": ["Die", "Ket\u00b7ten", "ras\u00b7selnd", "an", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sang so fr\u00f6hliglich:", "tokens": ["Und", "sang", "so", "fr\u00f6h\u00b7lig\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Liebchen lieb' ich, denn ich weis,", "tokens": ["Mein", "Lieb\u00b7chen", "lieb'", "ich", ",", "denn", "ich", "weis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "KON", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Liebchen liebet mich.", "tokens": ["Mein", "Lieb\u00b7chen", "lie\u00b7bet", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O harter, harter Vater,", "tokens": ["O", "har\u00b7ter", ",", "har\u00b7ter", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der ri\u00df ihn ab von mir!", "tokens": ["Der", "ri\u00df", "ihn", "ab", "von", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Grausam, grausamer Schiffer,", "tokens": ["Grau\u00b7sam", ",", "grau\u00b7sa\u00b7mer", "Schif\u00b7fer", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der fort ihn nahm von hier!", "tokens": ["Der", "fort", "ihn", "nahm", "von", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "PPER", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Seitdem bin ich so stille nun,", "tokens": ["Seit\u00b7dem", "bin", "ich", "so", "stil\u00b7le", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So still aus Lieb' um dich,", "tokens": ["So", "still", "aus", "Lieb'", "um", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und lieb' mein Liebchen, denn ich weis,", "tokens": ["Und", "lieb'", "mein", "Lieb\u00b7chen", ",", "denn", "ich", "weis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Liebchen liebet mich.", "tokens": ["Mein", "Lieb\u00b7chen", "lie\u00b7bet", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O w\u00e4r' ich eine Schwalbe,", "tokens": ["O", "w\u00e4r'", "ich", "ei\u00b7ne", "Schwal\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie schl\u00fcpft' ich zu ihm heim!", "tokens": ["Wie", "schl\u00fcpft'", "ich", "zu", "ihm", "heim", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Oder w\u00e4r' ich eine Nachtigall,", "tokens": ["O\u00b7der", "w\u00e4r'", "ich", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ich s\u00e4ng' in Schlaf ihn ein.", "tokens": ["Ich", "s\u00e4ng'", "in", "Schlaf", "ihn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "K\u00f6nnt ich ihn an, nur an ihn sehn,", "tokens": ["K\u00f6nnt", "ich", "ihn", "an", ",", "nur", "an", "ihn", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKVZ", "$,", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergn\u00fcgt und froh w\u00e4r ich!", "tokens": ["Ver\u00b7gn\u00fcgt", "und", "froh", "w\u00e4r", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich lieb mein Liebchen, denn ich weis,", "tokens": ["Ich", "lieb", "mein", "Lieb\u00b7chen", ",", "denn", "ich", "weis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "$,", "KON", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Liebchen liebet mich.", "tokens": ["Mein", "Lieb\u00b7chen", "lie\u00b7bet", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kann ich, als ich am Ufer stand,", "tokens": ["Kann", "ich", ",", "als", "ich", "am", "U\u00b7fer", "stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tag vergessen je?", "tokens": ["Den", "Tag", "ver\u00b7ges\u00b7sen", "je", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sah ihn nun zum leztenmal,", "tokens": ["Und", "sah", "ihn", "nun", "zum", "lez\u00b7ten\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den nie ich wieder seh.", "tokens": ["Den", "nie", "ich", "wie\u00b7der", "seh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er kehrt' auf mich sein Auge noch,", "tokens": ["Er", "kehrt'", "auf", "mich", "sein", "Au\u00b7ge", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, wie sprach das in mich! \u2013", "tokens": ["Ach", ",", "wie", "sprach", "das", "in", "mich", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "ART", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Liebchen lieb' ich, denn ich weis,", "tokens": ["Mein", "Lieb\u00b7chen", "lieb'", "ich", ",", "denn", "ich", "weis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "KON", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Liebchen liebet mich.", "tokens": ["Mein", "Lieb\u00b7chen", "lie\u00b7bet", "mich", "."], "token_info": 