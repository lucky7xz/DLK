{"dta.poem.9457": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "ViII .  \n Die Achte und Letzte Abthei-  \n lung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Mauerwerck bin ich und zierlich Gebeu/", "tokens": ["Ein", "Mau\u00b7er\u00b7werck", "bin", "ich", "und", "zier\u00b7lich", "Ge\u00b7beu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "KON", "ADJD", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die Br\u00fcste seyn artlich wie W\u00e4lle gebauet/", "tokens": ["Die", "Br\u00fcs\u00b7te", "seyn", "art\u00b7lich", "wie", "W\u00e4l\u00b7le", "ge\u00b7bau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJD", "KOKOM", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Dadurch ich den edelen Frieden geschauet/", "tokens": ["Da\u00b7durch", "ich", "den", "e\u00b7de\u00b7len", "Frie\u00b7den", "ge\u00b7schau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sie zwingen den Liebsten und machen mich frey.", "tokens": ["Sie", "zwin\u00b7gen", "den", "Liebs\u00b7ten", "und", "ma\u00b7chen", "mich", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Es hatte der K\u00f6nig den H\u00fctern gegeben", "tokens": ["Es", "hat\u00b7te", "der", "K\u00f6\u00b7nig", "den"