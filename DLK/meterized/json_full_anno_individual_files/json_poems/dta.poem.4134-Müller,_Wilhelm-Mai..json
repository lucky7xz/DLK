{"dta.poem.4134": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Mai.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geh\u00fcllt in s\u00fc\u00dfe, bange D\u00e4mmerungen:", "tokens": ["Ge\u00b7h\u00fcllt", "in", "s\u00fc\u00b7\u00dfe", ",", "ban\u00b7ge", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es wird so viel zu meinem Preis gesungen,", "tokens": ["Es", "wird", "so", "viel", "zu", "mei\u00b7nem", "Preis", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df mir die Lust am Liede fast vergangen.", "tokens": ["Da\u00df", "mir", "die", "Lust", "am", "Lie\u00b7de", "fast", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "W\u00e4rst du so hei\u00df von seligem Verlangen,", "tokens": ["W\u00e4rst", "du", "so", "hei\u00df", "von", "se\u00b7li\u00b7gem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie eine Lilie, deren wei\u00dfe Zungen", "tokens": ["Wie", "ei\u00b7ne", "Li\u00b7lie", ",", "de\u00b7ren", "wei\u00b7\u00dfe", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Den langen Tag nach k\u00fchlem Trost gerungen,", "tokens": ["Den", "lan\u00b7gen", "Tag", "nach", "k\u00fch\u00b7lem", "Trost", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis da\u00df sie m\u00fcd' und matt zur Erde hangen:", "tokens": ["Bis", "da\u00df", "sie", "m\u00fcd'", "und", "matt", "zur", "Er\u00b7de", "han\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADJD", "KON", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Komm her zu mir, ich gebe dir zu trinken,", "tokens": ["Komm", "her", "zu", "mir", ",", "ich", "ge\u00b7be", "dir", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So viel du magst, mein treuer deutscher Zecher,", "tokens": ["So", "viel", "du", "magst", ",", "mein", "treu\u00b7er", "deut\u00b7scher", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VMFIN", "$,", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus meinem bodenlosen Liebesbecher!", "tokens": ["Aus", "mei\u00b7nem", "bo\u00b7den\u00b7lo\u00b7sen", "Lie\u00b7bes\u00b7be\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Siehst du die hellen Thauestropfen blinken", "tokens": ["Siehst", "du", "die", "hel\u00b7len", "Thau\u00b7es\u00b7trop\u00b7fen", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort an den Lilien in der Morgensonne?", "tokens": ["Dort", "an", "den", "Li\u00b7li\u00b7en", "in", "der", "Mor\u00b7gen\u00b7son\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie m\u00e4\u00dfig schaltet ihr mit meiner Wonne!", "tokens": ["Wie", "m\u00e4\u00b7\u00dfig", "schal\u00b7tet", "ihr", "mit", "mei\u00b7ner", "Won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}