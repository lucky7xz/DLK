{"dta.poem.1886": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wann die Baurn ihr Gl\u00fccke w\u00fcsten/", "tokens": ["Wann", "die", "Baurn", "ihr", "Gl\u00fc\u00b7cke", "w\u00fcs\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fcrden sie sich h\u00f6her br\u00fcsten.", "tokens": ["W\u00fcr\u00b7den", "sie", "sich", "h\u00f6\u00b7her", "br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie erhalten alle Welt/", "tokens": ["Sie", "er\u00b7hal\u00b7ten", "al\u00b7le", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich wie sie das Feld erh\u00e4lt/", "tokens": ["Gleich", "wie", "sie", "das", "Feld", "er\u00b7h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leben von der Tyranney", "tokens": ["Le\u00b7ben", "von", "der", "Ty\u00b7ran\u00b7ney"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Vieler Herren Sorgen-frey:", "tokens": ["Vie\u00b7ler", "Her\u00b7ren", "Sor\u00b7gen\u00b7frey", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was der F\u00fcrst und B\u00fcrger essen", "tokens": ["Was", "der", "F\u00fcrst", "und", "B\u00fcr\u00b7ger", "es\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird von ihnen zugemessen.", "tokens": ["Wird", "von", "ih\u00b7nen", "zu\u00b7ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}