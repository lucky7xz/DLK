{"dta.poem.12228": {"metadata": {"author": {"name": "Nietzsche, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die kleine Hexe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-30172-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "             So lang noch h\u00fcbsch mein Leibchen,", "tokens": ["So", "lang", "noch", "h\u00fcbsch", "mein", "Leib\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "             Lohnt sichs schon, fromm zu sein.", "tokens": ["Lohnt", "sichs", "schon", ",", "fromm", "zu", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "             Man weiss, Gott liebt die Weibchen,", "tokens": ["Man", "weiss", ",", "Gott", "liebt", "die", "Weib\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "             Die h\u00fcbschen obendrein.", "tokens": ["Die", "h\u00fcb\u00b7schen", "o\u00b7ben\u00b7drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "         Er wird\u2019s dem art\u2019gen M\u00f6nchlein", "tokens": ["Er", "wird's", "dem", "art'\u00b7gen", "M\u00f6nch\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "         Gewisslich gern verzeihn,", "tokens": ["Ge\u00b7wiss\u00b7lich", "gern", "ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "             Dass er, gleich manchem M\u00f6nchlein,", "tokens": ["Dass", "er", ",", "gleich", "man\u00b7chem", "M\u00f6nch\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "PIAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "             So gern will bei mir sein.", "tokens": ["So", "gern", "will", "bei", "mir", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "             Kein grauer Kirchenvater!", "tokens": ["Kein", "grau\u00b7er", "Kir\u00b7chen\u00b7va\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "         Nein, jung noch und oft roth,", "tokens": ["Nein", ",", "jung", "noch", "und", "oft", "roth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "ADV", "KON", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "         Oft gleich dem grausten Kater", "tokens": ["Oft", "gleich", "dem", "graus\u00b7ten", "Ka\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "             Voll Eifersucht und Noth!", "tokens": ["Voll", "Ei\u00b7fer\u00b7sucht", "und", "Noth", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "             Ich liebe nicht die Greise,", "tokens": ["Ich", "lie\u00b7be", "nicht", "die", "Grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "             Er liebt die Alten nicht:", "tokens": ["Er", "liebt", "die", "Al\u00b7ten", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "         Wie wunderlich und weise", "tokens": ["Wie", "wun\u00b7der\u00b7lich", "und", "wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "         Hat Gott dies eingericht!", "tokens": ["Hat", "Gott", "dies", "ein\u00b7ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PDS", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "             Die Kirche weiss zu leben,", "tokens": ["Die", "Kir\u00b7che", "weiss", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "             Sie pr\u00fcft Herz und Gesicht.", "tokens": ["Sie", "pr\u00fcft", "Herz", "und", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "             St\u00e4ts will sie mir vergeben: \u2014", "tokens": ["St\u00e4ts", "will", "sie", "mir", "ver\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "         Ja wer vergiebt mir nicht!", "tokens": ["Ja", "wer", "ver\u00b7giebt", "mir", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man lispelt mit dem M\u00fcndchen,", "tokens": ["Man", "lis\u00b7pelt", "mit", "dem", "M\u00fcnd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "             Man knixt und geht hinaus", "tokens": ["Man", "knixt", "und", "geht", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "APZR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "             Und mit dem neuen S\u00fcndchen", "tokens": ["Und", "mit", "dem", "neu\u00b7en", "S\u00fcnd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "             L\u00f6scht man das alte aus.", "tokens": ["L\u00f6scht", "man", "das", "al\u00b7te", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "         Gelobt sei Gott auf Erden,", "tokens": ["Ge\u00b7lobt", "sei", "Gott", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "         Der h\u00fcbsche M\u00e4dchen liebt", "tokens": ["Der", "h\u00fcb\u00b7sche", "M\u00e4d\u00b7chen", "liebt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "             Und derlei Herzbeschwerden", "tokens": ["Und", "der\u00b7lei", "Herz\u00b7be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "             Sich selber gern vergiebt!", "tokens": ["Sich", "sel\u00b7ber", "gern", "ver\u00b7giebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "             So lang noch h\u00fcbsch mein Leibchen,", "tokens": ["So", "lang", "noch", "h\u00fcbsch", "mein", "Leib\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "         Lohnt sich\u2019s schon, fromm zu sein:", "tokens": ["Lohnt", "sich's", "schon", ",", "fromm", "zu", "sein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "$,", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "         Als altes Wackelweibchen", "tokens": ["Als", "al\u00b7tes", "Wa\u00b7ckel\u00b7weib\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "             Mag mich der Teufel frein!", "tokens": ["Mag", "mich", "der", "Teu\u00b7fel", "frein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}