{"dta.poem.14096": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Braut .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.57", "nl:0.42"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Seelig Weinen seel'ger Herzen!", "tokens": ["See\u00b7lig", "Wei\u00b7nen", "seel'\u00b7ger", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn das Herz nichts weiter will,", "tokens": ["Wenn", "das", "Herz", "nichts", "wei\u00b7ter", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIS", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht wei\u00df, ob es Lust, ob Schmerzen,", "tokens": ["Nicht", "wei\u00df", ",", "ob", "es", "Lust", ",", "ob", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "NN", "$,", "KOUS", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aber fr\u00f6hlich ist und still.", "tokens": ["A\u00b7ber", "fr\u00f6h\u00b7lich", "ist", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}