{"textgrid.poem.48692": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "13. Gott sei mir S\u00fcnder gn\u00e4dig", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht nur alleine nichts wei\u00df ich in mir zu finden,", "tokens": ["Nicht", "nur", "al\u00b7lei\u00b7ne", "nichts", "wei\u00df", "ich", "in", "mir", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "PIS", "VVFIN", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mit dem, erz\u00fcrnter Gott, ich k\u00f6nte vor dir stehn", "tokens": ["mit", "dem", ",", "er\u00b7z\u00fcrn\u00b7ter", "Gott", ",", "ich", "k\u00f6n\u00b7te", "vor", "dir", "stehn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "$,", "ADJA", "NN", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und mit beherzter Stirn dir unter Augen gehn:", "tokens": ["und", "mit", "be\u00b7herz\u00b7ter", "Stirn", "dir", "un\u00b7ter", "Au\u00b7gen", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ich reize dich noch auf mit meinen b\u00f6sen S\u00fcnden.", "tokens": ["ich", "rei\u00b7ze", "dich", "noch", "auf", "mit", "mei\u00b7nen", "b\u00f6\u00b7sen", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ja, la\u00df auch itzt nicht nach dich ferner zu entz\u00fcnden!", "tokens": ["Ja", ",", "la\u00df", "auch", "itzt", "nicht", "nach", "dich", "fer\u00b7ner", "zu", "ent\u00b7z\u00fcn\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVIMP", "ADV", "ADV", "PTKNEG", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie? soll ich mich denn auf f\u00fcr deiner Hochheit bl\u00f6hn,", "tokens": ["Wie", "?", "soll", "ich", "mich", "denn", "auf", "f\u00fcr", "dei\u00b7ner", "Hoch\u00b7heit", "bl\u00f6hn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "PRF", "ADV", "APPR", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ein Pharis\u00e4er sein, mein nichtigs Tun erh\u00f6hn,", "tokens": ["ein", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "sein", ",", "mein", "nich\u00b7tigs", "Tun", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAINF", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "um dich zu s\u00f6hnen aus mit B\u00f6sem unterwinden?", "tokens": ["um", "dich", "zu", "s\u00f6h\u00b7nen", "aus", "mit", "B\u00f6\u00b7sem", "un\u00b7ter\u00b7win\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "PTKZU", "VVINF", "APPR", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach nein! Du kennst uns wol, du scharfer Herzergr\u00fcnder,", "tokens": ["Ach", "nein", "!", "Du", "kennst", "uns", "wol", ",", "du", "schar\u00b7fer", "Her\u00b7zer\u00b7gr\u00fcn\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ich sag' es frei heraus: ich bin ein armer S\u00fcnder,", "tokens": ["ich", "sag'", "es", "frei", "he\u00b7raus", ":", "ich", "bin", "ein", "ar\u00b7mer", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der deiner G\u00fcte darf, soll er erl\u00f6set sein.", "tokens": ["der", "dei\u00b7ner", "G\u00fc\u00b7te", "darf", ",", "soll", "er", "er\u00b7l\u00f6\u00b7set", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VMFIN", "$,", "VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Schau meine Notturft an und sei mir, Gnade, gn\u00e4dig!", "tokens": ["Schau", "mei\u00b7ne", "Not\u00b7turft", "an", "und", "sei", "mir", ",", "Gna\u00b7de", ",", "gn\u00e4\u00b7dig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "KON", "VAFIN", "PPER", "$,", "NN", "$,", "ADJD", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der du die weite Welt von aller Schuld sprichst ledig,