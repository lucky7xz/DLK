{"textgrid.poem.42001": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Bl\u00fcthenhain, der duftend sie umfangen,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Bl\u00fcthenhain, der duftend sie umfangen,", "tokens": ["Du", "Bl\u00fc\u00b7then\u00b7hain", ",", "der", "duf\u00b7tend", "sie", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du weiches Gr\u00fcn, wo sie geschlummert hat,", "tokens": ["Du", "wei\u00b7ches", "Gr\u00fcn", ",", "wo", "sie", "ge\u00b7schlum\u00b7mert", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PWAV", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Blumen, die der zarte Fu\u00df zertrat,", "tokens": ["Ihr", "Blu\u00b7men", ",", "die", "der", "zar\u00b7te", "Fu\u00df", "zer\u00b7trat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie zieht zu euch mich schmeichelndes Verlangen;", "tokens": ["Wie", "zieht", "zu", "euch", "mich", "schmei\u00b7cheln\u00b7des", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch z\u00f6gernd hemmt den Schritt geweihtes Bangen,", "tokens": ["Doch", "z\u00f6\u00b7gernd", "hemmt", "den", "Schritt", "ge\u00b7weih\u00b7tes", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn heilig ist der Ort, dem sie genaht,", "tokens": ["Denn", "hei\u00b7lig", "ist", "der", "Ort", ",", "dem", "sie", "ge\u00b7naht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wandeln darf kein Sterblicher den Pfad,", "tokens": ["Und", "wan\u00b7deln", "darf", "kein", "Sterb\u00b7li\u00b7cher", "den", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VMFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den leis' und leicht die G\u00f6ttliche gegangen.", "tokens": ["Den", "leis'", "und", "leicht", "die", "G\u00f6tt\u00b7li\u00b7che", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort, wo der West mit s\u00fc\u00dfen D\u00fcften spielt,", "tokens": ["Dort", ",", "wo", "der", "West", "mit", "s\u00fc\u00b7\u00dfen", "D\u00fcf\u00b7ten", "spielt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo plaudernd sich die reine Welle kr\u00e4uselt,", "tokens": ["Wo", "plau\u00b7dernd", "sich", "die", "rei\u00b7ne", "Wel\u00b7le", "kr\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dort will ich ruhn in sehnsuchtsvoller Lust.", "tokens": ["Dort", "will", "ich", "ruhn", "in", "sehn\u00b7suchts\u00b7vol\u00b7ler", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du linder Hauch, der meinen Busen k\u00fchlt,", "tokens": ["Du", "lin\u00b7der", "Hauch", ",", "der", "mei\u00b7nen", "Bu\u00b7sen", "k\u00fchlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ach, so weich um meine Wangen s\u00e4uselt,", "tokens": ["Und", "ach", ",", "so", "weich", "um", "mei\u00b7ne", "Wan\u00b7gen", "s\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hobst du vielleicht auch ihre zarte Brust?", "tokens": ["Hobst", "du", "viel\u00b7leicht", "auch", "ih\u00b7re", "zar\u00b7te", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du Bl\u00fcthenhain, der duftend sie umfangen,", "tokens": ["Du", "Bl\u00fc\u00b7then\u00b7hain", ",", "der", "duf\u00b7tend", "sie", "um\u00b7fan\u00b7gen", ","], "token_info": ["wo