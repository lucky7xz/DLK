{"dta.poem.18066": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Sinnliche Erzehlung  \n von der mechanischen  \n Verfertigung des deutschen  \n Original-St\u00fcckes von Cato.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": ". . . . Noch hab ich allezeit,", "tokens": [".", ".", ".", ".", "Noch", "hab", "ich", "al\u00b7le\u00b7zeit", ","], "token_info": ["punct", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$.", "$.", "$.", "$.", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Seitdem das Schicksal mich an diesen Ort gef\u00fchret,", "tokens": ["Seit\u00b7dem", "das", "Schick\u00b7sal", "mich", "an", "die\u00b7sen", "Ort", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PRF", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das z\u00e4rtste Vaterhertz in seiner Brust gesp\u00fcret.", "tokens": ["Das", "z\u00e4rts\u00b7te", "Va\u00b7ter\u00b7hertz", "in", "sei\u00b7ner", "Brust", "ge\u00b7sp\u00fc\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}