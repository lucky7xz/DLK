{"dta.poem.15791": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Wortspiel schelten sie, doch scheint es angemessen", "tokens": ["Das", "Wort\u00b7spiel", "schel\u00b7ten", "sie", ",", "doch", "scheint", "es", "an\u00b7ge\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sprache, welche ganz hat ihre Bahn gemessen.", "tokens": ["Der", "Spra\u00b7che", ",", "wel\u00b7che", "ganz", "hat", "ih\u00b7re", "Bahn", "ge\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df sie vom Anbeginn, eh' es ihr war bewu\u00dft,", "tokens": ["Da\u00df", "sie", "vom", "An\u00b7be\u00b7ginn", ",", "eh'", "es", "ihr", "war", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein dunkles Wortspiel war, wird ihr nun klar bewu\u00dft.", "tokens": ["Ein", "dunk\u00b7les", "Wort\u00b7spiel", "war", ",", "wird", "ihr", "nun", "klar", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "VAFIN", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Womit unwissentlich sie allerorten spielen,", "tokens": ["Wo\u00b7mit", "un\u00b7wis\u00b7sent\u00b7lich", "sie", "al\u00b7le\u00b7ror\u00b7ten", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Komm' und geflissentlich la\u00df uns mit Worten spielen!", "tokens": ["Komm'", "und", "ge\u00b7flis\u00b7sent\u00b7lich", "la\u00df", "uns", "mit", "Wor\u00b7ten", "spie\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "ADJD", "VVIMP", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}}}}}