{"dta.poem.2613": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Helden-Ruhm/  \n  Hn. M. v. H. den 31. Julii  1667.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es sollen/ Held/ dein Grab die Thr\u00e4nen nicht entweyhen/", "tokens": ["Es", "sol\u00b7len", "/", "Held", "/", "dein", "Grab", "die", "Thr\u00e4\u00b7nen", "nicht", "ent\u00b7wey\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "NN", "$(", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ein betr\u00fcbtes Ach dem Sarch beschwerlich seyn.", "tokens": ["Und", "ein", "be\u00b7tr\u00fcb\u00b7tes", "Ach", "dem", "Sarch", "be\u00b7schwer\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wird dir keine Hand hier Nelck\u2019 und Lilgen streuen;", "tokens": ["Es", "wird", "dir", "kei\u00b7ne", "Hand", "hier", "Nelck'", "und", "Lil\u00b7gen", "streu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "ADV", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn Buhler scharret man in solches Blumwerck ein.", "tokens": ["Denn", "Buh\u00b7ler", "schar\u00b7ret", "man", "in", "sol\u00b7ches", "Blum\u00b7werck", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PIS", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es darff kein Klage-Weib ein traurig Winseln f\u00fchren/", "tokens": ["Es", "darff", "kein", "Kla\u00b7ge\u00b7Weib", "ein", "trau\u00b7rig", "Win\u00b7seln", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ART", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Lydier ihr Thon der wird hier nur verlacht.", "tokens": ["Der", "Ly\u00b7di\u00b7er", "ihr", "Thon", "der", "wird", "hier", "nur", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ART", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer deine Bahre will nach den Verdiensten zieren/", "tokens": ["Wer", "dei\u00b7ne", "Bah\u00b7re", "will", "nach", "den", "Ver\u00b7diens\u00b7ten", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+++-+-+-", "measure": "unknown.measure.septa"}, "line.8": {"text": "Der schaffe/ da\u00df die Grufft von Blitz und Donner kracht.", "tokens": ["Der", "schaf\u00b7fe", "/", "da\u00df", "die", "Grufft", "von", "Blitz", "und", "Don\u00b7ner", "kracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$(", "KOUS", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Fahnen freyer Flug/ die hagelnden Musqueten/", "tokens": ["Der", "Fah\u00b7nen", "frey\u00b7er", "Flug", "/", "die", "ha\u00b7geln\u00b7den", "Mus\u00b7que\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "(klang/", "tokens": ["(", "klang", "/"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Der Drommeln rauher Thon/ der Spie\u00df- und Schwerter-", "tokens": ["Der", "Drom\u00b7meln", "rau\u00b7her", "Thon", "/", "der", "Spie\u00df", "und", "Schwer\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "TRUNC", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Granaten in der Lufft/ und Schloossen aus Falckneten/", "tokens": ["Gra\u00b7na\u00b7ten", "in", "der", "Lufft", "/", "und", "Schloos\u00b7sen", "aus", "Falck\u00b7ne\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "KON", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Die stimmen/ ", "tokens": ["Die", "stim\u00b7men", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Bello na hat sich selbst in blancken Stahl geschlossen/", "tokens": ["Bel\u00b7lo", "na", "hat", "sich", "selbst", "in", "blan\u00b7cken", "Stahl", "ge\u00b7schlos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und will die letzte Pflicht/ dir/ ihrem Sohne/ thun:", "tokens": ["Und", "will", "die", "letz\u00b7te", "Pflicht", "/", "dir", "/", "ih\u00b7rem", "Soh\u00b7ne", "/", "thun", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "$(", "PPER", "$(", "PPOSAT", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und Helden die ihr Blut vors Vaterland vergossen/", "tokens": ["Und", "Hel\u00b7den", "die", "ihr", "Blut", "vors", "Va\u00b7ter\u00b7land", "ver\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "L\u00e4\u00dft dieser Trauer-Dienst nicht in den Gr\u00e4bern ruhn.", "tokens": ["L\u00e4\u00dft", "die\u00b7ser", "Trau\u00b7e\u00b7rDienst", "nicht", "in", "den", "Gr\u00e4\u00b7bern", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wich d\u00fcnckt Arminius/ der Kern von Teutschen Rittern", "tokens": ["Wich", "d\u00fcnckt", "Ar\u00b7mi\u00b7nius", "/", "der", "Kern", "von", "Teut\u00b7schen", "Rit\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "NE", "$(", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "Erscheint in der Gestalt/ wie er die R\u00f6mer schlug:", "tokens": ["Er\u00b7scheint", "in", "der", "Ge\u00b7stalt", "/", "wie", "er", "die", "R\u00f6\u00b7mer", "schlug", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und dorten seh ich auch sich den Tuisco wittern/", "tokens": ["Und", "dor\u00b7ten", "seh", "ich", "auch", "sich", "den", "Tu\u00b7is\u00b7co", "wit\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "PRF", "ART", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der tausendfachen Raub von seinen Feinden trug.", "tokens": ["Der", "tau\u00b7send\u00b7fa\u00b7chen", "Raub", "von", "sei\u00b7nen", "Fein\u00b7den", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jhr\u2019 Augen f\u00fcnckeln noch und brennen voller Flammen/", "tokens": ["Ih\u00b7r'", "Au\u00b7gen", "f\u00fcn\u00b7ckeln", "noch", "und", "bren\u00b7nen", "vol\u00b7ler", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "KON", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.23": {"text": "Der Mund bricht auf alt Teutsch in solche Worte lo\u00df:", "tokens": ["Der", "Mund", "bricht", "auf", "alt", "Teutsch", "in", "sol\u00b7che", "Wor\u00b7te", "lo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJD", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Ahnen feurger Geist mu\u00df auch auf S\u00f6hne stammen;", "tokens": ["Der", "Ah\u00b7nen", "feur\u00b7ger", "Geist", "mu\u00df", "auch", "auf", "S\u00f6h\u00b7ne", "stam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VMFIN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zu unsrer Nachwelt Ruhm/ wuchs uns das Hertze gro\u00df.", "tokens": ["Zu", "uns\u00b7rer", "Nach\u00b7welt", "Ruhm", "/", "wuchs", "uns", "das", "Hert\u00b7ze", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "VVFIN", "PPER", "PDS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das Fell von einem Thier so wir zu erst geschlagen/", "tokens": ["Das", "Fell", "von", "ei\u00b7nem", "Thier", "so", "wir", "zu", "erst", "ge\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "PPER", "APPR", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Kolbe so da hat die eigne Hand geschnitzt/", "tokens": ["Die", "Kol\u00b7be", "so", "da", "hat", "die", "eig\u00b7ne", "Hand", "ge\u00b7schnitzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und Wunden auf der Brust die wir mit Lust ertragen/", "tokens": ["Und", "Wun\u00b7den", "auf", "der", "Brust", "die", "wir", "mit", "Lust", "er\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "ART", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die haben/ Kinder/ euch die Gr\u00e4ntzen stets besch\u00fctzt.", "tokens": ["Die", "ha\u00b7ben", "/", "Kin\u00b7der", "/", "euch", "die", "Gr\u00e4nt\u00b7zen", "stets", "be\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "NN", "$(", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die Faust war nur gelehrt den Streich wol zu vollf\u00fchren/", "tokens": ["Die", "Faust", "war", "nur", "ge\u00b7lehrt", "den", "Streich", "wol", "zu", "voll\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der Thaten Tapfferkeit schrieb keiner an die Wand.", "tokens": ["Der", "Tha\u00b7ten", "Tapf\u00b7fer\u00b7keit", "schrieb", "kei\u00b7ner", "an", "die", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Rom zwang den Krei\u00df der Welt/ und konte triumphiren/", "tokens": ["Rom", "zwang", "den", "Krei\u00df", "der", "Welt", "/", "und", "kon\u00b7te", "tri\u00b7um\u00b7phi\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$(", "KON", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch niemahls \u00fcber uns/ und \u00fcber unser Land.", "tokens": ["Doch", "nie\u00b7mahls", "\u00fc\u00b7ber", "uns", "/", "und", "\u00fc\u00b7ber", "un\u00b7ser", "Land", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "$(", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ach h\u00e4tte doch der Arm der sich in Blut gewaschen", "tokens": ["Ach", "h\u00e4t\u00b7te", "doch", "der", "Arm", "der", "sich", "in", "Blut", "ge\u00b7wa\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VAFIN", "ADV", "ART", "NN", "ART", "PRF", "APPR", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Feder auch so wol als Spie\u00df und Schwerdt gebraucht!", "tokens": ["Die", "Fe\u00b7der", "auch", "so", "wol", "als", "Spie\u00df", "und", "Schwerdt", "ge\u00b7braucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "KOUS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Viel Helden legen nicht vergessen in der Aschen/", "tokens": ["Viel", "Hel\u00b7den", "le\u00b7gen", "nicht", "ver\u00b7ges\u00b7sen", "in", "der", "A\u00b7schen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKNEG", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es w\u00e4r\u2019 ihr wahrer Ruhm nicht in der Lufft verraucht.", "tokens": ["Es", "w\u00e4r'", "ihr", "wah\u00b7rer", "Ruhm", "nicht", "in", "der", "Lufft", "ver\u00b7raucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Jedoch die Tapfferkeit bl\u00fcht noch in unsern Erben/", "tokens": ["Je\u00b7doch", "die", "Tapf\u00b7fer\u00b7keit", "bl\u00fcht", "noch", "in", "un\u00b7sern", "Er\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der angebohrne Muth steigt Cedern gleich empor/", "tokens": ["Der", "an\u00b7ge\u00b7bohr\u00b7ne", "Muth", "steigt", "Ce\u00b7dern", "gleich", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und l\u00e4st den Sieges-Krantz der V\u00e4ter nicht verderben/", "tokens": ["Und", "l\u00e4st", "den", "Sie\u00b7ges\u00b7Krantz", "der", "V\u00e4\u00b7ter", "nicht", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Er thut sich noch vielmehr als wie ein L\u00f6w hervor.", "tokens": ["Er", "thut", "sich", "noch", "viel\u00b7mehr", "als", "wie", "ein", "L\u00f6w", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "KOUS", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"t