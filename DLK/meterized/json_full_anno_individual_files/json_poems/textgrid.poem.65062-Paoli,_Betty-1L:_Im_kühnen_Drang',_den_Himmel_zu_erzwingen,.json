, "tokens": ["O", ",", "selbst", "wenn", "dei\u00b7ne", "Ar\u00b7me", "mich", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "KOUS", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und uns're Augen Freudenthr\u00e4nen weinen,", "tokens": ["Und", "un\u00b7s'\u00b7re", "Au\u00b7gen", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Seh' pl\u00f6tzlich ich die Flammenschrift erscheinen:", "tokens": ["Seh'", "pl\u00f6tz\u00b7lich", "ich", "die", "Flam\u00b7men\u00b7schrift", "er\u00b7schei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbden Tod nur wird dir diese Liebe bringen!\u00ab \u2013", "tokens": ["\u00bb", "den", "Tod", "nur", "wird", "dir", "die\u00b7se", "Lie\u00b7be", "brin\u00b7gen", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ART", "NN", "ADV", "VAFIN", "PPER", "PDAT", "NN", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den Tod? den Tod? o selige Verhei\u00dfung!", "tokens": ["Den", "Tod", "?", "den", "Tod", "?", "o", "se\u00b7li\u00b7ge", "Ver\u00b7hei\u00b7\u00dfung", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wird der ti