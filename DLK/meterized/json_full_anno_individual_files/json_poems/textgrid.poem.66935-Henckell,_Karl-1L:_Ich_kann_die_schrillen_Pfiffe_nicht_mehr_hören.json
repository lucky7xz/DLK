{"textgrid.poem.66935": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich kann die schrillen Pfiffe nicht mehr h\u00f6ren", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann die schrillen Pfiffe nicht mehr h\u00f6ren", "tokens": ["Ich", "kann", "die", "schril\u00b7len", "Pfif\u00b7fe", "nicht", "mehr", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der schweren Wagen, die voll Schergen sind,", "tokens": ["Der", "schwe\u00b7ren", "Wa\u00b7gen", ",", "die", "voll", "Scher\u00b7gen", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sehne mich nach sausend stillen F\u00f6hren,", "tokens": ["Und", "seh\u00b7ne", "mich", "nach", "sau\u00b7send", "stil\u00b7len", "F\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ihren Harfen spielt der Sommerwind.", "tokens": ["In", "ih\u00b7ren", "Har\u00b7fen", "spielt", "der", "Som\u00b7mer\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier schwankt das Herz von j\u00e4hestem Emp\u00f6ren", "tokens": ["Hier", "schwankt", "das", "Herz", "von", "j\u00e4\u00b7hes\u00b7tem", "Em\