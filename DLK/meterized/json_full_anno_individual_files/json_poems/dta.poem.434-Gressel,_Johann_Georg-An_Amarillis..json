{"dta.poem.434": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Amarillis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Amarillis deine Pracht", "tokens": ["A\u00b7ma\u00b7ril\u00b7lis", "dei\u00b7ne", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat mich verliebt gemacht.", "tokens": ["Hat", "mich", "ver\u00b7liebt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich schwinde! ich vergeh/", "tokens": ["Ich", "schwin\u00b7de", "!", "ich", "ver\u00b7geh", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich werde halb erbla\u00dft", "tokens": ["Ich", "wer\u00b7de", "halb", "er\u00b7bla\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ich dich bey mir seh/", "tokens": ["Wenn", "ich", "dich", "bey", "mir", "seh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich wei\u00df du liebst mich auch/", "tokens": ["Ich", "wei\u00df", "du", "liebst", "mich", "auch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ob gleich dein Angesichte", "tokens": ["Ob", "gleich", "dein", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Liebe macht zu nichte/", "tokens": ["Die", "Lie\u00b7be", "macht", "zu", "nich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKA", "PIS", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "So liebet doch mein Hertz/", "tokens": ["So", "lie\u00b7bet", "doch", "mein", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Das frey von allen Schertz", "tokens": ["Das", "frey", "von", "al\u00b7len", "Schertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "In heissen Flammen steht.", "tokens": ["In", "heis\u00b7sen", "Flam\u00b7men", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Denn liest die Liebe man nicht an der Stirne/", "tokens": ["Denn", "liest", "die", "Lie\u00b7be", "man", "nicht", "an", "der", "Stir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PIS", "PTKNEG", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "So ist sie doch im Hertzen und Gehirne.", "tokens": ["So", "ist", "sie", "doch", "im", "Hert\u00b7zen", "und", "Ge\u00b7hir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}