{"dta.poem.800": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXVII.  \n Grabschrifft eines Hochber\u00fchmbten Mannes.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer lieg ich/ nicht mehr Ich/ ein abgelebte Leichen ", "tokens": ["Hjer", "lieg", "ich", "/", "nicht", "mehr", "Ich", "/", "ein", "ab\u00b7ge\u00b7leb\u00b7te", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$(", "PTKNEG", "ADV", "PPER", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ruht vnter diesem Stein/ der Geist der Erd\u2019 vnd See", "tokens": ["Ruht", "vn\u00b7ter", "die\u00b7sem", "Stein", "/", "der", "Geist", "der", "Erd'", "vnd", "See"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$(", "ART", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd Sternen/ durch gesucht/ Sucht nunmehr in der h\u00f6h\u2019", "tokens": ["Vnd", "Ster\u00b7nen", "/", "durch", "ge\u00b7sucht", "/", "Sucht", "nun\u00b7mehr", "in", "der", "h\u00f6h'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "$(", "APPR", "VVPP", "$(", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die ewig\u2019 Ewigkeit/ die hier nicht zuerreichen.", "tokens": ["Die", "e\u00b7wig'", "E\u00b7wig\u00b7keit", "/", "die", "hier", "nicht", "zu\u00b7er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die welt hat nichts als dunst/ was lebt mu\u00df stracks erblei-", "tokens": ["Die", "welt", "hat", "nichts", "als", "dunst", "/", "was", "lebt", "mu\u00df", "stracks", "er\u00b7blei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIS", "KOKOM", "ADV", "$(", "PWS", "VVFIN", "VMFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "chen: ", "tokens": ["chen", ":"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Die wissenschaft ist wahn/ die sch\u00f6nheit: leichter Schne\u2019/", "tokens": ["Die", "wis\u00b7sen\u00b7schaft", "ist", "wahn", "/", "die", "sch\u00f6n\u00b7heit", ":", "leich\u00b7ter", "Schne'", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADJD", "$(", "ART", "NN", "$.", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Adel frembde Pracht. Zeigt etwas das jtzt steh\u2019", "tokens": ["Der", "A\u00b7del", "fremb\u00b7de", "Pracht", ".", "Zeigt", "et\u00b7was", "das", "jtzt", "steh'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$.", "VVFIN", "PIS", "ART", "ADV", "VVFIN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Vnd nicht dem rawen grim der zeitten m\u00fcsse weichen.", "tokens": ["Vnd", "nicht", "dem", "ra\u00b7wen", "grim", "der", "zeit\u00b7ten", "m\u00fcs\u00b7se", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mein gutt/ mein standist hin/ kein Freund wei\u00df mehr von", "tokens": ["Mein", "gutt", "/", "mein", "stan\u00b7dist", "hin", "/", "kein", "Freund", "wei\u00df", "mehr", "von"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "$(", "PPOSAT", "VVFIN", "PTKVZ", "$(", "PIAT", "NN", "VVFIN", "ADV", "APPR"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "mir/ ", "tokens": ["mir", "/"], "token_info": ["word", "punct"], "pos": ["PPER", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Mein Ruhm hat auch sein grab/ man l\u00e4st doch alles hier/", "tokens": ["Mein", "Ruhm", "hat", "auch", "sein", "grab", "/", "man", "l\u00e4st", "doch", "al\u00b7les", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$(", "PIS", "VVFIN", "ADV", "PIS", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vmb da\u00df ihr Menschen pflegt/ was Ewig. zuverlieren.", "tokens": ["Vmb", "da\u00df", "ihr", "Men\u00b7schen", "pflegt", "/", "was", "E\u00b7wig", ".", "zu\u00b7ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "VVFIN", "$(", "PWS", "ADJD", "$.", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Di\u00df was ihr leben nennt\u2019 jhr sterblichen/ ist todt", "tokens": ["Di\u00df", "was", "ihr", "le\u00b7ben", "nennt'", "jhr", "sterb\u00b7li\u00b7chen", "/", "ist", "todt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PRELS", "PPER", "VVINF", "VVFIN", "PPER", "VVINF", "$(", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was jhr f\u00fcr todt anschaw\u2019t ist leben sonder noth.", "tokens": ["Was", "jhr", "f\u00fcr", "todt", "an\u00b7scha\u00b7w't", "ist", "le\u00b7ben", "son\u00b7der", "noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJD", "VVPP", "VAFIN", "VVINF", "KON", "NN", "$."], "meter": "-+----+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Die welt mu\u00df in die grufft/ die grufft zum Himmel f\u00fchren. ", "tokens": ["Die", "welt", "mu\u00df", "in", "die", "grufft", "/", "die", "grufft", "zum", "Him\u00b7mel", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "$(", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}