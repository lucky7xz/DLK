{"textgrid.poem.63756": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier haben wahrlich alle Menschlichkeiten", "tokens": ["Hier", "ha\u00b7ben", "wahr\u00b7lich", "al\u00b7le", "Menschlich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr Stelldichein. An des Genusses Arm", "tokens": ["Ihr", "Stell\u00b7dich\u00b7ein", ".", "An", "des", "Ge\u00b7nus\u00b7ses", "Arm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Schlendert das s\u00fc\u00dfe Nichtstun durch den Schwarm,", "tokens": ["Schlen\u00b7dert", "das", "s\u00fc\u00b7\u00dfe", "Nichts\u00b7tun", "durch", "den", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.4": {"text": "Und toller Leichtsinn tanzt dem Paar zur Seiten.", "tokens": ["Und", "tol\u00b7ler", "Leicht\u00b7sinn", "tanzt", "dem", "Paar", "zur", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es sprach von nordischen Bedenklichkeiten", "tokens": ["Es", "sprach", "von", "nor\u00b7di\u00b7schen", "Be\u00b7denk\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Natur sie los und bannte Reu' und Harm.", "tokens": ["Na\u00b7tur", "sie", "los", "und", "bann\u00b7te", "Reu'", "und", "Harm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "KON", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schw\u00fcl sind die Tage und die N\u00e4chte warm \u2013", "tokens": ["Schw\u00fcl", "sind", "die", "Ta\u00b7ge", "und", "die", "N\u00e4ch\u00b7te", "warm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "ART", "NN", "ADJD", "$("], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Das Laster mag am liebsten nackend schreiten.", "tokens": ["Das", "Las\u00b7ter", "mag", "am", "liebs\u00b7ten", "na\u00b7ckend", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKA", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht ist das Alter zahm, die Jugend bl\u00f6de.", "tokens": ["Nicht", "ist", "das", "Al\u00b7ter", "zahm", ",", "die", "Ju\u00b7gend", "bl\u00f6\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein jeder f\u00fchlt im brausenden Gewimmel", "tokens": ["Ein", "je\u00b7der", "f\u00fchlt", "im", "brau\u00b7sen\u00b7den", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geborgen sich und seine liebsten S\u00fcnden.", "tokens": ["Ge\u00b7bor\u00b7gen", "sich", "und", "sei\u00b7ne", "liebs\u00b7ten", "S\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So treibt er, was er mag, und ist es schn\u00f6de,", "tokens": ["So", "treibt", "er", ",", "was", "er", "mag", ",", "und", "ist", "es", "schn\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,", "KON", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er denkt getrost: selbst Gottes Aug' im Himmel", "tokens": ["Er", "denkt", "ge\u00b7trost", ":", "selbst", "Got\u00b7tes", "Aug'", "im", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "$.", "ADV", "NN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wei\u00df im Gew\u00fchl dich nicht herauszufinden.", "tokens": ["Wei\u00df", "im", "Ge\u00b7w\u00fchl", "dich", "nicht", "her\u00b7aus\u00b7zu\u00b7fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Hier haben wahrlich alle Menschlichkeiten", "tokens": ["Hier", "ha\u00b7ben", "wahr\u00b7lich", "al\u00b7le", "Menschlich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr Stelldichein. An des Genu