{"textgrid.poem.41724": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und den Ruf hab ich vernommen aus dem k\u00fchnen Dichtermunde", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und den Ruf hab ich vernommen aus dem k\u00fchnen Dichtermunde", "tokens": ["Und", "den", "Ruf", "hab", "ich", "ver\u00b7nom\u00b7men", "aus", "dem", "k\u00fch\u00b7nen", "Dich\u00b7ter\u00b7mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Und ich nahm das Wort zu Herzen, nahm es f\u00fcr Prophetenkunde,", "tokens": ["Und", "ich", "nahm", "das", "Wort", "zu", "Her\u00b7zen", ",", "nahm", "es", "f\u00fcr", "Pro\u00b7phe\u00b7ten\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Fr\u00f6hlich will das Schwert ich tragen, fr\u00f6hlich f\u00fcr mein Volk es schwingen,", "tokens": ["Fr\u00f6h\u00b7lich", "will", "das", "Schwert", "ich", "tra\u00b7gen", ",", "fr\u00f6h\u00b7lich", "f\u00fcr", "mein", "Volk", "es", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,", "ADJD", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Jubelnd deutsche Schlachtenlieder, unserm Feind entgegen singen.", "tokens": ["Ju\u00b7belnd", "deut\u00b7sche", "Schlach\u00b7ten\u00b7lie\u00b7der", ",", "un\u00b7serm", "Feind", "ent\u00b7ge\u00b7gen", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "PPOSAT", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Nicht die Kraft ein Schwert von Eisen in der schwachen Hand zu z\u00fccken,", "tokens": ["Nicht", "die", "Kraft", "ein", "Schwert", "von", "Ei\u00b7sen", "in", "der", "schwa\u00b7chen", "Hand", "zu", "z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Nicht der Mut aus tiefen Wunden blutge Blumen uns zu pfl\u00fccken.", "tokens": ["Nicht", "der", "Mut", "aus", "tie\u00b7fen", "Wun\u00b7den", "blut\u00b7ge", "Blu\u00b7men", "uns", "zu", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Nicht die Kunst den Blitz zu lenken aus dem m\u00f6rd'rischen Gescho\u00df \u2013", "tokens": ["Nicht", "die", "Kunst", "den", "Blitz", "zu", "len\u00b7ken", "aus", "dem", "m\u00f6rd'\u00b7ri\u00b7schen", "Ge\u00b7scho\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Solches ward uns nicht gegeben, solches nicht der Frauen Los.", "tokens": ["Sol\u00b7ches", "ward", "uns", "nicht", "ge\u00b7ge\u00b7ben", ",", "sol\u00b7ches", "nicht", "der", "Frau\u00b7en", "Los", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "PIS", "PTKNEG", "ART", "NN", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Aber wenn Ihr zieht zum Streite f\u00fcr des Vaterlandes Rechte,", "tokens": ["A\u00b7ber", "wenn", "Ihr", "zieht", "zum", "Strei\u00b7te", "f\u00fcr", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sticken wir die Freiheitsfahne, die Euch leitet im Gefechte,", "tokens": ["Sti\u00b7cken", "wir", "die", "Frei\u00b7heits\u00b7fah\u00b7ne", ",", "die", "Euch", "lei\u00b7tet", "im", "Ge\u00b7fech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Schlingen wir um Eure Schultern, sch\u00f6ngewebte Kriegerbinden", "tokens": ["Schlin\u00b7gen", "wir", "um", "Eu\u00b7re", "Schul\u00b7tern", ",", "sch\u00f6n\u00b7ge\u00b7web\u00b7te", "Krie\u00b7ger\u00b7bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sind es wir die Eure Wunden pflegen, Eure Lorbeern winden.", "tokens": ["Sind", "es", "wir", "die", "Eu\u00b7re", "Wun\u00b7den", "pfle\u00b7gen", ",", "Eu\u00b7re", "Lor\u00b7beern", "win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ART", "PPOSAT", "NN", "VVINF", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Doch so lang Ihr Euch nicht r\u00fcstet, eine Freiheitsschlacht zu schlagen,", "tokens": ["Doch", "so", "lang", "Ihr", "Euch", "nicht", "r\u00fcs\u00b7tet", ",", "ei\u00b7ne", "Frei\u00b7heits\u00b7schlacht", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "PPER", "PPER", "PTKNEG", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "F\u00fcr die h\u00f6chsten Menschenrechte eine k\u00fchnen Strau\u00df zu wagen,", "tokens": ["F\u00fcr", "die", "h\u00f6chs\u00b7ten", "Men\u00b7schen\u00b7rech\u00b7te", "ei\u00b7ne", "k\u00fch\u00b7nen", "Strau\u00df", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch so lang noch Eure Waffen in der engen Scheide bleiben,", "tokens": ["Doch", "so", "lang", "noch", "Eu\u00b7re", "Waf\u00b7fen", "in", "der", "en\u00b7gen", "Schei\u00b7de", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Werden wir es denn verm\u00f6gen Euch hinaus ins Feld zu treiben?", "tokens": ["Wer\u00b7den", "wir", "es", "denn", "ver\u00b7m\u00f6\u00b7gen", "Euch", "hin\u00b7aus", "ins", "Feld", "zu", "trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Alle M\u00e4dchen m\u00fcssen schw\u00f6ren keinen, keinen Mann zu minnen", "tokens": ["Al\u00b7le", "M\u00e4d\u00b7chen", "m\u00fcs\u00b7sen", "schw\u00f6\u00b7ren", "kei\u00b7nen", ",", "kei\u00b7nen", "Mann", "zu", "min\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "VMINF", "$,", "PIAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Der nicht f\u00fcr die Freiheit stritte seinem Volk sie zu gewinnen.", "tokens": ["Der", "nicht", "f\u00fcr", "die", "Frei\u00b7heit", "strit\u00b7te", "sei\u00b7nem", "Volk", "sie", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie Frau Gertrud einst gesprochen, m\u00fc\u00dften alle Frauen sprechen,", "tokens": ["Wie", "Frau", "Ger\u00b7trud", "einst", "ge\u00b7spro\u00b7chen", ",", "m\u00fc\u00df\u00b7ten", "al\u00b7le", "Frau\u00b7en", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NE", "ADV", "VVPP", "$,", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Als sie Stauffacher den Gatten, hie\u00df der Schweizer Knechtschaft brechen.", "tokens": ["Als", "sie", "Stauf\u00b7fa\u00b7cher", "den", "Gat\u00b7ten", ",", "hie\u00df", "der", "Schwei\u00b7zer", "Knecht\u00b7schaft", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+---+-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.6": {"line.1": {"text": "Also, also m\u00fc\u00dft es werden, k\u00f6nnt ich meiner Schwestern Herzen", "tokens": ["Al\u00b7so", ",", "al\u00b7so", "m\u00fc\u00dft", "es", "wer\u00b7den", ",", "k\u00f6nnt", "ich", "mei\u00b7ner", "Schwes\u00b7tern", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "So begeistern wie ich selber, f\u00fchle meines Volkes Schmerzen.", "tokens": ["So", "be\u00b7geis\u00b7tern", "wie", "ich", "sel\u00b7ber", ",", "f\u00fch\u00b7le", "mei\u00b7nes", "Vol\u00b7kes", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "PPER", "ADV", "$,", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "K\u00f6nnte ich die kalten Herzen, die nur kleine Qual und Freuden", "tokens": ["K\u00f6nn\u00b7te", "ich", "die", "kal\u00b7ten", "Her\u00b7zen", ",", "die", "nur", "klei\u00b7ne", "Qual", "und", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "F\u00fcllen und in Schlummer singen, k\u00f6nnt ich sie zum Gro\u00dfen leiten.", "tokens": ["F\u00fcl\u00b7len", "und", "in", "Schlum\u00b7mer", "sin\u00b7gen", ",", "k\u00f6nnt", "ich", "sie", "zum", "Gro\u00b7\u00dfen", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "VVINF", "$,", "VVFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Was vermag ein deutsches M\u00e4dchen, still und arm in enger Zelle,", "tokens": ["Was", "ver\u00b7mag", "ein", "deut\u00b7sches", "M\u00e4d\u00b7chen", ",", "still", "und", "arm", "in", "en\u00b7ger", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Aber frei gleich wie vom Berge niedersch\u00e4umt die freie Quelle \u2013", "tokens": ["A\u00b7ber", "frei", "gleich", "wie", "vom", "Ber\u00b7ge", "nie\u00b7der\u00b7sch\u00e4umt", "die", "frei\u00b7e", "Quel\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KOKOM", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Aber singend wie das V\u00f6glein, das sich wiegt in blauen L\u00fcften,", "tokens": ["A\u00b7ber", "sin\u00b7gend", "wie", "das", "V\u00f6\u00b7glein", ",", "das", "sich", "wiegt", "in", "blau\u00b7en", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aber feurig wie der Blitzstrahl, kommt aus dunklen Wolkenkl\u00fcften:", "tokens": ["A\u00b7ber", "feu\u00b7rig", "wie", "der", "Blitz\u00b7strahl", ",", "kommt", "aus", "dunk\u00b7len", "Wol\u00b7ken\u00b7kl\u00fcf\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Was vermag ein solches M\u00e4dchen, dies zu schaffen will ich streben", "tokens": ["Was", "ver\u00b7mag", "ein", "sol\u00b7ches", "M\u00e4d\u00b7chen", ",", "dies", "zu", "schaf\u00b7fen", "will", "ich", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "PIAT", "NN", "$,", "PDS", "PTKZU", "VVINF", "VMFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "F\u00fcr die Freiheit f\u00fcr den Fortschritt weihe ich mein ganzes Leben.", "tokens": ["F\u00fcr", "die", "Frei\u00b7heit", "f\u00fcr", "den", "Fort\u00b7schritt", "wei\u00b7he", "ich", "mein", "gan\u00b7zes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Denn mein Herz kennt nur ein Sehnen, nur ein stetig Vorw\u00e4rtsringen", "tokens": ["Denn", "mein", "Herz", "kennt", "nur", "ein", "Seh\u00b7nen", ",", "nur", "ein", "ste\u00b7tig", "Vor\u00b7w\u00e4rts\u00b7rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "ART", "ADJD", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und dem Vaterland geh\u00f6rt es und der Freiheit will ich singen.", "tokens": ["Und", "dem", "Va\u00b7ter\u00b7land", "ge\u00b7h\u00f6rt", "es", "und", "der", "Frei\u00b7heit", "will", "ich", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "KON", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Singen, denn im Kampf mit Liedern, denn im Kampf mit k\u00fchnen Reden,", "tokens": ["Sin\u00b7gen", ",", "denn", "im", "Kampf", "mit", "Lie\u00b7dern", ",", "denn", "im", "Kampf", "mit", "k\u00fch\u00b7nen", "Re\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "APPRART", "NN", "APPR", "NN", "$,", "KON", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Darf auch ich, die Fahne tragend, zu den Gleichgesinnten treten,", "tokens": ["Darf", "auch", "ich", ",", "die", "Fah\u00b7ne", "tra\u00b7gend", ",", "zu", "den", "Gleich\u00b7ge\u00b7sinn\u00b7ten", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "$,", "ART", "NN", "VVPP", "$,", "APPR", "ART",