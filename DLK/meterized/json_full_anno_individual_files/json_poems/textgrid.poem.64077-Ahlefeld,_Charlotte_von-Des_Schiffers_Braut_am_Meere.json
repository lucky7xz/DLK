{"textgrid.poem.64077": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Des Schiffers Braut am Meere", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es braust der Sturm \u2013 mit schaumbedeckten Wellen", "tokens": ["Es", "braust", "der", "Sturm", "\u2013", "mit", "schaum\u00b7be\u00b7deck\u00b7ten", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erhebt sich rauschend das erz\u00fcrnte Meer,", "tokens": ["Er\u00b7hebt", "sich", "rau\u00b7schend", "das", "er\u00b7z\u00fcrn\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und rascher Blitze Feuerstrahlen hellen", "tokens": ["Und", "ra\u00b7scher", "Blit\u00b7ze", "Feu\u00b7er\u00b7strah\u00b7len", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der D\u00e4mmrung Nebel grausend um mich her.", "tokens": ["Der", "D\u00e4mm\u00b7rung", "Ne\u00b7bel", "grau\u00b7send", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort in der H\u00fctte mir ein Obdach w\u00e4hlen,", "tokens": ["Dort", "in", "der", "H\u00fct\u00b7te", "mir", "ein", "Ob\u00b7dach", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "M\u00f6cht' ich so gern, doch ach, der nasse Blick", "tokens": ["M\u00f6cht'", "ich", "so", "gern", ",", "doch", "ach", ",", "der", "nas\u00b7se", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "$,", "ADV", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schaut um aufs neu das bange Herz zu qu\u00e4len,", "tokens": ["Schaut", "um", "aufs", "neu", "das", "ban\u00b7ge", "Herz", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "ADJD", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach dem emp\u00f6rten Element zur\u00fcck.", "tokens": ["Nach", "dem", "em\u00b7p\u00f6r\u00b7ten", "E\u00b7le\u00b7ment", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und kann die Schauerszene nicht verlassen,", "tokens": ["Und", "kann", "die", "Schau\u00b7ers\u00b7ze\u00b7ne", "nicht", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die das Gem\u00fcth mit schwarzer Ahndung f\u00fcllt,", "tokens": ["Die", "das", "Ge\u00b7m\u00fcth", "mit", "schwar\u00b7zer", "Ahn\u00b7dung", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn des Geliebten fernes Schiff umfassen", "tokens": ["Denn", "des", "Ge\u00b7lieb\u00b7ten", "fer\u00b7nes", "Schiff", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Wellen tobend, rings von Graun umh\u00fcllt.", "tokens": ["Die", "Wel\u00b7len", "to\u00b7bend", ",", "rings", "von", "Graun", "um\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und doppelten Gefahren Preis gegeben,", "tokens": ["Und", "dop\u00b7pel\u00b7ten", "Ge\u00b7fah\u00b7ren", "Preis", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Dem Drohn des Himmels, und der Wellen Wuth,", "tokens": ["Dem", "Drohn", "des", "Him\u00b7mels", ",", "und", "der", "Wel\u00b7len", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geht jetzt vielleicht sein ewig theures Leben", "tokens": ["Geht", "jetzt", "viel\u00b7leicht", "sein", "e\u00b7wig", "theu\u00b7res", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vernichtet unter in der tiefen Fluth.", "tokens": ["Ver\u00b7nich\u00b7tet", "un\u00b7ter", "in", "der", "tie\u00b7fen", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Entsetzen fasst mich \u2013 Nebelschleier schwanken", "tokens": ["Ent\u00b7set\u00b7zen", "fasst", "mich", "\u2013", "Ne\u00b7bel\u00b7schlei\u00b7er", "schwan\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$(", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie d\u00fcstrer Flor vor meinem Angesicht.", "tokens": ["Wie", "d\u00fcst\u00b7rer", "Flor", "vor", "mei\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Herz steht still \u2013 es schwinden die Gedanken,", "tokens": ["Mein", "Herz", "steht", "still", "\u2013", "es", "schwin\u00b7den", "die", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$(", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich m\u00f6chte weinen \u2013 ach und kann es nicht!", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wei\u00b7nen", "\u2013", "ach", "und", "kann", "es", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$(", "XY", "KON", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ihr M\u00e4chte dort in jenen fernen H\u00f6hen,", "tokens": ["Ihr", "M\u00e4ch\u00b7te", "dort", "in", "je\u00b7nen", "fer\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Ihr den Sturm zu uns herab gesandt,", "tokens": ["Die", "Ihr", "den", "Sturm", "zu", "uns", "her\u00b7ab", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O lasst sein Schiff nicht scheiternd untergehen,", "tokens": ["O", "lasst", "sein", "Schiff", "nicht", "schei\u00b7ternd", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zeigt Rettung bietend ihm ein sichres Land.", "tokens": ["Zeigt", "Ret\u00b7tung", "bie\u00b7tend", "ihm", "ein", "sich\u00b7res", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVPP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und sendet in die drohenden Gefahren", "tokens": ["Und", "sen\u00b7det", "in", "die", "dro\u00b7hen\u00b7den", "Ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Hoffnung lichten, morgenrothen Strahl,", "tokens": ["Der", "Hoff\u00b7nung", "lich\u00b7ten", ",", "mor\u00b7gen\u00b7ro\u00b7then", "Strahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Lasst ihn den Muth im Busen treu bewahren", "tokens": ["Lasst", "ihn", "den", "Muth", "im", "Bu\u00b7sen", "treu", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und lindert tr\u00f6stend der Erwartung Qual.", "tokens": ["Und", "lin\u00b7dert", "tr\u00f6s\u00b7tend", "der", "Er\u00b7war\u00b7tung", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Doch ist des Todes Loos ihm schon gefallen,", "tokens": ["Doch", "ist", "des", "To\u00b7des", "Loos", "ihm", "schon", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NE", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So l\u00f6scht auch mir des Daseyns goldnes Licht.", "tokens": ["So", "l\u00f6scht", "auch", "mir", "des", "Da\u00b7seyns", "gold\u00b7nes", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit ihm sinkt meine Welt, auch ich will mit ihm fallen,", "tokens": ["Mit", "ihm", "sinkt", "mei\u00b7ne", "Welt", ",", "auch", "ich", "will", "mit", "ihm", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "m