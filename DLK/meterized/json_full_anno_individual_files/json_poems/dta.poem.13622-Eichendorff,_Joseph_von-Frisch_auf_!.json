{"dta.poem.13622": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Frisch auf !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und hoch \u00fcber dem allersch\u00f6nsten Wald", "tokens": ["Und", "hoch", "\u00fc\u00b7ber", "dem", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Machte sie l\u00e4chelnd auf einmal Halt.", "tokens": ["Mach\u00b7te", "sie", "l\u00e4\u00b7chelnd", "auf", "ein\u00b7mal", "Halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADV", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da sah ich erschrocken zwischen den B\u00e4umen", "tokens": ["Da", "sah", "ich", "er\u00b7schro\u00b7cken", "zwi\u00b7schen", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Meine Heimath unten wie in Tr\u00e4umen,", "tokens": ["Mei\u00b7ne", "Hei\u00b7math", "un\u00b7ten", "wie", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KOKOM", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Das Schlo\u00df, den Garten und die stille Luft,", "tokens": ["Das", "Schlo\u00df", ",", "den", "Gar\u00b7ten", "und", "die", "stil\u00b7le", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die blauen Berge dahinter im Duft", "tokens": ["Die", "blau\u00b7en", "Ber\u00b7ge", "da\u00b7hin\u00b7ter", "im", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PAV", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und alle die sch\u00f6ne alte Zeit", "tokens": ["Und", "al\u00b7le", "die", "sch\u00f6\u00b7ne", "al\u00b7te", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "In der wundersamen Einsamkeit.", "tokens": ["In", "der", "wun\u00b7der\u00b7sa\u00b7men", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Und als ich mich wandte, war ich allein,", "tokens": ["Und", "als", "ich", "mich", "wand\u00b7te", ",", "war", "ich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Das Ro\u00df nur wiehert' in den Morgen hinein,", "tokens": ["Das", "Ro\u00df", "nur", "wie\u00b7hert'", "in", "den", "Mor\u00b7gen", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.11": {"text": "Mir aber war's als w\u00e4r' ich wieder jung,", "tokens": ["Mir", "a\u00b7ber", "wa\u00b7r's", "als", "w\u00e4r'", "ich", "wie\u00b7der", "jung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "KOKOM", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und wu\u00dfte der Lieder noch genung!", "tokens": ["Und", "wu\u00df\u00b7te", "der", "Lie\u00b7der", "noch", "ge\u00b7nung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}