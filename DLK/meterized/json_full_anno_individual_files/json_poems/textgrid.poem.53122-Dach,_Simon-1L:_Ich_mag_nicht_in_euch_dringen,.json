{"textgrid.poem.53122": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich mag nicht in euch dringen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich mag nicht in euch dringen,", "tokens": ["Ich", "mag", "nicht", "in", "euch", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Seiten, meine Zier,", "tokens": ["Ihr", "Sei\u00b7ten", ",", "mei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr wolt mit gute singen,", "tokens": ["Ihr", "wolt", "mit", "gu\u00b7te", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt aber folget mir!", "tokens": ["Jetzt", "a\u00b7ber", "fol\u00b7get", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wil mich lassen h\u00f6ren", "tokens": ["Ich", "wil", "mich", "las\u00b7sen", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem Breutgam vnd der Braut", "tokens": ["Dem", "Breut\u00b7gam", "vnd", "der", "Braut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "KON", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu sonderlichen Ehren,", "tokens": ["Zu", "son\u00b7der\u00b7li\u00b7chen", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Folgt, klinget rein vnd laut!", "tokens": ["Folgt", ",", "klin\u00b7get", "rein", "vnd", "laut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Breutgam ist ergeben", "tokens": ["Der", "Breut\u00b7gam", "ist", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Musen edlen Kunst,", "tokens": ["Der", "Mu\u00b7sen", "ed\u00b7len", "Kunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat durch das Hoffe-Leben", "tokens": ["Hat", "durch", "das", "Hof\u00b7fe\u00b7Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erhalten Gnad' vnd Gunst,", "tokens": ["Er\u00b7hal\u00b7ten", "Gnad'", "vnd", "Gunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da sind sein eigen worden", "tokens": ["Da", "sind", "sein", "ei\u00b7gen", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJD", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erfahrung vnd Verstand,", "tokens": ["Er\u00b7fah\u00b7rung", "vnd", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Rh\u00fcmt an jhm Witz vnd Handt.", "tokens": ["Rh\u00fcmt", "an", "jhm", "Witz", "vnd", "Handt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Braut hat jhre Jugend", "tokens": ["Die", "Braut", "hat", "jhre", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Durch vnbewegten Schlu\u00df", "tokens": ["Durch", "vn\u00b7be\u00b7weg\u00b7ten", "Schlu\u00df"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Vnschuld, Zucht vnd Tugend", "tokens": ["Mit", "Vn\u00b7schuld", ",", "Zucht", "vnd", "Tu\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verbunden, dessen mu\u00df", "tokens": ["Ver\u00b7bun\u00b7den", ",", "des\u00b7sen", "mu\u00df"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "PDS", "VMFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bey ", "tokens": ["Bey"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Der sie hat wollen dienen,", "tokens": ["Der", "sie", "hat", "wol\u00b7len", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein gutes Zeugni\u00df seyn.", "tokens": ["Ein", "gu\u00b7tes", "Zeug\u00b7ni\u00df", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Einigkeit in Sitten,", "tokens": ["Die", "Ei\u00b7nig\u00b7keit", "in", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sinnen gleiche Tracht", "tokens": ["Der", "Sin\u00b7nen", "glei\u00b7che", "Tracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat beyder Hertz erstritten", "tokens": ["Hat", "bey\u00b7der", "Hertz", "er\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd in die Gluet gebracht,", "tokens": ["Vnd", "in", "die", "Gluet", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Eins mu\u00df das andre lieben,", "tokens": ["Eins", "mu\u00df", "das", "and\u00b7re", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es wird der ", "tokens": ["Es", "wird", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Auch von Ihr vnterschrieben,", "tokens": ["Auch", "von", "Ihr", "vn\u00b7ter\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das ist der Heyrath-Bund.", "tokens": ["Das", "ist", "der", "Hey\u00b7ra\u00b7th\u00b7Bund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Der Himmel wird f\u00fcr allen", "tokens": ["Der", "Him\u00b7mel", "wird", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ersucht vmb wolergehn,", "tokens": ["Er\u00b7sucht", "vmb", "wo\u00b7ler\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der l\u00e4sst es jhm gefallen", "tokens": ["Der", "l\u00e4sst", "es", "jhm", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd wil zu Diensten stehn.", "tokens": ["Vnd", "wil", "zu", "Diens\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Herbstzeit wird genommen", "tokens": ["Der", "Herbst\u00b7zeit", "wird", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Frostes strenger Zwangk,", "tokens": ["Des", "Fros\u00b7tes", "stren\u00b7ger", "Zwangk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Pregel-strom ist kommen", "tokens": ["Der", "Pre\u00b7gel\u00b7strom", "ist", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In seinen alten Gangk.", "tokens": ["In", "sei\u00b7nen", "al\u00b7ten", "Gangk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Tag bricht an von ferne", "tokens": ["Der", "Tag", "bricht", "an", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch sch\u00f6nes Morgenroht,", "tokens": ["Durch", "sch\u00f6\u00b7nes", "Mor\u00b7gen\u00b7roht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Nacht ist voller Sterne,", "tokens": ["Die", "Nacht", "ist", "vol\u00b7ler", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Lufft wei\u00df keine Noht:", "tokens": ["Die", "Lufft", "wei\u00df", "kei\u00b7ne", "Noht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mich soltt' es wunder haben,", "tokens": ["Mich", "soltt'", "es", "wun\u00b7der", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann dieses grosse Heer", "tokens": ["Wann", "die\u00b7ses", "gros\u00b7se", "Heer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Himmels-Gunst vnd Gaben", "tokens": ["Der", "Him\u00b7mels\u00b7Gunst", "vnd", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein b\u00f6ses Zeichen wer'?", "tokens": ["Ein", "b\u00f6\u00b7ses", "Zei\u00b7chen", "wer'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich aber w\u00fcnsch euch beyden", "tokens": ["Ich", "a\u00b7ber", "w\u00fcnsch", "euch", "bey\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im vbrigen dazu", "tokens": ["Im", "vbri\u00b7gen", "da\u00b7zu"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "PAV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Gantz vnbekr\u00e4nckte Frewden", "tokens": ["Gantz", "vn\u00b7be\u00b7kr\u00e4nck\u00b7te", "Frew\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sampt aller Gn\u00fcg vnd Rhue,", "tokens": ["Sampt", "al\u00b7ler", "Gn\u00fcg", "vnd", "Rhue", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich, der ich angetrieben", "tokens": ["Ich", ",", "der", "ich", "an\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch ewrer Liebe Macht", "tokens": ["Durch", "ew\u00b7rer", "Lie\u00b7be", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die\u00df Hochzeit-Lied geschrieben", "tokens": ["Die\u00df", "Hoch\u00b7zeit\u00b7Lied", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PDS", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Heut vmb die Mitternacht.", "tokens": ["Heut", "vmb", "die", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich mag nicht in euch dringen,", "tokens": ["Ich", "mag", "nicht", "in", "euch", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Seiten, meine Zier,", "tokens": ["Ihr", "Sei\u00b7ten", ",", "mei\u00b7ne", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr wolt mit gute singen,", "tokens": ["Ihr", "wolt", "mit", "gu\u00b7te", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt aber folget mir!", "tokens": ["Jetzt", "a\u00b7ber", "fol\u00b7get", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wil mich lassen h\u00f6ren", "tokens": ["Ich", "wil", "mich", "las\u00b7sen", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "