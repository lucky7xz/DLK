{"textgrid.poem.65082": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: In eine H\u00f6lle hast du mich versto\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In eine H\u00f6lle hast du mich versto\u00dfen,", "tokens": ["In", "ei\u00b7ne", "H\u00f6l\u00b7le", "hast", "du", "mich", "ver\u00b7sto\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An deren Eingang alle Sterne schwinden,", "tokens": ["An", "de\u00b7ren", "Ein\u00b7gang", "al\u00b7le", "Ster\u00b7ne", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Sel'gen w\u00e4hnte ich mich zu verbinden,", "tokens": ["Den", "Sel'\u00b7gen", "w\u00e4hn\u00b7te", "ich", "mich", "zu", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nun sind die Verdammten mir Genossen.", "tokens": ["Und", "nun", "sind", "die", "Ver\u00b7damm\u00b7ten", "mir", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Gnadenfrist der Reue ist verflossen,", "tokens": ["Die", "Gna\u00b7den\u00b7frist", "der", "Reu\u00b7e", "ist", "ver\u00b7flos\u00b7sen", ","], "token_info": [