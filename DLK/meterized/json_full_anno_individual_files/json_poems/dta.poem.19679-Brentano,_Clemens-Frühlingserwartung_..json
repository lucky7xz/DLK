{"dta.poem.19679": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingserwartung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schlagt ihr muntern Nachtigallen,               ", "tokens": ["Schlagt", "ihr", "mun\u00b7tern", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft den hellen, reinen Ton,", "tokens": ["La\u00dft", "den", "hel\u00b7len", ",", "rei\u00b7nen", "Ton", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die dichten Str\u00e4ucher fallen,", "tokens": ["Durch", "die", "dich\u00b7ten", "Str\u00e4u\u00b7cher", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seyd gebeten singet schon:", "tokens": ["Seyd", "ge\u00b7be\u00b7ten", "sin\u00b7get", "schon", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "VVPP", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihr Schach Schimel und Hirsch", "tokens": ["Und", "ihr", "Schach", "Schi\u00b7mel", "und", "Hirsch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und Esra Saul und M\u00fcrsch,", "tokens": ["Und", "Es\u00b7ra", "Saul", "und", "M\u00fcrsch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Pincus, Moses, Meyer", "tokens": ["Pin\u00b7cus", ",", "Mo\u00b7ses", ",", "Me\u00b7yer"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "K\u00f6mmt zu dieser Feyer,", "tokens": ["K\u00f6mmt", "zu", "die\u00b7ser", "Fe\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Heut mu\u00df Fr\u00fchling seyn.", "tokens": ["Heut", "mu\u00df", "Fr\u00fch\u00b7ling", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Klingts nicht wie neu Gold dies Singen,", "tokens": ["Klingts", "nicht", "wie", "neu", "Gold", "dies", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "KOKOM", "ADJD", "NN", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach so s\u00fcsse kann wohl kaum", "tokens": ["Ach", "so", "s\u00fcs\u00b7se", "kann", "wohl", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "ADJA", "VMFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aaronis Leibrock klingen", "tokens": ["Aa\u00b7ro\u00b7nis", "Lei\u00b7brock", "klin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit den Cimbeln an dem Saum:", "tokens": ["Mit", "den", "Cim\u00b7beln", "an", "dem", "Saum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihr Schach, Schimel und Hirsch", "tokens": ["Und", "ihr", "Schach", ",", "Schi\u00b7mel", "und", "Hirsch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Und Esra, Saul und M\u00fcrsch", "tokens": ["Und", "Es\u00b7ra", ",", "Saul", "und", "M\u00fcrsch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "$,", "NE", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Macht kein Streit und H\u00e4ndel,", "tokens": ["Macht", "kein", "Streit", "und", "H\u00e4n\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Bindt die Schuh mit B\u00e4ndel,", "tokens": ["Bindt", "die", "Schuh", "mit", "B\u00e4n\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Heut mu\u00df Fr\u00fchling seyn.", "tokens": ["Heut", "mu\u00df", "Fr\u00fch\u00b7ling", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}