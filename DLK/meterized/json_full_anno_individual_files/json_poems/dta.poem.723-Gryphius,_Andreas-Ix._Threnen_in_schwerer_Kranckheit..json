{"dta.poem.723": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  Threnen in schwerer Kranckheit.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin nicht der ich war/ die kr\u00e4ffte sind verschwunden. ", "tokens": ["Ich", "bin", "nicht", "der", "ich", "war", "/", "die", "kr\u00e4ff\u00b7te", "sind", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "PPER", "VAFIN", "$(", "ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die glider sind verd\u00f6rr't/ als ein durchbrandter graus. ", "tokens": ["Die", "gli\u00b7der", "sind", "ver\u00b7d\u00f6rr't", "/", "als", "ein", "durch\u00b7brand\u00b7ter", "graus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir schauwt der schwartze Tod zu beyden Augen au\u00df.", "tokens": ["Mir", "schauwt", "der", "schwart\u00b7ze", "Tod", "zu", "bey\u00b7den", "Au\u00b7gen", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich werde von mir selbst nicht mehr in mir gefunden. ", "tokens": ["Ich", "wer\u00b7de", "von", "mir", "selbst", "nicht", "mehr", "in", "mir", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Athem wil nicht fort/ die zunge steht gebunden.", "tokens": ["Der", "A\u00b7them", "wil", "nicht", "fort", "/", "die", "zun\u00b7ge", "steht", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "PTKVZ", "$(", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer siht nicht/ wenn er siht die abgezehrte mau\u00df", "tokens": ["Wer", "siht", "nicht", "/", "wenn", "er", "siht", "die", "ab\u00b7ge\u00b7zehr\u00b7te", "mau\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die armen sonder fleisch/ das di\u00df mein sch", "tokens": ["Die", "ar\u00b7men", "son\u00b7der", "fleisch", "/", "das", "di\u00df", "mein", "sch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADJD", "$(", "ART", "PDS", "PPOSAT", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Leib ein brechen wird/ noch inner wenig stunden.", "tokens": ["Der", "Leib", "ein", "bre\u00b7chen", "wird", "/", "noch", "in\u00b7ner", "we\u00b7nig", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVINF", "VAFIN", "$(", "ADV", "ADJD", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gleich wie die wiesen Blum lebt wenn das licht der Welt", "tokens": ["Gleich", "wie", "die", "wie\u00b7sen", "Blum", "lebt", "wenn", "das", "licht", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NE", "VVFIN", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hervor bricht/ vnd noch ehr der Mittag weggeht/ f\u00e4lt;", "tokens": ["Her\u00b7vor", "bricht", "/", "vnd", "noch", "ehr", "der", "Mit\u00b7tag", "weg\u00b7geht", "/", "f\u00e4lt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "KON", "ADV", "NN", "ART", "NN", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bin ich auch benetzt mit Threnentaw ankommen.", "tokens": ["So", "bin", "ich", "auch", "be\u00b7netzt", "mit", "Thre\u00b7nen\u00b7taw", "an\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So sterb ich vor der zeit. O Erden gute nacht! ", "tokens": ["So", "sterb", "ich", "vor", "der", "zeit", ".", "O", "Er\u00b7den", "gu\u00b7te", "nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein st\u00fcndlin laufft zum end/ jtzt hab ich au\u00dfgewacht", "tokens": ["Mein", "st\u00fcnd\u00b7lin", "laufft", "zum", "end", "/", "jtzt", "hab", "ich", "au\u00df\u00b7ge\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$(", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd werde von dem schlaff des Todes eingenommen.", "tokens": ["Vnd", "wer\u00b7de", "von", "dem", "schlaff", "des", "To\u00b7des", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}