{"textgrid.poem.64883": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bange nicht vor k\u00fcnft'gen Tagen,", "tokens": ["Ban\u00b7ge", "nicht", "vor", "k\u00fcnft'\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor der Liebe Unbestand!", "tokens": ["Vor", "der", "Lie\u00b7be", "Un\u00b7be\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herzen, die wie unsre schlagen", "tokens": ["Her\u00b7zen", ",", "die", "wie", "uns\u00b7re", "schla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "KOKOM", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eint ein unzerrei\u00dfbar Band!", "tokens": ["Eint", "ein", "un\u00b7zer\u00b7rei\u00df\u00b7bar", "Band", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Geister, die wie unsre streben", "tokens": ["Geis\u00b7ter", ",", "die", "wie", "uns\u00b7re", "stre\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "KOKOM", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Wahrheit Sonnenbahn,", "tokens": ["Zu", "der", "Wahr\u00b7heit", "Son\u00b7nen\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind dem wechselvollen Leben", "tokens": ["Sind", "dem", "wech\u00b7sel\u00b7vol\u00b7len", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun und nimmer unterthan.", "tokens": ["Nun", "und", "nim\u00b7mer", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hat uns denn ein Bund vereinet,", "tokens": ["Hat", "uns", "denn", "ein", "Bund", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ihn jeder Tag gebiert,", "tokens": ["Wie", "ihn", "je\u00b7der", "Tag", "ge\u00b7biert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo gek\u00fc\u00dft, gelacht, geweinet,", "tokens": ["Wo", "ge\u00b7k\u00fc\u00dft", ",", "ge\u00b7lacht", ",", "ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich auch geschieden wird?", "tokens": ["End\u00b7lich", "auch", "ge\u00b7schie\u00b7den", "wird", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unsre Liebe ist Erkennen,", "tokens": ["Uns\u00b7re", "Lie\u00b7be", "ist", "Er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streben nach dem gleichen Ziel!", "tokens": ["Stre\u00b7ben", "nach", "dem", "glei\u00b7chen", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was kann solche Liebe trennen,", "tokens": ["Was", "kann", "sol\u00b7che", "Lie\u00b7be", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unerreicht vom Schicksalsspiel?", "tokens": ["Un\u00b7er\u00b7reicht", "vom", "Schick\u00b7sals\u00b7spiel", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Tausend Lichtgedanken ranken", "tokens": ["Tau\u00b7send", "Licht\u00b7ge\u00b7dan\u00b7ken", "ran\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["CARD", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich um uns und halten fest,", "tokens": ["Sich", "um", "uns", "und", "hal\u00b7ten", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Ohne Weichen, ohne Wanken,", "tokens": ["Oh\u00b7ne", "Wei\u00b7chen", ",", "oh\u00b7ne", "Wan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich an meine Brust gepre\u00dft.", "tokens": ["Dich", "an", "mei\u00b7ne", "Brust", "ge\u00b7pre\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Einen engen Pfad bestrahlen", "tokens": ["Ei\u00b7nen", "en\u00b7gen", "Pfad", "be\u00b7strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleiche Sterne dir, wie mir, \u2013", "tokens": ["Glei\u00b7che", "Ster\u00b7ne", "dir", ",", "wie", "mir", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "PPER", "$,", "PWAV", "PPER", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt' ich auf dem rauhen, schmalen,", "tokens": ["K\u00f6nnt'", "ich", "auf", "dem", "rau\u00b7hen", ",", "schma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jemals trennen mich von dir?", "tokens": ["Je\u00b7mals", "tren\u00b7nen", "mich", "von", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "M\u00f6glich, da\u00df in k\u00fcnft'gen Zeiten", "tokens": ["M\u00f6g\u00b7lich", ",", "da\u00df", "in", "k\u00fcnft'\u00b7gen", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Andrem Gl\u00fcck dein Wunsch noch winkt,", "tokens": ["A\u00b7ndrem", "Gl\u00fcck", "dein", "Wunsch", "noch", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Born der Seligkeiten", "tokens": ["Und", "den", "Born", "der", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst von fremden Lippen trinkt.", "tokens": ["Einst", "von", "frem\u00b7den", "Lip\u00b7pen", "trinkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "M\u00f6glich, da\u00df in neuer Regung", "tokens": ["M\u00f6g\u00b7lich", ",", "da\u00df", "in", "neu\u00b7er", "Re\u00b7gung"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst auch meine Seele schwillt", "tokens": ["Einst", "auch", "mei\u00b7ne", "See\u00b7le", "schwillt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in st\u00fcrmischer Bewegung", "tokens": ["Und", "in", "st\u00fcr\u00b7mi\u00b7scher", "Be\u00b7we\u00b7gung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin sich neigt zu fremdem Bild:", "tokens": ["Hin", "sich", "neigt", "zu", "frem\u00b7dem", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch den Geistesbund zu brechen,", "tokens": ["Doch", "den", "Geis\u00b7tes\u00b7bund", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sich leuchtend um uns flicht,", "tokens": ["Der", "sich", "leuch\u00b7tend", "um", "uns", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVPP", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dich von mir loszusprechen, \u2013", "tokens": ["Und", "dich", "von", "mir", "los\u00b7zu\u00b7spre\u00b7chen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVINF", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das, mein Lieb, vermagst du nicht!", "tokens": ["Das", ",", "mein", "Lieb", ",", "ver\u00b7magst", "du", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch dein Lieben zu vergessen", "tokens": ["Doch", "dein", "Lie\u00b7ben", "zu", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst bei andrer Augen Schein,", "tokens": ["Einst", "bei", "an\u00b7drer", "Au\u00b7gen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaube mir, ich werde dessen", "tokens": ["Glau\u00b7be", "mir", ",", "ich", "wer\u00b7de", "des\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "PPER", "VAFIN", "PDS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun und nimmer f\u00e4hig sein.", "tokens": ["Nun", "und", "nim\u00b7mer", "f\u00e4\u00b7hig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Drum, ob sich auch unsre Seelen", "tokens": ["Drum", ",", "ob", "sich", "auch", "uns\u00b7re", "See\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "PRF", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einst auch beugen neuem Joch,", "tokens": ["Einst", "auch", "beu\u00b7gen", "neu\u00b7em", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird uns Freundschaft stets verm\u00e4hlen,", "tokens": ["Wird", "uns", "Freund\u00b7schaft", "stets", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Bund bleibt ewig doch!", "tokens": ["Und", "der", "Bund", "bleibt", "e\u00b7wig", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Bange nicht vor k\u00fcnft'gen Tagen,", "tokens": ["Ban\u00b7ge", "nicht", "vor", "k\u00fcnft'\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "lin