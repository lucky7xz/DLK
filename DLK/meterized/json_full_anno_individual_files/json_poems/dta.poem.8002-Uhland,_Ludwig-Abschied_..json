{"dta.poem.8002": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Abschied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Was klinget und singet die Stra\u00df\u2019 herauf?", "tokens": ["Was", "klin\u00b7get", "und", "sin\u00b7get", "die", "Stra\u00df'", "her\u00b7auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ihr Jungfern, machet die Fenster auf!", "tokens": ["Ihr", "Jung\u00b7fern", ",", "ma\u00b7chet", "die", "Fens\u00b7ter", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Es ziehet der Bursch in die Weite,", "tokens": ["Es", "zie\u00b7het", "der", "Bursch", "in", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sie geben ihm das Geleite.", "tokens": ["Sie", "ge\u00b7ben", "ihm", "das", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wohl jauchzen die Andern und schwingen die H\u00fct\u2019,", "tokens": ["Wohl", "jauch\u00b7zen", "die", "An\u00b7dern", "und", "schwin\u00b7gen", "die", "H\u00fct'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Viel B\u00e4nder darauf und viel edle Bl\u00fcth\u2019,", "tokens": ["Viel", "B\u00e4n\u00b7der", "da\u00b7rauf", "und", "viel", "ed\u00b7le", "Bl\u00fcth'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PAV", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch dem Burschen gef\u00e4llt nicht die Sitte,", "tokens": ["Doch", "dem", "Bur\u00b7schen", "ge\u00b7f\u00e4llt", "nicht", "die", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Geht still und bleich in der Mitte.", "tokens": ["Geht", "still", "und", "bleich", "in", "der", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wohl klingen die Kannen, wohl funkelt der Wein;", "tokens": ["Wohl", "klin\u00b7gen", "die", "Kan\u00b7nen", ",", "wohl", "fun\u00b7kelt", "der", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u201etrink aus und trink wieder, lieb Bruder mein!\u201c", "tokens": ["\u201e", "trink", "aus", "und", "trink", "wie\u00b7der", ",", "lieb", "Bru\u00b7der", "mein", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADV", "$,", "ADJD", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201emit dem Abschiedsweine nur fliehet,", "tokens": ["\u201e", "mit", "dem", "Ab\u00b7schieds\u00b7wei\u00b7ne", "nur", "flie\u00b7het", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der da innen mir brennet und gl\u00fchet!\u201c", "tokens": ["Der", "da", "in\u00b7nen", "mir", "bren\u00b7net", "und", "gl\u00fc\u00b7het", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.4": {"line.1": {"text": "Und drau\u00dfen am allerletzten Haus,", "tokens": ["Und", "drau\u00b7\u00dfen", "am", "al\u00b7ler\u00b7letz\u00b7ten", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da gucket ein M\u00e4gdlein zum Fenster heraus,", "tokens": ["Da", "gu\u00b7cket", "ein", "M\u00e4gd\u00b7lein", "zum", "Fens\u00b7ter", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Sie m\u00f6cht\u2019 ihre Thr\u00e4nen verdecken", "tokens": ["Sie", "m\u00f6cht'", "ih\u00b7re", "Thr\u00e4\u00b7nen", "ver\u00b7de\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mit Gelbveiglein und Rosenst\u00f6cken.", "tokens": ["Mit", "Gelb\u00b7veig\u00b7lein", "und", "Ro\u00b7sen\u00b7st\u00f6\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und drau\u00dfen am allerletzten Haus,", "tokens": ["Und", "drau\u00b7\u00dfen", "am", "al\u00b7ler\u00b7letz\u00b7ten", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da schl\u00e4gt der Bursche die Augen auf,", "tokens": ["Da", "schl\u00e4gt", "der", "Bur\u00b7sche", "die", "Au\u00b7gen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schl\u00e4gt sie nieder mit Schmerze", "tokens": ["Und", "schl\u00e4gt", "sie", "nie\u00b7der", "mit", "Schmer\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und leget die Hand auf\u2019s Herze.", "tokens": ["Und", "le\u00b7get", "die", "Hand", "auf's", "Her\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "\u201eherr Bruder! und hast du noch keinen Strau\u00df,", "tokens": ["\u201e", "herr", "Bru\u00b7der", "!", "und", "hast", "du", "noch", "kei\u00b7nen", "Strau\u00df", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "KON", "VAFIN", "PPER", "ADV", "PIAT", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dort winken und wanken viel Blumen heraus.", "tokens": ["Dort", "win\u00b7ken", "und", "wan\u00b7ken", "viel", "Blu\u00b7men", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wohlauf, du Sch\u00f6nste von Allen,", "tokens": ["Wohl\u00b7auf", ",", "du", "Sch\u00f6ns\u00b7te", "von", "Al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "La\u00df ein Str\u00e4u\u00dflein herunterfallen!\u201c", "tokens": ["La\u00df", "ein", "Str\u00e4u\u00df\u00b7lein", "her\u00b7un\u00b7ter\u00b7fal\u00b7len", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "\u201eihr Br\u00fcder, was sollte das Str\u00e4u\u00dflein mir?", "tokens": ["\u201e", "ihr", "Br\u00fc\u00b7der", ",", "was", "soll\u00b7te", "das", "Str\u00e4u\u00df\u00b7lein", "mir", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PWS", "VMFIN", "ART", "NN", "PPER", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich hab\u2019 ja kein liebes Liebchen, wie ihr.", "tokens": ["Ich", "hab'", "ja", "kein", "lie\u00b7bes", "Lieb\u00b7chen", ",", "wie", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "ADJA", "NN", "$,", "PWAV", "PPER", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "An der Sonne w\u00fcrd\u2019 es vergehen,", "tokens": ["An", "der", "Son\u00b7ne", "w\u00fcrd'", "es", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Wind, der w\u00fcrd\u2019 es verwehen.\u201c", "tokens": ["Der", "Wind", ",", "der", "w\u00fcrd'", "es", "ver\u00b7we\u00b7hen", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Und weiter, ja weiter mit Sang und mit Klang!", "tokens": ["Und", "wei\u00b7ter", ",", "ja", "wei\u00b7ter", "mit", "Sang", "und", "mit", "Klang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und das M\u00e4gdlein lauschet und horchet noch lang.", "tokens": ["Und", "das", "M\u00e4gd\u00b7lein", "lau\u00b7schet", "und", "hor\u00b7chet", "noch", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u201eo weh! er ziehet, der Knabe,", "tokens": ["\u201e", "o", "weh", "!", "er", "zie\u00b7het", ",", "der", "Kna\u00b7be", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "PTKVZ", "$.", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den ich stille geliebet habe.", "tokens": ["Den", "ich", "stil\u00b7le", "ge\u00b7lie\u00b7bet", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "VVPP", "VAFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Da steh\u2019 ich, ach! mit der Liebe mein,", "tokens": ["Da", "steh'", "ich", ",", "ach", "!", "mit", "der", "Lie\u00b7be", "mein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ITJ", "$.", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit Rosen und mit Gelbveigelein;", "tokens": ["Mit", "Ro\u00b7sen", "und", "mit", "Gelb\u00b7vei\u00b7ge\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Dem ich Alles g\u00e4be so gerne,", "tokens": ["Dem", "ich", "Al\u00b7les", "g\u00e4\u00b7be", "so", "ger\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Der ist nun in der Ferne.\u201c", "tokens": ["Der", "ist", "nun", "in", "der", "Fer\u00b7ne", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}