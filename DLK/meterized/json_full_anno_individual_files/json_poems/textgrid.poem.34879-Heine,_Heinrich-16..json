{"textgrid.poem.34879": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kind mit gro\u00dfem K\u00fcrbiskopf,", "tokens": ["Ein", "Kind", "mit", "gro\u00b7\u00dfem", "K\u00fcr\u00b7bis\u00b7kopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hellblondem Schnurrbart, greisem Zopf,", "tokens": ["Hell\u00b7blon\u00b7dem", "Schnurr\u00b7bart", ",", "grei\u00b7sem", "Zopf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit spinnig langen, doch starken \u00c4rmchen,", "tokens": ["Mit", "spin\u00b7nig", "lan\u00b7gen", ",", "doch", "star\u00b7ken", "\u00c4rm\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit Riesenmagen, doch kurzen Ged\u00e4rmchen \u2013", "tokens": ["Mit", "Rie\u00b7sen\u00b7ma\u00b7gen", ",", "doch", "kur\u00b7zen", "Ge\u00b7d\u00e4rm\u00b7chen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "ADJA", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ein Wechselbalg, den ein Korporal,", "tokens": ["Ein", "Wech\u00b7sel\u00b7balg", ",", "den", "ein", "Kor\u00b7po\u00b7ral", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Anstatt des S\u00e4uglings, den er stahl,", "tokens": ["An\u00b7statt", "des", "S\u00e4ug\u00b7lings", ",", "den", "er", "stahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Heimlich gelegt in unsre Wiege \u2013", "tokens": ["Heim\u00b7lich", "ge\u00b7legt", "in", "uns\u00b7re", "Wie\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Die Mi\u00dfgeburt, die mit der L\u00fcge,", "tokens": ["Die", "Mi\u00df\u00b7ge\u00b7burt", ",", "die", "mit", "der", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit seinem geliebten Windspiel vielleicht,", "tokens": ["Mit", "sei\u00b7nem", "ge\u00b7lieb\u00b7ten", "Wind\u00b7spiel", "viel\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Der alte Sodomiter gezeugt \u2013", "tokens": ["Der", "al\u00b7te", "So\u00b7do\u00b7mi\u00b7ter", "ge\u00b7zeugt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Nicht brauch ich das Unget\u00fcm zu nennen \u2013", "tokens": ["Nicht", "brauch", "ich", "das", "Un\u00b7ge\u00b7t\u00fcm", "zu", "nen\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG",