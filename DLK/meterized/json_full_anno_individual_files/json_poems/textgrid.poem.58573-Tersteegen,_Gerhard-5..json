{"textgrid.poem.58573": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Zions Tochter soll als Braut noch hier auf Erden", "tokens": ["Wenn", "Zi\u00b7ons", "Toch\u00b7ter", "soll", "als", "Braut", "noch", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NN", "VMFIN", "KOUS", "NN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von allem Unflat tief und ganz gereinigt werden,", "tokens": ["Von", "al\u00b7lem", "Un\u00b7flat", "tief", "und", "ganz", "ge\u00b7rei\u00b7nigt", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ADJD", "KON", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bei\u00dft die Lauge scharf, hernach ein Feuer brennt,", "tokens": ["So", "bei\u00dft", "die", "Lau\u00b7ge", "scharf", ",", "her\u00b7nach", "ein", "Feu\u00b7er", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "$,", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis alle Schlacken sind vom reinen Gold getrennt.", "tokens": ["Bis", "al\u00b7le", "Schla\u00b7cken", "sind", "vom", "rei\u00b7nen", "Gold", "ge\u00b7trennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So kehrt der Herr dann ein ins Heiligtum der Seelen,", "tokens": ["So", "kehrt", "der", "Herr", "dann", "ein", "ins", "Hei\u00b7lig\u00b7tum", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Um seine Wohnung da auf ewig zu erw\u00e4hlen,", "tokens": ["Um", "sei\u00b7ne", "Woh\u00b7nung", "da", "auf", "e\u00b7wig", "zu", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADV", "APPR", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Seel' versammelt ist, Gott ruhet \u00fcber sie,", "tokens": ["Die", "Seel'", "ver\u00b7sam\u00b7melt", "ist", ",", "Gott", "ru\u00b7het", "\u00fc\u00b7ber", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er deckt sie Tag und Nacht, er liebt sie sp\u00e4t und fr\u00fch.", "tokens": ["Er", "deckt", "sie", "Tag", "und", "Nacht", ",", "er", "liebt", "sie", "sp\u00e4t", "und", "fr\u00fch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn Zions Tochter soll als Braut noch hier auf Erden", "tokens": ["Wenn", "Zi\u00b7ons", "Toch\u00b7ter", "soll", "als", "Braut",