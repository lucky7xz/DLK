{"dta.poem.2657": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noth-Recht de\u00df Todes/  \n Bey Beerdigung Hn.   L.   C. F. den 1. Mar-  \n tii 1674.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Entweyhet denn der Tod auch Themis heilge Stuffen/", "tokens": ["Ent\u00b7wey\u00b7het", "denn", "der", "Tod", "auch", "The\u00b7mis", "heil\u00b7ge", "Stuf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcbet ungehem\u0303t sein grausam Noth-Recht aus?", "tokens": ["Und", "\u00fc\u00b7bet", "un\u00b7ge\u00b7hem\u0303t", "sein", "grau\u00b7sam", "Noth\u00b7Recht", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VAINF", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Heist er f\u00fcr sein Gericht Astr\u00e4ens Priester ruffen/", "tokens": ["Heist", "er", "f\u00fcr", "sein", "Ge\u00b7richt", "A\u00b7str\u00e4\u00b7ens", "Pries\u00b7ter", "ruf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "NE", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schiebt Gebot und Zwang denselben in das Hau\u00df?", "tokens": ["Und", "schiebt", "Ge\u00b7bot", "und", "Zwang", "den\u00b7sel\u00b7ben", "in", "das", "Hau\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "PDAT", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So hat die Sterbligkeit wol eiserne Gesetze/", "tokens": ["So", "hat", "die", "Ster\u00b7blig\u00b7keit", "wol", "ei\u00b7ser\u00b7ne", "Ge\u00b7set\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Die auch die Tugend nicht durch l\u00e4utern linder macht;", "tokens": ["Die", "auch", "die", "Tu\u00b7gend", "nicht", "durch", "l\u00e4u\u00b7tern", "lin\u00b7der", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So sind von Stahl geschmidt der Parcen schwartze Netze/", "tokens": ["So", "sind", "von", "Stahl", "ge\u00b7schmidt", "der", "Par\u00b7cen", "schwart\u00b7ze", "Net\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "VVPP", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Libitina nimmt Frey-Briefe nicht in acht.", "tokens": ["Und", "Li\u00b7bi\u00b7ti\u00b7na", "nimmt", "Frey\u00b7Brie\u00b7fe", "nicht", "in", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "NN", "PTKNEG", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier hilfft uns keine Frist/ kein Vorstand macht uns sicher/", "tokens": ["Hier", "hilfft", "uns", "kei\u00b7ne", "Frist", "/", "kein", "Vor\u00b7stand", "macht", "uns", "si\u00b7cher", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$(", "PIAT", "NN", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kein Ausflucht bringt verschub/ der Tod verf\u00fchrt sein Recht;", "tokens": ["Kein", "Aus\u00b7flucht", "bringt", "ver\u00b7schub", "/", "der", "Tod", "ver\u00b7f\u00fchrt", "sein", "Recht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "VVFIN", "$(", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es gelten vor ihm nicht der Rechts-Gelahrten B\u00fccher/", "tokens": ["Es", "gel\u00b7ten", "vor", "ihm", "nicht", "der", "Rechts\u00b7Ge\u00b7lahr\u00b7ten", "B\u00fc\u00b7cher", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKNEG", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bewei\u00df und Zeigungen/ ja Eyde sind zu schlecht.", "tokens": ["Be\u00b7wei\u00df", "und", "Zei\u00b7gun\u00b7gen", "/", "ja", "Ey\u00b7de", "sind", "zu", "schlecht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "ADV", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Sein Urtheil ist gemein/ und wird so scharff vollzogen/", "tokens": ["Sein", "Ur\u00b7theil", "ist", "ge\u00b7mein", "/", "und", "wird", "so", "scharff", "voll\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "KON", "VAFIN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df nicht ein eintzig Mensch sich auszuschliessen wei\u00df:", "tokens": ["Da\u00df", "nicht", "ein", "eint\u00b7zig", "Mensch", "sich", "aus\u00b7zu\u00b7schlies\u00b7sen", "wei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJD", "NN", "PRF", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da wird nicht Kunst noch Witz noch Wissenschafft erwogen/", "tokens": ["Da", "wird", "nicht", "Kunst", "noch", "Witz", "noch", "Wis\u00b7sen\u00b7schafft", "er\u00b7wo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "ADV", "NN", "ADV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den Klug- und Th\u00f6richten bef\u00e4llt der Todes-Schwei\u00df.", "tokens": ["Den", "Klug", "und", "Th\u00f6\u00b7rich\u00b7ten", "be\u00b7f\u00e4llt", "der", "To\u00b7des\u00b7Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Alleine dieses kan uns noch Vergn\u00fcgen bringen/", "tokens": ["Al\u00b7lei\u00b7ne", "die\u00b7ses", "kan", "uns", "noch", "Ver\u00b7gn\u00fc\u00b7gen", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df der Gerechten Lob und Name nicht vergeht.", "tokens": ["Da\u00df", "der", "Ge\u00b7rech\u00b7ten", "Lob", "und", "Na\u00b7me", "nicht", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es hebet sie der Ruhm auff seine g\u00f6ldne Schwingen/", "tokens": ["Es", "he\u00b7bet", "sie", "der", "Ruhm", "auff", "sei\u00b7ne", "g\u00f6ld\u00b7ne", "Schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn in Vergessenheit der andern Leiche steht.", "tokens": ["Wenn", "in", "Ver\u00b7ges\u00b7sen\u00b7heit", "der", "an\u00b7dern", "Lei\u00b7che", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn ihre Wercke sind/ und ruhn in GOttes Segen/", "tokens": ["Denn", "ih\u00b7re", "Wer\u00b7cke", "sind", "/", "und", "ruhn", "in", "Got\u00b7tes", "Se\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$(", "KON", "VVFIN", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und ihre Wurtzel wird von keinem Sturm bewegt/", "tokens": ["Und", "ih\u00b7re", "Wurt\u00b7zel", "wird", "von", "kei\u00b7nem", "Sturm", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Jhr Hau\u00df ist wie ein Schlo\u00df auff einem Fels gelegen/", "tokens": ["Ihr", "Hau\u00df", "ist", "wie", "ein", "Schlo\u00df", "auff", "ei\u00b7nem", "Fels", "ge\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und gr\u00fcnen wie ein Zweig/ der Lebens-Fr\u00fcchte tr\u00e4gt.", "tokens": ["Und", "gr\u00fc\u00b7nen", "wie", "ein", "Zweig", "/", "der", "Le\u00b7bens\u00b7Fr\u00fcch\u00b7te", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ja ihr Ged\u00e4chtn\u00fc\u00df wird von Frommen stets gepriesen:", "tokens": ["Ja", "ihr", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "wird", "von", "From\u00b7men", "stets", "ge\u00b7prie\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es schallt ihr Lobgesang durch alle Theil der Welt;", "tokens": ["Es", "schallt", "ihr", "Lob\u00b7ge\u00b7sang", "durch", "al\u00b7le", "Theil", "der", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df sie sich ihrem GOtt gewehr und treu erwiesen/", "tokens": ["Da\u00df", "sie", "sich", "ih\u00b7rem", "Gott", "ge\u00b7wehr", "und", "treu", "er\u00b7wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Laster hart gestrafft/ die Bo\u00dfheit abgestellt.", "tokens": ["Die", "Las\u00b7ter", "hart", "ge\u00b7strafft", "/", "die", "Bo\u00df\u00b7heit", "ab\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$(", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df sie des Rechtens Weg den Armen nie verschrencket/", "tokens": ["Da\u00df", "sie", "des", "Rech\u00b7tens", "Weg", "den", "Ar\u00b7men", "nie", "ver\u00b7schren\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Noch den Unschuldigen in seiner Noth gedr\u00fcckt;", "tokens": ["Noch", "den", "Un\u00b7schul\u00b7di\u00b7gen", "in", "sei\u00b7ner", "Noth", "ge\u00b7dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Da\u00df arme W\u00e4isen sie/ und Witwen nie gekr\u00e4ncket/", "tokens": ["Da\u00df", "ar\u00b7me", "W\u00e4i\u00b7sen", "sie", "/", "und", "Wit\u00b7wen", "nie", "ge\u00b7kr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "$(", "KON", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und den Bedr\u00e4ngten offt mit ihrem Rath erquickt.", "tokens": ["Und", "den", "Be\u00b7dr\u00e4ng\u00b7ten", "offt", "mit", "ih\u00b7rem", "Rath", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Herr Friedrich/ der nunmehr dem Tod dingpfl\u00fcchtig worden/", "tokens": ["Herr", "Fried\u00b7rich", "/", "der", "nun\u00b7mehr", "dem", "Tod", "ding\u00b7pfl\u00fcch\u00b7tig", "wor\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "ART", "ADV", "ART", "NN", "ADJD", "VAPP", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.34": {"text": "Astr\u00e4ens grosser Ruhm/ der Rechte sondre Zier/", "tokens": ["A\u00b7str\u00e4\u00b7ens", "gros\u00b7ser", "Ruhm", "/", "der", "Rech\u00b7te", "sond\u00b7re", "Zier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wird auch mit Recht gezehlt zu der Gerechten Orden/", "tokens": ["Wird", "auch", "mit", "Recht", "ge\u00b7zehlt", "zu", "der", "Ge\u00b7rech\u00b7ten", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und stellet uns zum Trost sein herrlich Beyspiel f\u00fcr.", "tokens": ["Und", "stel\u00b7let", "uns", "zum", "Trost", "sein", "herr\u00b7lich", "Bey\u00b7spiel", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "ADJD", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Erst hat er sich bem\u00fcht den HErren zu erkennen/", "tokens": ["Erst", "hat", "er", "sich", "be\u00b7m\u00fcht", "den", "Her\u00b7ren", "zu", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Weil aller Wei\u00dfheit F\u00fcll\u2019 an dem Erk\u00e4ntn\u00fc\u00df liegt/", "tokens": ["Weil", "al\u00b7ler", "Wei\u00df\u00b7heit", "F\u00fcll'", "an", "dem", "Er\u00b7k\u00e4nt\u00b7n\u00fc\u00df", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und die Gottseligkeit ein Kleinod ist zu nennen/", "tokens": ["Und", "die", "Gott\u00b7se\u00b7lig\u00b7keit", "ein", "Klei\u00b7nod", "ist", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das aller Sch\u00e4tze Werth/ und Reichthumb \u00fcberwiegt.", "tokens": ["Das", "al\u00b7ler", "Sch\u00e4t\u00b7ze", "Werth", "/", "und", "Reicht\u00b7humb", "\u00fc\u00b7berw\u00b7iegt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "$(", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Sein eifrig Christenthum kan unv