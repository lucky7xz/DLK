{"textgrid.poem.54284": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die verstellte Lesbia", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Will man den Minen nach von ihr ein Urtheil f\u00e4llen,", "tokens": ["Will", "man", "den", "Mi\u00b7nen", "nach", "von", "ihr", "ein", "Ur\u00b7theil", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "APPR", "APPR", "PPOSAT", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So sind dieselben zwar ein Bild der Sittsamkeit,", "tokens": ["So", "sind", "die\u00b7sel\u00b7ben", "zwar", "ein", "Bild", "der", "Sitt\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDAT", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn Lesbia wei\u00df sich von aussen so zu stellen,", "tokens": ["Denn", "Les\u00b7bia", "wei\u00df", "sich", "von", "aus\u00b7sen", "so", "zu", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PRF", "APPR", "NE", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Da\u00df man in Anfang ihr so Lieb als Hertze weyht.", "tokens": ["Da\u00df", "man", "in", "An\u00b7fang", "ihr", "so", "Lieb", "als", "Hert\u00b7ze", "weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PPER", "ADV", "NN", "KOKOM", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wenn man selbge lernt ein wenig n\u00e4her kennen,", "tokens": ["Doch", "wenn", "man", "selb\u00b7ge", "lernt", "ein", "we\u00b7nig", "n\u00e4\u00b7her", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVFIN", "ART", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ihr die Masque nur vom Angesichte reist,", "tokens": ["Und", "ihr", "die", "Mas\u00b7que", "nur", "vom", "An\u00b7ge\u00b7sich\u00b7te", "reist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wird man sie mit recht ein Abentheuer nennen,", "tokens": ["So", "wird", "man", "sie", "mit", "recht", "ein", "A\u00b7bent\u00b7heu\u00b7er", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "APPR", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es ist nicht alles Gold, was sch\u00f6n und herrlich gleist.", "tokens": ["Es", "ist", "nicht", "al\u00b7les", "Gold", ",", "was", "sch\u00f6n", "und", "herr\u00b7lich", "gleist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PIAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Will man den Minen nach von ihr ein Urtheil f\u00e4llen,", "tokens": ["Will", "man", "den", "Mi\u00b7nen", "nach", "von", "ihr", "