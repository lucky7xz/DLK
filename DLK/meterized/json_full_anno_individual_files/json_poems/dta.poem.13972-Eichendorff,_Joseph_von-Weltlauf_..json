{"dta.poem.13972": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Weltlauf .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.42", "ca:0.42", "it:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So viel Gipfel als da funkeln,", "tokens": ["So", "viel", "Gip\u00b7fel", "als", "da", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KOKOM", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah'n wir abendlich verdunkeln,", "tokens": ["Sah'n", "wir", "a\u00b7bend\u00b7lich", "ver\u00b7dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es hat die alte Nacht", "tokens": ["Und", "es", "hat", "die", "al\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles wieder gleich gemacht.", "tokens": ["Al\u00b7les", "wie\u00b7der", "gleich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}