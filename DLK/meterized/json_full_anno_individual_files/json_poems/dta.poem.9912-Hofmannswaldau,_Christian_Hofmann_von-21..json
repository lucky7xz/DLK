{"dta.poem.9912": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von dem feuer/ wasser/ luft/ werd zugleich ich zubereitet/", "tokens": ["Von", "dem", "feu\u00b7er", "/", "was\u00b7ser", "/", "luft", "/", "werd", "zu\u00b7gleich", "ich", "zu\u00b7be\u00b7rei\u00b7tet", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "PWS", "$(", "VVFIN", "$(", "VAFIN", "ADV", "PPER", "VVFIN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Zu dem lieben von natur angefrischet und geleitet/", "tokens": ["Zu", "dem", "lie\u00b7ben", "von", "na\u00b7tur", "an\u00b7ge\u00b7fri\u00b7schet", "und", "ge\u00b7lei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "APPR", "NN", "VVPP", "KON", "VVPP", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Niemahl w\u00e4hr\u2019 ich lang genug/ ob ich lange schon gew\u00e4hret;", "tokens": ["Nie\u00b7mahl", "w\u00e4hr'", "ich", "lang", "ge\u00b7nug", "/", "ob", "ich", "lan\u00b7ge", "schon", "ge\u00b7w\u00e4h\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "ADV", "$(", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Den\u0303 je mehr man mich geneust/ desto mehr man mich begehret.", "tokens": ["De\u00f1", "je", "mehr", "man", "mich", "ge\u00b7neust", "/", "des\u00b7to", "mehr", "man", "mich", "be\u00