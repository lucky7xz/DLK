{"dta.poem.9707": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein gewisses hau\u00df.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die zihim wohnen hier/", "tokens": ["Die", "zi\u00b7him", "woh\u00b7nen", "hier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die ohim auch de\u00dfgleichen/", "tokens": ["Die", "o\u00b7him", "auch", "de\u00df\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADV", "$("], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Man findet da der igel ihr quartier/", "tokens": ["Man", "fin\u00b7det", "da", "der", "i\u00b7gel", "ihr", "quar\u00b7tier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00f6ret bald von drachen und von eulen", "tokens": ["Und", "h\u00f6\u00b7ret", "bald", "von", "dra\u00b7chen", "und", "von", "eu\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "KON", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Eins umb das andre heulen:", "tokens": ["Eins", "umb", "das", "and\u00b7re", "heu\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bald schwartze raben schreyen/", "tokens": ["Bald", "schwart\u00b7ze", "ra\u00b7ben", "schre\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVINF", "VVINF", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Die geyer und die weyhen/", "tokens": ["Die", "ge\u00b7yer", "und", "die", "wey\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Und was noch mehr von solcher rauber-zunfft/", "tokens": ["Und", "was", "noch", "mehr", "von", "sol\u00b7cher", "rau\u00b7ber\u00b7zunfft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die halten hier auch die zusammenkunfft.", "tokens": ["Die", "hal\u00b7ten", "hier", "auch", "die", "zu\u00b7sam\u00b7men\u00b7kunfft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Feldteuffel und dergleichen saubre geister/", "tokens": ["Feld\u00b7teuf\u00b7fel", "und", "derg\u00b7lei\u00b7chen", "saub\u00b7re", "geis\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PIS", "ADJA", "NN", "$("], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.11": {"text": "So ihnen \u00e4hnlich seyn/", "tokens": ["So", "ih\u00b7nen", "\u00e4hn\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Die finden sich mit ihrem obermeister", "tokens": ["Die", "fin\u00b7den", "sich", "mit", "ih\u00b7rem", "o\u00b7ber\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "In grosser anzahl ein.", "tokens": ["In", "gros\u00b7ser", "an\u00b7zahl", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Der kobolt legt auch seinen kram hier aus.", "tokens": ["Der", "ko\u00b7bolt", "legt", "auch", "sei\u00b7nen", "kram", "hier", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Nun rathe zu: Was ist das vor ein hau\u00df?", "tokens": ["Nun", "ra\u00b7the", "zu", ":", "Was", "ist", "das", "vor", "ein", "hau\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$.", "PWS", "VAFIN", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}