{"dta.poem.13119": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewarum \u201e, versezte sie, doch scheinen sie vergn\u00fcgt:", "tokens": ["\u201e", "wa\u00b7rum", "\u201e", ",", "ver\u00b7sez\u00b7te", "sie", ",", "doch", "schei\u00b7nen", "sie", "ver\u00b7gn\u00fcgt", ":"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$(", "$,", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201edas zeigt, da\u00df ihrem Geist nichts an dem Reichthum ligt.", "tokens": ["\u201e", "das", "zeigt", ",", "da\u00df", "ih\u00b7rem", "Geist", "nichts", "an", "dem", "Reicht\u00b7hum", "ligt", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "535\u201dDer Wissenschaften Sinn verachtet alle L\u00fcste:", "tokens": ["\"", "Der", "Wis\u00b7sen\u00b7schaf\u00b7ten", "Sinn", "ver\u00b7ach\u00b7tet", "al\u00b7le", "L\u00fcs\u00b7te", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewann er nur nicht so sehr um Nahrung sorgen m\u00fc\u00dfte.", "tokens": ["\u201e", "wann", "er", "nur", "nicht", "so", "sehr", "um", "Nah\u00b7rung", "sor\u00b7gen", "m\u00fc\u00df\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ADV", "PTKNEG", "ADV", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edas hem\u0303t der K\u00fcnste Trieb, Flei\u00df, Aufnahm, St\u00e4rck\u2019 und Macht;", "tokens": ["\u201e", "das", "hem\u0303t", "der", "K\u00fcns\u00b7te", "Trieb", ",", "Flei\u00df", ",", "Auf\u00b7nahm", ",", "St\u00e4rck", "und", "Macht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "ART", "ADJA", "NN", "$,", "NN", "$,", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ejhr Ansehn wird geschw\u00e4cht, und sie zu lezt veracht.", "tokens": ["\u201e", "jhr", "An\u00b7sehn", "wird", "ge\u00b7schw\u00e4cht", ",", "und", "sie", "zu", "lezt", "ver\u00b7acht", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$,", "KON", "PPER", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So sollte man ..... gab ich zur Antwort; aber sie", "tokens": ["So", "soll\u00b7te", "man", ".....", "gab", "ich", "zur", "Ant\u00b7wort", ";", "a\u00b7ber", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "$.", "VVFIN", "PPER", "APPRART", "NN", "$.", "KON", "PPER"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "540Ermahnte mich: \u201dVernimm, was diese sich vor M\u00fch", "tokens": ["mich", ":", "\"", "Ver\u00b7nimm", ",", "was", "die\u00b7se", "sich", "vor", "M\u00fch"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$.", "$(", "VVIMP", "$,", "PWS", "PDAT", "PRF", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "\u201evor Geist und Hoheit gibt! betrachte von den Frauen", "tokens": ["\u201e", "vor", "Geist", "und", "Ho\u00b7heit", "gibt", "!", "be\u00b7trach\u00b7te", "von", "den", "Frau\u00b7en"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "KON", "NN", "VVFIN", "$.", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edas edelste Gesicht! Ich wandte mich zu schauen,", "tokens": ["\u201e", "das", "e\u00b7dels\u00b7te", "Ge\u00b7sicht", "!", "Ich", "wand\u00b7te", "mich", "zu", "schau\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und nahme wahr, wie sie von unserm guten Rath,", "tokens": ["Und", "nah\u00b7me", "wahr", ",", "wie", "sie", "von", "un\u00b7serm", "gu\u00b7ten", "Rath", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$,", "PWAV", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich wu\u00dfte nicht um was, mit Reiz und Anmuth bat.", "tokens": ["Ich", "wu\u00df\u00b7te", "nicht", "um", "was", ",", "mit", "Reiz", "und", "An\u00b7muth", "bat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PRELS", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "545\u201dMein \u201d, sagte sie zu ihm, man mu\u00df die Kinder ehren,", "tokens": ["\"", "Mein", "\"", ",", "sag\u00b7te", "sie", "zu", "ihm", ",", "man", "mu\u00df", "die", "Kin\u00b7der", "eh\u00b7ren", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "$(", "$,", "VVFIN", "PPER", "APPR", "PPER", "$,", "PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201eund so den Eltern es, da\u00df man sie liebt, erkl\u00e4ren;", "tokens": ["\u201e", "und", "so", "den", "El\u00b7tern", "es", ",", "da\u00df", "man", "sie", "liebt", ",", "er\u00b7kl\u00e4\u00b7ren", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "KON", "ADV", "ART", "NN", "PPER", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201ewer diesen zugethan, der herzet auch das Kind,", "tokens": ["\u201e", "wer", "die\u00b7sen", "zu\u00b7ge\u00b7than", ",", "der", "her\u00b7zet", "auch", "das", "Kind", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PDAT", "ADJD", "$,", "PRELS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eweil er in seiner Lust die Lust der Eltern findt.", "tokens": ["\u201e", "weil", "er", "in", "sei\u00b7ner", "Lust", "die", "Lust", "der", "El\u00b7tern", "findt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}