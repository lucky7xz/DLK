{"textgrid.poem.38538": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6ne Erde", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn hochentz\u00fckt mein Auge sieht,", "tokens": ["Wenn", "ho\u00b7chent\u00b7z\u00fckt", "mein", "Au\u00b7ge", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sch\u00f6n die Erde Gottes bl\u00fcht,", "tokens": ["Wie", "sch\u00f6n", "die", "Er\u00b7de", "Got\u00b7tes", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie alles Wesen angeschmiegt", "tokens": ["Wie", "al\u00b7les", "We\u00b7sen", "an\u00b7ge\u00b7schmiegt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An ihren Segensbr\u00fcsten liegt;", "tokens": ["An", "ih\u00b7ren", "Se\u00b7gens\u00b7br\u00fcs\u00b7ten", "liegt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und sie, voll Mutterfreundlichkeit,", "tokens": ["Und", "sie", ",", "voll", "Mut\u00b7ter\u00b7freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich jedes ihrer Kinder freut,", "tokens": ["Sich", "je\u00b7des", "ih\u00b7rer", "Kin\u00b7der", "freut", ","], "token_info": ["