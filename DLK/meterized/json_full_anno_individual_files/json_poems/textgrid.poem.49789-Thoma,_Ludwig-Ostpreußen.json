{"textgrid.poem.49789": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ostpreu\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heimaterde,", "tokens": ["Hei\u00b7ma\u00b7ter\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.2": {"text": "Du, zerstampft von w\u00fctenden Rossen,", "tokens": ["Du", ",", "zer\u00b7stampft", "von", "w\u00fc\u00b7ten\u00b7den", "Ros\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Du, mi\u00dfhandelt, zerw\u00fchlt von Geschossen,", "tokens": ["Du", ",", "mi\u00df\u00b7han\u00b7delt", ",", "zer\u00b7w\u00fchlt", "von", "Ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVPP", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Darfst nicht ruhen.", "tokens": ["Darfst", "nicht", "ru\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Flei\u00dfige H\u00e4nde,", "tokens": ["Flei\u00b7\u00dfi\u00b7ge", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die im Ungl\u00fcck nicht feiern wollen,", "tokens": ["Die", "im", "Un\u00b7gl\u00fcck", "nicht", "fei\u00b7ern", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "--