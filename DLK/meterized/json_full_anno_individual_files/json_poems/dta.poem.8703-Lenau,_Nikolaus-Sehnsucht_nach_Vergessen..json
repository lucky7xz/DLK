{"dta.poem.8703": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht nach Vergessen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Lethe! brich die Fesseln des Ufers, gie\u00dfe", "tokens": ["Le\u00b7the", "!", "brich", "die", "Fes\u00b7seln", "des", "U\u00b7fers", ",", "gie\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$.", "ADJD", "ART", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Aus der Schattenwelt mir her\u00fcber deine", "tokens": ["Aus", "der", "Schat\u00b7ten\u00b7welt", "mir", "her\u00b7\u00fc\u00b7ber", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "PPOSAT"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Welle, da\u00df den Wunden der bangen Seel' ich", "tokens": ["Wel\u00b7le", ",", "da\u00df", "den", "Wun\u00b7den", "der", "ban\u00b7gen", "Seel'", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Trinke Genesung.", "tokens": ["Trin\u00b7ke", "Ge\u00b7ne\u00b7sung", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$."], "meter": "+-+--", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Fr\u00fchling kommt mit Duft und Gesang und Liebe,", "tokens": ["Fr\u00fch\u00b7ling", "kommt", "mit", "Duft", "und", "Ge\u00b7sang", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Will wie sonst mir sinken ans Herz, doch schl\u00e4gt ihm", "tokens": ["Will", "wie", "sonst", "mir", "sin\u00b7ken", "ans", "Herz", ",", "doch", "schl\u00e4gt", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "KOKOM", "ADV", "PPER", "VVFIN", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Nicht das Herz entgegen wie sonst; \u2014 o Lethe!", "tokens": ["Nicht", "das", "Herz", "ent\u00b7ge\u00b7gen", "wie", "sonst", ";", "o", "Le\u00b7the", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "PTKVZ", "KOKOM", "ADV", "$.", "$(", "FM", "FM", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Sende die Welle!", "tokens": ["Sen\u00b7de", "die", "Wel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}