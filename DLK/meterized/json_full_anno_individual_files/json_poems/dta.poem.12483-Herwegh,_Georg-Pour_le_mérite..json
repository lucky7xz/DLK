{"dta.poem.12483": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Pour le m\u00e9rite.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie wollen dir den Tag entfernen,", "tokens": ["Sie", "wol\u00b7len", "dir", "den", "Tag", "ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der schon so frisch am Himmel weht:", "tokens": ["Der", "schon", "so", "frisch", "am", "Him\u00b7mel", "weht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist's, was in den neuen Sternen", "tokens": ["Das", "ist's", ",", "was", "in", "den", "neu\u00b7en", "Ster\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr dich, mein Volk, geschrieben steht!", "tokens": ["F\u00fcr", "dich", ",", "mein", "Volk", ",", "ge\u00b7schrie\u00b7ben", "steht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man giebt als Futter deinen Blicken", "tokens": ["Man", "giebt", "als", "Fut\u00b7ter", "dei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KOUS", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sterne kalten, falschen Schein;", "tokens": ["Der", "Ster\u00b7ne", "kal\u00b7ten", ",", "fal\u00b7schen", "Schein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du magst sie all' zusammenflicken,", "tokens": ["Du", "magst", "sie", "all'", "zu\u00b7sam\u00b7men\u00b7fli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie werden keine Sonne sein.", "tokens": ["Sie", "wer\u00b7den", "kei\u00b7ne", "Son\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht Eine Lanze wird es brechen,", "tokens": ["Nicht", "Ei\u00b7ne", "Lan\u00b7ze", "wird", "es", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das neue, zahme Ritterthum;", "tokens": ["Das", "neu\u00b7e", ",", "zah\u00b7me", "Rit\u00b7ter\u00b7thum", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kaum wird ein S\u00e4nger f\u00fcr dich sprechen,", "tokens": ["Kaum", "wird", "ein", "S\u00e4n\u00b7ger", "f\u00fcr", "dich", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man macht ja selbst die S\u00e4nger stumm.", "tokens": ["Man", "macht", "ja", "selbst", "die", "S\u00e4n\u00b7ger", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn sie nicht drau\u00dfen Freiheit rufen,", "tokens": ["Wenn", "sie", "nicht", "drau\u00b7\u00dfen", "Frei\u00b7heit", "ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du in Galle \u00fcbersch\u00e4umst", "tokens": ["Da\u00df", "du", "in", "Gal\u00b7le", "\u00fc\u00b7bersc\u00b7h\u00e4umst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und hoch mit flammenspr\u00fch'nden Hufen", "tokens": ["Und", "hoch", "mit", "flam\u00b7men\u00b7spr\u00fch'n\u00b7den", "Hu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich gegen deine Dr\u00e4nger b\u00e4umst;", "tokens": ["Dich", "ge\u00b7gen", "dei\u00b7ne", "Dr\u00e4n\u00b7ger", "b\u00e4umst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}