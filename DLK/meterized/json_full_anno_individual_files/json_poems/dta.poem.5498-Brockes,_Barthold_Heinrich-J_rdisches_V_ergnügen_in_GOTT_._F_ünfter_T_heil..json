{"dta.poem.5498": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das hundert tausendmahl an Gr\u00f6sse den Erd-Kreis selber", "tokens": ["Das", "hun\u00b7dert", "tau\u00b7send\u00b7mahl", "an", "Gr\u00f6s\u00b7se", "den", "Erd\u00b7Kreis", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "ADV", "APPR", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00fcbersteiget!", "tokens": ["\u00fc\u00b7bers\u00b7tei\u00b7get", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wann aber dieses Ueberlegen mir im Gesch\u00f6pf den Sch\u00f6pfer", "tokens": ["Wann", "a\u00b7ber", "die\u00b7ses", "Ue\u00b7ber\u00b7le\u00b7gen", "mir", "im", "Ge\u00b7sch\u00f6pf", "den", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PDAT", "NN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "zeiget;", "tokens": ["zei\u00b7get", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "So deucht mich, da\u00df mir gegen ihn die grosse Sonne so", "tokens": ["So", "deucht", "mich", ",", "da\u00df", "mir", "ge\u00b7gen", "ihn", "die", "gros\u00b7se", "Son\u00b7ne", "so"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "verkleint,", "tokens": ["ver\u00b7kleint", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Als dieses Sonnen-Bild im Tr\u00f6pfgen, ja noch unendlich", "tokens": ["Als", "die\u00b7ses", "Son\u00b7nen\u00b7Bild", "im", "Tr\u00f6pf\u00b7gen", ",", "ja", "noch", "un\u00b7end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "APPRART", "NN", "$,", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.8": {"text": "kleiner, scheint.", "tokens": ["klei\u00b7ner", ",", "scheint", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}