{"dta.poem.7037": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aufmunterung zum Lobe GOTTES,  \n beym Genu\u00df allerley Erstlinge  \n von Fr\u00fcchten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich anjetzt in diesem Jahre", "tokens": ["Da", "ich", "an\u00b7jetzt", "in", "die\u00b7sem", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der reiffenKirschen\nErbsen\nBohnen\nAepffel\nPfirschen\nTrauben Erstling' esse,", "tokens": ["Der", "reif\u00b7fen", "Kir\u00b7schen", "Erb\u00b7sen", "Boh\u00b7nen", "A\u00b7e\u00b7pf\u00b7fel", "Pfir\u00b7schen", "Trau\u00b7ben", "Erst\u00b7ling'", "es\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "NN", "NN", "ADJA", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "So gieb, O! liebster GOTT, da\u00df ich, mit Danck, ermesse", "tokens": ["So", "gieb", ",", "O", "!", "liebs\u00b7ter", "GoTT", ",", "da\u00df", "ich", ",", "mit", "Danck", ",", "er\u00b7mes\u00b7se"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVIMP", "$,", "ITJ", "$.", "ADJA", "NN", "$,", "KOUS", "PPER", "$,", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Huld, die ich darin durch Dich aufs Neu erfahre!", "tokens": ["Die", "Huld", ",", "die", "ich", "da\u00b7rin", "durch", "Dich", "aufs", "Neu", "er\u00b7fah\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PAV", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da mein vergn\u00fcgter Gaum, Zung, Magen und Gebl\u00fchte,", "tokens": ["Da", "mein", "ver\u00b7gn\u00fcg\u00b7ter", "Gaum", ",", "Zung", ",", "Ma\u00b7gen", "und", "Ge\u00b7bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erfrischt, getr\u00e4nckt, gen\u00e4hrt,", "tokens": ["Er\u00b7frischt", ",", "ge\u00b7tr\u00e4nckt", ",", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vergn\u00fcgt, erquickt, vermehrt,", "tokens": ["Ver\u00b7gn\u00fcgt", ",", "er\u00b7quickt", ",", "ver\u00b7mehrt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Gest\u00e4rckt und rege wird; Auf! auf! o mein Gem\u00fchte,", "tokens": ["Ge\u00b7st\u00e4rckt", "und", "re\u00b7ge", "wird", ";", "Auf", "!", "auf", "!", "o", "mein", "Ge\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJA", "VAFIN", "$.", "APPR", "$.", "PTKVZ", "$.", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "La\u00df nicht den Leib allein", "tokens": ["La\u00df", "nicht", "den", "Leib", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Dadurch vergn\u00fcgt, ergetzt,", "tokens": ["Da\u00b7durch", "ver\u00b7gn\u00fcgt", ",", "er\u00b7getzt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Erfrischt, getr\u00e4nckt, gen\u00e4hret seyn!", "tokens": ["Er\u00b7frischt", ",", "ge\u00b7tr\u00e4nckt", ",", "ge\u00b7n\u00e4h\u00b7ret", "seyn", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sey Seele, wenn der Leib der Fr\u00fcchte Zucker sp\u00fcret,", "tokens": ["Sey", "See\u00b7le", ",", "wenn", "der", "Leib", "der", "Fr\u00fcch\u00b7te", "Zu\u00b7cker", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch auch ger\u00fchret,", "tokens": ["Doch", "auch", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Und dencke doch an Den, durch Dessen weise Macht", "tokens": ["Und", "den\u00b7cke", "doch", "an", "Den", ",", "durch", "Des\u00b7sen", "wei\u00b7se", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NE", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Feld-Baum- und H\u00fclsen-Frucht, f\u00fcr dich hervor gebracht,", "tokens": ["Feld\u00b7Baum", "und", "H\u00fcl\u00b7sen\u00b7Frucht", ",", "f\u00fcr", "dich", "her\u00b7vor", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$,", "APPR", "PPER", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gewachsen und gereifft! Durch den der Nahrungs-Safft", "tokens": ["Ge\u00b7wach\u00b7sen", "und", "ge\u00b7reifft", "!", "Durch", "den", "der", "Nah\u00b7rungs\u00b7\u00b7S\u00b7afft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVPP", "$.", "APPR", "ART", "ART", "NN"], "meter": "-+-+--+--++-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Durch den der Wachsthums-Trieb den B\u00e4umen eingesenckt,", "tokens": ["Durch", "den", "der", "Wachst\u00b7hums\u00b7Trieb", "den", "B\u00e4u\u00b7men", "ein\u00b7ge\u00b7senckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der sie mit W\u00e4rme n\u00e4hrt, der sie mit Regen tr\u00e4nckt,", "tokens": ["Der", "sie", "mit", "W\u00e4r\u00b7me", "n\u00e4hrt", ",", "der", "sie", "mit", "Re\u00b7gen", "tr\u00e4nckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durch den des Saamens Geist mit so geheimer Krafft", "tokens": ["Durch", "den", "des", "Saa\u00b7mens", "Geist", "mit", "so", "ge\u00b7hei\u00b7mer", "Krafft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Begabet und versehen! Es hat zu aller Zeit", "tokens": ["Be\u00b7ga\u00b7bet", "und", "ver\u00b7se\u00b7hen", "!", "Es", "hat", "zu", "al\u00b7ler", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVINF", "$.", "PPER", "VAFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Die Menschheit eine Art von Danckbarkeit,", "tokens": ["Die", "Menschheit", "ei\u00b7ne", "Art", "von", "Dan\u00b7ck\u00b7bar\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Der GOttheit, im Genu\u00df der ersten Frucht, gewiesen.", "tokens": ["Der", "Got\u00b7theit", ",", "im", "Ge\u00b7nu\u00df", "der", "ers\u00b7ten", "Frucht", ",", "ge\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Durch Opfer haben sie den Geber stets gepriesen:", "tokens": ["Durch", "Op\u00b7fer", "ha\u00b7ben", "sie", "den", "Ge\u00b7ber", "stets", "ge\u00b7prie\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Heyden thaten es. So ist es noch der Brauch", "tokens": ["Die", "Hey\u00b7den", "tha\u00b7ten", "es", ".", "So", "ist", "es", "noch", "der", "Brauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$.", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bey unsern Juden auch.", "tokens": ["Bey", "un\u00b7sern", "Ju\u00b7den", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Bey allen Erstlingen zum Sch\u00f6pfer sich zu lencken,", "tokens": ["Bey", "al\u00b7len", "Erst\u00b7lin\u00b7gen", "zum", "Sch\u00f6p\u00b7fer", "sich", "zu", "len\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Und bey der neuen Gab des Gebers zu gedencken,", "tokens": ["Und", "bey", "der", "neu\u00b7en", "Gab", "des", "Ge\u00b7bers", "zu", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wie! wollen denn die Christen gantz allein,", "tokens": ["Wie", "!", "wol\u00b7len", "denn", "die", "Chris\u00b7ten", "gantz", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "ADV", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "So unerk\u00e4nntlich seyn?", "tokens": ["So", "un\u00b7er\u00b7k\u00e4nnt\u00b7lich", "seyn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Und nicht einmal (der Opfer zu geschweigen)", "tokens": ["Und", "nicht", "ein\u00b7mal", "(", "der", "Op\u00b7fer", "zu", "ge\u00b7schwei\u00b7gen", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "GoTT ein\u2019 Erkentlichkeit, nur im Gedancken zeigen?", "tokens": ["GoTT", "ein'", "Er\u00b7kent\u00b7lich\u00b7keit", ",", "nur", "im", "Ge\u00b7dan\u00b7cken", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ach GOTT! du Urquell aller Gaben,", "tokens": ["Ach", "GoTT", "!", "du", "Ur\u00b7quell", "al\u00b7ler", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Du Weiser Vater, Du, aus Dessen Allmachts-Hand", "tokens": ["Du", "Wei\u00b7ser", "Va\u00b7ter", ",", "Du", ",", "aus", "Des\u00b7sen", "All\u00b7machts\u00b7Hand"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So manche Frucht, an welcher wir uns laben,", "tokens": ["So", "man\u00b7che", "Frucht", ",", "an", "wel\u00b7cher", "wir", "uns", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Uns dargereichet wird, ach! sch\u00e4rffe den Verstand!", "tokens": ["Uns", "dar\u00b7ge\u00b7rei\u00b7chet", "wird", ",", "ach", "!", "sch\u00e4rf\u00b7fe", "den", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAFIN", "$,", "ITJ", "$.", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Gieb da\u00df in Deinen Wunderwercken", "tokens": ["Gieb", "da\u00df", "in", "Dei\u00b7nen", "Wun\u00b7der\u00b7wer\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Wir Deine Weisheit, Macht und Vater-Liebe mercken!", "tokens": ["Wir", "Dei\u00b7ne", "Weis\u00b7heit", ",", "Macht", "und", "Va\u00b7ter\u00b7Lie\u00b7be", "mer\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "La\u00df uns, so offt wir Erstling' essen,", "tokens": ["La\u00df", "uns", ",", "so", "offt", "wir", "Erst\u00b7ling'", "es\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADV", "ADV", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.39": {"text": "Doch Dein, o Vater, nicht vergessen!", "tokens": ["Doch", "Dein", ",", "o", "Va\u00b7ter", ",", "nicht", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "$,", "FM", "NN", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "La\u00df ein vergn\u00fcgt GOtt Lob, das wir f\u00fcr Freuden lallen,", "tokens": ["La\u00df", "ein", "ver\u00b7gn\u00fcgt", "Gott", "Lob", ",", "das", "wir", "f\u00fcr", "Freu\u00b7den", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "VVFIN", "NN", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Aus Gnaden Dir gefallen!", "tokens": ["Aus", "Gna\u00b7den", "Dir", "ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "So wirst Du, HERR, in diesem Leben,", "tokens": ["So", "wirst", "Du", ",", "HeRR", ",", "in", "die\u00b7sem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Noch \u00f6ffters Erstlinge, wo es uns n\u00fctzlich geben.", "tokens": ["Noch", "\u00f6ff\u00b7ters", "Erst\u00b7lin\u00b7ge", ",", "wo", "es", "uns", "n\u00fctz\u00b7lich", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}