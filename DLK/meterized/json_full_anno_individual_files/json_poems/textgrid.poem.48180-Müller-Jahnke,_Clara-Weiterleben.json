{"textgrid.poem.48180": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Weiterleben", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht, da\u00df du ihm ein pr\u00e4chtig Denkmal baust,", "tokens": ["Nicht", ",", "da\u00df", "du", "ihm", "ein", "pr\u00e4ch\u00b7tig", "Denk\u00b7mal", "baust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "PPER", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "mit tausend Tr\u00e4nen seine Gruft betaust,", "tokens": ["mit", "tau\u00b7send", "Tr\u00e4\u00b7nen", "sei\u00b7ne", "Gruft", "be\u00b7taust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und heimlich hoffst, da\u00df euch der Tod vereint,", "tokens": ["und", "heim\u00b7lich", "hoffst", ",", "da\u00df", "euch", "der", "Tod", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "nicht dadurch ehrst du den gestorbnen Freund.", "tokens": ["nicht", "da\u00b7durch", "ehrst", "du", "den", "ge\u00b7storb\u00b7nen", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Wenn du das Werk, das ihm nicht mehr gelang,", "tokens": ["Wenn", "du", "das", "Werk", ",", "das",