{"textgrid.poem.32549": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da wohl kein Menschenkind die Lunge", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da wohl kein Menschenkind die Lunge", "tokens": ["Da", "wohl", "kein", "Men\u00b7schen\u00b7kind", "die", "Lun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu seiner Nebenchristen Ruhm", "tokens": ["Zu", "sei\u00b7ner", "Ne\u00b7benc\u00b7hris\u00b7ten", "Ruhm"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Je \u00fcberhitzt, so springt man drum", "tokens": ["Je", "\u00fc\u00b7berh\u00b7itzt", ",", "so", "springt", "man", "drum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVFIN", "PIS", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Engeln selbst nicht besser um,", "tokens": ["Mit", "En\u00b7geln", "selbst", "nicht", "bes\u00b7ser", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und spricht: es geh' von Satans Zunge", "tokens": ["Und", "spricht", ":", "es", "geh'", "von", "Sa\u00b7tans", "Zun\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein wahres Wort, doch ich will zu der Wahrheit Ruhm", "tokens": ["Kein", "wah\u00b7res", "Wort", ",", "doch", "ich", "will", "zu", "der", "Wahr\u00b7heit", "Ruhm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "KON", "PPER", "VMFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Durch folgende Geschichte zeigen,", "tokens": ["Durch", "fol\u00b7gen\u00b7de", "Ge\u00b7schich\u00b7te", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie sey auch selbst den Teufeln eigen.", "tokens": ["Sie", "sey", "auch", "selbst", "den", "Teu\u00b7feln", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Himmel, der die Ehen schlie\u00dft,", "tokens": ["Der", "Him\u00b7mel", ",", "der", "die", "E\u00b7hen", "schlie\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gab Carveln einst Trotz seiner grauen Haare,", "tokens": ["Gab", "Car\u00b7veln", "einst", "Trotz", "sei\u00b7ner", "grau\u00b7en", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und seiner h\u00f6chst verlegnen Waare", "tokens": ["Und", "sei\u00b7ner", "h\u00f6chst", "ver\u00b7leg\u00b7nen", "Waa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Einfall ein, der oft beym J\u00fcngling mislich ist,", "tokens": ["Den", "Ein\u00b7fall", "ein", ",", "der", "oft", "beym", "J\u00fcng\u00b7ling", "mis\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "ADV", "APPRART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein junges Weib, das seines Durstes sich zu sch\u00e4men,", "tokens": ["Ein", "jun\u00b7ges", "Weib", ",", "das", "sei\u00b7nes", "Durs\u00b7tes", "sich", "zu", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gar nicht gesonnen war, zu nehmen.", "tokens": ["Gar", "nicht", "ge\u00b7son\u00b7nen", "war", ",", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "VAFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zwar hoft er ganz getrost sein K\u00e4tchen w\u00fcrde sich", "tokens": ["Zwar", "hoft", "er", "ganz", "ge\u00b7trost", "sein", "K\u00e4t\u00b7chen", "w\u00fcr\u00b7de", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "VAFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus treuer Z\u00e4rtlichkeit zum Fastentisch bequemen,", "tokens": ["Aus", "treu\u00b7er", "Z\u00e4rt\u00b7lich\u00b7keit", "zum", "Fas\u00b7ten\u00b7tisch", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch statt des W\u00f6rtchens ", "tokens": ["Doch", "statt", "des", "W\u00f6rt\u00b7chens"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Das vor der Stirn ihm stand, stand zu Hans Carvels Jammer", "tokens": ["Das", "vor", "der", "Stirn", "ihm", "stand", ",", "stand", "zu", "Hans", "Car\u00b7vels", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "NE", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein andres Wort vor K\u00e4tchens Herzenskammer;", "tokens": ["Ein", "and\u00b7res", "Wort", "vor", "K\u00e4t\u00b7chens", "Her\u00b7zen\u00b7skam\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und Carvel sann drum Tag und Nacht", "tokens": ["Und", "Car\u00b7vel", "sann", "drum", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PAV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Auf Mittel um sein Haupt f\u00fcr Ungl\u00fcck zu beh\u00fcten:", "tokens": ["Auf", "Mit\u00b7tel", "um", "sein", "Haupt", "f\u00fcr", "Un\u00b7gl\u00fcck", "zu", "be\u00b7h\u00fc\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Allein stets zog er Rathhaus Nieten,", "tokens": ["Al\u00b7lein", "stets", "zog", "er", "Rath\u00b7haus", "Nie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und selbst ein kleiner Rausch, der Herzen freudig macht", "tokens": ["Und", "selbst", "ein", "klei\u00b7ner", "Rausch", ",", "der", "Her\u00b7zen", "freu\u00b7dig", "macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Half Carveln nicht. Um K\u00e4tchens Fleisch zu qu\u00e4len,", "tokens": ["Half", "Car\u00b7veln", "nicht", ".", "Um", "K\u00e4t\u00b7chens", "Fleisch", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKNEG", "$.", "APPR", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und zur Erbauung ihrer Seelen", "tokens": ["Und", "zur", "Er\u00b7bau\u00b7ung", "ih\u00b7rer", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Lie\u00df ers indessen nicht an guten Lehren fehlen,", "tokens": ["Lie\u00df", "ers", "in\u00b7des\u00b7sen", "nicht", "an", "gu\u00b7ten", "Leh\u00b7ren", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch da er blo\u00df die k\u00fcnftgen Gaben prie\u00df,", "tokens": ["Doch", "da", "er", "blo\u00df", "die", "k\u00fcnft\u00b7gen", "Ga\u00b7ben", "prie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und gar kein zeitlich Pr\u00f6bchen wie\u00df;", "tokens": ["Und", "gar", "kein", "zeit\u00b7lich", "Pr\u00f6b\u00b7chen", "wie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So ward durch die Gardienenpredigt", "tokens": ["So", "ward", "durch", "die", "Gar\u00b7die\u00b7nen\u00b7pre\u00b7digt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.22": {"text": "Sie nicht erbaut, und er nicht seiner Angst entledigt.", "tokens": ["Sie", "nicht", "er\u00b7baut", ",", "und", "er", "nicht", "sei\u00b7ner", "Angst", "ent\u00b7le\u00b7digt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "$,", "KON", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sein Leben war nunmehr Ein b\u00f6ser Traum,", "tokens": ["Sein", "Le\u00b7ben", "war", "nun\u00b7mehr", "Ein", "b\u00f6\u00b7ser", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Selbst wenn er K\u00e4tchen sah so glaubt er kaum \u2013", "tokens": ["Selbst", "wenn", "er", "K\u00e4t\u00b7chen", "sah", "so", "glaubt", "er", "kaum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NE", "VVFIN", "ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und Thomas, der auch ehr nicht glaubte", "tokens": ["Und", "Tho\u00b7mas", ",", "der", "auch", "ehr", "nicht", "glaub\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "PRELS", "ADV", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Bis seines Meisters himmlische Geduld", "tokens": ["Bis", "sei\u00b7nes", "Meis\u00b7ters", "himm\u00b7li\u00b7sche", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ihm eine Wundenprob erlaubte,", "tokens": ["Ihm", "ei\u00b7ne", "Wun\u00b7den\u00b7prob", "er\u00b7laub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Das war sein Mann. Ganz ohne K\u00e4tchens Schuld,", "tokens": ["Das", "war", "sein", "Mann", ".", "Ganz", "oh\u00b7ne", "K\u00e4t\u00b7chens", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Die nie ihn weckte, denn wozu w\u00e4rs n\u00fctz gewesen,", "tokens": ["Die", "nie", "ihn", "weck\u00b7te", ",", "denn", "wo\u00b7zu", "w\u00e4rs", "n\u00fctz", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$,", "KON", "PWAV", "VAFIN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wer kann von Dornen Trauben lesen?", "tokens": ["Wer", "kann", "von", "Dor\u00b7nen", "Trau\u00b7ben", "le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Schlief er nie fest \u2013 Als er nun einst so schlief,", "tokens": ["Schlief", "er", "nie", "fest", "\u2013", "Als", "er", "nun", "einst", "so", "schlief", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$(", "KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.32": {"text": "D\u00fcnkts ihm, da\u00df Asmodi ihn rief,", "tokens": ["D\u00fcnkts", "ihm", ",", "da\u00df", "As\u00b7mo\u00b7di", "ihn", "rief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "KOUS", "NE", "PPER", "VVFIN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.33": {"text": "Und sprach: \u00bbnimm diesen Ring, so lange", "tokens": ["Und", "sprach", ":", "\u00bb", "nimm", "die\u00b7sen", "Ring", ",", "so", "lan\u00b7ge"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "VVIMP", "PDAT", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Er dir am Finger sticht,", "tokens": ["Er", "dir", "am", "Fin\u00b7ger", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Sey, Carvel, dir nicht bange,", "tokens": ["Sey", ",", "Car\u00b7vel", ",", "dir", "nicht", "ban\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NE", "$,", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Da\u00df man in deinen Garten bricht\u00ab \u2013", "tokens": ["Da\u00df", "man", "in", "dei\u00b7nen", "Gar\u00b7ten", "bricht", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "O Gott bezahl es dir! schrie hier der gute Alte;", "tokens": ["O", "Gott", "be\u00b7zahl", "es", "dir", "!", "schrie", "hier", "der", "gu\u00b7te", "Al\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "PPER", "$.", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So werd ich denn wenn ich den Ring behalte,", "tokens": ["So", "werd", "ich", "denn", "wenn", "ich", "den", "Ring", "be\u00b7hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Doch wieder meines Lebens froh \u2013", "tokens": ["Doch", "wie\u00b7der", "mei\u00b7nes", "Le\u00b7bens", "froh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Und als er dies so eifrig dachte,", "tokens": ["Und", "als", "er", "dies", "so", "eif\u00b7rig", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Da\u00df er entz\u00fcckt davon erwachte,", "tokens": ["Da\u00df", "er", "ent\u00b7z\u00fcckt", "da\u00b7von", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Da stach sein Finger \u2013 Rathet wo?", "tokens": ["Da", "stach", "sein", "Fin\u00b7ger", "\u2013", "Ra\u00b7thet", "wo", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "NN", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da wohl kein Menschenkind die Lunge", "tokens": ["Da", "wohl", "kein", "Men\u00b7schen\u00b7kind", "die", "Lun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu seiner Nebenchristen Ruhm", "tokens": ["Zu", "sei\u00b7ner", "Ne\u00b7benc\u00b7hris\u00b7ten", "Ruhm"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Je \u00fcberhitzt, so springt man drum", "tokens": ["Je", "\u00fc\u00b7berh\u00b7itzt", ",", "so", "springt", "man", "drum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVFIN", "PIS", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Engeln selbst nicht besser um,", "tokens": ["Mit", "En\u00b7geln", "selbst", "nicht", "bes\u00b7ser", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKNEG", "ADJD", "PTKVZ", "$,"], "meter": "-+