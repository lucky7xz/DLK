"}, "line.18": {"text": "Lie\u00df ers indessen nicht an guten Lehren fehlen,", "tokens": ["Lie\u00df", "ers", "in\u00b7des\u00b7sen", "nicht", "an", "gu\u00b7ten", "Leh\u00b7ren", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch da er blo\u00df die k\u00fcnftgen Gaben prie\u00df,", "tokens": ["Doch", "da", "er", "blo\u00df", "die", "k\u00fcnft\u00b7gen", "Ga\u00b7ben", "prie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und gar kein zeitlich Pr\u00f6bchen wie\u00df;", "tokens": ["Und", "gar", "kein", "zeit\u00b7lich", "Pr\u00f6b\u00b7chen", "wie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "So ward durch die Gardienenpredigt", "tokens": ["So", "ward", "durch", "die", "Gar\u00b7die\u00b7nen\u00b7pre\u00b7digt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.22": {"text": "Sie nicht erbaut, und er nicht seiner Angst entledigt.", "tokens": ["Sie", "nicht", "er\u00b7baut", ",", "und", "er", "nicht", "sei\u00b7ner", "Angst", "ent\u00b7le\u00b7digt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "$,", "KON", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Sein Leben war nunmehr Ein b\u00f6ser Traum,", "tokens": ["Sein", "Le\u00b7ben", "war", "nun\u00b7mehr", "Ein", "b\u00f6\u00b7ser", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Selbst wenn er K\u00e4tchen sah so glaubt er kaum \u2013", "tokens": ["Selbst", "wenn", "er", "K\u00e4t\u00b7chen", "sah", "so", "glaubt", "er", "kaum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NE", "VVFIN", "ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und Thomas, der auch ehr nicht glaubte", "tokens": ["Und", "Tho\u00b7mas", ",", "der", "auch", "ehr", "nicht", "glaub\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "PRELS", "ADV", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Bis seines Meisters himmlische Geduld", "tokens": ["Bis", "sei\u00b7nes", "Meis\u00b7ters", "himm\u00b7li\u00b7sche", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ihm eine Wundenprob erlaubte,", "tokens": ["Ihm", "ei\u00b7ne", "Wun\u00b7den\u00b7prob", "er\u00b7laub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Das war sein Mann. Ganz ohne K\u00e4tchens Schuld,", "tokens": ["Das", "war", "sein", "Mann", ".", "Ganz", "oh\u00b7ne", "K\u00e4t\u00b7chens", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOS