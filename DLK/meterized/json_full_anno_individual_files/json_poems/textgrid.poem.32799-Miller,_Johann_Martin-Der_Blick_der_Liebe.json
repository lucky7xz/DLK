{"textgrid.poem.32799": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Der Blick der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War das nicht ein Blick der Liebe,", "tokens": ["War", "das", "nicht", "ein", "Blick", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der aus ihrem Auge sprach?", "tokens": ["Der", "aus", "ih\u00b7rem", "Au\u00b7ge", "sprach", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah es nicht bethr\u00e4nt, und tr\u00fcbe", "tokens": ["Sah", "es", "nicht", "be\u00b7thr\u00e4nt", ",", "und", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVPP", "$,", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir mit stiller Sehnsucht nach?", "tokens": ["Mir", "mit", "stil\u00b7ler", "Sehn\u00b7sucht", "nach", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja, bei Gott! Sie mu\u00df es wissen,", "tokens": ["Ja", ",", "bei", "Gott", "!", "Sie", "mu\u00df", "es", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "NN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich so verwundet bin;", "tokens": ["Da\u00df", "ich", "so", "ver\u00b7wun\u00b7det", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df, von Mitleid hingerissen,", "tokens": ["Mu\u00df", ",", "von", "Mit\u00b7leid", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch f\u00fcr mich im stillen gl\u00fchn! \u2013", "tokens": ["Auch", "f\u00fcr", "mich", "im", "stil\u00b7len", "gl\u00fchn", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPER", "APPRART", "ADJA", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O ihr Liebesengel, r\u00fchret", "tokens": ["O", "ihr", "Lie\u00b7be\u00b7sen\u00b7gel", ",", "r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch das Flehn des Leidenden,", "tokens": ["Euch", "das", "Flehn", "des", "Lei\u00b7den\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "O so steigt herab und f\u00fchret", "tokens": ["O", "so", "steigt", "her\u00b7ab", "und", "f\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "ADV", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich zu meiner Heiligen!", "tokens": ["Mich", "zu", "mei\u00b7ner", "Hei\u00b7li\u00b7gen", "!"], "token_info": ["word", "word", "word",