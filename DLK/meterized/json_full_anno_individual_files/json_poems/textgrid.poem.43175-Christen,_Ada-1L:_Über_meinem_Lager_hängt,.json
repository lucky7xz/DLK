{"textgrid.poem.43175": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00dcber meinem Lager h\u00e4ngt,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber meinem Lager h\u00e4ngt,", "tokens": ["\u00dc\u00b7ber", "mei\u00b7nem", "La\u00b7ger", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welk, bestaubt und abgestorben,", "tokens": ["Welk", ",", "be\u00b7staubt", "und", "ab\u00b7ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein beflorter Lorbeerkranz", "tokens": ["Ein", "be\u00b7flor\u00b7ter", "Lor\u00b7beer\u00b7kranz"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Neben Myrthen, l\u00e4ngst verdorben.", "tokens": ["Ne\u00b7ben", "Myr\u00b7then", ",", "l\u00e4ngst", "ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und in meinem Fiebertraum", "tokens": ["Und", "in", "mei\u00b7nem", "Fie\u00b7ber\u00b7traum"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaute ich sie wieder bl\u00fchen \u2013", "tokens": ["Schau\u00b7te", "ich", "sie", "wie\u00b7der", "bl\u00fc\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mich selber jugendfreudig", "tokens": ["Und", "mich", "sel\u00b7ber", "ju\u00b7gend\u00b7freu\u00b7dig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter ihrem Duft ergl\u00fchen.", "tokens": ["Un\u00b7ter", "ih\u00b7rem", "Duft", "er\u00b7gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber ach, das Fieber schwand.", "tokens": ["A\u00b7ber", "ach", ",", "das", "Fie\u00b7ber", "schwand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welk, so wie mein eig'nes Leben,", "tokens": ["Welk", ",", "so", "wie", "mein", "eig'\u00b7nes", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "KOKOM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaue ich die Kr\u00e4nze dort", "tokens": ["Schau\u00b7e", "ich", "die", "Kr\u00e4n\u00b7ze", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur an d\u00fcnnen F\u00e4den schweben.", "tokens": ["Nur", "an", "d\u00fcn\u00b7nen", "F\u00e4\u00b7den", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00dcber meinem Lager h\u00e4ngt,", "tokens": ["\u00dc\u00b7ber", "mei\u00b