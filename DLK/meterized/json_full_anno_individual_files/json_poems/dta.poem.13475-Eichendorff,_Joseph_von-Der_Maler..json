{"dta.poem.13475": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Maler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Wolken, eh' im n\u00e4cht'gen Land", "tokens": ["Aus", "Wol\u00b7ken", ",", "eh'", "im", "n\u00e4cht'\u00b7gen", "Land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erwacht die Kreaturen,", "tokens": ["Er\u00b7wacht", "die", "Kre\u00b7a\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Langt Gottes Hand,", "tokens": ["Langt", "Got\u00b7tes", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Zieht durch die stillen Fluren", "tokens": ["Zieht", "durch", "die", "stil\u00b7len", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gewaltig die Konturen,", "tokens": ["Ge\u00b7wal\u00b7tig", "die", "Kon\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Strom, Wald und Felsenwand.", "tokens": ["Strom", ",", "Wald", "und", "Fel\u00b7sen\u00b7wand", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wach' auf, wach' auf! die Lerche ruft,", "tokens": ["Wach'", "auf", ",", "wach'", "auf", "!", "die", "Ler\u00b7che", "ruft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aurora taucht die Strahlen", "tokens": ["Au\u00b7ro\u00b7ra", "taucht", "die", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vertr\u00e4umt in Duft,", "tokens": ["Ver\u00b7tr\u00e4umt", "in", "Duft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Beginnt auf Berg und Thalen,", "tokens": ["Be\u00b7ginnt", "auf", "Berg", "und", "Tha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ringsum ein himmlisch Malen", "tokens": ["Ring\u00b7sum", "ein", "himm\u00b7lisch", "Ma\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "In Meer und Land und Luft.", "tokens": ["In", "Meer", "und", "Land", "und", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und durch die Stille, Lichtgeschm\u00fcckt,", "tokens": ["Und", "durch", "die", "Stil\u00b7le", ",", "Licht\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus wunderbaren Locken", "tokens": ["Aus", "wun\u00b7der\u00b7ba\u00b7ren", "Lo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Engel blickt. \u2014", "tokens": ["Ein", "En\u00b7gel", "blickt", "."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Da rauscht der Wald erschrocken,", "tokens": ["Da", "rauscht", "der", "Wald", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da gehn die Morgenglocken,", "tokens": ["Da", "gehn", "die", "Mor\u00b7gen\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Gipfel stehn verz\u00fcckt.", "tokens": ["Die", "Gip\u00b7fel", "stehn", "ver\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O lichte Augen, ernst und mild,", "tokens": ["O", "lich\u00b7te", "Au\u00b7gen", ",", "ernst", "und", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kann nicht von euch lassen!", "tokens": ["Ich", "kann", "nicht", "von", "euch", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bald wieder wild", "tokens": ["Bald", "wie\u00b7der", "wild"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "St\u00fcrmt's her von Sorg' und Hassen \u2014", "tokens": ["St\u00fcrmt's", "her", "von", "Sor\u00b7g'", "und", "Has\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "APPR", "NN", "KON", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Durch die verworrnen Gassen", "tokens": ["Durch", "die", "ver\u00b7worr\u00b7nen", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fchr mich, mein g\u00f6ttlich Bild!", "tokens": ["F\u00fchr", "mich", ",", "mein", "g\u00f6tt\u00b7lich", "Bild", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}