{"textgrid.poem.43979": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich verschmachte vor Verlangen,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich verschmachte vor Verlangen,", "tokens": ["Ich", "ver\u00b7schmach\u00b7te", "vor", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Phillis zu umfangen.", "tokens": ["Mei\u00b7ne", "Phil\u00b7lis", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Harter Himmel, z\u00fcrnst du noch?", "tokens": ["Har\u00b7ter", "Him\u00b7mel", ",", "z\u00fcrnst", "du", "noch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Faule Stunden, eilet doch!", "tokens": ["Fau\u00b7le", "Stun\u00b7den", ",", "ei\u00b7let", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eilet doch, ihr faulen Stunden,", "tokens": ["Ei\u00b7let", "doch", ",", "ihr", "fau\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erbarmt euch meiner Noth!", "tokens": ["Und", "er\u00b7barmt", "euch", "mei\u00b7ner", "Noth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird der Ri\u00df nicht bald verbunden,", "tokens": ["Wird", "der", "Ri\u00df", "nicht", "bald", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blutet sich mein Herze todt.", "tokens": ["Blu\u00b7tet", "sich", "mein", "Her\u00b7ze", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebste Seele, las dich finden!", "tokens": ["Liebs\u00b7te", "See\u00b7le", ",", "las", "dich", "fin\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich spaziere durch die Linden,", "tokens": ["Ich", "spa\u00b7zie\u00b7re", "durch", "die", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Durch die Th\u00e4ler, durch den Hayn", "tokens": ["Durch", "die", "Th\u00e4\u00b7ler", ",", "durch", "den", "Hayn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In Begleitung s\u00fc\u00dfer Pein;", "tokens": ["In", "Be\u00b7glei\u00b7tung", "s\u00fc\u00b7\u00dfer", "Pein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich durchkrieche Strauch und H\u00f6hlen,", "tokens": ["Ich", "durch\u00b7krie\u00b7che", "Strauch", "und", "H\u00f6h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Such in W\u00e4ldern weit und nah", "tokens": ["Such", "in", "W\u00e4l\u00b7dern", "weit", "und", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Vertraute meiner Seelen,", "tokens": ["Die", "Ver\u00b7trau\u00b7te", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dennoch ist sie nirgends da.", "tokens": ["Den\u00b7noch", "ist", "sie", "nir\u00b7gends", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich beschw\u00f6re selbst die Hirten", "tokens": ["Ich", "be\u00b7schw\u00f6\u00b7re", "selbst", "die", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey den Heerden, bey den Myrthen,", "tokens": ["Bey", "den", "Heer\u00b7den", ",", "bey", "den", "Myr\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vielleicht der Liebe Pflicht", "tokens": ["Die", "viel\u00b7leicht", "der", "Lie\u00b7be", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die bunten St\u00f6cke flicht:", "tokens": ["Um", "die", "bun\u00b7ten", "St\u00f6\u00b7cke", "flicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wist ihr nicht der Phillis Spuren?", "tokens": ["Wist", "ihr", "nicht", "der", "Phil\u00b7lis", "Spu\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Habt ihr nicht mein Kind erblickt?", "tokens": ["Habt", "ihr", "nicht", "mein", "Kind", "er\u00b7blickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kommt sie nicht mehr auf die Fluren,", "tokens": ["Kommt", "sie", "nicht", "mehr", "auf", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo wir manchen Strau\u00df gepfl\u00fcckt?", "tokens": ["Wo", "wir", "man\u00b7chen", "Strau\u00df", "ge\u00b7pfl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die ihr alles h\u00f6rt und saget,", "tokens": ["Die", "ihr", "al\u00b7les", "h\u00f6rt", "und", "sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Luft und Forst und Meer durchjaget,", "tokens": ["Luft", "und", "Forst", "und", "Meer", "durch\u00b7ja\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Echo, Sonne, Mond und Wind,", "tokens": ["E\u00b7cho", ",", "Son\u00b7ne", ",", "Mond", "und", "Wind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt mir doch, wo steckt mein Kind?", "tokens": ["Sagt", "mir", "doch", ",", "wo", "steckt", "mein", "Kind", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll sie schon verg\u00f6ttert werden,", "tokens": ["Soll", "sie", "schon", "ver\u00b7g\u00f6t\u00b7tert", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Beth ich sie vielleicht herab,", "tokens": ["Be\u00b7th", "ich", "sie", "viel\u00b7leicht", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Oder ziert sie noch die Erden,", "tokens": ["O\u00b7der", "ziert", "sie", "noch", "die", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "O so reis ich bis ans Grab.", "tokens": ["O", "so", "reis", "ich", "bis", "ans", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sage selbst, entri\u00dfne Seele,", "tokens": ["Sa\u00b7ge", "selbst", ",", "ent\u00b7ri\u00df\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher Weinberg, welche H\u00f6hle,", "tokens": ["Wel\u00b7cher", "Wein\u00b7berg", ",", "wel\u00b7che", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher unbekandte Wald", "tokens": ["Wel\u00b7cher", "un\u00b7be\u00b7kand\u00b7te", "Wald"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist anjezt dein Aufenthalt?", "tokens": ["Ist", "an\u00b7jezt", "dein", "Auf\u00b7ent\u00b7halt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sage mir, damit ich folge,", "tokens": ["Sa\u00b7ge", "mir", ",", "da\u00b7mit", "ich", "fol\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4r es auch des Nilus Strand,", "tokens": ["W\u00e4r", "es", "auch", "des", "Ni\u00b7lus", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4r es auch die kalte Wolge,", "tokens": ["W\u00e4r", "es", "auch", "die", "kal\u00b7te", "Wol\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Z\u00f6g ich gern durch Ei\u00df und Sand.", "tokens": ["Z\u00f6g", "ich", "gern", "durch", "Ei\u00df", "und", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weis mir nichts Bericht zu geben?", "tokens": ["Weis", "mir", "nichts", "Be\u00b7richt", "zu", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O was ist das vor ein Leben,", "tokens": ["O", "was", "ist", "das", "vor", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VAFIN", "ART", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ich jezo ohne sie", "tokens": ["Das", "ich", "je\u00b7zo", "oh\u00b7ne", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als mein Joch zur Baare zieh!", "tokens": ["Als", "mein", "Joch", "zur", "Baa\u00b7re", "zieh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmel, las dir nicht erst fluchen,", "tokens": ["Him\u00b7mel", ",", "las", "dir", "nicht", "erst", "flu\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich begehre sie von dir \u2013", "tokens": ["Ich", "be\u00b7geh\u00b7re", "sie", "von", "dir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bin ich nicht ein Thor im Suchen?", "tokens": ["Bin", "ich", "nicht", "ein", "Thor", "im", "Su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Phillis lebt ja selbst in mir.", "tokens": ["Phil\u00b7lis", "lebt", "ja", "selbst", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich verschmachte vor Verlangen,", "tokens": ["Ich", "ver\u00b7schmach\u00b7te", "vor", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Phillis zu umfangen.", "tokens": ["Mei\u00b7ne", "Phil\u00b7lis", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Harter Himmel, z\u00fcrnst du noch?", "tokens": ["Har\u00b7ter", "Him\u00b7mel", ",", "z\u00fcrnst", "du", "noch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Faule Stunden, eilet doch!", "tokens": ["Fau\u00b7le", "Stun\u00b7den", ",", "ei\u00b7let", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eilet doch, ihr faulen Stunden,", "tokens": ["Ei\u00b7let", "doch", ",", "ihr", "fau\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erbarmt euch meiner Noth!", "tokens": ["Und", "er\u00b7barmt", "euch", "mei\u00b7ner", "Noth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird der Ri\u00df nicht bald verbunden,", "tokens": ["Wird", "der", "Ri\u00df", "nicht", "bald", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blutet sich mein Herze todt.", "tokens": ["Blu\u00b7tet", "sich", "mein", "Her\u00b7ze", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Liebste Seele, las dich finden!", "tokens": ["Liebs\u00b7te", "See\u00b7le", ",", "las", "dich", "fin\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich spaziere durch die Linden,", "tokens": ["Ich", "spa\u00b7zie\u00b7re", "durch", "die", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NE", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Durch die Th\u00e4ler, durch den Hayn", "tokens": ["Durch", "die", "Th\u00e4\u00b7ler", ",", "durch", "den", "Hayn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In Begleitung s\u00fc\u00dfer Pein;", "tokens": ["In", "Be\u00b7glei\u00b7tung", "s\u00fc\u00b7\u00dfer", "Pein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich durchkrieche Strauch und H\u00f6hlen,", "tokens": ["Ich", "durch\u00b7krie\u00b7che", "Strauch", "und", "H\u00f6h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Such in W\u00e4ldern weit und nah", "tokens": ["Such", "in", "W\u00e4l\u00b7dern", "weit", "und", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Vertraute meiner Seelen,", "tokens": ["Die", "Ver\u00b7trau\u00b7te", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dennoch ist sie nirgends da.", "tokens": ["Den\u00b7noch", "ist", "sie", "nir\u00b7gends", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich beschw\u00f6re selbst die Hirten", "tokens": ["Ich", "be\u00b7schw\u00f6\u00b7re", "selbst", "die", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bey den Heerden, bey den Myrthen,", "tokens": ["Bey", "den", "Heer\u00b7den", ",", "bey", "den", "Myr\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vielleicht der Liebe Pflicht", "tokens": ["Die", "viel\u00b7leicht", "der", "Lie\u00b7be", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die bunten St\u00f6cke flicht:", "tokens": ["Um", "die", "bun\u00b7ten", "St\u00f6\u00b7cke", "flicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wist ihr nicht der Phillis Spuren?", "tokens": ["Wist", "ihr", "nicht", "der", "Phil\u00b7lis", "Spu\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Habt ihr nicht mein Kind erblickt?", "tokens": ["Habt", "ihr", "nicht", "mein", "Kind", "er\u00b7blickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kommt sie nicht mehr auf die Fluren,", "tokens": ["Kommt", "sie", "nicht", "mehr", "auf", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo wir manchen Strau\u00df gepfl\u00fcckt?", "tokens": ["Wo", "wir", "man\u00b7chen", "Strau\u00df", "ge\u00b7pfl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Die ihr alles h\u00f6rt und saget,", "tokens": ["Die", "ihr", "al\u00b7les", "h\u00f6rt", "und", "sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Luft und Forst und Meer durchjaget,", "tokens": ["Luft", "und", "Forst", "und", "Meer", "durch\u00b7ja\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Echo, Sonne, Mond und Wind,", "tokens": ["E\u00b7cho", ",", "Son\u00b7ne", ",", "Mond", "und", "Wind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt mir doch, wo steckt mein Kind?", "tokens": ["Sagt", "mir", "doch", ",", "wo", "steckt", "mein", "Kind", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll sie schon verg\u00f6ttert werden,", "tokens": ["Soll", "sie", "schon", "ver\u00b7g\u00f6t\u00b7tert", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Beth ich sie vielleicht herab,", "tokens": ["Be\u00b7th", "ich", "sie", "viel\u00b7leicht", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Oder ziert sie noch die Erden,", "tokens": ["O\u00b7der", "ziert", "sie", "noch", "die", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "O so reis ich bis ans Grab.", "tokens": ["O", "so", "reis", "ich", "bis", "ans", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Sage selbst, entri\u00dfne Seele,", "tokens": ["Sa\u00b7ge", "selbst", ",", "ent\u00b7ri\u00df\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher Weinberg, welche H\u00f6hle,", "tokens": ["Wel\u00b7cher", "Wein\u00b7berg", ",", "wel\u00b7che", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welcher unbekandte Wald", "tokens": ["Wel\u00b7cher", "un\u00b7be\u00b