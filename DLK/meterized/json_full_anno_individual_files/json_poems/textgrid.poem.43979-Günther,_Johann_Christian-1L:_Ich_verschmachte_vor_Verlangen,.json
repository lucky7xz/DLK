], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O was ist das vor ein Leben,", "tokens": ["O", "was", "ist", "das", "vor", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VAFIN", "ART", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ich jezo ohne sie", "tokens": ["Das", "ich", "je\u00b7zo", "oh\u00b7ne", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als mein Joch zur Baare zieh!", "tokens": ["Als", "mein", "Joch", "zur", "Baa\u00b7re", "zieh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmel, las dir nicht erst fluchen,", "tokens": ["Him\u00b7mel", ",", "las", "dir", "nicht", "erst", "flu\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich begehre sie von dir \u2013", "tokens": ["Ich", "be\u00b7geh\u00b7re", "sie", "von", "dir", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bin ich nicht ein Thor im Suchen?", "tokens": ["Bin", "ich", "nicht", "ein", "Thor", "im", "Su\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Phillis lebt ja selbst in mir.", "tokens": ["Phil\u00b7lis", "lebt", "ja", "selbst", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}