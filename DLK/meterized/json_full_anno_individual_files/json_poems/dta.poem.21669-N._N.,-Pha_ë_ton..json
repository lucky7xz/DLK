{"dta.poem.21669": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Pha \u00eb ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Deinen Sonnenwagen, ich will ihn lenken!", "tokens": ["Dei\u00b7nen", "Son\u00b7nen\u00b7wa\u00b7gen", ",", "ich", "will", "ihn", "len\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Her mit den Z\u00fcgeln, vorw\u00e4rts in die Gestirne!", "tokens": ["Her", "mit", "den", "Z\u00fc\u00b7geln", ",", "vor\u00b7w\u00e4rts", "in", "die", "Ge\u00b7stir\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mag ich verr\u00fccken auch droben die ewige Ordnung,", "tokens": ["Mag", "ich", "ver\u00b7r\u00fc\u00b7cken", "auch", "dro\u00b7ben", "die", "e\u00b7wi\u00b7ge", "Ord\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Frech sie st\u00f6rend.", "tokens": ["Frech", "sie", "st\u00f6\u00b7rend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Warum ruht deiner heiligen Strahlen Abglanz,", "tokens": ["Wa\u00b7rum", "ruht", "dei\u00b7ner", "hei\u00b7li\u00b7gen", "Strah\u00b7len", "Ab\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Goldumlockter Erzeuger, auf meiner Stirne?", "tokens": ["Gold\u00b7um\u00b7lock\u00b7ter", "Er\u00b7zeu\u00b7ger", ",", "auf", "mei\u00b7ner", "Stir\u00b7ne", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Mag mich h\u00f6hnen die Welt \u2014 ich f\u00fchle die Gluthen", "tokens": ["Mag", "mich", "h\u00f6h\u00b7nen", "die", "Welt", "ich", "f\u00fch\u00b7le", "die", "Glu\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "ART", "NN", "$(", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+---+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Olympischen Geistes!", "tokens": ["O\u00b7lym\u00b7pi\u00b7schen", "Geis\u00b7tes", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Falls ich nicht st\u00fcrze sogleich beim k\u00fchnen Beginnen,", "tokens": ["Falls", "ich", "nicht", "st\u00fcr\u00b7ze", "sog\u00b7leich", "beim", "k\u00fch\u00b7nen", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Bleibe mir als Beweis des Sonnenfluges", "tokens": ["Blei\u00b7be", "mir", "als", "Be\u00b7weis", "des", "Son\u00b7nen\u00b7flu\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "KOUS", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.11": {"text": "Statt blonder Locken, zu Aschenflocken versenget", "tokens": ["Statt", "blon\u00b7der", "Lo\u00b7cken", ",", "zu", "A\u00b7schen\u00b7flo\u00b7cken", "ver\u00b7sen\u00b7get"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Ein grauer Scheitel!", "tokens": ["Ein", "grau\u00b7er", "Schei\u00b7tel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "St\u00fcrze denn, unseliger Pha", "tokens": ["St\u00fcr\u00b7ze", "denn", ",", "un\u00b7se\u00b7li\u00b7ger", "Pha"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.14": {"text": "Ph\u00f6bus herrscht in ewig heiterem Gleichma\u00df.", "tokens": ["Ph\u00f6\u00b7bus", "herrscht", "in", "e\u00b7wig", "hei\u00b7te\u00b7rem", "Gleich\u00b7ma\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Pha", "tokens": ["Pha"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Wird nur die Echo.", "tokens": ["Wird", "nur", "die", "E\u00b7cho", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}