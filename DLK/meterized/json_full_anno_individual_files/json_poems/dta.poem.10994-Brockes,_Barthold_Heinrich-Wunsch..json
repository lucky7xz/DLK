{"dta.poem.10994": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich finde meine gr\u00f6\u00dfte Freude, an deiner Creaturen Zier,", "tokens": ["Ich", "fin\u00b7de", "mei\u00b7ne", "gr\u00f6\u00df\u00b7te", "Freu\u00b7de", ",", "an", "dei\u00b7ner", "Crea\u00b7tu\u00b7ren", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ach m\u00f6chte diese meine Lust dir zum gef\u00e4llgen Opfer t\u00fcgen,", "tokens": ["Ach", "m\u00f6ch\u00b7te", "die\u00b7se", "mei\u00b7ne", "Lust", "dir", "zum", "ge\u00b7f\u00e4ll\u00b7gen", "Op\u00b7fer", "t\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PDS", "PPOSAT", "NN", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.3": {"text": "Mein\u2019 Anmuth dir gef\u00e4llig seyn! ach la\u00df, o Herr, ich fleh zu dir,", "tokens": ["Mein'", "An\u00b7muth", "dir", "ge\u00b7f\u00e4l\u00b7lig", "seyn", "!", "ach", "la\u00df", ",", "o", "Herr", ",", "ich", "fleh", "zu", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "VAINF", "$.", "XY", "XY", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Mein durch dein Werk gewirkt Vergn\u00fcgen, aus Gnad und Liebe,", "tokens": ["Mein", "durch", "dein", "Werk", "ge\u00b7wirkt", "Ver\u00b7gn\u00fc\u00b7gen", ",", "aus", "Gnad", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "PPOSAT", "NN", "VVPP", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}