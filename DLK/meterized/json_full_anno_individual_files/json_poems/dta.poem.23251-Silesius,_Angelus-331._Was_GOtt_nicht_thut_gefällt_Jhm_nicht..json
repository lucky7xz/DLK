{"dta.poem.23251": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "331. Was GOtt nicht thut/ gef\u00e4llt Jhm  \n nicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Gott mu\u00df der Anfang sein/ da\u00df Mittel un\u0303 da\u00df Ende/", "tokens": ["Gott", "mu\u00df", "der", "An\u00b7fang", "sein", "/", "da\u00df", "Mit\u00b7tel", "u\u00f1", "da\u00df", "En\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VAINF", "$(", "KOUS", "NN", "KON", "KOUS", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wo Jhm gefallen solln die Werke deiner H\u00e4ude.", "tokens": ["Wo", "Jhm", "ge\u00b7fal\u00b7len", "solln", "die", "Wer\u00b7ke", "dei\u00b7ner", "H\u00e4u\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}