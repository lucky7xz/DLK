{"dta.poem.1278": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Nicht eine Allein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Soll mein Gesichte denn ein einig Leit-Stern bin-", "tokens": ["Soll", "mein", "Ge\u00b7sich\u00b7te", "denn", "ein", "ei\u00b7nig", "Leit\u00b7S\u00b7tern", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "KON", "ART", "ADJD", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den?", "tokens": ["den", "?"], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Des Himmels rundter Krei\u00df hegt Lichter ohne Zahl:", "tokens": ["Des", "Him\u00b7mels", "rund\u00b7ter", "Krei\u00df", "hegt", "Lich\u00b7ter", "oh\u00b7ne", "Zahl", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie leicht verirret sich die ungewisse Wahl/", "tokens": ["Wie", "leicht", "ver\u00b7ir\u00b7ret", "sich", "die", "un\u00b7ge\u00b7wis\u00b7se", "Wahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PRF", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Stets in die Sonne sehn macht starres Aug-Erblin-", "tokens": ["Stets", "in", "die", "Son\u00b7ne", "sehn", "macht", "star\u00b7res", "Aug\u00b7Er\u00b7blin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "VVFIN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "den;", "tokens": ["den", ";"], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Bey einem Anblick kan ich kein Vergn\u00fcgen finden.", "tokens": ["Bey", "ei\u00b7nem", "An\u00b7blick", "kan", "ich", "kein", "Ver\u00b7gn\u00fc\u00b7gen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}