{"dta.poem.589": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die \u00fcbelsehende  Lupina,  da sie sich be-  \n schlaffen lassen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Lupina brich den Brief/ den eine fremde Hand", "tokens": ["Lu\u00b7pi\u00b7na", "brich", "den", "Brief", "/", "den", "ei\u00b7ne", "frem\u00b7de", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "$(", "ART", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Als einen Gl\u00fcckes-Wunsch zu deiner Wohnung schicket/", "tokens": ["Als", "ei\u00b7nen", "Gl\u00fc\u00b7ckes\u00b7Wunsch", "zu", "dei\u00b7ner", "Woh\u00b7nung", "schi\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil dich der ", "tokens": ["Weil", "dich", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Und du als Jungfer mehrst mit Leuten unser Land.", "tokens": ["Und", "du", "als", "Jung\u00b7fer", "mehrst", "mit", "Leu\u00b7ten", "un\u00b7ser", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "NN", "ADV", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es ist die gantze Stadt mit L\u00e4stern zwar erf\u00fcllt/", "tokens": ["Es", "ist", "die", "gant\u00b7ze", "Stadt", "mit", "L\u00e4s\u00b7tern", "zwar", "er\u00b7f\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den guten Leumuth will ein schwartzer Kyehn-Ru\u00df f\u00e4rben;", "tokens": ["Den", "gu\u00b7ten", "Leu\u00b7muth", "will", "ein", "schwart\u00b7zer", "Kyehn\u00b7Ru\u00df", "f\u00e4r\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und da\u00df du ungefreyt die Jungfer lassen sterben/", "tokens": ["Und", "da\u00df", "du", "un\u00b7ge\u00b7freyt", "die", "Jung\u00b7fer", "las\u00b7sen", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verursacht/ da\u00df man dich vor eine H - - schillt.", "tokens": ["Ver\u00b7ur\u00b7sacht", "/", "da\u00df", "man", "dich", "vor", "ei\u00b7ne", "H", "schillt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PIS", "PRF", "APPR", "ART", "XY", "$(", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch dieses schimpffet nicht/ weil man die Jungfer nennt", "tokens": ["Doch", "die\u00b7ses", "schimpf\u00b7fet", "nicht", "/", "weil", "man", "die", "Jung\u00b7fer", "nennt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PTKNEG", "$(", "KOUS", "PIS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die schon zum f\u00fcnfften mahl die Welt als Jungfer mehren.", "tokens": ["Die", "schon", "zum", "f\u00fcnff\u00b7ten", "mahl", "die", "Welt", "als", "Jung\u00b7fer", "meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "ADV", "ART", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie solt ", "tokens": ["Wie", "solt"], "token_info": ["word", "word"], "pos": ["PWAV", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Er wird ein schadhafft Glas auch vor ein Glas erkennt.", "tokens": ["Er", "wird", "ein", "schad\u00b7hafft", "Glas", "auch", "vor", "ein", "Glas", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Weil nun ein schadhafft Glas so wie ein gutes heist/", "tokens": ["Weil", "nun", "ein", "schad\u00b7hafft", "Glas", "so", "wie", "ein", "gu\u00b7tes", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So mu\u00df ich gleich als du das allbern Thun verlachen/", "tokens": ["So", "mu\u00df", "ich", "gleich", "als", "du", "das", "all\u00b7bern", "Thun", "ver\u00b7la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df dich der Neidhard will zu einer H - - machen", "tokens": ["Da\u00df", "dich", "der", "Neid\u00b7hard", "will", "zu", "ei\u00b7ner", "H", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "APPR", "ART", "XY", "$(", "$(", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Da man die H - - liebt/ und in die Jungfer schmeist.", "tokens": ["Da", "man", "die", "H", "liebt", "/", "und", "in", "die", "Jung\u00b7fer", "schmeist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "XY", "$(", "$(", "VVFIN", "$(", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Da man in ", "tokens": ["Da", "man", "in"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "Was redt der Herre da? was soll der Schimpff bedeuten", "tokens": ["Was", "redt", "der", "Her\u00b7re", "da", "?", "was", "soll", "der", "Schimpff", "be\u00b7deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$.", "PWS", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Man zieh die Pfeiffen ein von so verha\u00dften Leuten/", "tokens": ["Man", "zieh", "die", "Pfeif\u00b7fen", "ein", "von", "so", "ver\u00b7ha\u00df\u00b7ten", "Leu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ART", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mein M\u00e4dgen will nicht mahl als Jungfer seyn geehrt.", "tokens": ["Mein", "M\u00e4d\u00b7gen", "will", "nicht", "mahl", "als", "Jung\u00b7fer", "seyn", "ge\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "ADV", "KOUS", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was wunder ist es dann/ da\u00df auch ", "tokens": ["Was", "wun\u00b7der", "ist", "es", "dann", "/", "da\u00df", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "NN", "VAFIN", "PPER", "ADV", "$(", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Nicht mehr gescholten seyn/ da\u00df sie den Nahmen hasset", "tokens": ["Nicht", "mehr", "ge\u00b7schol\u00b7ten", "seyn", "/", "da\u00df", "sie", "den", "Nah\u00b7men", "has\u00b7set"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VVPP", "VAINF", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.23": {"text": "Worauf ein Knittel-Dirn nicht in Gedancken passet/", "tokens": ["Wo\u00b7rauf", "ein", "Knit\u00b7tel\u00b7Dirn", "nicht", "in", "Ge\u00b7dan\u00b7cken", "pas\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PTKNEG", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und da\u00df sie ferner nicht wil seyn des Neides Ziel.", "tokens": ["Und", "da\u00df", "sie", "fer\u00b7ner", "nicht", "wil", "seyn", "des", "Nei\u00b7des", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "VMFIN", "PPOSAT", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Denn wozu nutzet es/ da\u00df man die Jungferschafft/", "tokens": ["Denn", "wo\u00b7zu", "nut\u00b7zet", "es", "/", "da\u00df", "man", "die", "Jung\u00b7fer\u00b7schafft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "$(", "KOUS", "PIS", "ART", "NN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Die so beschwerlich ist/ mu\u00df bis ins Eh-Bett tragen?", "tokens": ["Die", "so", "be\u00b7schwer\u00b7lich", "ist", "/", "mu\u00df", "bis", "ins", "Eh\u00b7Bett", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$(", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df man mit Keusch-seyn sich mu\u00df alle Tage plagen?", "tokens": ["Da\u00df", "man", "mit", "Keuschseyn", "sich", "mu\u00df", "al\u00b7le", "Ta\u00b7ge", "pla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PRF", "VMFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.28": {"text": "Das zehn mahl bittrer ist als scharffer Wermuths-Safft.", "tokens": ["Das", "zehn", "mahl", "bit\u00b7trer", "ist", "als", "scharf\u00b7fer", "Wer\u00b7muths\u00b7\u00b7S\u00b7afft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADV", "ADJD", "VAFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Viel besser ists/ da\u00df man auch sonder Ehe schmeckt/", "tokens": ["Viel", "bes\u00b7ser", "ists", "/", "da\u00df", "man", "auch", "son\u00b7der", "E\u00b7he", "schmeckt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$(", "KOUS", "PIS", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Was das beschworne Band der vesten Treue schencket/", "tokens": ["Was", "das", "be\u00b7schwor\u00b7ne", "Band", "der", "ves\u00b7ten", "Treu\u00b7e", "schen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Da\u00df man den freyen Leib zur ", "tokens": ["Da\u00df", "man", "den", "frey\u00b7en", "Leib", "zur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "APPRART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Und unbejocht den Hals zu ihrem Wagen streckt.", "tokens": ["Und", "un\u00b7be\u00b7jocht", "den", "Hals", "zu", "ih\u00b7rem", "Wa\u00b7gen", "streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Nessel brennet schon eh sie zum Busch gedeyt/", "tokens": ["Die", "Nes\u00b7sel", "bren\u00b7net", "schon", "eh", "sie", "zum", "Busch", "ge\u00b7deyt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und was ein Hacke wird das kr\u00fcmmet sich bey Zeiten/", "tokens": ["Und", "was", "ein", "Ha\u00b7cke", "wird", "das", "kr\u00fcm\u00b7met", "sich", "bey", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VAFIN", "PDS", "VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So machts ", "tokens": ["So", "machts"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.36": {"text": "Und lernt als Jungfer an des Ehstands S\u00fcssigkeit.", "tokens": ["Und", "lernt", "als", "Jung\u00b7fer", "an", "des", "Eh\u00b7stands", "S\u00fcs\u00b7sig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Bohne d\u00fcngt das Land/ so sie hervor gebr", "tokens": ["Die", "Boh\u00b7ne", "d\u00fcngt", "das", "Land", "/", "so", "sie", "her\u00b7vor", "ge\u00b7br"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "ADV", "PPER", "PTKVZ", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die sch\u00f6n gezierte Blum/ die man ", "tokens": ["Die", "sch\u00f6n", "ge\u00b7zier\u00b7te", "Blum", "/", "die", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NE", "$(", "PRELS", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Macht/ da\u00df die Fettigkeit ihr Lust Bett nie verkennet ", "tokens": ["Macht", "/", "da\u00df", "die", "Fet\u00b7tig\u00b7keit", "ihr", "Lust", "Bett", "nie", "ver\u00b7ken\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "ART", "NN", "PPOSAT", "NN", "NN", "ADV", "VVFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.40": {"text": "So hat ", "tokens": ["So", "hat"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.41": {"text": "Doch alles weggelegt/ und nur auf dis gesehn/", "tokens": ["Doch", "al\u00b7les", "weg\u00b7ge\u00b7legt", "/", "und", "nur", "auf", "dis", "ge\u00b7sehn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVPP", "$(", "KON", "ADV", "APPR", "PDS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So l\u00e4st sich deine That mit Lobe noch bebl\u00fcmen/", "tokens": ["So", "l\u00e4st", "sich", "dei\u00b7ne", "That", "mit", "Lo\u00b7be", "noch", "be\u00b7bl\u00fc\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Sie ist nicht Scheltens werth/ sie ist vielmehr zu r\u00fchmen/", "tokens": ["Sie", "ist", "nicht", "Schel\u00b7tens", "werth", "/", "sie", "ist", "viel\u00b7mehr", "zu", "r\u00fch\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "ADJD", "$(", "PPER", "VAFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Denn weil dir dein Gesicht durch Unfall will entgehn/ ", "tokens": ["Denn", "weil", "dir", "dein", "Ge\u00b7sicht", "durch", "Un\u00b7fall", "will", "ent\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "So hast du wol gedacht/ da\u00df/ da vier Augen mehr", "tokens": ["So", "hast", "du", "wol", "ge\u00b7dacht", "/", "da\u00df", "/", "da", "vier", "Au\u00b7gen", "mehr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$(", "KOUS", "$(", "ADV", "CARD", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Als zweene k\u00f6nnen sehen/ und dich dazu bequehmet/", "tokens": ["Als", "zwee\u00b7ne", "k\u00f6n\u00b7nen", "se\u00b7hen", "/", "und", "dich", "da\u00b7zu", "be\u00b7queh\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "VMFIN", "VVINF", "$(", "KON", "PRF", "PAV", "VVFIN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.47": {"text": "Da\u00df man mit zwenen dich zu deinen noch bes\u00e4met", "tokens": ["Da\u00df", "man", "mit", "zwe\u00b7nen", "dich", "zu", "dei\u00b7nen", "noch", "be\u00b7s\u00e4\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "VVFIN", "PRF", "APPR", "PPOSAT", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Damit dein Mangel sich im Uberflu\u00df verkehr.", "tokens": ["Da\u00b7mit", "dein", "Man\u00b7gel", "sich", "im", "U\u00b7ber\u00b7flu\u00df", "ver\u00b7kehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Drum lache nur mit mir die tummen Leute aus/", "tokens": ["Drum", "la\u00b7che", "nur", "mit", "mir", "die", "tum\u00b7men", "Leu\u00b7te", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Die wegen deinen Nutz/ und h\u00f6chstbed\u00fcrfftgen Sachen/", "tokens": ["Die", "we\u00b7gen", "dei\u00b7nen", "Nutz", "/", "und", "h\u00f6chst\u00b7be\u00b7d\u00fcrfft\u00b7gen", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "$(", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Dich wollen mit Gewalt zu einer H - - machen", "tokens": ["Dich", "wol\u00b7len", "mit", "Ge\u00b7walt", "zu", "ei\u00b7ner", "H", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "APPR", "ART", "XY", "$(", "$(", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.52": {"text": "Und reithe stattlich zu auf einen Pahten-Schmaus.", "tokens": ["Und", "rei\u00b7the", "statt\u00b7lich", "zu", "auf", "ei\u00b7nen", "Pah\u00b7ten\u00b7Schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKZU", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}