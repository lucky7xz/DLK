{"textgrid.poem.24659": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein gutes Wort mu\u00dft du im Herzen tragen,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gutes Wort mu\u00dft du im Herzen tragen,", "tokens": ["Ein", "gu\u00b7tes", "Wort", "mu\u00dft", "du", "im", "Her\u00b7zen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seinen Wert enth\u00fcllt dir ", "tokens": ["Und", "sei\u00b7nen", "Wert", "ent\u00b7h\u00fcllt", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stets dringt dein Aug nicht nach des Meeres Grunde,", "tokens": ["Stets", "dringt", "dein", "Aug", "nicht", "nach", "des", "Mee\u00b7res", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An tr\u00fcben tiefer als an hellen Tagen.", "tokens": ["An", "tr\u00fc\u00b7ben", "tie\u00b7fer", "als", "an", "hel\u00b7len", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJD", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zuweilen gibt ein lichter Blick dir Kunde", "tokens": ["Zu\u00b7wei\u00b7len", "gibt", "ein", "lich\u00b7ter", "Blick", "dir", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Herzen, die in toten Dingen schlagen,", "tokens": ["Von", "Her\u00b7zen", ",", "die", "in", "to\u00b7ten", "Din\u00b7gen", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn du nur verstehest recht zu fragen,", "tokens": ["Und", "wenn", "du", "nur", "ver\u00b7ste\u00b7hest", "recht", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erf\u00e4hrst du manches auch aus stummem Munde.", "tokens": ["Er\u00b7f\u00e4hrst", "du", "man\u00b7ches", "auch", "aus", "stum\u00b7mem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Drum flieh aus deinem Selbst, dem starren, kalten,", "tokens": ["Drum", "flieh", "aus", "dei\u00b7nem", "Selbst", ",", "dem", "star\u00b7ren", ",", "kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Weltalls Seele daf\u00fcr einzutauschen,", "tokens": ["Des", "Welt\u00b7alls", "See\u00b7le", "da\u00b7f\u00fcr", "ein\u00b7zu\u00b7tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df dir des Lebens wogende Gewalten,", "tokens": ["La\u00df", "dir", "des", "Le\u00b7bens", "wo\u00b7gen\u00b7de", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "---+--+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Genu\u00df und Qualen, durch die Seele rauschen,", "tokens": ["Ge\u00b7nu\u00df", "und", "Qua\u00b7len", ",", "durch", "die", "See\u00b7le", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und kannst du eine Melodie erlauschen,", "tokens": ["Und", "kannst", "du", "ei\u00b7ne", "Me\u00b7lo\u00b7die", "er\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So strebe, ihren Nachhall festzuhalten!", "tokens": ["So", "stre\u00b7be", ",", "ih\u00b7ren", "Nach\u00b7hall", "fest\u00b7zu\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein gutes Wort mu\u00dft du im Herzen tragen,", "tokens": ["Ein", "gu\u00b7tes", "Wort", "mu\u00dft", "du", "im", "Her\u00b7zen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"},