{"dta.poem.15919": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "124.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war ein Mann \u2014 vielleicht ist mancher noch im Raume \u2014", "tokens": ["Es", "war", "ein", "Mann", "viel\u00b7leicht", "ist", "man\u00b7cher", "noch", "im", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ADV", "VAFIN", "PIS", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem alles wohl gelang, doch alles nur im Traume.", "tokens": ["Dem", "al\u00b7les", "wohl", "ge\u00b7lang", ",", "doch", "al\u00b7les", "nur", "im", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVFIN", "$,", "ADV", "PIS", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Traume sang er sch\u00f6n, im Traume sprach er gut,", "tokens": ["Im", "Trau\u00b7me", "sang", "er", "sch\u00f6n", ",", "im", "Trau\u00b7me", "sprach", "er", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Traume hatt' er Geld, im Traume hohen Muth.", "tokens": ["Im", "Trau\u00b7me", "hatt'", "er", "Geld", ",", "im", "Trau\u00b7me", "ho\u00b7hen", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "NN", "$,", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im Traume war er jung, im Traume hochgeehrt,", "tokens": ["Im", "Trau\u00b7me", "war", "er", "jung", ",", "im", "Trau\u00b7me", "hoch\u00b7geehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Traume kerngesund, im Traume grundgelehrt.", "tokens": ["Im", "Trau\u00b7me", "kern\u00b7ge\u00b7sund", ",", "im", "Trau\u00b7me", "grund\u00b7ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Im Traume pflanzte er den Garten blumenreich,", "tokens": ["Im", "Trau\u00b7me", "pflanz\u00b7te", "er", "den", "Gar\u00b7ten", "blu\u00b7men\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Traume baute er sein Haus Pall\u00e4sten gleich.", "tokens": ["Im", "Trau\u00b7me", "bau\u00b7te", "er", "sein", "Haus", "Pal\u00b7l\u00e4s\u00b7ten", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Im Traume that er, was ihm Lust und Freude machte,", "tokens": ["Im", "Trau\u00b7me", "that", "er", ",", "was", "ihm", "Lust", "und", "Freu\u00b7de", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PWS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und leid that es ihm nur, wenn er vom Traum erwachte.", "tokens": ["Und", "leid", "that", "es", "ihm", "nur", ",", "wenn", "er", "vom", "Traum", "er\u00b7wach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Drum sucht' er alsobald zum Traum zur\u00fcckzukehren,", "tokens": ["Drum", "sucht'", "er", "al\u00b7so\u00b7bald", "zum", "Traum", "zu\u00b7r\u00fcck\u00b7zu\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um zu genie\u00dfen, was sein Wachen mu\u00dft' entbehren.", "tokens": ["Um", "zu", "ge\u00b7nie\u00b7\u00dfen", ",", "was", "sein", "Wa\u00b7chen", "mu\u00dft'", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ungl\u00fccklich hat er nicht sein Leben hingebracht,", "tokens": ["Un\u00b7gl\u00fcck\u00b7lich", "hat", "er", "nicht", "sein", "Le\u00b7ben", "hin\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er im Leben mehr getr\u00e4umet als gewacht.", "tokens": ["Weil", "er", "im", "Le\u00b7ben", "mehr", "ge\u00b7tr\u00e4u\u00b7met", "als", "ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVPP", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Doch gl\u00fccklich war er nicht; nur gl\u00fccklich ist der Wache,", "tokens": ["Doch", "gl\u00fcck\u00b7lich", "war", "er", "nicht", ";", "nur", "gl\u00fcck\u00b7lich", "ist", "der", "Wa\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PTKNEG", "$.", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der nicht bedarf, da\u00df erst ein Traum ihn gl\u00fccklich mache.", "tokens": ["Der", "nicht", "be\u00b7darf", ",", "da\u00df", "erst", "ein", "Traum", "ihn", "gl\u00fcck\u00b7lich", "ma\u00b7che", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}