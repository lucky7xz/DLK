{"textgrid.poem.53255": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was wir nie recht durfften hoffen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was wir nie recht durfften hoffen,", "tokens": ["Was", "wir", "nie", "recht", "durff\u00b7ten", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nun th\u00e4tlich ja vnd war,", "tokens": ["Ist", "nun", "th\u00e4t\u00b7lich", "ja", "vnd", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADV", "KON", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir sind ausser der Gefahr,", "tokens": ["Wir", "sind", "aus\u00b7ser", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der Fried' ist nun getroffen,", "tokens": ["Denn", "der", "Fried'", "ist", "nun", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der das wilde Krieges-Schwerd", "tokens": ["Der", "das", "wil\u00b7de", "Krie\u00b7ge\u00b7sSchwerd"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ewig vor vns wird verstecken,", "tokens": ["E\u00b7wig", "vor", "vns", "wird", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der des Landes Furcht vnd Schrecken,", "tokens": ["Der", "des", "Lan\u00b7des", "Furcht", "vnd", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In ein Frewden-Fest verkehrt.", "tokens": ["In", "ein", "Fre\u00b7wden\u00b7Fest", "ver\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott, es ist durch dich geschehen,", "tokens": ["Gott", ",", "es", "ist", "durch", "dich", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du verschaffst vns diese Ruh,", "tokens": ["Du", "ver\u00b7schaffst", "vns", "die\u00b7se", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An vns Preussen lessest du", "tokens": ["An", "vns", "Preus\u00b7sen", "les\u00b7sest", "du"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jetzt ein Gnaden-Zeichen sehen,", "tokens": ["Jetzt", "ein", "Gna\u00b7den\u00b7Zei\u00b7chen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was so manches Land von dir", "tokens": ["Was", "so", "man\u00b7ches", "Land", "von", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PIAT", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit Gebet' vnd nassen Wangen", "tokens": ["Mit", "Ge\u00b7bet'", "vnd", "nas\u00b7sen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch zur zeit nicht kan erlangen,", "tokens": ["Noch", "zur", "zeit", "nicht", "kan", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Vater, das besitzen wir.", "tokens": ["Va\u00b7ter", ",", "das", "be\u00b7sit\u00b7zen", "wir", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was ersiehst du an vns Armen,", "tokens": ["Was", "er\u00b7siehst", "du", "an", "vns", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du vns so gn\u00e4dig bist?", "tokens": ["Da\u00df", "du", "vns", "so", "gn\u00e4\u00b7dig", "bist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob es vnsre Tugend ist?", "tokens": ["Ob", "es", "vns\u00b7re", "Tu\u00b7gend", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein, es ist, Herr, dein Erbarmen,", "tokens": ["Nein", ",", "es", "ist", ",", "Herr", ",", "dein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer kein Zeugn\u00fc\u00df haben kan", "tokens": ["Wer", "kein", "Zeug\u00b7n\u00fc\u00df", "ha\u00b7ben", "kan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "VAINF", "VMFIN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Deiner vnersch\u00f6pfften G\u00fcete,", "tokens": ["Dei\u00b7ner", "vner\u00b7sch\u00f6pff\u00b7ten", "G\u00fce\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Der erlernet dein Gem\u00fcete,", "tokens": ["Der", "er\u00b7ler\u00b7net", "dein", "Ge\u00b7m\u00fce\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht er nur vns Preussen an.", "tokens": ["Sieht", "er", "nur", "vns", "Preus\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach was sollen wir erzwingen?", "tokens": ["Ach", "was", "sol\u00b7len", "wir", "er\u00b7zwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was hat vnser Lobgesang,", "tokens": ["Was", "hat", "vn\u00b7ser", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welches dir, O Gott, zu danck", "tokens": ["Wel\u00b7ches", "dir", ",", "O", "Gott", ",", "zu", "danck"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "$,", "NE", "NN", "$,", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wol vnd t\u00e4glich m\u00f6chte klingen?", "tokens": ["Wol", "vnd", "t\u00e4g\u00b7lich", "m\u00f6ch\u00b7te", "klin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Es bek\u00fcmmert vnsern Geist,", "tokens": ["Es", "be\u00b7k\u00fcm\u00b7mert", "vn\u00b7sern", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df wir recht dich zu erheben", "tokens": ["Da\u00df", "wir", "recht", "dich", "zu", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hie in solcher Armut leben,", "tokens": ["Hie", "in", "sol\u00b7cher", "Ar\u00b7mut", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die vns Mund vnd Zunge schleusst.", "tokens": ["Die", "vns", "Mund", "vnd", "Zun\u00b7ge", "schleusst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch wird dir auch vnser lallen,", "tokens": ["Doch", "wird", "dir", "auch", "vn\u00b7ser", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn es nur von Hertzen geht", "tokens": ["Wenn", "es", "nur", "von", "Hert\u00b7zen", "geht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd in ernster Andacht steht,", "tokens": ["Vnd", "in", "erns\u00b7ter", "An\u00b7dacht", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebster Vater, wol gefallen,", "tokens": ["Liebs\u00b7ter", "Va\u00b7ter", ",", "wol", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ein zerst\u00fcmmelter Gesang,", "tokens": ["Ein", "zer\u00b7st\u00fcm\u00b7mel\u00b7ter", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn die Seel' jhn nur gebieret", "tokens": ["Wenn", "die", "Seel'", "jhn", "nur", "ge\u00b7bie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vnd jhn durch die Wolcken f\u00fchret,", "tokens": ["Vnd", "jhn", "durch", "die", "Wol\u00b7cken", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Hat vor dir den besten Klang.", "tokens": ["Hat", "vor", "dir", "den", "bes\u00b7ten", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Darumb sollen vnsre Seiten", "tokens": ["Da\u00b7rumb", "sol\u00b7len", "vns\u00b7re", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses durch die gantze Welt,", "tokens": ["Die\u00b7ses", "durch", "die", "gant\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der Sonnen Liecht hinfellt,", "tokens": ["Wo", "der", "Son\u00b7nen", "Liecht", "hin\u00b7fellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fchmlich wissen au\u00dfzubreiten,", "tokens": ["R\u00fchm\u00b7lich", "wis\u00b7sen", "au\u00df\u00b7zu\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja es sol auch Kindes Kind", "tokens": ["Ja", "es", "sol", "auch", "Kin\u00b7des", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VMFIN", "ADV", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses Gnaden-Werck erfahren,", "tokens": ["Die\u00b7ses", "Gna\u00b7den\u00b7\u00b7Werck", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo nach diesen vnsern Jahren", "tokens": ["Wo", "nach", "die\u00b7sen", "vn\u00b7sern", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Leute zu gewarten sind.", "tokens": ["Leu\u00b7te", "zu", "ge\u00b7war\u00b7ten", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gieb nur, da\u00df au\u00df deinem Saale", "tokens": ["Gieb", "nur", ",", "da\u00df", "au\u00df", "dei\u00b7nem", "Saa\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch dein Himmel-Kind das Recht", "tokens": ["Auch", "dein", "Him\u00b7mel\u00b7Kind", "das", "Recht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer vngebeugt vnd schlecht", "tokens": ["Im\u00b7mer", "vn\u00b7ge\u00b7beugt", "vnd", "schlecht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnser gantzes Land bestrahle,", "tokens": ["Vn\u00b7ser", "gant\u00b7zes", "Land", "be\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonst, wo bey vns Menschen schon", "tokens": ["Sonst", ",", "wo", "bey", "vns", "Men\u00b7schen", "schon"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "PPER", "NN", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Gerechtigkeit verbleichet", "tokens": ["Die", "Ge\u00b7rech\u00b7tig\u00b7keit", "ver\u00b7blei\u00b7chet"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vnd in jhr Gestirn entweichet,", "tokens": ["Vnd", "in", "jhr", "Ge\u00b7stirn", "ent\u00b7wei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fehrt der Friede mit davon.", "tokens": ["Fehrt", "der", "Frie\u00b7de", "mit", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Was wir nie recht durfften hoffen,", "tokens": ["Was", "wir", "nie", "recht", "durff\u00b7ten", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nun th\u00e4tlich ja vnd war,", "tokens": ["Ist", "nun", "th\u00e4t\u00b7lich", "ja", "vnd", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ADV", "KON", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir sind ausser der Gefahr,", "tokens": ["Wir", "sind", "aus\u00b7ser", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic