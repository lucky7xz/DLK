{"dta.poem.15815": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Markt ist voll, die Welt will mit sich selbst verkehren;", "tokens": ["Der", "Markt", "ist", "voll", ",", "die", "Welt", "will", "mit", "sich", "selbst", "ver\u00b7keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VMFIN", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Nord kann nicht den S\u00fcd, der West den Ost entbehren.", "tokens": ["Der", "Nord", "kann", "nicht", "den", "S\u00fcd", ",", "der", "West", "den", "Ost", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "La\u00dft alles kommen, was die Fremde Fremdes hat,", "tokens": ["La\u00dft", "al\u00b7les", "kom\u00b7men", ",", "was", "die", "Frem\u00b7de", "Frem\u00b7des", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "VVINF", "$,", "PRELS", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcgts zum Heimischen! ihm ist das kein Verrath.", "tokens": ["Und", "f\u00fcgts", "zum", "Hei\u00b7mi\u00b7schen", "!", "ihm", "ist", "das", "kein", "Ver\u00b7rath", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "PPER", "VAFIN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur holt von Nachbarn nicht, was wir erst ihnen gaben,", "tokens": ["Nur", "holt", "von", "Nach\u00b7barn", "nicht", ",", "was", "wir", "erst", "ih\u00b7nen", "ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und borgt nicht, was wir l\u00e4ngst im Hause besser haben!", "tokens": ["Und", "borgt", "nicht", ",", "was", "wir", "l\u00e4ngst", "im", "Hau\u00b7se", "bes\u00b7ser", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "APPRART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}