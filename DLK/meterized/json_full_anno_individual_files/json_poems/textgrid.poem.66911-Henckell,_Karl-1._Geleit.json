{"textgrid.poem.66911": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1. Geleit", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr geht ins Leben hinein,", "tokens": ["Ihr", "geht", "ins", "Le\u00b7ben", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Zweige der gr\u00fcnenden Welt in der erhobenen Hand,", "tokens": ["Zwei\u00b7ge", "der", "gr\u00fc\u00b7nen\u00b7den", "Welt", "in", "der", "er\u00b7ho\u00b7be\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Um eure jungen Stirnen spielt der aufgehende Schein", "tokens": ["Um", "eu\u00b7re", "jun\u00b7gen", "Stir\u00b7nen", "spielt", "der", "auf\u00b7ge\u00b7hen\u00b7de", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Einer Sonne, die euch f\u00fchrt in das kommende Land.", "tokens": ["Ei\u00b7ner", "Son\u00b7ne", ",", "die", "euch", "f\u00fchrt", "in", "das", "kom\u00b7men\u00b7de", "Land", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Was eure V\u00e4ter voll M\u00fch,", "tokens": ["Was", "eu\u00b7re", "V\u00e4\u00b7ter", "voll", "M\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Was eure M\u00fctter voll Weh ringend und darbend gebaut,", "tokens": ["Was", "eu\u00b7re", "M\u00fct\u00b7ter", "voll", "Weh", "rin\u00b7gend", "und", "dar\u00b7bend", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "NN", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gab euch den heiligen Grund, drauf ihr in segnender Fr\u00fch", "tokens": ["Gab", "euch", "den", "hei\u00b7li\u00b7gen", "Grund", ",", "drauf", "ihr", "in", "seg\u00b7nen\u00b7der", "Fr\u00fch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PAV", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Aufsteigt zum fruchtbaren Tag. Seht, wie das Licht euch vertraut!", "tokens": ["Auf\u00b7steigt", "zum", "frucht\u00b7ba\u00b7ren", "Tag", ".", "Seht", ",", "wie", "das", "Licht", "euch", "ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$.", "VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Seht, wie das Licht euch begr\u00fc\u00dft,", "tokens": ["Seht", ",", "wie", "das", "Licht", "euch", "be\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Kinder der wandelnden Zeit, J\u00fcnger des neuen Geschlechts!", "tokens": ["Kin\u00b7der", "der", "wan\u00b7deln\u00b7den", "Zeit", ",", "J\u00fcn\u00b7ger", "des", "neu\u00b7en", "Ge\u00b7schlechts", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Vieles, was m\u00e4chtig bis heut, vieles war traurig und w\u00fcst,", "tokens": ["Vie\u00b7les", ",", "was", "m\u00e4ch\u00b7tig", "bis", "heut", ",", "vie\u00b7les", "war", "trau\u00b7rig", "und", "w\u00fcst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADJD", "APPR", "ADV", "$,", "PIS", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Aber es w\u00e4chst in der Welt Ordnung des reineren Rechts.", "tokens": ["A\u00b7ber", "es", "w\u00e4chst", "in", "der", "Welt", "Ord\u00b7nung", "des", "rei\u00b7ne\u00b7ren", "Rechts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "Seht, wie der Kampf euer harrt", "tokens": ["Seht", ",", "wie", "der", "Kampf", "eu\u00b7er", "harrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Schlechtes noch schreckt euern Schritt, Schatten der D\u00e4mmerung sinkt,", "tokens": ["Schlech\u00b7tes", "noch", "schreckt", "eu\u00b7ern", "Schritt", ",", "Schat\u00b7ten", "der", "D\u00e4m\u00b7me\u00b