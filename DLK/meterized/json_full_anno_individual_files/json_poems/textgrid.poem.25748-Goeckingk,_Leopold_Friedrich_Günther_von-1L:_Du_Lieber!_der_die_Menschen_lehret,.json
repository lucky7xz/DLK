{"textgrid.poem.25748": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Lieber! der die Menschen lehret,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Lieber! der die Menschen lehret,", "tokens": ["Du", "Lie\u00b7ber", "!", "der", "die", "Men\u00b7schen", "leh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$.", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergn\u00fcgt zu seyn in Gott, und sich!", "tokens": ["Ver\u00b7gn\u00fcgt", "zu", "seyn", "in", "Gott", ",", "und", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "APPR", "NN", "$,", "KON", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und den nicht Einer singen h\u00f6ret,", "tokens": ["Und", "den", "nicht", "Ei\u00b7ner", "sin\u00b7gen", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKNEG", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So lauschend, und so froh, als ich!", "tokens": ["So", "lau\u00b7schend", ",", "und", "so", "froh", ",", "als", "ich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was machst du? Singst du neue Lieder", "tokens": ["Was", "machst", "du", "?", "Singst", "du", "neu\u00b7e", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Nymphen an der Zorga vor?", "tokens": ["Den", "Nym\u00b7phen", "an", "der", "Zor\u00b7ga", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "H\u00f6rt Chloe zu? Sind Musenbr\u00fcder", "tokens": ["H\u00f6rt", "Chloe", "zu", "?", "Sind", "Mu\u00b7sen\u00b7br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "NE", "PTKVZ", "$.", "VAFIN", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.8": {"text": "Bei dir? Ist es das Musenchor?", "tokens": ["Bei", "dir", "?", "Ist", "es", "das", "Mu\u00b7sen\u00b7chor", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.9": {"text": "W\u00fc\u00dft' ich's, o Freund, ich fl\u00f6ge, fl\u00f6ge", "tokens": ["W\u00fc\u00dft'", "ich's", ",", "o", "Freund", ",", "ich", "fl\u00f6\u00b7ge", ",", "fl\u00f6\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIS", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Noch diesen Abend sp\u00e4t, zu dir!", "tokens": ["Noch", "die\u00b7sen", "A\u00b7bend", "sp\u00e4t", ",", "zu", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ADJD", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich fa\u00dfte dich beim Arm, und z\u00f6ge", "tokens": ["Ich", "fa\u00df\u00b7te", "dich", "beim", "Arm", ",", "und", "z\u00f6\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Mit deinen Musen dich zu mir!", "tokens": ["Mit", "dei\u00b7nen", "Mu\u00b7sen", "dich", "zu", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Versteht sich, wenn die neuen Lieder", "tokens": ["Ver\u00b7steht", "sich", ",", "wenn", "die", "neu\u00b7en", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Gesungen w\u00e4ren, morgen fr\u00fch \u2013", "tokens": ["Ge\u00b7sun\u00b7gen", "w\u00e4\u00b7ren", ",", "mor\u00b7gen", "fr\u00fch", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und Abends, sp\u00e4t erst, w\u00e4r' ich wieder", "tokens": ["Und", "A\u00b7bends", ",", "sp\u00e4t", "erst", ",", "w\u00e4r'", "ich", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "ADJD", "ADV", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "In meiner Closterzelle, die", "tokens": ["In", "mei\u00b7ner", "Clos\u00b7ter\u00b7zel\u00b7le", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Mit Kaiser Nero's goldnen S\u00e4len", "tokens": ["Mit", "Kai\u00b7ser", "Nero's", "gold\u00b7nen", "S\u00e4\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NE", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Ich nicht vertauschte.", "tokens": ["Ich", "nicht", "ver\u00b7tauschte", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Sorgen qu\u00e4len", "tokens": ["Sor\u00b7gen", "qu\u00e4\u00b7len"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "In meiner Closterzelle mich", "tokens": ["In", "mei\u00b7ner", "Clos\u00b7ter\u00b7zel\u00b7le", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Nur manchen Tag, wenn Herzen fehlen,", "tokens": ["Nur", "man\u00b7chen", "Tag", ",", "wenn", "Her\u00b7zen", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "In die mein Herz, wenn's voll ist, sich", "tokens": ["In", "die", "mein", "Herz", ",", "wenn's", "voll", "ist", ",", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "PPOSAT", "NN", "$,", "KOUS", "ADJD", "VAFIN", "$,", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ergie\u00dfen kann.", "tokens": ["Er\u00b7gie\u00b7\u00dfen", "kann", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "In Nero's S\u00e4len,", "tokens": ["In", "Nero's", "S\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Und w\u00e4r' ich ", "tokens": ["Und", "w\u00e4r'", "ich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.26": {"text": "Glaub' ich, mich alle Tage qu\u00e4len.", "tokens": ["Glaub'", "ich", ",", "mich", "al\u00b7le", "Ta\u00b7ge", "qu\u00e4\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Erfahrung lehrt's; ich habe nie", "tokens": ["Er\u00b7fah\u00b7rung", "lehrt's", ";", "ich", "ha\u00b7be", "nie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Ein hohes Dach, ein pr\u00e4chtig Haus", "tokens": ["Ein", "ho\u00b7hes", "Dach", ",", "ein", "pr\u00e4ch\u00b7tig", "Haus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Von Sorgenschw\u00e4rmen leer gefunden,", "tokens": ["Von", "Sor\u00b7gen\u00b7schw\u00e4r\u00b7men", "leer", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Die meisten oft bei einem Schmaus.", "tokens": ["Die", "meis\u00b7ten", "oft", "bei", "ei\u00b7nem", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und, Lieber, ohne Zweifel hast", "tokens": ["Und", ",", "Lie\u00b7ber", ",", "oh\u00b7ne", "Zwei\u00b7fel", "hast"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "$,", "KOUI", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Auf deinen Reisen in den Stunden", "tokens": ["Auf", "dei\u00b7nen", "Rei\u00b7sen", "in", "den", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Der Mu\u00dfe, wenn von Hirsch und Hunden", "tokens": ["Der", "Mu\u00b7\u00dfe", ",", "wenn", "von", "Hirsch", "und", "Hun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Zur\u00fcck du warest, halb geschunden,", "tokens": ["Zu\u00b7r\u00fcck", "du", "wa\u00b7rest", ",", "halb", "ge\u00b7schun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VAFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Auch du derselben wohl gefunden", "tokens": ["Auch", "du", "der\u00b7sel\u00b7ben", "wohl", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PDAT", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "In einem f\u00fcrstlichen Pallast!", "tokens": ["In", "ei\u00b7nem", "f\u00fcrst\u00b7li\u00b7chen", "Pal\u00b7last", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Und also, weil Erfahrung lehrt,", "tokens": ["Und", "al\u00b7so", ",", "weil", "Er\u00b7fah\u00b7rung", "lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Da\u00df eben in die goldnen S\u00e4le", "tokens": ["Da\u00df", "e\u00b7ben", "in", "die", "gold\u00b7nen", "S\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Die Freude nicht war eingekehrt,", "tokens": ["Die", "Freu\u00b7de", "nicht", "war", "ein\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "So tausch' ich nicht!", "tokens": ["So", "tausch'", "ich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.41": {"text": "Die kleine Kehle", "tokens": ["Die", "klei\u00b7ne", "Keh\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.42": {"text": "Der Virtuosin Philomele", "tokens": ["Der", "Vir\u00b7tu\u00b7o\u00b7sin", "Phi\u00b7lo\u00b7me\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.43": {"text": "H\u00f6r' ich so gut, bei meiner Seele,", "tokens": ["H\u00f6r'", "ich", "so", "gut", ",", "bei", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Das kleine Ding h\u00f6r' ich so gut", "tokens": ["Das", "klei\u00b7ne", "Ding", "h\u00f6r'", "ich", "so", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "In meinem kleinen Sanssoucis,", "tokens": ["In", "mei\u00b7nem", "klei\u00b7nen", "Sans\u00b7sou\u00b7cis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.47": {"text": "In seinem gro\u00dfen!", "tokens": ["In", "sei\u00b7nem", "gro\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.48": {"text": "Also zieh,", "tokens": ["Al\u00b7so", "zieh", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.49": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.50": {"text": "Den alten Reiserock nur an!", "tokens": ["Den", "al\u00b7ten", "Rei\u00b7se\u00b7rock", "nur", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Und komm gegangen, komm geritten,", "tokens": ["Und", "komm", "ge\u00b7gan\u00b7gen", ",", "komm", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$,", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Da\u00df ich mein Herz in deines sch\u00fctten,", "tokens": ["Da\u00df", "ich", "mein", "Herz", "in", "dei\u00b7nes", "sch\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Und dich nach K\u00f6nigen in H\u00fctten,", "tokens": ["Und", "dich", "nach", "K\u00f6\u00b7ni\u00b7gen", "in", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Und meinem ", "tokens": ["Und", "mei\u00b7nem"], "token_info": ["word", "word"], "pos": ["KON", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Du Lieber! der die Menschen lehret,", "tokens": ["Du", "Lie\u00b7ber", "!", "der", "die", "Men\u00b7schen", "leh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$.", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergn\u00fcgt zu seyn in Gott, und sich!", "tokens": ["Ver\u00b7gn\u00fcgt", "zu", "seyn", "in", "Gott", ",", "und", "sich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "APPR", "NN", "$,", "KON", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und den nicht Einer singen h\u00f6ret,", "tokens": ["Und", "den", "nicht", "Ei\u00b7ner", "sin\u00b7gen", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKNEG", "PIS", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So lauschend, und so froh, als ich!", "tokens": ["So", "lau\u00b7schend", ",", "und", "so", "froh", ",", "als", "ich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was machst du? Singst du neue Lieder", "tokens": ["Was", "machst", "du", "?", "Singst", "du", "neu\u00b7e", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Nymphen an der Zorga vor?", "tokens": ["Den", "Nym\u00b7phen", "an", "der", "Zor\u00b7ga", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "H\u00f6rt Chloe zu? Sind Musenbr\u00fcder", "tokens": ["H\u00f6rt", "Chloe", "zu", "?", "Sind", "Mu\u00b7sen\u00b7br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "NE", "PTKVZ", "$.", "VAFIN", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.8": {"text": "Bei dir? Ist es das Musenchor?", "tokens": ["Bei", "dir", "?", "Ist", "es", "das", "Mu\u00b7sen\u00b7chor", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.9": {"text": "W\u00fc\u00dft' ich's, o Freund, ich fl\u00f6ge, fl\u00f6ge", "tokens": ["W\u00fc\u00dft'", "ich's", ",", "o", "Freund", ",", "ich", "fl\u00f6\u00b7ge", ",", "fl\u00f6\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIS", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Noch diesen Abend sp\u00e4t, zu dir!", "tokens": ["Noch", "die\u00b7sen", "A\u00b7bend", "sp\u00e4t", ",", "zu", "dir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ADJD", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ich fa\u00dfte dich beim Arm, und z\u00f6ge", "tokens": ["Ich", "fa\u00df\u00b7te", "dich", "beim", "Arm", ",", "und", "z\u00f6\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Mit deinen Musen dich zu mir!", "tokens": ["Mit", "dei\u00b7nen", "Mu\u00b7sen", "dich", "zu", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Versteht sich, wenn die neuen Lieder", "tokens": ["Ver\u00b7steht", "sich", ",", "wenn", "die", "neu\u00b7en", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Gesungen w\u00e4ren, morgen fr\u00fch \u2013", "tokens": ["Ge\u00b7sun\u00b7gen", "w\u00e4\u00b7ren", ",", "mor\u00b7gen", "fr\u00fch", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und Abends, sp\u00e4t erst, w\u00e4r' ich wieder", "tokens": ["Und", "A\u00b7bends", ",", "sp\u00e4t", "erst", ",", "w\u00e4r'", "ich", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "ADJD", "ADV", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "In meiner Closterzelle, die", "tokens": ["In", "mei\u00b7ner", "Clos\u00b7ter\u00b7zel\u00b7le", ",", "die"], "token_info": ["word