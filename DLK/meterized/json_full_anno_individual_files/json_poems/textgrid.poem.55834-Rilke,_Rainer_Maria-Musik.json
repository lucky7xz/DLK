{"textgrid.poem.55834": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Musik", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was spielst du, Knabe? Durch die G\u00e4rten gings", "tokens": ["Was", "spielst", "du", ",", "Kna\u00b7be", "?", "Durch", "die", "G\u00e4r\u00b7ten", "gings"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$.", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie viele Schritte, fl\u00fcsternde Befehle.", "tokens": ["wie", "vie\u00b7le", "Schrit\u00b7te", ",", "fl\u00fcs\u00b7tern\u00b7de", "Be\u00b7feh\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Was spielst du, Knabe? Siehe deine Seele", "tokens": ["Was", "spielst", "du", ",", "Kna\u00b7be", "?", "Sie\u00b7he", "dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$.", "VVIMP", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "verfing sich in den St\u00e4ben der Syrinx.", "tokens": ["ver\u00b7fing", "sich", "in", "den", "St\u00e4\u00b7ben", "der", "Sy\u00b7rinx", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was lockst du sie? Der Klang ist wie ein Kerker,", "tokens": ["Was", "lockst", "du", "sie", "?", "Der", "Klang", "ist", "wie", "ein", "Ker\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$.", "ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "darin sie sich vers\u00e4umt und sich versehnt;", "tokens": ["da\u00b7rin", "sie", "sich", "ver\u00b7s\u00e4umt", "und", "sich", "ver\u00b7sehnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVPP", "KON", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "stark ist dein Leben, doch dein Lied ist st\u00e4rker,", "tokens": ["stark", "ist", "dein", "Le\u00b7ben", ",", "doch", "dein", "Lied", "ist", "st\u00e4r\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "an deine Sehnsucht schluchzend angelehnt. \u2013", "tokens": ["an", "dei\u00b7ne", "Sehn\u00b7sucht", "schluch\u00b7zend", "an\u00b7ge\u00b7lehnt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Gieb ihr ein Schweigen, da\u00df die Seele leise", "tokens": ["Gieb", "ihr", "ein", "Schwei\u00b7gen", ",", "da\u00df", "die", "See\u00b7le", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "KOUS", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "heimkehre in das Flutende und Viele,", "tokens": ["heim\u00b7keh\u00b7re", "in", "das", "Flu\u00b7ten\u00b7de", "und",