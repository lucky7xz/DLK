{"dta.poem.13404": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der 65 Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Die Andacht lobet in dem Stillen", "tokens": ["Die", "An\u00b7dacht", "lo\u00b7bet", "in", "dem", "Stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Zion Dich, den grossen HErrn,", "tokens": ["Zu", "Zi\u00b7on", "Dich", ",", "den", "gros\u00b7sen", "Herrn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was sie gelobt, will sie erf\u00fcllen,", "tokens": ["Was", "sie", "ge\u00b7lobt", ",", "will", "sie", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir GOTT bezahlt sie solches gern;", "tokens": ["Dir", "GoTT", "be\u00b7zahlt", "sie", "sol\u00b7ches", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "PPER", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr heisses Flehn, ihr br\u00fcnstig Beten,", "tokens": ["Ihr", "heis\u00b7ses", "Flehn", ",", "ihr", "br\u00fcns\u00b7tig", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Barmherziger! erh\u00f6rest Du,", "tokens": ["Barm\u00b7her\u00b7zi\u00b7ger", "!", "er\u00b7h\u00f6\u00b7rest", "Du", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum kommt die Noth vor Dich getreten,", "tokens": ["Drum", "kommt", "die", "Noth", "vor", "Dich", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es naht sich alles Fleisch herzu.", "tokens": ["Es", "naht", "sich", "al\u00b7les", "Fleisch", "her\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O! machten uns doch unsre S\u00fcnden", "tokens": ["O", "!", "mach\u00b7ten", "uns", "doch", "uns\u00b7re", "S\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht roth in deiner Gegenwart!", "tokens": ["Nicht", "roth", "in", "dei\u00b7ner", "Ge\u00b7gen\u00b7wart", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey uns ist Missethat zu finden!", "tokens": ["Bey", "uns", "ist", "Mis\u00b7se\u00b7that", "zu", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sie dr\u00fccket uns! ach! wie so hart!", "tokens": ["Sie", "dr\u00fc\u00b7cket", "uns", "!", "ach", "!", "wie", "so", "hart", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "XY", "$.", "PWAV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sey ausges\u00f6hnt durch wahre Reue,", "tokens": ["Sey", "aus\u00b7ge\u00b7s\u00f6hnt", "durch", "wah\u00b7re", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Busse ruft: Vergieb! Geduld!", "tokens": ["Die", "Bus\u00b7se", "ruft", ":", "Ver\u00b7gieb", "!", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VVIMP", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Stell ins Vergessen! o verzeihe!", "tokens": ["Stell", "ins", "Ver\u00b7ges\u00b7sen", "!", "o", "ver\u00b7zei\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "FM", "FM", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erla\u00df uns doch die S\u00fcndenschuld!", "tokens": ["Er\u00b7la\u00df", "uns", "doch", "die", "S\u00fcn\u00b7den\u00b7schuld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gl\u00fcckseelig! welchen Du erkohren,", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", "!", "wel\u00b7chen", "Du", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PWAT", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er darf n\u00e4her zu Dir gehn,", "tokens": ["Da\u00df", "er", "darf", "n\u00e4\u00b7her", "zu", "Dir", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu wohnen inner deinen Thoren,", "tokens": ["Zu", "woh\u00b7nen", "in\u00b7ner", "dei\u00b7nen", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In heilgen H\u00f6fen sich zu sehn;", "tokens": ["In", "heil\u00b7gen", "H\u00f6\u00b7fen", "sich", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Welch reichen Trost hat er zu hoffen,", "tokens": ["Welch", "rei\u00b7chen", "Trost", "hat", "er", "zu", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Auserw\u00e4hlte, dieser Mann!", "tokens": ["Der", "Au\u00b7ser\u00b7w\u00e4hl\u00b7te", ",", "die\u00b7ser", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der deinen Tempel immer offen", "tokens": ["Der", "dei\u00b7nen", "Tem\u00b7pel", "im\u00b7mer", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Dich in Andacht schauen kann.", "tokens": ["Und", "Dich", "in", "An\u00b7dacht", "schau\u00b7en", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Erh\u00f6rung wollest Du uns senden,", "tokens": ["Er\u00b7h\u00f6\u00b7rung", "wol\u00b7lest", "Du", "uns", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Zepter werd auf uns geneigt", "tokens": ["Ihr", "Zep\u00b7ter", "werd", "auf", "uns", "ge\u00b7neigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Aus denen so gerechten H\u00e4nden,", "tokens": ["Aus", "de\u00b7nen", "so", "ge\u00b7rech\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die sich ganz wunderbar bezeigt;", "tokens": ["Die", "sich", "ganz", "wun\u00b7der\u00b7bar", "be\u00b7zeigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "GoTT, unser Heyl! den wir erheben,", "tokens": ["GoTT", ",", "un\u00b7ser", "Heyl", "!", "den", "wir", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$.", "ART", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Erdenkinder Zuversicht,", "tokens": ["Der", "Er\u00b7den\u00b7kin\u00b7der", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der fernen V\u00f6lkerschaft Bestreben,", "tokens": ["Der", "fer\u00b7nen", "V\u00f6l\u00b7ker\u00b7schaft", "Be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die sich am Weltmeer eingericht.", "tokens": ["Die", "sich", "am", "Welt\u00b7meer", "ein\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der durch sein kr\u00e4ftiges Befehlen", "tokens": ["Der", "durch", "sein", "kr\u00e4f\u00b7ti\u00b7ges", "Be\u00b7feh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Berge feste setzen kann,", "tokens": ["Die", "Ber\u00b7ge", "fes\u00b7te", "set\u00b7zen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die seine hohe Macht erzehlen,", "tokens": ["Die", "sei\u00b7ne", "ho\u00b7he", "Macht", "er\u00b7zeh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Womit Er ewig angethan,", "tokens": ["Wo\u00b7mit", "Er", "e\u00b7wig", "an\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Meere Wasserwogen Brausen", "tokens": ["Der", "Mee\u00b7re", "Was\u00b7ser\u00b7wo\u00b7gen", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wellen Toben h\u00f6r ich zu;", "tokens": ["Der", "Wel\u00b7len", "To\u00b7ben", "h\u00f6r", "ich", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Emp\u00f6rte V\u00f6lkerschaften sausen;", "tokens": ["Em\u00b7p\u00f6r\u00b7te", "V\u00f6l\u00b7ker\u00b7schaf\u00b7ten", "sau\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Allm\u00e4chtiger! das stillest Du!", "tokens": ["All\u00b7m\u00e4ch\u00b7ti\u00b7ger", "!", "das", "stil\u00b7lest", "Du", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die\u00df alles sehn die Nationen,", "tokens": ["Die\u00df", "al\u00b7les", "sehn", "die", "Na\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die in demselben Himmels-Strich", "tokens": ["Die", "in", "dem\u00b7sel\u00b7ben", "Him\u00b7mels\u00b7Strich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dort an jenen Enden wohnen,", "tokens": ["Und", "dort", "an", "je\u00b7nen", "En\u00b7den", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sehn es, und entsetzen sich,", "tokens": ["Sie", "sehn", "es", ",", "und", "ent\u00b7set\u00b7zen", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sehen deine Wunderzeichen,", "tokens": ["Sie", "se\u00b7hen", "dei\u00b7ne", "Wun\u00b7der\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die unumschr\u00e4nkte hohe Macht,", "tokens": ["Die", "un\u00b7um\u00b7schr\u00e4nk\u00b7te", "ho\u00b7he", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erschrocken stehn sie da wie Leichen,", "tokens": ["Er\u00b7schro\u00b7cken", "stehn", "sie", "da", "wie", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sprechen: Wer hat das gedacht!", "tokens": ["Und", "spre\u00b7chen", ":", "Wer", "hat", "das", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "PWS", "VAFIN", "PDS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du bleibest aber auch hingegen", "tokens": ["Du", "blei\u00b7best", "a\u00b7ber", "auch", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Ursprung aller Fr\u00f6hlichkeit,", "tokens": ["Der", "Ur\u00b7sprung", "al\u00b7ler", "Fr\u00f6h\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was sich vor Creaturen regen,", "tokens": ["Was", "sich", "vor", "Crea\u00b7tu\u00b7ren", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die machet deine Hand erfreut,", "tokens": ["Die", "ma\u00b7chet", "dei\u00b7ne", "Hand", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Von Freuden sind sie fr\u00fch schon trunken,", "tokens": ["Von", "Freu\u00b7den", "sind", "sie", "fr\u00fch", "schon", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jhr Lobgesang ert\u00f6nt Dich da,", "tokens": ["Ihr", "Lob\u00b7ge\u00b7sang", "er\u00b7t\u00f6nt", "Dich", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wenn das Tagelicht gesunken,", "tokens": ["Und", "wenn", "das", "Ta\u00b7ge\u00b7licht", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So schallt noch ihr Halleluja!", "tokens": ["So", "schallt", "noch", "ihr", "Hal\u00b7le\u00b7lu\u00b7ja", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "HeRR! deine wundergrosse Milde", "tokens": ["HeRR", "!", "dei\u00b7ne", "wun\u00b7der\u00b7gros\u00b7se", "Mil\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Besuchet oft das d\u00fcrre Land,", "tokens": ["Be\u00b7su\u00b7chet", "oft", "das", "d\u00fcr\u00b7re", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Begiesset Fluhren und Gefilde,", "tokens": ["Be\u00b7gies\u00b7set", "Fluh\u00b7ren", "und", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie reich wird denn ihr fetter Stand!", "tokens": ["Wie", "reich", "wird", "denn", "ihr", "fet\u00b7ter", "Stand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In GOttes vollem Br\u00fcnnlein qvillen", "tokens": ["In", "Got\u00b7tes", "vol\u00b7lem", "Br\u00fcnn\u00b7lein", "qvil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Segens-Wasser immerdar,", "tokens": ["Die", "Se\u00b7gens\u00b7Was\u00b7ser", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Getreyde mu\u00df die Hufen f\u00fcllen,", "tokens": ["Ge\u00b7trey\u00b7de", "mu\u00df", "die", "Hu\u00b7fen", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So baut das Land dein gutes Jahr.", "tokens": ["So", "baut", "das", "Land", "dein", "gu\u00b7tes", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}