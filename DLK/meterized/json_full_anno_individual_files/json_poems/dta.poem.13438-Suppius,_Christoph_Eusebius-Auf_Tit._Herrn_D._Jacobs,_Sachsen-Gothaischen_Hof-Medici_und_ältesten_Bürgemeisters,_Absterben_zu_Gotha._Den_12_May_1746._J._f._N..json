{"dta.poem.13438": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Auf Tit.  \n  Herrn D. Jacobs,  \n Sachsen-Gothaischen Hof-Medici und \u00e4ltesten  \n B\u00fcrgemeisters,  \n  Absterben zu Gotha.  \n Den 12 May 1746.  \n J. f. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Auf der Gerechten Wegen wandeln,", "tokens": ["Auf", "der", "Ge\u00b7rech\u00b7ten", "We\u00b7gen", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ein gel\u00e4utertes Bem\u00fchn", "tokens": ["Durch", "ein", "ge\u00b7l\u00e4u\u00b7ter\u00b7tes", "Be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach Billigkeit mit Sanftmuth handeln,", "tokens": ["Nach", "Bil\u00b7lig\u00b7keit", "mit", "Sanft\u00b7muth", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hei\u00dft; sich der Sterblichkeit entziehn;", "tokens": ["Hei\u00dft", ";", "sich", "der", "Sterb\u00b7lich\u00b7keit", "ent\u00b7ziehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und denen ist bereits hienieden", "tokens": ["Und", "de\u00b7nen", "ist", "be\u00b7reits", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein sch\u00f6n, ein lieblich Loos beschieden,", "tokens": ["Ein", "sch\u00f6n", ",", "ein", "lieb\u00b7lich", "Loos", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein ewig Denkmahl zubereit,", "tokens": ["Ein", "e\u00b7wig", "Denk\u00b7mahl", "zu\u00b7be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die nur f\u00fcr andrer Wohl gelebet,", "tokens": ["Die", "nur", "f\u00fcr", "an\u00b7drer", "Wohl", "ge\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die sich zu deren Heyl bestrebet;", "tokens": ["Die", "sich", "zu", "de\u00b7ren", "Heyl", "be\u00b7stre\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PRELAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Tugend folgt Unsterblichkeit;", "tokens": ["Der", "Tu\u00b7gend", "folgt", "U\u00b7nsterb\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Ein Geist, der so erhaben denket,", "tokens": ["Ein", "Geist", ",", "der", "so", "er\u00b7ha\u00b7ben", "den\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch wohlversorgten Unterricht,", "tokens": ["Durch", "wohl\u00b7ver\u00b7sorg\u00b7ten", "Un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wird sparsam nur der Welt geschenket,", "tokens": ["Wird", "spar\u00b7sam", "nur", "der", "Welt", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er k\u00f6mmt mit jedem Fr\u00fchling nicht;", "tokens": ["Er", "k\u00f6mmt", "mit", "je\u00b7dem", "Fr\u00fch\u00b7ling", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An einem jeden fr\u00fchen Morgen", "tokens": ["An", "ei\u00b7nem", "je\u00b7den", "fr\u00fc\u00b7hen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr das gemeine Beste sorgen,", "tokens": ["F\u00fcr", "das", "ge\u00b7mei\u00b7ne", "Bes\u00b7te", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Es \u00fcberlegt zu Mitternacht,", "tokens": ["Es", "\u00fc\u00b7ber\u00b7legt", "zu", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hei\u00dft bey selbsteigenen Beschwerden", "tokens": ["Hei\u00dft", "bey", "selbs\u00b7tei\u00b7ge\u00b7nen", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Tage freylich hier auf Erden", "tokens": ["Die", "Ta\u00b7ge", "frey\u00b7lich", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht eben ruhig zugebracht.", "tokens": ["Nicht", "e\u00b7ben", "ru\u00b7hig", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jedoch auf die vollbrachten Pflichten", "tokens": ["Je\u00b7doch", "auf", "die", "voll\u00b7brach\u00b7ten", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Erfolgt ein ungemeiner Lohn;", "tokens": ["Er\u00b7folgt", "ein", "un\u00b7ge\u00b7mei\u00b7ner", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man lebt in k\u00fcnftigen Geschichten,", "tokens": ["Man", "lebt", "in", "k\u00fcnf\u00b7ti\u00b7gen", "Ge\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bindet sich die eigne Kron,", "tokens": ["Und", "bin\u00b7det", "sich", "die", "eig\u00b7ne", "Kron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil man der Tugend sich beflissen,", "tokens": ["Weil", "man", "der", "Tu\u00b7gend", "sich", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So macht ein ruhiges Gewissen", "tokens": ["So", "macht", "ein", "ru\u00b7hi\u00b7ges", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Uns t\u00e4glich mit uns selbst vergn\u00fcgt,", "tokens": ["Uns", "t\u00e4g\u00b7lich", "mit", "uns", "selbst", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es schenket ein zufrieden Herze,", "tokens": ["Es", "schen\u00b7ket", "ein", "zu\u00b7frie\u00b7den", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df man auch in dem gr\u00f6\u00dften Schmerze", "tokens": ["Da\u00df", "man", "auch", "in", "dem", "gr\u00f6\u00df\u00b7ten", "Schmer\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Des Todes Bitterkeit besiegt.", "tokens": ["Des", "To\u00b7des", "Bit\u00b7ter\u00b7keit", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie sch\u00f6n! wenn Arme, Witwen, Waysen,", "tokens": ["Wie", "sch\u00f6n", "!", "wenn", "Ar\u00b7me", ",", "Wit\u00b7wen", ",", "Way\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJD", "$.", "KOUS", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Unschuld, die nicht mehr bedr\u00e4ngt,", "tokens": ["Wenn", "Un\u00b7schuld", ",", "die", "nicht", "mehr", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Helfer, den Erretter preisen,", "tokens": ["Den", "Hel\u00b7fer", ",", "den", "Er\u00b7ret\u00b7ter", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Und Liebes-Thr\u00e4nen untermengt?", "tokens": ["Und", "Lie\u00b7bes\u00b7Thr\u00e4\u00b7nen", "un\u00b7ter\u00b7mengt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie angenehm! bey seinem Scheiden", "tokens": ["Wie", "an\u00b7ge\u00b7nehm", "!", "bey", "sei\u00b7nem", "Schei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht den geringsten Vorwurf leiden,", "tokens": ["Nicht", "den", "ge\u00b7rings\u00b7ten", "Vor\u00b7wurf", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den man denn ungeheuchelt giebt,", "tokens": ["Den", "man", "denn", "un\u00b7ge\u00b7heu\u00b7chelt", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hingegen von uns h\u00f6ren sagen,", "tokens": ["Hin\u00b7ge\u00b7gen", "von", "uns", "h\u00f6\u00b7ren", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Bevor man uns noch weggetragen:", "tokens": ["Be\u00b7vor", "man", "uns", "noch", "weg\u00b7ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der hat die Tugend recht geliebt.", "tokens": ["Der", "hat", "die", "Tu\u00b7gend", "recht", "ge\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kommt nun des Allerh\u00f6chsten G\u00fcte", "tokens": ["Kommt", "nun", "des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bey alle diesen noch hinzu,", "tokens": ["Bey", "al\u00b7le", "die\u00b7sen", "noch", "hin\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PDS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schenkt ein christliches Gem\u00fcthe,", "tokens": ["Und", "schenkt", "ein", "christ\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie herrlich wird sodenn die Ruh!", "tokens": ["Wie", "herr\u00b7lich", "wird", "so\u00b7denn", "die", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer wollte da mit Kronen tauschen!", "tokens": ["Wer", "woll\u00b7te", "da", "mit", "Kro\u00b7nen", "tau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Jahre gehen ohne Rauschen,", "tokens": ["Die", "Jah\u00b7re", "ge\u00b7hen", "oh\u00b7ne", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ohnunberuhiget vorbey;", "tokens": ["Ohn\u00b7un\u00b7be\u00b7ru\u00b7hi\u00b7get", "vor\u00b7bey", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Erblickt man sich an seinem Ende,", "tokens": ["Er\u00b7blickt", "man", "sich", "an", "sei\u00b7nem", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So hebt die Andacht uns die H\u00e4nde", "tokens": ["So", "hebt", "die", "An\u00b7dacht", "uns", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Zu beten, da\u00df es sanfte sey!", "tokens": ["Zu", "be\u00b7ten", ",", "da\u00df", "es", "sanf\u00b7te", "sey", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Will man hievon ein Beyspiel haben?", "tokens": ["Will", "man", "hie\u00b7von", "ein", "Bey\u00b7spiel", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Tugend ruft zur Grust heraus:", "tokens": ["Die", "Tu\u00b7gend", "ruft", "zur", "Grust", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die\u00df alles waren meine Gaben,", "tokens": ["Die\u00df", "al\u00b7les", "wa\u00b7ren", "mei\u00b7ne", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und so sah euer Jacobs aus!", "tokens": ["Und", "so", "sah", "eu\u00b7er", "Ja\u00b7cobs", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Deswegen wird in jenem Leben", "tokens": ["Des\u00b7we\u00b7gen", "wird", "in", "je\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jhm nun Unsterblichkeit gegeben,", "tokens": ["Jhm", "nun", "U\