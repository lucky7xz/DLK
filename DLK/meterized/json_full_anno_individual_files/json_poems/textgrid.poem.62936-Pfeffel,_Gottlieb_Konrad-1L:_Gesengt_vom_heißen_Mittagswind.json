["VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mit hohlem Aug und heiserm Aechzen,", "tokens": ["Mit", "hoh\u00b7lem", "Aug", "und", "hei\u00b7serm", "A\u00b7ech\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Nach einem Tropfen Wassers lechzen.", "tokens": ["Nach", "ei\u00b7nem", "Trop\u00b7fen", "Was\u00b7sers", "lech\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Itzt bricht sein Herz, voll sch\u00f6ner Wuth", "tokens": ["Itzt", "bricht", "sein", "Herz", ",", "voll", "sch\u00f6\u00b7ner", "Wuth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Rei\u00dft er mit der gest\u00e4hlten Spitze", "tokens": ["Rei\u00dft", "er", "mit", "der", "ge\u00b7st\u00e4hl\u00b7ten", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Des Schnabels eine tiefe Ritze", "tokens": ["Des", "Schna\u00b7bels", "ei\u00b7ne", "tie\u00b7fe", "Rit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Sich in die Brust, und spritzt sein Blut", "tokens": ["Sich", "in", "die", "Brust", ",", "und", "spritzt", "sein", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Den Kindern in die d\u00fcrre Kehle.", "tokens": ["Den", "Kin\u00b7dern", "in", "die", "d\u00fcr\u00b7re", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Sie trinken froh den Purpursaft", "tokens": ["Sie", "trin\u00b7ken", "froh", "den", "Pur\u00b7pur\u00b7saft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und sch\u00f6pfen, wie vom frischen Oele", "tokens": ["Und", "sch\u00f6p\u00b7fen", ",", "wie", "vom", "fri\u00b7schen", "O\u00b7e\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.22": {"text": "Die seichte Lampe, neue Kraft.", "tokens": ["Die", "seich\u00b7te", "Lam\u00b7pe", ",", "neu\u00b7e", "Kraft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nur folgt dem schaurichten Befehle", "tokens": ["Nur", "folgt", "dem", "schau\u00b7rich\u00b7ten", "Be\u00b7feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.24": {"text": "Das j\u00fcngste nicht. Sein starrer Blick", "tokens": ["Das", "j\u00fcngs\u00b7te", "nicht", ".", "Sein", "star\u00b7rer", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "PTKNEG", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Klebt auf der Wunde; seine Seele", "tokens": ["Klebt", "auf", "der", "Wun\u00b7de", ";", "sei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Zerrei\u00dft ihr Band; es sinkt zur\u00fcck;", "tokens": ["Zer\u00b7rei\u00dft", "ihr", "Band", ";", "es", "sinkt", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Verh\u00fcllt sein Haupt mit seinem Fl\u00fcgel \u2013", "tokens": ["Ver\u00b7h\u00fcllt", "sein", "Haupt", "mit", "sei\u00b7nem", "Fl\u00fc\u00b7gel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und stirbt. Von dem geweihten H\u00fcgel", "tokens": ["Und", "stirbt", ".", "Von", "dem", "ge\u00b7weih\u00b7ten", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Schaut Vater Zevs mit stiller Lust", "tokens": ["Schaut", "Va\u00b7ter", "Zevs", "mit", "stil\u00b7ler", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "In dieses Heiligthum der Liebe.", "tokens": ["In", "die\u00b7ses", "Hei\u00b7lig\u00b7thum", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Er weint. Der g\u00f6ttlichste der Triebe,", "tokens": ["Er", "weint", ".", "Der", "g\u00f6tt\u00b7lichs\u00b7te", "der", "Trie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Das Mitleid, schwellt des R\u00e4chers Brust;", "tokens": ["Das", "Mit\u00b7leid", ",", "schwellt", "des", "R\u00e4\u00b7chers", "Brust", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Er wischt den Tod vom Augenliede", "tokens": ["Er", "wischt", "den", "Tod", "vom", "Au\u00b7gen\u00b7lie\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Des M\u00e4rtyrers. Der Pelikan", "tokens": ["Des", "M\u00e4r\u00b7ty\u00b7rers", ".", "Der", "Pe\u00b7li\u00b7kan"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Wacht gl\u00e4nzend auf, und der Chronide", "tokens": ["Wacht", "gl\u00e4n\u00b7zend", "auf", ",", "und", "der", "Chro\u00b7ni\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "PTKVZ", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Nimmt ihn zum zweyten Vogel an:", "tokens": ["Nimmt", "ihn", "zum", "zwey\u00b7ten", "Vo\u00b7gel", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Doch nicht als Diener seines Zornes,", "tokens": ["Doch", "nicht", "als", "Die\u00b7ner", "sei\u00b7nes", "Zor\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOUS", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Der mit dem Blitz bewafnet ist;", "tokens": ["Der", "mit", "dem", "Blitz", "be\u00b7waf\u00b7net", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Als Tr\u00e4ger jenes Segenhornes,", "tokens": ["Als", "Tr\u00e4\u00b7ger", "je\u00b7nes", "Se\u00b7gen\u00b7hor\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Das er auf fromme Kinder gie\u00dft.", "tokens": ["Das", "er", "auf", "from\u00b7me", "Kin\u00b7der", "gie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gesengt vom hei\u00dfen Mittagswind", "tokens": ["Ge\u00b7sengt", "vom", "hei\u00b7\u00dfen", "Mit\u00b7tags\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erstarb die Flur. Die Nymphe klagte", "tokens": ["Er\u00b7starb", "die", "Flur", ".", "Die", "Nym\u00b7phe", "klag\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am trocknen Quell; und t\u00e4glich jagte", "tokens": ["Am", 