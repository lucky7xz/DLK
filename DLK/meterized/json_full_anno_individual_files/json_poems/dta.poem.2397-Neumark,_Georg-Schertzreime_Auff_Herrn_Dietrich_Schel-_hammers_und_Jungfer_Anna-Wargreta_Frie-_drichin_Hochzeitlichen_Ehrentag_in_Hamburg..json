{"dta.poem.2397": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Schertzreime  \n  Auff Herrn Dietrich Schel-  \n hammers  \n und  \n  Jungfer Anna-Wargreta Frie-  \n drichin Hochzeitlichen Ehrentag in  \n Hamburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fckk zu Herr Br\u00e4utigam! gl\u00fckk zu zum neuen Leben/", "tokens": ["Gl\u00fckk", "zu", "Herr", "Br\u00e4u\u00b7ti\u00b7gam", "!", "gl\u00fckk", "zu", "zum", "neu\u00b7en", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NE", "$.", "ADJD", "APPR", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der grosse Gnadengott der woll\u2019 euch reichlich geben", "tokens": ["Der", "gros\u00b7se", "Gna\u00b7den\u00b7gott", "der", "woll'", "euch", "reich\u00b7lich", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Vor Gl", "tokens": ["Vor", "Gl"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Zu Rom erlanget hat; was Metellin begehret", "tokens": ["Zu", "Rom", "er\u00b7lan\u00b7get", "hat", ";", "was", "Me\u00b7tel\u00b7lin", "be\u00b7ge\u00b7hret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "VAFIN", "$.", "PWS", "NE", "VVFIN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Bey seiner Lebenszeit/ das sey auch Euch gewehret", "tokens": ["Bey", "sei\u00b7ner", "Le\u00b7bens\u00b7zeit", "/", "das", "sey", "auch", "Euch", "ge\u00b7weh\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PDS", "VAFIN", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vom hohen Himmel selbst. Der Anfang ist gemacht/", "tokens": ["Vom", "ho\u00b7hen", "Him\u00b7mel", "selbst", ".", "Der", "An\u00b7fang", "ist", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$.", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zu der Gl\u00fckseligkeit: Gott hat Euch zugebracht", "tokens": ["Zu", "der", "Gl\u00fck\u00b7se\u00b7lig\u00b7keit", ":", "Gott", "hat", "Euch", "zu\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "NN", "VAFIN", "PPER", "VVPP"], "meter": "--++-+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Solch ein beliebtes Mensch die gleichsam wie durchsternet", "tokens": ["Solch", "ein", "be\u00b7lieb\u00b7tes", "Mensch", "die", "gleich\u00b7sam", "wie", "durchs\u00b7ter\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN", "ART", "ADJD", "KOKOM", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit sch\u00f6nen Tugenden/ die gleichsam wie durchkernet", "tokens": ["Mit", "sch\u00f6\u00b7nen", "Tu\u00b7gen\u00b7den", "/", "die", "gleich\u00b7sam", "wie", "durch\u00b7ker\u00b7net"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "ART", "ADJD", "KOKOM", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit lauter Fr\u00f6mmigkeit; wer steht doch nicht das Licht", "tokens": ["Mit", "lau\u00b7ter", "Fr\u00f6m\u00b7mig\u00b7keit", ";", "wer", "steht", "doch", "nicht", "das", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$.", "PWS", "VVFIN", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der \u00e4dlen Keuschheit selbst in ihrem Angesicht\u2019/", "tokens": ["Der", "\u00e4d\u00b7len", "Keuschheit", "selbst", "in", "ih\u00b7rem", "An\u00b7ge\u00b7sicht'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Jm fall man Achtung giebt auf ihre fch\u00f6ne Sitten?", "tokens": ["Jm", "fall", "man", "Ach\u00b7tung", "giebt", "auf", "ih\u00b7re", "fch\u00f6\u00b7ne", "Sit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer urtheilt anders doch aus ihren sanfften Schritten", "tokens": ["Wer", "ur\u00b7theilt", "an\u00b7ders", "doch", "aus", "ih\u00b7ren", "sanff\u00b7ten", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als da\u00df sie stille sey/ die euch wird anvertraut?", "tokens": ["Als", "da\u00df", "sie", "stil\u00b7le", "sey", "/", "die", "euch", "wird", "an\u00b7ver\u00b7traut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVFIN", "VAFIN", "$(", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gl\u00fckk zu Herr Breutigam zu solcher lieben Braut.", "tokens": ["Gl\u00fckk", "zu", "Herr", "Breu\u00b7ti\u00b7gam", "zu", "sol\u00b7cher", "lie\u00b7ben", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NE", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mein schauet die Gestallt/ schaut ihre sch\u00f6ne Wangen/", "tokens": ["Mein", "schau\u00b7et", "die", "Ge\u00b7stallt", "/", "schaut", "ih\u00b7re", "sch\u00f6\u00b7ne", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ART", "NN", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Felder keuscher Zucht/ wie sie so lieblich prangen", "tokens": ["Die", "Fel\u00b7der", "keu\u00b7scher", "Zucht", "/", "wie", "sie", "so", "lieb\u00b7lich", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "PWAV", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mit Milch und Blut vermischt/ seht wie Sie ihren", "tokens": ["Mit", "Milch", "und", "Blut", "ver\u00b7mischt", "/", "seht", "wie", "Sie", "ih\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$(", "VVFIN", "KOKOM", "PPER", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Stral", "tokens": ["Stral"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Den liebbestrahlten Blitz viel hundert hundert mal", "tokens": ["Den", "lieb\u00b7be\u00b7strahl\u00b7ten", "Blitz", "viel", "hun\u00b7dert", "hun\u00b7dert", "mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "CARD", "CARD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auf euch verliebet wirfft. Dort jener Aganippen", "tokens": ["Auf", "euch", "ver\u00b7lie\u00b7bet", "wirfft", ".", "Dort", "je\u00b7ner", "A\u00b7gan\u00b7ip\u00b7pen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "VVFIN", "$.", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Schmekkt lang so s\u00fcsse nicht als ihre Rosenlippen.", "tokens": ["Schmekkt", "lang", "so", "s\u00fcs\u00b7se", "nicht", "als", "ih\u00b7re", "Ro\u00b7sen\u00b7lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "VVFIN", "PTKNEG", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "(welchs Jhr Herr Breutigam am aller besten wisst", "tokens": ["(", "welchs", "Ihr", "Herr", "Breu\u00b7ti\u00b7gam", "am", "al\u00b7ler", "bes\u00b7ten", "wisst"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAT", "PPOSAT", "NN", "NE", "APPRART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Weil euch die Prob\u2019 allein nur zugelassen ist.)", "tokens": ["Weil", "euch", "die", "Prob'", "al\u00b7lein", "nur", "zu\u00b7ge\u00b7las\u00b7sen", "ist", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich gl\u00e4ube sicherlich/ wen sich anitzo f\u00fcnde", "tokens": ["Ich", "gl\u00e4u\u00b7be", "si\u00b7cher\u00b7lich", "/", "wen", "sich", "a\u00b7nit\u00b7zo", "f\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$(", "PWS", "PRF", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Klisthenis Tochter selbst/ und hier zugegen st\u00fcnde", "tokens": ["Klist\u00b7he\u00b7nis", "Toch\u00b7ter", "selbst", "/", "und", "hier", "zu\u00b7ge\u00b7gen", "st\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADV", "$(", "KON", "ADV", "ADJD", "ADJA"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.28": {"text": "Das sch\u00f6ne Griechenbild/ Sie w\u00fcrde warlich frey", "tokens": ["Das", "sch\u00f6\u00b7ne", "Grie\u00b7chen\u00b7bild", "/", "Sie", "w\u00fcr\u00b7de", "war\u00b7lich", "frey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Bekennen/ da\u00df Sie nun weit \u00fcbertroffen sey.", "tokens": ["Be\u00b7ken\u00b7nen", "/", "da\u00df", "Sie", "nun", "weit", "\u00fc\u00b7ber\u00b7trof\u00b7fen", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und zwar was ihre Zier/ was ihre wehrte Tugend/", "tokens": ["Und", "zwar", "was", "ih\u00b7re", "Zier", "/", "was", "ih\u00b7re", "wehr\u00b7te", "Tu\u00b7gend", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWS", "PPOSAT", "NN", "$(", "PWS", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Was Leibsgeschiklichkeit/ was rechtbejahrte Jugend", "tokens": ["Was", "Leibs\u00b7ge\u00b7schik\u00b7lich\u00b7keit", "/", "was", "recht\u00b7be\u00b7jahr\u00b7te", "Ju\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "$(", "PWS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und Sch\u00f6nheit anbelangt/ an ihr die euch vertraut", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "an\u00b7be\u00b7langt", "/", "an", "ihr", "die", "euch", "ver\u00b7traut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "$(", "APPR", "PPOSAT", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Gl\u00fckk zu Herr Breutigam zu solcher sch\u00f6nen Braut!", "tokens": ["Gl\u00fckk", "zu", "Herr", "Breu\u00b7ti\u00b7gam", "zu", "sol\u00b7cher", "sch\u00f6\u00b7nen", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "NE", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich\u2019 merk es von ihr ab/ ihr werdet an ihr haben", "tokens": ["Ich'", "merk", "es", "von", "ihr", "ab", "/", "ihr", "wer\u00b7det", "an", "ihr", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$(", "PPER", "VAFIN", "APPR", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ein solch Mensch/ die euch wird nach Hertzenswunsche", "tokens": ["Ein", "solch", "Mensch", "/", "die", "euch", "wird", "nach", "Hert\u00b7zens\u00b7wun\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$(", "PRELS", "PPER", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "laben.", "tokens": ["la\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Man kans ja leichtlich sehn. Sie gibt es an den Tag/", "tokens": ["Man", "kans", "ja", "leicht\u00b7lich", "sehn", ".", "Sie", "gibt", "es", "an", "den", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "VVINF", "$.", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Durch einen keuschen Blikk/ was Sie ersreuen mag.", "tokens": ["Durch", "ei\u00b7nen", "keu\u00b7schen", "Blikk", "/", "was", "Sie", "er\u00b7sreu\u00b7en", "mag."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "PWS", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Perikles der ist nie nicht aus dem Hause gangen", "tokens": ["Pe\u00b7rik\u00b7les", "der", "ist", "nie", "nicht", "aus", "dem", "Hau\u00b7se", "gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "VAFIN", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Er hat znvor aus Lieb\u2019 Aspasten \u00fcmfangen", "tokens": ["Er", "hat", "zn\u00b7vor", "aus", "Lieb'", "As\u00b7pas\u00b7ten", "\u00fcm\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "NN", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Sein liebes Ehgemahl nach lieblichem Gebrauch.", "tokens": ["Sein", "lie\u00b7bes", "Eh\u00b7ge\u00b7mahl", "nach", "lieb\u00b7li\u00b7chem", "Ge\u00b7brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wolan so machet es mit eurem Liebchen auch.", "tokens": ["Wo\u00b7lan", "so", "ma\u00b7chet", "es", "mit", "eu\u00b7rem", "Lieb\u00b7chen", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "(jhr wisst wol was noch mehr) ich wei\u00df Sie wird her", "tokens": ["(", "jhr", "wisst", "wol", "was", "noch", "mehr", ")", "ich", "wei\u00df", "Sie", "wird", "her"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "PWS", "ADV", "ADV", "$(", "PPER", "VVFIN", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "gegen", "tokens": ["ge\u00b7gen"], "token_info": ["word"], "pos": ["APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.45": {"text": "Euch die Versprechung thun/ da\u00df Sie auf allen Wegen/", "tokens": ["Euch", "die", "Ver\u00b7spre\u00b7chung", "thun", "/", "da\u00df", "Sie", "auf", "al\u00b7len", "We\u