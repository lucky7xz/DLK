{"dta.poem.43": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die schnell entstandnen und  \n schnell vergangnen Wasserblasen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sa\u00df an einen gr\u00fcnen Rande", "tokens": ["Ich", "sa\u00df", "an", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Baches, der mit gelben Sande", "tokens": ["Des", "Ba\u00b7ches", ",", "der", "mit", "gel\u00b7ben", "San\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf seinen Boden angef\u00fcllt,", "tokens": ["Auf", "sei\u00b7nen", "Bo\u00b7den", "an\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wasser das dar\u00fcber quillt", "tokens": ["Das", "Was\u00b7ser", "das", "da\u00b7r\u00fc\u00b7ber", "quillt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PAV", "VVFIN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Das sprudelte auf Kieselsteinen,", "tokens": ["Das", "spru\u00b7del\u00b7te", "auf", "Kie\u00b7sel\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da sah ich Wasser-Blasen scheinen.", "tokens": ["Da", "sah", "ich", "Was\u00b7ser\u00b7Bla\u00b7sen", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Blasen hat ich kaum gesehen,", "tokens": ["Die", "Bla\u00b7sen", "hat", "ich", "kaum", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie sich in die R\u00fcnde drehen,", "tokens": ["Wie", "sie", "sich", "in", "die", "R\u00fcn\u00b7de", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit manchen Farben lieblich spieln,", "tokens": ["Mit", "man\u00b7chen", "Far\u00b7ben", "lieb\u00b7lich", "spieln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sie gleich von einander fieln,", "tokens": ["Da", "sie", "gleich", "von", "ein\u00b7an\u00b7der", "fi\u00b7eln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PRF", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und im zerplatzen sich verlohren,", "tokens": ["Und", "im", "zer\u00b7plat\u00b7zen", "sich", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "VVFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da sie ein Wasserfall gebohren.", "tokens": ["Da", "sie", "ein", "Was\u00b7ser\u00b7fall", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich dachte so sind auch die Blikke,", "tokens": ["Ich", "dach\u00b7te", "so", "sind", "auch", "die", "Blik\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von einem scheinenden Gel\u00fckke", "tokens": ["Von", "ei\u00b7nem", "schei\u00b7nen\u00b7den", "Ge\u00b7l\u00fck\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das oft im Lebenslauf entsteht", "tokens": ["Das", "oft", "im", "Le\u00b7bens\u00b7lauf", "ent\u00b7steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und pl\u00f6zlich wiederum vergeht.", "tokens": ["Und", "pl\u00f6z\u00b7lich", "wie\u00b7de\u00b7rum", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es lacht uns an, als wie verg\u00fcldet,", "tokens": ["Es", "lacht", "uns", "an", ",", "als", "wie", "ver\u00b7g\u00fcl\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "KOKOM", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ist doch blos aus Schaum gebildet.", "tokens": ["Und", "ist", "doch", "blos", "aus", "Schaum", "ge\u00b7bil\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}