{"dta.poem.19473": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Jetzt rede du!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Du warest mir ein t\u00e4glich Wanderziel,", "tokens": ["Du", "wa\u00b7rest", "mir", "ein", "t\u00e4g\u00b7lich", "Wan\u00b7der\u00b7ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Viellieber Wald, in dumpfen Jugendtagen,", "tokens": ["Viel\u00b7lie\u00b7ber", "Wald", ",", "in", "dum\u00b7pfen", "Ju\u00b7gend\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich hatte dir getr\u00e4umten Gl\u00fccks so viel", "tokens": ["Ich", "hat\u00b7te", "dir", "ge\u00b7tr\u00e4um\u00b7ten", "Gl\u00fccks", "so", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Anzuvertraun, so wahren Schmerz zu klagen.", "tokens": ["An\u00b7zu\u00b7ver\u00b7traun", ",", "so", "wah\u00b7ren", "Schmerz", "zu", "kla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und wieder such' ich dich, du dunkler Hort,", "tokens": ["Und", "wie\u00b7der", "such'", "ich", "dich", ",", "du", "dunk\u00b7ler", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und deines Wipfelmeers gewaltig Rauschen \u2014", "tokens": ["Und", "dei\u00b7nes", "Wip\u00b7fel\u00b7meers", "ge\u00b7wal\u00b7tig", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt rede du! Ich lasse dir das Wort!", "tokens": ["Jetzt", "re\u00b7de", "du", "!", "Ich", "las\u00b7se", "dir", "das", "Wort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verstummt ist Klag' und Jubel. Ich will lauschen.", "tokens": ["Ver\u00b7stummt", "ist", "Klag'", "und", "Ju\u00b7bel", ".", "Ich", "will", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NN", "$.", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}