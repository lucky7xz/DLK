{"dta.poem.20484": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erbarme dich/ du sch\u00f6nheit dieser welt/", "tokens": ["Er\u00b7bar\u00b7me", "dich", "/", "du", "sch\u00f6n\u00b7heit", "die\u00b7ser", "welt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$(", "PPER", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und nimm von mir die f\u00e4ssel meiner seelen!", "tokens": ["Und", "nimm", "von", "mir", "die", "f\u00e4s\u00b7sel", "mei\u00b7ner", "see\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPER", "ART", "NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn stahl und ei\u00df die brust umschlossen h\u00e4lt/", "tokens": ["Wenn", "stahl", "und", "ei\u00df", "die", "brust", "um\u00b7schlos\u00b7sen", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "KON", "ADJD", "ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch sclaverey mich auff den todt zu qv\u00e4len/", "tokens": ["Durch", "scla\u00b7ve\u00b7rey", "mich", "auff", "den", "todt", "zu", "qv\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPR", "ART", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So dencke doch/ der ursprung meiner noth", "tokens": ["So", "den\u00b7cke", "doch", "/", "der", "ur\u00b7sprung", "mei\u00b7ner", "noth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "$(", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist schon der tod.", "tokens": ["Ist", "schon", "der", "tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ist schon der tod ein opffer deiner lust/", "tokens": ["Ist", "schon", "der", "tod", "ein", "opf\u00b7fer", "dei\u00b7ner", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und soll mein hertz in heisser glut verbrennen;", "tokens": ["Und", "soll", "mein", "hertz", "in", "heis\u00b7ser", "glut", "ver\u00b7bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wollst du nur die alabaster-brust", "tokens": ["So", "wollst", "du", "nur", "die", "a\u00b7la\u00b7bas\u00b7ter\u00b7brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu guter letzt mir zum altare g\u00f6nnen.", "tokens": ["Zu", "gu\u00b7ter", "letzt", "mir", "zum", "al\u00b7ta\u00b7re", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADV", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn ein altar zum opffern mu\u00df ja seyn", "tokens": ["Denn", "ein", "al\u00b7tar", "zum", "opf\u00b7fern", "mu\u00df", "ja", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VMFIN", "ADV", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein harter stein.", "tokens": ["Ein", "har\u00b7ter", "stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ein harter stein wird durch die tropffen weich:", "tokens": ["Ein", "har\u00b7ter", "stein", "wird", "durch", "die", "tropf\u00b7fen", "weich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein auge l\u00e4st auch gantze str\u00f6hme rinnen;", "tokens": ["Mein", "au\u00b7ge", "l\u00e4st", "auch", "gant\u00b7ze", "str\u00f6h\u00b7me", "rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jedoch umsonst. Wem bist du endlich gleich?", "tokens": ["Je\u00b7doch", "um\u00b7sonst", ".", "Wem", "bist", "du", "end\u00b7lich", "gleich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PWS", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erweicht nur blut die diamanten sinnen?", "tokens": ["Er\u00b7weicht", "nur", "blut", "die", "di\u00b7a\u00b7man\u00b7ten", "sin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So k\u00fchle dann darinnen deinen muth/", "tokens": ["So", "k\u00fch\u00b7le", "dann", "da\u00b7rin\u00b7nen", "dei\u00b7nen", "muth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hier ist mein blut.", "tokens": ["Hier", "ist", "mein", "blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Hier ist mein blut/ das treu und liebe weyht/", "tokens": ["Hier", "ist", "mein", "blut", "/", "das", "treu", "und", "lie\u00b7be", "weyht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$(", "ART", "ADJD", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die mich zuerst in sclaverey gest\u00fcrtzet.", "tokens": ["Die", "mich", "zu\u00b7erst", "in", "scla\u00b7ve\u00b7rey", "ge\u00b7st\u00fcrt\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was brachte mich um meine g\u00fcldne zeit?", "tokens": ["Was", "brach\u00b7te", "mich", "um", "mei\u00b7ne", "g\u00fcld\u00b7ne", "zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wer hat den weg der freyheit abgek\u00fcrtzet?", "tokens": ["Wer", "hat", "den", "weg", "der", "frey\u00b7heit", "ab\u00b7ge\u00b7k\u00fcrt\u00b7zet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der rauber war/ ach da\u00df ichs sagen mu\u00df!", "tokens": ["Der", "rau\u00b7ber", "war", "/", "ach", "da\u00df", "ichs", "sa\u00b7gen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "XY", "KOUS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein eintzger ku\u00df.", "tokens": ["Ein", "eintz\u00b7ger", "ku\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ein eintzger ku\u00df legt mir die ketten an/", "tokens": ["Ein", "eintz\u00b7ger", "ku\u00df", "legt", "mir", "die", "ket\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "VVFIN", "PTKVZ", "$("], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So kan mich auch ein ku\u00df davon entbinden.", "tokens": ["So", "kan", "mich", "auch", "ein", "ku\u00df", "da\u00b7von", "ent\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wiewohl der wunsch ist gantz umsonst gethan/", "tokens": ["Wie\u00b7wohl", "der", "wunsch", "ist", "gantz", "um\u00b7sonst", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein seuffzer soll in tauber lufft verschwinden.", "tokens": ["Mein", "seuff\u00b7zer", "soll", "in", "tau\u00b7ber", "lufft", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So mu\u00df mein hertz mit doppelt schwerer pein", "tokens": ["So", "mu\u00df", "mein", "hertz", "mit", "dop\u00b7pelt", "schwe\u00b7rer", "pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gestraffet seyn.", "tokens": ["Ge\u00b7straf\u00b7fet", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Gestraffet seyn/ und ohne missethat/", "tokens": ["Ge\u00b7straf\u00b7fet", "seyn", "/", "und", "oh\u00b7ne", "mis\u00b7se\u00b7that", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "$(", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird auch kein gott vor recht und billig sprechen.", "tokens": ["Wird", "auch", "kein", "gott", "vor", "recht", "und", "bil\u00b7lig", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "APPR", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wei\u00df ich nicht/ wer mich gestraffet hat;", "tokens": ["Doch", "wei\u00df", "ich", "nicht", "/", "wer", "mich", "ge\u00b7straf\u00b7fet", "hat", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$(", "PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vielleicht will sich der himmel an mir r\u00e4chen.", "tokens": ["Viel\u00b7leicht", "will", "sich", "der", "him\u00b7mel", "an", "mir", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das macht/ du warst/ und mehr als er/ zugleich", "tokens": ["Das", "macht", "/", "du", "warst", "/", "und", "mehr", "als", "er", "/", "zu\u00b7gleich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "$(", "PPER", "VAFIN", "$(", "KON", "ADV", "KOUS", "PPER", "$(", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein himmelreich.", "tokens": ["Mein", "him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Mein himmelreich wird mir zur h\u00f6llen-pein.", "tokens": ["Mein", "him\u00b7mel\u00b7reich", "wird", "mir", "zur", "h\u00f6l\u00b7len\u00b7pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich soll und mu\u00df/ ich will auch nur verderben;", "tokens": ["Ich", "soll", "und", "mu\u00df", "/", "ich", "will", "auch", "nur", "ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "$(", "PPER", "VMFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die freyheit wird in meinem grabe seyn;", "tokens": ["Die", "frey\u00b7heit", "wird", "in", "mei\u00b7nem", "gra\u00b7be", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Drum will ich gern als sclave grausam sterben.", "tokens": ["Drum", "will", "ich", "gern", "als", "scla\u00b7ve", "grau\u00b7sam", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "KOKOM", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mein hertz giebt nur den seuffzer noch von sich:", "tokens": ["Mein", "hertz", "giebt", "nur", "den", "seuff\u00b7zer", "noch", "von", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Erbarme dich!", "tokens": ["Er\u00b7bar\u00b7me", "dich", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}