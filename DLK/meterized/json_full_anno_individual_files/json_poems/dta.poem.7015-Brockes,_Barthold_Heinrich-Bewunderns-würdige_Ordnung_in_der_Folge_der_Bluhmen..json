{"dta.poem.7015": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bewunderns-w\u00fcrdige Ordnung  \n in der Folge der Bluhmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch seh' ich einen neuen Strahl", "tokens": ["Noch", "seh'", "ich", "ei\u00b7nen", "neu\u00b7en", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von GOTTES Weisheit, Macht und Liebe.", "tokens": ["Von", "GoT\u00b7TES", "Weis\u00b7heit", ",", "Macht", "und", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn W\u00e4rm\u2019 und Feuchtigkeit die Bluhmen sonder Zahl,", "tokens": ["Wenn", "W\u00e4rm'", "und", "Feuch\u00b7tig\u00b7keit", "die", "Bluh\u00b7men", "son\u00b7der", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht nach einander, auf einmahl,", "tokens": ["Nicht", "nach", "ein\u00b7an\u00b7der", ",", "auf", "ein\u00b7mahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADV", "$,", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie es nat\u00fcrlich scheint, aus Erd\u2019 und Saamen triebe;", "tokens": ["Wie", "es", "na\u00b7t\u00fcr\u00b7lich", "scheint", ",", "aus", "Erd'", "und", "Saa\u00b7men", "trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So folgte ja gewi\u00df: es bliebe", "tokens": ["So", "folg\u00b7te", "ja", "ge\u00b7wi\u00df", ":", "es", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Welt geschm\u00fcckt nur blos auf eine kurtze Zeit:", "tokens": ["Die", "Welt", "ge\u00b7schm\u00fcckt", "nur", "blos", "auf", "ei\u00b7ne", "kurt\u00b7ze", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hernach w\u00fcrd\u2019 alles weit und breit,", "tokens": ["Her\u00b7nach", "w\u00fcrd'", "al\u00b7les", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ver\u00f6det, w\u00fcst und traurig stehen.", "tokens": ["Ver\u00b7\u00f6\u00b7det", ",", "w\u00fcst", "und", "trau\u00b7rig", "ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da wir hingegen jetzt so ordentlich,", "tokens": ["Da", "wir", "hin\u00b7ge\u00b7gen", "jetzt", "so", "or\u00b7dent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wenn eine Bluhme welckt, und sich", "tokens": ["Wenn", "ei\u00b7ne", "Bluh\u00b7me", "welckt", ",", "und", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "KON", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Verliert, ein\u2019 andre kommen sehen.", "tokens": ["Ver\u00b7liert", ",", "ein'", "and\u00b7re", "kom\u00b7men", "se\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "PIS", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "F\u00fcr solche Wunder, die allein", "tokens": ["F\u00fcr", "sol\u00b7che", "Wun\u00b7der", ",", "die", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Durch Deine weise Macht, zu unsrer Lust geschehen,", "tokens": ["Durch", "Dei\u00b7ne", "wei\u00b7se", "Macht", ",", "zu", "uns\u00b7rer", "Lust", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "La\u00df uns, o Sch\u00f6pfer, Dich bewundernd, danckbar seyn!", "tokens": ["La\u00df", "uns", ",", "o", "Sch\u00f6p\u00b7fer", ",", "Dich", "be\u00b7wun\u00b7dernd", ",", "dan\u00b7ck\u00b7bar", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$,", "PPER", "VVPP", "$,", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}