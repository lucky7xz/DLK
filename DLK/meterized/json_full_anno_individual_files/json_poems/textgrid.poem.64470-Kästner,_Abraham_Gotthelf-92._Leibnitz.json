{"textgrid.poem.64470": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "92. Leibnitz", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von mir ward ", "tokens": ["Von", "mir", "ward"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Warf Sachsen einst Hannover vor:", "tokens": ["Warf", "Sach\u00b7sen", "einst", "Han\u00b7no\u00b7ver", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir, sprach Cheruscien, hie\u00df ihn der Zufall leben,", "tokens": ["Dir", ",", "sprach", "Che\u00b7rus\u00b7ci\u00b7en", ",", "hie\u00df", "ihn", "der", "Zu\u00b7fall", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "NE", "$,", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mir, sein erkannter Werth, nach dem ich ihn erkor;", "tokens": ["Mir", ",", "sein", "er\u00b7kann\u00b7ter", "Werth", ",", "nach", "dem", "ich", "ihn", "er\u00b7kor", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Gl\u00fcck gab dir ihn erst, du liessest dir ihn nehmen,", "tokens": ["Das", "Gl\u00fcck", "gab", "dir", "ihn", "erst", ",", "du", "lies\u00b7sest", "dir", "ihn", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "w