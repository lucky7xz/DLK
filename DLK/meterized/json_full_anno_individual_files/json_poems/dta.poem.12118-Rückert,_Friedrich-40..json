{"dta.poem.12118": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Held, du bist im Kampf f\u00fcrs Vaterland gefallen,", "tokens": ["O", "Held", ",", "du", "bist", "im", "Kampf", "f\u00fcrs", "Va\u00b7ter\u00b7land", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "APPRART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum steht dein Bild mit Recht hier in des Tempels Hallen.", "tokens": ["Drum", "steht", "dein", "Bild", "mit", "Recht", "hier", "in", "des", "Tem\u00b7pels", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verrathen hat man dich, geopfert dich im Leben;", "tokens": ["Ver\u00b7ra\u00b7then", "hat", "man", "dich", ",", "ge\u00b7o\u00b7pfert", "dich", "im", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "PPER", "$,", "VVPP", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur S\u00fchnung mu\u00dfte man dich so im Tod erheben.", "tokens": ["Zur", "S\u00fch\u00b7nung", "mu\u00df\u00b7te", "man", "dich", "so", "im", "Tod", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PIS", "PRF", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Heil dir! wie hochgeehrt du k\u00f6nntest stehn auf Erden,", "tokens": ["Heil", "dir", "!", "wie", "hoch\u00b7geehrt", "du", "k\u00f6nn\u00b7test", "stehn", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PWAV", "VVFIN", "PPER", "VMFIN", "VVINF", "APPR", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Zum Heil'gen konntest du doch nur als M\u00e4rtrer werden.", "tokens": ["Zum", "Heil'\u00b7gen", "konn\u00b7test", "du", "doch", "nur", "als", "M\u00e4r\u00b7trer", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "ADV", "ADV", "KOUS", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}