{"dta.poem.11871": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh wie das Aehrenfeld vom goldnen Abendduft", "tokens": ["Sieh", "wie", "das", "A\u00b7eh\u00b7ren\u00b7feld", "vom", "gold\u00b7nen", "A\u00b7bend\u00b7duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "---+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Befriedigt schweigt, und tief heraus die Wachtel ruft.", "tokens": ["Be\u00b7frie\u00b7digt", "schweigt", ",", "und", "tief", "he\u00b7raus", "die", "Wach\u00b7tel", "ruft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "KON", "ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie ruft: So lange hab' ich euer Feld geh\u00fctet,", "tokens": ["Sie", "ruft", ":", "So", "lan\u00b7ge", "hab'", "ich", "eu\u00b7er", "Feld", "ge\u00b7h\u00fc\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun h\u00fct' ich's euch nicht mehr, denn ich hab' ausgebr\u00fctet.", "tokens": ["Nun", "h\u00fct'", "ich's", "euch", "nicht", "mehr", ",", "denn", "ich", "hab'", "aus\u00b7ge\u00b7br\u00fc\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "PTKNEG", "ADV", "$,", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Habt Dank, da\u00df ihr geschont, solang ich hier gewohnt;", "tokens": ["Habt", "Dank", ",", "da\u00df", "ihr", "ge\u00b7schont", ",", "so\u00b7lang", "ich", "hier", "ge\u00b7wohnt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "VVPP", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kommt, erntet nun, und seid von Segen reich belohnt!", "tokens": ["Kommt", ",", "ern\u00b7tet", "nun", ",", "und", "seid", "von", "Se\u00b7gen", "reich", "be\u00b7lohnt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "$,", "KON", "VAFIN", "APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Aehren nicken drein im letzten Abendschein,", "tokens": ["Die", "A\u00b7eh\u00b7ren", "ni\u00b7cken", "drein", "im", "letz\u00b7ten", "A\u00b7bend\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Geerntet wollen sie am n\u00e4chsten Morgen seyn.", "tokens": ["Ge\u00b7ern\u00b7tet", "wol\u00b7len", "sie", "am", "n\u00e4chs\u00b7ten", "Mor\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VMFIN", "PPER", "APPRART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Vor einem andern Klang verstummt der Wachtel Sang,", "tokens": ["Vor", "ei\u00b7nem", "an\u00b7dern", "Klang", "ver\u00b7stummt", "der", "Wach\u00b7tel", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sicheln h\u00e4mmert man das stille Dorf entlang.", "tokens": ["Die", "Si\u00b7cheln", "h\u00e4m\u00b7mert", "man", "das", "stil\u00b7le", "Dorf", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "O k\u00f6nnten wir es froh erwarten wie die reifen,", "tokens": ["O", "k\u00f6nn\u00b7ten", "wir", "es", "froh", "er\u00b7war\u00b7ten", "wie", "die", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "KOKOM", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn \u00fcber Nacht man so wird uns die Sichel schleifen.", "tokens": ["Wenn", "\u00fc\u00b7ber", "Nacht", "man", "so", "wird", "uns", "die", "Si\u00b7chel", "schlei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PIS", "ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}