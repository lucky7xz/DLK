{"textgrid.poem.65042": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doch eh' ich, um den Kranz mir zu erstreben,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch eh' ich, um den Kranz mir zu erstreben,", "tokens": ["Doch", "eh'", "ich", ",", "um", "den", "Kranz", "mir", "zu", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "$,", "KOUI", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um heimzukehren mit dem gold'nen Vlie\u00dfe,", "tokens": ["Um", "heim\u00b7zu\u00b7keh\u00b7ren", "mit", "dem", "gold'\u00b7nen", "Vlie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "VVIZU", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von meinem Selbst herunter dingen lie\u00dfe,", "tokens": ["Von", "mei\u00b7nem", "Selbst", "her\u00b7un\u00b7ter", "din\u00b7gen", "lie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und L\u00fcge br\u00e4chte in mein innres Leben.", "tokens": ["Und", "L\u00fc\u00b7ge", "br\u00e4ch\u00b7te", "in", "mein", "inn\u00b7res", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Eh' ich die Stimme, die mir Gott gegeben,", "tokens": ["Eh'", "ich", "die", "Stim\u00b7me", ",", "die", "mir", "Gott", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu fremden Weisen sich bequemen hie\u00dfe,", "tokens": ["Zu", "frem\u00b7den", "Wei\u00b7sen", "sich", "be\u00b7que\u00b7men", "hie\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kehrt' ich den R\u00fccken jenem Paradiese", "tokens": ["Kehrt'", "ich", "den", "R\u00fc\u00b7cken", "je\u00b7nem", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und s\u00e4he stolz den Beifall mir entschweben.", "tokens": ["Und", "s\u00e4\u00b7he", "stolz", "den", "Bei\u00b7fall", "mir", "ent\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Eh' wollt' ich einsam in der W\u00fcste singen,", "tokens": ["Eh'", "wollt'", "ich", "ein\u00b7sam", "in", "der", "W\u00fcs\u00b7te", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eh' lie\u00df ich tr\u00e4umend meines Liedes Laute,", "tokens": ["Eh'", "lie\u00df", "ich", "tr\u00e4u\u00b7mend", "mei\u00b7nes", "Lie\u00b7des", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von allen Menschen ungeh\u00f6rt verklingen!", "tokens": ["Von", "al\u00b7len", "Men\u00b7schen", "un\u00b7ge\u00b7h\u00f6rt", "ver\u00b7klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das einz'ge Ziel, nach dem mein Auge schaute,", "tokens": ["Das", "einz'\u00b7ge", "Ziel", ",", "nach", "dem", "mein", "Au\u00b7ge", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es w\u00e4re: unentweiht zur\u00fcckzubringen", "tokens": ["Es", "w\u00e4\u00b7re", ":", "un\u00b7ent\u00b7weiht", "zu\u00b7r\u00fcck\u00b7zu\u00b7brin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "$.", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das heil'ge Pfand, das Gott mir anvertraute! \u2013", "tokens": ["Das", "heil'\u00b7ge", "Pfand", ",", "das", "Gott", "mir", "an\u00b7ver\u00b7trau\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch eh' ich, um den Kranz mir zu erstreben,", "tokens": ["Doch", "eh'", "ich", ",", "um", "den", "Kranz", "mir", "zu", "er\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER"