{"textgrid.poem.49120": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Vom Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bin ich, Gott, doch gegen dich,", "tokens": ["Wer", "bin", "ich", ",", "Gott", ",", "doch", "ge\u00b7gen", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dennoch unterwind' ich mich", "tokens": ["Und", "den\u00b7noch", "un\u00b7ter\u00b7wind'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dir mich \u00f6ffters zu bereden.", "tokens": ["Mit", "dir", "mich", "\u00f6ff\u00b7ters", "zu", "be\u00b7re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darff Staub und Asch, darff d\u00fcrres Heu,", "tokens": ["Darff", "Staub", "und", "Asch", ",", "darff", "d\u00fcr\u00b7res", "Heu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "$,", "VMFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Darff fl\u00fcchtigs Laub und \u00f6de Spreu,", "tokens": ["Darff", "fl\u00fcch\u00b7tigs", "Laub", "und", "\u00f6\u00b7de", "Spreu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Mensch, ach Herr, sich nicht entbl\u00f6den,", "tokens": ["Ein", "Mensch", ",", "ach", "Herr", ",", "sich", "nicht", "ent\u00b7bl\u00f6\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "XY", "NN", "$,", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er, o Sch\u00f6pffer aller Welt,", "tokens": ["Da\u00df", "er", ",", "o", "Sch\u00f6pf\u00b7fer", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dich, wenn er will, zu Rede stellt?", "tokens": ["Dich", ",", "wenn", "er", "will", ",", "zu", "Re\u00b7de", "stellt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VMFIN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was fehlt mir, auch wie schlecht es ist,", "tokens": ["Was", "fehlt", "mir", ",", "auch", "wie", "schlecht", "es", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADV", "KOKOM", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum dich mein Herz nicht frey begr\u00fcst!", "tokens": ["Drum", "dich", "mein", "Herz", "nicht", "frey", "be\u00b7gr\u00fcst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bin doch nie dir zu geringe.", "tokens": ["Und", "bin", "doch", "nie", "dir", "zu", "ge\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zutritt hat nicht Maa\u00df noch Ziel,", "tokens": ["Der", "Zu\u00b7tritt", "hat", "nicht", "Maa\u00df", "noch", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Bitte bittet nie zu viel,", "tokens": ["Die", "Bit\u00b7te", "bit\u00b7tet", "nie", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und klopfft nicht, da\u00df sie nicht durchdringe,", "tokens": ["Und", "klopfft", "nicht", ",", "da\u00df", "sie", "nicht", "durch\u00b7drin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist, da\u00df mein Wunsch dein' Hoheit scheut,", "tokens": ["Ist", ",", "da\u00df", "mein", "Wunsch", "dein'", "Ho\u00b7heit", "scheut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So stehstu, eh er k\u00f6mmt, bereit.", "tokens": ["So", "steh\u00b7stu", ",", "eh", "er", "k\u00f6mmt", ",", "be\u00b7reit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Wunder aller Lieb' und G\u00fct,", "tokens": ["O", "Wun\u00b7der", "al\u00b7ler", "Lieb'", "und", "G\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ist ein Mensch von dem Gem\u00fcth?", "tokens": ["Wo", "ist", "ein", "Mensch", "von", "dem", "Ge\u00b7m\u00fcth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie l\u00e4sset sich die Welt doch feyren,", "tokens": ["Wie", "l\u00e4s\u00b7set", "sich", "die", "Welt", "doch", "fey\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eh man sie anzusprechen kriegt,", "tokens": ["Eh", "man", "sie", "an\u00b7zu\u00b7spre\u00b7chen", "kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch mehr, wenn man nicht leicht vergn\u00fcgt,", "tokens": ["Noch", "mehr", ",", "wenn", "man", "nicht", "leicht", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PIS", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie wei\u00df sie, was sie giebt, zu s\u00e4uren,", "tokens": ["Wie", "wei\u00df", "sie", ",", "was", "sie", "giebt", ",", "zu", "s\u00e4u\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und hebt mans doch nicht Himmelan,", "tokens": ["Und", "hebt", "mans", "doch", "nicht", "Him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Heist man ein undankbarer Mann.", "tokens": ["Heist", "man", "ein", "un\u00b7dank\u00b7ba\u00b7rer", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "An dich und dein erhabnes Hau\u00df", "tokens": ["An", "dich", "und", "dein", "er\u00b7hab\u00b7nes", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schick' ich nur einen Seuffzer au\u00df,", "tokens": ["Schick'", "ich", "nur", "ei\u00b7nen", "Seuff\u00b7zer", "au\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So komm ich an und werd' erh\u00f6ret.", "tokens": ["So", "komm", "ich", "an", "und", "werd'", "er\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir Erdenklo\u00df ist das Gesicht,", "tokens": ["Mir", "Er\u00b7den\u00b7klo\u00df", "ist", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Daf\u00fcr der Cherub deckt sein Licht,", "tokens": ["Da\u00b7f\u00fcr", "der", "Che\u00b7rub", "deckt", "sein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nicht die heilge Stett verwehret,", "tokens": ["Und", "nicht", "die", "heil\u00b7ge", "Stett", "ver\u00b7weh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo der, dem alle Herrschaft frohnt,", "tokens": ["Wo", "der", ",", "dem", "al\u00b7le", "Herr\u00b7schaft", "frohnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In unbeschriebnen Ehren wohnt.", "tokens": ["In", "un\u00b7be\u00b7schrieb\u00b7nen", "Eh\u00b7ren", "wohnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bedarff wer viel, wer ist dem gut?", "tokens": ["Be\u00b7darff", "wer", "viel", ",", "wer", "ist", "dem", "gut", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ADV", "$,", "PWS", "VAFIN", "ART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Besitzt wer viel, wie bl\u00e4st sein Muth!", "tokens": ["Be\u00b7sitzt", "wer", "viel", ",", "wie", "bl\u00e4st", "sein", "Muth", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PWS", "ADV", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du, o der ganzen Welt Berather,", "tokens": ["Du", ",", "o", "der", "gan\u00b7zen", "Welt", "Be\u00b7ra\u00b7ther", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hast viel und giebst doch f\u00fcr und f\u00fcr,", "tokens": ["Hast", "viel", "und", "giebst", "doch", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KON", "VVFIN", "ADV", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Je \u00e4rmer wer, je lieber dir.", "tokens": ["Je", "\u00e4r\u00b7mer", "wer", ",", "je", "lie\u00b7ber", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PWS", "$,", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O reicher Gott und milder Vater,", "tokens": ["O", "rei\u00b7cher", "Gott", "und", "mil\u00b7der", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "O h\u00f6chstes und auch tiefstes Gut,", "tokens": ["O", "h\u00f6chs\u00b7tes", "und", "auch", "tiefs\u00b7tes", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das nie sich spaart und nie verthut!", "tokens": ["Das", "nie", "sich", "spaart", "und", "nie", "ver\u00b7thut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PRF", "ADJD", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schreckt Satan mich, so zeigstu dich,", "tokens": ["Schreckt", "Sa\u00b7tan", "mich", ",", "so", "zeigs\u00b7tu", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ha\u00dft mich die Welt, so liebstu mich,", "tokens": ["Ha\u00dft", "mich", "die", "Welt", ",", "so", "liebs\u00b7tu", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00fcckt mich viel Kreuz, so hilffstu tragen;", "tokens": ["Dr\u00fcckt", "mich", "viel", "Kreuz", ",", "so", "hilffs\u00b7tu", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verdammt mich selbst mein Fleisch und Blut,", "tokens": ["Ver\u00b7dammt", "mich", "selbst", "mein", "Fleisch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So machstu durch dein Kind mir Muth", "tokens": ["So", "machs\u00b7tu", "durch", "dein", "Kind", "mir", "Muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und labst mich, da ich m\u00fcst verzagen.", "tokens": ["Und", "labst", "mich", ",", "da", "ich", "m\u00fcst", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ja, was ich habe, will und bin,", "tokens": ["Ja", ",", "was", "ich", "ha\u00b7be", ",", "will", "und", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWS", "PPER", "VAFIN", "$,", "VMFIN", "KON", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist gut, stell' ich es dir nur hin.", "tokens": ["Ist", "gut", ",", "stell'", "ich", "es", "dir", "nur", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VVFIN", "PPER", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach, f\u00fchr' auf dieser sch\u00f6nen Bahn", "tokens": ["Ach", ",", "f\u00fchr'", "auf", "die\u00b7ser", "sch\u00f6\u00b7nen", "Bahn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu dir, Herr, meinen Geist stets an", "tokens": ["Zu", "dir", ",", "Herr", ",", "mei\u00b7nen", "Geist", "stets", "an"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lasse dein Gespr\u00e4ch mich weiden.", "tokens": ["Und", "las\u00b7se", "dein", "Ge\u00b7spr\u00e4ch", "mich", "wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Hertz ist wie ein schwerer Stein", "tokens": ["Mein", "Hertz", "ist", "wie", "ein", "schwe\u00b7rer", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sinkt durch sich nur H\u00f6llenein;", "tokens": ["Und", "sinkt", "durch", "sich", "nur", "H\u00f6l\u00b7len\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zermalm' es du durch Kreutz und Leiden,", "tokens": ["Zer\u00b7malm'", "es", "du", "durch", "Kreutz", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn wird es leicht, denn steigt es woll", "tokens": ["Denn", "wird", "es", "leicht", ",", "denn", "steigt", "es", "woll"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wird von deinem Hertzen voll.", "tokens": ["Und", "wird", "von", "dei\u00b7nem", "Hert\u00b7zen", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gib aber, da\u00df, mein Gott, ich dir", "tokens": ["Gib", "a\u00b7ber", ",", "da\u00df", ",", "mein", "Gott", ",", "ich", "dir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "$,", "PPOSAT", "NN", "$,", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nichts, was dir mi\u00dff\u00e4llt, bringe f\u00fcr,", "tokens": ["Nichts", ",", "was", "dir", "mi\u00df\u00b7f\u00e4llt", ",", "brin\u00b7ge", "f\u00fcr", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich wei\u00df nicht, was ich bitten solle.", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "bit\u00b7ten", "sol\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Aug' erkennt nur, was es sieht,", "tokens": ["Mein", "Aug'", "er\u00b7kennt", "nur", ",", "was", "es", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist Erd' und ist um Erd bem\u00fcht", "tokens": ["Ist", "Erd'", "und", "ist", "um", "Erd", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "VAFIN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zweiffelt selbst stets, was es wolle;", "tokens": ["Und", "zweif\u00b7felt", "selbst", "stets", ",", "was", "es", "wol\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du weist es, Jesu, o mein Licht,", "tokens": ["Du", "weist", "es", ",", "Je\u00b7su", ",", "o", "mein", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NE", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bitt du vor mich, so fehl' ich nicht.", "tokens": ["Bitt", "du", "vor", "mich", ",", "so", "fehl'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer bin ich, Gott, doch gegen dich,", "tokens": ["Wer", "bin", "ich", ",", "Gott", ",", "doch", "ge\u00b7gen", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "$,", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dennoch unterwind' ich mich", "tokens": ["Und", "den\u00b7noch", "un\u00b7ter\u00b7wind'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dir mich \u00f6ffters zu bereden.", "tokens": ["Mit", "dir", "mich", "\u00f6ff\u00b7ters", "zu", "be\u00b7re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darff Staub und Asch, darff d\u00fcrres Heu,", "tokens": ["Darff", "Staub", "und", "Asch", ",", "darff", "d\u00fcr\u00b7res", "Heu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "$,", "VMFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Darff fl\u00fcchtigs Laub und \u00f6de Spreu,", "tokens": ["Darff", "fl\u00fcch\u00b7tigs", "Laub", "und", "\u00f6\u00b7de", "Spreu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Mensch, ach Herr, sich nicht entbl\u00f6den,", "tokens": ["Ein", "Mensch", ",", "ach", "Herr", ",", "sich", "nicht", "ent\u00b7bl\u00f6\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "XY", "NN", "$,", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er, o Sch\u00f6pffer aller Welt,", "tokens": ["Da\u00df", "er", ",", "o", "Sch\u00f6pf\u00b7fer", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "FM", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dich, wenn er will, zu Rede stellt?", "tokens": ["Dich", ",", "wenn", "er", "will", ",", "zu", "Re\u00b7de", "stellt", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VMFIN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Was fehlt mir, auch wie schlecht es ist,", "tokens": ["Was", "fehlt", "mir", ",", "auch", "wie", "schlecht", "es", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADV", "KOKOM", "VVFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum dich mein Herz nicht frey begr\u00fcst!", "tokens": ["Drum", "dich", "mein", "Herz", "nicht", "frey", "be\u00b7gr\u00fcst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bin doch nie dir zu geringe.", "tokens": ["Und", "bin", "doch", "nie", "dir", "zu", "ge\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zutritt hat nicht Maa\u00df noch Ziel,", "tokens": ["Der", "Zu\u00b7tritt", "hat", "nicht", "Maa\u00df", "noch", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Bitte bittet nie zu viel,", "tokens": ["Die", "Bit\u00b7te", "bit\u00b7tet", "nie", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und klopfft nicht, da\u00df sie nicht durchdringe,", "tokens": ["Und", "klopfft", "nicht", ",", "da\u00df", "sie", "nicht", "durch\u00b7drin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist, da\u00df mein Wunsch dein' Hoheit scheut,", "tokens": ["Ist", ",", "da\u00df", "mein", "Wunsch", "dein'", "Ho\u00b7heit", "scheut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So stehstu, eh er k\u00f6mmt, bereit.", "tokens": ["So", "steh\u00b7stu", ",", "eh", "er", "k\u00f6mmt", ",", "be\u00b7reit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O Wunder aller Lieb' und G\u00fct,", "tokens": ["O", "Wun\u00b7der", "al\u00b7ler", "Lieb'", "und", "G\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ist ein Mensch von dem Gem\u00fcth?", "tokens": ["Wo", "ist", "ein", "Mensch", "von", "dem", "Ge\u00b7m\u00fcth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie l\u00e4sset sich die Welt doch feyren,", "tokens": ["Wie", "l\u00e4s\u00b7set", "sich", "die", "Welt", "doch", "fey\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Eh man sie anzusprechen kriegt,", "tokens": ["Eh", "man", "sie", "an\u00b7zu\u00b7spre\u00b7chen", "kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch mehr, wenn man nicht leicht vergn\u00fcgt,", "tokens": ["Noch", "mehr", ",", "wenn", "man", "nicht", "leicht", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PIS", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie wei\u00df sie, was sie giebt, zu s\u00e4uren,", "tokens": ["Wie", "wei\u00df", "sie", ",", "was", "sie", "giebt", ",", "zu", "s\u00e4u\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und hebt mans doch nicht Himmelan,", "tokens": ["Und", "hebt", "mans", "doch", "nicht", "Him\u00b7me\u00b7lan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Heist man ein undankbarer Mann.", "tokens": ["Heist", "man", "ein", "un\u00b7dank\u00b7ba\u00b7rer", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "An dich und dein erhabnes Hau\u00df", "tokens": ["An", "dich", "und", "dein", "er\u00b7hab\u00b7nes", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schick' ich nur einen Seuffzer au\u00df,", "tokens": ["Schick'", "ich", "nur", "ei\u00b7nen", "Seuff\u00b7zer", "au\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So komm ich an und werd' erh\u00f6ret.", "tokens": ["So", "komm