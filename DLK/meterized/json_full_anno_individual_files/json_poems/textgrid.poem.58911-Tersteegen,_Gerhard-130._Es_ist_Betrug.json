{"textgrid.poem.58911": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "130. Es ist Betrug", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die S\u00fcnde gehet sanft hinein,", "tokens": ["Die", "S\u00fcn\u00b7de", "ge\u00b7het", "sanft", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schmeichelt, dich ins Netz zu kriegen;", "tokens": ["Sie", "schmei\u00b7chelt", ",", "dich", "ins", "Netz", "zu", "krie\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf kurze Freud' folgt bittre Pein,", "tokens": ["Auf", "kur\u00b7ze", "Freud'", "folgt", "bitt\u00b7re", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Drum la\u00df dich nicht also betr\u00fcgen!", "tokens": ["Drum", "la\u00df", "dich", "nicht", "al\u00b7so", "be\u00b7tr\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die S\u00fcnde gehet sanft hinein,", "tokens": ["Die", "S\u00fcn\u00b7de", "ge\u00b7het", "sanft", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2