{"dta.poem.9548": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem Frantz\u00f6sischen.  \n An der geliebten bette.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Werth und begl\u00fcckter platz/ Melindens rosen-bette/", "tokens": ["Werth", "und", "be\u00b7gl\u00fcck\u00b7ter", "platz", "/", "Me\u00b7lin\u00b7dens", "ro\u00b7sen\u00b7bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$(", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du eintzger zeuge dieser welt/", "tokens": ["Du", "eintz\u00b7ger", "zeu\u00b7ge", "die\u00b7ser", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von aller liebligkeit die sie verborgen h\u00e4lt/", "tokens": ["Von", "al\u00b7ler", "lieb\u00b7lig\u00b7keit", "die", "sie", "ver\u00b7bor\u00b7gen", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "PPER", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach wenn ich doch dich zum verr\u00e4ther h\u00e4tte!", "tokens": ["Ach", "wenn", "ich", "doch", "dich", "zum", "ver\u00b7r\u00e4\u00b7ther", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ADV", "PRF", "APPRART", "ADJA", "VAFIN", "$."], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Ach da\u00df du zu getreu und zu verschwiegen bist!", "tokens": ["Ach", "da\u00df", "du", "zu", "ge\u00b7treu", "und", "zu", "ver\u00b7schwie\u00b7gen", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "PTKA", "ADJD", "KON", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vertraue mir denn nur/ was sonst erfreulich ist.", "tokens": ["Ver\u00b7trau\u00b7e", "mir", "denn", "nur", "/", "was", "sonst", "er\u00b7freu\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$(", "PWS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gedenckt sie nicht an den/ der sie verehrt im schweigen/", "tokens": ["Ge\u00b7denckt", "sie", "nicht", "an", "den", "/", "der", "sie", "ver\u00b7ehrt", "im", "schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "ART", "$(", "PRELS", "PPER", "VVPP", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wenn sie halb erwacht/ mit sich alleine spricht/", "tokens": ["Und", "wenn", "sie", "halb", "er\u00b7wacht", "/", "mit", "sich", "al\u00b7lei\u00b7ne", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVPP", "$(", "APPR", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nennt sie mich unversehns und in gedancken nicht/", "tokens": ["Nennt", "sie", "mich", "un\u00b7ver\u00b7sehns", "und", "in", "ge\u00b7dan\u00b7cken", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "KON", "APPR", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und h\u00f6rst du keinen wunsch aus ihrem hertzen steigen?", "tokens": ["Und", "h\u00f6rst", "du", "kei\u00b7nen", "wunsch", "aus", "ih\u00b7rem", "hert\u00b7zen", "stei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}