{"textgrid.poem.54556": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott selbst blitzt auf dich zu/", "tokens": ["Gott", "selbst", "blitzt", "auf", "dich", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "APPR", "PPER", "PTKZU", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und reist die Tochter dir/ mein Herr/ aus Arm und Hertzen/", "tokens": ["Und", "reist", "die", "Toch\u00b7ter", "dir", "/", "mein", "Herr", "/", "aus", "Arm", "und", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$(", "PPOSAT", "NN", "$(", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du du f\u00fchlst/ doch nicht sie/ die grimmsten Todtes-Schmertzen/", "tokens": ["Du", "du", "f\u00fchlst", "/", "doch", "nicht", "sie", "/", "die", "grimms\u00b7ten", "Tod\u00b7tes\u00b7Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$(", "ADV", "PTKNEG", "PPER", "$(", "ART", "ADJA", "NN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Best\u00fcrtzter Vater du!", "tokens": ["Be\u00b7st\u00fcrtz\u00b7ter", "Va\u00b7ter", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gott wei\u00df warum ers thu.", "tokens": ["Gott", "wei\u00df", "wa\u00b7rum", "ers", "thu", "."], "token_info": ["word", "word", "word"