{"dta.poem.7739": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solang es in dir st\u00fcrmt, so tr\u00f6ste dich: Du bist", "tokens": ["So\u00b7lang", "es", "in", "dir", "st\u00fcrmt", ",", "so", "tr\u00f6s\u00b7te", "dich", ":", "Du", "bist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch eine Speich' am Rad, das stets im Wirbel ist.", "tokens": ["Auch", "ei\u00b7ne", "Speich'", "am", "Rad", ",", "das", "stets", "im", "Wir\u00b7bel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und ward es still in dir, so magst du sanftger\u00fchrt", "tokens": ["Und", "ward", "es", "still", "in", "dir", ",", "so", "magst", "du", "sanft\u00b7ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "$,", "ADV", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zuschauer seyn des Spiels, das dir die Welt auff\u00fchrt.", "tokens": ["Zu\u00b7schau\u00b7er", "seyn", "des", "Spiels", ",", "das", "dir", "die", "Welt", "auf\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn als Mitspieler selbst du Beifall nicht erhieltest;", "tokens": ["Wenn", "als", "Mit\u00b7spie\u00b7ler", "selbst", "du", "Bei\u00b7fall", "nicht", "er\u00b7hiel\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "ADV", "PPER", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du spieltest nicht umsonst, wenn dir zur Lust du spieltest.", "tokens": ["Du", "spiel\u00b7test", "nicht", "um\u00b7sonst", ",", "wenn", "dir", "zur", "Lust", "du", "spiel\u00b7test", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}