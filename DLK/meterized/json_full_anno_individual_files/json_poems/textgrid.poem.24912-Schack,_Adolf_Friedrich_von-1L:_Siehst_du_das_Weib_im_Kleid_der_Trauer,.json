, "measure": "iambic.tetra"}, "line.5": {"text": "Mehr noch, als da mir starb der Gatte,", "tokens": ["Mehr", "noch", ",", "als", "da", "mir", "starb", "der", "Gat\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verwitwet bin ich nun durch ihn.", "tokens": ["Ver\u00b7wit\u00b7wet", "bin", "ich", "nun", "durch", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Wen soll ich an die Brust nun pressen?", "tokens": ["Wen", "soll", "ich", "an", "die", "Brust", "nun", "pres\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf wessen Lippen, ach, auf wessen", "tokens": ["Auf", "wes\u00b7sen", "Lip\u00b7pen", ",", "ach", ",", "auf", "wes\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ITJ", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00fcck' ich den warmen Mutterku\u00df?", "tokens": ["Dr\u00fcck", "ich", "den", "war\u00b7men", "Mut\u00b7ter\u00b7ku\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wird mich jetzt im Alter st\u00fctzen,", "tokens": ["Wer", "wird", "mich", "jetzt", "im", "Al\u00b7ter", "st\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", 