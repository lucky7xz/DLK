{"textgrid.poem.24912": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Siehst du das Weib im Kleid der Trauer,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehst du das Weib im Kleid der Trauer,", "tokens": ["Siehst", "du", "das", "Weib", "im", "Kleid", "der", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Tag f\u00fcr Tag seit Jahresdauer", "tokens": ["Das", "Tag", "f\u00fcr", "Tag", "seit", "Jah\u00b7res\u00b7dau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Rom dahinwankt hauptverh\u00fcllt,", "tokens": ["Durch", "Rom", "da\u00b7hin\u00b7wankt", "haupt\u00b7ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine H\u00fcgel all, die sieben,", "tokens": ["Und", "sei\u00b7ne", "H\u00fc\u00b7gel", "all", ",", "die", "sie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIAT", "$,", "PRELS", "CARD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Rastlos vom Schmerz umhergetrieben,", "tokens": ["Rast\u00b7los", "vom", "Schmerz", "um\u00b7her\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit lauter Weheklage f\u00fcllt?", "tokens": ["Mit", "lau\u00b7ter", "We\u00b7he\u00b7kla\u00b7ge", "f\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schon fr\u00fche mu\u00dfte sie den Gatten", "tokens": ["Schon", "fr\u00fc\u00b7he", "mu\u00df\u00b7te", "sie", "den", "Gat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In seiner V\u00e4ter Gruft bestatten;", "tokens": ["In", "sei\u00b7ner", "V\u00e4\u00b7ter", "Gruft", "be\u00b7stat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kunde ward ihr dann gebracht,", "tokens": ["Die", "Kun\u00b7de", "ward", "ihr", "dann", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er, den sie geliebt vor allen,", "tokens": ["Da\u00df", "er", ",", "den", "sie", "ge\u00b7liebt", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "VVPP", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Sohn, ihr Lentulus, gefallen", "tokens": ["Ihr", "Sohn", ",", "ihr", "Len\u00b7tu\u00b7lus", ",", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Cann\u00e4s m\u00f6rderischer Schlacht.", "tokens": ["In", "Can\u00b7n\u00e4s", "m\u00f6r\u00b7de\u00b7ri\u00b7scher", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und als ihr kam der Trauerbote,", "tokens": ["Und", "als", "ihr", "kam", "der", "Trau\u00b7er\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da, selber bleich wie eine Tote,", "tokens": ["Da", ",", "sel\u00b7ber", "bleich", "wie", "ei\u00b7ne", "To\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Rief sie am Herd die G\u00f6tter an:", "tokens": ["Rief", "sie", "am", "Herd", "die", "G\u00f6t\u00b7ter", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbla\u00dft mich, ihr Lenker der Geschicke,", "tokens": ["\u00bb", "la\u00dft", "mich", ",", "ihr", "Len\u00b7ker", "der", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Allein auf Erden nicht zur\u00fccke!", "tokens": ["Al\u00b7lein", "auf", "Er\u00b7den", "nicht", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erl\u00f6st mich von des Lebens Bann!\u00ab", "tokens": ["Er\u00b7l\u00f6st", "mich", "von", "des", "Le\u00b7bens", "Bann", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zw\u00f6lf Monde sind seitdem geschwunden;", "tokens": ["Zw\u00f6lf", "Mon\u00b7de", "sind", "seit\u00b7dem", "ge\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat den Tod zu allen Stunden", "tokens": ["Sie", "hat", "den", "Tod", "zu", "al\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als einz'gen Retter sich erfleht;", "tokens": ["Als", "einz'\u00b7gen", "Ret\u00b7ter", "sich", "er\u00b7fleht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie trat durch jede Tempelpforte", "tokens": ["Sie", "trat", "durch", "je\u00b7de", "Tem\u00b7pel\u00b7pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und stammelte dieselben Worte,", "tokens": ["Und", "stam\u00b7mel\u00b7te", "die\u00b7sel\u00b7ben", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch unerh\u00f6rt blieb ihr Gebet.", "tokens": ["Doch", "un\u00b7er\u00b7h\u00f6rt", "blieb", "ihr", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Und, Asche auf das Haupt sich streuend,", "tokens": ["Und", ",", "A\u00b7sche", "auf", "das", "Haupt", "sich", "streu\u00b7end", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "APPR", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Irrt sie, den Wehruf stets erneuend,", "tokens": ["Irrt", "sie", ",", "den", "We\u00b7hruf", "stets", "er\u00b7neu\u00b7end", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Quirinal zum Palatin:", "tokens": ["Vom", "Qui\u00b7ri\u00b7nal", "zum", "Pa\u00b7la\u00b7tin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbdas einz'ge war er, was ich hatte;", "tokens": ["\u00bb", "das", "einz'\u00b7ge", "war", "er", ",", "was", "ich", "hat\u00b7te", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "VAFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mehr noch, als da mir starb der Gatte,", "tokens": ["Mehr", "noch", ",", "als", "da", "mir", "starb", "der", "Gat\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verwitwet bin ich nun durch ihn.", "tokens": ["Ver\u00b7wit\u00b7wet", "bin", "ich", "nun", "durch", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wen soll ich an die Brust nun pressen?", "tokens": ["Wen", "soll", "ich", "an", "die", "Brust", "nun", "pres\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf wessen Lippen, ach, auf wessen", "tokens": ["Auf", "wes\u00b7sen", "Lip\u00b7pen", ",", "ach", ",", "auf", "wes\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ITJ", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00fcck' ich den warmen Mutterku\u00df?", "tokens": ["Dr\u00fcck", "ich", "den", "war\u00b7men", "Mut\u00b7ter\u00b7ku\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer wird mich jetzt im Alter st\u00fctzen,", "tokens": ["Wer", "wird", "mich", "jetzt", "im", "Al\u00b7ter", "st\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer plaudernd mir zur Seite sitzen,", "tokens": ["Wer", "plau\u00b7dernd", "mir", "zur", "Sei\u00b7te", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seitdem dahin mein Lentulus?", "tokens": ["Seit\u00b7dem", "da\u00b7hin", "mein", "Len\u00b7tu\u00b7lus", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PAV", "PPOSAT", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach, hold und sch\u00f6n, mit achtzehn Jahren", "tokens": ["Ach", ",", "hold", "und", "sch\u00f6n", ",", "mit", "acht\u00b7zehn", "Jah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Schwerter blutiger Barbaren", "tokens": ["Durch", "Schwer\u00b7ter", "blu\u00b7ti\u00b7ger", "Bar\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Fiel er dem grimmen Mars zum Raub,", "tokens": ["Fiel", "er", "dem", "grim\u00b7men", "Mars", "zum", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fern dem Sitz der hohen Ahnen", "tokens": ["Und", "fern", "dem", "Sitz", "der", "ho\u00b7hen", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Umschweifen ruhlos nun die Manen", "tokens": ["Um\u00b7schwei\u00b7fen", "ruh\u00b7los", "nun", "die", "Ma\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des J\u00fcnglings windverwehten Staub.", "tokens": ["Des", "J\u00fcng\u00b7lings", "wind\u00b7ver\u00b7weh\u00b7ten", "Staub", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mich aber h\u00e4lt, da\u00df von der Erde", "tokens": ["Mich", "a\u00b7ber", "h\u00e4lt", ",", "da\u00df", "von", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich nicht hinweggenommen werde,", "tokens": ["Ich", "nicht", "hin\u00b7weg\u00b7ge\u00b7nom\u00b7men", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der strengen G\u00f6tter Machtgebot;", "tokens": ["Der", "stren\u00b7gen", "G\u00f6t\u00b7ter", "Macht\u00b7ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts rettet mich vom Leid, dem herben;", "tokens": ["Nichts", "ret\u00b7tet", "mich", "vom", "Leid", ",", "dem", "her\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "$,", "PRELS", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In Jammer mu\u00df ich ewig sterben,", "tokens": ["In", "Jam\u00b7mer", "mu\u00df", "ich", "e\u00b7wig", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ewig flieht mich doch der Tod!\u00ab", "tokens": ["Und", "e\u00b7wig", "flieht", "mich", "doch", "der", "Tod", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So klagt sie laut; da pl\u00f6tzlich schreitet,", "tokens": ["So", "klagt", "sie", "laut", ";", "da", "pl\u00f6tz\u00b7lich", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Jubelruf des Volks begleitet,", "tokens": ["Vom", "Ju\u00b7bel\u00b7ruf", "des", "Volks", "be\u00b7glei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Erzgewand mit hurt'gem Fu\u00df", "tokens": ["Im", "Erz\u00b7ge\u00b7wand", "mit", "hurt'\u00b7gem", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein junger Krieger durchs Gedr\u00e4nge;", "tokens": ["Ein", "jun\u00b7ger", "Krie\u00b7ger", "durchs", "Ge\u00b7dr\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbsieh da!\u00ab \u2013 so ruft es aus der Menge \u2013", "tokens": ["\u00bb", "sieh", "da", "!", "\u00ab", "\u2013", "so", "ruft", "es", "aus", "der", "Men\u00b7ge", "\u2013"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$.", "$(", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbmetella, sieh! dein Lentulus!\u00ab", "tokens": ["\u00bb", "me\u00b7tel\u00b7la", ",", "sieh", "!", "dein", "Len\u00b7tu\u00b7lus", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$,", "VVIMP", "$.", "PPOSAT", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Und sprachlos, ohne sich zu regen,", "tokens": ["Und", "sprach\u00b7los", ",", "oh\u00b7ne", "sich", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOUI", "PRF", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Starrt sie dem Kommenden entgegen,", "tokens": ["Starrt", "sie", "dem", "Kom\u00b7men\u00b7den", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "An ihren Busen sinkt der Sohn;", "tokens": ["An", "ih\u00b7ren", "Bu\u00b7sen", "sinkt", "der", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbo Mutter, Mutter! lang im Lager", "tokens": ["\u00bb", "o", "Mut\u00b7ter", ",", "Mut\u00b7ter", "!", "lang", "im", "La\u00b7ger"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "FM", "NN", "$,", "NN", "$.", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hielt mich gefangen der Karthager;", "tokens": ["Hielt", "mich", "ge\u00b7fan\u00b7gen", "der", "Kar\u0