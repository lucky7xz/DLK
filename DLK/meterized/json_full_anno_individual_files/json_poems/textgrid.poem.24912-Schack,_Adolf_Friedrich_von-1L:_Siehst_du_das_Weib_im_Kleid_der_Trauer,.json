mbic.tetra"}, "line.5": {"text": "Ihr Sohn, ihr Lentulus, gefallen", "tokens": ["Ihr", "Sohn", ",", "ihr", "Len\u00b7tu\u00b7lus", ",", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Cann\u00e4s m\u00f6rderischer Schlacht.", "tokens": ["In", "Can\u00b7n\u00e4s", "m\u00f6r\u00b7de\u00b7ri\u00b7scher", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Und als ihr kam der Trauerbote,", "tokens": ["Und", "als", "ihr", "kam", "der", "Trau\u00b7er\u00b7bo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da, selber bleich wie eine Tote,", "tokens": ["Da", ",", "sel\u00b7ber", "bleich", "wie", "ei\u00b7ne", "To\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Rief sie am Herd die G\u00f6tter an:", "tokens": ["Rief", "sie", "am", "Herd", "die", "G\u00f6t\u00b7ter", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbla\u00dft mich, ihr Lenker der Geschicke,", "tokens": ["\u00bb", "la\u00dft", "mich", ",", "ihr", "Len\u00b7ker", "der", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Allein auf Erden nicht zur\u00fccke!", "tokens": ["Al\u00b7lein", "auf", "Er\u00b7den", "nicht", "zu\u00b7r\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Erl\u00f6st mich von des Lebens Bann!\u00ab", "tokens": ["Er\u00b7l\u00f6st", "mich", "von", "des", "Le\u00b7bens", "Bann", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Zw\u00f6lf Monde sind seitdem geschwunden;", "tokens": ["Zw\u00f6lf", "Mon\u00b7de", "sind", "seit\u00b7dem", "ge\u00b7schwun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat den Tod zu allen Stunden", "tokens": ["Sie", "hat", "den", "Tod", "zu", "al\u00b7len", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als einz'gen Retter sich erfleht;", "tokens": ["Als", "einz'\u00b7gen", "Ret\u00b7ter", "sich", "er\u00b7fleht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie trat durch jede Tempelpforte", "tokens": ["Sie", "trat", "durch", "je\u00b7de", "Tem\u00b7pel\u00b7pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.t