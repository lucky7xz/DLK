{"dta.poem.6849": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann deutlich, nett und rein ein Vorwurff wird erblickt;", "tokens": ["Wann", "deut\u00b7lich", ",", "nett", "und", "rein", "ein", "Vor\u00b7wurff", "wird", "er\u00b7blickt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "KON", "ADJD", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entsteht es blo\u00df, da\u00df er sich in ein P\u00fcnctgen dr\u00fcckt.", "tokens": ["Ent\u00b7steht", "es", "blo\u00df", ",", "da\u00df", "er", "sich", "in", "ein", "P\u00fcnct\u00b7gen", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hingegen wird das Sehn verwirret und verst\u00f6ret,", "tokens": ["Hin\u00b7ge\u00b7gen", "wird", "das", "Sehn", "ver\u00b7wir\u00b7ret", "und", "ver\u00b7st\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn, da man etwan eins von unsern Augen presst,", "tokens": ["Wenn", ",", "da", "man", "et\u00b7wan", "eins", "von", "un\u00b7sern", "Au\u00b7gen", "presst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PIS", "ADV", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man gleich das Nervchen abwerts kehret,", "tokens": ["Man", "gleich", "das", "Nerv\u00b7chen", "ab\u00b7werts", "keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da an zween Orten dann die Glandul wird beweget,", "tokens": ["Da", "an", "zween", "Or\u00b7ten", "dann", "die", "Glan\u00b7dul", "wird", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "VVFIN", "NN", "ADV", "ART", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wodurch dasselbe Bild so gleich gedoppelt l\u00e4sst,", "tokens": ["Wo\u00b7durch", "das\u00b7sel\u00b7be", "Bild", "so", "gleich", "ge\u00b7dop\u00b7pelt", "l\u00e4sst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und folglich sind im Geist und in der Phantasey", "tokens": ["Und", "folg\u00b7lich", "sind", "im", "Geist", "und", "in", "der", "Phan\u00b7ta\u00b7sey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Durch diese dopple Schilderey", "tokens": ["Durch", "die\u00b7se", "dopp\u00b7le", "Schil\u00b7de\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Verwirrungen erreget.", "tokens": ["Ver\u00b7wir\u00b7run\u00b7gen", "er\u00b7re\u00b7get", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}}}}