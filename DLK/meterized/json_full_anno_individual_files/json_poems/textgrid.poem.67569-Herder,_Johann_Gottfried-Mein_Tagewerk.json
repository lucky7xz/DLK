", "Le\u00b7bens", "M\u00fc\u00b7he", ";", "doch", "des", "Gl\u00fc\u00b7ckes", "viel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "NN", "$.", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Gew\u00e4hrt die M\u00fche; wie mit Sch\u00f6pferskraft,", "tokens": ["Ge\u00b7w\u00e4hrt", "die", "M\u00fc\u00b7he", ";", "wie", "mit", "Sch\u00f6p\u00b7fer\u00b7skraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$.", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Selbstbewu\u00dftsein reget sie uns warm.", "tokens": ["Mit", "Selbst\u00b7be\u00b7wu\u00df\u00b7tsein", "re\u00b7get", "sie", "uns", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum f\u00fchl Entschlu\u00df, so lange Lebenssaft", "tokens": ["Drum", "f\u00fchl", "Ent\u00b7schlu\u00df", ",", "so", "lan\u00b7ge", "Le\u00b7bens\u00b7saft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dir quillet, und kein Feind soll Deinen Arm", "tokens": ["Dir", "quil\u00b7let", ",", "und", "kein", "Feind", "soll", "Dei\u00b7nen", "Arm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PIAT", "NN", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.19": {"line.1": {"text": "Verr\u00fccken, wenn Du schnellst, der L\u00fcfte Scherz,", "tokens": ["Ver\u00b7r\u00fc\u00b7cken", ",", "wenn", "Du", "schnellst", ",", "der", "L\u00fcf\u00b7te", "Sch