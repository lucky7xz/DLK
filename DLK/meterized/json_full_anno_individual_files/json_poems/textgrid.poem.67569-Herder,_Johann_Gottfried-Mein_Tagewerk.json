{"textgrid.poem.67569": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Mein Tagewerk", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So komm, o komme, meines Lebens Stab,", "tokens": ["So", "komm", ",", "o", "kom\u00b7me", ",", "mei\u00b7nes", "Le\u00b7bens", "Stab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "VVFIN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gef\u00e4hrte, der von fr\u00fch auf mit mir schritt,", "tokens": ["Ge\u00b7f\u00e4hr\u00b7te", ",", "der", "von", "fr\u00fch", "auf", "mit", "mir", "schritt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJD", "APPR", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Komm, s\u00fc\u00dfe M\u00fch, und leite auf und ab", "tokens": ["Komm", ",", "s\u00fc\u00b7\u00dfe", "M\u00fch", ",", "und", "lei\u00b7te", "auf", "und", "ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Lebensh\u00fcgel eines Wandres Tritt,", "tokens": ["Den", "Le\u00b7bens\u00b7h\u00fc\u00b7gel", "ei\u00b7nes", "Wand\u00b7res", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der oft ermattet! Ziel- und h\u00fcttelos", "tokens": ["Der", "oft", "er\u00b7mat\u00b7tet", "!", "Ziel", "und", "h\u00fct\u00b7te\u00b7los"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "VVPP", "$.", "TRUNC", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Irr' ich in W\u00fcsten; sei, o Arbeit, Du", "tokens": ["Irr'", "ich", "in", "W\u00fcs\u00b7ten", ";", "sei", ",", "o", "Ar\u00b7beit", ",", "Du"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "APPR", "NN", "$.", "VAFIN", "$,", "FM", "NN", "$,", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir F\u00fchrerin, da\u00df in der Ruhe Schoo\u00df", "tokens": ["Mir", "F\u00fch\u00b7re\u00b7rin", ",", "da\u00df", "in", "der", "Ru\u00b7he", "Schoo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich nicht unw\u00fcrdig meines Lebens ruh'!", "tokens": ["Ich", "nicht", "un\u00b7w\u00fcr\u00b7dig", "mei\u00b7nes", "Le\u00b7bens", "ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn Ohnmacht der Zerstreuung selbst ist Schmach,", "tokens": ["Denn", "Ohn\u00b7macht", "der", "Zer\u00b7streu\u00b7ung", "selbst", "ist", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ist Tantal's Strafe; sehnend irrt sein Blick", "tokens": ["Ist", "Tan\u00b7tal's", "Stra\u00b7fe", ";", "seh\u00b7nend", "irrt", "sein", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NE", "NN", "$.", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom Silberstrom zum Apfelgold, und ach!", "tokens": ["Vom", "Sil\u00b7ber\u00b7strom", "zum", "Ap\u00b7fel\u00b7gold", ",", "und", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,", "KON", "XY", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er kehrt nur immer sehnender zur\u00fcck.", "tokens": ["Er", "kehrt", "nur", "im\u00b7mer", "seh\u00b7nen\u00b7der", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nimm, was es sei, mein Geist, in Deinen Blick,", "tokens": ["Nimm", ",", "was", "es", "sei", ",", "mein", "Geist", ",", "in", "Dei\u00b7nen", "Blick", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "VAFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und f\u00e4ndest Du am schwer erreichten Ziel", "tokens": ["Und", "f\u00e4n\u00b7dest", "Du", "am", "schwer", "er\u00b7reich\u00b7ten", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKA", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur Deinen matten Pfeil. Des Lebens Gl\u00fcck", "tokens": ["Nur", "Dei\u00b7nen", "mat\u00b7ten", "Pfeil", ".", "Des", "Le\u00b7bens", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist Lebens M\u00fche; doch des Gl\u00fcckes viel", "tokens": ["Ist", "Le\u00b7bens", "M\u00fc\u00b7he", ";", "doch", "des", "Gl\u00fc\u00b7ckes", "viel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "NN", "$.", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gew\u00e4hrt die M\u00fche; wie mit Sch\u00f6pferskraft,", "tokens": ["Ge\u00b7w\u00e4hrt", "die", "M\u00fc\u00b7he", ";", "wie", "mit", "Sch\u00f6p\u00b7fer\u00b7skraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$.", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Selbstbewu\u00dftsein reget sie uns warm.", "tokens": ["Mit", "Selbst\u00b7be\u00b7wu\u00df\u00b7tsein", "re\u00b7get", "sie", "uns", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Drum f\u00fchl Entschlu\u00df, so lange Lebenssaft", "tokens": ["Drum", "f\u00fchl", "Ent\u00b7schlu\u00df", ",", "so", "lan\u00b7ge", "Le\u00b7bens\u00b7saft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dir quillet, und kein Feind soll Deinen Arm", "tokens": ["Dir", "quil\u00b7let", ",", "und", "kein", "Feind", "soll", "Dei\u00b7nen", "Arm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PIAT", "NN", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Verr\u00fccken, wenn Du schnellst, der L\u00fcfte Scherz,", "tokens": ["Ver\u00b7r\u00fc\u00b7cken", ",", "wenn", "Du", "schnellst", ",", "der", "L\u00fcf\u00b7te", "Scherz", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Pfeil; nur eh der Tod ihn Dir entrei\u00dft,", "tokens": ["Den", "Pfeil", ";", "nur", "eh", "der", "Tod", "ihn", "Dir", "ent\u00b7rei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADV", "KOUS", "ART", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil Du noch schl\u00e4gst (Du schl\u00e4gst nicht immer, Herz!),", "tokens": ["Weil", "Du", "noch", "schl\u00e4gst", "(", "Du", "schl\u00e4gst", "nicht", "im\u00b7mer", ",", "Herz", "!", ")", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$(", "PPER", "VVFIN", "PTKNEG", "ADV", "$,", "NN", "$.", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So f\u00fchle Dich und wirk und schaffe, Geist!", "tokens": ["So", "f\u00fch\u00b7le", "Dich", "und", "wirk", "und", "schaf\u00b7fe", ",", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ADJD", "KON", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Denn einst wird's um mich Abend. Jener Blick", "tokens": ["Denn", "einst", "wird's", "um", "mich", "A\u00b7bend", ".", "Je\u00b7ner", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPER", "NN", "$.", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der sch\u00f6nen Sonn' erlischt und tr\u00e4ufelt Thau", "tokens": ["Der", "sch\u00f6\u00b7nen", "Sonn'", "er\u00b7lischt", "und", "tr\u00e4u\u00b7felt", "Thau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Statt Strahlen nieder; Zephyr kehrt zur\u00fcck", "tokens": ["Statt", "Strah\u00b7len", "nie\u00b7der", ";", "Ze\u00b7phyr", "kehrt", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "PTKVZ", "$.", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zum jungen Morgenroth und l\u00e4\u00dft der Au'", "tokens": ["Zum", "jun\u00b7gen", "Mor\u00b7gen\u00b7roth", "und", "l\u00e4\u00dft", "der", "Au'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nur kalte Schauer. Tief verstummt umher", "tokens": ["Nur", "kal\u00b7te", "Schau\u00b7er", ".", "Tief", "ver\u00b7stummt", "um\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$.", "ADJD", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Chor der V\u00f6gel, senkt die Schwingen ab", "tokens": ["Das", "Chor", "der", "V\u00f6\u00b7gel", ",", "senkt", "die", "Schwin\u00b7gen", "ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und schlummert; um Dich rings in Luft und Meer", "tokens": ["Und", "schlum\u00b7mert", ";", "um", "Dich", "rings", "in", "Luft", "und", "Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "APPR", "PPER", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Von Erd' zu Himmel wird's ", "tokens": ["Von", "Erd'", "zu", "Him\u00b7mel", "wird's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wird, wie Du, Geist, denn bist. Es schlie\u00dfet sich", "tokens": ["Wird", ",", "wie", "Du", ",", "Geist", ",", "denn", "bist", ".", "Es", "schlie\u00b7\u00dfet", "sich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "$,", "NN", "$,", "KON", "VAFIN", "$.", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Seele wie die Blume. Zarter Leim", "tokens": ["Die", "See\u00b7le", "wie", "die", "Blu\u00b7me", ".", "Zar\u00b7ter", "Leim"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "NN", "$.", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Lebens, Du erstarrest; Dir entwich", "tokens": ["Des", "Le\u00b7bens", ",", "Du", "er\u00b7star\u00b7rest", ";", "Dir", "ent\u00b7wich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "$.", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Balsam, und der lebensschwangre Keim", "tokens": ["Dein", "Bal\u00b7sam", ",", "und", "der", "le\u00b7bens\u00b7schwang\u00b7re", "Keim"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Der Thaten liegt erstorben. Jenes Bild,", "tokens": ["Der", "Tha\u00b7ten", "liegt", "ers\u00b7tor\u00b7ben", ".", "Je\u00b7nes", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$.", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Wahnbild, hie\u00df der Sieger aller Welt,", "tokens": ["Ein", "Wahn\u00b7bild", ",", "hie\u00df", "der", "Sie\u00b7ger", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hie\u00df Alexander einst: die Asche f\u00fcllt", "tokens": ["Hie\u00df", "A\u00b7lex\u00b7an\u00b7der", "einst", ":", "die", "A\u00b7sche", "f\u00fcllt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Jetzt ihren Sarg nicht mehr; der k\u00fchne Held", "tokens": ["Jetzt", "ih\u00b7ren", "Sarg", "nicht", "mehr", ";", "der", "k\u00fch\u00b7ne", "Held"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Zerf\u00e4llt beim Fingerregen. Und sein Lauf", "tokens": ["Zer\u00b7f\u00e4llt", "beim", "Fin\u00b7ger\u00b7re\u00b7gen", ".", "Und", "sein", "Lauf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll Wunderthaten ist uns Fabel, Wind", "tokens": ["Voll", "Wun\u00b7der\u00b7tha\u00b7ten", "ist", "uns", "Fa\u00b7bel", ",", "Wind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "NN", "VAFIN", "PPER", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Fern' in leere Fl\u00f6ten, Pfennigkauf", "tokens": ["Der", "Fern'", "in", "lee\u00b7re", "Fl\u00f6\u00b7ten", ",", "Pfen\u00b7nig\u00b7kauf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Stra\u00dfens\u00e4nger. Alle sind, sie sind", "tokens": ["Der", "Stra\u00b7\u00dfen\u00b7s\u00e4n\u00b7ger", ".", "Al\u00b7le", "sind", ",", "sie", "sind"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "PIS", "VAFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Uns Fabeln, Hercul, Solon und Homer,", "tokens": ["Uns", "Fa\u00b7beln", ",", "Her\u00b7cul", ",", "So\u00b7lon", "und", "Ho\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "NE", "$,", "NE", "KON", "NE", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Achill und Hektor, sind ein Todtenbein", "tokens": ["A\u00b7chill", "und", "Hek\u00b7tor", ",", "sind", "ein", "Tod\u00b7ten\u00b7bein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "NE", "$,", "VAFIN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und Namenschall; ihr gro\u00dfes Thatenheer", "tokens": ["Und", "Na\u00b7men\u00b7schall", ";", "ihr", "gro\u00b7\u00dfes", "Tha\u00b7ten\u00b7heer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist M\u00e4rchen, M\u00e4rchen auf dem Leichenstein.", "tokens": ["Ist", "M\u00e4r\u00b7chen", ",", "M\u00e4r\u00b7chen", "auf", "dem", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Drum weil ich lebe, leb' ich. Komm, o Stab", "tokens": ["Drum", "weil", "ich", "le\u00b7be", ",", "leb'", "ich", ".", "Komm", ",", "o", "Stab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PAV", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$.", "VVFIN", "$,", "FM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Wandrers! Dir zur Seite Gutes thun,", "tokens": ["Des", "Wand\u00b7rers", "!", "Dir", "zur", "Sei\u00b7te", "Gu\u00b7tes", "thun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist Lohn f\u00fcr mich und Leben. Tod und Grab,", "tokens": ["Ist", "Lohn", "f\u00fcr", "mich", "und", "Le\u00b7ben", ".", "Tod", "und", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PPER", "KON", "NN", "$.", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Grab und Tod hei\u00dft bald genug uns \u2013 ruhn.", "tokens": ["Und", "Grab", "und", "Tod", "hei\u00dft", "bald", "ge\u00b7nug", "uns", "\u2013", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "w