{"textgrid.poem.38914": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Herzen war es stille,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Herzen war es stille,", "tokens": ["Im", "Her\u00b7zen", "war", "es", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wahnsinn lag an Ketten;", "tokens": ["Der", "Wahn\u00b7sinn", "lag", "an", "Ket\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da regt sich b\u00f6ser Wille,", "tokens": ["Da", "regt", "sich", "b\u00f6\u00b7ser", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Kerker ihn zu retten,", "tokens": ["Vom", "Ker\u00b7ker", "ihn", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Tollen los zu machen:", "tokens": ["Den", "Tol\u00b7len", "los", "zu", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da h\u00f6rt man Pauken klingen,", "tokens": ["Da", "h\u00f6rt", "man", "Pau\u00b7ken", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da bricht hervor mit Lachen", "tokens": ["Da", "bricht", "her\u00b7vor", "mit", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Trommeten-Klang und Krachen,", "tokens": ["Trom\u00b7me\u00b7ten\u00b7Klang", "und", "Kra\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Dazwischen Fl\u00f6ten singen,", "tokens": ["Da\u00b7zwi\u00b7schen", "Fl\u00f6\u00b7ten", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und Pfeifent\u00f6ne springen", "tokens": ["Und", "Pfei\u00b7fen\u00b7t\u00f6\u00b7ne", "sprin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Mit gellendem Geschrei", "tokens": ["Mit", "gel\u00b7len\u00b7dem", "Ge\u00b7schrei"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Zwischen dr\u00f6hnenden t\u00f6nenden Geigen", "tokens": ["Zwi\u00b7schen", "dr\u00f6h\u00b7nen\u00b7den", "t\u00f6\u00b7nen\u00b7den", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "In rasender Wuth herbei,", "tokens": ["In", "ra\u00b7sen\u00b7der", "Wuth", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Das wilde Gem\u00fcth zu zeigen,", "tokens": ["Das", "wil\u00b7de", "Ge\u00b7m\u00fcth", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Und grimmig zu morden das stille kindliche Schweigen. \u2013", "tokens": ["Und", "grim\u00b7mig", "zu", "mor\u00b7den", "das", "stil\u00b7le", "kind\u00b7li\u00b7che", "Schwei\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wohin dreht sich der Reigen?", "tokens": ["Wo\u00b7hin", "dreht", "sich", "der", "Rei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Was sucht die springende Menge", "tokens": ["Was", "sucht", "die", "sprin\u00b7gen\u00b7de", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im windenden Gedr\u00e4nge? \u2013", "tokens": ["Im", "win\u00b7den\u00b7den", "Ge\u00b7dr\u00e4n\u00b7ge", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor\u00fcber! Es gl\u00e4nzen die Lichter,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "!", "Es", "gl\u00e4n\u00b7zen", "die", "Lich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wir tummeln uns n\u00e4her und dichter,", "tokens": ["Wir", "tum\u00b7meln", "uns", "n\u00e4\u00b7her", "und", "dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Es jauchzt in uns das bl\u00f6de Herz;", "tokens": ["Es", "jauchzt", "in", "uns", "das", "bl\u00f6\u00b7de", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Lauter t\u00f6net", "tokens": ["Lau\u00b7ter", "t\u00f6\u00b7net"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Grimmer dr\u00f6hnet", "tokens": ["Grim\u00b7mer", "dr\u00f6h\u00b7net"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ihr Cymbeln, ihr Pfeifen! bet\u00e4ubet den Schmerz,", "tokens": ["Ihr", "Cym\u00b7beln", ",", "ihr", "Pfei\u00b7fen", "!", "be\u00b7t\u00e4u\u00b7bet", "den", "Schmerz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Er werde zum Scherz! \u2013", "tokens": ["Er", "wer\u00b7de", "zum", "Scherz", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Du winkst mir, holdes Angesicht?", "tokens": ["Du", "winkst", "mir", ",", "hol\u00b7des", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es lacht der Mund, der Augen Licht;", "tokens": ["Es", "lacht", "der", "Mund", ",", "der", "Au\u00b7gen", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Herbei, da\u00df ich dich fasse,", "tokens": ["Her\u00b7bei", ",", "da\u00df", "ich", "dich", "fas\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Schweben wieder lasse;", "tokens": ["Im", "Schwe\u00b7ben", "wie\u00b7der", "las\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich wei\u00df, die Sch\u00f6nheit bald zerbricht,", "tokens": ["Ich", "wei\u00df", ",", "die", "Sch\u00f6n\u00b7heit", "bald", "zer\u00b7bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Mund verstummt, der lieblich spricht,", "tokens": ["Der", "Mund", "ver\u00b7stummt", ",", "der", "lieb\u00b7lich", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dich fa\u00dft des Todes Arm.", "tokens": ["Dich", "fa\u00dft", "des", "To\u00b7des", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Was winkst, du, Sch\u00e4del, freundlich mir?", "tokens": ["Was", "winkst", ",", "du", ",", "Sch\u00e4\u00b7del", ",", "freund\u00b7lich", "mir", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "$,", "NE", "$,", "ADJD", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Kein Kummer mir, nicht Angst und Harm,", "tokens": ["Kein", "Kum\u00b7mer", "mir", ",", "nicht", "Angst", "und", "Harm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "$,", "PTKNEG", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df du so bald erbleichest hier,", "tokens": ["Da\u00df", "du", "so", "bald", "er\u00b7blei\u00b7chest", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wohl heut, wohl morgen.", "tokens": ["Wohl", "heut", ",", "wohl", "mor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Was sollen die Sorgen?", "tokens": ["Was", "sol\u00b7len", "die", "Sor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Ich lebe und schwebe im Reigen vor\u00fcber vor dir. \u2013", "tokens": ["Ich", "le\u00b7be", "und", "schwe\u00b7be", "im", "Rei\u00b7gen", "vor\u00b7\u00fc\u00b7ber", "vor", "dir", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "ADV", "APPR", "PPER", "$.", "$("], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}}, "stanza.4": {"line.1": {"text": "Heut lieb ich dich,", "tokens": ["Heut", "lieb", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Jetzt meinst du mich;", "tokens": ["Jetzt", "meinst", "du", "mich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ach, Noth und Angst sie lauern", "tokens": ["Ach", ",", "Noth", "und", "Angst", "sie", "lau\u00b7ern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "KON", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schon hinter diesen Mauern,", "tokens": ["Schon", "hin\u00b7ter", "die\u00b7sen", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Seufzer schwer und thr\u00e4nend Leid", "tokens": ["Und", "Seuf\u00b7zer", "schwer", "und", "thr\u00e4\u00b7nend", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ADJD", "KON", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stehn schon bereit,", "tokens": ["Stehn", "schon", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Dich zu umstricken;", "tokens": ["Dich", "zu", "um\u00b7stri\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Froh la\u00df uns blicken", "tokens": ["Froh", "la\u00df", "uns", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVIMP", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Vernichtung an und grausen Tod;", "tokens": ["Ver\u00b7nich\u00b7tung", "an", "und", "grau\u00b7sen", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was will die Angst, was will uns Noth?", "tokens": ["Was", "will", "die", "Angst", ",", "was", "will", "uns", "Noth", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$,", "PWS", "VMFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wir dr\u00fccken", "tokens": ["Wir", "dr\u00fc\u00b7cken"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Im Taumel die Hand;", "tokens": ["Im", "Tau\u00b7mel", "die", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Mich r\u00fchrt dein Gewand,", "tokens": ["Mich", "r\u00fchrt", "dein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Du schwebest dahin, ich taumle zur\u00fcck \u2013", "tokens": ["Du", "schwe\u00b7best", "da\u00b7hin", ",", "ich", "taum\u00b7le", "zu\u00b7r\u00fcck", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "PPER", "VVFIN", "PTKVZ", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Auch Verzweiflung ist Gl\u00fcck.", "tokens": ["Auch", "Ver\u00b7zwei\u00b7flung", "ist", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Aus diesem Entz\u00fccken,", "tokens": ["Aus", "die\u00b7sem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-++-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Und was wir heut lachten,", "tokens": ["Und", "was", "wir", "heut", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Entsprie\u00dft wohl Verachten", "tokens": ["Ent\u00b7sprie\u00dft", "wohl", "Ver\u00b7ach\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und giftiger Neid;", "tokens": ["Und", "gif\u00b7ti\u00b7ger", "Neid", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "O herrliche Zeit!", "tokens": ["O", "herr\u00b7li\u00b7che", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wenn ich dich verh\u00f6hne,", "tokens": ["Wenn", "ich", "dich", "ver\u00b7h\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Winkt dort mir die Sch\u00f6ne,", "tokens": ["Winkt", "dort", "mir", "die", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.8": {"text": "Und wird meine Braut;", "tokens": ["Und", "wird", "mei\u00b7ne", "Braut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Die andere schaut", "tokens": ["Die", "an\u00b7de\u00b7re", "schaut"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Noch k\u00fchner darein;", "tokens": ["Noch", "k\u00fch\u00b7ner", "da\u00b7rein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Soll dies' es denn sein? \u2013", "tokens": ["Soll", "dies'", "es", "denn", "sein", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "ADV", "VAINF", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "So taumeln wir alle", "tokens": ["So", "tau\u00b7meln", "wir", "al\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im Schwindel die Halle", "tokens": ["Im", "Schwin\u00b7del", "die", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Des Lebens hinab,", "tokens": ["Des", "Le\u00b7bens", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Kein Lieben, kein Leben,", "tokens": ["Kein", "Lie\u00b7ben", ",", "kein", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kein Sein uns gegeben,", "tokens": ["Kein", "Sein", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nur Tr\u00e4umen und Grab:", "tokens": ["Nur", "Tr\u00e4u\u00b7men", "und", "Grab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da unten bedecken", "tokens": ["Da", "un\u00b7ten", "be\u00b7de\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wohl Blumen und Klee", "tokens": ["Wohl", "Blu\u00b7men", "und", "Klee"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Noch grimmere Schrecken,", "tokens": ["Noch", "grim\u00b7me\u00b7re", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Noch wilderes Weh;", "tokens": ["Noch", "wil\u00b7de\u00b7res", "Weh", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Drum lauter ihr Cymbeln, du Paukenklang,", "tokens": ["Drum", "lau\u00b7ter", "ihr", "Cym\u00b7beln", ",", "du", "Pau\u00b7ken\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PIAT", "PPOSAT", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Noch schreiender gellender H\u00f6rnergesang!", "tokens": ["Noch", "schrei\u00b7e