_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da regt sich b\u00f6ser Wille,", "tokens": ["Da", "regt", "sich", "b\u00f6\u00b7ser", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Kerker ihn zu retten,", "tokens": ["Vom", "Ker\u00b7ker", "ihn", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Tollen los zu machen:", "tokens": ["Den", "Tol\u00b7len", "los", "zu", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da h\u00f6rt man Pauken klingen,", "tokens": ["Da", "h\u00f6rt", "man", "Pau\u00b7ken", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da bricht hervor mit Lachen", "tokens": ["Da", "bricht", "her\u00b7vor", "mit", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Trommeten-Klang und Krachen,", "tokens": ["Trom\u00b7me\u00b7ten\u00b7Klang", "und", "Kra\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.9": {"text": "Dazwischen Fl\u00f6ten singen,", "tokens": ["Da\u00b7zwi\u00b7schen", "Fl\u00f6\u00b7ten", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und Pfeifent\u00f6ne springen", "tokens": ["Und", "Pfei\u00b7fen\u00b7t\u00f6\u00b7ne", "sprin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Mit gellendem Geschrei", "tokens": ["Mit", "gel\u00b7len\u00b7dem", "Ge\u00b7schrei"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Zwischen dr\u00f6hnenden t\u00f6nenden Geigen", "tokens": ["Zwi\u00b7schen", "dr\u00f6h\u00b7nen\u00b7den", "t\u00f6\u00b7nen\u00b7den", "Gei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "In rasender Wuth herbei,", "tokens": ["In", "ra\u00b7sen\u00b7der", "Wuth", "her\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Das wilde Gem\u00fcth zu zeigen,", "tokens": ["Das", "wil\u00b7de", "Ge\u00b7m\u00fcth", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Und grimmig zu morden das stille kindliche Schweigen. \u2013", "tokens": ["Und", "grim\u00b7mig", "zu", "mor\u00b7den", "das", "stil\u00b7le", "kind\u00b7li\u00b7che", "Schwei\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "PTKZU", "VVINF", "ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Wohin dreht s