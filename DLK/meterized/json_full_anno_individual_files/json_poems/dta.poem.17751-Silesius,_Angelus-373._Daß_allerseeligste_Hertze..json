{"dta.poem.17751": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "373.  Da\u00df allerseeligste Hertze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ein reines Hertz schaut Gott/ ein heilges schm\u00e4ket Jhn:", "tokens": ["Ein", "rei\u00b7nes", "Hertz", "schaut", "Gott", "/", "ein", "heil\u00b7ges", "schm\u00e4\u00b7ket", "Jhn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$(", "ART", "ADJA", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ein Verliebetes wil er zu Wohnen ziehn.", "tokens": ["In", "ein", "Ver\u00b7lie\u00b7be\u00b7tes", "wil", "er", "zu", "Woh\u00b7nen", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie seelig ist der Mensch der sich befleist und \u00fcbt/", "tokens": ["Wie", "see\u00b7lig", "ist", "der", "Mensch", "der", "sich", "be\u00b7fleist", "und", "\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "PRF", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df Jhm sein Hertze wird rein Heilig und verliebt!", "tokens": ["Da\u00df", "Jhm", "sein", "Hert\u00b7ze", "wird", "rein", "Hei\u00b7lig", "und", "ver\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "VVFIN", "VAFIN", "ADJD", "ADJA", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}