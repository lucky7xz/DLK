{"dta.poem.23438": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Stille Messe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Pfaffe bin ich nie gewesen,", "tokens": ["Ein", "Pfaf\u00b7fe", "bin", "ich", "nie", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr aber sollt mich doch verstehn:", "tokens": ["Ihr", "a\u00b7ber", "sollt", "mich", "doch", "ver\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will euch heute Messe lesen,", "tokens": ["Ich", "will", "euch", "heu\u00b7te", "Mes\u00b7se", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr euch zu Gott dem Herren flehn.", "tokens": ["F\u00fcr", "euch", "zu", "Gott", "dem", "Her\u00b7ren", "flehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und steh' ich hier aus steilem Pfade,", "tokens": ["Und", "steh'", "ich", "hier", "aus", "stei\u00b7lem", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So steh' ich doch in Gottes Hand:", "tokens": ["So", "steh'", "ich", "doch", "in", "Got\u00b7tes", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Me\u00dfgewand ist Gottes Gnade", "tokens": ["Mein", "Me\u00df\u00b7ge\u00b7wand", "ist", "Got\u00b7tes", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Monstranz mein Vaterland.", "tokens": ["Und", "die", "Mons\u00b7tranz", "mein", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Wir sind der Leib des Herren heute,", "tokens": ["Wir", "sind", "der", "Leib", "des", "Her\u00b7ren", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir leiden seine Qual und Pein,", "tokens": ["Wir", "lei\u00b7den", "sei\u00b7ne", "Qual", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sind der frechen Willk\u00fcr Beute \u2014", "tokens": ["Wir", "sind", "der", "fre\u00b7chen", "Will\u00b7k\u00fcr", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O Herr vom Himmel sie darein!", "tokens": ["O", "Herr", "vom", "Him\u00b7mel", "sie", "da\u00b7rein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "PPER", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Verwandl' uns, Herr, uns deine Knechte", "tokens": ["Ver\u00b7wandl'", "uns", ",", "Herr", ",", "uns", "dei\u00b7ne", "Knech\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch dieses heil'ge Sacrament!", "tokens": ["Durch", "die\u00b7ses", "heil'\u00b7ge", "Sa\u00b7cra\u00b7ment", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gieb du uns deines Sohnes Rechte,", "tokens": ["Gieb", "du", "uns", "dei\u00b7nes", "Soh\u00b7nes", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der uns ja deine Kinder nennt!", "tokens": ["Der", "uns", "ja", "dei\u00b7ne", "Kin\u00b7der", "nennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mach den Bedr\u00fcckern die Bedr\u00fcckten,", "tokens": ["Mach", "den", "Be\u00b7dr\u00fc\u00b7ckern", "die", "Be\u00b7dr\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mach all' an Recht und Freiheit gleich!", "tokens": ["Mach", "all'", "an", "Recht", "und", "Frei\u00b7heit", "gleich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gieb den Bedr\u00e4ngten und Geb\u00fcckten", "tokens": ["Gieb", "den", "Be\u00b7dr\u00e4ng\u00b7ten", "und", "Ge\u00b7b\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hienieden schon dein Himmelreich!", "tokens": ["Hien\u00b7ie\u00b7den", "schon", "dein", "Him\u00b7mel\u00b7reich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}