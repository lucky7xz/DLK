{"dta.poem.11565": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An Clara.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6re das lieblichste Wunder, das ich f\u00fcrwahr nicht er-", "tokens": ["H\u00f6\u00b7re", "das", "lieb\u00b7lichs\u00b7te", "Wun\u00b7der", ",", "das", "ich", "f\u00fcr\u00b7wahr", "nicht", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "TRUNC"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Auch erdichtet w\u00e4r' es wohl sch\u00f6n, doch sah ich's mit", "tokens": ["Auch", "er\u00b7dich\u00b7tet", "w\u00e4r'", "es", "wohl", "sch\u00f6n", ",", "doch", "sah", "ich's", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PIS", "APPR"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Unter dem bl\u00fchenden Apfelbaum sa\u00df ich auf dem be-", "tokens": ["Un\u00b7ter", "dem", "bl\u00fc\u00b7hen\u00b7den", "Ap\u00b7fel\u00b7baum", "sa\u00df", "ich", "auf", "dem", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "TRUNC"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.4": {"text": "M\u00e4uerchen, still in Gedanken vertieft; es ruhte das neue", "tokens": ["M\u00e4u\u00b7er\u00b7chen", ",", "still", "in", "Ge\u00b7dan\u00b7ken", "ver\u00b7tieft", ";", "es", "ruh\u00b7te", "das", "neu\u00b7e"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "APPR", "NN", "VVPP", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Testament mir halbge\u00f6ffnet zwischen den Fingern,", "tokens": ["Tes\u00b7ta\u00b7ment", "mir", "halb\u00b7ge\u00b7\u00f6ff\u00b7net", "zwi\u00b7schen", "den", "Fin\u00b7gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Klein und zierlich gebunden: (es kam vom treuesten", "tokens": ["Klein", "und", "zier\u00b7lich", "ge\u00b7bun\u00b7den", ":(", "es", "kam", "vom", "treu\u00b7es\u00b7ten"], "token_info": ["word", "word", "word", "word", "emoticon", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VVPP", "$(", "PPER", "VVFIN", "APPRART", "ADJA"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ach, du ruhest nun auch, mir unvergessen, im Grabe!)", "tokens": ["Ach", ",", "du", "ru\u00b7hest", "nun", "auch", ",", "mir", "un\u00b7ver\u00b7ges\u00b7sen", ",", "im", "Gra\u00b7be", "!", ")"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,", "PPER", "ADJD", "$,", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Lange sa\u00df ich und blickte nicht auf; mit Einem so l\u00e4\u00dft sich", "tokens": ["Lan\u00b7ge", "sa\u00df", "ich", "und", "blick\u00b7te", "nicht", "auf", ";", "mit", "Ei\u00b7nem", "so", "l\u00e4\u00dft", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "PTKNEG", "PTKVZ", "$.", "APPR", "ART", "ADV", "VVFIN", "PRF"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Mir ein Schmetterling nieder auf's Buch, er hebet und", "tokens": ["Mir", "ein", "Schmet\u00b7ter\u00b7ling", "nie\u00b7der", "auf's", "Buch", ",", "er", "he\u00b7bet", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "PTKVZ", "APPRART", "NN", "$,", "PPER", "VVFIN", "KON"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Dunkle Fl\u00fcgel mit schillerndem Blau, er dreht sich und", "tokens": ["Dunk\u00b7le", "Fl\u00fc\u00b7gel", "mit", "schil\u00b7lern\u00b7dem", "Blau", ",", "er", "dreht", "sich", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,", "PPER", "VVFIN", "PRF", "KON"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Hin und her auf dem Rande. Was suchst du, reizender", "tokens": ["Hin", "und", "her", "auf", "dem", "Ran\u00b7de", ".", "Was", "suchst", "du", ",", "rei\u00b7zen\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "KON", "ADV", "APPR", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "$,", "ADJA"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Lockte die blaue Decke dich an, der gl\u00e4nzende Goldschnitt?", "tokens": ["Lock\u00b7te", "die", "blau\u00b7e", "De\u00b7cke", "dich", "an", ",", "der", "gl\u00e4n\u00b7zen\u00b7de", "Gold\u00b7schnitt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Sahst du, get\u00e4uscht, im B\u00fcchlein die herrlichste Wunder-", "tokens": ["Sahst", "du", ",", "ge\u00b7t\u00e4uscht", ",", "im", "B\u00fcch\u00b7lein", "die", "herr\u00b7lichs\u00b7te", "Wun\u00b7der"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVPP", "$,", "APPRART", "NN", "ART", "ADJA", "TRUNC"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.14": {"text": "Oder zogen geheim dich himmlische Kr\u00e4fte hernieder", "tokens": ["O\u00b7der", "zo\u00b7gen", "ge\u00b7heim", "dich", "himm\u00b7li\u00b7sche", "Kr\u00e4f\u00b7te", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPER", "ADJA", "NN", "PTKVZ"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.15": {"text": "Des lebendigen Worts? Ich mu\u00df es glauben, denn", "tokens": ["Des", "le\u00b7ben\u00b7di\u00b7gen", "Worts", "?", "Ich", "mu\u00df", "es", "glau\u00b7ben", ",", "denn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,", "KON"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Weilest du noch, wie gebannt und scheinst wie trunken,", "tokens": ["Wei\u00b7lest", "du", "noch", ",", "wie", "ge\u00b7bannt", "und", "scheinst", "wie", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "VVPP", "KON", "VVFIN", "KOKOM", "ADJD", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.17": {"text": "Aber von nun an bist du auf alle Tage gesegnet!", "tokens": ["A\u00b7ber", "von", "nun", "an", "bist", "du", "auf", "al\u00b7le", "Ta\u00b7ge", "ge\u00b7seg\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "APZR", "VAFIN", "PPER", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+----+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Unverletzlich dein Leib, dir altern nimmer die Schwingen!", "tokens": ["Un\u00b7ver\u00b7letz\u00b7lich", "dein", "Leib", ",", "dir", "al\u00b7tern", "nim\u00b7mer", "die", "Schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.19": {"text": "Und wohin du k\u00fcnftig die zarten F\u00fc\u00dfe wirst setzen,", "tokens": ["Und", "wo\u00b7hin", "du", "k\u00fcnf\u00b7tig", "die", "zar\u00b7ten", "F\u00fc\u00b7\u00dfe", "wirst", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Thauet Segen von dir. Jezt eile hinunter zum Garten,", "tokens": ["Thau\u00b7et", "Se\u00b7gen", "von", "dir", ".", "Jezt", "ei\u00b7le", "hin\u00b7un\u00b7ter", "zum", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPER", "$.", "ADV", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.21": {"text": "Den das beste M\u00e4dchen besucht am fr\u00fchesten Morgen,", "tokens": ["Den", "das", "bes\u00b7te", "M\u00e4d\u00b7chen", "be\u00b7sucht", "am", "fr\u00fc\u00b7hes\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Eile zur Lilie du, gleich wird die Knospe sich \u00f6ffnen", "tokens": ["Ei\u00b7le", "zur", "Li\u00b7lie", "du", ",", "gleich", "wird", "die", "Knos\u00b7pe", "sich", "\u00f6ff\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "PPER", "$,", "ADV", "VAFIN", "ART", "NN", "PRF", "VVINF"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.23": {"text": "Unter dir, dann k\u00fcsse sie tief in den Busen: von Stund an", "tokens": ["Un\u00b7ter", "dir", ",", "dann", "k\u00fcs\u00b7se", "sie", "tief", "in", "den", "Bu\u00b7sen", ":", "von", "Stund", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$.", "APPR", "NN", "APPR"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "G\u00f6ttlich befruchtet, athmet sie Geist und himmlisches Leben.", "tokens": ["G\u00f6tt\u00b7lich", "be\u00b7fruch\u00b7tet", ",", "ath\u00b7met", "sie", "Geist", "und", "himm\u00b7li\u00b7sches", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "PPER", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.25": {"text": "Wenn die Gute nun kommt, vor den hohen Stengel ge-", "tokens": ["Wenn", "die", "Gu\u00b7te", "nun", "kommt", ",", "vor", "den", "ho\u00b7hen", "Sten\u00b7gel", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.26": {"text": "Steht sie befangen, entz\u00fcckt von paradiesischer N\u00e4he,", "tokens": ["Steht", "sie", "be\u00b7fan\u00b7gen", ",", "ent\u00b7z\u00fcckt", "von", "pa\u00b7ra\u00b7die\u00b7si\u00b7scher", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Ahnungsvoll wie im Traum die holde Seele versunken.", "tokens": ["Ah\u00b7nungs\u00b7voll", "wie", "im", "Traum", "die", "hol\u00b7de", "See\u00b7le", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "---+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}