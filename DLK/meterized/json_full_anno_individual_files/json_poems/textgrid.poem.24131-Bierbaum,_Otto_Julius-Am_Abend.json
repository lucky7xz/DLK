{"textgrid.poem.24131": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Am Abend", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir haben auch die Sinne wohlgethan,", "tokens": ["Mir", "ha\u00b7ben", "auch", "die", "Sin\u00b7ne", "wohl\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mich lachte auch das Leben s\u00fc\u00dfe an;", "tokens": ["Mich", "lach\u00b7te", "auch", "das", "Le\u00b7ben", "s\u00fc\u00b7\u00dfe", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nun bin ich matt.", "tokens": ["Nun", "bin", "ich", "matt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Nun sehn ich mich nach einem stillen Schlu\u00df,", "tokens": ["Nun", "sehn", "ich", "mich", "nach", "ei\u00b7nem", "stil\u00b7len", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nach einem tiefen Schlafe, der kein Mu\u00df", "tokens": ["Nach", "ei\u00b7nem", "tie\u00b7fen", "Schla\u00b7fe", ",", "der", "kein", "Mu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"t