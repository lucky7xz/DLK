{"textgrid.poem.61080": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Friedrich Rotbart", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief im Scho\u00dfe des Kyffh\u00e4users", "tokens": ["Tief", "im", "Scho\u00b7\u00dfe", "des", "Kyff\u00b7h\u00e4u\u00b7sers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei der Ampel rotem Schein", "tokens": ["Bei", "der", "Am\u00b7pel", "ro\u00b7tem", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzt der alte Kaiser Friedrich", "tokens": ["Sitzt", "der", "al\u00b7te", "Kai\u00b7ser", "Fried\u00b7rich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "An dem Tisch von Marmorstein.", "tokens": ["An", "dem", "Tisch", "von", "Mar\u00b7mor\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihn umwallt der Purpurmantel,", "tokens": ["Ihn", "um\u00b7wallt", "der", "Pur\u00b7pur\u00b7man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn umf\u00e4ngt der R\u00fcstung Pracht,", "tokens": ["Ihn", "um\u00b7f\u00e4ngt", "der", "R\u00fcs\u00b7tung", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch auf seinen Augenwimpern", "tokens": ["Doch", "auf", "sei\u00b7nen", "Au\u00b7gen\u00b7wim\u00b7pern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt des Schlafes tiefe Nacht.", "tokens": ["Liegt", "des", "Schla\u00b7fes", "tie\u00b7fe", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vorgesunken ruht das Antlitz,", "tokens": ["Vor\u00b7ge\u00b7sun\u00b7ken", "ruht", "das", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drin sich Ernst und Milde paart;", "tokens": ["Drin", "sich", "Ernst", "und", "Mil\u00b7de", "paart", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "NE", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den Marmortisch gewachsen", "tokens": ["Durch", "den", "Mar\u00b7mor\u00b7tisch", "ge\u00b7wach\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist sein langer, goldner Bart.", "tokens": ["Ist", "sein", "lan\u00b7ger", ",", "gold\u00b7ner", "Bart", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Rings wie eh'rne Bilder stehen", "tokens": ["Rings", "wie", "eh'r\u00b7ne", "Bil\u00b7der", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Ritter um ihn her,", "tokens": ["Sei\u00b7ne", "Rit\u00b7ter", "um", "ihn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Harnischgl\u00e4nzend, schwertumg\u00fcrtet,", "tokens": ["Har\u00b7nischgl\u00e4n\u00b7zend", ",", "schwer\u00b7tum\u00b7g\u00fcr\u00b7tet", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aber tief im Schlaf wie er.", "tokens": ["A\u00b7ber", "tief", "im", "Schlaf", "wie", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Heinrich auch, der Ofterdinger,", "tokens": ["Hein\u00b7rich", "auch", ",", "der", "Of\u00b7ter\u00b7din\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist in ihrer stummen Schar,", "tokens": ["Ist", "in", "ih\u00b7rer", "stum\u00b7men", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den liederreichen Lippen,", "tokens": ["Mit", "den", "lie\u00b7der\u00b7rei\u00b7chen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem blondgelockten Haar.", "tokens": ["Mit", "dem", "blond\u00b7ge\u00b7lock\u00b7ten", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Seine Harfe ruht dem S\u00e4nger", "tokens": ["Sei\u00b7ne", "Har\u00b7fe", "ruht", "dem", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Linken ohne Klang;", "tokens": ["In", "der", "Lin\u00b7ken", "oh\u00b7ne", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch auf seiner hohen Stirne", "tokens": ["Doch", "auf", "sei\u00b7ner", "ho\u00b7hen", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schl\u00e4ft ein k\u00fcnftiger Gesang.", "tokens": ["Schl\u00e4ft", "ein", "k\u00fcnf\u00b7ti\u00b7ger", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Alles schweigt, nur hin und wieder", "tokens": ["Al\u00b7les", "schweigt", ",", "nur", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ADV", "PTKVZ", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4llt ein Tropfen vom Gestein,", "tokens": ["F\u00e4llt", "ein", "Trop\u00b7fen", "vom", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis der gro\u00dfe Morgen pl\u00f6tzlich", "tokens": ["Bis", "der", "gro\u00b7\u00dfe", "Mor\u00b7gen", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bricht mit Feuersglut herein;", "tokens": ["Bricht", "mit", "Feu\u00b7ers\u00b7glut", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bis der Adler stolzen Fluges", "tokens": ["Bis", "der", "Ad\u00b7ler", "stol\u00b7zen", "Flu\u00b7ges"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um des Berges Gipfel zieht,", "tokens": ["Um", "des", "Ber\u00b7ges", "Gip\u00b7fel", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df vor seines Fittichs Rauschen", "tokens": ["Da\u00df", "vor", "sei\u00b7nes", "Fit\u00b7tichs", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dort der Rabenschwarm entflieht.", "tokens": ["Dort", "der", "Ra\u00b7ben\u00b7schwarm", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber dann wie ferner Donner", "tokens": ["A\u00b7ber", "dann", "wie", "fer\u00b7ner", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rollt es durch den Berg herauf,", "tokens": ["Rollt", "es", "durch", "den", "Berg", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Kaiser greift zum Schwerte,", "tokens": ["Und", "der", "Kai\u00b7ser", "greift", "zum", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Ritter wachen auf.", "tokens": ["Und", "die", "Rit\u00b7ter", "wa\u00b7chen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Laut in seinen Angeln dr\u00f6hnend", "tokens": ["Laut", "in", "sei\u00b7nen", "An\u00b7geln", "dr\u00f6h\u00b7nend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tut sich auf das eh'rne Tor;", "tokens": ["Tut", "sich", "auf", "das", "eh'r\u00b7ne", "Tor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Barbarossa mit den Seinen", "tokens": ["Bar\u00b7ba\u00b7ros\u00b7sa", "mit", "den", "Sei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steigt im Waffenschmuck empor.", "tokens": ["Steigt", "im", "Waf\u00b7fen\u00b7schmuck", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Auf dem Helm tr\u00e4gt er die Krone", "tokens": ["Auf", "dem", "Helm", "tr\u00e4gt", "er", "die", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den Sieg in seiner Hand;", "tokens": ["Und", "den", "Sieg", "in", "sei\u00b7ner", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwerter blitzen, Harfen klingen,", "tokens": ["Schwer\u00b7ter", "blit\u00b7zen", ",", "Har\u00b7fen", "klin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo er schreitet durch das Land.", "tokens": ["Wo", "er", "schrei\u00b7tet", "durch", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und dem alten Kaiser beugen", "tokens": ["Und", "dem", "al\u00b7ten", "Kai\u00b7ser", "beu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich die V\u00f6lker allzugleich", "tokens": ["Sich", "die", "V\u00f6l\u00b7ker", "all\u00b7zu\u00b7gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aufs neu' zu Aachen gr\u00fcndet", "tokens": ["Und", "aufs", "neu'"