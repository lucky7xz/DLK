], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wittelsbach wird er genannt,", "tokens": ["Der", "Wit\u00b7tels\u00b7bach", "wird", "er", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In seinem treuen Scho\u00dfe kann", "tokens": ["In", "sei\u00b7nem", "treu\u00b7en", "Scho\u00b7\u00dfe", "kann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich bergen jedweder Untertan.", "tokens": ["Sich", "ber\u00b7gen", "jed\u00b7we\u00b7der", "Un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und als das siebente Kn\u00e4blein kam,", "tokens": ["Und", "als", "das", "sie\u00b7ben\u00b7te", "Kn\u00e4\u00b7blein", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er K\u00f6nig Rupprecht zum Paten nahm,", "tokens": ["Er", "K\u00f6\u00b7nig", "Rup\u00b7precht", "zum", "Pa\u00b7ten", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das ist ein Brauch von altem Korn,", "tokens": ["Das", "ist", "ein", "Brauch", "von", "al\u00b7tem", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da