{"textgrid.poem.66276": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Sabbathlied", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gottesruhe, Sabbathsstille,", "tokens": ["Got\u00b7tes\u00b7ru\u00b7he", ",", "Sab\u00b7baths\u00b7stil\u00b7le", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm vom Thron des Ewigen,", "tokens": ["Komm", "vom", "Thron", "des", "E\u00b7wi\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Bring der hohen Andacht F\u00fclle", "tokens": ["Bring", "der", "ho\u00b7hen", "An\u00b7dacht", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das Herz der Feiernden,", "tokens": ["In", "das", "Herz", "der", "Fei\u00b7ern\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Da\u00df sie sich der Welt entschwingen,", "tokens": ["Da\u00df", "sie", "sich", "der", "Welt", "ent\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00f6ren, beten, danken, singen.", "tokens": ["H\u00f6\u00b7ren", ",", "be\u00b7ten", ",", "dan\u00b7ken", ",", "sin\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mach sie himmlisch, unsre Seelen;", "tokens": ["Mach", "sie", "himm\u00b7lisch", ",", "uns\u00b7re", "See\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreck die Fluth der Leidenschaft;", "tokens": ["Schreck", "die", "Fluth", "der", "Lei\u00b7den\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn uns Erdensorgen qu\u00e4len,", "tokens": ["Wenn", "uns", "Er\u00b7den\u00b7sor\u00b7gen", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geist des Herrn, so gib uns Kraft,", "tokens": ["Geist", "des", "Herrn", ",", "so", "gib", "uns", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADV", "VVIMP", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wahrheit, Leben, Licht und St\u00e4rke,", "tokens": ["Wahr\u00b7heit", ",", "Le\u00b7ben", ",", "Licht", "und", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hinzuschaun auf Gottes Werke.", "tokens": ["Hin\u00b7zu\u00b7schaun", "auf", "Got\u00b7tes", "Wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Treibe banger Zweifel Wolke", "tokens": ["Trei\u00b7be", "ban\u00b7ger", "Zwei\u00b7fel", "Wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des H\u00f6rers Seele weg,", "tokens": ["Von", "des", "H\u00f6\u00b7rers", "See\u00b7le", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeig dem bluterkauften Volke,", "tokens": ["Zeig", "dem", "blu\u00b7ter\u00b7kauf\u00b7ten", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mittler! deiner Sendung Zweck,", "tokens": ["Mitt\u00b7ler", "!", "dei\u00b7ner", "Sen\u00b7dung", "Zweck", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lehr uns glauben, hoffen, lieben,", "tokens": ["Lehr", "uns", "glau\u00b7ben", ",", "hof\u00b7fen", ",", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schmach erdulden, Demuth \u00fcben.", "tokens": ["Schmach", "er\u00b7dul\u00b7den", ",", "De\u00b7muth", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nimm dich, Jesu, der Verirrten", "tokens": ["Nimm", "dich", ",", "Je\u00b7su", ",", "der", "Ver\u00b7irr\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "NE", "$,", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Unter deiner Heerde an;", "tokens": ["Un\u00b7ter", "dei\u00b7ner", "Heer\u00b7de", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Strafe, die dein Volk verwirrten", "tokens": ["Stra\u00b7fe", ",", "die", "dein", "Volk", "ver\u00b7wirr\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald mit Laster, bald mit Wahn;", "tokens": ["Bald", "mit", "Las\u00b7ter", ",", "bald", "mit", "Wahn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die den Unsinn t\u00e4uschend schm\u00fccken", "tokens": ["Die", "den", "Un\u00b7sinn", "t\u00e4u\u00b7schend", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und von Einfalt uns verr\u00fccken.", "tokens": ["Und", "von", "Ein\u00b7falt", "uns", "ver\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Heilig seien die Ges\u00e4nge,", "tokens": ["Hei\u00b7lig", "sei\u00b7en", "die", "Ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wahr die Predigt, die uns lehrt,", "tokens": ["Wahr", "die", "Pre\u00b7digt", ",", "die", "uns", "lehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gotterf\u00fcllt der Beter Menge,", "tokens": ["Got\u00b7ter\u00b7f\u00fcllt", "der", "Be\u00b7ter", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des S\u00fcnders Herz bekehrt;", "tokens": ["Und", "des", "S\u00fcn\u00b7ders", "Herz", "be\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sammle Thr\u00e4nen, die die Deinen", "tokens": ["Samm\u00b7le", "Thr\u00e4\u00b7nen", ",", "die", "die", "Dei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "PRELS", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Gluth der Andacht weinen.", "tokens": ["In", "der", "Gluth", "der", "An\u00b7dacht", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Glaubig la\u00df des Himmels Erben", "tokens": ["Glau\u00b7big", "la\u00df", "des", "Him\u00b7mels", "Er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich dem Liebesmahle nahn,", "tokens": ["Sich", "dem", "Lie\u00b7bes\u00b7mah\u00b7le", "nahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner m\u00fcsse zum Verderben", "tokens": ["Kei\u00b7ner", "m\u00fcs\u00b7se", "zum", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinen Leib, dein Blut empfahn;", "tokens": ["Dei\u00b7nen", "Leib", ",", "dein", "Blut", "em\u00b7pfahn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df, die Sichern zu erwecken,", "tokens": ["La\u00df", ",", "die", "Si\u00b7chern", "zu", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie des Kelches Blitze schrecken.", "tokens": ["Sie", "des", "Kel\u00b7ches", "Blit\u00b7ze", "schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Tr\u00f6ste, die mit schwerem Herzen,", "tokens": ["Tr\u00f6s\u00b7te", ",", "die", "mit", "schwe\u00b7rem", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott! vor dir im Tempel stehn,", "tokens": ["Gott", "!", "vor", "dir", "im", "Tem\u00b7pel", "stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh herab auf ihre Schmerzen", "tokens": ["Sieh", "her\u00b7ab", "auf", "ih\u00b7re", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der stummen Blicke Flehn;", "tokens": ["Und", "der", "stum\u00b7men", "Bli\u00b7cke", "Flehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wittwen, Waisen, Arme tr\u00f6ste,", "tokens": ["Witt\u00b7wen", ",", "Wai\u00b7sen", ",", "Ar\u00b7me", "tr\u00f6s\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gottes Sohn, der sie erl\u00f6ste;", "tokens": ["Got\u00b7tes", "Sohn", ",", "der", "sie", "er\u00b7l\u00f6s\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lindre der Gefangnen Ketten,", "tokens": ["Lind\u00b7re", "der", "Ge\u00b7fang\u00b7nen", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach erbarm dich ihrer Schmach,", "tokens": ["Ach", "er\u00b7barm", "dich", "ih\u00b7rer", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJD", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie in den Tempel treten,", "tokens": ["Wenn", "sie", "in", "den", "Tem\u00b7pel", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Fesseln rasseln nach:", "tokens": ["Und", "die", "Fes\u00b7seln", "ras\u00b7seln", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "St\u00e4rke, die in Finsternissen", "tokens": ["St\u00e4r\u00b7ke", ",", "die", "in", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deinen Sabbath feiern m\u00fcssen.", "tokens": ["Dei\u00b7nen", "Sab\u00b7ba\u00b7th", "fei\u00b7ern", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Wenn wir unsre H\u00e4nd' erheben", "tokens": ["Wenn", "wir", "uns\u00b7re", "H\u00e4nd'", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr den F\u00fcrsten, f\u00fcr das Land,", "tokens": ["F\u00fcr", "den", "F\u00fcrs\u00b7ten", ",", "f\u00fcr", "das", "Land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr der treuen B\u00fcrger Leben,", "tokens": ["F\u00fcr", "der", "treu\u00b7en", "B\u00fcr\u00b7ger", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr die Werke deiner Hand,", "tokens": ["F\u00fcr", "die", "Wer\u00b7ke", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und f\u00fcr Jesu Christi Glieder,", "tokens": ["Und", "f\u00fcr", "Je\u00b7su", "Chris\u00b7ti", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sende dann Erh\u00f6rung nieder.", "tokens": ["Sen\u00b7de", "dann", "Er\u00b7h\u00f6\u00b7rung", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wenn wir Vaterunser beten", "tokens": ["Wenn", "wir", "Va\u00b7ter\u00b7un\u00b7ser", "be\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fcr des kranken Bruders Qual,", "tokens": ["F\u00fcr", "des", "kran\u00b7ken", "Bru\u00b7ders", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der mit bangen Seelenn\u00f6then", "tokens": ["Der", "mit", "ban\u00b7gen", "See\u00b7len\u00b7n\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht hinab ins Todesthal;", "tokens": ["Sieht", "hin\u00b7ab", "ins", "To\u00b7dest\u00b7hal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! so la\u00df auf unser Flehen", "tokens": ["Ach", "!", "so", "la\u00df", "auf", "un\u00b7ser", "Fle\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ADV", "VVIMP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Qual vor\u00fcbergehen!", "tokens": ["Sei\u00b7ne", "Qual", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Bis wir alle hingelangen", "tokens": ["Bis", "wir", "al\u00b7le", "hin\u00b7ge\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hin zu dir, o Lebensf\u00fcrst,", "tokens": ["Hin", "zu", "dir", ",", "o", "Le\u00b7bens\u00b7f\u00fcrst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo du von der Deinen Wangen", "tokens": ["Wo", "du", "von", "der", "Dei\u00b7nen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Thr\u00e4nen trocknen wirst:", "tokens": ["Al\u00b7le", "Thr\u00e4\u00b7nen", "trock\u00b7nen", "wirst", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo wir, Engel Gottes, euern", "tokens": ["Wo", "wir", ",", "En\u00b7gel", "Got\u00b7tes", ",", "eu\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "$,", "NE", "NN", "$,", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gro\u00dfen Sabbath mit euch feiern.", "tokens": ["Gro\u00b7\u00dfen", "Sab\u00b7ba\u00b7th", "mit", "euch", "fei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Gottesruhe, Sabbathsstille,", "tokens": ["Got\u00b7tes\u00b7ru\u00b7he", ",", "Sab\u00b7baths\u00b7stil\u00b7le", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm vom Thron des Ewigen,", "tokens": ["Komm", "vom", "Thron", "des", "E\u00b7wi\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "ADJA", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Bring der hohen Andacht F\u00fclle", "tokens": ["Bring", "der", "ho\u00b7hen", "An\u00b7dacht", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das Herz der Feiernden,", "tokens": ["In", "das", "Herz", "der", "Fei\u00b7ern\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Da\u00df sie sich der Welt entschwingen,", "tokens": ["Da\u00df", "sie", "sich", "der", "Welt", "ent\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "tro