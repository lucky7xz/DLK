{"dta.poem.733": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XiX.  An sein Buch das er  H. Michael Borck  \n \u00fcbersendet.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Geh smmer/ weil dich ja gelustet hin zu gehen. ", "tokens": ["Geh", "sm\u00b7mer", "/", "weil", "dich", "ja", "ge\u00b7lus\u00b7tet", "hin", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "KOUS", "PPER", "ADV", "VVPP", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd suche Preussens ", "tokens": ["Vnd", "su\u00b7che", "Preus\u00b7sens"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Vnd Frid\u2019 vnd Krieg verm\u00e4hlt. als in verkn\u00fcpfter ", "tokens": ["Vnd", "Frid'", "vnd", "Krieg", "ver\u00b7m\u00e4hlt", ".", "als", "in", "ver\u00b7kn\u00fcpf\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVPP", "$.", "KOUS", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vnd sprich dem zu/ durch den die grosse Stadt kan stehen.", "tokens": ["Vnd", "sprich", "dem", "zu", "/", "durch", "den", "die", "gros\u00b7se", "Stadt", "kan", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "PTKZU", "$(", "APPR", "ART", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er den die Tugend liebt/ den trewe must' erh\u00f6hen/ ", "tokens": ["Er", "den", "die", "Tu\u00b7gend", "liebt", "/", "den", "tre\u00b7we", "must'", "er\u00b7h\u00f6\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ART", "NN", "VVFIN", "$(", "ART", "ADJA", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den redligkeit erkiest/ der raht f\u00fcr alles weh' ", "tokens": ["Den", "red\u00b7lig\u00b7keit", "er\u00b7kiest", "/", "der", "raht", "f\u00fcr", "al\u00b7les", "weh'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "APPR", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In schweren f\u00e4llen wei\u00df/ ergetzt (ob schon der Schne ", "tokens": ["In", "schwe\u00b7ren", "f\u00e4l\u00b7len", "wei\u00df", "/", "er\u00b7getzt", "(", "ob", "schon", "der", "Sch\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVINF", "VVFIN", "$(", "VVPP", "$(", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der greisen Jahr jhn deckt) sich t\u00e4glich mit Astreen", "tokens": ["Der", "grei\u00b7sen", "Jahr", "jhn", "deckt", ")", "sich", "t\u00e4g\u00b7lich", "mit", "A\u00b7streen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$(", "PRF", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ob schon Minerva selbst/ vor seinem mund\u2019 erbleicht:", "tokens": ["Ob", "schon", "Mi\u00b7ner\u00b7va", "selbst", "/", "vor", "sei\u00b7nem", "mund'", "er\u00b7bleicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "ADV", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ob Phoebus jhm die Leyer; die wage Themis reicht;", "tokens": ["Ob", "Phoe\u00b7bus", "jhm", "die", "Le\u00b7yer", ";", "die", "wa\u00b7ge", "The\u00b7mis", "reicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "ART", "NN", "$.", "ART", "VVFIN", "NE", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Ist seiner freundtligkeit do", "tokens": ["Ist", "sei\u00b7ner", "freundt\u00b7lig\u00b7keit", "do"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er ists der mir viel ", "tokens": ["Er", "ists", "der", "mir", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er ists/ der mich nicht nur ins Angesicht geprei\u00df't/ ", "tokens": ["Er", "ists", "/", "der", "mich", "nicht", "nur", "ins", "An\u00b7ge\u00b7sicht", "ge\u00b7prei\u00df't", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PRELS", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wen k\u00f6nt ich dir wol sonst zum Freund vnd Richter setzen. ", "tokens": ["Wen", "k\u00f6nt", "ich", "dir", "wol", "sonst", "zum", "Freund", "vnd", "Rich\u00b7ter", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "ADV", "ADV", "APPRART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}