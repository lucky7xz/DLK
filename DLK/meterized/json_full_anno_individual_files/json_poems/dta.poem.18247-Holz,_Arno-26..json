{"dta.poem.18247": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "26.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Und hoch schl\u00e4gst du empor in deinen Tr\u00e4umen,", "tokens": ["Und", "hoch", "schl\u00e4gst", "du", "em\u00b7por", "in", "dei\u00b7nen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn dir die Sehnsucht ihre Fl\u00fcgel leiht", "tokens": ["Wenn", "dir", "die", "Sehn\u00b7sucht", "ih\u00b7re", "Fl\u00fc\u00b7gel", "leiht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mit dir wandelt unter Bl\u00fcthenb\u00e4umen.", "tokens": ["Und", "mit", "dir", "wan\u00b7delt", "un\u00b7ter", "Bl\u00fc\u00b7then\u00b7b\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kein W\u00f6lkchen segelt durch das Blau der Luft,", "tokens": ["Kein", "W\u00f6lk\u00b7chen", "se\u00b7gelt", "durch", "das", "Blau", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Knospen brechen und die Fr\u00fcchte schwellen,", "tokens": ["Die", "Knos\u00b7pen", "bre\u00b7chen", "und", "die", "Fr\u00fcch\u00b7te", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und fernhin schaukelt sich's wie Rosenduft", "tokens": ["Und", "fern\u00b7hin", "schau\u00b7kelt", "sich's", "wie", "Ro\u00b7sen\u00b7duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sanft \u00fcber sanft bewegten Meereswellen.", "tokens": ["Sanft", "\u00fc\u00b7ber", "sanft", "be\u00b7weg\u00b7ten", "Mee\u00b7res\u00b7wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch dringt auch Erd und Himmel auf dich ein,", "tokens": ["Doch", "dringt", "auch", "Erd", "und", "Him\u00b7mel", "auf", "dich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "KON", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es l\u00e4\u00dft dich nie an einem Orte rasten;", "tokens": ["Es", "l\u00e4\u00dft", "dich", "nie", "an", "ei\u00b7nem", "Or\u00b7te", "ras\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn ach, dich lockt ein ferner Zauberschein", "tokens": ["Denn", "ach", ",", "dich", "lockt", "ein", "fer\u00b7ner", "Zau\u00b7ber\u00b7schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ruhlos mu\u00dft du nun die Welt durchhasten.", "tokens": ["Und", "ruh\u00b7los", "mu\u00dft", "du", "nun", "die", "Welt", "durch\u00b7has\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie oft nicht glaubtest du den stillen Strand", "tokens": ["Wie", "oft", "nicht", "glaub\u00b7test", "du", "den", "stil\u00b7len", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PTKNEG", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der selgen Inseln schon ersp\u00e4ht zu haben,", "tokens": ["Der", "sel\u00b7gen", "In\u00b7seln", "schon", "er\u00b7sp\u00e4ht", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch tratest du dann z\u00f6gernd an ihr Land,", "tokens": ["Doch", "tra\u00b7test", "du", "dann", "z\u00f6\u00b7gernd", "an", "ihr", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So war's nur, drin die Hoffnung zu begraben.", "tokens": ["So", "wa\u00b7r's", "nur", ",", "drin", "die", "Hoff\u00b7nung", "zu", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "$,", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Doch die du scheidend schon so oft beweint,", "tokens": ["Doch", "die", "du", "schei\u00b7dend", "schon", "so", "oft", "be\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADJD", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Stets ist sie noch erwacht aus ihrem Tode", "tokens": ["Stets", "ist", "sie", "noch", "er\u00b7wacht", "aus", "ih\u00b7rem", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hat sich schmeichelnd wieder dir vereint", "tokens": ["Und", "hat", "sich", "schmei\u00b7chelnd", "wie\u00b7der", "dir", "ver\u00b7eint"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PRF", "ADJD", "ADV", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und trank mit dir und brach von deinem Brode.", "tokens": ["Und", "trank", "mit", "dir", "und", "brach", "von", "dei\u00b7nem", "Bro\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Du aber f\u00fcgtest dann in Klang und Wort", "tokens": ["Du", "a\u00b7ber", "f\u00fcg\u00b7test", "dann", "in", "Klang", "und", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von neuem deine ewigen Gef\u00fchle", "tokens": ["Von", "neu\u00b7em", "dei\u00b7ne", "e\u00b7wi\u00b7gen", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sp\u00e4htest nach dem hei\u00dfersehnten Port,", "tokens": ["Und", "sp\u00e4h\u00b7test", "nach", "dem", "hei\u00b7\u00dfer\u00b7sehn\u00b7ten", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der dich errettet aus dem Weltgew\u00fchle.", "tokens": ["Der", "dich", "er\u00b7ret\u00b7tet", "aus", "dem", "Welt\u00b7ge\u00b7w\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Denn wen sein wilder Strudel erst erfa\u00dft,", "tokens": ["Denn", "wen", "sein", "wil\u00b7der", "Stru\u00b7del", "erst", "er\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der ist f\u00fcr alle Ewigkeit verloren;", "tokens": ["Der", "ist", "f\u00fcr", "al\u00b7le", "E\u00b7wig\u00b7keit", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein G\u00f6tterbote l\u00e4dt ihn mehr zu Gast", "tokens": ["Kein", "G\u00f6t\u00b7ter\u00b7bo\u00b7te", "l\u00e4dt", "ihn", "mehr", "zu", "Gast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und besser w\u00e4r's, er w\u00e4re nie geboren.", "tokens": ["Und", "bes\u00b7ser", "w\u00e4r's", ",", "er", "w\u00e4\u00b7re", "nie", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Drum harre aus, wie du bisher gethan,", "tokens": ["Drum", "har\u00b7re", "aus", ",", "wie", "du", "bis\u00b7her", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und halte fest an allem, was dir theuer;", "tokens": ["Und", "hal\u00b7te", "fest", "an", "al\u00b7lem", ",", "was", "dir", "theu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PIS", "$,", "PWS", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zwar nur gebrechlich ist dein schwanker Kahn,", "tokens": ["Zwar", "nur", "ge\u00b7brech\u00b7lich", "ist", "dein", "schwan\u00b7ker", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch sitzt die Liebe ja an seinem Steuer.", "tokens": ["Doch", "sitzt", "die", "Lie\u00b7be", "ja", "an", "sei\u00b7nem", "Steu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}