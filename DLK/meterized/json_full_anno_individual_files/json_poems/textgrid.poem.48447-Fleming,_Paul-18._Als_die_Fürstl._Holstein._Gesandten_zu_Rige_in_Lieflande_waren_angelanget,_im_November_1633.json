{"textgrid.poem.48447": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "18. Als die F\u00fcrstl. Holstein. Gesandten zu Rige in Lieflande waren angelanget, im November 1633", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was warf das b\u00f6se Meer f\u00fcr freche Wellen auf,", "tokens": ["Was", "warf", "das", "b\u00f6\u00b7se", "Meer", "f\u00fcr", "fre\u00b7che", "Wel\u00b7len", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "als Wind und Wasser uns best\u00fcrmeten zuhauf'!", "tokens": ["als", "Wind", "und", "Was\u00b7ser", "uns", "be\u00b7st\u00fcr\u00b7me\u00b7ten", "zu\u00b7hauf'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "PWS", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein Schiff war tausent Angst und N\u00f6ten untergeben,", "tokens": ["Ein", "Schiff", "war", "tau\u00b7sent", "Angst", "und", "N\u00f6\u00b7ten", "un\u00b7ter\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "CARD", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "doch f\u00fchrt' es so viel Gl\u00fcck' und Gutes auch beineben.", "tokens": ["doch", "f\u00fchrt'", "es", "so", "viel", "Gl\u00fcck", "und", "Gu\u00b7tes", "auch", "bei\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+---+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Euch sah der gr\u00fcne Greis, der Vater blauer Flut,", "tokens": ["Euch", "sah", "der", "gr\u00fc\u00b7ne", "Greis", ",", "der", "Va\u00b7ter", "blau\u00b7er", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "euch, die das Gl\u00fccke liebt und, was ihr w\u00fcndschet, tut.", "tokens": ["euch", ",", "die", "das", "Gl\u00fc\u00b7cke", "liebt", "und", ",", "was", "ihr", "w\u00fcnd\u00b7schet", ",", "tut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "KON", "$,", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Euch sah er und sprach: Wie? Soll ich hier die nicht sch\u00fctzen,", "tokens": ["Euch", "sah", "er", "und", "sprach", ":", "Wie", "?", "Soll", "ich", "hier", "die", "nicht", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "$.", "PWAV", "$.", "VMFIN", "PPER", "ADV", "ART", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "ihr G\u00f6tter, die auch mir ink\u00fcnftig werden n\u00fctzen?", "tokens": ["ihr", "G\u00f6t\u00b7ter", ",", "die", "auch", "mir", "in\u00b7k\u00fcnf\u00b7tig", "wer\u00b7den", "n\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,