{"textgrid.poem.56071": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die Flamingos", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Spiegelbildern wie von Fragonard", "tokens": ["In", "Spie\u00b7gel\u00b7bil\u00b7dern", "wie", "von", "Fra\u00b7gon\u00b7ard"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KOKOM", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ist doch von ihrem Wei\u00df und ihrer R\u00f6te", "tokens": ["ist", "doch", "von", "ih\u00b7rem", "Wei\u00df", "und", "ih\u00b7rer", "R\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nicht mehr gegeben, als dir einer b\u00f6te,", "tokens": ["nicht", "mehr", "ge\u00b7ge\u00b7ben", ",", "als", "dir", "ei\u00b7ner", "b\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVPP", "$,", "KOUS", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wenn er von seiner Freundin sagt: sie war", "tokens": ["wenn", "er", "von", "sei\u00b7ner", "Freun\u00b7din", "sagt", ":", "sie", "war"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "noch sanft von Schlaf. Denn steigen sie ins Gr\u00fcne", "tokens": ["noch", "sanft", "von", "Schlaf", ".", "Denn", "stei\u00b7gen", "sie", "ins", "Gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "$.", "KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und stehn, auf rosa Stielen leicht gedreht,", "tokens": ["und", "stehn", ",", "auf", "ro\u00b7sa", "Stie\u00b7len", "leicht", "ge\u00b7dreht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "beisammen, bl\u00fchend, wie in einem Beet,", "tokens": ["bei\u00b7sam\u00b7men", ",", "bl\u00fc\u00b7hend", ",", "wie", "in", "ei\u00b7nem", "Beet", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "VVPP", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "verf\u00fchren sie verf\u00fchrender als Phryne", "tokens": ["ver\u00b7f\u00fch\u00b7ren", "sie", "ver\u00b7f\u00fch\u00b7ren\u00b7der", "als", "Phry\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "sich selber; bis sie ihres Auges Bleiche", "tokens": ["sich", "sel\u00b7ber", ";", "bis", "sie", "ih\u00b7res", "Au\u00b7ges", "Blei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "$.", "KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "hinhalsend bergen in der eignen Weiche,", "tokens": ["hin\u00b7hal\u00b7send", "ber\u00b7gen", "in", "der", "eig\u00b7nen", "Wei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in welcher Schwarz und Fruchtrot sich versteckt.", "tokens": ["in", "wel\u00b7cher", "Schwarz", "und", "Fruch\u00b7trot", "sich", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auf einmal kreischt ein Neid durch die Voli\u00e8re;", "tokens": ["Auf", "ein\u00b7mal", "kreischt", "ein", "Neid", "durch", "die", "Vo\u00b7li\u00b7\u00e8\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "sie aber haben sich erstaunt gestreckt", "tokens": ["sie", "a\u00b7ber", "ha\u00b7ben", "sich", "er\u00b7staunt", "ge\u00b7streckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PRF", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und schreiten einzeln ins Imagin\u00e4re.", "tokens": ["und", "schrei\u00b7ten", "ein\u00b7zeln", "ins", "I\u00b7mag\u00b7i\u00b7n\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "In Spiegelbildern wie von Fragonard", "tokens": ["In", "Spie\u00b7gel\u00b7bil\u00b7dern", "wie", "von", "Fra\u00b7gon\u00b7ard"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KOKOM", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ist doch von ihrem Wei\u00df und ihrer R\u00f6te", "tokens": ["ist", "doch", "von", "ih\u00b7rem", "Wei\u00df", "und", "ih\u00b7rer", "R\u00f6\u00b7te"