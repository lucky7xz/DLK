{"dta.poem.14244": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Entfernte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Getrennt ist l\u00e4ngst schon uns'res Lebens Reise,", "tokens": ["Ge\u00b7trennt", "ist", "l\u00e4ngst", "schon", "un\u00b7s'\u00b7res", "Le\u00b7bens", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es trieb mein Herz durch licht' und dunkle Stunden.", "tokens": ["Es", "trieb", "mein", "Herz", "durch", "licht'", "und", "dunk\u00b7le", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem festern Blick erweitern sich die Kreise,", "tokens": ["Dem", "fes\u00b7tern", "Blick", "er\u00b7wei\u00b7tern", "sich", "die", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Duft ist jenes erste Reich verschwunden \u2014", "tokens": ["In", "Duft", "ist", "je\u00b7nes", "ers\u00b7te", "Reich", "ver\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PDAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch, wie die Pfade einsam sich verwildern,", "tokens": ["Doch", ",", "wie", "die", "Pfa\u00b7de", "ein\u00b7sam", "sich", "ver\u00b7wil\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was ich seitdem von Lust und Leid bezwungen,", "tokens": ["Was", "ich", "seit\u00b7dem", "von", "Lust", "und", "Leid", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Geliebt, geirrt, gesungen:", "tokens": ["Ge\u00b7liebt", ",", "ge\u00b7irrt", ",", "ge\u00b7sun\u00b7gen", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich knie' vor Dir in all' den tausend Bildern.", "tokens": ["Ich", "knie'", "vor", "Dir", "in", "all'", "den", "tau\u00b7send", "Bil\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PIS", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}