{"textgrid.poem.37637": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Rundgesang gegen die Unterdr\u00fccker des Werdenden", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, ihr meine deutschen Br\u00fcder,", "tokens": ["Auf", ",", "ihr", "mei\u00b7ne", "deut\u00b7schen", "Br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feiern wollen wir die Nacht,", "tokens": ["Fei\u00b7ern", "wol\u00b7len", "wir", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schallen soll der Trost der Lieder", "tokens": ["Schal\u00b7len", "soll", "der", "Trost", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh' der Morgenstern erwacht,", "tokens": ["Eh'", "der", "Mor\u00b7gens\u00b7tern", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "La\u00dft die Stunden uns befl\u00fcgeln,", "tokens": ["La\u00dft", "die", "Stun\u00b7den", "uns", "be\u00b7fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df wir aus der dunkeln Zeit,", "tokens": ["Da\u00df", "wir", "aus", "der", "dun\u00b7keln", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie die Lerchen von den H\u00fcgeln", "tokens": ["Wie", "die", "Ler\u00b7chen", "von", "den", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fl\u00fcchten in die G\u00f6ttlichkeit.", "tokens": ["Fl\u00fcch\u00b7ten", "in", "die", "G\u00f6tt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alter Glanz ist nun verflogen,", "tokens": ["Al\u00b7ter", "Glanz", "ist", "nun", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gestern ist ein leeres Wort,", "tokens": ["Ge\u00b7stern", "ist", "ein", "lee\u00b7res", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Scham hat unsre Wang' umzogen,", "tokens": ["Scham", "hat", "uns\u00b7re", "Wang'", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch der neue Tag scheint dort.", "tokens": ["Doch", "der", "neu\u00b7e", "Tag", "scheint", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unersch\u00f6pflich ist die Jugend,", "tokens": ["Un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "ist", "die", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jeder Tag ein Sch\u00f6pfungstag,", "tokens": ["Je\u00b7der", "Tag", "ein", "Sch\u00f6p\u00b7fungs\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wer mit froher, reiner Tugend", "tokens": ["Wer", "mit", "fro\u00b7her", ",", "rei\u00b7ner", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00f6rdert was sein Volk vermag.", "tokens": ["F\u00f6r\u00b7dert", "was", "sein", "Volk", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eine Ernte ist getreten", "tokens": ["Ei\u00b7ne", "Ern\u00b7te", "ist", "ge\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem Feinde in den Koth,", "tokens": ["Von", "dem", "Fein\u00b7de", "in", "den", "Koth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh' ihn unsre Schwerter m\u00e4hten,", "tokens": ["Eh'", "ihn", "uns\u00b7re", "Schwer\u00b7ter", "m\u00e4h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch wir wuchsen auch in Noth,", "tokens": ["Doch", "wir", "wuch\u00b7sen", "auch", "in", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine Saat ist aufgestiegen,", "tokens": ["Ei\u00b7ne", "Saat", "ist", "auf\u00b7ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drachenz\u00e4hne setzt die Brut,", "tokens": ["Dra\u00b7chen\u00b7z\u00e4h\u00b7ne", "setzt", "die", "Brut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mag es brechen, will's nicht biegen,", "tokens": ["Mag", "es", "bre\u00b7chen", ",", "will's", "nicht", "bie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jugend hat ein hei\u00dfes Blut.", "tokens": ["Ju\u00b7gend", "hat", "ein", "hei\u00b7\u00dfes", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bei gest\u00fcrzten Edeltannen", "tokens": ["Bei", "ge\u00b7st\u00fcrz\u00b7ten", "E\u00b7del\u00b7tan\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigt die Saat viel freier auf,", "tokens": ["Steigt", "die", "Saat", "viel", "frei\u00b7er", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als wenn seltne Strahlen rannen", "tokens": ["Als", "wenn", "selt\u00b7ne", "Strah\u00b7len", "ran\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Durch der Wipfel S\u00e4ulenknauf;", "tokens": ["Durch", "der", "Wip\u00b7fel", "S\u00e4u\u00b7len\u00b7knauf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ruhmess\u00e4ulen setzen Gr\u00e4nzen", "tokens": ["Ruh\u00b7mes\u00b7s\u00e4u\u00b7len", "set\u00b7zen", "Gr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsrer Jugend frischem Gl\u00fcck,", "tokens": ["Uns\u00b7rer", "Ju\u00b7gend", "fri\u00b7schem", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Frischer Lorbeer soll dich kr\u00e4nzen,", "tokens": ["Fri\u00b7scher", "Lor\u00b7beer", "soll", "dich", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deckt kein alter Kranz den Blick.", "tokens": ["Deckt", "kein", "al\u00b7ter", "Kranz", "den", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hebt die H\