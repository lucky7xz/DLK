ord", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hoch lacht herein der kristallne Dom,", "tokens": ["Hoch", "lacht", "her\u00b7ein", "der", "kris\u00b7tall\u00b7ne", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Darunter f\u00e4chelnde L\u00fcfte.", "tokens": ["Da\u00b7run\u00b7ter", "f\u00e4\u00b7cheln\u00b7de", "L\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Von Blumen umduftet, im w\u00e4rmenden Schein,", "tokens": ["Von", "Blu\u00b7men", "um\u00b7duf\u00b7tet", ",", "im", "w\u00e4r\u00b7men\u00b7den", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf breitem Gipfel steh ich allein;", "tokens": ["Auf", "brei\u00b7tem", "Gip\u00b7fel", "steh", "ich", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich lehne mich l\u00e4chelnd auf meinen Stab,", "tokens": ["Ich", "leh\u00b7ne", "mich", "l\u00e4\u00b7chelnd", "auf", "mei\u00b7nen", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mein Aug streift selig landauf, landab;", "tokens": ["Mein", "Aug", "streift", "se\u00b7lig", "land\u00b7auf", ",", "land\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und all mein Leiden vergessen. \u2013 \u2013 \u2013", "tokens": ["Und", "all", "mein", "Lei\u00b7den", "ver\u00b7ges\u00b7sen", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVPP", "$.", "$(", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Und sei es der sinnber\u00fcckende Tod,", "tokens": ["Und", "sei", "es", "der", "sinn\u00b7be\u00b7r\u00fc\u00b7cken\u00b7de", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich will nicht mehr hungern um leiblich Brot.", "tokens": ["Ich", "will", "nicht", "mehr", "hun\u00b7gern", "um", "leib\u00b7lich", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVFIN", "APPR", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will dich halten, du sonnig Bild,", "tokens": ["Ich", "will", "dich", "hal\u00b7ten", ",", "du", "son\u00b7nig", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Solang nur pochend das Herz noch schwillt \u2013", "tokens": ["So\u00b7lang", "nur", "po\u00b7chend", "das", "Herz", "noch", "schwillt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ich schw\u00f6r es auf Leben und Sterben.", "tokens": ["Ich", "schw\u00f6r", "es", "auf", "Le\u00b7ben", "und", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}