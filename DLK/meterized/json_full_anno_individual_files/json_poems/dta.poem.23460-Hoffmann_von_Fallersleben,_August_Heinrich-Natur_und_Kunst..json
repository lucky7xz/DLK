{"dta.poem.23460": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Natur und Kunst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O gro\u00dfe herrliche Natur!", "tokens": ["O", "gro\u00b7\u00dfe", "herr\u00b7li\u00b7che", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du kommst mit Donner und Blitz und Sturmesgebrause,", "tokens": ["Du", "kommst", "mit", "Don\u00b7ner", "und", "Blitz", "und", "Stur\u00b7mes\u00b7ge\u00b7brau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erf\u00fcllst mit Bangen Wald und Flur,", "tokens": ["Er\u00b7f\u00fcllst", "mit", "Ban\u00b7gen", "Wald", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Schrecken und Angst Palast und Klause.", "tokens": ["Mit", "Schre\u00b7cken", "und", "Angst", "Pa\u00b7last", "und", "Klau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "O gro\u00dfe herrliche Natur!", "tokens": ["O", "gro\u00b7\u00dfe", "herr\u00b7li\u00b7che", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Wort dem\u00fcthigt die Welt und alles Leben:", "tokens": ["Dein", "Wort", "de\u00b7m\u00fct\u00b7higt", "die", "Welt", "und", "al\u00b7les", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es schweiget jede Creatur,", "tokens": ["Es", "schwei\u00b7get", "je\u00b7de", "Crea\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Es staunet Tiger und Leu, und K\u00f6nige beben.", "tokens": ["Es", "stau\u00b7net", "Ti\u00b7ger", "und", "Leu", ",", "und", "K\u00f6\u00b7ni\u00b7ge", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "KON", "NN", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "O gro\u00dfe herrliche Natur!", "tokens": ["O", "gro\u00b7\u00dfe", "herr\u00b7li\u00b7che", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bringst zum Schweigen die Welt mit Donnergetose,", "tokens": ["Du", "bringst", "zum", "Schwei\u00b7gen", "die", "Welt", "mit", "Don\u00b7ner\u00b7ge\u00b7to\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und \u2014 mehr vermag noch die Censur,", "tokens": ["Und", "mehr", "ver\u00b7mag", "noch", "die", "Cen\u00b7sur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die thut's gelassen mit einer Federpose.", "tokens": ["Die", "thut's", "ge\u00b7las\u00b7sen", "mit", "ei\u00b7ner", "Fe\u00b7der\u00b7po\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}