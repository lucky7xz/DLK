{"textgrid.poem.65785": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "59", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn neu nichts ist, wenn nur Vergangnes bringend", "tokens": ["Wenn", "neu", "nichts", "ist", ",", "wenn", "nur", "Ver\u00b7gang\u00b7nes", "brin\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PIS", "VAFIN", "$,", "KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gegenwart sich um uns her bewegt;", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", "sich", "um", "uns", "her", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PRF", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie schwindelt unser Hirn, das, nach Erfindung ringend,", "tokens": ["Wie", "schwin\u00b7delt", "un\u00b7ser", "Hirn", ",", "das", ",", "nach", "Er\u00b7fin\u00b7dung", "rin\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "PDS", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Uralter Kinder B\u00fcrden wieder tr\u00e4gt!", "tokens": ["Ur\u00b7al\u00b7ter", "Kin\u00b7der", "B\u00fcr\u00b7den", "wie\u00b7der", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O k\u00f6nnte nur Geschicht' auf einen Flug", "tokens": ["O", "k\u00f6nn\u00b7te", "nur", "Ge\u00b7schicht'", "auf", "ei\u00b7nen", "