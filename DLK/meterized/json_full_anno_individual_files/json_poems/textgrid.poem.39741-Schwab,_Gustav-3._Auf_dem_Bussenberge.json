{"textgrid.poem.39741": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "3. Auf dem Bussenberge", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weithin, weithin wollt' ich streifen", "tokens": ["Weit\u00b7hin", ",", "weit\u00b7hin", "wollt'", "ich", "strei\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf des freien H\u00fcgels Rand,", "tokens": ["Auf", "des", "frei\u00b7en", "H\u00fc\u00b7gels", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der den Blick l\u00e4\u00dft ferne schweifen", "tokens": ["Der", "den", "Blick", "l\u00e4\u00dft", "fer\u00b7ne", "schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Schneegebirge Land.", "tokens": ["In", "der", "Schnee\u00b7ge\u00b7bir\u00b7ge", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort im Gr\u00fcnen und im Blauen,", "tokens": ["Dort", "im", "Gr\u00fc\u00b7nen", "und", "im", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem alten Mauerstein", "tokens": ["Auf", "dem", "al\u00b7ten", "Mau\u00b7er\u00b7stein"], "token_info": ["word", "word", "word", "word"