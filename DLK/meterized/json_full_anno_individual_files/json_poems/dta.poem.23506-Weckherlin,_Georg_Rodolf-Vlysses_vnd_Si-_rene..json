{"dta.poem.23506": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Vlysses vnd Si-  \n rene.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So ist der wollust nu der lohn", "tokens": ["So", "ist", "der", "wol\u00b7lust", "nu", "der", "lohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd port/ der Euch so lang verieret;", "tokens": ["Vnd", "port", "/", "der", "Euch", "so", "lang", "ve\u00b7rie\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$(", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Jhr \u00f6ft/ weil Jhr lang davon", "tokens": ["Den", "Ihr", "\u00f6ft", "/", "weil", "Ihr", "lang", "da\u00b7von"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$(", "KOUS", "PPER", "ADJD", "PAV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Jhn zu vermehren/ gar verlieret:", "tokens": ["Jhn", "zu", "ver\u00b7meh\u00b7ren", "/", "gar", "ver\u00b7lie\u00b7ret", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$(", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit wollust du abwech\u00dflen must/", "tokens": ["Mit", "wol\u00b7lust", "du", "ab\u00b7wech\u00df\u00b7len", "must", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VMFIN", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich allzeit fr\u00f6lich zuerlaben:", "tokens": ["Dich", "all\u00b7zeit", "fr\u00f6\u00b7lich", "zu\u00b7er\u00b7la\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Man kan ja so mancherlay lust/", "tokens": ["Man", "kan", "ja", "so", "man\u00b7cher\u00b7lay", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "PIAT", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Als vilerlay gesch\u00e4ften haben.", "tokens": ["Als", "vi\u00b7ler\u00b7lay", "ge\u00b7sch\u00e4f\u00b7ten", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}