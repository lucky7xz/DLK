{"dta.poem.2942": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "141.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo in Behaglichkeit sich darf die Seele wiegen,", "tokens": ["Wo", "in", "Be\u00b7hag\u00b7lich\u00b7keit", "sich", "darf", "die", "See\u00b7le", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PRF", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verliert der Geist den Trieb zur Heimat aufzufliegen.", "tokens": ["Ver\u00b7liert", "der", "Geist", "den", "Trieb", "zur", "Hei\u00b7mat", "auf\u00b7zu\u00b7flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was dich zum Himmel spornt, dar\u00fcber willst du klagen?", "tokens": ["Was", "dich", "zum", "Him\u00b7mel", "spornt", ",", "da\u00b7r\u00fc\u00b7ber", "willst", "du", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "PAV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimm an mit Dank auch gottgesandtes Unbehagen!", "tokens": ["Nimm", "an", "mit", "Dank", "auch", "gott\u00b7ge\u00b7sand\u00b7tes", "Un\u00b7be\u00b7ha\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "meas