{"textgrid.poem.55268": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Umgekehrt", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind die im Ungl\u00fcck, die wir lieben,", "tokens": ["Sind", "die", "im", "Un\u00b7gl\u00fcck", ",", "die", "wir", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das wird uns wahrlich ba\u00df betr\u00fcben;", "tokens": ["Das", "wird", "uns", "wahr\u00b7lich", "ba\u00df", "be\u00b7tr\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sind aber gl\u00fccklich, die wir hassen,", "tokens": ["Sind", "a\u00b7ber", "gl\u00fcck\u00b7lich", ",", "die", "wir", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das will sich gar nicht begreifen lassen;", "tokens": ["Das", "will", "sich", "gar", "nicht", "be\u00b7grei\u00b7fen", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "ADV", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Umgekehrt ist's ein Jubilo,", "tokens": ["Um\u00b7ge\u00b7kehrt", "ist's", "ein", "Ju\u00b7bi\u00b7lo", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,"], "meter