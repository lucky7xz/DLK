{"dta.poem.14460": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An den heiligen Joseph.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn tr\u00fcbe Schleier alles grau umweben,", "tokens": ["Wenn", "tr\u00fc\u00b7be", "Schlei\u00b7er", "al\u00b7les", "grau", "um\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur bleichen Ferne wird das ganze Leben,", "tokens": ["Zur", "blei\u00b7chen", "Fer\u00b7ne", "wird", "das", "gan\u00b7ze", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will Heimath oft sich tr\u00f6stend zeigen;", "tokens": ["Will", "Hei\u00b7math", "oft", "sich", "tr\u00f6s\u00b7tend", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Morgenroth die gold'nen H\u00f6hen steigen,", "tokens": ["Aus", "Mor\u00b7gen\u00b7roth", "die", "gold'\u00b7nen", "H\u00f6\u00b7hen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und aus dem stillen, wundervollen Duft", "tokens": ["Und", "aus", "dem", "stil\u00b7len", ",", "wun\u00b7der\u00b7vol\u00b7len", "Duft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Eine wohlbekannte Stimm' hin\u00fcberruft.", "tokens": ["Ei\u00b7ne", "wohl\u00b7be\u00b7kann\u00b7te", "Stimm'", "hin\u00b7\u00fc\u00b7ber\u00b7ruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}