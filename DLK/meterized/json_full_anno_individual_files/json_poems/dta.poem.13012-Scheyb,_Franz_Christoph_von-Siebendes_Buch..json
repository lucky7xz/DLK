{"dta.poem.13012": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Tugend sezte sich, so fieng ein Murmeln an,", "tokens": ["Die", "Tu\u00b7gend", "sez\u00b7te", "sich", ",", "so", "fi\u00b7eng", "ein", "Mur\u00b7meln", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Weil von dem Kreise sich noch niemand vorgethan.", "tokens": ["Weil", "von", "dem", "Krei\u00b7se", "sich", "noch", "nie\u00b7mand", "vor\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PRF", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich konnte nichts verstehn; es lie\u00df, als ob man", "tokens": ["Ich", "konn\u00b7te", "nichts", "ver\u00b7stehn", ";", "es", "lie\u00df", ",", "als", "ob", "man"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$.", "PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "streite,", "tokens": ["strei\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Und etwan zum Entschlu\u00df die Meinungen bereite.", "tokens": ["Und", "et\u00b7wan", "zum", "Ent\u00b7schlu\u00df", "die", "Mei\u00b7nun\u00b7gen", "be\u00b7rei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--++--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Das Summen w\u00e4hrte so, wie wann von ungef\u00e4hr", "tokens": ["Das", "Sum\u00b7men", "w\u00e4hr\u00b7te", "so", ",", "wie", "wann", "von", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWAV", "PWAV", "APPR", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein frischer Morgen-Wind das Spiegel-glatte Meer", "tokens": ["Ein", "fri\u00b7scher", "Mor\u00b7gen\u00b7Wind", "das", "Spie\u00b7gel\u00b7glat\u00b7te", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In die Bewegung treibt, da\u00df die gerollte Wellen", "tokens": ["In", "die", "Be\u00b7we\u00b7gung", "treibt", ",", "da\u00df", "die", "ge\u00b7roll\u00b7te", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sich an den steilen Strand, an Stein und Klippen prellen;", "tokens": ["Sich", "an", "den", "stei\u00b7len", "Strand", ",", "an", "Stein", "und", "Klip\u00b7pen", "prel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der ganze Saal ward reg: hier sp\u00fchrte man ein Wort;", "tokens": ["Der", "gan\u00b7ze", "Saal", "ward", "reg", ":", "hier", "sp\u00fchr\u00b7te", "man", "ein", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$.", "ADV", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "10Da lispelt' Eine die\u00df, was anders Jene dort.", "tokens": ["lis\u00b7pelt'", "Ei\u00b7ne", "die\u00df", ",", "was", "an\u00b7ders", "Je\u00b7ne", "dort", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PDS", "$,", "PRELS", "ADV", "PDAT", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Es redten zwey und zwey, auch drey und mehr zusammen,", "tokens": ["Es", "red\u00b7ten", "zwey", "und", "zwey", ",", "auch", "drey", "und", "mehr", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "KON", "CARD", "$,", "ADV", "CARD", "KON", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nachdem Begier und Geist und Eifer Plaz bekamen.", "tokens": ["Nach\u00b7dem", "Be\u00b7gier", "und", "Geist", "und", "Ei\u00b7fer", "Plaz", "be\u00b7ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}