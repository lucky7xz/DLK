ord", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So h\u00f6re rasseln des Silbers Klang,", "tokens": ["So", "h\u00f6\u00b7re", "ras\u00b7seln", "des", "Sil\u00b7bers", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deinen Ohren magst du trauen!\u00ab", "tokens": ["Dei\u00b7nen", "Oh\u00b7ren", "magst", "du", "trau\u00b7en", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "Er schwingt den schweren Beutel hoch,", "tokens": ["Er", "schwingt", "den", "schwe\u00b7ren", "Beu\u00b7tel", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht harrend unter der Schwelle;", "tokens": ["Steht", "har\u00b7rend", "un\u00b7ter", "der", "Schwel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch aus den blinden Augen springt", "tokens": ["Doch", "aus", "den", "blin\u00b7den", "Au\u00b7gen", "springt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Greis die zornige Quelle.", "tokens": ["Dem", "Greis", "die", "zor\u00b7ni\u00b7ge", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.27": {"line.1": {"text": "\u00bbweh mir, da\u00df ich nicht wandeln kann,", "tokens": ["\u00bb", "weh", "mir", ",", "da\u00df", "ich", "nicht", "wan\u00b7deln", "kann", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl mir, da\u00df ich nicht kann sehen!", "tokens": ["Wohl", "mir", ",", "da\u00df", "ich", "nicht", "kann", "se\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es darf in meiner Halle Thor", "tokens": ["Es", "darf", "in", "mei\u00b7ner", "Hal\u00b7le", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Sohnes M\u00f6rder mir stehen.", "tokens": ["Des", "Soh\u00b7nes", "M\u00f6r\u00b7der", "mir", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.28": {"line.1": {"text": "Er labt den Blick an meiner Faust,", "tokens": ["Er", "labt", "den", "Blick", "an", "mei\u00b7ner", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die nicht mehr wei\u00df zu schlagen;", "tokens": ["Die", "nicht", "mehr", "wei\u00df", "zu", "schla\u00b7gen", ";"], "token_info": ["w