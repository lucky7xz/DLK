-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du, wandle nur aus meinem Haus,", "tokens": ["Du", ",", "wand\u00b7le", "nur", "aus", "mei\u00b7nem", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist \u00fcberall verloren!\u00ab", "tokens": ["Bist", "\u00fc\u00b7be\u00b7rall", "ver\u00b7lo\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "So sitzt der blinde Greis im Stuhl,", "tokens": ["So", "sitzt", "der", "blin\u00b7de", "Greis", "im", "Stuhl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "R\u00fchrt keines seiner Glieder,", "tokens": ["R\u00fchrt", "kei\u00b7nes", "sei\u00b7ner", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schl\u00e4gt mit seiner Stimme Schall", "tokens": ["Und", "schl\u00e4gt", "mit", "sei\u00b7ner", "Stim\u00b7me", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den M\u00f6rder doch darnieder.", "tokens": ["Den", "M\u00f6r\u00b7der", "doch", "dar\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}