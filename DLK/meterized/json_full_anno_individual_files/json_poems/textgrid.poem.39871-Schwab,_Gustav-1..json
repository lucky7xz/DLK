{"textgrid.poem.39871": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Thorstein in der Halle sitzt,", "tokens": ["Herr", "Thor\u00b7stein", "in", "der", "Hal\u00b7le", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Der blinde Greis in Schmerzen,", "tokens": ["Der", "blin\u00b7de", "Greis", "in", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Enkel liegt in seinem Arm", "tokens": ["Ein", "En\u00b7kel", "liegt", "in", "sei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und weinet ihm am Herzen.", "tokens": ["Und", "wei\u00b7net", "ihm", "am", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo ist dein Vater, kleines Kind? \u2013", "tokens": ["Wo", "ist", "dein", "Va\u00b7ter", ",", "klei\u00b7nes", "Kind", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Feind hat ihn erschlagen.", "tokens": ["Sein", "Feind", "hat", "ihn", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So tr\u00f6ste dich die Mutter dein! \u2013", "tokens": ["So", "tr\u00f6s\u00b7te", "dich", "die", "Mut\u00b7ter", "dein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tot ist sie von dem Klagen.", "tokens": ["Tot", "ist", "sie", "von", "dem", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So h\u00fcte doch Allvater dich,", "tokens": ["So", "h\u00fc\u00b7te", "doch", "All\u00b7va\u00b7ter", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lasse dich in Frieden schlafen,", "tokens": ["Las\u00b7se", "dich", "in", "Frie\u00b7den", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wachsen hoch und werden stark,", "tokens": ["Und", "wach\u00b7sen", "hoch", "und", "wer\u00b7den", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis du den Feind kannst strafen!", "tokens": ["Bis", "du", "den", "Feind", "kannst", "stra\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In der Halle sitzt der blinde Greis,", "tokens": ["In", "der", "Hal\u00b7le", "sitzt", "der", "blin\u00b7de", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Er segnet seinen Enkel:", "tokens": ["Er", "seg\u00b7net", "sei\u00b7nen", "En\u00b7kel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein Aug' ist dunkel, mein Arm ist schwach,", "tokens": ["\u00bb", "mein", "Aug'", "ist", "dun\u00b7kel", ",", "mein", "Arm", "ist", "schwach", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es beben meine Schenkel.", "tokens": ["Es", "be\u00b7ben", "mei\u00b7ne", "Schen\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O s\u00e4nke nicht die welke Hand,", "tokens": ["O", "s\u00e4n\u00b7ke", "nicht", "die", "wel\u00b7ke", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So oft ich sie will heben!", "tokens": ["So", "oft", "ich", "sie", "will", "he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was kann ich so in halbem Tod,", "tokens": ["Was", "kann", "ich", "so", "in", "hal\u00b7bem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und du mit halbem Leben?\u00ab", "tokens": ["Und", "du", "mit", "hal\u00b7bem", "Le\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So sitzt der blinde Greis und klagt;", "tokens": ["So", "sitzt", "der", "blin\u00b7de", "Greis", "und", "klagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da pocht es an die Pforte,", "tokens": ["Da", "pocht", "es", "an", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00f6ffnet leis und ruft herein", "tokens": ["Und", "\u00f6ff\u00b7net", "leis", "und", "ruft", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Schwelle die fl\u00fccht'gen Worte:", "tokens": ["Zur", "Schwel\u00b7le", "die", "fl\u00fccht'\u00b7gen", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbdie Braut sie mir raubten, es war dein Sohn", "tokens": ["\u00bb", "die", "Braut", "sie", "mir", "raub\u00b7ten", ",", "es", "war", "dein", "Sohn"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "PPER", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dabei, den hab' ich erschlagen;", "tokens": ["Da\u00b7bei", ",", "den", "hab'", "ich", "er\u00b7schla\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ART", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und willst du ihn r\u00e4chen, es werden dich", "tokens": ["Und", "willst", "du", "ihn", "r\u00e4\u00b7chen", ",", "es", "wer\u00b7den", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVINF", "$,", "PPER", "VAFIN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die alten F\u00fc\u00dfe nicht tragen.", "tokens": ["Die", "al\u00b7ten", "F\u00fc\u00b7\u00dfe", "nicht", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Schnell ist mein Tritt, irr ist mein Gang,", "tokens": ["Schnell", "ist", "mein", "Tritt", ",", "irr", "ist", "mein", "Gang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Wolf gleich in der W\u00fcsten,", "tokens": ["Dem", "Wolf", "gleich", "in", "der", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es soll nach meinem roten Blut", "tokens": ["Es", "soll", "nach", "mei\u00b7nem", "ro\u00b7ten", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergebens euch gel\u00fcsten.", "tokens": ["Ver\u00b7ge\u00b7bens", "euch", "ge\u00b7l\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch Bu\u00dfe biet' ich dir genug:", "tokens": ["Doch", "Bu\u00b7\u00dfe", "biet'", "ich", "dir", "ge\u00b7nug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du kannst den Beutel nicht schauen,", "tokens": ["Du", "kannst", "den", "Beu\u00b7tel", "nicht", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So h\u00f6re rasseln des Silbers Klang,", "tokens": ["So", "h\u00f6\u00b7re", "ras\u00b7seln", "des", "Sil\u00b7bers", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Deinen Ohren magst du trauen!\u00ab", "tokens": ["Dei\u00b7nen", "Oh\u00b7ren", "magst", "du", "trau\u00b7en", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Er schwingt den schweren Beutel hoch,", "tokens": ["Er", "schwingt", "den", "schwe\u00b7ren", "Beu\u00b7tel", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht harrend unter der Schwelle;", "tokens": ["Steht", "har\u00b7rend", "un\u00b7ter", "der", "Schwel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch aus den blinden Augen springt", "tokens": ["Doch", "aus", "den", "blin\u00b7den", "Au\u00b7gen", "springt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Greis die zornige Quelle.", "tokens": ["Dem", "Greis", "die", "zor\u00b7ni\u00b7ge", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbweh mir, da\u00df ich nicht wandeln kann,", "tokens": ["\u00bb", "weh", "mir", ",", "da\u00df", "ich", "nicht", "wan\u00b7deln", "kann", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl mir, da\u00df ich nicht kann sehen!", "tokens": ["Wohl", "mir", ",", "da\u00df", "ich", "nicht", "kann", "se\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Es darf in meiner Halle Thor", "tokens": ["Es", "darf", "in", "mei\u00b7ner", "Hal\u00b7le", "Thor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Sohnes M\u00f6rder mir stehen.", "tokens": ["Des", "Soh\u00b7nes", "M\u00f6r\u00b7der", "mir", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Er labt den Blick an meiner Faust,", "tokens": ["Er", "labt", "den", "Blick", "an", "mei\u00b7ner", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die nicht mehr wei\u00df zu schlagen;", "tokens": ["Die", "nicht", "mehr", "wei\u00df", "zu", "schla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er meint, da\u00df ich das liebste Kind", "tokens": ["Er", "meint", ",", "da\u00df", "ich", "das", "liebs\u00b7te", "Kind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Beutel m\u00fcsse tragen.", "tokens": ["Im", "Beu\u00b7tel", "m\u00fcs\u00b7se", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Aus dem Herzen, wo den Sohn ich trag',", "tokens": ["Aus", "dem", "Her\u00b7zen", ",", "wo", "den", "Sohn", "ich", "trag'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Aus dem Herzen hol' ich die Waffen;", "tokens": ["Aus", "dem", "Her\u00b7zen", "hol'", "ich", "die", "Waf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Fl\u00fcche schick' ich nach dir aus,", "tokens": ["Die", "Fl\u00fc\u00b7che", "schick'", "ich", "nach", "dir", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sollen mir Rache schaffen.", "tokens": ["Die", "sol\u00b7len", "mir", "Ra\u00b7che", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Den Fluch all' deinem Tritt und Schritt", "tokens": ["Den", "Fluch", "all'", "dei\u00b7nem", "Tritt", "und", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und deinem schn\u00f6den Gelde,", "tokens": ["Und", "dei\u00b7nem", "schn\u00f6\u00b7den", "Gel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' ihn l\u00e4ngst hinaus gesandt,", "tokens": ["Ich", "hab'", "ihn", "l\u00e4ngst", "hin\u00b7aus", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er harret dein im Felde.", "tokens": ["Er", "har\u00b7ret", "dein", "im", "Fel\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Er gehet um in meinem Stamm,", "tokens": ["Er", "ge\u00b7het", "um", "in", "mei\u00b7nem", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er schreit in Aller Ohren;", "tokens": ["Er", "schreit", "in", "Al\u00b7ler", "Oh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du, wandle nur aus meinem Haus,", "tokens": ["Du", ",", "wand\u00b7le", "nur", "aus", "mei\u00b7nem", "Haus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist \u00fcberall verloren!\u00ab", "tokens": ["Bist", "\u00fc\u00b7be\u00b7rall", "ver\u00b7lo\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "So sitzt der blinde Greis im Stuhl,", "tokens": ["So", "sitzt", "der", "blin\u00b7de", "Greis", "im", "Stuhl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "R\u00fchrt keines seiner Glieder,", "tokens": ["R\u00fchrt", "kei\u00b7nes", "sei\u00b7ner", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schl\u00e4gt mit seiner Stimme Schall", "tokens": ["Und", "schl\u00e4gt", "mit", "sei\u00b7ner", "Stim\u00b7me", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den M\u00f6rder doch darnieder.", "tokens": ["Den", "M\u00f6r\u00b7der", "doch", "dar\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Herr Thorstein in der Halle sitzt,", "tokens": ["Herr", "Thor\u00b7stein", "in", "der", "Hal\u00b7le", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Der blinde Greis in Schmerzen,", "tokens": ["Der", "blin\u00b7de", "Greis", "in", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Enkel liegt in seinem Arm", "tokens": ["Ein", "En\u00b7kel", "liegt", "in", "sei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und weinet ihm am Herzen.", "tokens": ["Und", "wei\u00b7net", "ihm", "am", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Wo ist dein Vater, kleines Kind? \u2013", "tokens": ["Wo", "ist", "dein", "Va\u00b7ter", ",", "klei\u00b7nes", "Kind", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Feind hat ihn erschlagen.", "tokens": ["Sein", "Feind", "hat", "ihn", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So tr\u00f6ste dich die Mutter dein! \u2013", "tokens": ["So", "tr\u00f6s\u00b7te", "dich", "die", "Mut\u00b7ter", "dein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tot ist sie von dem Klagen.", "tokens": ["Tot", "ist", "sie", "von", "dem", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "So h\u00fcte doch Allvater dich,", "tokens": ["So", "h\u00fc\u00b7te", "doch", "All\u00b7va\u00b7ter", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lasse dich in Frieden schlafen,", "tokens": ["Las\u00b7se", "dich", "in", "Frie\u00b7den", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wachsen hoch und werden stark,", "tokens": ["Und", "wach\u00b7sen", "hoch", "und", "wer\u00b7den", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis du den Feind kannst strafen!", "tokens": ["Bis", "du", "den", "Feind", "kannst", "stra\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "In der Halle sitzt der blinde Greis,", "tokens": ["In", "der", "Hal\u00b7le", "sitzt", "der", "blin\u00b7de", "Greis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Er segnet seinen Enkel:", "tokens": ["Er", "seg\u00b7net", "sei\u00b7nen", "En\u00b7kel", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmein Aug' ist dunkel, mein Arm ist schwach,", "tokens": ["\u00bb", "mein", "Aug'", "ist", "dun\u00b7kel", ",", "mein", "Arm", "ist", "schwach", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es beben meine Schenkel.", "tokens": ["Es", "be\u00b7ben", "mei\u00b7ne", "Schen\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "O s\u00e4nke nicht die welke Hand,", "tokens": ["O", "s\u00e4n\u00b7ke", "nicht", "die", "wel\u00b7ke", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So oft ich sie will heben!", "tokens": ["So", "oft", "ich", "sie", "will", "he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was kann ich so in halbem Tod,", "tokens": ["Was", "kann", "ich", "so", "in", "hal\u00b7bem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und du mit halbem Leben?\u00ab", "tokens": ["Und", "du", "mit", "hal\u00b7bem", "Le\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "So sitzt der blinde Greis und klagt;", "tokens": ["So", "sitzt", "der", "blin\u00b7de", "Greis", "und", "klagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da pocht es an die Pforte,", "tokens": ["Da", "pocht", "es", "an", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00f6ffnet leis und ruft herein", "tokens": ["Und", "\u00f6ff\u00b7net", "leis", "und", "ruft", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Schwelle die fl\u00fccht'gen Worte:", "tokens": ["Zur", "Schwel\u00b7le", "die", "fl\u00fccht'\u00b7gen", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.23": {"line.1": {"text": "\u00bbdie Braut sie mir raubten, es war dein Sohn", "tokens": ["\u00bb", "die", "Braut", "sie", "mir", "raub\u00b7ten", ",", "es", "war", "dein", "Sohn"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "PPER", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Dabei, den hab' ich erschlagen;", "tokens": ["Da\u00b7bei", ",", "den", "hab'", "ich", "er\u00b7schla\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ART", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und willst du ihn r\u00e4chen, es werden dich", "tokens": ["Und", "willst", "du", "ihn", "r\u00e4\u00b7chen", ",", "es", "wer\u00b7den", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVINF", "$,", "PPER", "VAFIN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die alten F\u00fc\u00dfe nicht tragen.", "tokens": ["Die", "al\u00b7ten", "F\