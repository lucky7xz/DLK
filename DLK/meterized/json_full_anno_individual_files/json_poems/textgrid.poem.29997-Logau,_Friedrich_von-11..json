{"textgrid.poem.29997": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Witwen k\u00fcnnen noch wol dulden, wann die M\u00e4nner gehn zun Todten;", "tokens": ["Wit\u00b7wen", "k\u00fcn\u00b7nen", "noch", "wol", "dul\u00b7den", ",", "wann", "die", "M\u00e4n\u00b7ner", "gehn", "zun", "Tod\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dann die Licke zu erv\u00f6llen, hat Gott nirgend wo verboten;", "tokens": ["Dann", "die", "Li\u00b7cke", "zu", "er\u00b7v\u00f6l\u00b7len", ",", "hat", "Gott", "nir\u00b7gend", "wo", "ver\u00b7bo\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$,", "VAFIN", "NN", "ADV", "PWAV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Drum de\u00df Todes bittres nemen kan durch s\u00fcsses wieder nemen", "tokens": ["Drum", "de\u00df", "To\u00b7des", "bitt\u00b7res", "ne\u00b7men", "kan", "durch", "s\u00fcs\u00b7ses", "wie\u00b7der", "ne\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADJA", "VVINF", "VMFIN", "APPR", "ADJA", "ADV", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Eine Witfrau ihr bes\u00fcssen und den Tod also besch\u00e4men.", "tokens": ["Ei\u00b7ne", "Wit\u00b7frau", "ihr", "be\u00b7s\u00fcs\u00b7sen", "und", "den", "Tod", "al\u00b7so", "be\u00b7sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "KON", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Ihr, Frau Braut, habt auch genummen; gebe Gott, was ihr genummen,", "tokens": ["Ihr", ",", "Frau", "Braut", ",", "habt", "auch", "ge\u00b7num\u00b7men", ";", "ge\u00b7be", "Gott", ",", "was", "ihr", "ge\u00b7num\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "NN", "$,", "VAFIN", "ADV", "VVPP", "$.", "VVFIN", "NN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Da\u00df damit das Gl\u00fccke selbsten euch sey in die Arme kummen!", "tokens": ["Da\u00df", "da\u00b7mit", "das", "Gl\u00fc\u00b7cke", "selbs\u00b7ten", "euch", "sey", "in", "die", "Ar\u00b7me", "kum\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ART", "NN", "VVFIN", "PPER", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "--+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Witwen k\u00fcnnen noch wol dulden, wann die M\u00e4nner gehn zun Todten;", "tokens": ["Wit\u00b7wen", "k\u00fcn\u00b7nen", "noch", "wol", "dul\u00b7den", ",", "wann", "die", "M\u00e4n\u00b7ner", "gehn", "zun", "Tod\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Dann die Licke zu erv\u00f6l