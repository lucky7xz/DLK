{"textgrid.poem.57385": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Endlich darf sie mir einmal doch wohl die z\u00fcrnende Thr\u00e4ne", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich darf sie mir einmal doch wohl die z\u00fcrnende Thr\u00e4ne", "tokens": ["End\u00b7lich", "darf", "sie", "mir", "ein\u00b7mal", "doch", "wohl", "die", "z\u00fcr\u00b7nen\u00b7de", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+---+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Rinnen, endlich mein Schmerz sagen, wie bitter er ist.", "tokens": ["Rin\u00b7nen", ",", "end\u00b7lich", "mein", "Schmerz", "sa\u00b7gen", ",", "wie", "bit\u00b7ter", "er", "ist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "B\u00fcrdet mir Stolz nicht auf, wenn ich von Entweihungen rede;", "tokens": ["B\u00fcr\u00b7det", "mir", "Stolz", "nicht", "auf", ",", "wenn", "ich", "von", "Ent\u00b7wei\u00b7hun\u00b7gen", "re\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKNEG", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Wer so lange wie ich duldet', und schwieg, ist nicht stolz.", "tokens": ["Wer", "so", "lan\u00b7ge", "wie", "ich", "dul\u00b7det'", ",", "und", "schwieg", ",", "ist", "nicht", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "KOKOM", "PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Vor Dolmetschungen ach bewahret mich, G\u00f6ttinnen, hab' ich", "tokens": ["Vor", "Dol\u00b7met\u00b7schun\u00b7gen", "ach", "be\u00b7wah\u00b7ret", "mich", ",", "G\u00f6t\u00b7tin\u00b7nen", ",", "hab'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPER", "$,", "NN", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+--++-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Allen Musen gelleht; aber sie h\u00f6rten mich nicht.", "tokens": ["Al\u00b7len", "Mu\u00b7sen", "gel\u00b7leht", ";", "a\u00b7ber", "sie", "h\u00f6r\u00b7ten", "mich", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$.", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Auch dem dritten Ohr des lazed\u00e4monischen Ph\u00f6bus", "tokens": ["Auch", "dem", "drit\u00b7ten", "Ohr", "des", "la\u00b7ze\u00b7d\u00e4\u00b7mo\u00b7ni\u00b7schen", "Ph\u00f6\u00b7bus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Fleht' ich umsonst, und ach selber dem vierten umsonst!", "tokens": ["Fleht'", "ich", "um\u00b7sonst", ",", "und", "ach", "sel\u00b7ber", "dem", "vier\u00b7ten", "um\u00b7sonst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "ADV", "ADV", "ART", "ADJA", "ADV", "$."], "meter": "-+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Hattest, Apollo der Kriegerstadt, du allein denn nicht Pfeile,", "tokens": ["Hat\u00b7test", ",", "A\u00b7pol\u00b7lo", "der", "Krie\u00b7ger\u00b7stadt", ",", "du", "al\u00b7lein", "denn", "nicht", "Pfei\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NE", "ART", "NN", "$,", "PPER", "ADV", "ADV", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-+--++-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.10": {"text": "Dass du, mich rettend, damit tr\u00e4fst die transl\u00e4tinge Faust?", "tokens": ["Dass", "du", ",", "mich", "ret\u00b7tend", ",", "da\u00b7mit", "tr\u00e4fst", "die", "trans\u00b7l\u00e4\u00b7tin\u00b7ge", "Faust", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "VVPP", "$,", "PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Gallier haben noch j\u00fcngst mich \u00fcbersetzt: doch sie w\u00e4hnens", "tokens": ["Gal\u00b7lier", "ha\u00b7ben", "noch", "j\u00fcngst", "mich", "\u00fc\u00b7bers\u00b7etzt", ":", "doch", "sie", "w\u00e4h\u00b7nens"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "ADV", "PPER", "VVPP", "$.", "KON", "PPER", "ADV"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Nur; sie haben mich dort \u00fcber den Lethe gebetet.", "tokens": ["Nur", ";", "sie", "ha\u00b7ben", "mich", "dort", "\u00fc\u00b7ber", "den", "Le\u00b7the", "ge\u00b7be\u00b7tet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.13": {"text": "O wie grub mir der Wunden so viel ihr triefender Dolch ein,", "tokens": ["O", "wie", "grub", "mir", "der", "Wun\u00b7den", "so", "viel", "ihr", "trie\u00b7fen\u00b7der", "Dolch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Und wie r\u00f6thete sich mir die getroffene Brust!", "tokens": ["Und", "wie", "r\u00f6\u00b7the\u00b7te", "sich", "mir", "die", "ge\u00b7trof\u00b7fe\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PRF", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Und so klage denn ich, das niemals klagte? Wohlan denn,", "tokens": ["Und", "so", "kla\u00b7ge", "denn", "ich", ",", "das", "nie\u00b7mals", "klag\u00b7te", "?", "Wo\u00b7hlan", "denn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "PPER", "$,", "PRELS", "ADV", "VVFIN", "$.", "ADV", "ADV", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Ich ermanne mich jetzt, trage mein Schicksal, wie sonst.", "tokens": ["Ich", "er\u00b7man\u00b7ne", "mich", "jetzt", ",", "tra\u00b7ge", "mein", "Schick\u00b7sal", ",", "wie", "sonst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "ADV", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "