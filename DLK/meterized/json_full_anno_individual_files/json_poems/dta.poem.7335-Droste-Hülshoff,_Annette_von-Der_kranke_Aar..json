{"dta.poem.7335": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der kranke Aar.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Am d\u00fcrren Baum, im fetten Wiesengras", "tokens": ["Am", "d\u00fcr\u00b7ren", "Baum", ",", "im", "fet\u00b7ten", "Wie\u00b7sen\u00b7gras"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Stier behaglich wiederk\u00e4ut' den Fra\u00df;", "tokens": ["Ein", "Stier", "be\u00b7hag\u00b7lich", "wie\u00b7der\u00b7k\u00e4ut'", "den", "Fra\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf niederm Ast ein wunder Adler sa\u00df,", "tokens": ["Auf", "nie\u00b7derm", "Ast", "ein", "wun\u00b7der", "Ad\u00b7ler", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein kranker Aar mit gebrochnen Schwingen.", "tokens": ["Ein", "kran\u00b7ker", "Aar", "mit", "ge\u00b7broch\u00b7nen", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201esteig' auf, mein Vogel, in die blaue Luft,", "tokens": ["\u201e", "steig'", "auf", ",", "mein", "Vo\u00b7gel", ",", "in", "die", "blau\u00b7e", "Luft", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich schau dir nach aus meinem Kr\u00e4uterduft.\u201c \u2014", "tokens": ["Ich", "schau", "dir", "nach", "aus", "mei\u00b7nem", "Kr\u00e4u\u00b7ter\u00b7duft", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201eweh, weh, umsonst die Sonne ruft", "tokens": ["\u201e", "weh", ",", "weh", ",", "um\u00b7sonst", "die", "Son\u00b7ne", "ruft"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "PTKVZ", "$,", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den kranken Aar mit gebrochnen Schwingen!\u201c \u2014", "tokens": ["Den", "kran\u00b7ken", "Aar", "mit", "ge\u00b7broch\u00b7nen", "Schwin\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "\u201eo Vogel, warst so stolz und freventlich", "tokens": ["\u201e", "o", "Vo\u00b7gel", ",", "warst", "so", "stolz", "und", "fre\u00b7vent\u00b7lich"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "NE", "$,", "VAFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wolltest keine Fessel ewiglich!\u201c \u2014", "tokens": ["Und", "woll\u00b7test", "kei\u00b7ne", "Fes\u00b7sel", "e\u00b7wig\u00b7lich", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VMFIN", "PIAT", "NN", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201eweh, weh, zu Viele \u00fcber mich,", "tokens": ["\u201e", "weh", ",", "weh", ",", "zu", "Vie\u00b7le", "\u00fc\u00b7ber", "mich", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PTKVZ", "$,", "APPR", "PIS", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Adler all, \u2014 brachen mir die Schwingen!\u201c", "tokens": ["Und", "Ad\u00b7ler", "all", ",", "bra\u00b7chen", "mir", "die", "Schwin\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "PIAT", "$,", "$(", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201eso flattre in dein Nest, vom Aste fort,", "tokens": ["\u201e", "so", "flatt\u00b7re", "in", "dein", "Nest", ",", "vom", "As\u00b7te", "fort", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein Aechzen schier die Kr\u00e4uter mir verdorrt.\u201c", "tokens": ["Dein", "A\u00b7ech\u00b7zen", "schier", "die", "Kr\u00e4u\u00b7ter", "mir", "ver\u00b7dorrt", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ART", "NN", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "\u201eweh, weh, kein Nest hab' ich hinfort,", "tokens": ["\u201e", "weh", ",", "weh", ",", "kein", "Nest", "hab'", "ich", "hin\u00b7fort", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PTKVZ", "$,", "PIAT", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verbannter Aar mit gebrochnen Schwingen!\u201c", "tokens": ["Ver\u00b7bann\u00b7ter", "Aar", "mit", "ge\u00b7broch\u00b7nen", "Schwin\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u201eo Vogel, w\u00e4rst du eine Henne doch,", "tokens": ["\u201e", "o", "Vo\u00b7gel", ",", "w\u00e4rst", "du", "ei\u00b7ne", "Hen\u00b7ne", "doch", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$,", "VAFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dein Nestchen h\u00e4ttest du, im Ofenloch.\u201c", "tokens": ["Dein", "Nest\u00b7chen", "h\u00e4t\u00b7test", "du", ",", "im", "O\u00b7fen\u00b7loch", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "$,", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201eweh, weh, viel lieber ein Adler noch,", "tokens": ["\u201e", "weh", ",", "weh", ",", "viel", "lie\u00b7ber", "ein", "Ad\u00b7ler", "noch", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PTKVZ", "$,", "ADV", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Viel lieber ein Aar mit gebrochnen Schwingen!\u201c", "tokens": ["Viel", "lie\u00b7ber", "ein", "Aar", "mit", "ge\u00b7broch\u00b7nen", "Schwin\u00b7gen", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}}}}