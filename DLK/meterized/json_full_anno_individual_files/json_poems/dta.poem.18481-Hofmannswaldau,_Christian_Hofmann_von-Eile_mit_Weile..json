{"dta.poem.18481": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eile mit Weile.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwar eile/ doch nicht sehr/ was langsam wird vollendet/", "tokens": ["Zwar", "ei\u00b7le", "/", "doch", "nicht", "sehr", "/", "was", "lang\u00b7sam", "wird", "voll\u00b7en\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADV", "PTKNEG", "ADV", "$(", "PWS", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat offt uns mehr gen\u00fctzt/ als das/ was bald geendet.", "tokens": ["Hat", "offt", "uns", "mehr", "ge\u00b7n\u00fctzt", "/", "als", "das", "/", "was", "bald", "ge\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ADV", "VVPP", "$(", "KOUS", "ART", "$(", "PWS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}