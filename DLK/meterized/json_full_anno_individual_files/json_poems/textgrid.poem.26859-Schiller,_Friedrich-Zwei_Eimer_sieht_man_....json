{"textgrid.poem.26859": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Eimer sieht man ...", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Eimer sieht man ab und auf", "tokens": ["Zwei", "Ei\u00b7mer", "sieht", "man", "ab", "und", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PIS", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem Brunnen steigen,", "tokens": ["In", "ei\u00b7nem", "Brun\u00b7nen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schwebt der eine voll herauf,", "tokens": ["Und", "schwebt", "der", "ei\u00b7ne", "voll", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ART", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df sich der andre neigen.", "tokens": ["Mu\u00df", "sich", "der", "and\u00b7re", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie wandern rastlos hin und her,", "tokens": ["Sie", "wan\u00b7dern", "rast\u00b7los", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Abwechselnd voll und wieder leer,", "tokens": ["Ab\u00b7wech\u00b7selnd", "voll", "und", "wie\u00b7der", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und bringst du diesen an den Mund,", "tokens": ["Und", "bringst", "du", "die\u00b7sen", "an", "den", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "H\u00e4ngt jener in dem tiefsten Grund,", "tokens": ["H\u00e4ngt", "je\u00b7ner", "in", "dem", "tiefs\u00b7ten", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nie k\u00f6nnen sie mit ihren Gaben", "tokens": ["Nie", "k\u00f6n\u00b7nen", "sie", "mit", "ih\u00b7ren", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In gleichem Augenblick dich laben.", "tokens": ["In", "glei\u00b7chem", "Au\u00b7gen\u00b7blick", "dich", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zwei Eimer sieht man ab und auf", "tokens": ["Zwei", "Ei\u00b7mer", "sieht", "man", "ab", "und", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PIS", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem Brunnen steigen,", "tokens": ["In", "ei\u00b7nem", "Brun\u00b7nen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.