{"dta.poem.4007": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Menschenbeifall .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Ist nicht heilig mein Herz, sch\u00f6nren Lebens voll,", "tokens": ["Ist", "nicht", "hei\u00b7lig", "mein", "Herz", ",", "sch\u00f6n\u00b7ren", "Le\u00b7bens", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "PPOSAT", "NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Seit ich liebe? Warum achtetet ihr mich mehr,", "tokens": ["Seit", "ich", "lie\u00b7be", "?", "Wa\u00b7rum", "ach\u00b7te\u00b7tet", "ihr", "mich", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Da ich stolzer und wilder,", "tokens": ["Da", "ich", "stol\u00b7zer", "und", "wil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "ADJA", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wortereicher und leerer war?", "tokens": ["Wor\u00b7te\u00b7rei\u00b7cher", "und", "lee\u00b7rer", "war", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Ach! der Menge gef\u00e4llt, was auf den Marktplatz", "tokens": ["Ach", "!", "der", "Men\u00b7ge", "ge\u00b7f\u00e4llt", ",", "was", "auf", "den", "Markt\u00b7platz"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "VVPP", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "taugt,", "tokens": ["taugt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und es ehret der Knecht nur den Gewaltsamen;", "tokens": ["Und", "es", "eh\u00b7ret", "der", "Knecht", "nur", "den", "Ge\u00b7walt\u00b7sa\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "An das G\u00f6ttliche glauben", "tokens": ["An", "das", "G\u00f6tt\u00b7li\u00b7che", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Die allein, die es selber sind.", "tokens": ["Die", "al\u00b7lein", ",", "die", "es", "sel\u00b7ber", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}