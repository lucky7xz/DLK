{"dta.poem.2288": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An das Frauenzimmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sagt mir doch, geliebte Sch\u00f6nen,", "tokens": ["Sagt", "mir", "doch", ",", "ge\u00b7lieb\u00b7te", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist euch Amor denn nicht sichtbar?", "tokens": ["Ist", "euch", "A\u00b7mor", "denn", "nicht", "sicht\u00b7bar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NE", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder sagt ihrs niemand wieder,", "tokens": ["O\u00b7der", "sagt", "ihrs", "nie\u00b7mand", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil er allzu oft erscheinet?", "tokens": ["Weil", "er", "all\u00b7zu", "oft", "er\u00b7schei\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O! ihr d\u00fcrst es nicht verbergen,", "tokens": ["O", "!", "ihr", "d\u00fcrst", "es", "nicht", "ver\u00b7ber\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn er euch gleich oft erscheinet.", "tokens": ["Wenn", "er", "euch", "gleich", "oft", "er\u00b7schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Kan ein Gott euch Schande bringen?", "tokens": ["Kan", "ein", "Gott", "euch", "Schan\u00b7de", "brin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn er euch des Nachts belauschet,", "tokens": ["Wenn", "er", "euch", "des", "Nachts", "be\u00b7lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn er euch des Tages lokket:", "tokens": ["Wenn", "er", "euch", "des", "Ta\u00b7ges", "lok\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "O! so sagt es, euch zur Ehre,", "tokens": ["O", "!", "so", "sagt", "es", ",", "euch", "zur", "Eh\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "PPER", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Freunden oder G\u00f6nnern wieder.", "tokens": ["Freun\u00b7den", "o\u00b7der", "G\u00f6n\u00b7nern", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dann wird euch ein ieder loben.", "tokens": ["Dann", "wird", "euch", "ein", "ie\u00b7der", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Oder wolt ihrs mir entdekken:", "tokens": ["O\u00b7der", "wolt", "ihrs", "mir", "ent\u00b7dek\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "So will ich, ihr solt es schen,", "tokens": ["So", "will", "ich", ",", "ihr", "solt", "es", "schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Euch einmal den Amor fangen.", "tokens": ["Euch", "ein\u00b7mal", "den", "A\u00b7mor", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NE", "VVFIN", "$."], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Dann k\u00f6nt ihr mit goldnen Strikken,", "tokens": ["Dann", "k\u00f6nt", "ihr", "mit", "gold\u00b7nen", "Strik\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Ihn an euer Bette binden,", "tokens": ["Ihn", "an", "eu\u00b7er", "Bet\u00b7te", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Da\u00df er Wunsch und Klagen h\u00f6re.", "tokens": ["Da\u00df", "er", "Wunsch", "und", "Kla\u00b7gen", "h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Dann k\u00f6nt ihr ihm alles klagen,", "tokens": ["Dann", "k\u00f6nt", "ihr", "ihm", "al\u00b7les", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PIS", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Und ihn eher nicht befreien,", "tokens": ["Und", "ihn", "e\u00b7her", "nicht", "be\u00b7frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Bis er sich mit euch vers\u00f6hnet,", "tokens": ["Bis", "er", "sich", "mit", "euch", "ver\u00b7s\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Bis er alle Kammersorgen", "tokens": ["Bis", "er", "al\u00b7le", "Kam\u00b7mer\u00b7sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Mit der Kammerlust verwechselt;", "tokens": ["Mit", "der", "Kam\u00b7mer\u00b7lust", "ver\u00b7wech\u00b7selt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Bis er sich in allen St\u00fckken", "tokens": ["Bis", "er", "sich", "in", "al\u00b7len", "St\u00fck\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "G\u00fctig, wie ein Gott, erwiesen.", "tokens": ["G\u00fc\u00b7tig", ",", "wie", "ein", "Gott", ",", "er\u00b7wie\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "O! wie werdet ihr die G\u00fcte", "tokens": ["O", "!", "wie", "wer\u00b7det", "ihr", "die", "G\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Des gefangnen Gottes preisen.", "tokens": ["Des", "ge\u00b7fang\u00b7nen", "Got\u00b7tes", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Ruft mich nur, wenn er erscheinet,", "tokens": ["Ruft", "mich", "nur", ",", "wenn", "er", "er\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Denn ich wei\u00df ihn gut zu fangen.", "tokens": ["Denn", "ich", "wei\u00df", "ihn", "gut", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}