{"textgrid.poem.40317": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Niemals sah ich die Nacht begl\u00e4nzter!", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemals sah ich die Nacht begl\u00e4nzter!", "tokens": ["Nie\u00b7mals", "sah", "ich", "die", "Nacht", "be\u00b7gl\u00e4nz\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Diamantisch reizen die Fernen.", "tokens": ["Di\u00b7a\u00b7man\u00b7tisch", "rei\u00b7zen", "die", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Durch mein staubiges Kellerfenster", "tokens": ["Durch", "mein", "stau\u00b7bi\u00b7ges", "Kel\u00b7ler\u00b7fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "schielt der Schein der Gaslaternen,", "tokens": ["schielt", "der", "Schein", "der", "Gas\u00b7la\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "schielt auf meine frierenden H\u00e4nde,", "tokens": ["schielt", "auf", "mei\u00b7ne", "frie\u00b7ren\u00b7den", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "und mich qu\u00e4len Wollustbilder.", "tokens": ["und", "mich", "qu\u00e4\u00b7len", "Wol\u00b7lust\u00b7bil\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Grau sind diese nackten W\u00e4nde;", "tokens": ["Grau", "sind", "die\u00b7se", "nack\u00b7ten", "W\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "doch sie flimmern. Und mein wilder", "tokens": ["doch", "sie", "flim\u00b7mern", ".", "Und", "mein", "wil\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$.", "KON", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "irrender Wille kann sich nicht mehr t\u00e4uschen:", "tokens": ["ir\u00b7ren\u00b7der", "Wil\u00b7le", "kann", "sich", "nicht", "mehr", "t\u00e4u\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "unsre L\u00fcste wollen fruchtbar sein.", "tokens": ["uns\u00b7re", "L\u00fcs\u00b7te", "wol\u00b7len", "frucht\u00b7bar", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mit den Schatten meiner keuschen", "tokens": ["Mit", "den", "Schat\u00b7ten", "mei\u00b7ner", "keu\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kammer spielt ein schw\u00fcler Schein:", "tokens": ["Kam\u00b7mer", "spielt", "ein", "schw\u00fc\u00b7ler", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "an den hohen H\u00e4usern dr\u00fcben gl\u00fchen", "tokens": ["an", "den", "ho\u00b7hen", "H\u00e4u\u00b7sern", "dr\u00fc\u00b7ben", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aus der Finsternis die Fenster,", "tokens": ["aus", "der", "Fins\u00b7ter\u00b7nis", "die", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wo die Freudenm\u00e4dchen bl\u00fchen \u2013", "tokens": ["wo", "die", "Freu\u00b7den\u00b7m\u00e4d\u00b7chen", "bl\u00fc\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "niemals sah ich die Nacht begl\u00e4nzter!", "tokens": ["nie\u00b7mals", "sah", "ich", "die", "Nacht", "be\u00b7gl\u00e4nz\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und die Sterne sind wie brennende Blicke;", "tokens": ["Und", "die", "Ster\u00b7ne", "sind", "wie", "bren\u00b7nen\u00b7de", "Bli\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Welten sehnen sich nach mir!", "tokens": ["Wel\u00b7ten", "seh\u00b7nen", "sich", "nach", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich verschmachte. Ich ersticke.", "tokens": ["Ich", "ver\u00b7schmach\u00b7te", ".", "Ich", "er\u00b7sti\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja: ich frevelte an ihr \u2013", "tokens": ["Ja", ":", "ich", "fre\u00b7vel\u00b7te", "an", "ihr", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "ihr, der ich entrinnen wollte", "tokens": ["ihr", ",", "der", "ich", "ent\u00b7rin\u00b7nen", "woll\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und mich wie ein M\u00f6nch verkroch,", "tokens": ["und", "mich", "wie", "ein", "M\u00f6nch", "ver\u00b7kroch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "der dem Licht der Sinne grollte,", "tokens": ["der", "dem", "Licht", "der", "Sin\u00b7ne", "groll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "aber es entz\u00fcckt ihn doch!", "tokens": ["a\u00b7ber", "es", "ent\u00b7z\u00fcckt", "ihn", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Selbst in meiner kalten Zelle", "tokens": ["Selbst", "in", "mei\u00b7ner", "kal\u00b7ten", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00fchle ich das Leben toben,", "tokens": ["f\u00fch\u00b7le", "ich", "das", "Le\u00b7ben", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der ich wagte, dieses schnelle", "tokens": ["der", "ich", "wag\u00b7te", ",", "die\u00b7ses", "schnel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herz zu d\u00e4mpfen. Aber oben", "tokens": ["Herz", "zu", "d\u00e4mp\u00b7fen", ".", "A\u00b7ber", "o\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$.", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00fcber meinem dunklen Tale,", "tokens": ["\u00fc\u00b7ber", "mei\u00b7nem", "dunk\u00b7len", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine flammenden Fanale.", "tokens": ["Dei\u00b7ne", "flam\u00b7men\u00b7den", "Fa\u00b7na\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Blick hinaufgewandt", "tokens": ["Und", "den", "Blick", "hin\u00b7auf\u00b7ge\u00b7wandt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "ruf ich aus dem tiefen Turme", "tokens": ["ruf", "ich", "aus", "dem", "tie\u00b7fen", "Tur\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meiner \u00c4ngste zu dir hoch:", "tokens": ["mei\u00b7ner", "\u00c4ngs\u00b7te", "zu", "dir", "hoch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00f6ttin, wandle dich zum Wurme,", "tokens": ["G\u00f6t\u00b7tin", ",", "wand\u00b7le", "dich", "zum", "Wur\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "sei im Wurme G\u00f6ttin noch!", "tokens": ["sei", "im", "Wur\u00b7me", "G\u00f6t\u00b7tin", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sausend schaukelt eine Not mein Herz", "tokens": ["Sau\u00b7send", "schau\u00b7kelt", "ei\u00b7ne", "Not", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wie in erster s\u00fc\u00dfer Knabenfr\u00fche;", "tokens": ["wie", "in", "ers\u00b7ter", "s\u00fc\u00b7\u00dfer", "Kna\u00b7ben\u00b7fr\u00fc\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "ich verschmachte! ich vergl\u00fche!", "tokens": ["ich", "ver\u00b7schmach\u00b7te", "!", "ich", "ver\u00b7gl\u00fc\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "jeder Stern ist mir ein Schmerz!", "tokens": ["je\u00b7der", "Stern", "ist", "mir", "ein", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ihre Strahlen sind wie stechende Ruten", "tokens": ["Ih\u00b7re", "Strah\u00b7len", "sind", "wie", "ste\u00b7chen\u00b7de", "Ru\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "marternd, wenn du mich nicht k\u00fchlst,", "tokens": ["mar\u00b7ternd", ",", "wenn", "du", "mich", "nicht", "k\u00fchlst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn nicht Du mit deinem gn\u00e4digen Blute", "tokens": ["wenn", "nicht", "Du", "mit", "dei\u00b7nem", "gn\u00e4\u00b7di\u00b7gen", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "meine d\u00fcrstende Inbrunst stillst!", "tokens": ["mei\u00b7ne", "d\u00fcrs\u00b7ten\u00b7de", "In\u00b7brunst", "stillst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.12": {"line.1": {"text": "Sieh, da lichtet sich ein neues Fenster,", "tokens": ["Sieh", ",", "da", "lich\u00b7tet", "sich", "ein", "neu\u00b7es", "Fens\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "zuckt ein steiler Kerzenstreifen \u2013", "tokens": ["zuckt", "ein", "stei\u00b7ler", "Ker\u00b7zen\u00b7strei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "niemals sah ich die Nacht begl\u00e4nzter!", "tokens": ["nie\u00b7mals", "sah", "ich", "die", "Nacht", "be\u00b7gl\u00e4nz\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ja, entz\u00fcnde dich dem Reifen,", "tokens": ["Ja", ",", "ent\u00b7z\u00fcn\u00b7de", "dich", "dem", "Rei\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "alle andern sind verblichen.", "tokens": ["al\u00b7le", "an\u00b7dern", "sind", "ver\u00b7bli\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hinter jenen schwarzen Scheiben", "tokens": ["Hin\u00b7ter", "je\u00b7nen", "schwar\u00b7zen", "Schei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "schlafen alle Ordentlichen \u2013", "tokens": ["schla\u00b7fen", "al\u00b7le", "Or\u00b7dent\u00b7li\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "schlafen, wie sie immer schliefen,", "tokens": ["schla\u00b7fen", ",", "wie", "sie", "im\u00b7mer", "schlie\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wenn die Gottheit Ordnung ", "tokens": ["wenn", "die", "Got\u00b7theit", "Ord\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "w\u00e4hrend mir aus magischen Tiefen", "tokens": ["w\u00e4h\u00b7rend", "mir", "aus", "ma\u00b7gi\u00b7schen", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "auftaucht mit melodischem Ruf", "tokens": ["auf\u00b7taucht", "mit", "me\u00b7lo\u00b7di\u00b7schem", "Ruf"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.15": {"line.1": {"text": "Niemals sah ich die Nacht begl\u00e4nzter!", "tokens": ["Nie\u00b7mals", "sah", "ich", "die", "Nacht", "be\u00b7gl\u00e4nz\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Diamantisch reizen die Fernen.", "tokens": ["Di\u00b7a\u00b7man\u00b7tisch", "rei\u00b7zen", "die", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Durch mein staubiges Kellerfenster", "tokens": ["Durch", "mein", "stau\u00b7bi\u00b7ges", "Kel\u00b7ler\u00b7fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "schielt der Schein der Gaslaternen,", "tokens": ["schielt", "der", "Schein", "der", "Gas\u00b7la\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "schielt auf meine frierenden H\u00e4nde,", "tokens": ["schielt", "auf", "mei\u00b7ne", "frie\u00b7ren\u00b7den", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "und mich qu\u00e4len Wollustbilder.", "tokens": ["und", "mich", "qu\u00e4\u00b7len", "Wol\u00b7lust\u00b7bil\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Grau sind diese nackten W\u00e4nde;", "tokens": ["Grau", "sind", "die\u00b7se", "nack\u00b7ten", "W\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "doch sie flimmern. Und mein wilder", "tokens": ["doch", "sie", "flim\u00b7mern", ".", "Und", "mein", "wil\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "$.", "KON", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "irrender Wille kann sich nicht mehr t\u00e4uschen:", "tokens": ["ir\u00b7ren\u00b7der", "Wil\u00b7le", "kann", "sich", "nicht", "mehr", "t\u00e4u\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "unsre L\u00fcste wollen fruchtbar sein.", "tokens": ["uns\u00b7re", "L\u00fcs\u00b7te", "wol\u00b7len", "frucht\u00b7bar", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mit den Schatten meiner keuschen", "tokens": ["Mit", "den", "Schat\u00b7ten", "mei\u00b7ner", "keu\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kammer spielt ein schw\u00fcler Schein:", "tokens": ["Kam\u00b7mer", "spielt", "ein", "schw\u00fc\u00b7ler", "Schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "an den hohen H\u00e4usern dr\u00fcben gl\u00fchen", "tokens": ["an", "den", "ho\u00b7hen", "H\u00e4u\u00b7sern", "dr\u00fc\u00b7ben", "gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aus der Finsternis die Fenster,", "tokens": ["aus", "der", "Fins\u00b7ter\u00b7nis", "die", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wo die Freudenm\u00e4dchen bl\u00fchen \u2013", "tokens": ["wo", "die", "Freu\u00b7den\u00b7m\u00e4d\u00b7chen", "bl\u00fc\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "niemals sah ich die Nacht begl\u00e4nzter!", "tokens": ["nie\u00b7mals", "sah", "ich", "die", "Nacht", "be\u00b7gl\u00e4nz\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Und die Sterne sind wie brennende Blicke;", "tokens": ["Und", "die", "Ster\u00b7ne", "sind", "wie", "bren\u00b7nen\u00b7de", "Bli\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Welten sehnen sich nach mir!", "tokens": ["Wel\u00b7ten", "seh\u00b7nen", "sich", "nach", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich verschmachte. Ich ersticke.", "tokens": ["Ich", "ver\u00b7schmach\u00b7te", ".", "Ich", "er\u00b7sti\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja: ich frevelte an ihr \u2013", "tokens": ["Ja", ":", "ich", "fre\u00b7vel\u00b7te", "an", "ihr", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "ihr, der ich entrinnen wollte", "tokens": ["ihr", ",", "der", "ich", "ent\u00b7rin\u00b7nen", "woll\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVINF", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und mich wie ein M\u00f6nch verkroch,", "tokens": ["und", "mich", "wie", "ein", "M\u00f6nch", "ver\u00b7kroch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "der dem Licht der Sinne grollte,", "tokens": ["der", "dem", "Licht", "der", "Sin\u00b7ne", "groll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "aber es entz\u00fcckt ihn doch!", "tokens": ["a\u00b7ber", "es", "ent\u00b7z\u00fcckt", "ihn", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Selbst in meiner kalten Zelle", "tokens": ["Selbst", "in", "mei\u00b7ner", "kal\u00b7ten", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "f\u00fchle ich das Leben toben,", "tokens": ["f\u00fch\u00b7le", "ich", "das", "Le\u00b7ben", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "der ich wagte, dieses schnelle", "tokens": ["der", "ich", "wag\u00b7te", ",", "die\u00b7ses", "schnel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herz zu d\u00e4mpfen. Aber oben", "tokens": ["Herz", "zu", "d\u00e4mp\u00b7fen", ".", "A\u00b7ber", "o\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$.", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "\u00fcber meinem dunklen Tale,", "tokens": ["\u00fc\u00b7ber", "mei\u00b7nem", "dunk\u00b7len", "Ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine flammenden Fanale.", "tokens": ["Dei\u00b7ne", "flam\u00b7men\u00b7den", "Fa\u00b7na\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Blick hinaufgewandt", "tokens": ["Und", "den", "Blick", "hin\u00b7auf\u00b7ge\u00b7wandt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "ruf ich aus dem tiefen Turme", "tokens": ["ruf", "ich", "aus", "dem", "tie\u00b7fen", "Tur\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "meiner \u00c4ngste zu dir hoch:", "tokens": ["mei\u00b7ner", "\u00c4ngs\u00b7te", "zu", "dir", "hoch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00f6ttin, wandle dich zum Wurme,", "to