{"dta.poem.9485": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n ODE/  \n  Darinnen die ersten beyden Verse Tro-  \n ch\u00e4isch/ der 3. und 4. Jambisch/ der 5. wider Tro-  \n ch\u00e4isch/ der 6. und 7. aber Dacty-  \n lisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich nun", "tokens": ["Soll", "ich", "nun"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Dieses thun", "tokens": ["Die\u00b7ses", "thun"], "token_info": ["word", "word"], "pos": ["PDS", "VVINF"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "und bleiben hier", "tokens": ["und", "blei\u00b7ben", "hier"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ohn\u2019 alle zier!", "tokens": ["Ohn'", "al\u00b7le", "zier", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}