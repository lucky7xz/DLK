{"dta.poem.1563": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff das selige Absterben Frauen Anna Magda-  \n lena Abschatzin gebohrner Reibnitzin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Dich solt ich/ Edler Geist/ und deiner Tugend Gaben/", "tokens": ["Dich", "solt", "ich", "/", "Ed\u00b7ler", "Geist", "/", "und", "dei\u00b7ner", "Tu\u00b7gend", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$(", "ADJA", "NN", "$(", "KON", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So dich ins Sternen-Reich numehr versetzet haben/", "tokens": ["So", "dich", "ins", "Ster\u00b7nen\u00b7Reich", "nu\u00b7mehr", "ver\u00b7set\u00b7zet", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach meiner Schuldigkeit durch kluger Worte Zier", "tokens": ["Nach", "mei\u00b7ner", "Schul\u00b7dig\u00b7keit", "durch", "klu\u00b7ger", "Wor\u00b7te", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu ehren seyn bem\u00fcht; allein es heist mit mir:", "tokens": ["Zu", "eh\u00b7ren", "seyn", "be\u00b7m\u00fcht", ";", "al\u00b7lein", "es", "heist", "mit", "mir", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAINF", "VVFIN", "$.", "ADV", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo Thr\u00e4nen auffs Papier und in die Dinte schiessen/", "tokens": ["Wo", "Thr\u00e4\u00b7nen", "auffs", "Pa\u00b7pier", "und", "in", "die", "Din\u00b7te", "schies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan kein gelinder Reim aus reiner Feder fliessen.", "tokens": ["Kan", "kein", "ge\u00b7lin\u00b7der", "Reim", "aus", "rei\u00b7ner", "Fe\u00b7der", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da/ wo der schwartze Rock allein im Leyde gehet/", "tokens": ["Da", "/", "wo", "der", "schwart\u00b7ze", "Rock", "al\u00b7lein", "im", "Ley\u00b7de", "ge\u00b7het", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "ART", "ADJA", "NN", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Florne Feder-Pusch zum Sprunge fertig stehet/", "tokens": ["Der", "Flor\u00b7ne", "Fe\u00b7der\u00b7Pusch", "zum", "Sprun\u00b7ge", "fer\u00b7tig", "ste\u00b7het", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und nach dem Winde tanzt/ wenn der gezwungne Fu\u00df", "tokens": ["Und", "nach", "dem", "Win\u00b7de", "tanzt", "/", "wenn", "der", "ge\u00b7zwung\u00b7ne", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sich irgends auff ein Jahr davon enthalten mu\u00df/", "tokens": ["Sich", "ir\u00b7gends", "auff", "ein", "Jahr", "da\u00b7von", "ent\u00b7hal\u00b7ten", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "ART", "NN", "PAV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Kan wohl der freye Sinn ein sch\u00f6nes Grab-Lied schreiben.", "tokens": ["Kan", "wohl", "der", "frey\u00b7e", "Sinn", "ein", "sch\u00f6\u00b7nes", "Grab\u00b7Lied", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch da\u00df nicht meine Pflicht mag gantz zur\u00fccke bleiben/", "tokens": ["Doch", "da\u00df", "nicht", "mei\u00b7ne", "Pflicht", "mag", "gantz", "zu\u00b7r\u00fc\u00b7cke", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "PPOSAT", "NN", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So soll der Seligsten die kurtze Grabschrifft stehn:", "tokens": ["So", "soll", "der", "Se\u00b7ligs\u00b7ten", "die", "kurt\u00b7ze", "Grab\u00b7schrifft", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Nein Leser/ der du wirst die Grufft vor\u00fcber gehn/", "tokens": ["Nein", "Le\u00b7ser", "/", "der", "du", "wirst", "die", "Grufft", "vor\u00b7\u00fc\u00b7ber", "gehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$(", "PRELS", "PPER", "VAFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein Schatz der Fr\u00f6mmigkeit/ ein Spiegel Edler Frauen/", "tokens": ["Ein", "Schatz", "der", "Fr\u00f6m\u00b7mig\u00b7keit", "/", "ein", "Spie\u00b7gel", "Ed\u00b7ler", "Frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "ie\u00df hier/ was sterblich war/ der Erden anvertrauen.", "tokens": ["ie\u00df", "hier", "/", "was", "sterb\u00b7lich", "war", "/", "der", "Er\u00b7den", "an\u00b7ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PWS", "ADJD", "VAFIN", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "hr wohlverdienter Ruhm wird in viel Hertzen leben/", "tokens": ["hr", "wohl\u00b7ver\u00b7dien\u00b7ter", "Ruhm", "wird", "in", "viel", "Hert\u00b7zen", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "i\u00df sie des H\u00f6chsten Hand wird in den Himmel heben.", "tokens": ["i\u00df", "sie", "des", "H\u00f6chs\u00b7ten", "Hand", "wird", "in", "den", "Him\u00b7mel", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}