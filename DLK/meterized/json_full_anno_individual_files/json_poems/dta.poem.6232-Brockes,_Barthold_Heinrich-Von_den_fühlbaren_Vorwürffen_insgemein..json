{"dta.poem.6232": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den f\u00fchlbaren Vorw\u00fcrffen  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch T\u00f6n'und durchs Ger\u00e4usch wird unser Ohr bewegt,", "tokens": ["Durch", "T\u00f6n'und", "durchs", "Ge\u00b7r\u00e4usch", "wird", "un\u00b7ser", "Ohr", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es r\u00fchren das Gesicht der Sonnen helle Blitze,", "tokens": ["Es", "r\u00fch\u00b7ren", "das", "Ge\u00b7sicht", "der", "Son\u00b7nen", "hel\u00b7le", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Farben Glantz wird uns durch sie zur Schau gelegt,", "tokens": ["Der", "Far\u00b7ben", "Glantz", "wird", "uns", "durch", "sie", "zur", "Schau", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "APPR", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jm F\u00fchlen sp\u00fcret man die K\u00e4lt\u2019 nud auch die Hitze,", "tokens": ["Jm", "F\u00fch\u00b7len", "sp\u00fc\u00b7ret", "man", "die", "K\u00e4lt'", "nud", "auch", "die", "Hit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was weich, was hart, was fl\u00fcssig, scharff und glatt,", "tokens": ["Was", "weich", ",", "was", "hart", ",", "was", "fl\u00fcs\u00b7sig", ",", "scharff", "und", "glatt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$,", "PRELS", "ADJD", "$,", "PRELS", "ADJD", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was trocken ist, was feucht. Es liefern S\u00e4fft\u2019und Fr\u00fcchte,", "tokens": ["Was", "tro\u00b7cken", "ist", ",", "was", "feucht", ".", "Es", "lie\u00b7fern", "S\u00e4fft'und", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PRELS", "ADJD", "$.", "PPER", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "In mancherley Geschmack, so mancherley Gerichte.", "tokens": ["In", "man\u00b7cher\u00b7ley", "Ge\u00b7schmack", ",", "so", "man\u00b7cher\u00b7ley", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus Bl\u00fcht-und Rauch-Werck dampfft ein\u2019angenehme Krafft;", "tokens": ["Aus", "Bl\u00fcht\u00b7\u00b7und", "Rauch\u00b7\u00b7Werck", "dampfft", "ein'\u00b7an\u00b7ge\u00b7neh\u00b7me", "Krafft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein jeder Vorwurf zeigt durch die Erfahrung an,", "tokens": ["Ein", "je\u00b7der", "Vor\u00b7wurf", "zeigt", "durch", "die", "Er\u00b7fah\u00b7rung", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und giebt uns eine Wissenschafft,", "tokens": ["Und", "giebt", "uns", "ei\u00b7ne", "Wis\u00b7sen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df er in uns was w\u00fcrckt, und was er w\u00fcrcken kan.", "tokens": ["Da\u00df", "er", "in", "uns", "was", "w\u00fcrckt", ",", "und", "was", "er", "w\u00fcr\u00b7cken", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PIS", "VVFIN", "$,", "KON", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}