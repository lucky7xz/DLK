{"dta.poem.151": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Todte redet sie an.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Frolocke/ Stoltze/ nicht/ mein blasses Schatten-Ich", "tokens": ["Fro\u00b7lo\u00b7cke", "/", "Stolt\u00b7ze", "/", "nicht", "/", "mein", "blas\u00b7ses", "Schat\u00b7ten\u00b7Ich"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "PTKNEG", "$(", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Das soll zu steten Schmertz dich qu\u00e4hlen \u00e4nstiglich.", "tokens": ["Das", "soll", "zu", "ste\u00b7ten", "Schmertz", "dich", "qu\u00e4h\u00b7len", "\u00e4ns\u00b7tig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ADJA", "NN", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Geist/ der heischet Rach sie wird nicht aussen bleiben/", "tokens": ["Mein", "Geist", "/", "der", "hei\u00b7schet", "Rach", "sie", "wird", "nicht", "aus\u00b7sen", "blei\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "VVFIN", "NN", "PPER", "VAFIN", "PTKNEG", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Hertzens-Quaal/ die soll dich Tag und Nacht umtreiben.", "tokens": ["Die", "Hert\u00b7zens\u00b7Quaal", "/", "die", "soll", "dich", "Tag", "und", "Nacht", "um\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die grimmen Furien sind schon auf Rach bedacht/", "tokens": ["Die", "grim\u00b7men", "Fu\u00b7ri\u00b7en", "sind", "schon", "auf", "Rach", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch dein Gewissen ist dir noch nicht auffgewacht/", "tokens": ["Doch", "dein", "Ge\u00b7wis\u00b7sen", "ist", "dir", "noch", "nicht", "auff\u00b7ge\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "La\u00df aber erst den Schwarm aus seinen Schrancken brechen/", "tokens": ["La\u00df", "a\u00b7ber", "erst", "den", "Schwarm", "aus", "sei\u00b7nen", "Schran\u00b7cken", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wird der Hertzens-Wurm dich allzugrausahm stechen.", "tokens": ["So", "wird", "der", "Hert\u00b7zens\u00b7Wurm", "dich", "all\u00b7zu\u00b7grau\u00b7sahm", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}