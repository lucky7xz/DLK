"pos": ["PWAV", "PPER", "PTKVZ", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wir m\u00fcssen doch den strengen Herrn", "tokens": ["Wir", "m\u00fcs\u00b7sen", "doch", "den", "stren\u00b7gen", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein wenig n\u00e4her sehn.", "tokens": ["Ein", "we\u00b7nig", "n\u00e4\u00b7her", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Heraus, heraus, ihr Kindlein all!", "tokens": ["He\u00b7raus", ",", "he\u00b7raus", ",", "ihr", "Kin\u00b7dlein", "all", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sieht recht munter aus.", "tokens": ["Er", "sieht", "recht", "mun\u00b7ter", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heraus, heraus, mit Sang und Schall!", "tokens": ["He\u00b7raus", ",", "he\u00b7raus", ",", "mit", "Sang", "und", "Schall", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist ein lust'ger Strau\u00df.", "tokens": ["Das", "ist", "ein", "lust'\u00b7ger", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Was gr\u00fcne Flur, was Blumen bunt!", "tokens": ["Was", "gr\u00fc\u00b7ne", "Flur", ",", "was", "Blu\u00b7men", "bunt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "$,", "PWS", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir haben Schnee und Eis.", "tokens": ["Wir", "ha\u00b7ben", "Schnee", "und", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie flimmert alles rings und rund", "tokens": ["Wie", "flim\u00b7mert", "al\u00b7les", "rings", "und", "rund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PIS", "ADV", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So silbern und so wei\u00df!", "tokens": ["So", "sil\u00b7bern", "und", "so", "wei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr Br\u00fcder auf! herunter frisch!", "tokens": ["Ihr", "Br\u00fc\u00b7der", "auf", "!", "her\u00b7un\u00b7ter", "frisch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fragt nur nicht erst Papa,", "tokens": ["Fragt", "nur", "nicht", "erst", "Pa\u00b7pa", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "ADV", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "La\u00dft B\u00fccher gleich und Schreibetisch:", "tokens": ["La\u00dft", "B\u00fc\u00b7cher", "gleich", "und", "Schrei\u00b7be\u00b7tisch", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der erste Schnee ist da.", "tokens": ["Der", "ers\u00b7te", "Schnee", "ist", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Komm, Schwesterlein, komm flink heraus,", "tokens": ["Komm", ",", "Schwes\u00b7ter\u00b7lein", ",", "komm", "flink", "he\u00b7raus", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mag schelten die Mama,", "tokens": ["Mag", "schel\u00b7ten", "die", "Ma\u00b7ma", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Pupp' und Strickzeug la\u00df zu Haus,", "tokens": ["Und", "Pupp'", "und", "Strick\u00b7zeug", "la\u00df", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der erste Schnee ist da.", "tokens": ["Der", "ers\u00b7te", "Schnee", "ist", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nun lustig schnell den Berg hinab", "tokens": ["Nun", "lus\u00b7tig", "schnell", "den", "Berg", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Schnee und \u00fcber Eis,", "tokens": ["Durch", "Schnee", "und", "\u00fc\u00b7ber", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und werft mit Schnee und sch\u00fcttelt's ab,", "tokens": ["Und", "werft", "mit", "Schnee", "und", "sch\u00fct\u00b7telt's", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es Mama nicht wei\u00df.", "tokens": ["Da\u00df", "es", "Ma\u00b7ma", "nicht", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ei Winter, lust'ger Wintersmann,", "tokens": ["Ei", "Win\u00b7ter", ",", "lust'\u00b7ger", "Win\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sagen, du seist kalt,", "tokens": ["Sie", "sa\u00b7gen", ",", "du", "seist", "kalt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4rst ganz mit Pelzen angethan", "tokens": ["W\u00e4rst", "ganz", "mit", "Pel\u00b7zen", "an\u00b7ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gr\u00e4mlich sehr und alt.", "tokens": ["Und", "gr\u00e4m\u00b7lich", "sehr", "und", "alt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ei Wintersmann, warum nicht gar,", "tokens": ["Ei", "Win\u00b7ters\u00b7mann", ",", "wa\u00b7rum", "nicht", "gar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PWAV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab' es gleich gedacht,", "tokens": ["Ich", "hab'", "es", "gleich", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie kennen dich nur schlecht, nicht wahr?", "tokens": ["Sie", "ken\u00b7nen", "dich", "nur", "schlecht", ",", "nicht", "wahr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir hast dich kund gemacht.", "tokens": ["Mir", "hast", "dich", "kund", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du bist ein lustiger Gesell", "tokens": ["Du", "bist", "ein", "lus\u00b7ti\u00b7ger", "Ge\u00b7sell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und munter wie ein Fisch,", "tokens": ["Und", "mun\u00b7ter", "wie", "ein", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hast Augen himmelblau und hell,", "tokens": ["Hast", "Au\u00b7gen", "him\u00b7mel\u00b7blau", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bist just wie ich so frisch.", "tokens": ["Bist", "just", "wie", "ich", "so", "frisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wir sind dir gut, wir Kinder hier,", "tokens": ["Wir", "sind", "dir", "gut", ",", "wir", "Kin\u00b7der", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bringst tausend Spa\u00df, du Wicht,", "tokens": ["Bringst", "tau\u00b7send", "Spa\u00df", ",", "du", "Wicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und siehst du, daf\u00fcr werf' ich dir", "tokens": ["Und", "siehst", "du", ",", "da\u00b7f\u00fcr", "wer\u00b7f'", "ich", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PAV", "VAFIN", "PPER", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Schneeball in's Gesicht.", "tokens": ["Den", "Schnee\u00b7ball", "in's", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Wintermann mit Reif und Eis", "tokens": ["Der", "Win\u00b7ter\u00b7mann", "mit", "Reif", "und", "Eis"], "token_info": ["word", "wor