{"dta.poem.19686": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Gustav Adolphs Tod .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach k\u00f6nnt ich meine Stimm dem Donner gleich er-               ", "tokens": ["Ach", "k\u00f6nnt", "ich", "mei\u00b7ne", "Stimm", "dem", "Don\u00b7ner", "gleich", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "heben,", "tokens": ["he\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df sie, die weite Welt erschreckend, m\u00f6g erbeben,", "tokens": ["Da\u00df", "sie", ",", "die", "wei\u00b7te", "Welt", "er\u00b7schre\u00b7ckend", ",", "m\u00f6g", "er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJA", "NN", "VVPP", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wollt ich ersteigen bald, trostlos und ruhelos", "tokens": ["Wollt", "ich", "er\u00b7stei\u00b7gen", "bald", ",", "trost\u00b7los", "und", "ru\u00b7he\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den allerh\u00f6chsten Berg, zu alles Geists verwundern,", "tokens": ["Den", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Berg", ",", "zu", "al\u00b7les", "Geists", "ver\u00b7wun\u00b7dern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mir \u00fcberlauter Macht aus meiner Brust ausdundern:", "tokens": ["Mir", "\u00fc\u00b7berl\u00b7au\u00b7ter", "Macht", "aus", "mei\u00b7ner", "Brust", "aus\u00b7dun\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gustav der Gro\u00df ist todt, todt ist Gustav der Gro\u00df.", "tokens": ["Gus\u00b7tav", "der", "Gro\u00df", "ist", "todt", ",", "todt", "ist", "Gus\u00b7tav", "der", "Gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "VAFIN", "ADJD", "$,", "ADJD", "VAFIN", "NE", "ART", "NE", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Ihn hat das wilde Meer der Schweden Schatz getragen,", "tokens": ["Ihn", "hat", "das", "wil\u00b7de", "Meer", "der", "Schwe\u00b7den", "Schatz", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu uns so still und glat, dem Meerzug nicht zu schaden,", "tokens": ["Zu", "uns", "so", "still", "und", "glat", ",", "dem", "Meer\u00b7zug", "nicht", "zu", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$,", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ihm war so lieb und werth des K\u00f6nigs Gegenwart,", "tokens": ["Ihm", "war", "so", "lieb", "und", "werth", "des", "K\u00f6\u00b7nigs", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Wind enthielt sich auch von allem Sturm und Rasen,", "tokens": ["Der", "Wind", "ent\u00b7hielt", "sich", "auch", "von", "al\u00b7lem", "Sturm", "und", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erfreuend sich allein die Segel aufzublasen,", "tokens": ["Er\u00b7freu\u00b7end", "sich", "al\u00b7lein", "die", "Se\u00b7gel", "auf\u00b7zu\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PRF", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Beg\u00fcnstigend nach Wunsch des Helden Ueberfahrt.", "tokens": ["Be\u00b7g\u00fcns\u00b7ti\u00b7gend", "nach", "Wunsch", "des", "Hel\u00b7den", "Ue\u00b7ber\u00b7fahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}