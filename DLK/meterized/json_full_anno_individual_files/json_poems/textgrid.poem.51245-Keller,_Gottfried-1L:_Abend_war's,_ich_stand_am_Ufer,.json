{"textgrid.poem.51245": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Abend war's, ich stand am Ufer,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abend war's, ich stand am Ufer,", "tokens": ["A\u00b7bend", "wa\u00b7r's", ",", "ich", "stand", "am", "U\u00b7fer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wo die Wellen freudig rauschten", "tokens": ["Wo", "die", "Wel\u00b7len", "freu\u00b7dig", "rauschten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, vom S\u00fcden her gewaltig", "tokens": ["Und", ",", "vom", "S\u00fc\u00b7den", "her", "ge\u00b7wal\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPRART", "NN", "APZR", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hergeeilt, am Strand ersch\u00e4umten.", "tokens": ["Her\u00b7ge\u00b7eilt", ",", "am", "Strand", "er\u00b7sch\u00e4um\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Violett war ihr Gewand,", "tokens": ["Vi\u00b7o\u00b7lett", "war", "ihr", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch sie trugen rote Kronen,", "tokens": ["Doch", "sie", "tru\u00b7gen", "ro\u00b7te", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die von Haupt zu Haupt sie warfen,", "tokens": ["Die", "von", "Haupt", "zu", "Haupt", "sie", "war\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Klangvoll ineinanderflie\u00dfend.", "tokens": ["Klang\u00b7voll", "in\u00b7ein\u00b7an\u00b7der\u00b7flie\u00b7\u00dfend", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Durch der Wolken wildes Jagen,", "tokens": ["Durch", "der", "Wol\u00b7ken", "wil\u00b7des", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Einsam, sah der Abendstern,", "tokens": ["Ein\u00b7sam", ",", "sah", "der", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Gl\u00e4nzend, wie der Sch\u00f6nheit Auge,", "tokens": ["Gl\u00e4n\u00b7zend", ",", "wie", "der", "Sch\u00f6n\u00b7heit", "Au\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Gro\u00df ergl\u00fchend, wie die Sehnsucht.", "tokens": ["Gro\u00df", "er\u00b7gl\u00fc\u00b7hend", ",", "wie", "die", "Sehn\u00b7sucht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PWAV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich sagte zu den Wellen:", "tokens": ["Und", "ich", "sag\u00b7te", "zu", "den", "Wel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbnoch so laut und flei\u00dfig seid ihr?", "tokens": ["\u00bb", "noch", "so", "laut", "und", "flei\u00b7\u00dfig", "seid", "ihr", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch ich seh nicht, was ihr schaffet,", "tokens": ["Doch", "ich", "seh", "nicht", ",", "was", "ihr", "schaf\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn kein Segel ist zu finden,", "tokens": ["Denn", "kein", "Se\u00b7gel", "ist", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil es Nacht wird und die m\u00fcde", "tokens": ["Weil", "es", "Nacht", "wird", "und", "die", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "KON", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sorgenvolle Woche hingeht!\u00ab", "tokens": ["Sor\u00b7gen\u00b7vol\u00b7le", "Wo\u00b7che", "hin\u00b7geht", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Und sie riefen laut erbrausend:", "tokens": ["Und", "sie", "rie\u00b7fen", "laut", "er\u00b7brau\u00b7send", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbfeierabend ist's, wir tanzen", "tokens": ["\u00bb", "fei\u00b7er\u00b7a\u00b7bend", "ist's", ",", "wir", "tan\u00b7zen"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "VAFIN", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eben noch f\u00fcr uns ein T\u00e4nzchen!", "tokens": ["E\u00b7ben", "noch", "f\u00fcr", "uns", "ein", "T\u00e4nz\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Hirt den Schnitterinnen", "tokens": ["Wie", "der", "Hirt", "den", "Schnit\u00b7te\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Abendlich den Reigen bl\u00e4set,", "tokens": ["A\u00b7bend\u00b7lich", "den", "Rei\u00b7gen", "bl\u00e4\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Also spielt der wilde Bruder", "tokens": ["Al\u00b7so", "spielt", "der", "wil\u00b7de", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Uns, der hei\u00dfe F\u00f6hn, zum Tanze,", "tokens": ["Uns", ",", "der", "hei\u00b7\u00dfe", "F\u00f6hn", ",", "zum", "Tan\u00b7ze", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und er darf uns alle k\u00fcssen!", "tokens": ["Und", "er", "darf", "uns", "al\u00b7le", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In der Freiheit, in der Freude", "tokens": ["In", "der", "Frei\u00b7heit", ",", "in", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schlagen wir f\u00fcr uns ein St\u00fcndchen!", "tokens": ["Schla\u00b7gen", "wir", "f\u00fcr", "uns", "ein", "St\u00fcnd\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wollt ein Schiff uns jetzt befahren,", "tokens": ["Wollt", "ein", "Schiff", "uns", "jetzt", "be\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "M\u00fc\u00dft es untergehn und brechen!", "tokens": ["M\u00fc\u00dft", "es", "un\u00b7ter\u00b7gehn", "und", "bre\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wir raten dir nicht minder:", "tokens": ["Und", "wir", "ra\u00b7ten", "dir", "nicht", "min\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freiheit gib auch du den Wellen", "tokens": ["Frei\u00b7heit", "gib", "auch", "du", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVIMP", "ADV", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deines Blutes einmal wieder,", "tokens": ["Dei\u00b7nes", "Blu\u00b7tes", "ein\u00b7mal", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df das Schifflein untergehen", "tokens": ["La\u00df", "das", "Schif\u00b7flein", "un\u00b7ter\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem schweren goldnen Bilde,", "tokens": ["Mit", "dem", "schwe\u00b7ren", "gold\u00b7nen", "Bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der ungeschlachten Schiff'rin,", "tokens": ["Mit", "der", "un\u00b7ge\u00b7schlach\u00b7ten", "Schiff'\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die dein wogend Herz bef\u00e4hrt", "tokens": ["Die", "dein", "wo\u00b7gend", "Herz", "be\u00b7f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schon so lang und es bedr\u00fcckt!", "tokens": ["Schon", "so", "lang", "und", "es", "be\u00b7dr\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "La\u00df die Furcht und la\u00df die Hoffnung", "tokens": ["La\u00df", "die", "Furcht", "und", "la\u00df", "die", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "KON", "VVIMP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "In emp\u00f6rter Flut versinken", "tokens": ["In", "em\u00b7p\u00f6r\u00b7ter", "Flut", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und erfreue dich der Freiheit!\u00ab", "tokens": ["Und", "er\u00b7freu\u00b7e", "dich", "der", "Frei\u00b7heit", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach! die allzutreuen Wellen", "tokens": ["Ach", "!", "die", "all\u00b7zu\u00b7treu\u00b7en", "Wel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines unterjochten Blutes", "tokens": ["Mei\u00b7nes", "un\u00b7ter\u00b7joch\u00b7ten", "Blu\u00b7tes"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollen es nicht sinken lassen;", "tokens": ["Wol\u00b7len", "es", "nicht", "sin\u00b7ken", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer taucht empor es wieder,", "tokens": ["Im\u00b7mer", "taucht", "em\u00b7por", "es", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Triumphierend f\u00e4hrt's empor,", "tokens": ["Tri\u00b7um\u00b7phie\u00b7rend", "f\u00e4hrt's", "em\u00b7por", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schiff und Bild, ach, Schiff und G\u00f6tzin!", "tokens": ["Schiff", "und", "Bild", ",", "ach", ",", "Schiff", "und", "G\u00f6t\u00b7zin", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ITJ", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Einzig hilft, es rasch entheben", "tokens": ["Ein\u00b7zig", "hilft", ",", "es", "rasch", "ent\u00b7he\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und es in der Luft erw\u00fcrgen!", "tokens": ["Und", "es", "in", "der", "Luft", "er\u00b7w\u00fcr\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Also tat ich in der Nacht,", "tokens": ["Al\u00b7so", "tat", "ich", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Still in einer Fr\u00fchlingsnacht.", "tokens": ["Still", "in", "ei\u00b7ner", "Fr\u00fch\u00b7lings\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen schwachen Seufzer h\u00f6rt ich,", "tokens": ["Ei\u00b7nen", "schwa\u00b7chen", "Seuf\u00b7zer", "h\u00f6rt", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deutlich, wie aus weiter Ferne;", "tokens": ["Deut\u00b7lich", ",", "wie", "aus", "wei\u00b7ter", "Fer\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPR", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn von den Bet\u00f6rten endlich", "tokens": ["Denn", "von", "den", "Be\u00b7t\u00f6r\u00b7ten", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Auch einmal vergessen werden", "tokens": ["Auch", "ein\u00b7mal", "ver\u00b7ges\u00b7sen", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tut den Vielgeliebten weh,", "tokens": ["Tut", "den", "Viel\u00b7ge\u00b7lieb\u00b7ten", "weh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sie f\u00fchlen's in der Ferne.", "tokens": ["Und", "sie", "f\u00fch\u00b7len's", "in", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART",