{"dta.poem.11806": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ragen himmelan die kalten dort und stolzen", "tokens": ["Was", "ra\u00b7gen", "him\u00b7me\u00b7lan", "die", "kal\u00b7ten", "dort", "und", "stol\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "ADV", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bergriesen, denen nie ist Schnee und Eis geschmolzen?", "tokens": ["Berg\u00b7rie\u00b7sen", ",", "de\u00b7nen", "nie", "ist", "Schnee", "und", "Eis", "ge\u00b7schmol\u00b7zen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "ADV", "VAFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Die Sonn' im Aufgang scheint sich \u00fcber sie zu w\u00e4lzen,", "tokens": ["Die", "Sonn'", "im", "Auf\u00b7gang", "scheint", "sich", "\u00fc\u00b7ber", "sie", "zu", "w\u00e4l\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "PRF", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch kann ihr Lebenstral den Todesfrost nicht schmelzen.", "tokens": ["Doch", "kann", "ihr", "Le\u00b7ben\u00b7stral", "den", "To\u00b7des\u00b7frost", "nicht", "schmel\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und nur wo tiefer dringt herab ins niedre Thal,", "tokens": ["Und", "nur", "wo", "tie\u00b7fer", "dringt", "her\u00b7ab", "ins", "nied\u00b7re", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWAV", "ADJD", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weckt Erdenlebenslust der Himmelslebenstral.", "tokens": ["Weckt", "Er\u00b7den\u00b7le\u00b7bens\u00b7lust", "der", "Him\u00b7mels\u00b7le\u00b7ben\u00b7stral", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Was ists? w\u00e4r' etwa kalt die Sonn' in ihrer N\u00e4he,", "tokens": ["Was", "ists", "?", "w\u00e4r'", "et\u00b7wa", "kalt", "die", "Sonn'", "in", "ih\u00b7rer", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$.", "VAFIN", "ADV", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schiene w\u00e4rmer dem, der sie vom weiten s\u00e4he?", "tokens": ["Und", "schie\u00b7ne", "w\u00e4r\u00b7mer", "dem", ",", "der", "sie", "vom", "wei\u00b7ten", "s\u00e4\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "$,", "PRELS", "PPER", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nein, sondern ob der Welt so hoch ist Sonnenmacht,", "tokens": ["Nein", ",", "son\u00b7dern", "ob", "der", "Welt", "so", "hoch", "ist", "Son\u00b7nen\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "KOUS", "ART", "NN", "ADV", "ADJD", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df keinen Unterschied die Spanne h\u00f6her macht.", "tokens": ["Da\u00df", "kei\u00b7nen", "Un\u00b7ter\u00b7schied", "die", "Span\u00b7ne", "h\u00f6\u00b7her", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die stolzen haben sich der Erden \u00fcberhoben,", "tokens": ["Die", "stol\u00b7zen", "ha\u00b7ben", "sich", "der", "Er\u00b7den", "\u00fc\u00b7berh\u00b7o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kamen n\u00e4her nicht darum dem Himmel droben.", "tokens": ["Und", "ka\u00b7men", "n\u00e4\u00b7her", "nicht", "da\u00b7rum", "dem", "Him\u00b7mel", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKNEG", "PAV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Himmelssonne nun, zu der Bescheidnen Trost,", "tokens": ["Die", "Him\u00b7mels\u00b7son\u00b7ne", "nun", ",", "zu", "der", "Be\u00b7scheid\u00b7nen", "Trost", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gibt diesen Lebensw\u00e4rm' und jenen Todesfrost.", "tokens": ["Gibt", "die\u00b7sen", "Le\u00b7bens\u00b7w\u00e4rm'", "und", "je\u00b7nen", "To\u00b7des\u00b7frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}