{"dta.poem.8622": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "N\u00e4chtliche Wanderung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist finster, schw\u00fcl und bang,", "tokens": ["Die", "Nacht", "ist", "fins\u00b7ter", ",", "schw\u00fcl", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wind im Walde tost;", "tokens": ["Der", "Wind", "im", "Wal\u00b7de", "tost", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wandre fort die Nacht entlang,", "tokens": ["Ich", "wand\u00b7re", "fort", "die", "Nacht", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und finde keinen Trost.", "tokens": ["Und", "fin\u00b7de", "kei\u00b7nen", "Trost", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und mir zur Seite, engelmild,", "tokens": ["Und", "mir", "zur", "Sei\u00b7te", ",", "en\u00b7gel\u00b7mild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und, ach, so schmerzlich traut,", "tokens": ["Und", ",", "ach", ",", "so", "schmerz\u00b7lich", "traut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zieht mein Geleite hin, das Bild", "tokens": ["Zieht", "mein", "Ge\u00b7lei\u00b7te", "hin", ",", "das", "Bild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von meiner todten Braut.", "tokens": ["Von", "mei\u00b7ner", "tod\u00b7ten", "Braut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr bleiches Antlitz bittet mich,", "tokens": ["Ihr", "blei\u00b7ches", "Ant\u00b7litz", "bit\u00b7tet", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mich ihr s\u00fc\u00dfer Mund", "tokens": ["Was", "mich", "ihr", "s\u00fc\u00b7\u00dfer", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So z\u00e4rtlich bat und feierlich", "tokens": ["So", "z\u00e4rt\u00b7lich", "bat", "und", "fei\u00b7er\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer Sterbestund':", "tokens": ["In", "ih\u00b7rer", "Ster\u00b7be\u00b7stund'", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201ebezwinge fromm die Todeslust,", "tokens": ["\u201e", "be\u00b7zwin\u00b7ge", "fromm", "die", "To\u00b7des\u00b7lust", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edie dir im Auge starrt,", "tokens": ["\u201e", "die", "dir", "im", "Au\u00b7ge", "starrt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ewenn man mich bald von deiner Brust", "tokens": ["\u201e", "wenn", "man", "mich", "bald", "von", "dei\u00b7ner", "Brust"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "PIS", "PRF", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201efortreisset und verscharrt!\u201c", "tokens": ["\u201e", "fort\u00b7reis\u00b7set", "und", "ver\u00b7scharrt", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da unten braust der wilde Bach,", "tokens": ["Da", "un\u00b7ten", "braust", "der", "wil\u00b7de", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fchrt reichen, frischen Tod,", "tokens": ["F\u00fchrt", "rei\u00b7chen", ",", "fri\u00b7schen", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wogen rufen laut mir nach:", "tokens": ["Die", "Wo\u00b7gen", "ru\u00b7fen", "laut", "mir", "nach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ekomm, komm, und trinke Tod!\u201c", "tokens": ["\u201e", "komm", ",", "komm", ",", "und", "trin\u00b7ke", "Tod", "!", "\u201c"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$,", "KON", "VVFIN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das klingt so lieblich wie Musik.", "tokens": ["Das", "klingt", "so", "lieb\u00b7lich", "wie", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird wo ein Paar getraut;", "tokens": ["Wird", "wo", "ein", "Paar", "ge\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch zieht vom Sprunge mich zur\u00fcck", "tokens": ["Doch", "zieht", "vom", "Sprun\u00b7ge", "mich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wort der todten Braut.", "tokens": ["Das", "Wort", "der", "tod\u00b7ten", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Stets finstrer wird der Wolkendrang.", "tokens": ["Stets", "finst\u00b7rer", "wird", "der", "Wol\u00b7ken\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sturm im Walde br\u00fcllt,", "tokens": ["Der", "Sturm", "im", "Wal\u00b7de", "br\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ferne hebt sich Donnerklang,", "tokens": ["Und", "fer\u00b7ne", "hebt", "sich", "Don\u00b7ner\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der immer st\u00e4rker schwillt.", "tokens": ["Der", "im\u00b7mer", "st\u00e4r\u00b7ker", "schwillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O schl\u00e4ngle dich, du Wetterstrahl,", "tokens": ["O", "schl\u00e4ng\u00b7le", "dich", ",", "du", "Wet\u00b7ter\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herab, ein Faden mir,", "tokens": ["Her\u00b7ab", ",", "ein", "Fa\u00b7den", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der aus dem Labyrinth der Qual", "tokens": ["Der", "aus", "dem", "La\u00b7by\u00b7rinth", "der", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinaus mich f\u00fchrt zu ihr!", "tokens": ["Hin\u00b7aus", "mich", "f\u00fchrt", "zu", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}