{"textgrid.poem.37604": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Vaterland", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fest bei\u00df ich mich, mein schwankend Vaterland,", "tokens": ["Fest", "bei\u00df", "ich", "mich", ",", "mein", "schwan\u00b7kend", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PRF", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bei\u00df' ich dich mit allen Z\u00e4hnen,", "tokens": ["Und", "bei\u00df'", "ich", "dich", "mit", "al\u00b7len", "Z\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dir thut's nicht weh, ich mag nicht schrein.", "tokens": ["Dir", "thut's", "nicht", "weh", ",", "ich", "mag", "nicht", "schrein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPER", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sei's Liebeswuth, sei's h\u00e4\u00dflich ohn Verstand,", "tokens": ["Sei's", "Lie\u00b7bes\u00b7wuth", ",", "sei's", "h\u00e4\u00df\u00b7lich", "ohn", "Ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So tief ich einbei\u00df', bist du gerne mein,", "tokens": ["So", "tief", "ich", "ein\u00b7bei\u00df'", ",", "bist", "du", "ger\u00b7ne", "mein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PTKVZ", "$,", "VAFIN", "PPER", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Willst Mutterbrust dem Kinde sein.", "tokens": ["Willst", "Mut\u00b7ter\u00b7brust", "dem", "Kin\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So schwanke denn im Wind, du loser Sand!", "tokens": ["So", "schwan\u00b7ke", "denn", "im", "Wind", ",", "du", "lo\u00b7ser", "Sand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er schwankt, will meine lust'ge Wiege sein,", "tokens": ["Er", "schwankt", ",", "will", "mei\u00b7ne", "lust'\u00b7ge", "Wie\u00b7ge", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VMFIN", "PPOSAT", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Vaterland, und ich bin dein.", "tokens": ["Mein", "Va\u00b7ter\u00b7land", ",", "und", "ich", "bin", "dein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPER", "VAFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Fest bei\u00df ich mich, mein schwankend Vaterland,", "tokens": ["Fest", "bei\u00df", "ich", "mich", ",", "mein", "schwan\u00b7kend", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"