{"dta.poem.9552": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff sch\u00f6ne augen.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr sterne/ deren glantz der monden nicht kan gleichen/", "tokens": ["Ihr", "ster\u00b7ne", "/", "de\u00b7ren", "glantz", "der", "mon\u00b7den", "nicht", "kan", "glei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PRELAT", "NN", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr sonnen/ deren schein die sonne selbst mu\u00df weichen/", "tokens": ["Ihr", "son\u00b7nen", "/", "de\u00b7ren", "schein", "die", "son\u00b7ne", "selbst", "mu\u00df", "wei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PDS", "ADV", "ART", "NN", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr/ die ihr doppelt vor der liebe spiegel stellt/", "tokens": ["Ihr", "/", "die", "ihr", "dop\u00b7pelt", "vor", "der", "lie\u00b7be", "spie\u00b7gel", "stellt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr/ die ihr j", "tokens": ["Ihr", "/", "die", "ihr", "j"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$(", "ART", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Jhr/ die ihr gnade wist und ungunst auszutheilen/", "tokens": ["Ihr", "/", "die", "ihr", "gna\u00b7de", "wist", "und", "un\u00b7gunst", "aus\u00b7zu\u00b7thei\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr/ die ihr beydes k\u00f6nt verletzen und auch heilen/", "tokens": ["Ihr", "/", "die", "ihr", "bey\u00b7des", "k\u00f6nt", "ver\u00b7let\u00b7zen", "und", "auch", "hei\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPER", "PIS", "VMFIN", "VVINF", "KON", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr/ die verzweifflung uns so wohl als hoffnung gebt/", "tokens": ["Ihr", "/", "die", "ver\u00b7zweif\u00b7flung", "uns", "so", "wohl", "als", "hoff\u00b7nung", "gebt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ART", "NN", "PPER", "ADV", "ADV", "KOUS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verursacht da\u00df man stirbt/ und machet da\u00df man lebt.", "tokens": ["Ver\u00b7ur\u00b7sacht", "da\u00df", "man", "stirbt", "/", "und", "ma\u00b7chet", "da\u00df", "man", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PIS", "VVFIN", "$(", "KON", "VVFIN", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jhr sch\u00f6nsten augen seyds/ die ich hier will beschreiben/", "tokens": ["Ihr", "sch\u00f6ns\u00b7ten", "au\u00b7gen", "seyds", "/", "die", "ich", "hier", "will", "be\u00b7schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$(", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nah darff ich nicht bey euch/ weit kan ich auch nicht bleiben.", "tokens": ["Nah", "darff", "ich", "nicht", "bey", "euch", "/", "weit", "kan", "ich", "auch", "nicht", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "$(", "ADJD", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Der tag der ist mir nacht/ wenn ich euch schaue nicht/", "tokens": ["Der", "tag", "der", "ist", "mir", "nacht", "/", "wenn", "ich", "euch", "schau\u00b7e", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "PPER", "NN", "$(", "KOUS", "PPER", "PPER", "VVFIN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Seh ich euch/ werd ich blind/ wei\u00df nicht wie mir geschicht.", "tokens": ["Seh", "ich", "euch", "/", "werd", "ich", "blind", "/", "wei\u00df", "nicht", "wie", "mir", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$(", "VAFIN", "PPER", "ADJD", "$(", "VVFIN", "PTKNEG", "PWAV", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Doch dult ich dieses gern/ und will viel lieber leiden/", "tokens": ["Doch", "dult", "ich", "die\u00b7ses", "gern", "/", "und", "will", "viel", "lie\u00b7ber", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "ADV", "$(", "KON", "VMFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als weit entfernet seyn/ und eure sch\u00f6nheit meiden.", "tokens": ["Als", "weit", "ent\u00b7fer\u00b7net", "seyn", "/", "und", "eu\u00b7re", "sch\u00f6n\u00b7heit", "mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVPP", "VAINF", "$(", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein eintzig blick von euch vergn\u00fcget mich viel mehr/", "tokens": ["Ein", "eint\u00b7zig", "blick", "von", "euch", "ver\u00b7gn\u00fc\u00b7get", "mich", "viel", "mehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als strahlen die von sonn und sternen kommen her.", "tokens": ["Als", "strah\u00b7len", "die", "von", "sonn", "und", "ster\u00b7nen", "kom\u00b7men", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "APPR", "NE", "KON", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}