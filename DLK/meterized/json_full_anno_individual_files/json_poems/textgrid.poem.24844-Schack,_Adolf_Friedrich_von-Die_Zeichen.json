{"textgrid.poem.24844": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Zeichen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O M\u00e4dchen, durch all dein Lachen und Singen", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "durch", "all", "dein", "La\u00b7chen", "und", "Sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPR", "PIAT", "PPOSAT", "NN", "KON", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vernehm' ich ein leises Seufzen oft;", "tokens": ["Ver\u00b7nehm'", "ich", "ein", "lei\u00b7ses", "Seuf\u00b7zen", "oft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hoch klopft dir das Herz, als wollt' es zerspringen,", "tokens": ["Hoch", "klopft", "dir", "das", "Herz", ",", "als", "wollt'", "es", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Von dem, was es f\u00fcrchtet und tr\u00e4umt und hofft.", "tokens": ["Von", "dem", ",", "was", "es", "f\u00fcrch\u00b7tet", "und", "tr\u00e4umt", "und", "hofft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wie Wolken \u00fcber die bl\u00fchenden Matten,", "tokens": ["Wie", "Wol\u00b7ken", "\u00fc\u00b7ber", "die", "bl\u00fc\u00b7hen\u00b7den", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie \u00fcber wogende Saaten der Wind,", "tokens": ["Wie", "\u00fc\u00b7ber", "wo\u00b7gen\u00b7de", "Saa\u00b7ten", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So ziehen rastlos Gedankenschatten", "tokens": ["So", "zie\u00b7hen", "rast\u00b7los", "Ge\u00b7dan\u00b7ken\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ueber dein l\u00e4chelndes Antlitz, Kind!", "tokens": ["Ue\u00b7ber", "dein", "l\u00e4\u00b7cheln\u00b7des", "Ant\u00b7litz", ",", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Die Lippen im wachenden Traume bewegst du,", "tokens": ["Die", "Lip\u00b7pen", "im", "wa\u00b7chen\u00b7den", "Trau\u00b7me", "be\u00b7wegst", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es ist, als pfl\u00f6gst du mit Geistern Gespr\u00e4ch;", "tokens": ["Es", "ist", ",", "als", "pfl\u00f6gst", "du", "mit", "Geis\u00b7tern", "Ge\u00b7spr\u00e4ch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dann pl\u00f6tzlich die Augen zu Boden schl\u00e4gst du,", "tokens": ["Dann", "pl\u00f6tz\u00b7lich", "die", "Au\u00b7gen", "zu", "Bo\u00b7den", "schl\u00e4gst", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und hocherr\u00f6tend eilst du hinweg.", "tokens": ["Und", "ho\u00b7cher\u00b7r\u00f6\u00b7tend", "eilst", "du", "hin\u00b7weg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Wohl hab' ich die Zeichen erkannt; verhehle,", "tokens": ["Wohl", "hab'", "ich", "die", "Zei\u00b7chen", "er\u00b7kannt", ";", "ver\u00b7heh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$.", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Th\u00f6richtes M\u00e4dchen, es l\u00e4nger nicht!", "tokens": ["Th\u00f6\u00b7rich\u00b7tes", "M\u00e4d\u00b7chen", ",", "es", "l\u00e4n\u00b7ger", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJD", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dir flackert im Hauche der Liebe die Seele,", "tokens": ["Dir", "fla\u00b7ckert", "im", "Hau\u00b7che", "der", "Lie\u00b7be", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "