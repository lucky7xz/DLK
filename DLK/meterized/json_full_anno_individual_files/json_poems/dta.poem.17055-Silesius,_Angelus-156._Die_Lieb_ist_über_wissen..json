{"dta.poem.17055": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "156.  Die Lieb ist \u00fcber wissen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mit GOtt vereinigt seyn/ und seinen Ku\u00df genissen/", "tokens": ["Mit", "Gott", "ver\u00b7ei\u00b7nigt", "seyn", "/", "und", "sei\u00b7nen", "Ku\u00df", "ge\u00b7nis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAINF", "$(", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist besser als viel Ding ohn seine Liebe wissen.", "tokens": ["Ist", "bes\u00b7ser", "als", "viel", "Ding", "ohn", "sei\u00b7ne", "Lie\u00b7be", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}