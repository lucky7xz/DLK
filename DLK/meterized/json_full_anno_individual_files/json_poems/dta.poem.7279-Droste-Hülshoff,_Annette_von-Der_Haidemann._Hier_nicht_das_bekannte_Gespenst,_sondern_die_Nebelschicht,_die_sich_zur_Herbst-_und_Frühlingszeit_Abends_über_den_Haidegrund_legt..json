{"dta.poem.7279": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Haidemann.   Hier nicht das bekannte Gespenst, sondern die Nebelschicht, die sich zur  \n Herbst- und Fr\u00fchlingszeit Abends \u00fcber den Haidegrund legt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "\u201egeht, Kinder, nicht zu weit in's Bruch,", "tokens": ["\u201e", "geht", ",", "Kin\u00b7der", ",", "nicht", "zu", "weit", "in's", "Bruch", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "PTKNEG", "PTKA", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sonne sinkt, schon surrt den Flug", "tokens": ["Die", "Son\u00b7ne", "sinkt", ",", "schon", "surrt", "den", "Flug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Biene matter, schlafgehemmt,", "tokens": ["Die", "Bie\u00b7ne", "mat\u00b7ter", ",", "schlaf\u00b7ge\u00b7hemmt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Grunde schwimmt ein blasses Tuch,", "tokens": ["Am", "Grun\u00b7de", "schwimmt", "ein", "blas\u00b7ses", "Tuch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Haidemann k\u00f6mmt! \u2014\u201c", "tokens": ["Der", "Hai\u00b7de\u00b7mann", "k\u00f6mmt", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Die Knaben spielen fort am Raine,", "tokens": ["Die", "Kna\u00b7ben", "spie\u00b7len", "fort", "am", "Rai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie rupfen Gr\u00e4ser, schnellen Steine,", "tokens": ["Sie", "rup\u00b7fen", "Gr\u00e4\u00b7ser", ",", "schnel\u00b7len", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie pl\u00e4tschern in des Teiches Rinne,", "tokens": ["Sie", "pl\u00e4t\u00b7schern", "in", "des", "Tei\u00b7ches", "Rin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erhaschen die Phal\u00e4n' am Ried,", "tokens": ["Er\u00b7ha\u00b7schen", "die", "Pha\u00b7l\u00e4n'", "am", "Ried", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und freu'n sich, wenn die Wasserspinne", "tokens": ["Und", "freu'n", "sich", ",", "wenn", "die", "Was\u00b7ser\u00b7spin\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Langbeinig in die Binsen flieht.", "tokens": ["Lang\u00b7bei\u00b7nig", "in", "die", "Bin\u00b7sen", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201eihr Kinder, legt euch nicht in's Gras, \u2014", "tokens": ["\u201e", "ihr", "Kin\u00b7der", ",", "legt", "euch", "nicht", "in's", "Gras", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seht, wo noch grad' die Biene sa\u00df,", "tokens": ["Seht", ",", "wo", "noch", "grad'", "die", "Bie\u00b7ne", "sa\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wei\u00dfer Rauch die Glocken f\u00fcllt.", "tokens": ["Wie", "wei\u00b7\u00dfer", "Rauch", "die", "Glo\u00b7cken", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Scheu aus dem Busche glotzt der Haas,", "tokens": ["Scheu", "aus", "dem", "Bu\u00b7sche", "glotzt", "der", "Haas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Der Haidemann schwillt! \u2014\u201c", "tokens": ["Der", "Hai\u00b7de\u00b7mann", "schwillt", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Kaum hebt ihr schweres Haupt die Schmehle", "tokens": ["Kaum", "hebt", "ihr", "schwe\u00b7res", "Haupt", "die", "Schmeh\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch aus dem Dunst, in seine H\u00f6hle", "tokens": ["Noch", "aus", "dem", "Dunst", ",", "in", "sei\u00b7ne", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schiebt sich der K\u00e4fer und am Halme", "tokens": ["Schiebt", "sich", "der", "K\u00e4\u00b7fer", "und", "am", "Hal\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "KON", "APPRART", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die tr\u00e4ge Motte h\u00f6her kreucht,", "tokens": ["Die", "tr\u00e4\u00b7ge", "Mot\u00b7te", "h\u00f6\u00b7her", "kreucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sich fl\u00fcchtend vor dem feuchten Qualme,", "tokens": ["Sich", "fl\u00fcch\u00b7tend", "vor", "dem", "feuch\u00b7ten", "Qual\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der unter ihre Fl\u00fcgel steigt.", "tokens": ["Der", "un\u00b7ter", "ih\u00b7re", "Fl\u00fc\u00b7gel", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u201eihr Kinder, haltet euch bei Haus,", "tokens": ["\u201e", "ihr", "Kin\u00b7der", ",", "hal\u00b7tet", "euch", "bei", "Haus", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lauft ja nicht in das Bruch hinaus;", "tokens": ["Lauft", "ja", "nicht", "in", "das", "Bruch", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seht, wie bereits der Dorn ergraut,", "tokens": ["Seht", ",", "wie", "be\u00b7reits", "der", "Dorn", "er\u00b7graut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Drossel \u00e4chzt zum Nest hinaus,", "tokens": ["Die", "Dros\u00b7sel", "\u00e4chzt", "zum", "Nest", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Haidemann braut! \u2014\u201c", "tokens": ["Der", "Hai\u00b7de\u00b7mann", "braut", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Man sieht des Hirten Pfeife glimmen,", "tokens": ["Man", "sieht", "des", "Hir\u00b7ten", "Pfei\u00b7fe", "glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und vor ihm her die Heerde schwimmen,", "tokens": ["Und", "vor", "ihm", "her", "die", "Heer\u00b7de", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Proteus seine Robbenschaaren", "tokens": ["Wie", "Pro\u00b7teus", "sei\u00b7ne", "Rob\u00b7ben\u00b7schaa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heimschwemmt im grauen Ocean.", "tokens": ["Heim\u00b7schwemmt", "im", "grau\u00b7en", "O\u00b7cean", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Am Dach die Schwalben zwitschernd fahren", "tokens": ["Am", "Dach", "die", "Schwal\u00b7ben", "zwit\u00b7schernd", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und melancholisch kr\u00e4ht der Hahn.", "tokens": ["Und", "me\u00b7lan\u00b7cho\u00b7lisch", "kr\u00e4ht", "der", "Hahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "\u201eihr Kinder, bleibt am Hofe dicht,", "tokens": ["\u201e", "ihr", "Kin\u00b7der", ",", "bleibt", "am", "Ho\u00b7fe", "dicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seht, wie die feuchte Nebelschicht", "tokens": ["Seht", ",", "wie", "die", "feuch\u00b7te", "Ne\u00b7bel\u00b7schicht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon an des Pf\u00f6rtchens Klinke reicht;", "tokens": ["Schon", "an", "des", "Pf\u00f6rt\u00b7chens", "Klin\u00b7ke", "reicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Grunde schwimmt ein falsches Licht,", "tokens": ["Am", "Grun\u00b7de", "schwimmt", "ein", "fal\u00b7sches", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Haidemann steigt! \u2014\u201c", "tokens": ["Der", "Hai\u00b7de\u00b7mann", "steigt", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Nun strecken nur der F\u00f6hren Wipfel", "tokens": ["Nun", "stre\u00b7cken", "nur", "der", "F\u00f6h\u00b7ren", "Wip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch aus dem Dunste gr\u00fcne Gipfel,", "tokens": ["Noch", "aus", "dem", "Duns\u00b7te", "gr\u00fc\u00b7ne", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie \u00fcber'n Schnee Wacholderb\u00fcsche;", "tokens": ["Wie", "\u00fc\u00b7ber'n", "Schnee", "Wa\u00b7chol\u00b7der\u00b7b\u00fc\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein leises Brodeln quillt im Moor,", "tokens": ["Ein", "lei\u00b7ses", "Bro\u00b7deln", "quillt", "im", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein schwaches Schrillen, ein Gezische", "tokens": ["Ein", "schwa\u00b7ches", "Schril\u00b7len", ",", "ein", "Ge\u00b7zi\u00b7sche"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dringt aus der Niederung hervor.", "tokens": ["Dringt", "aus", "der", "Nie\u00b7de\u00b7rung", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "\u201eihr Kinder, kommt, kommt schnell herein,", "tokens": ["\u201e", "ihr", "Kin\u00b7der", ",", "kommt", ",", "kommt", "schnell", "her\u00b7ein", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "VVFIN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Irrlicht z\u00fcndet seinen Schein,", "tokens": ["Das", "Irr\u00b7licht", "z\u00fcn\u00b7det", "sei\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kr\u00f6te schwillt, die Schlang im Ried;", "tokens": ["Die", "Kr\u00f6\u00b7te", "schwillt", ",", "die", "Schlang", "im", "Ried", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt ist's unheimlich drau\u00dfen seyn,", "tokens": ["Jetzt", "ist's", "un\u00b7heim\u00b7lich", "drau\u00b7\u00dfen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Haidemann zieht! \u2014\u201c", "tokens": ["Der", "Hai\u00b7de\u00b7mann", "zieht", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Nun sinkt die letzte Nadel, rauchend", "tokens": ["Nun", "sinkt", "die", "letz\u00b7te", "Na\u00b7del", ",", "rau\u00b7chend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zergeht die Fichte, langsam tauchend", "tokens": ["Zer\u00b7geht", "die", "Fich\u00b7te", ",", "lang\u00b7sam", "tau\u00b7chend"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "CARD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Steigt Nebelschemen aus dem Moore,", "tokens": ["Steigt", "Ne\u00b7bel\u00b7sche\u00b7men", "aus", "dem", "Moo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit H\u00fcnenschritten gleitet's fort;", "tokens": ["Mit", "H\u00fc\u00b7nen\u00b7schrit\u00b7ten", "glei\u00b7tet's", "fort", ";"], "to