{"textgrid.poem.36833": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht nach Neapel", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum dieser Erde lieblichstes Schattengr\u00fcn,", "tokens": ["Kaum", "die\u00b7ser", "Er\u00b7de", "lieb\u00b7lichs\u00b7tes", "Schat\u00b7ten\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ariccia's alte Sikulerburg, und kaum", "tokens": ["A\u00b7ric\u00b7cia's", "al\u00b7te", "Si\u00b7ku\u00b7ler\u00b7burg", ",", "und", "kaum"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NE", "$,", "KON", "ADV"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der Cythia Hain, und ihres Spiegels", "tokens": ["Der", "Cyt\u00b7hia", "Hain", ",", "und", "ih\u00b7res", "Spie\u00b7gels"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "NE", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Duftiges Seeblau und Eichendunkel,", "tokens": ["Duf\u00b7ti\u00b7ges", "Seeb\u00b7lau", "und", "Ei\u00b7chen\u00b7dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "O Freund, erreicht' ich, und des Tyrrhenermeers", "tokens": ["O", "Freund", ",", "er\u00b7reicht'", "ich", ",", "und", "des", "Tyr\u00b7rhe\u00b7ner\u00b7meers"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$,", "KON", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Glanzreiche Pracht, und brennend im Abendgold", "tokens": ["Glanz\u00b7rei\u00b7che", "Pracht", ",", "und", "bren\u00b7nend", "im", "A\u00b7bend\u00b7gold"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "APPRART", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Lanuvium's Berg und meines Latium's", "tokens": ["La\u00b7nu\u00b7vi\u00b7um's", "Berg", "und", "mei\u00b7nes", "La\u00b7ti\u00b7um's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Tr\u00fcmmerbes\u00e4'te Campagna schaut' ich,", "tokens": ["Tr\u00fcm\u00b7mer\u00b7be\u00b7s\u00e4'\u00b7te", "Cam\u00b7pag\u00b7na", "schaut'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Und der Erinn'rung freudige Wehmuth rief", "tokens": ["Und", "der", "Er\u00b7inn'\u00b7rung", "freu\u00b7di\u00b7ge", "Weh\u00b7muth", "rief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mir schon lustselige Tage zur\u00fcck, da mir", "tokens": ["Mir", "schon", "lust\u00b7se\u00b7li\u00b7ge", "Ta\u00b7ge", "zu\u00b7r\u00fcck", ",", "da", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADJA", "NN", "PTKVZ", "$,", "KOUS", "PPER"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Im Golf Parthenope's, in Capri's", "tokens": ["Im", "Golf", "Par\u00b7the\u00b7no\u00b7pe's", ",", "in", "Ca\u00b7pri's"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NE", "NE", "$,", "APPR", "NE"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Felsiger Heimath und holder Wildni\u00df", "tokens": ["Fel\u00b7si\u00b7ger", "Hei\u00b7math", "und", "hol\u00b7der", "Wild\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN"], "meter": "+--++-+-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Die goldne Fluth, die lebenverj\u00fcngende,", "tokens": ["Die", "gold\u00b7ne", "Fluth", ",", "die", "le\u00b7ben\u00b7ver\u00b7j\u00fcn\u00b7gen\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus ros'gem Becher Hebe zu schl\u00fcrfen gab,", "tokens": ["Aus", "ros'\u00b7gem", "Be\u00b7cher", "He\u00b7be", "zu", "schl\u00fcr\u00b7fen", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und sieh' zum kaum verlass'nen Thore", "tokens": ["Und", "sieh'", "zum", "kaum", "ver\u00b7lass'\u00b7nen", "Tho\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchret den Trauernden schon der finst're,", "tokens": ["F\u00fch\u00b7ret", "den", "Trau\u00b7ern\u00b7den", "schon", "der", "finst'\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Von keinem Lied' bes\u00e4nftigte Gott zur\u00fcck.", "tokens": ["Von", "kei\u00b7nem", "Lied'", "be\u00b7s\u00e4nf\u00b7tig\u00b7te", "Gott", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Rom! was ist's, das heute so viel des Gifts", "tokens": ["O", "Rom", "!", "was", "ist's", ",", "das", "heu\u00b7te", "so", "viel", "des", "Gifts"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "PWS", "VAFIN", "$,", "PRELS", "ADV", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durch deine L\u00fcfte streut, und t\u00f6dtlich", "tokens": ["Durch", "dei\u00b7ne", "L\u00fcf\u00b7te", "streut", ",", "und", "t\u00f6dt\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00fcgel und Ufer und Thal entathmet?", "tokens": ["H\u00fc\u00b7gel", "und", "U\u00b7fer", "und", "Thal", "en\u00b7tath\u00b7met", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Ist es der Vorwelt dr\u00fcckender Moderhauch,", "tokens": ["Ist", "es", "der", "Vor\u00b7welt", "dr\u00fc\u00b7cken\u00b7der", "Mo\u00b7der\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Des gro\u00dfen Kirchhofs, den ich durchwandere,", "tokens": ["Des", "gro\u00b7\u00dfen", "Kirch\u00b7hofs", ",", "den", "ich", "durch\u00b7wan\u00b7de\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In dessen Denkstein, Grab und Inschrift", "tokens": ["In", "des\u00b7sen", "Denk\u00b7stein", ",", "Grab", "und", "In\u00b7schrift"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einsame Wand'rer und ernste Denker", "tokens": ["Ein\u00b7sa\u00b7me", "Wan\u00b7d'\u00b7rer", "und", "erns\u00b7te", "Den\u00b7ker"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADJA", "NN"], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Die Weltgeschichte lasen; vielleicht das Blut", "tokens": ["Die", "Welt\u00b7ge\u00b7schich\u00b7te", "la\u00b7sen", ";", "viel\u00b7leicht", "das", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das hier gestr\u00f6mt Jahrtausende durch, und tief", "tokens": ["Das", "hier", "ge\u00b7str\u00f6mt", "Jahr\u00b7tau\u00b7sen\u00b7de", "durch", ",", "und", "tief"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ADV", "VVPP", "NN", "PTKVZ", "$,", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Befleckt die Erde, welch ein Tiber", "tokens": ["Be\u00b7fleckt", "die", "Er\u00b7de", ",", "welch", "ein", "Ti\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fa\u00dft' es in seines Gestades Gr\u00e4nze?", "tokens": ["Fa\u00dft'", "es", "in", "sei\u00b7nes", "Ge\u00b7sta\u00b7des", "Gr\u00e4n\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Nicht wei\u00df ich's, Freund, doch sei dir bekannt: Zwar pflegt", "tokens": ["Nicht", "wei\u00df", "ich's", ",", "Freund", ",", "doch", "sei", "dir", "be\u00b7kannt", ":", "Zwar", "pflegt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PIS", "$,", "NN", "$,", "ADV", "VAFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN"], "meter": "-+----+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Mich treue Sorgfalt: Amor, mein steter Freund,", "tokens": ["Mich", "treu\u00b7e", "Sorg\u00b7falt", ":", "A\u00b7mor", ",", "mein", "ste\u00b7ter", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn l\u00e4ngst auch mit gesenktem Fl\u00fcgel,", "tokens": ["Wenn", "l\u00e4ngst", "auch", "mit", "ge\u00b7senk\u00b7tem", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist er doch immer noch mein Begleiter,", "tokens": ["Ist", "er", "doch", "im\u00b7mer", "noch", "mein", "Be\u00b7glei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Und k\u00fcrzt der Stunden Kummer und Ungeduld,", "tokens": ["Und", "k\u00fcrzt", "der", "Stun\u00b7den", "Kum\u00b7mer", "und", "Un\u00b7ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Traum und Schlaf einwiegend das Herz, wenn nicht", "tokens": ["In", "Traum", "und", "Schlaf", "ein\u00b7wie\u00b7gend", "das", "Herz", ",", "wenn", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "ART", "NN", "$,", "KOUS", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Diotimas Lehre, doch mit", "tokens": ["Mit", "Dio\u00b7ti\u00b7mas", "Leh\u00b7re", ",", "doch", "mit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "ADV", "APPR"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Raffael's Freuden und Benvenuto's.", "tokens": ["Raf\u00b7fael's", "Freu\u00b7den", "und", "Ben\u00b7ve\u00b7nu\u00b7to'", "s."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NE", "NN", "KON", "NE", "NE"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Wohl r\u00fchm' ich de\u00df mich! Aber in Rom d\u00fcnkt mir,", "tokens": ["Wohl", "r\u00fchm'", "ich", "de\u00df", "mich", "!", "A\u00b7ber", "in", "Rom", "d\u00fcnkt", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "PPER", "$.", "KON", "APPR", "NE", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als ob im Grab ich schlummr', und im Zaubergolf", "tokens": ["Als", "ob", "im", "Grab", "ich", "schlumm\u00b7r'", ",", "und", "im", "Zau\u00b7ber\u00b7golf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Neapels Psyche bald zur reinen", "tokens": ["Nea\u00b7pels", "Psy\u00b7che", "bald", "zur", "rei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADV", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nheit Elysiums auferst\u00fcnde.", "tokens": ["Sch\u00f6n\u00b7heit", "E\u00b7ly\u00b7si\u00b7ums", "auf\u00b7er\u00b7st\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Kaum dieser Erde lieblichstes Schattengr\u00fcn,", "tokens": ["Kaum", "die\u00b7ser", "Er\u00b7de", "lieb\u00b7lichs\u00b7tes", "Schat\u00b7ten\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ariccia's alte Sikulerburg, und kaum", "tokens": ["A\u00b7ric\u00b7cia's", "al\u00b7te", "Si\u00b7ku\u00b7ler\u00b7burg", ",", "und", "kaum"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NE", "$,", "KON", "ADV"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der Cythia Hain, und ihres Spiegels", "tokens": ["Der", "Cyt\u00b7hia", "Hain", ",", "und", "ih\u00b7res", "Spie\u00b7gels"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "NE", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Duftiges Seeblau und Eichendunkel,", "tokens": ["Duf\u00b7ti\u00b7ges", "Seeb\u00b7lau", "und", "Ei\u00b7chen\u00b7dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "O Freund, erreicht' ich, und des Tyrrhenermeers", "tokens": ["O", "Freund", ",", "er\u00b7reicht'", "ich", ",", "und", "des", "Tyr\u00b7rhe\u00b7ner\u00b7meers"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "$,", "KON", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Glanzreiche Pracht, und brennend im Abendgold", "tokens": ["Glanz\u00b7rei\u00b7che", "Pracht", ",", "und", "bren\u00b7nend", "im", "A\u00b7bend\u00b7gold"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "ADJD", "APPRART", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Lanuvium's Berg und meines Latium's", "tokens": ["La\u00b7nu\u00b7vi\u00b7um's", "Berg", "und", "mei\u00b7nes", "La\u00b7ti\u00b7um's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Tr\u00fcmmerbes\u00e4'te Campagna schaut' ich,", "tokens": ["Tr\u00fcm\u00b7mer\u00b7be\u00b7s\u00e4'\u00b7te", "Cam\u00b7pag\u00b7na", "schaut'", "ich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PPER", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.13": {"line.1": {"text": "Und der Erinn'rung freudige Wehmuth rief", "tokens": ["Und", "der", "Er\u00b7inn'\u00b7rung", "freu\u00b7di\u00b7ge", "Weh\u00b7muth", "rief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "