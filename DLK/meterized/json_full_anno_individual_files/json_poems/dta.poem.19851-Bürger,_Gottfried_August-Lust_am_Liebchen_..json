{"dta.poem.19851": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Lust am Liebchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Wie selig, wer sein Liebchen hat,               ", "tokens": ["Wie", "se\u00b7lig", ",", "wer", "sein", "Lieb\u00b7chen", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie selig lebt der Man!", "tokens": ["Wie", "se\u00b7lig", "lebt", "der", "Man", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "PIS", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "In Friedrichs oder Ludwigs Stadt", "tokens": ["In", "Fried\u00b7richs", "o\u00b7der", "Lud\u00b7wigs", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist keiner besser dran.", "tokens": ["Ist", "kei\u00b7ner", "bes\u00b7ser", "dran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er achtet\u2019s nicht, was Hof und Stadt", "tokens": ["Er", "ach\u00b7tet's", "nicht", ",", "was", "Hof", "und", "Stadt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Daf\u00fcr ihn bieten kan;", "tokens": ["Da\u00b7f\u00fcr", "ihn", "bie\u00b7ten", "kan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn er keinen Kreuzer hat,", "tokens": ["Und", "wenn", "er", "kei\u00b7nen", "Kreu\u00b7zer", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "D\u00fcnkt er sich Kr\u00f6sus dann.", "tokens": ["D\u00fcnkt", "er", "sich", "Kr\u00f6\u00b7sus", "dann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "NE", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Die Welt mag laufen, oder siehn;", "tokens": ["Die", "Welt", "mag", "lau\u00b7fen", ",", "o\u00b7der", "siehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,", "KON", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mag rollen um und um;", "tokens": ["Mag", "rol\u00b7len", "um", "und", "um", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "KON", "KOUI", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alles auf dem Kopfe gehn!", "tokens": ["Und", "al\u00b7les", "auf", "dem", "Kop\u00b7fe", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was k\u00fcmmert er sich drum?", "tokens": ["Was", "k\u00fcm\u00b7mert", "er", "sich", "drum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hui! ist sein Wort zu Strom und Wind,", "tokens": ["Hui", "!", "ist", "sein", "Wort", "zu", "Strom", "und", "Wind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "$.", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer macht aus euch sich was?", "tokens": ["Wer", "macht", "aus", "euch", "sich", "was", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPER", "PRF", "PWS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nichts mehr, als wehen kan der Wind,", "tokens": ["Nichts", "mehr", ",", "als", "we\u00b7hen", "kan", "der", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "$,", "KOUS", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Regen macht nur nas.", "tokens": ["Und", "Re\u00b7gen", "macht", "nur", "nas", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gram, Sorg\u2019 und Grille sind ihm Spot;", "tokens": ["Gram", ",", "Sor\u00b7g'", "und", "Gril\u00b7le", "sind", "ihm", "Spot", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Er f\u00fchlt sich frei und froh;", "tokens": ["Er", "f\u00fchlt", "sich", "frei", "und", "froh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und kr\u00e4ht, vergn\u00fcgt in seinem Gott,", "tokens": ["Und", "kr\u00e4ht", ",", "ver\u00b7gn\u00fcgt", "in", "sei\u00b7nem", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dulci Jubilo.", "tokens": ["In", "dul\u00b7ci", "Ju\u00b7bi\u00b7lo", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Durch seine Adern kreiset frisch", "tokens": ["Durch", "sei\u00b7ne", "A\u00b7dern", "krei\u00b7set", "frisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ungehemt sein Blut.", "tokens": ["Und", "un\u00b7ge\u00b7hemt", "sein", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gesunder ist er, wie ein Fisch,", "tokens": ["Ge\u00b7sun\u00b7der", "ist", "er", ",", "wie", "ein", "Fisch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seiner klaren Flut.", "tokens": ["In", "sei\u00b7ner", "kla\u00b7ren", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ihm schmekt sein Mal; er schlummert s\u00fcs,", "tokens": ["Ihm", "schmekt", "sein", "Mal", ";", "er", "schlum\u00b7mert", "s\u00fcs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei federleichtem Sin,", "tokens": ["Bei", "fe\u00b7der\u00b7leich\u00b7tem", "Sin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und tr\u00e4umt sich in ein Paradies", "tokens": ["Und", "tr\u00e4umt", "sich", "in", "ein", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seiner Eva hin.", "tokens": ["Mit", "sei\u00b7ner", "E\u00b7va", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "In G\u00f6tterfreuden schwimt der Man,", "tokens": ["In", "G\u00f6t\u00b7ter\u00b7freu\u00b7den", "schwimt", "der", "Man", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die kein Gedanke mist,", "tokens": ["Die", "kein", "Ge\u00b7dan\u00b7ke", "mist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der singen oder sagen kan,", "tokens": ["Der", "sin\u00b7gen", "o\u00b7der", "sa\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihn sein Liebchen k\u00fcst. \u2014", "tokens": ["Da\u00df", "ihn", "sein", "Lieb\u00b7chen", "k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch ach! was sing\u2019 ich in den Wind,", "tokens": ["Doch", "ach", "!", "was", "sing'", "ich", "in", "den", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und habe selber keins?", "tokens": ["Und", "ha\u00b7be", "sel\u00b7ber", "keins", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PIAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Evchen, Evchen, kom geschwind,", "tokens": ["O", "Ev\u00b7chen", ",", "Ev\u00b7chen", ",", "kom", "ge\u00b7schwind", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O kom und werde meins!", "tokens": ["O", "kom", "und", "wer\u00b7de", "meins", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VAFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}