ord", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbnicht ohne Flecken, ohne Schuld", "tokens": ["\u00bb", "nicht", "oh\u00b7ne", "Fle\u00b7cken", ",", "oh\u00b7ne", "Schuld"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PTKNEG", "APPR", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entrinn' ich diesem Zauberkreise!\u00ab \u2013", "tokens": ["Ent\u00b7rinn'", "ich", "die\u00b7sem", "Zau\u00b7ber\u00b7krei\u00b7se", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Da frommt die Weisheit nicht der Welt,", "tokens": ["Da", "frommt", "die", "Weis\u00b7heit", "nicht", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Gr\u00fcbeln und kein klug Bedenken.", "tokens": ["Kein", "Gr\u00fc\u00b7beln", "und", "kein", "klug", "Be\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Bogen rasch der Pfeil geschnellt!", "tokens": ["Vom", "Bo\u00b7gen", "rasch", "der", "Pfeil", "ge\u00b7schnellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hand des Ew'gen wird ihn lenken.", "tokens": ["Die", "Hand", "des", "Ew'\u00b7gen", "wird", "ihn", "len\u