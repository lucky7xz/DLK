{"textgrid.poem.65048": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist nicht schwer der Pflicht den Zoll", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist nicht schwer der Pflicht den Zoll", "tokens": ["Es", "ist", "nicht", "schwer", "der", "Pflicht", "den", "Zoll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des eignen Gl\u00fcckes darzubringen,", "tokens": ["Des", "eig\u00b7nen", "Gl\u00fc\u00b7ckes", "dar\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch gr\u00e4\u00dflich ist's und schauervoll", "tokens": ["Doch", "gr\u00e4\u00df\u00b7lich", "ist's", "und", "schau\u00b7er\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sich die Pflichten wirr verschlingen.", "tokens": ["Wenn", "sich", "die", "Pflich\u00b7ten", "wirr", "ver\u00b7schlin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn, was die eine von dir heischt,", "tokens": ["Wenn", ",", "was", "die", "ei\u00b7ne", "von", "dir", "heischt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ART", "ART", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verrath und Frevel an der zweiten;", "tokens": ["Ver\u00b7rath", "und", "Fre\u00b7vel", "an", "der", "zwei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn dein verzagend Herz zerfleischt", "tokens": ["Wenn", "dein", "ver\u00b7za\u00b7gend", "Herz", "zer\u00b7fleischt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "VVPP", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihrem grimmen sich Bestreiten.", "tokens": ["Von", "ih\u00b7rem", "grim\u00b7men", "sich", "Be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "PRF", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn du bedr\u00e4ngt, umstrickt, gequ\u00e4lt,", "tokens": ["Wenn", "du", "be\u00b7dr\u00e4ngt", ",", "um\u00b7strickt", ",", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht mehr vermagst zu unterscheiden,", "tokens": ["Nicht", "mehr", "ver\u00b7magst", "zu", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob gleichen Anspruchs Recht sie st\u00e4hlt,", "tokens": ["Ob", "glei\u00b7chen", "An\u00b7spruchs", "Recht", "sie", "st\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob eine heiliger von beiden! \u2013", "tokens": ["Ob", "ei\u00b7ne", "hei\u00b7li\u00b7ger", "von", "bei\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "ADJA", "APPR", "PIAT", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es scheint der Boden, der dich tr\u00e4gt", "tokens": ["Es", "scheint", "der", "Bo\u00b7den", ",", "der", "dich", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir unter deinem Tritt zu wanken,", "tokens": ["Dir", "un\u00b7ter", "dei\u00b7nem", "Tritt", "zu", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu tiefst in deine Seele schl\u00e4gt", "tokens": ["Zu", "tiefst", "in", "dei\u00b7ne", "See\u00b7le", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zweifel seine Tigerpranken.", "tokens": ["Der", "Zwei\u00b7fel", "sei\u00b7ne", "Ti\u00b7ger\u00b7pran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und durch den schaurigen Tumult", "tokens": ["Und", "durch", "den", "schau\u00b7ri\u00b7gen", "Tu\u00b7mult"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufst\u00f6hnt dein Inn'res, schmerzlich leise:", "tokens": ["Auf\u00b7st\u00f6hnt", "dein", "Inn'\u00b7res", ",", "schmerz\u00b7lich", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbnicht ohne Flecken, ohne Schuld", "tokens": ["\u00bb", "nicht", "oh\u00b7ne", "Fle\u00b7cken", ",", "oh\u00b7ne", "Schuld"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PTKNEG", "APPR", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entrinn' ich diesem Zauberkreise!\u00ab \u2013", "tokens": ["Ent\u00b7rinn'", "ich", "die\u00b7sem", "Zau\u00b7ber\u00b7krei\u00b7se", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da frommt die Weisheit nicht der Welt,", "tokens": ["Da", "frommt", "die", "Weis\u00b7heit", "nicht", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Gr\u00fcbeln und kein klug Bedenken.", "tokens": ["Kein", "Gr\u00fc\u00b7beln", "und", "kein", "klug", "Be\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vom Bogen rasch der Pfeil geschnellt!", "tokens": ["Vom", "Bo\u00b7gen", "rasch", "der", "Pfeil", "ge\u00b7schnellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hand des Ew'gen wird ihn lenken.", "tokens": ["Die", "Hand", "des", "Ew'\u00b7gen", "wird", "ihn", "len\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Anstatt des Zweifels gift'gen Keim", "tokens": ["An\u00b7statt", "des", "Zwei\u00b7fels", "gift'\u00b7gen", "Keim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dir zu pflegen und zu dulden,", "tokens": ["In", "dir", "zu", "pfle\u00b7gen", "und", "zu", "dul\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stell' dein Beginnen Gott anheim,", "tokens": ["Stell'", "dein", "Be\u00b7gin\u00b7nen", "Gott", "an\u00b7heim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vertrau' den Ausgang seinen Hulden!", "tokens": ["Ver\u00b7trau'", "den", "Aus\u00b7gang", "sei\u00b7nen", "Hul\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mag auch die Welt, vom Schein beirrt,", "tokens": ["Mag", "auch", "die", "Welt", ",", "vom", "Schein", "be\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$,", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Schuldigen dich beigesellen,", "tokens": ["Den", "Schul\u00b7di\u00b7gen", "dich", "bei\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Unrecht, das geschah, er wird", "tokens": ["Das", "Un\u00b7recht", ",", "das", "ge\u00b7schah", ",", "er", "wird"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es nicht auf deine Rechnung stellen.", "tokens": ["Es", "nicht", "auf", "dei\u00b7ne", "Rech\u00b7nung", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dann l\u00f6set sich der Widerspruch,", "tokens": ["Dann", "l\u00f6\u00b7set", "sich", "der", "Wi\u00b7der\u00b7spruch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Geist erquicket Sabbathstille,", "tokens": ["Den", "Geist", "er\u00b7quic\u00b7ket", "Sab\u00b7bath\u00b7stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn \u00fcber allem Schicksalsfluch", "tokens": ["Denn", "\u00fc\u00b7ber", "al\u00b7lem", "Schick\u00b7sals\u00b7fluch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Steht reine Kraft und reiner Wille!", "tokens": ["Steht", "rei\u00b7ne", "Kraft", "und", "rei\u00b7ner", "Wil\u00b7le", "!"], "toke