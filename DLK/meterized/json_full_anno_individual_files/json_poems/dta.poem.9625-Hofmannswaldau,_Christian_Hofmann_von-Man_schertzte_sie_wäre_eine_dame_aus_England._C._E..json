{"dta.poem.9625": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Man schertzte/ sie w\u00e4re eine dame  \n aus England.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt/ Celinde sey von Englischem gebl\u00fcte;", "tokens": ["Man", "sagt", "/", "Ce\u00b7lin\u00b7de", "sey", "von", "Eng\u00b7li\u00b7schem", "ge\u00b7bl\u00fc\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "NN", "VAFIN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich l\u00e4ugne warlich nicht/ was aller welt bekandt/", "tokens": ["Ich", "l\u00e4ug\u00b7ne", "war\u00b7lich", "nicht", "/", "was", "al\u00b7ler", "welt", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$(", "PWS", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es giebts uns sattsam kund ihr englischer verstand/", "tokens": ["Es", "giebts", "uns", "satt\u00b7sam", "kund", "ihr", "eng\u00b7li\u00b7scher", "ver\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr englisch wesen und ihr himmlisches gem\u00fcthe.", "tokens": ["Ihr", "eng\u00b7lisch", "we\u00b7sen", "und", "ihr", "himm\u00b7li\u00b7sches", "ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Noch mehr: die anmuth/ die in ihren mund gepr\u00e4get/", "tokens": ["Noch", "mehr", ":", "die", "an\u00b7muth", "/", "die", "in", "ih\u00b7ren", "mund", "ge\u00b7pr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ART", "NN", "$(", "ART", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sch\u00f6ne \u00e4hnligkeit/ die sie mit engeln f\u00fchrt/", "tokens": ["Die", "sch\u00f6\u00b7ne", "\u00e4hn\u00b7lig\u00b7keit", "/", "die", "sie", "mit", "en\u00b7geln", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Macht/ da\u00df vor andern ihr ein solches wort geb\u00fchrt/", "tokens": ["Macht", "/", "da\u00df", "vor", "an\u00b7dern", "ihr", "ein", "sol\u00b7ches", "wort", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "APPR", "PIS", "PPOSAT", "ART", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das nichts/ denn liebligkeit in seinen sylben tr\u00e4get.", "tokens": ["Das", "nichts", "/", "denn", "lieb\u00b7lig\u00b7keit", "in", "sei\u00b7nen", "syl\u00b7ben", "tr\u00e4\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$(", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Celinde/ weil du nun ein engel bist zu nennen/", "tokens": ["Ce\u00b7lin\u00b7de", "/", "weil", "du", "nun", "ein", "en\u00b7gel", "bist", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil nichts/ als englisches aus deinen augen blitzt;", "tokens": ["Weil", "nichts", "/", "als", "eng\u00b7li\u00b7sches", "aus", "dei\u00b7nen", "au\u00b7gen", "blitzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$(", "KOUS", "ADJA", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So la\u00df dann deine brust in liebe seyn erhitzt:", "tokens": ["So", "la\u00df", "dann", "dei\u00b7ne", "brust", "in", "lie\u00b7be", "seyn", "er\u00b7hitzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn liebe l\u00e4sset sich von engeln niemahls trennen.", "tokens": ["Denn", "lie\u00b7be", "l\u00e4s\u00b7set", "sich", "von", "en\u00b7geln", "nie\u00b7mahls", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PRF", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ja wilt du engel seyn/ so mustu dich bem\u00fchen/", "tokens": ["Ja", "wilt", "du", "en\u00b7gel", "seyn", "/", "so", "mus\u00b7tu", "dich", "be\u00b7m\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VMFIN", "PPER", "ADJD", "VAINF", "$(", "ADV", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df deine gegenwart mir \u00f6ffters sey gemein.", "tokens": ["Da\u00df", "dei\u00b7ne", "ge\u00b7gen\u00b7wart", "mir", "\u00f6ff\u00b7ters", "sey", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du weist/ da\u00df engel doch gern um die menschen seyn;", "tokens": ["Du", "weist", "/", "da\u00df", "en\u00b7gel", "doch", "gern", "um", "die", "men\u00b7schen", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "NE", "ADV", "ADV", "APPR", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie kanstu denn mit recht dein auge mir entziehen?", "tokens": ["Wie", "kans\u00b7tu", "denn", "mit", "recht", "dein", "au\u00b7ge", "mir", "ent\u00b7zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "APPR", "ADJD", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}