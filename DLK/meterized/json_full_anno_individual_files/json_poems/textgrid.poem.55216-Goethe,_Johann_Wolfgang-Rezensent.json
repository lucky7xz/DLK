{"textgrid.poem.55216": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Rezensent", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da hatt ich einen Kerl zu Gast,", "tokens": ["Da", "hatt", "ich", "ei\u00b7nen", "Kerl", "zu", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er war mir eben nicht zur Last;", "tokens": ["Er", "war", "mir", "e\u00b7ben", "nicht", "zur", "Last", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hatt just mein gew\u00f6hnlich Essen,", "tokens": ["Ich", "hatt", "just", "mein", "ge\u00b7w\u00f6hn\u00b7lich", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat sich der Kerl pumpsatt gefressen,", "tokens": ["Hat", "sich", "der", "Kerl", "pump\u00b7satt", "ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Nachtisch, was ich gespeichert hatt.", "tokens": ["Zum", "Nach\u00b7tisch", ",", "was", "ich", "ge\u00b7spei\u00b7chert", "hatt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und kaum ist mir der Kerl so satt,", "tokens": ["Und", "kaum", "ist", "mir", "der", "Kerl", "so", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Tut ihn der Teufel zum Nachbar f\u00fchren,", "tokens": ["Tut", "ihn", "der", "Teu\u00b7fel", "zum", "Nach\u00b7bar", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "++-+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "\u00dcber mein Essen zu r\u00e4sonieren:", "tokens": ["\u00dc\u00b7ber", "mein", "Es\u00b7sen", "zu", "r\u00e4\u00b7so\u00b7nie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "\u00bbdie Supp h\u00e4tt k\u00f6nnen gew\u00fcrzter sein,", "tokens": ["\u00bb", "die", "Supp", "h\u00e4tt", "k\u00f6n\u00b7nen", "ge\u00b7w\u00fcrz\u00b7ter", "sein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Der Braten brauner, firner der Wein.\u00ab", "tokens": ["Der", "Bra\u00b7ten", "brau\u00b7ner", ",", "fir\u00b7ner", "der", "Wein", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "ADJA", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Der Tausendsakerment!", "tokens": ["Der", "Tau\u00b7send\u00b7sa\u00b7ker\u00b7ment", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Schlagt ihn tot, den Hund! Es ist ein Rezensent.", "tokens": ["Schlagt", "ihn", "tot", ",", "den", "Hund", "!", "Es", "ist", "ein", "Re\u00b7zen\u00b7sent", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Da hatt ich einen Kerl zu Gast,", "tokens": ["Da", "hatt", "ich", "ei\u00b7nen", "Kerl", "zu", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er war mir eben nicht zur Last;", "tokens": ["Er", "war", "mir", "e\u00b7ben", "nicht", "zur", "Last", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hatt just mein gew\u00f6hnlich Essen,", "tokens": ["Ich", "hatt", "just", "mein", "ge\u00b7w\u00f6hn\u00b7lich", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat sich der Kerl pumpsatt gefressen,", "tokens": ["Hat", "sich", "der", "Kerl", "pump\u00b7satt", "ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Nachtisch, was ich gespeichert hatt.", "tokens": ["Zum", "Nach\u00b7tisch", ",", "was", "ich", "ge\u00b7spei\