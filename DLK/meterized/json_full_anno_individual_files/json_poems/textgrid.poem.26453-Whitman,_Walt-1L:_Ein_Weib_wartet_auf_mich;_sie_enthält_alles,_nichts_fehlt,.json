{"textgrid.poem.26453": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Weib wartet auf mich; sie enth\u00e4lt alles, nichts fehlt,", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Weib wartet auf mich; sie enth\u00e4lt alles, nichts fehlt,", "tokens": ["Ein", "Weib", "war\u00b7tet", "auf", "mich", ";", "sie", "ent\u00b7h\u00e4lt", "al\u00b7les", ",", "nichts", "fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "PIS", "$,", "PIS", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Doch fehlte alles, h\u00e4tte das Geschlecht und die Befruchtung des rechten Mannes gefehlt.", "tokens": ["Doch", "fehl\u00b7te", "al\u00b7les", ",", "h\u00e4t\u00b7te", "das", "Ge\u00b7schlecht", "und", "die", "Be\u00b7fruch\u00b7tung", "des", "rech\u00b7ten", "Man\u00b7nes", "ge\u00b7fehlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "VAFIN", "ART", "NN", "KON", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+--+-+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Alles enth\u00e4lt das Geschlecht,", "tokens": ["Al\u00b7les", "ent\u00b7h\u00e4lt", "das", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "K\u00f6rper, Seele, Bedeutung, Beweis, Reinheit, Feinheit, Endergebnis, Weiterverbreitung,", "tokens": ["K\u00f6r\u00b7per", ",", "See\u00b7le", ",", "Be\u00b7deu\u00b7tung", ","