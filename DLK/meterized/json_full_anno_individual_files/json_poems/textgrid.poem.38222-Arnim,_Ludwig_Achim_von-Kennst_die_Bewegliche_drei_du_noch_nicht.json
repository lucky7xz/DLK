{"textgrid.poem.38222": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Kennst die Bewegliche drei du noch nicht", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die 4 heilige 3 K\u00f6nig mit ihrem Steara,", "tokens": ["Die", "4", "hei\u00b7li\u00b7ge", "3", "K\u00f6\u00b7nig", "mit", "ih\u00b7rem", "Stea\u00b7ra", ","], "token_info": ["word", "number", "word", "number", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "CARD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Casper, der Melchar, der Baltes, der Beara,", "tokens": ["Der", "Cas\u00b7per", ",", "der", "Melc\u00b7har", ",", "der", "Bal\u00b7tes", ",", "der", "Be\u00b7a\u00b7ra", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NE", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie seaga de' nagelnuia Steara,", "tokens": ["Sie", "sea\u00b7ga", "de'", "na\u00b7gel\u00b7nu\u00b7ia", "Stea\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Potz Blitz! 's wird g'wi\u00df was Nuis draus weara.", "tokens": ["Potz", "Blitz", "!", "'s", "wird", "g'\u00b7wi\u00df", "was", "Nu\u00b7is", "draus", "wea\u00b7ra", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VAFIN", "ADV", "PWS", "NE", "PAV", "PTKVZ", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sie stiefla, sie waidla, sie f\u00fclla de Bauch,", "tokens": ["Sie", "stief\u00b7la", ",", "sie", "waid\u00b7la", ",", "sie", "f\u00fcl\u00b7la", "de", "Bauch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "NE", "$,", "PPER", "FM", "FM", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Und springa, wie d' Schelma, zum St\u00e4dtle hinaus.", "tokens": ["Und", "sprin\u00b7ga", ",", "wie", "d'", "Schel\u00b7ma", ",", "zum", "St\u00e4dt\u00b7le", "hin\u00b7aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NE", "NE", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und do sie sain kuma f\u00fcrs Herodes sei Th\u00fcr,", "tokens": ["Und", "do", "sie", "sain", "ku\u00b7ma", "f\u00fcrs", "He\u00b7ro\u00b7des", "sei", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Herodes der K\u00f6nig trat selbsta herf\u00fcr.", "tokens": ["He\u00b7ro\u00b7des", "der", "K\u00f6\u00b7nig", "trat", "selbs\u00b7ta", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Ey, wo k\u00f6mmt ihr her in so schneller Uyl?", "tokens": ["Ey", ",", "wo", "k\u00f6mmt", "ihr", "her", "in", "so", "schnel\u00b7ler", "U\u00b7yl", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sizt any aufs B\u00e4nkli, und g'ruhet a Wuyl.", "tokens": ["Sizt", "a\u00b7ny", "aufs", "B\u00e4nk\u00b7li", ",", "und", "g'\u00b7ru\u00b7het", "a", "Wuyl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$,", "KON", "VVFIN", "NE", "NE", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Mie k\u00f6nna nit gruahga, mie han nit de Wuyl.", "tokens": ["Mie", "k\u00f6n\u00b7na", "nit", "gru\u00b7ah\u00b7ga", ",", "mie", "han", "nit", "de", "Wuyl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PPER", "VAFIN", "PTKNEG", "NE", "NE", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Mie m\u00fcassa h\u00fcnt noch f\u00fcnfhalba M\u00fcyl.", "tokens": ["Mie", "m\u00fcas\u00b7sa", "h\u00fcnt", "noch", "f\u00fcnf\u00b7hal\u00b7ba", "M\u00fcyl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Ey woruma k\u00f6nnt' ir nit g'ruahga, es thut jo nit Nauth,", "tokens": ["Ey", "wo\u00b7ru\u00b7ma", "k\u00f6nnt'", "ir", "nit", "g'\u00b7ruah\u00b7ga", ",", "es", "thut", "jo", "nit", "Nauth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "PPER", "PTKNEG", "NE", "$,", "PPER", "VVFIN", "NE", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "I will \u00fcch vor gea a K\u00e4\u00df und a Brout.", "tokens": ["I", "will", "\u00fcch", "vor", "gea", "a", "K\u00e4\u00df", "und", "a", "Brout", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "VMFIN", "ADV", "APPR", "NE", "NE", "NE", "KON", "NE", "NE", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.15": {"text": "Mer m\u00f6aga kui K\u00e4s, mer m\u00f6aga kui Brout,", "tokens": ["Mer", "m\u00f6a\u00b7ga", "kui", "K\u00e4s", ",", "mer", "m\u00f6a\u00b7ga", "kui", "Brout", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "----+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Mer m\u00fcassa gau gea, 's thut werli gau Nauth.", "tokens": ["Mer", "m\u00fcas\u00b7sa", "gau", "gea", ",", "'s", "thut", "wer\u00b7li", "gau", "Nauth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "PPER", "VVFIN", "VAFIN", "NE", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ey m\u00f6agat er kui K\u00e4s, so fr\u00e4sset e Dreck,", "tokens": ["Ey", "m\u00f6a\u00b7gat", "er", "kui", "K\u00e4s", ",", "so", "fr\u00e4s\u00b7set", "e", "Dreck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADV", "VVFIN", "NE", "NE", "$,"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "Un sch\u00e4rt i ins Teufels paar Daza a weg.", "tokens": ["Un", "sch\u00e4rt", "i", "ins", "Teu\u00b7fels", "paar", "Da\u00b7za", "a", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "APPRART", "NN", "PIAT", "NN", "NE", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "Und do mer sin k\u00f6mma \u00fcbers St\u00e4dle hinaus,", "tokens": ["Und", "do", "mer", "sin", "k\u00f6m\u00b7ma", "\u00fc\u00b7bers", "St\u00e4d\u00b7le", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Denka mer, blos es der Herodes da Hobel fein aus.", "tokens": ["Den\u00b7ka", "mer", ",", "blos", "es", "der", "He\u00b7ro\u00b7des", "da", "Ho\u00b7bel", "fein", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADV", "PPER", "ART", "NN", "ADV", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Und do mer sin komma ge Betlahai,", "tokens": ["Und", "do", "mer", "sin", "kom\u00b7ma", "ge", "Bet\u00b7la\u00b7hai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "So finda mirs Kindli \u00e4 Mueters allai.", "tokens": ["So", "fin\u00b7da", "mirs", "Kind\u00b7li", "\u00e4", "Mu\u00b7e\u00b7ters", "al\u00b7lai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Und do mer im han brunge Butter, Nu\u00df, und a Milach,", "tokens": ["Und", "do", "mer", "im", "han", "brun\u00b7ge", "But\u00b7ter", ",", "Nu\u00df", ",", "und", "a", "Mi\u00b7lach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPRART", "VAFIN", "ADJA", "NN", "$,", "NN", "$,", "KON", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.24": {"text": "Hats Kindli klo bizli druf aini gschilacht.", "tokens": ["Hats", "Kind\u00b7li", "klo", "biz\u00b7li", "druf", "ai\u00b7ni", "gschi\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Sankt Joseph nahm waidli die Wiege-Schnuar,", "tokens": ["Sankt", "Jo\u00b7se\u00b7ph", "nahm", "waid\u00b7li", "die", "Wie\u00b7ge\u00b7Sch\u00b7nu\u00b7ar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "VVFIN", "NE", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Und macht go dem Kindli a Gugelfuar.", "tokens": ["Und", "macht", "go", "dem", "Kind\u00b7li", "a", "Gu\u00b7gel\u00b7fu\u00b7ar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ART", "NN", "NE", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.27": {"text": "Do stund es a Engela hinter der Th\u00fcr,", "tokens": ["Do", "stund", "es", "a", "En\u00b7ge\u00b7la", "hin\u00b7ter", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.28": {"text": "Und both es a M\u00fcmfeli Brout herf\u00fcr. \u2013", "tokens": ["Und", "both", "es", "a", "M\u00fcm\u00b7fe\u00b7li", "Brout", "her\u00b7f\u00fcr", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "NE", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.29": {"text": "Jez sin mer halt gestorba; und leaba nimmai,", "tokens": ["Jez", "sin", "mer", "halt", "ge\u00b7stor\u00b7ba", ";", "und", "le\u00b7a\u00b7ba", "nim\u00b7mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "KON", "FM", "FM", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.30": {"text": "Und liega zua K\u00f6lla am Bodasai.", "tokens": ["Und", "lie\u00b7ga", "zua", "K\u00f6l\u00b7la", "am", "Bo\u00b7da\u00b7sai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die 4 heilige 3 K\u00f6nig mit ihrem Steara,", "tokens": ["Die", "4", "hei\u00b7li\u00b7ge", "3", "K\u00f6\u00b7nig", "mit", "ih\u00b7rem", "Stea\u00b7ra", ","], "token_info": ["word", "number", "word", "number", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "CARD", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Casper, der Melchar, der Baltes, der Beara,", "tokens": ["Der", "Cas\u00b7per", ",", "der", "Melc\u00b7har", ",", "der", "Bal\u00b7tes", ",", "der", "Be\u00b7a\u00b7ra", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NE", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Sie seaga de' nagelnuia Steara,", "tokens": ["Sie", "sea\u00b7ga", "de'", "na\u00b7gel\u00b7nu\u00b7ia", "Stea\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "