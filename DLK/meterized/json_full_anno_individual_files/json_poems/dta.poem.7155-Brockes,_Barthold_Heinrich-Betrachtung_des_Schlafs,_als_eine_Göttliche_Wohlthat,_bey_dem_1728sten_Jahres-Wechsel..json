{"dta.poem.7155": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es spreche jeder denn mit br\u00fcnstigen Gem\u00fcthe,", "tokens": ["Es", "spre\u00b7che", "je\u00b7der", "denn", "mit", "br\u00fcns\u00b7ti\u00b7gen", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie theur, O GOTT, ist Deine G\u00fcte!", "tokens": ["Wie", "theur", ",", "O", "GoTT", ",", "ist", "Dei\u00b7ne", "G\u00fc\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "NE", "NE", "$,", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was hat nicht jeder Guts in seinem Haus erhalten!", "tokens": ["Was", "hat", "nicht", "je\u00b7der", "Guts", "in", "sei\u00b7nem", "Haus", "er\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn recht als wie ein Seegens-Gu\u00df", "tokens": ["Wenn", "recht", "als", "wie", "ein", "See\u00b7gens\u00b7Gu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "KOUS", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gesundheit, Nahrung, Ueberflu\u00df,", "tokens": ["Ge\u00b7sund\u00b7heit", ",", "Nah\u00b7rung", ",", "Ue\u00b7berf\u00b7lu\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wol den Jungen, als den Alten,", "tokens": ["So", "wol", "den", "Jun\u00b7gen", ",", "als", "den", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sich reichlich mitgetheilt; so huldreich zugewandt;", "tokens": ["Sich", "reich\u00b7lich", "mit\u00b7ge\u00b7theilt", ";", "so", "huld\u00b7reich", "zu\u00b7ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "$.", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es z\u00e4hlt es keine Zung, es fasst es kein Verstand.", "tokens": ["Es", "z\u00e4hlt", "es", "kei\u00b7ne", "Zung", ",", "es", "fasst", "es", "kein", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie mancher schlo\u00df ohn Unmuht, sonder Quaal,", "tokens": ["Wie", "man\u00b7cher", "schlo\u00df", "ohn", "Un\u00b7muht", ",", "son\u00b7der", "Qua\u00b7al", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Drey hundert f\u00fcnf und sechzig mal", "tokens": ["Drey", "hun\u00b7dert", "f\u00fcnf", "und", "sech\u00b7zig", "mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "CARD", "CARD", "KON", "CARD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die, mit dem s\u00fcssen Schlaf erf\u00fcllten Augen-Lieder,", "tokens": ["Die", ",", "mit", "dem", "s\u00fcs\u00b7sen", "Schlaf", "er\u00b7f\u00fcll\u00b7ten", "Au\u00b7gen\u00b7Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des Abends sanffte zu, und \u00f6ffnete sie wieder", "tokens": ["Des", "A\u00b7bends", "sanff\u00b7te", "zu", ",", "und", "\u00f6ff\u00b7ne\u00b7te", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des Morgens ja so offt. Wie mancher hat nicht minder", "tokens": ["Des", "Mor\u00b7gens", "ja", "so", "offt", ".", "Wie", "man\u00b7cher", "hat", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADV", "ADV", "$.", "PWAV", "PIS", "VAFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dergleichen Gl\u00fcck erlebt, da\u00df jedes seiner Kinder", "tokens": ["Derg\u00b7lei\u00b7chen", "Gl\u00fcck", "er\u00b7lebt", ",", "da\u00df", "je\u00b7des", "sei\u00b7ner", "Kin\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "NN", "VVPP", "$,", "KOUS", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jm abgewichnen Jahr, mehr als vier tausend Stunden", "tokens": ["Jm", "ab\u00b7ge\u00b7wich\u00b7nen", "Jahr", ",", "mehr", "als", "vier", "tau\u00b7send", "Stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "PIAT", "KOKOM", "CARD", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dergleichen Gl\u00fcck, nur blo\u00df im Schlaf, empfunden:", "tokens": ["Derg\u00b7lei\u00b7chen", "Gl\u00fcck", ",", "nur", "blo\u00df", "im", "Schlaf", ",", "emp\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ohn mehr, als noch so viel, die mancher ohn Verdru\u00df", "tokens": ["Ohn", "mehr", ",", "als", "noch", "so", "viel", ",", "die", "man\u00b7cher", "ohn", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "$,", "KOUS", "ADV", "ADV", "ADV", "$,", "PRELS", "PIS", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "(wofern er den ihm selber nicht gemacht)", "tokens": ["(", "wo\u00b7fern", "er", "den", "ihm", "sel\u00b7ber", "nicht", "ge\u00b7macht", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Jm Wachen vor\u2019ges Jahr behalten, zugebracht.", "tokens": ["Jm", "Wa\u00b7chen", "vor'\u00b7ges", "Jahr", "be\u00b7hal\u00b7ten", ",", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVINF", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}