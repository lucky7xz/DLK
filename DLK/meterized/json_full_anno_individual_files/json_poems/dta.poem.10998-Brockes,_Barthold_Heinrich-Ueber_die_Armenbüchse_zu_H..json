{"dta.poem.10998": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ueber die Armenb\u00fcchse zu  H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Werther Fremdling! eh die Ordnung f\u00fcr die Armen vor-\ngekehrt,", "tokens": ["Wert\u00b7her", "Fremd\u00b7ling", "!", "eh", "die", "Ord\u00b7nung", "f\u00fcr", "die", "Ar\u00b7men", "vor", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KOUS", "ART", "NN", "APPR", "ART", "NN", "TRUNC", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Warst du, durch ihr Aechzen, Seufzen, Klag und Betteln sehr", "tokens": ["Warst", "du", ",", "durch", "ihr", "A\u00b7ech\u00b7zen", ",", "Seuf\u00b7zen", ",", "Klag", "und", "Bet\u00b7teln", "sehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Jetzt, da du durch sie nicht leidest, leiden sie darum nicht minder:", "tokens": ["Jetzt", ",", "da", "du", "durch", "sie", "nicht", "lei\u00b7dest", ",", "lei\u00b7den", "sie", "da\u00b7rum", "nicht", "min\u00b7der", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "PTKNEG", "VVFIN", "$,", "VVFIN", "PPER", "PAV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "So vergi\u00df denn ihrer nicht. Jhr seyd eines Vaters Kinder.", "tokens": ["So", "ver\u00b7gi\u00df", "denn", "ih\u00b7rer", "nicht", ".", "Ihr", "seyd", "ei\u00b7nes", "Va\u00b7ters", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "PPOSAT", "PTKNEG", "$.", "PPER", "VAFIN", "ART", "NN", "NN", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Gieb, so wirst du wieder nehmen. Legst du f\u00fcr sie etwas ein:", "tokens": ["Gieb", ",", "so", "wirst", "du", "wie\u00b7der", "neh\u00b7men", ".", "Legst", "du", "f\u00fcr", "sie", "et\u00b7was", "ein", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVINF", "$.", "VVFIN", "PPER", "APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wird, so wie dein ganzes Leben, deine Reis\u2019 auch gl\u00fccklich seyn.", "tokens": ["Wird", ",", "so", "wie", "dein", "gan\u00b7zes", "Le\u00b7ben", ",", "dei\u00b7ne", "Reis'", "auch", "gl\u00fcck\u00b7lich", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADV", "KOKOM", "PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}