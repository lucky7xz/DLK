{"dta.poem.13517": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Zur Hochzeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was das f\u00fcr ein Gezwitscher ist!", "tokens": ["Was", "das", "f\u00fcr", "ein", "Ge\u00b7zwit\u00b7scher", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch's Blau die Schwalben zucken", "tokens": ["Durch's", "Blau", "die", "Schwal\u00b7ben", "zu\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schrei'n: \u201esie haben sich gek\u00fc\u00dft!\u201c", "tokens": ["Und", "schrei'n", ":", "\u201e", "sie", "ha\u00b7ben", "sich", "ge\u00b7k\u00fc\u00dft", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PRF", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Baum Rothkehlchen gucken.", "tokens": ["Vom", "Baum", "Roth\u00b7kehl\u00b7chen", "gu\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Storch stolzirt von Bein zu Bein;", "tokens": ["Der", "Storch", "stol\u00b7zirt", "von", "Bein", "zu", "Bein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eda mu\u00df ich fischen gehen \u2014\u201c", "tokens": ["\u201e", "da", "mu\u00df", "ich", "fi\u00b7schen", "ge\u00b7hen", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VVINF", "VVINF", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Abend wie im Traum darein", "tokens": ["Der", "A\u00b7bend", "wie", "im", "Traum", "da\u00b7rein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "APPRART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Schaut von den stillen H\u00f6hen.", "tokens": ["Schaut", "von", "den", "stil\u00b7len", "H\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wie im Traum von den H\u00f6hen", "tokens": ["Und", "wie", "im", "Traum", "von", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seh' ich Nachts meiner Liebsten Haus,", "tokens": ["Seh'", "ich", "Nachts", "mei\u00b7ner", "Liebs\u00b7ten", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Die Wolken dar\u00fcber gehen", "tokens": ["Die", "Wol\u00b7ken", "da\u00b7r\u00fc\u00b7ber", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und l\u00f6schen die Sterne aus.", "tokens": ["Und", "l\u00f6\u00b7schen", "die", "Ster\u00b7ne", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}