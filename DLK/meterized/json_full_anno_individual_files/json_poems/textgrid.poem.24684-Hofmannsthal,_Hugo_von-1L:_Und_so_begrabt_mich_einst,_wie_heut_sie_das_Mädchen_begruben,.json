{"textgrid.poem.24684": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und so begrabt mich einst, wie heut sie das M\u00e4dchen begruben,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und so begrabt mich einst, wie heut sie das M\u00e4dchen begruben,", "tokens": ["Und", "so", "be\u00b7grabt", "mich", "einst", ",", "wie", "heut", "sie", "das", "M\u00e4d\u00b7chen", "be\u00b7gru\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nahe dem blinkenden Strand, nahe dem schattigen Hain.", "tokens": ["Na\u00b7he", "dem", "blin\u00b7ken\u00b7den", "Strand", ",", "na\u00b7he", "dem", "schat\u00b7ti\u00b7gen", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "J\u00fcnglinge fa\u00dften sie sanft, und J\u00fcnglinge hoben die Bahre,", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "fa\u00df\u00b7ten", "sie", "sanft", ",", "und", "J\u00fcng\u00b7lin\u00b7ge", "ho\u00b7ben", "die", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$,", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Fl\u00f6ten umtanzten den Zug, Kr\u00e4nze umschwebten ihn dicht.", "tokens": ["Fl\u00f6\u00b7ten", "um\u00b7tanz\u00b7ten", "den", "Zug", ",", "Kr\u00e4