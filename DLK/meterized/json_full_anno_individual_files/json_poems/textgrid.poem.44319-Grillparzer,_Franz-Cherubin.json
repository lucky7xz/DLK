{"textgrid.poem.44319": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Cherubin", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du, die in meines Herzens Tiefen,", "tokens": ["Wer", "bist", "du", ",", "die", "in", "mei\u00b7nes", "Her\u00b7zens", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die nie der Liebe Sonnenblick durchstrahlt,", "tokens": ["Die", "nie", "der", "Lie\u00b7be", "Son\u00b7nen\u00b7blick", "durch\u00b7strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit unbekannter Zaubermacht gegriffen?", "tokens": ["Mit", "un\u00b7be\u00b7kann\u00b7ter", "Zau\u00b7ber\u00b7macht", "ge\u00b7grif\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer bist du, s\u00fc\u00dfe, reizende Gestalt?", "tokens": ["Wer", "bist", "du", ",", "s\u00fc\u00b7\u00dfe", ",", "rei\u00b7zen\u00b7de", "Ge\u00b7stalt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.5": {"text": "Gef\u00fchle, die im Grund der Seele schliefen,", "tokens": ["Ge\u00b7f\u00fch\u00b7le", ",", "die", "im", "Grund", "der", "See\u00b7le", "schlie\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hast du geweckt mit magischer Gewalt;", "tokens": ["Hast", "du", "ge\u00b7weckt", "mit", "ma\u00b7gi\u00b7scher", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gefesselt ist mein ganzes, tiefstes Wesen,", "tokens": ["Ge\u00b7fes\u00b7selt", "ist", "mein", "gan\u00b7zes", ",", "tiefs\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Kraft und Wille fehlt, das Band zu l\u00f6sen!", "tokens": ["Und", "Kraft", "und", "Wil\u00b7le", "fehlt", ",", "das", "Band", "zu", "l\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Seh ich der Glieder zarte F\u00fclle prangen,", "tokens": ["Seh", "ich", "der", "Glie\u00b7der", "zar\u00b7te", "F\u00fcl\u00b7le", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Entstellt durchs sch\u00f6ngeschm\u00fcckte Knabenkleid,", "tokens": ["Ent\u00b7stellt", "durchs", "sch\u00f6n\u00b7ge\u00b7schm\u00fcck\u00b7te", "Kna\u00b7ben\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das s\u00fc\u00dfe Rot der schamgef\u00e4rbten Wangen,", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "Rot", "der", "scham\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die bl\u00f6de, knabenhafte Sch\u00fcchternheit,", "tokens": ["Die", "bl\u00f6\u00b7de", ",", "kna\u00b7ben\u00b7haf\u00b7te", "Sch\u00fcch\u00b7tern\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das dunkle, erst erwachende Verlangen,", "tokens": ["Das", "dunk\u00b7le", ",", "erst", "er\u00b7wa\u00b7chen\u00b7de", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das brennend w\u00fcnscht und zu begehren scheut,", "tokens": ["Das", "bren\u00b7nend", "w\u00fcnscht", "und", "zu", "be\u00b7geh\u00b7ren", "scheut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "KON", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Flammenblick scheu in den Grund gegraben;", "tokens": ["Den", "Flam\u00b7men\u00b7blick", "scheu", "in", "den", "Grund", "ge\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "So scheinst du mir der reizendste der Knaben!", "tokens": ["So", "scheinst", "du", "mir", "der", "rei\u00b7zends\u00b7te", "der", "Kna\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch seh ich dieses Busens Wallen wieder,", "tokens": ["Doch", "seh", "ich", "die\u00b7ses", "Bu\u00b7sens", "Wal\u00b7len", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verr\u00e4terisch durchs neidsche Kleid gebl\u00e4ht,", "tokens": ["Ver\u00b7r\u00e4\u00b7te\u00b7risch", "durchs", "neid\u00b7sche", "Kleid", "ge\u00b7bl\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Nacken Silber, gleich des Schwans Gefieder,", "tokens": ["Des", "Na\u00b7cken", "Sil\u00b7ber", ",", "gleich", "des", "Schwans", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom weichen, seidnen Lockenhaar umweht,", "tokens": ["Vom", "wei\u00b7chen", ",", "seid\u00b7nen", "Lo\u00b7cken\u00b7haar", "um\u00b7weht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "H\u00f6r ich den hellen Klang der Zauberlieder,", "tokens": ["H\u00f6r", "ich", "den", "hel\u00b7len", "Klang", "der", "Zau\u00b7berl\u00b7ie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und was ein jeder Sinn noch leis ersp\u00e4ht,", "tokens": ["Und", "was", "ein", "je\u00b7der", "Sinn", "noch", "leis", "er\u00b7sp\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "PIAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Horch ich des Herzens ahndungsvollen T\u00f6nen;", "tokens": ["Horch", "ich", "des", "Her\u00b7zens", "ahn\u00b7dungs\u00b7vol\u00b7len", "T\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "VVINF", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So nenn ich dich die Krone aller Sch\u00f6nen.", "tokens": ["So", "nenn", "ich", "dich", "die", "Kro\u00b7ne", "al\u00b7ler", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Schlicht diesen Streit von k\u00e4mpfenden Gef\u00fchlen,", "tokens": ["Schlicht", "die\u00b7sen", "Streit", "von", "k\u00e4mp\u00b7fen\u00b7den", "Ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bez\u00e4hme dieses siedend hei\u00dfe Blut,", "tokens": ["Be\u00b7z\u00e4h\u00b7me", "die\u00b7ses", "sie\u00b7dend", "hei\u00b7\u00dfe", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df meinen Blick in diesen Reizen w\u00fchlen,", "tokens": ["La\u00df", "mei\u00b7nen", "Blick", "in", "die\u00b7sen", "Rei\u00b7zen", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "La\u00df mich der Lippen fieberische Glut", "tokens": ["La\u00df", "mich", "der", "Lip\u00b7pen", "fie\u00b7be\u00b7ri\u00b7sche", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In dieses Busens regen Wellen k\u00fchlen,", "tokens": ["In", "die\u00b7ses", "Bu\u00b7sens", "re\u00b7gen", "Wel\u00b7len", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und meiner K\u00fcsse r\u00e4uberische Flut", "tokens": ["Und", "mei\u00b7ner", "K\u00fcs\u00b7se", "r\u00e4u\u00b7be\u00b7ri\u00b7sche", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Soll das Geheimnis dir im Sturm entrei\u00dfen,", "tokens": ["Soll", "das", "Ge\u00b7heim\u00b7nis", "dir", "im", "Sturm", "ent\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Welch ein Geschlecht du w\u00fcrdigst sein zu hei\u00dfen.", "tokens": ["Welch", "ein", "Ge\u00b7schlecht", "du", "w\u00fcr\u00b7digst", "sein", "