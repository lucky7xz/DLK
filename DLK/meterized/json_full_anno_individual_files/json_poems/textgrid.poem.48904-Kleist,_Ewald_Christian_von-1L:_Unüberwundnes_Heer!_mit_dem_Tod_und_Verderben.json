lexandrine.iambic.hexa"}, "line.4": {"text": "Sie blizt durch dich auf ihn, und seine R\u00fccken bluten.", "tokens": ["Sie", "blizt", "durch", "dich", "auf", "ihn", ",", "und", "sei\u00b7ne", "R\u00fc\u00b7cken", "blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PPER", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Die Nachwelt wird auf dich, als auf ein Muster sehen;", "tokens": ["Die", "Nach\u00b7welt", "wird", "auf", "dich", ",", "als", "auf", "ein", "Mus\u00b7ter", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die k\u00fcnfftgen Helden ehren dich,", "tokens": ["Die", "k\u00fcnfft\u00b7gen", "Hel\u00b7den", "eh\u00b7ren", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ziehn dich den R\u00f6mern vor, dem C\u00e4sar ", "tokens": ["Ziehn", "dich", "den", "R\u00f6\u00b7mern", "vor", ",", "dem", "C\u00e4\u00b7sar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PRELS", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und B\u00f6hmens Felsen sind dir ewige Tropheen.", "tokens": ["Und", "B\u00f6h\u00b7mens", "Fel\u00b7sen", "sind", "dir", "e\u00b7wi\u00b7ge", "Tro\u00b7pheen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VAFIN", "PPER", "A