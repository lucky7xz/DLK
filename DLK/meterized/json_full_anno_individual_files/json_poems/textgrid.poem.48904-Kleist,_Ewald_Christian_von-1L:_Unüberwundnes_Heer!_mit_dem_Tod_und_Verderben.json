{"textgrid.poem.48904": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Un\u00fcberwundnes Heer! mit dem Tod und Verderben", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Un\u00fcberwundnes Heer! mit dem Tod und Verderben", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7und\u00b7nes", "Heer", "!", "mit", "dem", "Tod", "und", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "ART", "NN", "KON", "NN"], "meter": "+----+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "In Legionen Feinde dringt,", "tokens": ["In", "Le\u00b7gi\u00b7o\u00b7nen", "Fein\u00b7de", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um das der frohe Sieg die g\u00fcldnen Fl\u00fcgel schwingt", "tokens": ["Um", "das", "der", "fro\u00b7he", "Sieg", "die", "g\u00fcld\u00b7nen", "Fl\u00fc\u00b7gel", "schwingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Heer! bereit zum Siegen oder Sterben.", "tokens": ["O", "Heer", "!", "be\u00b7reit", "zum", "Sie\u00b7gen", "o\u00b7der", "Ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADJD", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sieh! Feinde deren Last die H\u00fcgel fast versinken", "tokens": ["Sieh", "!", "Fein\u00b7de", "de\u00b7ren", "Last", "die", "H\u00fc\u00b7gel", "fast", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "NN", "PRELAT", "NN", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Erdkreis beben macht,", "tokens": ["Den", "Erd\u00b7kreis", "be\u00b7ben", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ziehn gegen dich und drohn mit Qvaal und ewger Nacht;", "tokens": ["Ziehn", "ge\u00b7gen", "dich", "und", "drohn", "mit", "Qvaal", "und", "ew\u00b7ger", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "KON", "ADV", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Wasser fehlt wo ihre Ro\u00dfe trinken.", "tokens": ["Das", "Was\u00b7ser", "fehlt", "wo", "ih\u00b7re", "Ro\u00b7\u00dfe", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der d\u00fcrre schiele Neid treibt niedertr\u00e4chtge Schaaren", "tokens": ["Der", "d\u00fcr\u00b7re", "schie\u00b7le", "Neid", "treibt", "nie\u00b7der\u00b7tr\u00e4cht\u00b7ge", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus West und S\u00fcd heraus,", "tokens": ["Aus", "West", "und", "S\u00fcd", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Nordens H\u00f6hlen speyn, so wie des Osts, Barbaren", "tokens": ["Und", "Nor\u00b7dens", "H\u00f6h\u00b7len", "speyn", ",", "so", "wie", "des", "Osts", ",", "Bar\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NE", "NN", "VAINF", "$,", "ADV", "KOKOM", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Ungeheur, dich zu verschlingen, aus.", "tokens": ["Und", "Un\u00b7ge\u00b7heur", ",", "dich", "zu", "ver\u00b7schlin\u00b7gen", ",", "aus", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Verdopple deinen Muth! Der Feinde wilde Fluthen", "tokens": ["Ver\u00b7dopp\u00b7le", "dei\u00b7nen", "Muth", "!", "Der", "Fein\u00b7de", "wil\u00b7de", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hemmt ", "tokens": ["Hemmt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und die Gerechtigkeit verjagt den tollen Schwarm.", "tokens": ["Und", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "ver\u00b7jagt", "den", "tol\u00b7len", "Schwarm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie blizt durch dich auf ihn, und seine R\u00fccken bluten.", "tokens": ["Sie", "blizt", "durch", "dich", "auf", "ihn", ",", "und", "sei\u00b7ne", "R\u00fc\u00b7cken", "blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PPER", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die Nachwelt wird auf dich, als auf ein Muster sehen;", "tokens": ["Die", "Nach\u00b7welt", "wird", "auf", "dich", ",", "als", "auf", "ein", "Mus\u00b7ter", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die k\u00fcnfftgen Helden ehren dich,", "tokens": ["Die", "k\u00fcnfft\u00b7gen", "Hel\u00b7den", "eh\u00b7ren", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ziehn dich den R\u00f6mern vor, dem C\u00e4sar ", "tokens": ["Ziehn", "dich", "den", "R\u00f6\u00b7mern", "vor", ",", "dem", "C\u00e4\u00b7sar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PRELS", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und B\u00f6hmens Felsen sind dir ewige Tropheen.", "tokens": ["Und", "B\u00f6h\u00b7mens", "Fel\u00b7sen", "sind", "dir", "e\u00b7wi\u00b7ge", "Tro\u00b7pheen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nur schone, wie bisher, im Lauf von grossen Thaten", "tokens": ["Nur", "scho\u00b7ne", ",", "wie", "bis\u00b7her", ",", "im", "Lauf", "von", "gros\u00b7sen", "Tha\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ADV", "$,", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Landmann, der dein Feind nicht ist!", "tokens": ["Den", "Land\u00b7mann", ",", "der", "dein", "Feind", "nicht", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hilf seiner Noth, wenn du von Noth entfernet bist!", "tokens": ["Hilf", "sei\u00b7ner", "Noth", ",", "wenn", "du", "von", "Noth", "ent\u00b7fer\u00b7net", "bist", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Rauben \u00fcberla\u00df den Feigen und Croaten.", "tokens": ["Das", "Rau\u00b7ben", "\u00fc\u00b7berl\u00b7a\u00df", "den", "Fei\u00b7gen", "und", "Croa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Ich seh, ich sehe schon \u2013 freut euch, o Preu\u00dfens Freunde! \u2013", "tokens": ["Ich", "seh", ",", "ich", "se\u00b7he", "schon", "\u2013", "freut", "euch", ",", "o", "Preu\u00b7\u00dfens", "Freun\u00b7de", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$(", "VVFIN", "PPER", "$,", "FM", "NE", "NN", "$.", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Tage deines Ruhms sich nahn.", "tokens": ["Die", "Ta\u00b7ge", "dei\u00b7nes", "Ruhms", "sich", "nahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PRF", "ADJA", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Ungewittern ziehn die Wilden stolz heran.", "tokens": ["In", "Un\u00b7ge\u00b7wit\u00b7tern", "ziehn", "die", "Wil\u00b7den", "stolz", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "Du eilest ihnen nach, und dr\u00fcckst in schweren Eisen", "tokens": ["Du", "ei\u00b7lest", "ih\u00b7nen", "nach", ",", "und", "dr\u00fcckst", "in", "schwe\u00b7ren", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Tod tief ihren Schedeln ein,", "tokens": ["Den", "Tod", "tief", "ih\u00b7ren", "Sche\u00b7deln", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und kehrst voll Ruhm zur\u00fcck, die Deinen zu erfreun,", "tokens": ["Und", "kehrst", "voll", "Ruhm", "zu\u00b7r\u00fcck", ",", "die", "Dei\u00b7nen", "zu", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "PTKVZ", "$,", "PRELS", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die jauchzend dich empfahn, und ihre Retter preisen.", "tokens": ["Die", "jauch\u00b7zend", "dich", "em\u00b7pfahn", ",", "und", "ih\u00b7re", "Ret\u00b7ter", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Auch ich, ich werde noch, \u2013 verg\u00f6nn es mir o Himmel! \u2013", "tokens": ["Auch", "ich", ",", "ich", "wer\u00b7de", "noch", ",", "\u2013", "ver\u00b7g\u00f6nn", "es", "mir", "o", "Him\u00b7mel", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VAFIN", "ADV", "$,", "$(", "VVFIN", "PPER", "PPER", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einher vor wenig Helden ziehn.", "tokens": ["Ein\u00b7her", "vor", "we\u00b7nig", "Hel\u00b7den", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seh dich, stolzer Feind! den kleinen Haufen fliehn,", "tokens": ["Ich", "seh", "dich", ",", "stol\u00b7zer", "Feind", "!", "den", "klei\u00b7nen", "Hau\u00b7fen", "fliehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und find Ehr oder Tod im rasenden Get\u00fcmmel.", "tokens": ["Und", "find", "Ehr", "o\u00b7der", "Tod", "im", "ra\u00b7sen\u00b7den", "Ge\u00b7t\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Un\u00fcberwundnes Heer! mit dem Tod und Verderben", "tokens": ["Un\u00b7\u00fc\u00b7berw\u00b7und\u00b7nes", "Heer", "!", "mit", "dem", "Tod", "und", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "ART", "NN", "KON", "NN"], "meter": "+----+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "In Legionen Feinde dringt,", "tokens": ["In", "Le\u00b7gi\u00b7o\u00b7nen", "Fein\u00b7de", "dringt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um das der frohe Sieg die g\u00fcldnen Fl\u00fcgel schwingt", "tokens": ["Um", "das", "der", "fro\u00b7he", "Sieg", "die", "g\u00fcld\u00b7nen", "Fl\u00fc\u00b7gel", "schwingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "O Heer! bereit zum Siegen oder Sterben.", "tokens": ["O", "Heer", "!", "be\u00b7reit", "zum", "Sie\u00b7gen", "o\u00b7der", "Ster\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADJD", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "s