 "iambic.tri"}, "line.6": {"text": "Vnd lese mit Verdru\u00df,", "tokens": ["Vnd", "le\u00b7se", "mit", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weil ich mein Vngehewer", "tokens": ["Weil", "ich", "mein", "Vn\u00b7ge\u00b7he\u00b7wer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Nur so betriegen mu\u00df.", "tokens": ["Nur", "so", "be\u00b7trie\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die Glocken h\u00f6r ich schlagen:", "tokens": ["Die", "Glo\u00b7cken", "h\u00f6r", "ich", "schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zw\u00f6lff, eines, zwey, drey, vier,", "tokens": ["Zw\u00f6lff", ",", "ei\u00b7nes", ",", "zwey", ",", "drey", ",", "vier", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PIS", "$,", "CARD", "$,", "CARD", "$,", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00df mich immer plagen,", "tokens": ["Ich", "mu\u00df", "mich", "im\u00b7mer", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Schlaffwunsch h\u00fclffet mir,", "tokens": ["Kein", "Schlaff\u00b7wunsch", "h\u00fclf\u00b7fet", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN",