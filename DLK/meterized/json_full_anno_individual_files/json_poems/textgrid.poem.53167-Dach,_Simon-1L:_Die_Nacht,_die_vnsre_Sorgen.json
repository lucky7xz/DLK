{"textgrid.poem.53167": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Nacht, die vnsre Sorgen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht, die vnsre Sorgen", "tokens": ["Die", "Nacht", ",", "die", "vns\u00b7re", "Sor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch s\u00fcssen Schlaff bezwingt,", "tokens": ["Durch", "s\u00fcs\u00b7sen", "Schlaff", "be\u00b7zwingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rufft schon dem lichten Morgen,", "tokens": ["Rufft", "schon", "dem", "lich\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der sachtlich zu vn\u00df dringt,", "tokens": ["Der", "sacht\u00b7lich", "zu", "vn\u00df", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Sternen Glantz mu\u00df weichen", "tokens": ["Der", "Ster\u00b7nen", "Glantz", "mu\u00df", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd macht dem Tage Bahn:", "tokens": ["Vnd", "macht", "dem", "Ta\u00b7ge", "Bahn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich habe noch f\u00fcr Keichen", "tokens": ["Ich", "ha\u00b7be", "noch", "f\u00fcr", "Kei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Auge zugethan.", "tokens": ["Kein", "Au\u00b7ge", "zu\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Al\u00df alles ist entschlaffen,", "tokens": ["Al\u00df", "al\u00b7les", "ist", "ent\u00b7schlaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kutsch ich mich gleichfalls ein,", "tokens": ["Kutsch", "ich", "mich", "gleich\u00b7falls", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wei\u00df aber nichts zu schaffen,", "tokens": ["Wei\u00df", "a\u00b7ber", "nichts", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu \u00e4ngstig ist die Pein:", "tokens": ["Zu", "\u00e4ngs\u00b7tig", "ist", "die", "Pein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd darauff schlag ich Fewer", "tokens": ["Vnd", "dar\u00b7auff", "schlag", "ich", "Fe\u00b7wer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd lese mit Verdru\u00df,", "tokens": ["Vnd", "le\u00b7se", "mit", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weil ich mein Vngehewer", "tokens": ["Weil", "ich", "mein", "Vn\u00b7ge\u00b7he\u00b7wer"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Nur so betriegen mu\u00df.", "tokens": ["Nur", "so", "be\u00b7trie\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Glocken h\u00f6r ich schlagen:", "tokens": ["Die", "Glo\u00b7cken", "h\u00f6r", "ich", "schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zw\u00f6lff, eines, zwey, drey, vier,", "tokens": ["Zw\u00f6lff", ",", "ei\u00b7nes", ",", "zwey", ",", "drey", ",", "vier", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PIS", "$,", "CARD", "$,", "CARD", "$,", "CARD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00df mich immer plagen,", "tokens": ["Ich", "mu\u00df", "mich", "im\u00b7mer", "pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Schlaffwunsch h\u00fclffet mir,", "tokens": ["Kein", "Schlaff\u00b7wunsch", "h\u00fclf\u00b7fet", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein H\u00e4upt sinckt offt danieder,", "tokens": ["Mein", "H\u00e4upt", "sinckt", "offt", "da\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Augen mach ich zu,", "tokens": ["Die", "Au\u00b7gen", "mach", "ich", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Krieg Ohnmacht in die Glieder,", "tokens": ["Krieg", "Ohn\u00b7macht", "in", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Nicht aber etwas Ruh.", "tokens": ["Nicht", "a\u00b7ber", "et\u00b7was", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ist die\u00df nicht grosser Jammer?", "tokens": ["Ist", "die\u00df", "nicht", "gros\u00b7ser", "Jam\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein iedes h\u00fcllt sich ein", "tokens": ["Ein", "ie\u00b7des", "h\u00fcllt", "sich", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PRF", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd schl\u00e4fft in seiner Kammer,", "tokens": ["Vnd", "schl\u00e4fft", "in", "sei\u00b7ner", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch selbst der Mondenschein:", "tokens": ["Auch", "selbst", "der", "Mon\u00b7den\u00b7schein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Windchen ist verhanden,", "tokens": ["Kein", "Wind\u00b7chen", "ist", "ver\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Pregel ruht begn\u00fcgt,", "tokens": ["Der", "Pre\u00b7gel", "ruht", "be\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Auch schl\u00e4fft in seinen Banden", "tokens": ["Auch", "schl\u00e4fft", "in", "sei\u00b7nen", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der so gefangen liegt.", "tokens": ["Der", "so", "ge\u00b7fan\u00b7gen", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nur ich sitz \u00fcber ende", "tokens": ["Nur", "ich", "sitz", "\u00fc\u00b7ber", "en\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd nehme mit Beschwer", "tokens": ["Vnd", "neh\u00b7me", "mit", "Be\u00b7schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein H\u00e4upt in beyde H\u00e4nde", "tokens": ["Mein", "H\u00e4upt", "in", "bey\u00b7de", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd win\u00dfle so daher:", "tokens": ["Vnd", "win\u00df\u00b7le", "so", "da\u00b7her", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Solt jemand ietzt mich schawen,", "tokens": ["Solt", "je\u00b7mand", "ietzt", "mich", "scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Er h\u00e4tt ob meiner Quahl", "tokens": ["Er", "h\u00e4tt", "ob", "mei\u00b7ner", "Quahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mitleiden oder Grawen,", "tokens": ["Mit\u00b7lei\u00b7den", "o\u00b7der", "Gra\u00b7wen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch w\u00e4r er harter Stahl.", "tokens": ["Auch", "w\u00e4r", "er", "har\u00b7ter", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Erbarmt euch meiner Schmertzen,", "tokens": ["Er\u00b7barmt", "euch", "mei\u00b7ner", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr \u00c4rtzte, kompt zuhauff,", "tokens": ["Ihr", "\u00c4rtz\u00b7te", ",", "kompt", "zu\u00b7hauff", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nehmt meine Noht zu Hertzen,", "tokens": ["Nehmt", "mei\u00b7ne", "Noht", "zu", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schlagt ewre B\u00fccher auff,", "tokens": ["Schlagt", "ew\u00b7re", "B\u00fc\u00b7cher", "auff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wa\u00df ewer Raht wirdt bringen,", "tokens": ["Wa\u00df", "e\u00b7wer", "Raht", "wirdt", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auch w\u00e4r es GassenKoht,", "tokens": ["Auch", "w\u00e4r", "es", "Gas\u00b7sen", "Koht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich wil ihn in mich schlingen,", "tokens": ["Ich", "wil", "ihn", "in", "mich", "schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So gro\u00df ist meine Noht.", "tokens": ["So", "gro\u00df", "ist", "mei\u00b7ne", "Noht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ach das ich nur verdrossen", "tokens": ["Ach", "das", "ich", "nur", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PRELS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mach ewre Wissenschaft,", "tokens": ["Mach", "ew\u00b7re", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab vmbsonst genossen", "tokens": ["Ich", "hab", "vmbsonst", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So manchen Tranck vnd Safft,", "tokens": ["So", "man\u00b7chen", "Tranck", "vnd", "Safft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Leid ist nicht zu heben,", "tokens": ["Mein", "Leid", "ist", "nicht", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Es kriegt den Sieges Prei\u00df,", "tokens": ["Es", "kriegt", "den", "Sie\u00b7ges", "Prei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich mu\u00df verlohren geben,", "tokens": ["Ich", "mu\u00df", "ver\u00b7loh\u00b7ren", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vmbsonst ist Kunst vnd Flei\u00df.", "tokens": ["Vmbsonst", "ist", "Kunst", "vnd", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Mein Fieber ist verschwunden,", "tokens": ["Mein", "Fie\u00b7ber", "ist", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mich hungert allgemach,", "tokens": ["Mich", "hun\u00b7gert", "all\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich gebe den Gesunden", "tokens": ["Ich", "ge\u00b7be", "den", "Ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fast nirgends etwas nach,", "tokens": ["Fast", "nir\u00b7gends", "et\u00b7was", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Durst hat sich geleget,", "tokens": ["Mein", "Durst", "hat", "sich", "ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nur da\u00df der z\u00e4he Wust", "tokens": ["Nur", "da\u00df", "der", "z\u00e4\u00b7he", "Wust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Athems-K\u00fcrtz erreget", "tokens": ["Die", "A\u00b7thems\u00b7K\u00fcrtz", "er\u00b7re\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In meiner engen Brust.", "tokens": ["In", "mei\u00b7ner", "en\u00b7gen", "Brust", "."], "token_info": ["wo