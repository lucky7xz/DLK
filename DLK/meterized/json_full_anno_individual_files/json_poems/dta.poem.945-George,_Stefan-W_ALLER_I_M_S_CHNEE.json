{"dta.poem.945": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "W ALLER   I M   S CHNEE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lehre dich den sanften reiz des zimmers               ", "tokens": ["Ich", "leh\u00b7re", "dich", "den", "sanf\u00b7ten", "reiz", "des", "zim\u00b7mers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Empfinden und der trauten winkel raunen               ", "tokens": ["Emp\u00b7fin\u00b7den", "und", "der", "trau\u00b7ten", "win\u00b7kel", "rau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des feuers und des stummen lampen-flimmers               ", "tokens": ["Des", "feu\u00b7ers", "und", "des", "stum\u00b7men", "lam\u00b7pen\u00b7flim\u00b7mers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du hast daf&#252;r das gleiche m&#252;de staunen             ", "tokens": ["Du", "hast", "daf", "&#252;", "r", "das", "glei\u00b7che", "m", "&#252;", "de", "stau\u00b7nen"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus deiner bl&#228;sse fach ich keinen funken               ", "tokens": ["Aus", "dei\u00b7ner", "bl", "&#228;", "sse", "fach", "ich", "kei\u00b7nen", "fun\u00b7ken"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "FM", "FM", "FM", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich ziehe mich zur&#252;ck zum beigemache               ", "tokens": ["Ich", "zie\u00b7he", "mich", "zur", "&#252;", "ck", "zum", "bei\u00b7ge\u00b7ma\u00b7che"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sinne schweigsam in das knie gesunken:               ", "tokens": ["Und", "sin\u00b7ne", "schweig\u00b7sam", "in", "das", "knie", "ge\u00b7sun\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob jemals du erwachen wirst? erwache!             ", "tokens": ["Ob", "je\u00b7mals", "du", "er\u00b7wa\u00b7chen", "wirst", "?", "er\u00b7wa\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVINF", "VAFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So oft ich zagend mich zum vorhang kehre               ", "tokens": ["So", "oft", "ich", "za\u00b7gend", "mich", "zum", "vor\u00b7hang", "keh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ADJD", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sitzest noch wie anfangs in gedanken               ", "tokens": ["Du", "sit\u00b7zest", "noch", "wie", "an\u00b7fangs", "in", "ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein auge h&#228;ngt noch immer an der leere               ", "tokens": ["Dein", "au\u00b7ge", "h", "&#228;", "ngt", "noch", "im\u00b7mer", "an", "der", "lee\u00b7re"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "ADV", "ADV", "APPR", "ART", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dein schatten kreuzt des teppichs selbe ranken             ", "tokens": ["Dein", "schat\u00b7ten", "kreuzt", "des", "tep\u00b7pichs", "sel\u00b7be", "ran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was hindert dann noch dass unge&#252;bte               ", "tokens": ["Was", "hin\u00b7dert", "dann", "noch", "dass", "un\u00b7ge", "&#252;", "bte"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ART", "ADJA", "$(", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Vertauenslose flehen mir entfliesse:               ", "tokens": ["Ver\u00b7tau\u00b7ens\u00b7lo\u00b7se", "fle\u00b7hen", "mir", "ent\u00b7flies\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O gieb dass grosse mutter und betr&#252;bte               ", "tokens": ["O", "gieb", "dass", "gros\u00b7se", "mut\u00b7ter", "und", "be\u00b7tr", "&#252;", "bte"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "KON", "NE", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dieser seele wieder trost entspriesse.             ", "tokens": ["In", "die\u00b7ser", "see\u00b7le", "wie\u00b7der", "trost", "ent\u00b7spries\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}