{"textgrid.poem.66900": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1. Dem Einiger", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": " ... Gew\u00f6lk flog hin. Doch wieder nah und fern", "tokens": ["...", "Ge\u00b7w\u00f6lk", "flog", "hin", ".", "Doch", "wie\u00b7der", "nah", "und", "fern"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "NN", "VVFIN", "PTKVZ", "$.", "KON", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ging auf am Himmel blinkend Stern an Stern.", "tokens": ["Ging", "auf", "am", "Him\u00b7mel", "blin\u00b7kend", "Stern", "an", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie ich folgte der Planeten Schar,", "tokens": ["Und", "wie", "ich", "folg\u00b7te", "der", "Pla\u00b7ne\u00b7ten", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein seltsam Wunderzeichen nahm ich wahr.", "tokens": ["Ein", "selt\u00b7sam", "Wun\u00b7der\u00b7zei\u00b7chen", "nahm", "ich", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Geschrieben stand ein Wort aus Sternenglut,", "tokens": ["Ge\u00b7schrie\u00b7ben", "stand", "ein", "Wort", "aus", "Ster\u00b7neng\u00b7lut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das durch die Adern schwellend trieb mein Blut.", "tokens": ["Das", "durch", "die", "A\u00b7dern", "schwel\u00b7lend", "trieb", "mein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bballeinig\u00ab hie\u00df die Flammenschrift da droben,", "tokens": ["\u00bb", "al\u00b7lei\u00b7nig", "\u00ab", "hie\u00df", "die", "Flam\u00b7men\u00b7schrift", "da", "dro\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$(", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und mancher sah's, den gl\u00e4ubigen Blick erhoben.", "tokens": ["Und", "man\u00b7cher", "sah's", ",", "den", "gl\u00e4u\u00b7bi\u00b7gen", "Blick", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "In eines Kiefernwaldes Lichtung \u2013 wer", "tokens": ["In", "ei\u00b7nes", "Kie\u00b7fern\u00b7wal\u00b7des", "Lich\u00b7tung", "\u2013", "wer"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "NN", "$(", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Jagdrock lehnte sinnend am Gewehr?", "tokens": ["Im", "Jag\u00b7drock", "lehn\u00b7te", "sin\u00b7nend", "am", "Ge\u00b7wehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Welch wuchtige Stirn, welch zielgerader Blick!", "tokens": ["Welch", "wuch\u00b7ti\u00b7ge", "Stirn", ",", "welch", "ziel\u00b7ge\u00b7ra\u00b7der", "Blick", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PWAT", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "\u00bbzur Gr\u00f6\u00dfe mu\u00df des Vaterlands Geschick", "tokens": ["\u00bb", "zur", "Gr\u00f6\u00b7\u00dfe", "mu\u00df", "des", "Va\u00b7ter\u00b7lands", "Ge\u00b7schick"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Geh\u00e4mmert sein,\u00ab zuckt's durch die Erzgestalt,", "tokens": ["Ge\u00b7h\u00e4m\u00b7mert", "sein", ",", "\u00ab", "zuckt's", "durch", "die", "Erz\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,", "$(", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbund schweigt der Wille, donnre ich Gewalt.", "tokens": ["\u00bb", "und", "schweigt", "der", "Wil\u00b7le", ",", "donn\u00b7re", "ich", "Ge\u00b7walt", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df Deutschlands schwanker Gliederbau erstarke,", "tokens": ["Da\u00df", "Deutschlands", "schwan\u00b7ker", "Glie\u00b7der\u00b7bau", "er\u00b7star\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Sein faules Fleisch ausschneid' ich bis zum Marke.\u00ab", "tokens": ["Sein", "fau\u00b7les", "Fleisch", "aus\u00b7schneid'", "ich", "bis", "zum", "Mar\u00b7ke", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und schon erschien dem Schauenden ein Bild", "tokens": ["Und", "schon", "er\u00b7schien", "dem", "Schau\u00b7en\u00b7den", "ein", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des V\u00f6lkerschicksals \u2013 Sedans Schlachtgefild.", "tokens": ["Des", "V\u00f6l\u00b7ker\u00b7schick\u00b7sals", "\u2013", "Se\u00b7dans", "Schlacht\u00b7ge\u00b7fild", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Nebel wallten \u00fcber Tal und Flu\u00df,", "tokens": ["Die", "Ne\u00b7bel", "wall\u00b7ten", "\u00fc\u00b7ber", "Tal", "und", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kanonendonner folgend Schu\u00df auf Schu\u00df.", "tokens": ["Ka\u00b7no\u00b7nen\u00b7don\u00b7ner", "fol\u00b7gend", "Schu\u00df", "auf", "Schu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kolonne an Kolonne schlo\u00df den Ring,", "tokens": ["Ko\u00b7lon\u00b7ne", "an", "Ko\u00b7lon\u00b7ne", "schlo\u00df", "den", "Ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Drin sich das schuldige Opfer ganz verfing.", "tokens": ["Drin", "sich", "das", "schul\u00b7di\u00b7ge", "Op\u00b7fer", "ganz", "ver\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "\u00bbwas treibst du noch zum Kampf, verfallner Kaiser?", "tokens": ["\u00bb", "was", "treibst", "du", "noch", "zum", "Kampf", ",", "ver\u00b7fall\u00b7ner", "Kai\u00b7ser", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dein Thron zerspli\u00df wie morsche Fichtenreiser.\u00ab", "tokens": ["Dein", "Thron", "zer\u00b7spli\u00df", "wie", "mor\u00b7sche", "Fich\u00b7ten\u00b7rei\u00b7ser", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gebrochen stand mit hohlem Aug' er da,", "tokens": ["Ge\u00b7bro\u00b7chen", "stand", "mit", "hoh\u00b7lem", "Aug'", "er", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Fieberglut dem letzten Flackern nah.", "tokens": ["Des", "Fie\u00b7berg\u00b7lut", "dem", "letz\u00b7ten", "Fla\u00b7ckern", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil du der r\u00e4nkevollen Ruhmgier Sohn,", "tokens": ["Weil", "du", "der", "r\u00e4n\u00b7ke\u00b7vol\u00b7len", "Ruhm\u00b7gier", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Versank dein Stern in Nacht, Napoleon.", "tokens": ["Ver\u00b7sank", "dein", "Stern", "in", "Nacht", ",", "Na\u00b7po\u00b7le\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch zischte durch die Reihn kein geiler Spott,", "tokens": ["Doch", "zischte", "durch", "die", "Reihn", "kein", "gei\u00b7ler", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ernst klang's empor: \u00bbNun danket alle Gott!\u00ab", "tokens": ["Ernst", "klang's", "em\u00b7por", ":", "\u00bb", "Nun", "dan\u00b7ket", "al\u00b7le", "Gott", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$.", "$(", "ADV", "VVFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und klar und feierlich von Mund zu Munde", "tokens": ["Und", "klar", "und", "fei\u00b7er\u00b7lich", "von", "Mund", "zu", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Stieg der Choral aus aller Herzensgrunde.", "tokens": ["Stieg", "der", "Cho\u00b7ral", "aus", "al\u00b7ler", "Her\u00b7zens\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Wie der Gesang mir noch im Ohre rauscht!", "tokens": ["Wie", "der", "Ge\u00b7sang", "mir", "noch", "im", "Oh\u00b7re", "rauscht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Voll Kindesandacht hab ich still gelauscht.", "tokens": ["Voll", "Kin\u00b7de\u00b7san\u00b7dacht", "hab", "ich", "still", "ge\u00b7lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein neuer Hauch zog in die Seele ein:", "tokens": ["Ein", "neu\u00b7er", "Hauch", "zog", "in", "die", "See\u00b7le", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sei wert, des eignen Volkes Sohn zu sein!", "tokens": ["Sei", "wert", ",", "des", "eig\u00b7nen", "Vol\u00b7kes", "Sohn", "zu", "sein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ART", "ADJA", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der giftige Wurm der Zwietracht ist gef\