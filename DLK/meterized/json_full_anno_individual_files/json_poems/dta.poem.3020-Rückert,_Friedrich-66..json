{"dta.poem.3020": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "66.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du gehest ein in mich, und ich geh in dich ein;", "tokens": ["Du", "ge\u00b7hest", "ein", "in", "mich", ",", "und", "ich", "geh", "in", "dich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dich athm' ich ein und aus, ein Hauch von dir mein Seyn.", "tokens": ["Dich", "athm'", "ich", "ein", "und", "aus", ",", "ein", "Hauch", "von", "dir", "mein", "Seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,", "ART", "NN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6re dich in mir, und in dir f\u00fchl' ich mich,", "tokens": ["Ich", "h\u00f6\u00b7re", "dich", "in", "mir", ",", "und", "in", "dir", "f\u00fchl'", "ich", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "KON", "APPR", "PPER", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Alles sieht mein Aug' in dir, in Allem dich.", "tokens": ["Und", "Al\u00b7les", "sieht", "mein", "Aug'", "in", "dir", ",", "in", "Al\u00b7lem", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "APPR", "PIS", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du bist das Licht von mir, ich bin von dir der Schatten;", "tokens": ["Du", "bist", "das", "Licht", "von", "mir", ",", "ich", "bin", "von", "dir", "der", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "$,", "PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich m\u00f6cht' in dir zergehn, die Welt will's nicht gestatten.", "tokens": ["Ich", "m\u00f6cht'", "in", "dir", "zer\u00b7gehn", ",", "die", "Welt", "will's", "nicht", "ge\u00b7stat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,", "ART", "NN", "VMFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du bist das Licht in mir, und zehrest auf von innen", "tokens": ["Du", "bist", "das", "Licht", "in", "mir", ",", "und", "zeh\u00b7rest", "auf", "von", "in\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Schatten, da\u00df er mu\u00df der Welt zum Trotz zerrinnen.", "tokens": ["Den", "Schat\u00b7ten", ",", "da\u00df", "er", "mu\u00df", "der", "Welt", "zum", "Trotz", "zer\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O zehr die Welt in mir nur auf mit deinem Glanz,", "tokens": ["O", "zehr", "die", "Welt", "in", "mir", "nur", "auf", "mit", "dei\u00b7nem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "APPR", "PPER", "ADV", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mir nur halb gen\u00fcgt, nur du gen\u00fcgst mir ganz.", "tokens": ["Die", "mir", "nur", "halb", "ge\u00b7n\u00fcgt", ",", "nur", "du", "ge\u00b7n\u00fcgst", "mir", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$,", "ADV", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}