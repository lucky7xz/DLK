 "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Kriegeselend, von dir! Ach, wenn Erinnrung", "tokens": ["Krie\u00b7ge\u00b7se\u00b7lend", ",", "von", "dir", "!", "Ach", ",", "wenn", "E\u00b7rinn\u00b7rung"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "APPR", "PPER", "$.", "ITJ", "$,", "KOUS", "NN"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Deiner mich entheiterte: dann w\u00e4r ich der", "tokens": ["Dei\u00b7ner", "mich", "en\u00b7thei\u00b7ter\u00b7te", ":", "dann", "w\u00e4r", "ich", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ART"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Schuldige, sie nicht,", "tokens": ["Schul\u00b7di\u00b7ge", ",", "sie", "nicht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PTKNEG", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "M\u00fc\u00dfte, mich selber strafend, mir den Anklang", "tokens": ["M\u00fc\u00df\u00b7te", ",", "mich", "sel\u00b7ber", "stra\u00b7fend", ",", "mir", "den", "An\u00b7klang"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "ADV", "ADJD", "$,", "PPER", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mit der Siegerin dann verbieten, der es", "tokens": ["Mit", "der", "Sie\u00b7ge\u00b7rin", "dann", "ver\u00b7bie\u00b7ten", ",", "der", "es"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVINF", "$,", "PRELS", "PPER"], "m