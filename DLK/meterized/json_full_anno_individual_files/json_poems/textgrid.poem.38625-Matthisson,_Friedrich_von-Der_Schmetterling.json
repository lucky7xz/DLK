{"textgrid.poem.38625": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schmetterling", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Sylphide, schweb' in Fr\u00fchlings\u00e4ther!", "tokens": ["Sch\u00f6\u00b7ne", "Syl\u00b7phi\u00b7de", ",", "schweb'", "in", "Fr\u00fch\u00b7lings\u00b7\u00e4t\u00b7her", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Fleug' von Rose zu Rose! schau im Bache", "tokens": ["Fleug'", "von", "Ro\u00b7se", "zu", "Ro\u00b7se", "!", "schau", "im", "Ba\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "APPR", "NE", "$.", "ADJD", "APPRART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Fr\u00f6hlich deine Blumengestalt vom zarten", "tokens": ["Fr\u00f6h\u00b7lich", "dei\u00b7ne", "Blu\u00b7men\u00b7ge\u00b7stalt", "vom", "zar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "APPRART", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Spr\u00f6\u00dfling der Mirthe!", "tokens": ["Spr\u00f6\u00df\u00b7ling", "der", "Mir\u00b7the", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Heiter sei deines Daseyns Maitag! nimmer", "tokens": ["Hei\u00b7ter", "sei", "dei\u00b7nes", "Da\u00b7seyns", "Mai\u00b7tag", "!", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "NN", "$.", "ADV"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "M\u00fc\u00df' ein Bienchen dich schrecken wo du Nektar", "tokens": ["M\u00fc\u00df'", "ein", "Bien\u00b7chen", "dich", "schre\u00b7cken", "wo", "du", "Nek\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "PPER", "VVINF", "PWAV", "PPER", "NN"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Trinkst, und schonend fliege dir stets Cytherens", "tokens": ["Trinkst", ",", "und", "scho\u00b7nend", "flie\u00b7ge", "dir", "stets", "Cy\u00b7the\u00b7rens"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "ADJD", "VVFIN", "PPER", "ADV", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Vogel vor\u00fcber.", "tokens": ["Vo\u00b7gel", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Wann dich der Orkus aufnimmt, ruh' im Kranze", "tokens": ["Wann", "dich", "der", "Or\u00b7kus", "auf\u00b7nimmt", ",", "ruh'", "im", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Platons, welcher, wie du der armen Menschheit", "tokens": ["Pla\u00b7tons", ",", "wel\u00b7cher", ",", "wie", "du", "der", "ar\u00b7men", "Menschheit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "$,", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wonne, die Entschleyerung Psyches lehrte,", "tokens": ["Won\u00b7ne", ",", "die", "Ent\u00b7schley\u00b7e\