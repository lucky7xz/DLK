{"dta.poem.1276": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der berechtigte Ku\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wohnet nicht auff deinen Lippen/ meine Freude/ mein", "tokens": ["Woh\u00b7net", "nicht", "auff", "dei\u00b7nen", "Lip\u00b7pen", "/", "mei\u00b7ne", "Freu\u00b7de", "/", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Vergn\u00fcgen/", "tokens": ["Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Meine Seele/ meine Wonne/ ja mein Leben/ meine", "tokens": ["Mei\u00b7ne", "See\u00b7le", "/", "mei\u00b7ne", "Won\u00b7ne", "/", "ja", "mein", "Le\u00b7ben", "/", "mei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "ADV", "PPOSAT", "NN", "$(", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Ruh?", "tokens": ["Ruh", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Warum soll ich nicht das Meine/ wo ichs finde/ wieder", "tokens": ["Wa\u00b7rum", "soll", "ich", "nicht", "das", "Mei\u00b7ne", "/", "wo", "ichs", "fin\u00b7de", "/", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "ART", "PPOSAT", "$(", "PWAV", "PIS", "VVFIN", "$(", "ADV"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "kriegen?", "tokens": ["krie\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Alle Recht und Richter sprechen jedem ja das Seine", "tokens": ["Al\u00b7le", "Recht", "und", "Rich\u00b7ter", "spre\u00b7chen", "je\u00b7dem", "ja", "das", "Sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "PIS", "ADV", "ART", "PPOSAT"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "zu.", "tokens": ["zu", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}}}}}