{"dta.poem.515": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er seuffzet umsonst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Seuffzen gehet in die Lufft/", "tokens": ["Mein", "Seuff\u00b7zen", "ge\u00b7het", "in", "die", "Lufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hat die weite Welt zur Grufft/", "tokens": ["Und", "hat", "die", "wei\u00b7te", "Welt", "zur", "Grufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Und will in meiner Noht mir ein Geh\u00fclffe seyn.", "tokens": ["Und", "will", "in", "mei\u00b7ner", "Noht", "mir", "ein", "Ge\u00b7h\u00fclf\u00b7fe", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "PPER", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr traurger Wider Hall vermehret meine Klagen/", "tokens": ["Ihr", "traur\u00b7ger", "Wi\u00b7der", "Hall", "ver\u00b7meh\u00b7ret", "mei\u00b7ne", "Kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und l\u00e4st mich ohne Trost in meiner Angst verzagen.", "tokens": ["Und", "l\u00e4st", "mich", "oh\u00b7ne", "Trost", "in", "mei\u00b7ner", "Angst", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Rufft gleich die sanffte Stimm mir tr\u00f6stlich zu/", "tokens": ["Rufft", "gleich", "die", "sanff\u00b7te", "Stimm", "mir", "tr\u00f6st\u00b7lich", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "PPER", "ADJD", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Gebiert ihr Trost doch keine Ruh/", "tokens": ["Ge\u00b7biert", "ihr", "Trost", "doch", "kei\u00b7ne", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Weil ihn der Wider Schall nicht ", "tokens": ["Weil", "ihn", "der", "Wi\u00b7der", "Schall", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PTKNEG"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ich werde nicht von meiner Quaal erl\u00f6\u00dft.", "tokens": ["Ich", "wer\u00b7de", "nicht", "von", "mei\u00b7ner", "Qua\u00b7al", "er\u00b7l\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.11": {"text": "Ich kan der ", "tokens": ["Ich", "kan", "der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Nur h\u00fcte dich mein Licht/", "tokens": ["Nur", "h\u00fc\u00b7te", "dich", "mein", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Da\u00df dir ", "tokens": ["Da\u00df", "dir"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Mu\u00df am Geschicke weichen.", "tokens": ["Mu\u00df", "am", "Ge\u00b7schi\u00b7cke", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}