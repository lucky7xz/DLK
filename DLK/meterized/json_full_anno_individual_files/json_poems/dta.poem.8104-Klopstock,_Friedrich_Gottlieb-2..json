{"dta.poem.8104": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er betet, da st\u00fcrzt hoch herab", "tokens": ["Er", "be\u00b7tet", ",", "da", "st\u00fcrzt", "hoch", "her\u00b7ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Gebot vom Thron her, Flammen herab!", "tokens": ["Ein", "Ge\u00b7bot", "vom", "Thron", "her", ",", "Flam\u00b7men", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PTKVZ", "$,", "NN", "ADV", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Das Opfer versank schnell in der Glut,", "tokens": ["Das", "Op\u00b7fer", "ver\u00b7sank", "schnell", "in", "der", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die Wasser am Altar brannten in die H\u00f6h.", "tokens": ["Und", "die", "Was\u00b7ser", "am", "Al\u00b7tar", "brann\u00b7ten", "in", "die", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}