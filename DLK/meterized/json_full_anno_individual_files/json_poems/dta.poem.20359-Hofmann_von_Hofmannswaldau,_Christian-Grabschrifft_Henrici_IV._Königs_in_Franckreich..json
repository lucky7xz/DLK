{"dta.poem.20359": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft Henrici  IV.  K\u00f6nigs  \n in Franckreich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin durch schimpff und ernst zu meinem reiche kommen/", "tokens": ["Ich", "bin", "durch", "schimpff", "und", "ernst", "zu", "mei\u00b7nem", "rei\u00b7che", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "ADJD", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein unerh\u00f6rter mord hat mir es weggenommen.", "tokens": ["Ein", "un\u00b7er\u00b7h\u00f6r\u00b7ter", "mord", "hat", "mir", "es", "weg\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was halff mich/ was ich lied? was halff/ was ich gethan?", "tokens": ["Was", "halff", "mich", "/", "was", "ich", "lied", "?", "was", "halff", "/", "was", "ich", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$(", "PWS", "PPER", "VVFIN", "$.", "PWS", "VVFIN", "$(", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nachdem ein messer mehr als eine messe kan.", "tokens": ["Nach\u00b7dem", "ein", "mes\u00b7ser", "mehr", "als", "ei\u00b7ne", "mes\u00b7se", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIAT", "KOKOM", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}