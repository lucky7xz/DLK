{"dta.poem.21546": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Herbstgef\u00fchl .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! Das ist der Herbst! Es bl\u00e4st", "tokens": ["Ach", "!", "Das", "ist", "der", "Herbst", "!", "Es", "bl\u00e4st"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "PDS", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Scharf und kalt der Wind von Norden,", "tokens": ["Scharf", "und", "kalt", "der", "Wind", "von", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der blaue Himmel ist", "tokens": ["Und", "der", "blau\u00b7e", "Him\u00b7mel", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Tr\u00fcb und nebelgrau geworden.", "tokens": ["Tr\u00fcb", "und", "ne\u00b7bel\u00b7grau", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihre nackten Arme hebt", "tokens": ["Ih\u00b7re", "nack\u00b7ten", "Ar\u00b7me", "hebt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem Himmel auf die Linde,", "tokens": ["Zu", "dem", "Him\u00b7mel", "auf", "die", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die vor meinem Fenster steht,", "tokens": ["Die", "vor", "mei\u00b7nem", "Fens\u00b7ter", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie \u00e4chzt und st\u00f6hnt im Winde.", "tokens": ["Und", "sie", "\u00e4chzt", "und", "st\u00f6hnt", "im", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht mehr aus dem Gartenhag", "tokens": ["Nicht", "mehr", "aus", "dem", "Gar\u00b7ten\u00b7hag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Lichte Blumenaugen gr\u00fc\u00dfen,", "tokens": ["Lich\u00b7te", "Blu\u00b7men\u00b7au\u00b7gen", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit den Bl\u00e4ttern, welk und falb", "tokens": ["Mit", "den", "Bl\u00e4t\u00b7tern", ",", "welk", "und", "falb"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielt der Wind zu Wand\u2019rers F\u00fc\u00dfen.", "tokens": ["Spielt", "der", "Wind", "zu", "Wan\u00b7d'\u00b7rers", "F\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Alle Lieder sind verstummt,", "tokens": ["Al\u00b7le", "Lie\u00b7der", "sind", "ver\u00b7stummt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst mein V\u00f6glein schweigt im Bauer;", "tokens": ["Selbst", "mein", "V\u00f6\u00b7glein", "schweigt", "im", "Bau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcster \u00fcber aller Welt", "tokens": ["D\u00fcs\u00b7ter", "\u00fc\u00b7ber", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt der Genius der Trauer.", "tokens": ["Schwebt", "der", "Ge\u00b7nius", "der", "Trau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Trauer f\u00fcllt auch mir die Brust,", "tokens": ["Trau\u00b7er", "f\u00fcllt", "auch", "mir", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun des Nordens Sturme blasen,", "tokens": ["Nun", "des", "Nor\u00b7dens", "Stur\u00b7me", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Heimweh geht durchs Herz", "tokens": ["Und", "ein", "Heim\u00b7weh", "geht", "durchs", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der Heimath unterm Rasen \u2014", "tokens": ["Nach", "der", "Hei\u00b7math", "un\u00b7term", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}