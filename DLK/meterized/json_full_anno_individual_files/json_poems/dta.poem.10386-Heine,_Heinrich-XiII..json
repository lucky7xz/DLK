{"dta.poem.10386": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XiII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "O schw\u00f6re nicht und k\u00fcsse nur,", "tokens": ["O", "schw\u00f6\u00b7re", "nicht", "und", "k\u00fcs\u00b7se", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich glaube keinem Weiberschwur!", "tokens": ["Ich", "glau\u00b7be", "kei\u00b7nem", "Wei\u00b7ber\u00b7schwur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Wort ist s\u00fc\u00df, doch s\u00fc\u00dfer ist", "tokens": ["Dein", "Wort", "ist", "s\u00fc\u00df", ",", "doch", "s\u00fc\u00b7\u00dfer", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADV", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Ku\u00df, den ich dir abgek\u00fc\u00dft;", "tokens": ["Der", "Ku\u00df", ",", "den", "ich", "dir", "ab\u00b7ge\u00b7k\u00fc\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den hab' ich, und dran glaub' ich auch,", "tokens": ["Den", "hab'", "ich", ",", "und", "dran", "glaub'", "ich", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "KON", "PAV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Wort ist eitel Dunst und Hauch.", "tokens": ["Das", "Wort", "ist", "ei\u00b7tel", "Dunst", "und", "Hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}