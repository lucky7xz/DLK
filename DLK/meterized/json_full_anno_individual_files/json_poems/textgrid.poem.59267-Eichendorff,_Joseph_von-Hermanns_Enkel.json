{"textgrid.poem.59267": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Hermanns Enkel", "genre": "verse", "period": "N.A.", "pub_year": 1823, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Altdeutsch! \u2013 Altdeutsch? \u2013 Nun, das ist,", "tokens": ["Alt\u00b7deutsch", "!", "\u2013", "Alt\u00b7deutsch", "?", "\u2013", "Nun", ",", "das", "ist", ","], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "$(", "NN", "$.", "$(", "ADV", "$,", "PDS", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was man so in B\u00fcchern liest: \u2013", "tokens": ["Was", "man", "so", "in", "B\u00fc\u00b7chern", "liest", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PIS", "ADV", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kluge Rosse \u2013 pr\u00e4cht'ge Decken,", "tokens": ["Klu\u00b7ge", "Ros\u00b7se", "\u2013", "pr\u00e4cht'\u00b7ge", "De\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4ndel, Kruzifixe, Recken \u2013", "tokens": ["H\u00e4n\u00b7del", ",", "Kru\u00b7zi\u00b7fi\u00b7xe", ",", "Re\u00b7cken", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oh, wie herrlich strahlt dies Leben!", "tokens": ["Oh", ",", "wie", "herr\u00b7lich", "strahlt", "dies", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VVFIN", "PDS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "G\u00f6ttlich! \u2013 Doch mit Unterschied.", "tokens": ["G\u00f6tt\u00b7lich", "!", "\u2013", "Doch", "mit", "Un\u00b7ter\u00b7schied", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "$(", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Es versteht sich, da\u00df man's deute \u2013", "tokens": ["Es", "ver\u00b7steht", "sich", ",", "da\u00df", "man's", "deu\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "PIS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "'s w\u00e4r doch gar zu unbequem,", "tokens": ["'s", "w\u00e4r", "doch", "gar", "zu", "un\u00b7be\u00b7quem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn man alles w\u00f6rtlich n\u00e4hm,", "tokens": ["Wenn", "man", "al\u00b7les", "w\u00f6rt\u00b7lich", "n\u00e4hm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie's da durcheinander bl\u00fcht! \u2013", "tokens": ["Wie's", "da", "durch\u00b7ein\u00b7an\u00b7der", "bl\u00fcht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Diese Ritter \u2013 gute Leute,", "tokens": ["Die\u00b7se", "Rit\u00b7ter", "\u2013", "gu\u00b7te", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ehrlich, tapfer, brave Reiter \u2013", "tokens": ["Ehr\u00b7lich", ",", "tap\u00b7fer", ",", "bra\u00b7ve", "Rei\u00b7ter", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Gegen uns doch B\u00e4renh\u00e4uter!", "tokens": ["Ge\u00b7gen", "uns", "doch", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Eigentlich sind wir wohl weiter.", "tokens": ["Ei\u00b7gent\u00b7lich", "sind", "wir", "wohl", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.15": {"text": "Lehnstreu \u2013 Kl\u00f6ster \u2013 Barbarei \u2013", "tokens": ["Lehn\u00b7streu", "\u2013", "Kl\u00f6s\u00b7ter", "\u2013", "Bar\u00b7ba\u00b7rei", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Davon machen ", "tokens": ["Da\u00b7von", "ma\u00b7chen"], "token_info": ["word", "word"], "pos": ["PAV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Fangen wir ", "tokens": ["Fan\u00b7gen", "wir"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "F\u00fcrcht ich, bleibt es bei Gedichten", "tokens": ["F\u00fcrcht", "ich", ",", "bleibt", "es", "bei", "Ge\u00b7dich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Nein doch! Eines, geht mir bei,", "tokens": ["Nein", "doch", "!", "Ei\u00b7nes", ",", "geht", "mir", "bei", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$.", "PIS", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Eines bleibt doch: dies Vernichten", "tokens": ["Ei\u00b7nes", "bleibt", "doch", ":", "dies", "Ver\u00b7nich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "$.", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Aller Modesklaverei! \u2013", "tokens": ["Al\u00b7ler", "Mo\u00b7des\u00b7kla\u00b7ve\u00b7rei", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Hohe Vaterl\u00e4nderei!", "tokens": ["Ho\u00b7he", "Va\u00b7ter\u00b7l\u00e4n\u00b7de\u00b7rei", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Schnittst du los nicht Hermanns S\u00f6hne", "tokens": ["Schnittst", "du", "los", "nicht", "Her\u00b7manns", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Von des Halstuchs schn\u00f6den Schlingen,", "tokens": ["Von", "des", "Hal\u00b7stuchs", "schn\u00f6\u00b7den", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Worin, sonder Kraft und Sch\u00f6ne,", "tokens": ["Wo\u00b7rin", ",", "son\u00b7der", "Kraft", "und", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Unsre V\u00e4ter schm\u00e4hlich hingen?", "tokens": ["Uns\u00b7re", "V\u00e4\u00b7ter", "schm\u00e4h\u00b7lich", "hin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Gabst du nicht dem L\u00f6wen M\u00e4hne,", "tokens": ["Gabst", "du", "nicht", "dem", "L\u00f6\u00b7wen", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Die ihm frech die Zeit gestohlen?", "tokens": ["Die", "ihm", "frech", "die", "Zeit", "ge\u00b7stoh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Statt des wind'gen Fracks Geflatter", "tokens": ["Statt", "des", "win\u00b7d'\u00b7gen", "Fracks", "Ge\u00b7flat\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.30": {"text": "Der Litewka Schurz aus Polen,", "tokens": ["Der", "Li\u00b7tew\u00b7ka", "Schurz", "aus", "Po\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Statt des Franzen knabenglatter", "tokens": ["Statt", "des", "Fran\u00b7zen", "kna\u00b7ben\u00b7glat\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Schnauze: seinen Henri quatre? \u2013", "tokens": ["Schnau\u00b7ze", ":", "sei\u00b7nen", "Hen\u00b7ri", "quat\u00b7re", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PPOSAT", "NE", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Bruder, ich sag's unverhohlen,", "tokens": ["Bru\u00b7der", ",", "ich", "sag's", "un\u00b7ver\u00b7hoh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und auch du wirst's nicht bestreiten:", "tokens": ["Und", "auch", "du", "wir\u00b7st's", "nicht", "be\u00b7strei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VAFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Gro\u00dfe Zeichen gro\u00dfer Zeiten! \u2013", "tokens": ["Gro\u00b7\u00dfe", "Zei\u00b7chen", "gro\u00b7\u00dfer", "Zei\u00b7ten", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wahrlich, s\u00e4h ich nicht den Kragen", "tokens": ["Wahr\u00b7lich", ",", "s\u00e4h", "ich", "nicht", "den", "Kra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "\u00dcbern schwarzen Rock geschlagen,", "tokens": ["\u00dc\u00b7bern", "schwar\u00b7zen", "Rock", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Schien' mir alles Ironie.", "tokens": ["Schien'", "mir", "al\u00b7les", "I\u00b7ro\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Doch wie sprech ich da? Ironisch \u2013", "tokens": ["Doch", "wie", "sprech", "ich", "da", "?", "I\u00b7ron\u00b7isch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ADV", "$.", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Dieses Wort ist nicht teutonisch.", "tokens": ["Die\u00b7ses", "Wort", "ist", "nicht", "teu\u00b7to\u00b7nisch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.41": {"text": "Undeutsch ist die falsche Freude:", "tokens": ["Un\u00b7deutsch", "ist", "die", "fal\u00b7sche", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "K\u00fcnsteln am wahrhaften Wort!", "tokens": ["K\u00fcns\u00b7teln", "am", "wahr\u00b7haf\u00b7ten", "Wort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Ob auch feige Poesie", "tokens": ["Ob", "auch", "fei\u00b7ge", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.44": {"text": "Sauere Gesichter schneide:", "tokens": ["Sau\u00b7e\u00b7re", "Ge\u00b7sich\u00b7ter", "schnei\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.45": {"text": "Durch den welschen L\u00fcgenwitz", "tokens": ["Durch", "den", "wel\u00b7schen", "L\u00fc\u00b7gen\u00b7witz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Schreitet stramm der Deutsche fort", "tokens": ["Schrei\u00b7tet", "stramm", "der", "Deut\u00b7sche", "fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.47": {"text": "Hinter seiner Nasenspitz,", "tokens": ["Hin\u00b7ter", "sei\u00b7ner", "Na\u00b7sen\u00b7spitz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Aller Ehrlichkeiten Sitz,", "tokens": ["Al\u00b7ler", "Ehr\u00b7lich\u00b7kei\u00b7ten", "Sitz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Biderb immer gradeaus.", "tokens": ["Bi\u00b7derb", "im\u00b7mer", "gra\u00b7de\u00b7aus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.50": {"text": "Alles Welsche wird mir Graus,", "tokens": ["Al\u00b7les", "Wel\u00b7sche", "wird", "mir", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Seit ich steck im deutschen Kleide:", "tokens": ["Seit", "ich", "steck", "im", "deut\u00b7schen", "Klei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Du auch, Liebchen, w\u00e4hle gleich", "tokens": ["Du", "auch", ",", "Lieb\u00b7chen", ",", "w\u00e4h\u00b7le", "gleich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVIMP", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Deine Tracht dir altdeutsch aus!", "tokens": ["Dei\u00b7ne", "Tracht", "dir", "alt\u00b7deutsch", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.54": {"text": "Wie's auf Bildern noch zu schauen:", "tokens": ["Wie's", "auf", "Bil\u00b7dern", "noch", "zu", "schau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Wedel von dem Schweif der Pfauen,", "tokens": ["We\u00b7del", "von", "dem", "Schweif", "der", "Pfau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Dann von Spitzen, blumenreich,", "tokens": ["Dann", "von", "Spit\u00b7zen", ",", "blu\u00b7men\u00b7reich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Wie 'ne mittelm\u00e4\u00df'ge Scheibe,", "tokens": ["Wie", "'ne", "mit\u00b7tel\u00b7m\u00e4\u00df'\u00b7ge", "Schei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Eine steife Halsrotunde!", "tokens": ["Ei\u00b7ne", "stei\u00b7fe", "Hals\u00b7ro\u00b7tun\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "'s ist so \u00fcberm schlanken Leibe", "tokens": ["'s", "ist", "so", "\u00fc\u00b7berm", "schlan\u00b7ken", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.60": {"text": "Wie ein Regenschirm gespannt,", "tokens": ["Wie", "ein", "Re\u00b7gen\u00b7schirm", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Obendrauf dann statt dem Knopf", "tokens": ["O\u00b7ben\u00b7drauf", "dann", "statt", "dem", "Knopf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Schwebt der holde Frauenkopf,", "tokens": ["Schwebt", "der", "hol\u00b7de", "Frau\u00b7en\u00b7kopf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "In das Bl\u00fctenmeer von Kragen,", "tokens": ["In", "das", "Bl\u00fc\u00b7ten\u00b7meer", "von", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Ariadnen gleich, verschlagen. \u2013", "tokens": ["A\u00b7riad\u00b7nen", "gleich", ",", "ver\u00b7schla\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "ADV", "$,", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.65": {"text": "Oh, und ein moral'scher Kragen!", "tokens": ["Oh", ",", "und", "ein", "mo\u00b7ral'\u00b7scher", "Kra\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Denn wer ist da so gewandt,", "tokens": ["Denn", "wer", "ist", "da", "so", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.67": {"text": "Fl\u00fcsternd was ins Ohr zu sagen,", "tokens": ["Fl\u00fcs\u00b7ternd", "was", "ins", "Ohr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PRELS", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Was nicht gleich die andern wissen?", "tokens": ["Was", "nicht", "gleich", "die", "an\u00b7dern", "wis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADV", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.69": {"text": "Und \u2013 unm\u00f6glich ist das K\u00fcssen!", "tokens": ["Und", "\u2013", "un\u00b7m\u00f6g\u00b7lich", "ist", "das", "K\u00fcs\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Altdeutsch! \u2013 Altdeutsch? \u2013 Nun, das ist,", "tokens": ["Alt\u00b7deutsch", "!", "\u2013", "Alt\u00b7deutsch", "?", "\u2013", "Nun", ",", "das", "ist", ","], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "$(", "NN", "$.", "$(", "ADV", "$,", "PDS", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was man so in B\u00fcchern liest: \u2013", "tokens": ["Was", "man", "so", "in", "B\u00fc\u00b7chern", "liest", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PIS", "ADV", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kluge Rosse \u2013 pr\u00e4cht'ge Decken,", "tokens": ["Klu\u00b7ge", "Ros\u00b7se", "\u2013", "pr\u00e4cht'\u00b7ge", "De\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4ndel, Kruzifixe, Recken \u2013", "tokens": ["H\u00e4n\u00b7del", ",", "Kru\u00b7zi\u00b7fi\u00b7xe", ",", "Re\u00b7cken", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oh, wie herrlich strahlt dies Leben!", "tokens": ["Oh", ",", "wie", "herr\u00b7lich", "strahlt", "dies", "Le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VVFIN", "PDS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "G\u00f6ttlich! \u2013 Doch mit Unterschied.", "tokens": ["G\u00f6tt\u00b7lich", "!", "\u2013", "Doch", "mit", "Un\u00b7ter\u00b7schied", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "$(", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Es versteht sich, da\u00df man's deute \u2013", "tokens": ["Es", "ver\u00b7steht", "sich", ",", "da\u00df", "man's", "deu\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "PIS", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "'s w\u00e4r doch gar zu unbequem,", "tokens": ["'s", "w\u00e4r", "doch", "gar", "zu", "un\u00b7be\u00b7quem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn man alles w\u00f6rtlich n\u00e4hm,", "tokens": ["Wenn", "man", "al\u00b7les", "w\u00f6rt\u00b7lich", "n\u00e4hm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wie's da durcheinander bl\u00fcht! \u2013", "tokens": ["Wie's", "da", "durch\u00b7ein\u00b7an\u00b7der", "bl\u00fcht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Diese Ritter \u2013 gute Leute,", "tokens": ["Die\u00b7se", "Rit\u00b7ter", "\u2013", "gu\u00b7te", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ehrlich, tapfer, brave Reiter \u2013", "tokens": ["Ehr\u00b7lich", ",", "tap\u00b7fer", ",", "bra\u00b7ve", "Rei\u00b7ter", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Gegen uns doch B\u00e4renh\u00e4uter!", "tokens": ["Ge\u00b7gen", "uns", "doch", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Eigentlich sind wir wohl weiter.", "tokens": ["Ei\u00b7gent\u00b7lich", "sind", "wir", "wohl", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.15": {"text": "Lehnstreu \u2013 Kl\u00f6ster \u2013 Barbarei \u2013", "tokens": ["Lehn\u00b7streu", "\u2013", "Kl\u00f6s\u00b7ter", "\u2013", "Bar\u00b7ba\u00b7rei", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$(", "NN", "$(", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Davon machen ", "tokens": ["Da\u00b7von", "ma\u00b7chen"], "token_info": ["word", "word"], "pos": ["PAV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Fangen wir ", "tokens": ["Fan\u00b7gen", "wir"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.18": {"text": "F\u00fcrcht ich, bleibt es bei Gedichten", "tokens": ["F\u00fcrcht", "ich", ",", "bleibt", "es", "bei", "Ge\u00b7dich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Nein doch! Eines, geht mir bei,", "tokens": ["Nein", "doch", "!", "Ei\u00b7nes", ",", "geht", "mir", "bei", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$.", "PIS", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Eines bleibt doch: dies Vernichten", "tokens": ["Ei\u00b7nes", "bleibt", "doch", ":", "dies", "Ver\u00b7nich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "$.", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Aller Modesklaverei! \u2013", "tokens": ["Al\u00b7ler", "Mo\u00b7des\u00b7kla\u00b7ve\u00b7rei", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Hohe Vaterl\u00e4nderei!", "tokens": ["Ho\u00b7he", "Va\u00b7ter\u00b7l\u00e4n\u00b7de\u00b7rei", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Schnittst du los nicht Hermanns S\u00f6hne", "tokens": ["Schnittst", "du", "los", "nicht", "Her\u00b7manns", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Von des Halstuchs schn\u00f6den Schlingen,", "tokens": ["Von", "des", "Hal\u00b7stuchs", "schn\u00f6\u00b7den", "Schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Worin, sonder Kraft und Sch\u00f6ne,", "tokens": ["Wo\u00b7rin", ",", "son\u00b7der", "Kraft", "und", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Unsre V\u00e4ter schm\u00e4hlich hingen?", "tokens": ["Uns\u00b7re", "V\u00e4\u00b7ter", "schm\u00e4h\u00b7lich", "hin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Gabst du nicht dem L\u00f6wen M\u00e4hne,", "tokens": ["Gabst", "du", "nicht", "dem", "L\u00f6\u00b7wen", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Die ihm frech die Zeit gestohlen?", "tokens": ["Die", "ihm", "frech", "die", "Zeit", "ge\u00b7stoh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Statt des wind'gen Fracks Geflatter", "tokens": ["Statt", "des", "win\u00b7d'\u00b7gen", "Fracks", "Ge\u00b7flat\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.30": {"text": "Der Litewka Schurz aus Polen,", "tokens": ["Der", "Li\u00b7tew\u00b7ka", "Schurz", "aus", "Po\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Statt des Franzen knabenglatter", "tokens": ["Statt", "des", "Fran\u00b7zen", "kna\u00b7ben\u00b7glat\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Schnauze: seinen Henri quatre? \u2013", "tokens": ["Schnau\u00b7ze", ":", "sei\u00b7nen", "Hen\u00b7ri", "quat\u00b7re", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "PPOSAT", "NE", "NE", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Bruder, ich sag's unverhohlen,", "tokens": ["Bru\u00b7der", ",", "ich", "sag's", "un\u00b7ver\u00b7hoh\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und auch du wirst's nicht bestreiten:", "tokens": ["Und", "auch", "du