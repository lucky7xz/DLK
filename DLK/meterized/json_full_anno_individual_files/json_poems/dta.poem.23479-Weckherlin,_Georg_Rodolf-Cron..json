{"dta.poem.23479": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Georg-Friderich ist der sterb-", "tokens": ["Ge\u00b7or\u00b7gFri\u00b7de\u00b7rich", "ist", "der", "ster\u00b7b"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "TRUNC"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "lichen ehr/", "tokens": ["li\u00b7chen", "ehr", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Vnd von den G\u00f6ttern selbs geehret/", "tokens": ["Vnd", "von", "den", "G\u00f6t\u00b7tern", "selbs", "ge\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welche die Welt durch seines lebens lehr", "tokens": ["Wel\u00b7che", "die", "Welt", "durch", "sei\u00b7nes", "le\u00b7bens", "lehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Aller jhrer reichtumb gewehret.", "tokens": ["Al\u00b7ler", "jhrer", "reich\u00b7tumb", "ge\u00b7weh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dan Er ist ja an Tugenten so reich/", "tokens": ["Dan", "Er", "ist", "ja", "an", "Tu\u00b7gen\u00b7ten", "so", "reich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "APPR", "NN", "ADV", "ADJD", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Das wan sein Land seinem verdienst w\u00e4r", "tokens": ["Das", "wan", "sein", "Land", "sei\u00b7nem", "ver\u00b7dienst", "w\u00e4r"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PPOSAT", "VVPP", "VAFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "gleich/", "tokens": ["gleich", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "So w\u00fcrd man Mich darin aufgehen/", "tokens": ["So", "w\u00fcrd", "man", "Mich", "da\u00b7rin", "auf\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vnd wider\u2014nider\u2014gehen sehen.", "tokens": ["Vnd", "wi\u00b7der", "ni\u00b7der", "ge\u00b7hen", "se\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$(", "PTKVZ", "$(", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Aber der G\u00f6tter aigne hand/", "tokens": ["A\u00b7ber", "der", "G\u00f6t\u00b7ter", "aig\u00b7ne", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Die seine Sehl also g\u00f6tlich bereichet/", "tokens": ["Die", "sei\u00b7ne", "Sehl", "al\u00b7so", "g\u00f6t\u00b7lich", "be\u00b7rei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Beraitet Jhm in Jhrem Land", "tokens": ["Be\u00b7rai\u00b7tet", "Jhm", "in", "Ih\u00b7rem", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ein solche Cron/ deren der Vmbkrai\u00df wei-", "tokens": ["Ein", "sol\u00b7che", "Cron", "/", "de\u00b7ren", "der", "Vmb\u00b7krai\u00df", "wei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "$(", "PDS", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "chet:", "tokens": ["chet", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.16": {"text": "Doch soll seine stirn wehrt vnd wei\u00df", "tokens": ["Doch", "soll", "sei\u00b7ne", "stirn", "wehrt", "vnd", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Noch so bald nicht damit geer\u00f6net werden/", "tokens": ["Noch", "so", "bald", "nicht", "da\u00b7mit", "gee\u00b7r\u00f6\u00b7net", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKNEG", "PAV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Auf das Erl\u00e4nger bleib auf erden", "tokens": ["Auf", "das", "Er\u00b7l\u00e4n\u00b7ger", "bleib", "auf", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Diser Welt lieb/ hofnung/ wollust vnd prei\u00df.", "tokens": ["Di\u00b7ser", "Welt", "lieb", "/", "hof\u00b7nung", "/", "wol\u00b7lust", "vnd", "prei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "$(", "NN", "$(", "VMFIN", "KON", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}