{"dta.poem.12808": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als  \n Herr  D.  Joh. Franciscus Buddeus  \n das Academische Regiment wieder  \n niederlegte.  \n Jm namen Herr  M.  Uhles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So fallt, ihr schwestern! mir denn bey:", "tokens": ["So", "fallt", ",", "ihr", "schwes\u00b7tern", "!", "mir", "denn", "bey", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VVINF", "$.", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Buddeus klug im lehren und auch im regieren sey.", "tokens": ["Da\u00df", "Bud\u00b7deus", "klug", "im", "leh\u00b7ren", "und", "auch", "im", "re\u00b7gie\u00b7ren", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADJD", "APPRART", "VVINF", "KON", "ADV", "APPRART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Das gantze Saal-Athen", "tokens": ["Das", "gant\u00b7ze", "Saa\u00b7lA\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Will selber seinen ruhm erh\u00f6hn.", "tokens": ["Will", "sel\u00b7ber", "sei\u00b7nen", "ruhm", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein scepter hat die bosheit nur gedr\u00fccket.", "tokens": ["Sein", "scep\u00b7ter", "hat", "die", "bos\u00b7heit", "nur", "ge\u00b7dr\u00fc\u00b7cket", "."], "token