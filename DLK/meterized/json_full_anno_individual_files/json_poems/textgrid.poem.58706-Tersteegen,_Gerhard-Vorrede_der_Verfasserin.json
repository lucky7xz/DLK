{"textgrid.poem.58706": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede der Verfasserin", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesen Bl\u00e4ttern wird der gro\u00dfe Unterscheid", "tokens": ["In", "die\u00b7sen", "Bl\u00e4t\u00b7tern", "wird", "der", "gro\u00b7\u00dfe", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der falschen Weltlieb' von der reinen Lieb' gezeiget;", "tokens": ["Der", "fal\u00b7schen", "Welt\u00b7lieb'", "von", "der", "rei\u00b7nen", "Lieb'", "ge\u00b7zei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie selig ist ein Mensch, der in Abh\u00e4ngigkeit", "tokens": ["Wie", "se\u00b7lig", "ist", "ein", "Mensch", ",", "der", "in", "Ab\u00b7h\u00e4n\u00b7gig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von Gottes Liebe lebt \u2013 nichts ihn beschwert noch beuget!", "tokens": ["Von", "Got\u00b7tes", "Lie\u00b7be", "lebt", "\u2013", "nichts", "ihn", "be\u00b7schwert", "noch", "beu\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$(", "PIS", "PPER", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die gr\u00f6\u00dften Schmerzen selbst uns s\u00fc\u00df und lieblich sein,", "tokens": ["Die", "gr\u00f6\u00df\u00b7ten", "Schmer\u00b7zen", "selbst", "uns", "s\u00fc\u00df", "und", "lieb\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PPER", "ADJD", "KON", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn man sie williglich und Gott zuliebe tr\u00e4get;", "tokens": ["Wenn", "man", "sie", "wil\u00b7lig\u00b7lich", "und", "Gott", "zu\u00b7lie\u00b7be", "tr\u00e4\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "KON", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie so erg\u00f6tzlich ist die gr\u00f6\u00dfte Qual und Pein,", "tokens": ["Wie", "so", "er\u00b7g\u00f6tz\u00b7lich", "ist", "die", "gr\u00f6\u00df\u00b7te", "Qual", "und", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo diese sch\u00f6ne Glut im Herzen wird geheget!", "tokens": ["Wo", "die\u00b7se", "sch\u00f6\u00b7ne", "Glut", "im", "Her\u00b7zen", "wird", "ge\u00b7he\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die L\u00fcste dieser Welt sind voller Bitterkeit,", "tokens": ["Die", "L\u00fcs\u00b7te", "die\u00b7ser", "Welt", "sind", "vol\u00b7ler", "Bit\u00b7ter\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man ist gest\u00f6rt, verwirrt und immer wie im Treiben,", "tokens": ["Man", "ist", "ge\u00b7st\u00f6rt", ",", "ver\u00b7wirrt", "und", "im\u00b7mer", "wie", "im", "Trei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$,", "ADJD", "KON", "ADV", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man schicke sich aufs best', man suche nah und weit,", "tokens": ["Man", "schi\u00b7cke", "sich", "aufs", "best'", ",", "man", "su\u00b7che", "nah", "und", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPRART", "VVFIN", "$,", "PIS", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Herze wird beschwert und in der Klemme bleiben.", "tokens": ["Das", "Her\u00b7ze", "wird", "be\u00b7schwert", "und", "in", "der", "Klem\u00b7me", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "VVPP", "KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur du, o Gotteslieb', gibst uns der Freuden F\u00fcll',", "tokens": ["Nur", "du", ",", "o", "Got\u00b7tes\u00b7lieb'", ",", "gibst", "uns", "der", "Freu\u00b7den", "F\u00fcll'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du machst einf\u00e4ltig, sanft und friedsam unsre Herzen;", "tokens": ["Du", "machst", "ein\u00b7f\u00e4l\u00b7tig", ",", "sanft", "und", "fried\u00b7sam", "uns\u00b7re", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wer der falschen Lieb' zum Raub sich lassen will,", "tokens": ["Doch", "wer", "der", "fal\u00b7schen", "Lieb'", "zum", "Raub", "sich", "las\u00b7sen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "APPRART", "NN", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fchlt mitten in der Freud' im Grunde nichts als Schmerzen.", "tokens": ["F\u00fchlt", "mit\u00b7ten", "in", "der", "Freud'", "im", "Grun\u00b7de", "nichts", "als", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Den gro\u00dfen Unterschied la\u00dft uns denn hier besehn,", "tokens": ["Den", "gro\u00b7\u00dfen", "Un\u00b7ter\u00b7schied", "la\u00dft", "uns", "denn", "hier", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob man Gott angeh\u00f6rt, ob man ein Knecht der S\u00fcnden,", "tokens": ["Ob", "man", "Gott", "an\u00b7ge\u00b7h\u00f6rt", ",", "ob", "man", "ein", "Knecht", "der", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "VVPP", "$,", "KOUS", "PIS", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da jene ihren Weg in heil'ger Unschuld gehn,", "tokens": ["Da", "je\u00b7ne", "ih\u00b7ren", "Weg", "in", "heil'\u00b7ger", "Un\u00b7schuld", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist ihr Herz abgesp\u00e4nt von dem, was hier zu finden!", "tokens": ["Ist", "ihr", "Herz", "ab\u00b7ge\u00b7sp\u00e4nt", "von", "dem", ",", "was", "hier", "zu", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "APPR", "ART", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ein Sklav' der S\u00fcnden wird von dem Tyrann geplagt,", "tokens": ["Ein", "Skla\u00b7v'", "der", "S\u00fcn\u00b7den", "wird", "von", "dem", "Ty\u00b7rann", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+---+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er hat wohl nimmer Ruh, die beste Freud' ist Qu\u00e4len;", "tokens": ["Er", "hat", "wohl", "nim\u00b7mer", "Ruh", ",", "die", "bes\u00b7te", "Freud'", "ist", "Qu\u00e4\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN", "$,", "ART", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit Ekel und Verdru\u00df die b\u00f6se Lust ihn nagt,", "tokens": ["Mit", "E\u00b7kel", "und", "Ver\u00b7dru\u00df", "die", "b\u00f6\u00b7se", "Lust", "ihn", "nagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er lieget wie im Schlamm der Laster, nicht zu z\u00e4hlen.", "tokens": ["Er", "lie\u00b7get", "wie", "im", "Schlamm", "der", "Las\u00b7ter", ",", "nicht", "zu", "z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN", "ART", "NN", "$,", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur meines Gottes Sklav' sich stets h\u00f6chst frei befind't,", "tokens": ["Nur", "mei\u00b7nes", "Got\u00b7tes", "Skla\u00b7v'", "sich", "stets", "h\u00f6chst", "frei", "be\u00b7find't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "PRF", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nichts macht sein Herz bedr\u00e4ngt und 's wie in 'n Kerker bringet;", "tokens": ["Nichts", "macht", "sein", "Herz", "be\u00b7dr\u00e4ngt", "und", "'s", "wie", "in", "'n", "Ker\u00b7ker", "brin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "VVFIN", "KON", "PPER", "KOKOM", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Die reine Liebe ihn befreiet von der S\u00fcnd',", "tokens": ["Die", "rei\u00b7ne", "Lie\u00b7be", "ihn", "be\u00b7frei\u00b7et", "von", "der", "S\u00fcnd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von b\u00f6ser Sucht und Lust, die aus der H\u00f6ll' entspringet.", "tokens": ["Von", "b\u00f6\u00b7ser", "Sucht", "und", "Lust", ",", "die", "aus", "der", "H\u00f6ll'", "ent\u00b7sprin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Drum folgen wir getrost der reinen Liebe Spur,", "tokens": ["Drum", "fol\u00b7gen", "wir", "ge\u00b7trost", "der", "rei\u00b7nen", "Lie\u00b7be", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschm\u00e4hn die falsche Lieb' und ihr vermeint' Vergn\u00fcgen,", "tokens": ["Ver\u00b7schm\u00e4hn", "die", "fal\u00b7sche", "Lieb'", "und", "ihr", "ver\u00b7meint'", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir hassen ihren Weg, verleugnen die Natur;", "tokens": ["Wir", "has\u00b7sen", "ih\u00b7ren", "Weg", ",", "ver\u00b7leug\u00b7nen", "die", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die reine Liebe wird sie dermaleinst besiegen.", "tokens": ["Die", "rei\u00b7ne", "Lie\u00b7be", "wird", "sie", "der\u00b7ma\u00b7leinst", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ja, liebste Liebe, dir, nur dir der Sieg geb\u00fchrt,", "tokens": ["Ja", ",", "liebs\u00b7te", "Lie\u00b7be", ",", "dir", ",", "nur", "dir", "der", "Sieg", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "PPER", "$,", "ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir k\u00f6nnen nichts aus uns, als dir nur widerstreben;", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nichts", "aus", "uns", ",", "als", "dir", "nur", "wi\u00b7der\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "APPR", "PPER", "$,", "KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil man denn nur durch dich, o Liebe, triumphiert,", "tokens": ["Weil", "man", "denn", "nur", "durch", "dich", ",", "o", "Lie\u00b7be", ",", "tri\u00b7um\u00b7phiert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df man dir auch ganz und alle Ehre geben.", "tokens": ["So", "mu\u00df", "man", "dir", "auch", "ganz", "und", "al\u00b7le", "Eh\u00b7re", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "ADV", "ADV", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "In diesen Bl\u00e4ttern wird der gro\u00dfe Unterscheid", "tokens": ["In", "die\u00b7sen", "Bl\u00e4t\u00b7tern", "wird", "der", "gro\u00b7\u00dfe", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der falschen Weltlieb' von der reinen Lieb' gezeiget;", "tokens": ["Der", "fal\u00b7schen", "Welt\u00b7lieb'", "von", "der", "rei\u00b7nen", "Lieb'", "ge\u00b7zei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie selig ist ein Mensch, der in Abh\u00e4ngigkeit", "tokens": ["Wie", "se\u00b7lig", "ist", "ein", "Mensch", ",", "der", "in", "Ab\u00b7h\u00e4n\u00b7gig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von Gottes Liebe lebt \u2013 nichts ihn beschwert noch beuget!", "tokens": ["Von", "G