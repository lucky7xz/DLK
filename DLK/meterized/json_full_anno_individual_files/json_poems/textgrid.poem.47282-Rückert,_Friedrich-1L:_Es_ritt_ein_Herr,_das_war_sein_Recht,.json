+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geendet ist des Herren Lauf.", "tokens": ["Ge\u00b7en\u00b7det", "ist", "des", "Her\u00b7ren", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er spricht nicht mehr: \u00bbRo\u00df hin, Ro\u00df her!\u00ab", "tokens": ["Er", "spricht", "nicht", "mehr", ":", "\u00bb", "Ro\u00df", "hin", ",", "Ro\u00df", "her", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$.", "$(", "NN", "PTKVZ", "$,", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Er rafft sich auf und schreitet schwer", "tokens": ["Er", "rafft", "sich", "auf", "und", "schrei\u00b7tet", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit seinem Knecht einher.", "tokens": ["Mit", "sei\u00b7nem", "Knecht", "ein\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}