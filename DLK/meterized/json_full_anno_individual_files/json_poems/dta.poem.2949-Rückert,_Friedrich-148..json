{"dta.poem.2949": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "148.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer lehrt der jungen Schwalb' im Nest die Fliege kennen,", "tokens": ["Wer", "lehrt", "der", "jun\u00b7gen", "Schwalb'", "im", "Nest", "die", "Flie\u00b7ge", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach deren Raub sie soll beschwingt die Luft durchrennen?", "tokens": ["Nach", "de\u00b7ren", "Raub", "sie", "soll", "be\u00b7schwingt", "die", "Luft", "durch\u00b7ren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VMFIN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Mutter bringt dem Kind die Beute, die sie haschte,", "tokens": ["Die", "Mut\u00b7ter", "bringt", "dem", "Kind", "die", "Beu\u00b7te", ",", "die", "sie", "haschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und es sieht nicht, was es vom Mutterschnabel naschte.", "tokens": ["Und", "es", "sieht", "nicht", ",", "was", "es", "vom", "Mut\u00b7ter\u00b7schna\u00b7bel", "naschte", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Die Schwalbe kann nicht so zum Futter ihre Brut", "tokens": ["Die", "Schwal\u00b7be", "kann", "nicht", "so", "zum", "Fut\u00b7ter", "ih\u00b7re", "Brut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anf\u00fchren, wie die Henn' im H\u00fcnerhofe thut.", "tokens": ["An\u00b7f\u00fch\u00b7ren", ",", "wie", "die", "Henn'", "im", "H\u00fc\u00b7ner\u00b7ho\u00b7fe", "thut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie mu\u00df dem Trieb vertraun, und l\u00e4sset ihn gew\u00e4hren,", "tokens": ["Sie", "mu\u00df", "dem", "Trieb", "ver\u00b7traun", ",", "und", "l\u00e4s\u00b7set", "ihn", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,", "KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der einst ihr fl\u00fcckes Kind wird treiben sich zu n\u00e4hren.", "tokens": ["Der", "einst", "ihr", "fl\u00fc\u00b7ckes", "Kind", "wird", "trei\u00b7ben", "sich", "zu", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "VAFIN", "VVINF", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}