{"textgrid.poem.47824": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Blumen", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie kommen aus dem Scho\u00df der Nacht;", "tokens": ["Sie", "kom\u00b7men", "aus", "dem", "Scho\u00df", "der", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch w\u00e4ren unten sie geblieben,", "tokens": ["Doch", "w\u00e4\u00b7ren", "un\u00b7ten", "sie", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn nicht das Licht mit seiner Macht", "tokens": ["Wenn", "nicht", "das", "Licht", "mit", "sei\u00b7ner", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinauf ins Leben sie getrieben.", "tokens": ["Hin\u00b7auf", "ins", "Le\u00b7ben", "sie", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Holdselig aus der Erde bricht's", "tokens": ["Hold\u00b7se\u00b7lig", "aus", "der", "Er\u00b7de", "bricht's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NE"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Und bl\u00fcht nun \u00fcber alle Schranken;", "tokens": ["Und", "bl\u00fcht", "nun", "\u00fc\u00b7ber