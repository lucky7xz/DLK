{"dta.poem.5403": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Balsamina.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch hat mir die so s\u00fc\u00df', als holde, Pracht", "tokens": ["Auch", "hat", "mir", "die", "so", "s\u00fc\u00df'", ",", "als", "hol\u00b7de", ",", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADV", "ADJD", "$,", "KOUS", "ADJA", "$,", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der lieblich wei\u00df- und roth-gemischten Balsaminen,", "tokens": ["Der", "lieb\u00b7lich", "wei\u00df", "und", "ro\u00b7th\u00b7ge\u00b7mischten", "Bal\u00b7sa\u00b7mi\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zum Preise de\u00df, der sie gemacht,", "tokens": ["Zum", "Prei\u00b7se", "de\u00df", ",", "der", "sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4ngst der Betrachtung wehrt geschienen.", "tokens": ["L\u00e4ngst", "der", "Be\u00b7trach\u00b7tung", "wehrt", "ge\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Jhr ist im bunten Blumen-Reich,", "tokens": ["Ihr", "ist", "im", "bun\u00b7ten", "Blu\u00b7men\u00b7Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So wohl an Form\u2019, als Farbe, keine gleich.", "tokens": ["So", "wohl", "an", "Form'", ",", "als", "Far\u00b7be", ",", "kei\u00b7ne", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NE", "$,", "KOUS", "NN", "$,", "PIAT", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie scheint, wenn wir sie Anfangs sehn,", "tokens": ["Sie", "scheint", ",", "wenn", "wir", "sie", "An\u00b7fangs", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus mehrern Vl\u00e4ttern zu bestehn;", "tokens": ["Aus", "meh\u00b7rern", "Vl\u00e4t\u00b7tern", "zu", "be\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch schauen wir sie recht; bestehet die Figur", "tokens": ["Doch", "schau\u00b7en", "wir", "sie", "recht", ";", "be\u00b7ste\u00b7het", "die", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADJD", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Blos aus vier Bl\u00e4ttern nur,", "tokens": ["Blos", "aus", "vier", "Bl\u00e4t\u00b7tern", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die aber so verwunderlich verschrenckt,", "tokens": ["Die", "a\u00b7ber", "so", "ver\u00b7wun\u00b7der\u00b7lich", "ver\u00b7schrenckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Geformet und geordnet sitzen,", "tokens": ["Ge\u00b7for\u00b7met", "und", "ge\u00b7ord\u00b7net", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df es kein Mensch gedenckt,", "tokens": ["Da\u00df", "es", "kein", "Mensch", "ge\u00b7denckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Der nicht, mit Achtsamkeit, die Augen auf sie senckt;", "tokens": ["Der", "nicht", ",", "mit", "Acht\u00b7sam\u00b7keit", ",", "die", "Au\u00b7gen", "auf", "sie", "senckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "$,", "APPR", "NN", "$,", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da sie denn in der That", "tokens": ["Da", "sie", "denn", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "Fast die Figur von einer Gie\u00df-Kann\u2019 hat.", "tokens": ["Fast", "die", "Fi\u00b7gur", "von", "ei\u00b7ner", "Gie\u00df\u00b7Kann'", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.17": {"text": "An eines rothen Stengels Spitzen", "tokens": ["An", "ei\u00b7nes", "ro\u00b7then", "Sten\u00b7gels", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Sitzt erst ein breites Blatt,", "tokens": ["Sitzt", "erst", "ein", "brei\u00b7tes", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Das \u00fcberall sonst platt und glatt,", "tokens": ["Das", "\u00fc\u00b7be\u00b7rall", "sonst", "platt", "und", "glatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Doch oben, wo es sich sanft auszuh\u00f6hlen pflegt,", "tokens": ["Doch", "o\u00b7ben", ",", "wo", "es", "sich", "sanft", "aus\u00b7zu\u00b7h\u00f6h\u00b7len", "pflegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ein kleines gr\u00fcnes Spitzgen tr\u00e4gt;", "tokens": ["Ein", "klei\u00b7nes", "gr\u00fc\u00b7nes", "Spitz\u00b7gen", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "An dessen Fu\u00df erscheint der andern Bl\u00e4tter Par,", "tokens": ["An", "des\u00b7sen", "Fu\u00df", "er\u00b7scheint", "der", "an\u00b7dern", "Bl\u00e4t\u00b7ter", "Par", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVFIN", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die in der Mitten", "tokens": ["Die", "in", "der", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Nat\u00fcrlich schienen ausgeschnitten,", "tokens": ["Na\u00b7t\u00fcr\u00b7lich", "schie\u00b7nen", "aus\u00b7ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Und welche man so lieblich ausger\u00fcndet,", "tokens": ["Und", "wel\u00b7che", "man", "so", "lieb\u00b7lich", "aus\u00b7ge\u00b7r\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PIS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "So nett gebogen findet,", "tokens": ["So", "nett", "ge\u00b7bo\u00b7gen", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Da\u00df es ein offnes Hertz formirt,", "tokens": ["Da\u00df", "es", "ein", "off\u00b7nes", "Hertz", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "In welchem sich der Blick verliehrt,", "tokens": ["In", "wel\u00b7chem", "sich", "der", "Blick", "ver\u00b7liehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und sich in eine Tieffe f\u00fchrt,", "tokens": ["Und", "sich", "in", "ei\u00b7ne", "Tief\u00b7fe", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}