{"dta.poem.859": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlVI.  Auf den Sontag de\u00df Rechnung for-  \n dernden Hau\u00df Vatters/ oder  VIII.  Sontag  \n nach dem Fest der H. Dreyeinigkeit.  \n Luc. 16.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "HeRR! aller Herren h\u00f6chster Gott/ wo werd ich armer vor ", "tokens": ["HeRR", "!", "al\u00b7ler", "Her\u00b7ren", "h\u00f6chs\u00b7ter", "Gott", "/", "wo", "werd", "ich", "ar\u00b7mer", "vor"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PIAT", "NN", "ADJA", "NN", "$(", "PWAV", "VAFIN", "PPER", "ADJD", "PTKVZ"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "dir bleiben! ", "tokens": ["dir", "blei\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ich dein durchau\u00df vnn\u00fctzer Knecht: Mein Hertz erzittert", "tokens": ["Ich", "dein", "durch\u00b7au\u00df", "vn\u00b7n\u00fct\u00b7zer", "Knecht", ":", "Mein", "Hertz", "er\u00b7zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADV", "ADJA", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Tag vnd Nacht. ", "tokens": ["Tag", "vnd", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Weil mir das ernste donnerwortt durch Ohr vnd Mutt/", "tokens": ["Weil", "mir", "das", "erns\u00b7te", "don\u00b7ner\u00b7wortt", "durch", "Ohr", "vnd", "Mutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "vnd Geister kracht. ", "tokens": ["vnd", "Geis\u00b7ter", "kracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Thu Rechnung Mensch/ von Leib vnd Geist/ von reden/ le-", "tokens": ["Thu", "Rech\u00b7nung", "Mensch", "/", "von", "Leib", "vnd", "Geist", "/", "von", "re\u00b7den", "/", "le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "NN", "NN", "$(", "APPR", "NN", "KON", "NN", "$(", "APPR", "VVINF", "$(", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "sen/ thun vnd schreiben. ", "tokens": ["sen", "/", "thun", "vnd", "schrei\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVINF", "$(", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Wen solte nicht dein harter Spruch in de\u00df verzweifelns Ab-", "tokens": ["Wen", "sol\u00b7te", "nicht", "dein", "har\u00b7ter", "Spruch", "in", "de\u00df", "ver\u00b7zwei\u00b7felns", "Ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "grund treiben! ", "tokens": ["grund", "trei\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch wann/ du/ der in Knechts gestalt volkommen hat", "tokens": ["Doch", "wann", "/", "du", "/", "der", "in", "Knechts", "ge\u00b7stalt", "vol\u00b7kom\u00b7men", "hat"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$(", "PPER", "$(", "ART", "APPR", "NN", "ADJD", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sein Ampt vollbracht ", "tokens": ["sein", "Ampt", "voll\u00b7bracht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mir dein genung-thunselbst anbeutst/ wird wenn der heisse", "tokens": ["Mir", "dein", "ge\u00b7nung\u00b7thun\u00b7selbst", "an\u00b7beutst", "/", "wird", "wenn", "der", "heis\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$(", "VAFIN", "KOUS", "ART", "NN"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "zorn erwacht ", "tokens": ["zorn", "er\u00b7wacht"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sich der so scharffen Kl\u00e4ger Schaar vmbsonst an diese", "tokens": ["Sich", "der", "so", "scharf\u00b7fen", "Kl\u00e4\u00b7ger", "Schaar", "vmbsonst", "an", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "ADV", "ADJA", "NN", "NN", "ADV", "APPR", "PDAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Seele reiben. ", "tokens": ["See\u00b7le", "rei\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Waar ists/ da\u00df ich de\u00df H\u00f6chsten Gutt gar vnbedacht-", "tokens": ["Waar", "ists", "/", "da\u00df", "ich", "de\u00df", "H\u00f6chs\u00b7ten", "Gutt", "gar", "vn\u00b7be\u00b7dacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$(", "KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "samb hier verschwendt ", "tokens": ["samb", "hier", "ver\u00b7schwendt"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "VVPP"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Doch schaw' ich Licht in dieser Nacht/ das alle tr\u00fcbe Wol- ", "tokens": ["Doch", "scha\u00b7w'", "ich", "Licht", "in", "die\u00b7ser", "Nacht", "/", "das", "al\u00b7le", "tr\u00fc\u00b7be", "Wol"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PDAT", "NN", "$(", "ART", "PIAT", "ADJA", "TRUNC"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "cken treun't. ", "tokens": ["cken", "treun'", "t."], "token_info": ["word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wenn dein genungthun/ was mir fehlt/ Mein s\u00fcsser ", "tokens": ["Wenn", "dein", "ge\u00b7nung\u00b7thun", "/", "was", "mir", "fehlt", "/", "Mein", "s\u00fcs\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "VVINF", "$(", "PWS", "PPER", "VVFIN", "$(", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "wil ersetzen. ", "tokens": ["wil", "er\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Gib vnter dessen/ gib da\u00df ich dein Gutt so anwend in der", "tokens": ["Gib", "vn\u00b7ter", "des\u00b7sen", "/", "gib", "da\u00df", "ich", "dein", "Gutt", "so", "an\u00b7wend", "in", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "PRELAT", "$(", "VVIMP", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "ART"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Welt ", "tokens": ["Welt"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Da\u00df wenn der abgelebte Leib hier nicht mehr Hau\u00df auf", "tokens": ["Da\u00df", "wenn", "der", "ab\u00b7ge\u00b7leb\u00b7te", "Leib", "hier", "nicht", "mehr", "Hau\u00df", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "ADV", "PTKNEG", "PIAT", "NN", "APPR"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Erden h\u00e4lt ", "tokens": ["Er\u00b7den", "h\u00e4lt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der Geist sich f\u00fcr vnd f\u00fcr bey dir in ew\u2019ger H\u00fctten m\u00f6g er-", "tokens": ["Der", "Geist", "sich", "f\u00fcr", "vnd", "f\u00fcr", "bey", "dir", "in", "ew'\u00b7ger", "H\u00fct\u00b7ten", "m\u00f6g", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "APPR", "KON", "APPR", "APPR", "PPER", "APPR", "ADJA", "NN", "VMFIN", "TRUNC"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "getzen. ", "tokens": ["get\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}