{"textgrid.poem.33343": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Ode an den Leibstuhl", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kleiner Sitz, von dessen eig'nem Namen", "tokens": ["Du", "klei\u00b7ner", "Sitz", ",", "von", "des\u00b7sen", "eig'\u00b7nem", "Na\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man mit Respekt nur spricht,", "tokens": ["Man", "mit", "Res\u00b7pekt", "nur", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den t\u00e4glich doch die eckelste der Damen", "tokens": ["Den", "t\u00e4g\u00b7lich", "doch", "die", "ec\u00b7kels\u00b7te", "der", "Da\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "ART", "ADJA", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Besieht und f\u00fchlt und riecht.", "tokens": ["Be\u00b7sieht", "und", "f\u00fchlt", "und", "riecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du bist der gr\u00f6\u00dfte aller Opferherde,", "tokens": ["Du", "bist", "der", "gr\u00f6\u00df\u00b7te", "al\u00b7ler", "Op\u00b7fer\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf deinem Altar nur", "tokens": ["Auf", "dei\u00b7nem", "Al\u00b7tar", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zollt t\u00e4glich der galant're Theil der Erde", "tokens": ["Zollt", "t\u00e4g\u00b7lich", "der", "ga\u00b7lant'\u00b7re", "Theil", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Opfer der Natur.", "tokens": ["Sein", "Op\u00b7fer", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du bist der G\u00f6tze, der selbst Majest\u00e4ten", "tokens": ["Du", "bist", "der", "G\u00f6t\u00b7ze", ",", "der", "selbst", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Hinterhaupt entbl\u00f6\u00dft,", "tokens": ["Ihr", "Hin\u00b7ter\u00b7haupt", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Freund, vor dem sogar sich ohn' Errothen", "tokens": ["Der", "Freund", ",", "vor", "dem", "so\u00b7gar", "sich", "ohn'", "Er\u00b7ro\u00b7then"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADV", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Nonne sehen l\u00e4\u00dft.", "tokens": ["Die", "Non\u00b7ne", "se\u00b7hen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Erhaben setzt, wie auf den Sitz der G\u00f6tter,", "tokens": ["Er\u00b7ha\u00b7ben", "setzt", ",", "wie", "auf", "den", "Sitz", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Weise sich auf dich,", "tokens": ["Der", "Wei\u00b7se", "sich", "auf", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieht stolz herab, und l\u00e4\u00dft das Donnerwetter", "tokens": ["Sieht", "stolz", "her\u00b7ab", ",", "und", "l\u00e4\u00dft", "das", "Don\u00b7ner\u00b7wet\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Laut krachen unter sich.", "tokens": ["Laut", "kra\u00b7chen", "un\u00b7ter", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPR", "PRF", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du bist das wahre Ebenbild der Thronen", "tokens": ["Du", "bist", "das", "wah\u00b7re", "E\u00b7ben\u00b7bild", "der", "Thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf diesem Erbrevier;", "tokens": ["Auf", "die\u00b7sem", "Er\u00b7bre\u00b7vier", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Denn immer sitzt von vielen Millionen", "tokens": ["Denn", "im\u00b7mer", "sitzt", "von", "vie\u00b7len", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Einziger auf dir.", "tokens": ["Ein", "Ein\u00b7zi\u00b7ger", "auf", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du bist's allein, den Prunk und Etikette", "tokens": ["Du", "bist's", "al\u00b7lein", ",", "den", "Prunk", "und", "E\u00b7ti\u00b7ket\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Selbst mehr als Thronen ziert,", "tokens": ["Selbst", "mehr", "als", "Thro\u00b7nen", "ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn sag', bei welchem Thron' wird so zur Wette,", "tokens": ["Denn", "sag'", ",", "bei", "wel\u00b7chem", "Thron'", "wird", "so", "zur", "Wet\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PWAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als wie bei dir, hofirt?", "tokens": ["Als", "wie", "bei", "dir", ",", "ho\u00b7firt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Worin jedoch aus allen Sorgest\u00fchlen", "tokens": ["Wo\u00b7rin", "je\u00b7doch", "aus", "al\u00b7len", "Sor\u00b7ge\u00b7st\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein einziger dir gleicht,", "tokens": ["Kein", "ein\u00b7zi\u00b7ger", "dir", "gleicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ist dies: auf Thronen sitzt man oft sich Schw\u00fclen,", "tokens": ["Ist", "dies", ":", "auf", "Thro\u00b7nen", "sitzt", "man", "oft", "sich", "Schw\u00fc\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "$.", "APPR", "NN", "VVFIN", "PIS", "ADV", "PRF", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf dir sitzt man sich leicht.", "tokens": ["Auf", "dir", "sitzt", "man", "sich", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PRF", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Du beutst als Freund den Menschen hier auf Erden", "tokens": ["Du", "beutst", "als", "Freund", "den", "Men\u00b7schen", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gef\u00e4llig deinen Schoos,", "tokens": ["Ge\u00b7f\u00e4l\u00b7lig", "dei\u00b7nen", "Schoos", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und machest von den dr\u00fcckendsten Beschwerden", "tokens": ["Und", "ma\u00b7chest", "von", "den", "dr\u00fc\u00b7ckends\u00b7ten", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Menschlichkeit sie los.", "tokens": ["Der", "Menschlich\u00b7keit", "sie", "los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Zu dir wallfahrten gro\u00df' und kleine Geister,", "tokens": ["Zu", "dir", "wall\u00b7fahr\u00b7ten", "gro\u00df'", "und", "klei\u00b7ne", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie die Milzsucht qu\u00e4lt,", "tokens": ["Wenn", "sie", "die", "Milz\u00b7sucht", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du nimmst von ihnen weg den Seelenkleister,", "tokens": ["Du", "nimmst", "von", "ih\u00b7nen", "weg", "den", "See\u00b7len\u00b7kleis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der sie umnebelt h\u00e4lt.", "tokens": ["Der", "sie", "um\u00b7ne\u00b7belt", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Man sieht dich t\u00e4glich viele Wunder wirken,", "tokens": ["Man", "sieht", "dich", "t\u00e4g\u00b7lich", "vie\u00b7le", "Wun\u00b7der", "wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist der Ort, wohin", "tokens": ["Du", "bist", "der", "Ort", ",", "wo\u00b7hin"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "(so wie nach Mekka die bedr\u00e4ngten T\u00fcrken)", "tokens": ["(", "so", "wie", "nach", "Mek\u00b7ka", "die", "be\u00b7dr\u00e4ng\u00b7ten", "T\u00fcr\u00b7ken", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOKOM", "APPR", "NE", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die armen Kranken zieh'n.", "tokens": ["Die", "ar\u00b7men", "Kran\u00b7ken", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Du bist der Heilthumstuhl, an dem der Kranken", "tokens": ["Du", "bist", "der", "Heilt\u00b7hum\u00b7stuhl", ",", "an", "dem", "der", "Kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie fruchtlos Opfer zollt,", "tokens": ["Nie", "frucht\u00b7los", "Op\u00b7fer", "zollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil er daf\u00fcr gewi\u00df mit regem Danke", "tokens": ["Weil", "er", "da\u00b7f\u00fcr", "ge\u00b7wi\u00df", "mit", "re\u00b7gem", "Dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich die Genesung holt.", "tokens": ["Sich", "die", "Ge\u00b7ne\u00b7sung", "holt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Du bist der Chef, f\u00fcr den auf seinem Stuhle", "tokens": ["Du", "bist", "der", "Chef", ",", "f\u00fcr", "den", "auf", "sei\u00b7nem", "Stuh\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So mancher H** schwitzt,", "tokens": ["So", "man\u00b7cher", "H", "*", "*", "schwitzt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "PIAT", "XY", "XY", "XY", "VVFIN", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.3": {"text": "Der Gott, f\u00fcr den so manche Federspule", "tokens": ["Der", "Gott", ",", "f\u00fcr", "den", "so", "man\u00b7che", "Fe\u00b7der\u00b7spu\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Autors ab sich n\u00fctzt;", "tokens": ["Des", "Au\u00b7tors", "ab", "sich", "n\u00fctzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der Richterstuhl, wo \u00fcber die Gehirne", "tokens": ["Der", "Rich\u00b7ter\u00b7stuhl", ",", "wo", "\u00fc\u00b7ber", "die", "Ge\u00b7hir\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man streng Gerichte h\u00e4lt,", "tokens": ["Man", "streng", "Ge\u00b7rich\u00b7te", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Schlund, worein, gebrandmarkt an der Stirne,", "tokens": ["Der", "Schlund", ",", "wo\u00b7rein", ",", "ge\u00b7brand\u00b7markt", "an", "der", "Stir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So manches Wischchen f\u00e4llt.", "tokens": ["So", "man\u00b7ches", "Wi\u00b7schchen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "D'rum, da\u00df du mich dereinst nicht auch als Richter", "tokens": ["D'\u00b7rum", ",", "da\u00df", "du", "mich", "de\u00b7reinst", "nicht", "auch", "als", "Rich\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Verschlingst mit Haut und Haar,", "tokens": ["Ver\u00b7schlingst", "mit", "Haut", "und", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So bring' ich dir, du Erbfeind aller Dichter,", "tokens": ["So", "bring'", "ich", "dir", ",", "du", "Erb\u00b7feind", "al\u00b7ler", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dies Lied zum Opfer dar.", "tokens": ["Dies", "Lied", "zum", "Op\u00b7fer", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Du kleiner Sitz, von dessen eig'nem Namen", "tokens": ["Du", "klei\u00b7ner", "Sitz", ",", "von", "des\u00b7sen", "eig'\u00b7nem", "Na\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man mit Respekt nur spricht,", "tokens": ["Man", "mit", "Res\u00b7pekt", "nur", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den t\u00e4glich doch die eckelste der Damen", "tokens": ["Den", "t\u00e4g\u00b7lich", "doch", "die", "ec\u00b7kels\u00b7te", "der", "Da\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "ART", "ADJA", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Besieht und f\u00fchlt und riecht.", "tokens": ["Be\u00b7sieht", "und", "f\u00fchlt", "und", "riecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Du bist der gr\u00f6\u00dfte aller Opferherde,", "tokens": ["Du", "bist", "der", "gr\u00f6\u00df\u00b7te", "al\u00b7ler", "Op\u00b7fer\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf deinem Altar nur", "tokens": ["Auf", "dei\u00b7nem", "Al\u00b7tar", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zollt t\u00e4glich der galant're Theil der Erde", "tokens": ["Zollt", "t\u00e4g\u00b7lich", "der", "ga\u00b7lant'\u00b7re", "Theil", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Opfer der Natur.", "tokens": ["Sein", "Op\u00b7fer", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Du bist der G\u00f6tze, der selbst Majest\u00e4ten", "tokens": ["Du", "bist", "der", "G\u00f6t\u00b7ze", ",", "der", "selbst", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Hinterhaupt entbl\u00f6\u00dft,", "tokens": ["Ihr", "Hin\u00b7ter\u00b7haupt", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Freund, vor dem sogar si