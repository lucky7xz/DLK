{"dta.poem.7817": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Pegnesische  \n Fr\u00fclings-Freude.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Pinsel bildt den Leib/ der selbst die", "tokens": ["Kein", "Pin\u00b7sel", "bildt", "den", "Leib", "/", "der", "selbst", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$(", "ART", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kunst/ kan nichts allhier/ gibt der", "tokens": ["Die", "Kunst", "/", "kan", "nichts", "all\u00b7hier", "/", "gibt", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$(", "VMFIN", "PIS", "ADV", "$(", "VVFIN", "ART"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "die kaum ein sch\u00f6ners Werk auf Erden je", "tokens": ["die", "kaum", "ein", "sch\u00f6\u00b7ners", "Werk", "auf", "Er\u00b7den", "je"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Kiel/ auch seines Geists/ ob diesem", "tokens": ["Der", "Kiel", "/", "auch", "sei\u00b7nes", "Geists", "/", "ob", "die\u00b7sem"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NE", "$(", "ADV", "PPOSAT", "NN", "$(", "KOUS", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie soll die irdisch\u2019 Hand/ was himm-", "tokens": ["Wie", "soll", "die", "ir\u00b7disch'", "Hand", "/", "was", "him\u00b7m"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "PIAT", "NN", "$(", "PWS", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer G\u00f6tter mahlen will/ mu\u00df g\u00f6ttlichs", "tokens": ["Wer", "G\u00f6t\u00b7ter", "mah\u00b7len", "will", "/", "mu\u00df", "g\u00f6tt\u00b7lichs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "NN", "VVINF", "VMFIN", "$(", "VMFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}