{"textgrid.poem.47199": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Sommer war es mir ein Trost, mit Bl\u00fcten", "tokens": ["Im", "Som\u00b7mer", "war", "es", "mir", "ein", "Trost", ",", "mit", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPER", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gr\u00e4ber meiner Kinder zu umfloren;", "tokens": ["Die", "Gr\u00e4\u00b7ber", "mei\u00b7ner", "Kin\u00b7der", "zu", "um\u00b7flo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Neu glaubt' ich mir die bl\u00fchenden geboren,", "tokens": ["Neu", "glaubt'", "ich", "mir", "die", "bl\u00fc\u00b7hen\u00b7den", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sich die Knospen aufzubrechen m\u00fchten.", "tokens": ["Wenn", "sich", "die", "Knos\u00b7pen", "auf\u00b7zu\u00b7bre\u00b7chen", "m\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun aber bei des Winters strengem W\u00fcten", "tokens": ["Nun", "a\u00b7ber", "bei", "des", "Win\u00b7ters", "stren\u00b7gem", "W\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die zarten Fr\u00fchlingskinder sind erfroren,", "tokens": ["Die", "zar\u00b7ten", "Fr\u00fch\u00b7lings\u00b7kin\u00b7der", "sind", "er\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ging mir der s\u00fc\u00dfen T\u00e4uschung Spiel verloren,", "tokens": ["Ging", "mir", "der", "s\u00fc\u00b7\u00dfen", "T\u00e4u\u00b7schung", "Spiel", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Dichtung nur kann den Verlust verg\u00fcten.", "tokens": ["Und", "Dich\u00b7tung", "nur", "kann", "den", "Ver\u00b7lust", "ver\u00b7g\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Die Kinder meiner Wonne, meiner Schmerzen", "tokens": ["Die", "Kin\u00b7der", "mei\u00b7ner", "Won\u00b7ne", ",", "mei\u00b7ner", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind nicht begraben in der harten Erde,", "tokens": ["Sind", "nicht", "be\u00b7gra\u00b7ben", "in", "der", "har\u00b7ten", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie sind's in meinem weichen lockern Herzen;", "tokens": ["Sie", "sin\u00b7d's", "in", "mei\u00b7nem", "wei\u00b7chen", "lo\u00b7ckern", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Das wird zu einem Rosenfeuerherde,", "tokens": ["Das", "wird", "zu", "ei\u00b7nem", "Ro\u00b7sen\u00b7feu\u00b7er\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus welchem spr\u00fchn wie Flammen heil'ger Kerzen", "tokens": ["Aus", "wel\u00b7chem", "spr\u00fchn", "wie", "Flam\u00b7men", "heil'\u00b7ger", "Ker\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "VVINF", "KOKOM", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Trostlieder, die ich ziehn statt Lilien werde.", "tokens": ["Trost\u00b7lie\u00b7der", ",", "die", "ich", "ziehn", "statt", "Li\u00b7li\u00b7en", "wer\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "VVFIN", "APPR", "NE", "VAFIN", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Im Sommer war es mir ein Trost, mit Bl\u00fcten", "tokens": ["Im", "Som\u00b7mer", "war", "es", "mir", "ein", "Trost", ",", "mit", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPER", "ART", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text