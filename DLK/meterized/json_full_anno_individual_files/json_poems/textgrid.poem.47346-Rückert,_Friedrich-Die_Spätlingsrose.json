{"textgrid.poem.47346": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sp\u00e4tlingsrose", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe, Verzicht", "tokens": ["Sie\u00b7he", ",", "Ver\u00b7zicht"], "token_info": ["word", "punct", "word"], "pos": ["VVIMP", "$,", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wollt' ich nunmehr auf die Rosen leisten;", "tokens": ["Wollt'", "ich", "nun\u00b7mehr", "auf", "die", "Ro\u00b7sen", "leis\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hab' ich doch nicht,", "tokens": ["Hab'", "ich", "doch", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Weil sie mir bl\u00fchten, getr\u00e4umt wie die meisten.", "tokens": ["Weil", "sie", "mir", "bl\u00fch\u00b7ten", ",", "ge\u00b7tr\u00e4umt", "wie", "die", "meis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VVPP", "KOKOM", "ART", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Gl\u00fcckliches Los!", "tokens": ["Gl\u00fcck\u00b7li\u00b7ches", "Los", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Siehe, da ist noch ein Nachwuchs gekommen,", "tokens": ["Sie\u00b7he", ",", "da", "ist", "noch", "ein", "Nach\u00b7wuchs", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word"