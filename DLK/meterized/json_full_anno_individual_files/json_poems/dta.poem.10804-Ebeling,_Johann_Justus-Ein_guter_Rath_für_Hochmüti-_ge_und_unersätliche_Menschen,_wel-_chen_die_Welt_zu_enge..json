{"dta.poem.10804": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ein guter Rath f\u00fcr Hochm\u00fcti-  \n ge und uners\u00e4tliche Menschen, wel-  \n chen die Welt zu enge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Menschen die ihr diese Erde al-\nlein zu besizzen denkt,", "tokens": ["Men\u00b7schen", "die", "ihr", "die\u00b7se", "Er\u00b7de", "al", "lein", "zu", "be\u00b7siz\u00b7zen", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "PDAT", "NN", "TRUNC", "ADJD", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Eure Macht stets weiter breitet,", "tokens": ["Eu\u00b7re", "Macht", "stets", "wei\u00b7ter", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ihr gleich Alexander euch gar wollet unter-", "tokens": ["Die", "ihr", "gleich", "A\u00b7lex\u00b7an\u00b7der", "euch", "gar", "wol\u00b7let", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "NE", "PPER", "ADV", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn es m\u00f6glich hinterm Pol, neue Welten zu", "tokens": ["Wenn", "es", "m\u00f6g\u00b7lich", "hin\u00b7term", "Pol", ",", "neu\u00b7e", "Wel\u00b7ten", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "$,", "ADJA", "NN", "PTKZU"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Die eur Muth erobern k\u00f6nnte; Wie ist euch die", "tokens": ["Die", "eur", "Muth", "er\u00b7o\u00b7bern", "k\u00f6nn\u00b7te", ";", "Wie", "ist", "euch", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "VMFIN", "$.", "PWAV", "VAFIN", "PPER", "ART"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "K\u00f6nnt ihr da ihr Menschen heisset, wol allgegen-", "tokens": ["K\u00f6nnt", "ihr", "da", "ihr", "Men\u00b7schen", "heis\u00b7set", ",", "wol", "all\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Helden die ihr nimmer satt, bei der grossen Habsucht", "tokens": ["Hel\u00b7den", "die", "ihr", "nim\u00b7mer", "satt", ",", "bei", "der", "gros\u00b7sen", "Hab\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "PPER", "ADV", "ADJD", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Und bald hie, bald dorten hin, mit gespornten", "tokens": ["Und", "bald", "hie", ",", "bald", "dor\u00b7ten", "hin", ",", "mit", "ge\u00b7sporn\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADV", "$,", "ADV", "ADV", "PTKVZ", "$,", "APPR", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Stehet stille und bedenket, wie der Raum beschaf-", "tokens": ["Ste\u00b7het", "stil\u00b7le", "und", "be\u00b7den\u00b7ket", ",", "wie", "der", "Raum", "be\u00b7schaf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "KON", "VVFIN", "$,", "PWAV", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "Denn ihr damahls eingenommen, ehe die Mutter", "tokens": ["Denn", "ihr", "da\u00b7mahls", "ein\u00b7ge\u00b7nom\u00b7men", ",", "e\u00b7he", "die", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVPP", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Wie klein war der Wohnplaz nicht, da ihr als ge-", "tokens": ["Wie", "klein", "war", "der", "Wohn\u00b7plaz", "nicht", ",", "da", "ihr", "als", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "PTKNEG", "$,", "KOUS", "PPER", "KOUS", "TRUNC"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Zwischen Koth und Harn verstekt. O! ihr grossen", "tokens": ["Zwi\u00b7schen", "Koth", "und", "Harn", "vers\u00b7tekt", ".", "O", "!", "ihr", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "VVPP", "$.", "NE", "$.", "PPOSAT", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Wie gar bald ward die Begierde auch nach der Ge-", "tokens": ["Wie", "gar", "bald", "ward", "die", "Be\u00b7gier\u00b7de", "auch", "nach", "der", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "VAFIN", "ART", "NN", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+--+-+++-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Da ward ihr mit wenig Tropffen von den weissen", "tokens": ["Da", "ward", "ihr", "mit", "we\u00b7nig", "Tropf\u00b7fen", "von", "den", "weis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PIAT", "NN", "APPR", "ART", "ADJA"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Denket nach wie gros der Raum, denn ihr nach", "tokens": ["Den\u00b7ket", "nach", "wie", "gros", "der", "Raum", ",", "denn", "ihr", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "KOKOM", "ADJD", "ART", "NN", "$,", "KON", "PPER", "APPR"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Wenn die Nerven sind entspannt und die Seele aus-", "tokens": ["Wenn", "die", "Ner\u00b7ven", "sind", "ent\u00b7spannt", "und", "die", "See\u00b7le", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "KON", "ART", "NN", "TRUNC"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Alsdenn sind gar wenig Spannen, wenn das Le-", "tokens": ["Als\u00b7denn", "sind", "gar", "we\u00b7nig", "Span\u00b7nen", ",", "wenn", "das", "Le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "$,", "KOUS", "ART", "TRUNC"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Wo der K\u00f6rper kann verfaulen, nur zu euren Raum", "tokens": ["Wo", "der", "K\u00f6r\u00b7per", "kann", "ver\u00b7fau\u00b7len", ",", "nur", "zu", "eu\u00b7ren", "Raum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VMFIN", "VVINF", "$,", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.19": {"text": "Werdet ihr dies mit Vernunfft, als Vern\u00fcnfftige", "tokens": ["Wer\u00b7det", "ihr", "dies", "mit", "Ver\u00b7nunfft", ",", "als", "Ver\u00b7n\u00fcnff\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PDS", "APPR", "NN", "$,", "KOUS", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "So k\u00f6nnt ihr dadurch den Trieb eurer Habsucht", "tokens": ["So", "k\u00f6nnt", "ihr", "da\u00b7durch", "den", "Trieb", "eu\u00b7rer", "Hab\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}