{"textgrid.poem.65205": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Tr\u00fcmmer war die alte Welt gesunken!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Tr\u00fcmmer war die alte Welt gesunken!", "tokens": ["In", "Tr\u00fcm\u00b7mer", "war", "die", "al\u00b7te", "Welt", "ge\u00b7sun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Schwerpunkt missend, glitt sie todestrunken", "tokens": ["Den", "Schwer\u00b7punkt", "mis\u00b7send", ",", "glitt", "sie", "to\u00b7des\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Sturz zu Sturz durch schaurig \u00f6de Nacht.", "tokens": ["Von", "Sturz", "zu", "Sturz", "durch", "schau\u00b7rig", "\u00f6\u00b7de", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Glaube, der sie st\u00fctzend einst getragen,", "tokens": ["Der", "Glau\u00b7be", ",", "der", "sie", "st\u00fct\u00b7zend", "einst", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er war dahin, und mit ihm lag zerschlagen", "tokens": ["Er", "war", "da\u00b7hin", ",", "und", "mit", "ihm", "lag", "zer\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PAV", "$,", "KON", "APPR", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ihm entspross'ne F\u00fclle ihrer Macht.", "tokens": ["Die", "ihm", "ent\u00b7spross'\u00b7ne", "F\u00fcl\u00b7le", "ih\u00b7rer", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was konnte sie noch zeugen und geb\u00e4ren,", "tokens": ["Was", "konn\u00b7te", "sie", "noch", "zeu\u00b7gen", "und", "ge\u00b7b\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der G\u00f6tter, Freiheit, Vaterland Chim\u00e4ren,", "tokens": ["Der", "G\u00f6t\u00b7ter", ",", "Frei\u00b7heit", ",", "Va\u00b7ter\u00b7land", "Chi\u00b7m\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erfunden f\u00fcr der Thoren Wahnbedarf?", "tokens": ["Er\u00b7fun\u00b7den", "f\u00fcr", "der", "Tho\u00b7ren", "Wahn\u00b7be\u00b7darf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie, die von dunkeln M\u00e4chten Unterjochte,", "tokens": ["Sie", ",", "die", "von", "dun\u00b7keln", "M\u00e4ch\u00b7ten", "Un\u00b7ter\u00b7joch\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Gro\u00dfes zu bel\u00e4cheln nur vermochte", "tokens": ["Die", "Gro\u00b7\u00dfes", "zu", "be\u00b7l\u00e4\u00b7cheln", "nur", "ver\u00b7moch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und alles Hohe zu den Toten warf?", "tokens": ["Und", "al\u00b7les", "Ho\u00b7he", "zu", "den", "To\u00b7ten", "warf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Stets mehrten sich die finstern Unheilszeichen.", "tokens": ["Stets", "mehr\u00b7ten", "sich", "die", "fins\u00b7tern", "Un\u00b7heils\u00b7zei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Mannesstolz sah blasser Furcht man weichen,", "tokens": ["Den", "Man\u00b7ness\u00b7tolz", "sah", "blas\u00b7ser", "Furcht", "man", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die B\u00fcrgertugend k\u00e4uflich feilem Sinn!", "tokens": ["Die", "B\u00fcr\u00b7ger\u00b7tu\u00b7gend", "k\u00e4uf\u00b7lich", "fei\u00b7lem", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Klein wie die Seelen wurden ihre Ziele.", "tokens": ["Klein", "wie", "die", "See\u00b7len", "wur\u00b7den", "ih\u00b7re", "Zie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des P\u00f6bels Losungswort hie\u00df: Brot und Spiele!", "tokens": ["Des", "P\u00f6\u00b7bels", "Lo\u00b7sungs\u00b7wort", "hie\u00df", ":", "Brot", "und", "Spie\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der M\u00e4cht'gen Wahlspruch: Wollust und Gewinn!", "tokens": ["Der", "M\u00e4cht'\u00b7gen", "Wahl\u00b7spruch", ":", "Wol\u00b7lust", "und", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es l\u00f6sten sich die heiligsten der Bande,", "tokens": ["Es", "l\u00f6s\u00b7ten", "sich", "die", "hei\u00b7ligs\u00b7ten", "der", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "In Purpur ging, erhob'nen Haupts, die Schande,", "tokens": ["In", "Pur\u00b7pur", "ging", ",", "er\u00b7hob'\u00b7nen", "Haupts", ",", "die", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ver\u00f6det stand des Hauses trauter Herd.", "tokens": ["Ver\u00b7\u00f6\u00b7det", "stand", "des", "Hau\u00b7ses", "trau\u00b7ter", "Herd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo einst im Schmucke der verdienten Ehren", "tokens": ["Wo", "einst", "im", "Schmu\u00b7cke", "der", "ver\u00b7dien\u00b7ten", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Matronen sa\u00dfen, thronten jetzt Het\u00e4ren,", "tokens": ["Mat\u00b7ro\u00b7nen", "sa\u00b7\u00dfen", ",", "thron\u00b7ten", "jetzt", "He\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Geschm\u00fcckt mit eines F\u00fcrstentumes. Wert.", "tokens": ["Ge\u00b7schm\u00fcckt", "mit", "ei\u00b7nes", "F\u00fcrs\u00b7ten\u00b7tu\u00b7mes", ".", "Wert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein w\u00fcster Wahnsinn hielt die Welt umfangen;", "tokens": ["Ein", "w\u00fcs\u00b7ter", "Wahn\u00b7sinn", "hielt", "die", "Welt", "um\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Schminke Rot auf ihren fahlen Wangen,", "tokens": ["Der", "Schmin\u00b7ke", "Rot", "auf", "ih\u00b7ren", "fah\u00b7len", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erhoffte sie Bet\u00e4ubung vom Genu\u00df.", "tokens": ["Er\u00b7hoff\u00b7te", "sie", "Be\u00b7t\u00e4u\u00b7bung", "vom", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Umsonst! wohin sie ihren Schritt auch kehrte,", "tokens": ["Um\u00b7sonst", "!", "wo\u00b7hin", "sie", "ih\u00b7ren", "Schritt", "auch", "kehr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWAV", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Ekel vor sich selbst blieb ihr Gef\u00e4hrte,", "tokens": ["Der", "E\u00b7kel", "vor", "sich", "selbst", "blieb", "ihr", "Ge\u00b7f\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Als Schatten folgte ihr der Ueberdru\u00df.", "tokens": ["Als", "Schat\u00b7ten", "folg\u00b7te", "ihr", "der", "Ue\u00b7berd\u00b7ru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Kein Zauber war, der diesen Schatten bannte!", "tokens": ["Kein", "Zau\u00b7ber", "war", ",", "der", "die\u00b7sen", "Schat\u00b7ten", "bann\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tief in ihrem Innersten entbrannte", "tokens": ["Und", "tief", "in", "ih\u00b7rem", "In\u00b7ners\u00b7ten", "ent\u00b7brann\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ein wildes Sehnen nach dem Untergang. \u2013", "tokens": ["Ein", "wil\u00b7des", "Seh\u00b7nen", "nach", "dem", "Un\u00b7ter\u00b7gang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da, pl\u00f6tzlich, in so hoffnungslosem Trauern,", "tokens": ["Da", ",", "pl\u00f6tz\u00b7lich", ",", "in", "so", "hoff\u00b7nungs\u00b7lo\u00b7sem", "Trau\u00b7ern", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vernahm sie eine Stimme, die mit Schauern", "tokens": ["Ver\u00b7nahm", "sie", "ei\u00b7ne", "Stim\u00b7me", ",", "die", "mit", "Schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Schreckens und der Wonne sie durchdrang.", "tokens": ["Des", "Schre\u00b7ckens", "und", "der", "Won\u00b7ne", "sie", "durch\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ihr schwindelte! \u2013 Mit goldnem Licht umwob sich", "tokens": ["Ihr", "schwin\u00b7del\u00b7te", "!", "\u2013", "Mit", "gold\u00b7nem", "Licht", "um\u00b7wob", "sich"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "APPR", "ADJA", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Dunkel rings, ihr m\u00fcder Blick erhob sich,", "tokens": ["Das", "Dun\u00b7kel", "rings", ",", "ihr", "m\u00fc\u00b7der", "Blick", "er\u00b7hob", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Fu\u00df hielt inne auf dem Grabesgang!", "tokens": ["Ihr", "Fu\u00df", "hielt", "in\u00b7ne", "auf", "dem", "Gra\u00b7bes\u00b7gang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sie begann dem Wunderwort zu lauschen,", "tokens": ["Und", "sie", "be\u00b7gann", "dem", "Wun\u00b7der\u00b7wort", "zu", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das durch der St\u00fcrme und der Wogen Rauschen", "tokens": ["Das", "durch", "der", "St\u00fcr\u00b7me", "und", "der", "Wo\u00b7gen", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "KON", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus fernem Ost bis hin zum Tiber klang.", "tokens": ["Aus", "fer\u00b7nem", "Ost", "bis", "hin", "zum", "Ti\u00b7ber", "klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADV", "APPRART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Er sprach dies Wort den Herren und den Knechten", "tokens": ["Er", "sprach", "dies", "Wort", "den", "Her\u00b7ren", "und", "den", "Knech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Kampf und von dem Siege des Gerechten,", "tokens": ["Vom", "Kampf", "und", "von", "dem", "Sie\u00b7ge", "des", "Ge\u00b7rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von des Entsagens herber Seligkeit!", "tokens": ["Von", "des", "Ent\u00b7sa\u00b7gens", "her\u00b7ber", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von einer Liebe, die befreit, ents\u00fcndet,", "tokens": ["Von", "ei\u00b7ner", "Lie\u00b7be", ",", "die", "be\u00b7freit", ",", "ent\u00b7s\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und deren Strahl, da wo er trifft und z\u00fcndet,", "tokens": ["Und", "de\u00b7ren", "Strahl", ",", "da", "wo", "er", "trifft", "und", "z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "$,", "ADV", "PWAV", "PPER", "VVFIN", "KON", "VV