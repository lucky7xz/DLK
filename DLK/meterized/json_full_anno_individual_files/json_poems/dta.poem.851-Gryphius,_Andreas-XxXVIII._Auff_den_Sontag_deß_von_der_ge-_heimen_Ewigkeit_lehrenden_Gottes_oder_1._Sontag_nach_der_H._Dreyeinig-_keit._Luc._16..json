{"dta.poem.851": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXVIII.  Auff den Sontag de\u00df von der ge-  \n heimen Ewigkeit lehrenden Gottes/ oder  \n 1. Sontag nach der H. Dreyeinig-  \n keit. Luc. 16.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Onichts! O wahn! O Traum! worauff wir Menschen ba- ", "tokens": ["O\u00b7nichts", "!", "O", "wahn", "!", "O", "Traum", "!", "wo\u00b7rauff", "wir", "Men\u00b7schen", "ba"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NE", "PTKVZ", "$.", "NE", "NN", "$.", "PWAV", "PPER", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wen ", "tokens": ["wen"], "token_info": ["word"], "pos": ["PWS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Was hilfft der Taffel lust vnd stoltzer Kleider tracht?", "tokens": ["Was", "hilfft", "der", "Taf\u00b7fel", "lust", "vnd", "stolt\u00b7zer", "Klei\u00b7der", "tracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn die verdam\u2019te Seel in schwartzen flammen kracht", "tokens": ["Wenn", "die", "ver\u00b7dam'\u00b7te", "Seel", "in", "schwart\u00b7zen", "flam\u00b7men", "kracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "VVINF", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vnd nimmermehr nicht mag die minste rettung schawen", "tokens": ["Vnd", "nim\u00b7mer\u00b7mehr", "nicht", "mag", "die", "mins\u00b7te", "ret\u00b7tung", "scha\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.2": {"line.1": {"text": "Wie kan vns doch so hoch f\u00fcr noth vnd sterben grawen", "tokens": ["Wie", "kan", "vns", "doch", "so", "hoch", "f\u00fcr", "noth", "vnd", "ster\u00b7ben", "gra\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Wenn der/ so hier in angst ohn allen trost verschmacht", "tokens": ["Wenn", "der", "/", "so", "hier", "in", "angst", "ohn", "al\u00b7len", "trost", "ver\u00b7schmacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "$(", "ADV", "ADV", "APPR", "NN", "APPR", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff Gottes Ehren Thron wird ewig gro\u00df gemacht", "tokens": ["Auff", "Got\u00b7tes", "Eh\u00b7ren", "Thron", "wird", "e\u00b7wig", "gro\u00df", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "NN", "NN", "VAFIN", "ADJD", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mag jemand in der zeit auff lange Jahre bawen:", "tokens": ["Mag", "je\u00b7mand", "in", "der", "zeit", "auff", "lan\u00b7ge", "Jah\u00b7re", "ba\u00b7wen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.3": {"line.1": {"text": "Wenn vn\u00df der blasse Tod im augenblick abnimt/", "tokens": ["Wenn", "vn\u00df", "der", "blas\u00b7se", "Tod", "im", "au\u00b7gen\u00b7blick", "ab\u00b7nimt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bn\u00df ist das werthe Schlo\u00df der Ewigkeit bestimt/", "tokens": ["Bn\u00df", "ist", "das", "wert\u00b7he", "Schlo\u00df", "der", "E\u00b7wig\u00b7keit", "be\u00b7stimt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PDS", "VVFIN", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wem mag das tr\u00fcbe Thal der Erden denn belieben?", "tokens": ["Wem", "mag", "das", "tr\u00fc\u00b7be", "Thal", "der", "Er\u00b7den", "denn", "be\u00b7lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "GoTT ists der vnser Freundt vnd h\u00f6chste lust wil seyn!", "tokens": ["GoTT", "ists", "der", "vn\u00b7ser", "Freundt", "vnd", "h\u00f6chs\u00b7te", "lust", "wil", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Warumb denn achten wir/ die in der H\u00f6llen pein", "tokens": ["Wa\u00b7rumb", "denn", "ach\u00b7ten", "wir", "/", "die", "in", "der", "H\u00f6l\u00b7len", "pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "$(", "ART", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit jhrer gegenwartt die Freunde mehr betr\u00fcben?", "tokens": ["Mit", "jhrer", "ge\u00b7gen\u00b7wartt", "die", "Freun\u00b7de", "mehr", "be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}