{"textgrid.poem.33727": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh nur, so wie du stets vorbeigegangen,", "tokens": ["Geh", "nur", ",", "so", "wie", "du", "stets", "vor\u00b7bei\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADV", "KOKOM", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vorbei an mir, o Gl\u00fcck, wenn Gold und Ehre", "tokens": ["Vor\u00b7bei", "an", "mir", ",", "o", "Gl\u00fcck", ",", "wenn", "Gold", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "$,", "FM", "NN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Scho\u00df enthielt; mein Wahlspruch hei\u00dft: \u00bbEntbehre,", "tokens": ["Dein", "Scho\u00df", "ent\u00b7hielt", ";", "mein", "Wahl\u00b7spruch", "hei\u00dft", ":", "\u00bb", "Ent\u00b7beh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entsage jedem irdischen Verlangen!\u00ab", "tokens": ["Ent\u00b7sa\u00b7ge", "je\u00b7dem", "ir\u00b7di\u00b7schen", "Ver\u00b7lan\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Zwar hab' ich's stets mit Dankgef\u00fchl empfangen,", "tokens": ["Zwar", "hab'", "ich's", "stets", "mit", "Dank\u00b7ge\u00b7f\u00fchl", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gab mir das Schicksal eine weise Lehre,", "tokens": ["Gab", "mir", "das", "Schick\u00b7sal", "ei\u00b7ne", "wei\u00b7se", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch wenn ein Honigtropfen in die Leere", "tokens": ["Auch", "wenn", "ein", "Ho\u00b7nig\u00b7trop\u00b7fen", "in", "die", "Lee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Tage fiel, die nur ein Klaglied sangen.", "tokens": ["Der", "Ta\u00b7ge", "fiel", ",", "die", "nur", "ein", "Klag\u00b7lied", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Weht nur in unsern Fr\u00fchling, rauhe Winde!", "tokens": ["Weht", "nur", "in", "un\u00b7sern", "Fr\u00fch\u00b7ling", ",", "rau\u00b7he", "Win\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Schneefall soll mich in dem Glauben st\u00f6ren,", "tokens": ["Kein", "Schnee\u00b7fall", "soll", "mich", "in", "dem", "Glau\u00b7ben", "st\u0