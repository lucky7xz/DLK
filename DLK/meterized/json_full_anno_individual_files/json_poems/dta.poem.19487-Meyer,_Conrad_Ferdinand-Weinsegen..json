{"dta.poem.19487": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Weinsegen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Heut athm' ich mit den Sommerl\u00fcften", "tokens": ["Heut", "athm'", "ich", "mit", "den", "Som\u00b7mer\u00b7l\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die allerfeinsten W\u00fcrzen ein,", "tokens": ["Die", "al\u00b7ler\u00b7feins\u00b7ten", "W\u00fcr\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich kenne dieses seltne D\u00fcften:", "tokens": ["Ich", "ken\u00b7ne", "die\u00b7ses", "selt\u00b7ne", "D\u00fcf\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heut bl\u00fcht der echte Klosterwein.", "tokens": ["Heut", "bl\u00fcht", "der", "ech\u00b7te", "Klos\u00b7ter\u00b7wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier zog im Land die ersten Trauben", "tokens": ["Hier", "zog", "im", "Land", "die", "ers\u00b7ten", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zum ersten Liebesmahl der Abt,", "tokens": ["Zum", "ers\u00b7ten", "Lie\u00b7bes\u00b7mahl", "der", "Abt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der mit dem theuern Christenglauben", "tokens": ["Der", "mit", "dem", "theu\u00b7ern", "Chris\u00b7ten\u00b7glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Uns \u00f6de Heiden einst begabt.", "tokens": ["Uns", "\u00f6\u00b7de", "Hei\u00b7den", "einst", "be\u00b7gabt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Kloster, l\u00e4ngst ist's schon verschwunden,", "tokens": ["Das", "Klos\u00b7ter", ",", "l\u00e4ngst", "ist's", "schon", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerst\u00e4ubt mit Altar, Gruft und Chor,", "tokens": ["Zer\u00b7st\u00e4ubt", "mit", "Al\u00b7tar", ",", "Gruft", "und", "Chor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch steigt in diesen Mittagsstunden \u2014", "tokens": ["Doch", "steigt", "in", "die\u00b7sen", "Mit\u00b7tags\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So hei\u00dft's \u2014 der erste Abt empor.", "tokens": ["So", "hei\u00dft's", "der", "ers\u00b7te", "Abt", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$(", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nicht will er zu der Lese kommen,", "tokens": ["Nicht", "will", "er", "zu", "der", "Le\u00b7se", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo wild die Kelter \u00fcbersch\u00e4umt,", "tokens": ["Wo", "wild", "die", "Kel\u00b7ter", "\u00fc\u00b7bersc\u00b7h\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nein, wie sich ziemt f\u00fcr einen Frommen,", "tokens": ["Nein", ",", "wie", "sich", "ziemt", "f\u00fcr", "ei\u00b7nen", "From\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wann mystisch s\u00fc\u00df die Bl\u00fcthe tr\u00e4umt.", "tokens": ["Wann", "mys\u00b7tisch", "s\u00fc\u00df", "die", "Bl\u00fc\u00b7the", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was dort? Wer \u00f6ffnet still das Gatter?", "tokens": ["Was", "dort", "?", "Wer", "\u00f6ff\u00b7net", "still", "das", "Gat\u00b7ter", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$.", "PWS", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Berauscht die starke W\u00fcrze mich?", "tokens": ["Be\u00b7rauscht", "die", "star\u00b7ke", "W\u00fcr\u00b7ze", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein wallend blankes Rockgeflatter", "tokens": ["Ein", "wal\u00b7lend", "blan\u00b7kes", "Rock\u00b7ge\u00b7flat\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bewegt sich sacht und feierlich!", "tokens": ["Be\u00b7wegt", "sich", "sacht", "und", "fei\u00b7er\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist der Abt. Ich sehe b\u00fccken", "tokens": ["Es", "ist", "der", "Abt", ".", "Ich", "se\u00b7he", "b\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das edelgreise Haupt ihn dort,", "tokens": ["Das", "e\u00b7del\u00b7grei\u00b7se", "Haupt", "ihn", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die frechen Nachbarskinder dr\u00fccken", "tokens": ["Die", "fre\u00b7chen", "Nach\u00b7bars\u00b7kin\u00b7der", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich schleunig durch die Hecke fort.", "tokens": ["Sich", "schleu\u00b7nig", "durch", "die", "He\u00b7cke", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er pr\u00fcft genau die zarte Bl\u00fcte,", "tokens": ["Er", "pr\u00fcft", "ge\u00b7nau", "die", "zar\u00b7te", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die jungen Scho\u00dfe licht und gr\u00fcn,", "tokens": ["Die", "jun\u00b7gen", "Scho\u00b7\u00dfe", "licht", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Angesicht ist voller G\u00fcte", "tokens": ["Sein", "An\u00b7ge\u00b7sicht", "ist", "vol\u00b7ler", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und voll von herzlichem Bem\u00fchn.", "tokens": ["Und", "voll", "von", "herz\u00b7li\u00b7chem", "Be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hochw\u00fcrden blickt so hell und heiter,", "tokens": ["Hoch\u00b7w\u00fcr\u00b7den", "blickt", "so", "hell", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dies Jahr ger\u00e4th der Wein wie nie!", "tokens": ["Dies", "Jahr", "ge\u00b7r\u00e4\u00b7th", "der", "Wein", "wie", "nie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "ART", "NN", "KOKOM", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Er wandelt zu den Stufen weiter", "tokens": ["Er", "wan\u00b7delt", "zu", "den", "Stu\u00b7fen", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und geisterleicht ersteigt er sie.", "tokens": ["Und", "geis\u00b7ter\u00b7leicht", "er\u00b7steigt", "er", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Schon auf des Weinbergs H\u00f6he schreitet", "tokens": ["Schon", "auf", "des", "Wein\u00b7bergs", "H\u00f6\u00b7he", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er bei dem kleinen Winzerhaus.", "tokens": ["Er", "bei", "dem", "klei\u00b7nen", "Win\u00b7zer\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er setzt sich auf die Bank. Er breitet", "tokens": ["Er", "setzt", "sich", "auf", "die", "Bank", ".", "Er", "brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Geisterh\u00e4nde m\u00e4chtig aus.", "tokens": ["Die", "Geis\u00b7ter\u00b7h\u00e4n\u00b7de", "m\u00e4ch\u00b7tig", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er segnet seine Klosterreben,", "tokens": ["Er", "seg\u00b7net", "sei\u00b7ne", "Klos\u00b7ter\u00b7re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sein eigen vielgeliebtes Kind,", "tokens": ["Sein", "ei\u00b7gen", "viel\u00b7ge\u00b7lieb\u00b7tes", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns Ketzer segnet er daneben,", "tokens": ["Uns", "Ket\u00b7zer", "seg\u00b7net", "er", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die seines Weinbergs Erben sind.", "tokens": ["Die", "sei\u00b7nes", "Wein\u00b7bergs", "Er\u00b7ben", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}