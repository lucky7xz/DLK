{"textgrid.poem.35234": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wem behagt Aprillenwetter?", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem behagt Aprillenwetter?", "tokens": ["Wem", "be\u00b7hagt", "Ap\u00b7ril\u00b7len\u00b7wet\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wem des Hundsgestirnes Hitz'?", "tokens": ["Wem", "des", "Hunds\u00b7ges\u00b7tir\u00b7nes", "Hitz'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wem des Herbstes falbe Bl\u00e4tter?", "tokens": ["Wem", "des", "Herbs\u00b7tes", "fal\u00b7be", "Bl\u00e4t\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand, der nicht sparet Witz.", "tokens": ["Nie\u00b7mand", ",", "der", "nicht", "spa\u00b7ret", "Witz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PTKNEG", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich will nun kaltsinnig loben", "tokens": ["Ich", "will", "nun", "kalts\u00b7in\u00b7nig", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die begrau'te Winterszeit,", "tokens": ["Die", "be\u00b7grau'\u00b7te", "Win\u00b7ter\u00b7szeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die uns unsre Augen weid't,", "tokens": ["Die", "uns", "uns\u00b7re", "Au\u00b7gen", "weid't", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und auch billig wird erhoben.", "tokens": ["Und", "auch", "bil\u00b7lig", "wird", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ein fast bejahrter Alter", "tokens": ["Wie", "ein", "fast", "be\u00b7jahr\u00b7ter", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der schnellen Monden Flucht,", "tokens": ["Nach", "der", "schnel\u00b7len", "Mon\u00b7den", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzend bei dem Weinbehalter,", "tokens": ["Sit\u00b7zend", "bei", "dem", "Wein\u00b7be\u00b7hal\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kostet seiner Arbeit Frucht,", "tokens": ["Kos\u00b7tet", "sei\u00b7ner", "Ar\u00b7beit", "Frucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4lt die Ruhtag' f\u00fcr sein Leben", "tokens": ["H\u00e4lt", "die", "Ruh\u00b7tag'", "f\u00fcr", "sein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis zum vorgesteckten Ziel,", "tokens": ["Bis", "zum", "vor\u00b7ge\u00b7steck\u00b7ten", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da der grauen Haar' so viel", "tokens": ["Da", "der", "grau\u00b7en", "Haa\u00b7r'", "so", "viel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "ADV"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Strahlen gro\u00dfer Klugheit geben:", "tokens": ["Strah\u00b7len", "gro\u00b7\u00dfer", "Klug\u00b7heit", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also pfleget auch zu rasten", "tokens": ["Al\u00b7so", "pfle\u00b7get", "auch", "zu", "ras\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Jahrszeit Flucht und Eil,", "tokens": ["Al\u00b7ler", "Jahrs\u00b7zeit", "Flucht", "und", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und beginnet recht zu masten", "tokens": ["Und", "be\u00b7gin\u00b7net", "recht", "zu", "mas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An des wei\u00dfen Winters Seil.", "tokens": ["An", "des", "wei\u00b7\u00dfen", "Win\u00b7ters", "Seil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ceres wohnet in den Scheuern,", "tokens": ["Ce\u00b7res", "woh\u00b7net", "in", "den", "Scheu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bacchus bringt den s\u00fc\u00dfen Most,", "tokens": ["Bac\u00b7chus", "bringt", "den", "s\u00fc\u00b7\u00dfen", "Most", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und Pomona ihre Kost,", "tokens": ["Und", "Po\u00b7mo\u00b7na", "ih\u00b7re", "Kost", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPOSAT", "NN", "$,"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Sylvan kann beim Feuer feiern.", "tokens": ["Syl\u00b7van", "kann", "beim", "Feu\u00b7er", "fei\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schauet drau\u00df die wei\u00dfen Flocken,", "tokens": ["Schau\u00b7et", "drau\u00df", "die", "wei\u00b7\u00dfen", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie streichen hin und her,", "tokens": ["Wie", "sie", "strei\u00b7chen", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie sich zusammen stocken,", "tokens": ["Wie", "sie", "sich", "zu\u00b7sam\u00b7men", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie st\u00fcrmen \u00fcberquer!", "tokens": ["Wie", "sie", "st\u00fcr\u00b7men", "\u00fc\u00b7berq\u00b7uer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das ist ein gesundes Wetter,", "tokens": ["Das", "ist", "ein", "ge\u00b7sun\u00b7des", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und man heizt auch tapfer ein,", "tokens": ["Und", "man", "heizt", "auch", "tap\u00b7fer", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Horchend bei dem firnen Wein", "tokens": ["Hor\u00b7chend", "bei", "dem", "fir\u00b7nen", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Musik von einem Br\u00e4ter.", "tokens": ["Der", "Mu\u00b7sik", "von", "ei\u00b7nem", "Br\u00e4\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mich bed\u00fcnket, da\u00df die Sterne", "tokens": ["Mich", "be\u00b7d\u00fcn\u00b7ket", ",", "da\u00df", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Strahlen ba\u00df, wann's Winter ist;", "tokens": ["Strah\u00b7len", "ba\u00df", ",", "wann's", "Win\u00b7ter", "ist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann das Wasser hartet gerne", "tokens": ["Wann", "das", "Was\u00b7ser", "har\u00b7tet", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie Kristallstein durch Gefr\u00fcst,", "tokens": ["Wie", "Kris\u00b7tall\u00b7stein", "durch", "Ge\u00b7fr\u00fcst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "So mu\u00df man das Eis belaufen", "tokens": ["So", "mu\u00df", "man", "das", "Eis", "be\u00b7lau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mit der Schlittschuh' schnellem Holz;", "tokens": ["Mit", "der", "Schlitt\u00b7schuh'", "schnel\u00b7lem", "Holz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie ein Vogel oder Bolz,", "tokens": ["Wie", "ein", "Vo\u00b7gel", "o\u00b7der", "Bolz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "KON", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Rauscht man vorw\u00e4rts ohn' Verschnaufen.", "tokens": ["Rauscht", "man", "vor\u00b7w\u00e4rts", "ohn'", "Ver\u00b7schnau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Masken, Fastnacht, Schlittenfahren,", "tokens": ["Mas\u00b7ken", ",", "Fast\u00b7nacht", ",", "Schlit\u00b7ten\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reiten, Tanzen, Fechten \u00fcben,", "tokens": ["Rei\u00b7ten", ",", "Tan\u00b7zen", ",", "Fech\u00b7ten", "\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lass' ich unbemeldet fahren,", "tokens": ["Lass'", "ich", "un\u00b7be\u00b7mel\u00b7det", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie auch auf der Tafel schieben,", "tokens": ["Wie", "auch", "auf", "der", "Ta\u00b7fel", "schie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und erhebe das Studiren,", "tokens": ["Und", "er\u00b7he\u00b7be", "das", "Stu\u00b7di\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "So uns manche lange Nacht", "tokens": ["So", "uns", "man\u00b7che", "lan\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch wohl in das Bett gebracht,", "tokens": ["Auch", "wohl", "in", "das", "Bett", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Da\u00df wir Winterslust recht sp\u00fcren.", "tokens": ["Da\u00df", "wir", "Win\u00b7ters\u00b7lust", "recht", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wem behagt Aprillenwetter?", "tokens": ["Wem", "be\u00b7hagt", "Ap\u00b7ril\u00b7len\u00b7wet\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wem des Hundsgestirnes Hitz'?", "tokens": ["Wem", "des", "Hunds\u00b7ges\u00b7tir\u00b7nes", "Hitz'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wem des Herbstes falbe Bl\u00e4tter?", "tokens": ["Wem", "des", "Herbs\u00b7tes", "fal\u00b7be", "Bl\u00e4t\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand, der nicht sparet Witz.", "tokens": ["Nie\u00b7mand", ",", "der", "nicht", "spa\u00b7ret", "Witz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PTKNEG", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich will nun kaltsinnig loben", "tokens": ["Ich", "will", "nun", "kalts\u00b7in\u00b7nig", "lo\u00b7ben"], "token_info": ["word", "word", 