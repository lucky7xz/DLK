{"dta.poem.15715": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Doppelweg im Glauben und im Hoffen,", "tokens": ["Es", "ist", "ein", "Dop\u00b7pel\u00b7weg", "im", "Glau\u00b7ben", "und", "im", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Einzelnen ist der, und der dem Ganzen offen.", "tokens": ["Dem", "Ein\u00b7zel\u00b7nen", "ist", "der", ",", "und", "der", "dem", "Gan\u00b7zen", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "$,", "KON", "ART", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Dem Einzelnen kann nur Vollendung jenseit werden,", "tokens": ["Dem", "Ein\u00b7zel\u00b7nen", "kann", "nur", "Vol\u00b7len\u00b7dung", "jen\u00b7seit", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "NN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Doch wachsen soll das Heil des Ganzen hier auf Erden.", "tokens": ["Doch", "wach\u00b7sen", "soll", "das", "Heil", "des", "Gan\u00b7zen", "hier", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und nicht der Einzelne soll nur allein sich schwingen", "tokens": ["Und", "nicht", "der", "Ein\u00b7zel\u00b7ne", "soll", "nur", "al\u00b7lein", "sich", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "VMFIN", "ADV", "ADV", "PRF", "VVINF"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dorthin, er soll auch hier die Menschheit weiter bringen.", "tokens": ["Dor\u00b7thin", ",", "er", "soll", "auch", "hier", "die", "Menschheit", "wei\u00b7ter", "brin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Mein Sohn, alt ist der Wahn und allgemein verbreitet,", "tokens": ["Mein", "Sohn", ",", "alt", "ist", "der", "Wahn", "und", "all\u00b7ge\u00b7mein", "ver\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "VAFIN", "ART", "NN", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df diese Welt durch vier Weltalter abwerts schreitet;", "tokens": ["Da\u00df", "die\u00b7se", "Welt", "durch", "vier", "Welt\u00b7al\u00b7ter", "ab\u00b7werts", "schrei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "APPR", "CARD", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da\u00df in Verschlechterung sie immer tiefer sinkt,", "tokens": ["Da\u00df", "in", "Ver\u00b7schlech\u00b7te\u00b7rung", "sie", "im\u00b7mer", "tie\u00b7fer", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und rettungslos zuletzt den Kelch des Todes trinkt.", "tokens": ["Und", "ret\u00b7tungs\u00b7los", "zu\u00b7letzt", "den", "Kelch", "des", "To\u00b7des", "trinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Ansicht von der Welt mu\u00df werden umgedreht,", "tokens": ["Die", "An\u00b7sicht", "von", "der", "Welt", "mu\u00df", "wer\u00b7den", "um\u00b7ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VMFIN", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie, auch nicht im Kreis, da\u00df sie stets aufwerts geht;", "tokens": ["Da\u00df", "sie", ",", "auch", "nicht", "im", "Kreis", ",", "da\u00df", "sie", "stets", "auf\u00b7werts", "geht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "PTKNEG", "APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Da\u00df nicht gewaltsam sie zuletzt aus ihrem Kloben,", "tokens": ["Da\u00df", "nicht", "ge\u00b7walt\u00b7sam", "sie", "zu\u00b7letzt", "aus", "ih\u00b7rem", "Klo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ADJD", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vielmehr vers\u00f6hnend aus der Zwiespalt wird gehoben;", "tokens": ["Viel\u00b7mehr", "ver\u00b7s\u00f6h\u00b7nend", "aus", "der", "Zwies\u00b7palt", "wird", "ge\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Da\u00df ihr, nach endlicher der Gegens\u00e4tz' Ausgleichung,", "tokens": ["Da\u00df", "ihr", ",", "nach", "end\u00b7li\u00b7cher", "der", "Ge\u00b7gen\u00b7s\u00e4tz'", "Aus\u00b7glei\u00b7chung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Reich des Friedens bl\u00fcht, wie fern auch der Erreichung,", "tokens": ["Ein", "Reich", "des", "Frie\u00b7dens", "bl\u00fcht", ",", "wie", "fern", "auch", "der", "Er\u00b7rei\u00b7chung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "PWAV", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Wo mit erneutem Sinn die ganze Br\u00fcderschaar", "tokens": ["Wo", "mit", "er\u00b7neu\u00b7tem", "Sinn", "die", "gan\u00b7ze", "Br\u00fc\u00b7der\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lebt, wie im Anbeginn das erste Menschenpaar.", "tokens": ["Lebt", ",", "wie", "im", "An\u00b7be\u00b7ginn", "das", "ers\u00b7te", "Men\u00b7schen\u00b7paar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Mein Sohn, sowenig als des eignen Heiles Glauben,", "tokens": ["Mein", "Sohn", ",", "so\u00b7we\u00b7nig", "als", "des", "eig\u00b7nen", "Hei\u00b7les", "Glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KOKOM", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df diesen dir ans allgemeine Weltheil rauben.", "tokens": ["La\u00df", "die\u00b7sen", "dir", "ans", "all\u00b7ge\u00b7mei\u00b7ne", "Welt\u00b7heil", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}