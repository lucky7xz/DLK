{"dta.poem.10656": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "An die Liebste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Wo fern die Zeit die Sch\u00f6nheit gantz vertreibet/", "tokens": ["Wo", "fern", "die", "Zeit", "die", "Sch\u00f6n\u00b7heit", "gantz", "ver\u00b7trei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So brauche sie/ weil sie noch ist bey dir/", "tokens": ["So", "brau\u00b7che", "sie", "/", "weil", "sie", "noch", "ist", "bey", "dir", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "VAFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verwartet sie vollkommen f\u00fcr vnd f\u00fcr/", "tokens": ["Ver\u00b7war\u00b7tet", "sie", "voll\u00b7kom\u00b7men", "f\u00fcr", "vnd", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "KON", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So gib sie mir/ weil sie dir gleichwol bleibet.", "tokens": ["So", "gib", "sie", "mir", "/", "weil", "sie", "dir", "gleich\u00b7wol", "blei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPER", "$(", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}