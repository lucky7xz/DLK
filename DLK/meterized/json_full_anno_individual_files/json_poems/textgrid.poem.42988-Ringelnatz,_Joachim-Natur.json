{"textgrid.poem.42988": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Natur", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn immer sie mich fragen,", "tokens": ["Wenn", "im\u00b7mer", "sie", "mich", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob ich ein Freund sei der Natur,", "tokens": ["Ob", "ich", "ein", "Freund", "sei", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Was soll ich ihnen nur", "tokens": ["Was", "soll", "ich", "ih\u00b7nen", "nur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dann sagen?", "tokens": ["Dann", "sa\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Ich kann eine Bohrmaschine,", "tokens": ["Ich", "kann", "ei\u00b7ne", "Bohr\u00b7ma\u00b7schi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einen Hosentr\u00e4ger oder ein Kind", "tokens": ["Ei\u00b7nen", "Ho\u00b7sen\u00b7tr\u00e4\u00b7ger", "o\u00b7der", "ein", "Kind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "So lieben wie eine Bie