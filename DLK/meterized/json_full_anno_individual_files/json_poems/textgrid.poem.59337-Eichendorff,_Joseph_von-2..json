{"textgrid.poem.59337": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon wird es drau\u00dfen licht auf Berg und Talen;", "tokens": ["Schon", "wird", "es", "drau\u00b7\u00dfen", "licht", "auf", "Berg", "und", "Ta\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aurora, stille Braut, ihr sch\u00f6nen Strahlen,", "tokens": ["Au\u00b7ro\u00b7ra", ",", "stil\u00b7le", "Braut", ",", "ihr", "sch\u00f6\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die farb'gen Rauch aus Flu\u00df und W\u00e4ldern saugen,", "tokens": ["Die", "fa\u00b7rb'\u00b7gen", "Rauch", "aus", "Flu\u00df", "und", "W\u00e4l\u00b7dern", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Euch gr\u00fc\u00dfen neu die halbverschlafnen Augen.", "tokens": ["Euch", "gr\u00fc\u00b7\u00dfen", "neu", "die", "halb\u00b7ver\u00b7schlaf\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verr\u00e4trisch, sagt man, sei des Zimmers Schw\u00fcle,", "tokens": ["Ver\u00b7r\u00e4t\u00b7risch", ",", "sagt", "man", ",", "sei", "des", "Zim\u00b7mers", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "PIS", "$,", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo nachts ein M\u00e4dchen tr\u00e4umte vom Geliebten:", "tokens": ["Wo", "nachts", "ein", "M\u00e4d\u00b7chen", "tr\u00e4um\u00b7te", "vom", "Ge\u00b7lieb\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So komm herein, du rote, frische K\u00fchle,", "tokens": ["So", "komm", "her\u00b7ein", ",", "du", "ro\u00b7te", ",", "fri\u00b7sche", "K\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Fliegt in die blaue Luft, ihr sch\u00f6nen Tr\u00e4ume!", "tokens": ["Fliegt", "in", "die", "blau\u00b7e", "Luft", ",", "ihr", "sch\u00f6\u00b7nen", "Tr\u00e4u\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein furchtsam Kind, im stillen Haus erzogen,", "tokens": ["Ein", "furcht\u00b7sam", "Kind", ",", "im", "stil\u00b7len", "Haus", "er\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Konnt ich am Abendrot die Blicke weiden,", "tokens": ["Konnt", "ich", "am", "A\u00b7ben\u00b7drot", "die", "Bli\u00b7cke", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Tiefatmend in die laue Luft vor Freuden.", "tokens": ["Tie\u00b7fat\u00b7mend", "in", "die", "lau\u00b7e", "Luft", "vor", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.12": {"text": "Er hat um diese Stille mich betrogen.", "tokens": ["Er", "hat", "um", "die\u00b7se", "Stil\u00b7le", "mich", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Mit stolzen Augen, fremden sch\u00f6nen Worten", "tokens": ["Mit", "stol\u00b7zen", "Au\u00b7gen", ",", "frem\u00b7den", "sch\u00f6\u00b7nen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Lockt er die W\u00fcnsche aus dem stillen Hafen,", "tokens": ["Lockt", "er", "die", "W\u00fcn\u00b7sche", "aus", "dem", "stil\u00b7len", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.15": {"text": "Wo sie bei Sternenglanze selig schlafen,", "tokens": ["Wo", "sie", "bei", "Ster\u00b7nen\u00b7glan\u00b7ze", "se\u00b7lig", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Hinaus ins unbekannte Reich der Wogen;", "tokens": ["Hin\u00b7aus", "ins", "un\u00b7be\u00b7kann\u00b7te", "Reich", "der", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Da kommen Winde buhlend angeflogen,", "tokens": ["Da", "kom\u00b7men", "Win\u00b7de", "buh\u00b7lend", "an\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die zarte Hand zwingt nicht die wilden Wellen,", "tokens": ["Die", "zar\u00b7te", "Hand", "zwingt", "nicht", "die", "wil\u00b7den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Du mu\u00dft, wohin die vollen Segel schwellen.", "tokens": ["Du", "mu\u00dft", ",", "wo\u00b7hin", "die", "vol\u00b7len", "Se\u00b7gel", "schwel\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da zog er heimlich fort. \u2013 Seit jenem Morgen", "tokens": ["Da", "zog", "er", "heim\u00b7lich", "fort", ".", "\u2013", "Seit", "je\u00b7nem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "$(", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da hatt ich Not, hatt heimlich was zu sorgen.", "tokens": ["Da", "hatt", "ich", "Not", ",", "hatt", "heim\u00b7lich", "was", "zu", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "VAFIN", "ADJD", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn n\u00e4chtlich unten lag die stille Runde,", "tokens": ["Wenn", "n\u00e4cht\u00b7lich", "un\u00b7ten", "lag", "die", "stil\u00b7le", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Einf\u00f6rmig Rauschen herkam von den W\u00e4ldern,", "tokens": ["Ein\u00b7f\u00f6r\u00b7mig", "Rau\u00b7schen", "her\u00b7kam", "von", "den", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Pfeifend der Wind strich durch die \u00f6den Felder", "tokens": ["Pfei\u00b7fend", "der", "Wind", "strich", "durch", "die", "\u00f6\u00b7den", "Fel\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Und hin und her in D\u00f6rfern bellten Hunde,", "tokens": ["Und", "hin", "und", "her", "in", "D\u00f6r\u00b7fern", "bell\u00b7ten", "Hun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "ADV", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ach! wenn kein gl\u00fccklich Herz auf Erden wacht,", "tokens": ["Ach", "!", "wenn", "kein", "gl\u00fcck\u00b7lich", "Herz", "auf", "Er\u00b7den", "wacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PIAT", "ADJD", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Begr\u00fc\u00dften die verweinten Augen manche Nacht!", "tokens": ["Be\u00b7gr\u00fc\u00df\u00b7ten", "die", "ver\u00b7wein\u00b7ten", "Au\u00b7gen", "man\u00b7che", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie oft, wenn wir im Garten ruhig waren,", "tokens": ["Wie", "oft", ",", "wenn", "wir", "im", "Gar\u00b7ten", "ru\u00b7hig", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sagte mein Bruder mir vor vielen Jahren:", "tokens": ["Sag\u00b7te", "mein", "Bru\u00b7der", "mir", "vor", "vie\u00b7len", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "\u00bbdem sch\u00f6nen Lenz gleicht recht die erste Liebe.", "tokens": ["\u00bb", "dem", "sch\u00f6\u00b7nen", "Lenz", "gleicht", "recht", "die", "ers\u00b7te", "Lie\u00b7be", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann drau\u00dfen neu geschm\u00fcckt die Fr\u00fchlingsb\u00fchne,", "tokens": ["Wann", "drau\u00b7\u00dfen", "neu", "ge\u00b7schm\u00fcckt", "die", "Fr\u00fch\u00b7lings\u00b7b\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Reiter blitzend unten ziehn durchs Gr\u00fcne,", "tokens": ["Die", "Rei\u00b7ter", "blit\u00b7zend", "un\u00b7ten", "ziehn", "durchs", "Gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In blauer Luft die Lerchen lustig schwirren,", "tokens": ["In", "blau\u00b7er", "Luft", "die", "Ler\u00b7chen", "lus\u00b7tig", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "L\u00e4\u00dft sie sich weit ins Land hinaus verf\u00fchren,", "tokens": ["L\u00e4\u00dft", "sie", "sich", "weit", "ins", "Land", "hin\u00b7aus", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPRART", "NN", "APZR", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Fragt nicht, wohin, und mag sich gern verirren,", "tokens": ["Fragt", "nicht", ",", "wo\u00b7hin", ",", "und", "mag", "sich", "gern", "ver\u00b7ir\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "$,", "KON", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den Stimmen folgend, die sie wirrend f\u00fchren.", "tokens": ["Den", "Stim\u00b7men", "fol\u00b7gend", ",", "die", "sie", "wir\u00b7rend", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da wendet auf den Feldern sich der Wind,", "tokens": ["Da", "wen\u00b7det", "auf", "den", "Fel\u00b7dern", "sich", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die V\u00f6gel hoch durch Nebel ziehn nach Haus;", "tokens": ["Die", "V\u00f6\u00b7gel", "hoch", "durch", "Ne\u00b7bel", "ziehn", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Es wird so still, das sch\u00f6ne Fest ist aus.", "tokens": ["Es", "wird", "so", "still", ",", "das", "sch\u00f6\u00b7ne", "Fest", "ist", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "ADJA", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Gar weit die Heimat liegt, das sch\u00f6ne Kind", "tokens": ["Gar", "weit", "die", "Hei\u00b7mat", "liegt", ",", "das", "sch\u00f6\u00b7ne", "Kind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Findt nicht nach Hause mehr, nicht weiter fort \u2013", "tokens": ["Findt", "nicht", "nach", "Hau\u00b7se", "mehr", ",", "nicht", "wei\u00b7ter", "fort", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "ADV", "$,", "PTKNEG", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "H\u00fct dich, such fr\u00fch dir einen sichern Port!\u00ab", "tokens": ["H\u00fct", "dich", ",", "such", "fr\u00fch", "dir", "ei\u00b7nen", "si\u00b7chern", "Port", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "ADJD", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Schon wird es drau\u00dfen licht auf Berg und Talen;", "tokens": ["Schon", "wird", "es", "drau\u00b7\u00dfen", "licht", "auf", "Berg", "und", "Ta\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aurora, stille Braut, ihr sch\u00f6nen Strahlen,", "tokens": ["Au\u00b7ro\u00b7ra", ",", "stil\u00b7le", "Braut", ",", "ihr", "sch\u00f6\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die farb'gen Rauch aus Flu\u00df und W\u00e4ldern saugen,", "tokens": ["Die", "fa\u00b7rb'\u00b7gen", "Rauch", "aus", "Flu\u00df", "und", "W\u00e4l\u00b7dern", "sau\u00b7gen", ","], "toke