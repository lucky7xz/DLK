{"textgrid.poem.39482": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Schwestern lieb' ich, schwesterliche Sch\u00f6nen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Schwestern lieb' ich, schwesterliche Sch\u00f6nen,", "tokens": ["Zwei", "Schwes\u00b7tern", "lieb'", "ich", ",", "schwes\u00b7ter\u00b7li\u00b7che", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Einer hohen Mutter Z\u00fcge tragen;", "tokens": ["Die", "Ei\u00b7ner", "ho\u00b7hen", "Mut\u00b7ter", "Z\u00fc\u00b7ge", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur andrer Heimat Wiege, wo sie lagen,", "tokens": ["Nur", "an\u00b7drer", "Hei\u00b7mat", "Wie\u00b7ge", ",", "wo", "sie", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Konnt' in der Sitt' einander sie entw\u00f6hnen.", "tokens": ["Konnt'", "in", "der", "Sitt'", "ein\u00b7an\u00b7der", "sie", "ent\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie fe\u00dfeln mich mit ihrer Stimme T\u00f6nen,", "tokens": ["Sie", "fe\u00b7\u00df\u00b7eln", "mich", "mit", "ih\u00b7rer", "Stim\u00b7me", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Die zart und voll den Sinn der Rede sagen.", "tokens": ["Die", "zart", "und", "voll", "den", "Sinn", "der", "Re\u00b7de", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn eine schweigt, mu\u00df ich vermi\u00dfend klagen,", "tokens": ["Wenn", "ei\u00b7ne", "schweigt", ",", "mu\u00df", "ich", "ver\u00b7mi\u00b7\u00dfend", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,", "VMFIN", "PPER", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die ich h\u00f6re, scheint mir werth zu kr\u00f6nen.", "tokens": ["Und", "die", "ich", "h\u00f6\u00b7re", ",", "scheint", "mir", "werth", "zu", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich streb', entz\u00fcndet, ihnen nachzulallen,", "tokens": ["Ich", "streb'", ",", "ent\u00b7z\u00fcn\u00b7det", ",", "ih\u00b7nen", "nach\u00b7zu\u00b7lal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wie ein fernes Echo, matter, tr\u00