{"dta.poem.10390": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XvI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Liebste, sollst mir heute sagen:", "tokens": ["Liebs\u00b7te", ",", "sollst", "mir", "heu\u00b7te", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du nicht ein Traumgebild',", "tokens": ["Bist", "du", "nicht", "ein", "Traum\u00b7ge\u00b7bild'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie's in schw\u00fclen Sommertagen", "tokens": ["Wie's", "in", "schw\u00fc\u00b7len", "Som\u00b7mer\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Hirn des Dichters quillt?", "tokens": ["Aus", "dem", "Hirn", "des", "Dich\u00b7ters", "quillt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber nein, ein solches M\u00fcndchen,", "tokens": ["A\u00b7ber", "nein", ",", "ein", "sol\u00b7ches", "M\u00fcnd\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Solcher Augen Zauberlicht,", "tokens": ["Sol\u00b7cher", "Au\u00b7gen", "Zau\u00b7ber\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Solch ein liebes, s\u00fc\u00dfes Kindchen,", "tokens": ["Solch", "ein", "lie\u00b7bes", ",", "s\u00fc\u00b7\u00dfes", "Kind\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das erschafft der Dichter nicht.", "tokens": ["Das", "er\u00b7schafft", "der", "Dich\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Basilisken und Vampyre,", "tokens": ["Ba\u00b7si\u00b7lis\u00b7ken", "und", "Vam\u00b7py\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lindenw\u00fcrm' und Ungeheu'r,", "tokens": ["Lin\u00b7den\u00b7w\u00fcrm'", "und", "Un\u00b7ge\u00b7heu'r", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Solche schlimme Fabelthiere,", "tokens": ["Sol\u00b7che", "schlim\u00b7me", "Fa\u00b7belt\u00b7hie\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die erschafft des Dichters Feu'r.", "tokens": ["Die", "er\u00b7schafft", "des", "Dich\u00b7ters", "Feu'", "r."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PDS", "VVFIN", "ART", "NN", "NE", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber dich und deine T\u00fccke,", "tokens": ["A\u00b7ber", "dich", "und", "dei\u00b7ne", "T\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dein s\u00fc\u00dfes Angesicht,", "tokens": ["Und", "dein", "s\u00fc\u00b7\u00dfes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die falschen, frommen Blicke \u2014", "tokens": ["Und", "die", "fal\u00b7schen", ",", "from\u00b7men", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das erschafft der Dichter nicht.", "tokens": ["Das", "er\u00b7schafft", "der", "Dich\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}