{"textgrid.poem.35107": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Verst\u00e4ndige Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Liebe, die ich endlich nun erfa\u00dft", "tokens": ["O", "Lie\u00b7be", ",", "die", "ich", "end\u00b7lich", "nun", "er\u00b7fa\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und die du mich so ganz ergriffen hast,", "tokens": ["Und", "die", "du", "mich", "so", "ganz", "er\u00b7grif\u00b7fen", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PRF", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich nur dir, nur dir zu eigen bin,", "tokens": ["Da\u00df", "ich", "nur", "dir", ",", "nur", "dir", "zu", "ei\u00b7gen", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "$,", "ADV", "PPER", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nimm mich; nimm mich; ich gebe mich dir hin.", "tokens": ["Nimm", "mich", ";", "nimm", "mich", ";", "ich", "ge\u00b7be", "mich", "dir", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "VVIMP", "PPER", "$.", "PPER", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer sich mit seinem Sein in dich versenkt,", "tokens": ["Wer", "sich", "mit", "sei\u00b7nem", "Sein", "in", "dich", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem wird von dir ein besseres geschenkt,", "tokens": ["Dem", "wird", "von", "dir", "ein", "bes\u00b7se\u00b7res", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn was du von ihm nimmst, giebst du als Gl\u00fcck,", "tokens": ["Denn", "was", "du", "von", "ihm", "nimmst", ",", "giebst", "du", "als", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als Seligkeit ihm tausendfach zur\u00fcck.", "tokens": ["Als", "Se\u00b7lig\u00b7keit", "ihm", "tau\u00b7send\u00b7fach", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So will ich durch dich und in dir allein", "tokens": ["So", "will", "ich", "durch", "dich", "und", "in", "dir", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "KON", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur im Begl\u00fccken selbst auch gl\u00fccklich sein,", "tokens": ["Nur", "im", "Be\u00b7gl\u00fc\u00b7cken", "selbst", "auch", "gl\u00fcck\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Will nimmer rasten und will nimmer ruhn,", "tokens": ["Will", "nim\u00b7mer", "ras\u00b7ten", "und", "will", "nim\u00b7mer", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "KON", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur was du willst, nichts Anderes zu thun.", "tokens": ["Nur", "was", "du", "willst", ",", "nichts", "An\u00b7de\u00b7res", "zu", "thun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VMFIN", "$,", "PIS", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Jedoch damit ich ja nicht irre geh", "tokens": ["Je\u00b7doch", "da\u00b7mit", "ich", "ja", "nicht", "ir\u00b7re", "geh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "PTKNEG", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und unter Lieben schwach zu sein versteh,", "tokens": ["Und", "un\u00b7ter", "Lie\u00b7ben", "schwach", "zu", "sein", "ver\u00b7steh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ADJD", "PTKZU", "VAINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So gieb mir deinen Bruder an die Hand,", "tokens": ["So", "gieb", "mir", "dei\u00b7nen", "Bru\u00b7der", "an", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den klugen Lebensf\u00fchrer, den Verstand!", "tokens": ["Den", "klu\u00b7gen", "Le\u00b7bens\u00b7f\u00fch\u00b7rer", ",", "den", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O Liebe, die ich endlich nun erfa\u00dft", "tokens": ["O", "Lie\u00b7be", ",", "die", "ich", "end\u00b7lich", "nun", "er\u00b7fa\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und die du mich so ganz ergriffen hast,", "tokens": ["Und", "die", "du", "mich", "so", "ganz", "er\u00b7grif\u00b7fen", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "PRF", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df ich nur dir, nur dir zu eigen bin,", "tokens": ["Da\u00df", "ich", "nur", "dir", ",", "nur", "dir", "zu", "ei\u00b7gen", "bin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "$,", "ADV", "PPER", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nimm mich; nimm mich; ich gebe mich dir hin.", "tokens": ["Nimm", "mich", ";", "nimm", "mich", ";", "ich", "ge\u00b7be", "mich", "dir", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "VVIMP", "PPER", "$.", "PPER", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wer sich