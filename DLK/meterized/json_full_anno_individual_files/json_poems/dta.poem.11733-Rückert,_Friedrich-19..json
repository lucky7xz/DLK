{"dta.poem.11733": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Feuer war in Furcht, da\u00df es das Wasser hasche,", "tokens": ["Das", "Feu\u00b7er", "war", "in", "Furcht", ",", "da\u00df", "es", "das", "Was\u00b7ser", "ha\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und heimlich glimmend barg es sich im Haufen Asche.", "tokens": ["Und", "heim\u00b7lich", "glim\u00b7mend", "barg", "es", "sich", "im", "Hau\u00b7fen", "A\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Wasser kam und go\u00df den Aschenhaufen aus,", "tokens": ["Das", "Was\u00b7ser", "kam", "und", "go\u00df", "den", "A\u00b7schen\u00b7hau\u00b7fen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und suchen mu\u00dfte sich das Feur ein andres Haus.", "tokens": ["Und", "su\u00b7chen", "mu\u00df\u00b7te", "sich", "das", "Feur", "ein", "and\u00b7res", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Feuer barg im Wald sich in das gr\u00fcne Holz,", "tokens": ["Das", "Feu\u00b7er", "barg", "im", "Wald", "sich", "in", "das", "gr\u00fc\u00b7ne", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Wasser merkt' es nicht, da ward das Feuer stolz.", "tokens": ["Das", "Was\u00b7ser", "merkt'", "es", "nicht", ",", "da", "ward", "das", "Feu\u00b7er", "stolz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und als der Sommerwind die Ranken schlug zusammen,", "tokens": ["Und", "als", "der", "Som\u00b7mer\u00b7wind", "die", "Ran\u00b7ken", "schlug", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Feuer kam hervor, da stand der Wald in Flammen.", "tokens": ["Das", "Feu\u00b7er", "kam", "her\u00b7vor", ",", "da", "stand", "der", "Wald", "in", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da kam der Wolkenbruch und go\u00df den Waldbrand aus,", "tokens": ["Da", "kam", "der", "Wol\u00b7ken\u00b7bruch", "und", "go\u00df", "den", "Wald\u00b7brand", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wieder suchen mu\u00df das Feur ein andres Haus.", "tokens": ["Und", "wie\u00b7der", "su\u00b7chen", "mu\u00df", "das", "Feur", "ein", "and\u00b7res", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Das Feuer fl\u00fcchtete sich in den Kieselstein,", "tokens": ["Das", "Feu\u00b7er", "fl\u00fcch\u00b7te\u00b7te", "sich", "in", "den", "Kie\u00b7sel\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und warf sich in den Bach, ins Wasser selbst hinein.", "tokens": ["Und", "warf", "sich", "in", "den", "Bach", ",", "ins", "Was\u00b7ser", "selbst", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Das Wasser sucht' es rings und merkte nicht die List,", "tokens": ["Das", "Was\u00b7ser", "sucht'", "es", "rings", "und", "merk\u00b7te", "nicht", "die", "List", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sicher oft der Feind im Haus des Feindes ist.", "tokens": ["Wie", "si\u00b7cher", "oft", "der", "Feind", "im", "Haus", "des", "Fein\u00b7des", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "APPRART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und ruht am Mittag einst das Wasser schlummertrunken,", "tokens": ["Und", "ruht", "am", "Mit\u00b7tag", "einst", "das", "Was\u00b7ser", "schlum\u00b7mer\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann aus dem Kiesel springt das Feuer als ein Funken.", "tokens": ["Dann", "aus", "dem", "Kie\u00b7sel", "springt", "das", "Feu\u00b7er", "als", "ein", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}