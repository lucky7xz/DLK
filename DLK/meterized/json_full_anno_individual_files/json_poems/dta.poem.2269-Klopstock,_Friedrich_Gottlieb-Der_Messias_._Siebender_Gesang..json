{"dta.poem.2269": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n  Messias .  \n Siebender Gesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1756", "urn": "urn:nbn:de:kobv:b4-200905198237", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du, Eloa! du standst auf der Morgenr\u00f6the. Der Erde", "tokens": ["Du", ",", "E\u00b7loa", "!", "du", "standst", "auf", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", ".", "Der", "Er\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "NE", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "ART", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "H\u00fcter standen um ihn. Er sang in die m\u00e4chtige Harfe.", "tokens": ["H\u00fc\u00b7ter", "stan\u00b7den", "um", "ihn", ".", "Er", "sang", "in", "die", "m\u00e4ch\u00b7ti\u00b7ge", "Har\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Siehe, so werden die Auferstehungen jauchzen! so sang er!", "tokens": ["Sie\u00b7he", ",", "so", "wer\u00b7den", "die", "Auf\u00b7er\u00b7ste\u00b7hun\u00b7gen", "jauch\u00b7zen", "!", "so", "sang", "er", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VAFIN", "ART", "NN", "VVINF", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ewigkeit dir! komm, werde gebohren! o werde gebohren,", "tokens": ["E\u00b7wig\u00b7keit", "dir", "!", "komm", ",", "wer\u00b7de", "ge\u00b7boh\u00b7ren", "!", "o", "wer\u00b7de", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VVFIN", "$,", "VAFIN", "VVPP", "$.", "FM", "VAFIN", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Bluttag! \u2026 Er wandelt am Himmel herauf! Sein Nam ist, Erbarmer.", "tokens": ["Blut\u00b7tag", "!", "\u2026", "Er", "wan\u00b7delt", "am", "Him\u00b7mel", "her\u00b7auf", "!", "Sein", "Nam", "ist", ",", "Er\u00b7bar\u00b7mer", "."], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "$,", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Jhn, ihn segnen die Orionen, und rufen den kleinern", "tokens": ["Jhn", ",", "ihn", "seg\u00b7nen", "die", "O\u00b7rio\u00b7nen", ",", "und", "ru\u00b7fen", "den", "klei\u00b7nern"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Sonnen umher, die Sonnen den Erden: Du Tag! du Vers\u00f6hner!", "tokens": ["Son\u00b7nen", "um\u00b7her", ",", "die", "Son\u00b7nen", "den", "Er\u00b7den", ":", "Du", "Tag", "!", "du", "Ver\u00b7s\u00f6h\u00b7ner", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ART", "NN", "ART", "NN", "$.", "PPER", "NN", "$.", "PPER", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Theurer, sch\u00f6ner, blutender Tag, dich sandte die Liebe!", "tokens": ["Theu\u00b7rer", ",", "sch\u00f6\u00b7ner", ",", "blu\u00b7ten\u00b7der", "Tag", ",", "dich", "sand\u00b7te", "die", "Lie\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Harfe, t\u00f6ne darein! Er schaft, zu Engeln, den Staub um!", "tokens": ["Har\u00b7fe", ",", "t\u00f6\u00b7ne", "da\u00b7rein", "!", "Er", "schaft", ",", "zu", "En\u00b7geln", ",", "den", "Staub", "um", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PAV", "$.", "PPER", "ADJD", "$,", "APPR", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Ewigkeiten der Ruh sind seiner Triumphe Gefolge!", "tokens": ["E\u00b7wig\u00b7kei\u00b7ten", "der", "Ruh", "sind", "sei\u00b7ner", "Tri\u00b7um\u00b7phe", "Ge\u00b7fol\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Sieh, ich hebe mein Aug auf, und seh! Ein H\u00fcgel der Erden", "tokens": ["Sieh", ",", "ich", "he\u00b7be", "mein", "Aug", "auf", ",", "und", "seh", "!", "Ein", "H\u00fc\u00b7gel", "der", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Ist der Altar! Der Altar, er bebt vor dem kommenden Opfer!", "tokens": ["Ist", "der", "Al\u00b7tar", "!", "Der", "Al\u00b7tar", ",", "er", "bebt", "vor", "dem", "kom\u00b7men\u00b7den", "Op\u00b7fer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$.", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.13": {"text": "H\u00e4tte der Auszus\u00f6nende Sterne, wie Stein\u2019 aus den B\u00e4chen,", "tokens": ["H\u00e4t\u00b7te", "der", "Aus\u00b7zu\u00b7s\u00f6\u00b7nen\u00b7de", "Ster\u00b7ne", ",", "wie", "Stein'", "aus", "den", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "PWAV", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Aufgenommen, erbaut die Sterne dem Sohne zum Altar:", "tokens": ["Auf\u00b7ge\u00b7nom\u00b7men", ",", "er\u00b7baut", "die", "Ster\u00b7ne", "dem", "Soh\u00b7ne", "zum", "Al\u00b7tar", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Dennoch h\u00e4tte dem kommenden Opfer der Altar gezittert!", "tokens": ["Den\u00b7noch", "h\u00e4t\u00b7te", "dem", "kom\u00b7men\u00b7den", "Op\u00b7fer", "der", "Al\u00b7tar", "ge\u00b7zit\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Rings um schau ich. Wie l\u00e4cheln der Erde die helleren Sonnen!", "tokens": ["Rings", "um", "schau", "ich", ".", "Wie", "l\u00e4\u00b7cheln", "der", "Er\u00b7de", "die", "hel\u00b7le\u00b7ren", "Son\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "PPER", "$.", "PWAV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.17": {"text": "Und wie schwimmt ihr leichter Gefolge die Himmel herunter!", "tokens": ["Und", "wie", "schwimmt", "ihr", "leich\u00b7ter", "Ge\u00b7fol\u00b7ge", "die", "Him\u00b7mel", "her\u00b7un\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "O du Ruhe, des festlichsten unter den Festen! Du Sabbat!", "tokens": ["O", "du", "Ru\u00b7he", ",", "des", "fest\u00b7lichs\u00b7ten", "un\u00b7ter", "den", "Fes\u00b7ten", "!", "Du", "Sab\u00b7bat", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "ART", "ADJA", "APPR", "ART", "NN", "$.", "PPER", "NE", "$."], "meter": "+-+-+-+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.19": {"text": "Sabbat des Vaters und Sohns! Ich h\u00f6r, ich h\u00f6re, die Jubel,", "tokens": ["Sab\u00b7bat", "des", "Va\u00b7ters", "und", "Sohns", "!", "Ich", "h\u00f6r", ",", "ich", "h\u00f6\u00b7re", ",", "die", "Ju\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "T\u00f6nen von allen Harfen her\u00fcber! Der Seraphim Kronen", "tokens": ["T\u00f6\u00b7nen", "von", "al\u00b7len", "Har\u00b7fen", "her\u00b7\u00fc\u00b7ber", "!", "Der", "Se\u00b7ra\u00b7phim", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN", "ADV", "$.", "ART", "NN", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.21": {"text": "Sinken alle! Sie ist, die Sch\u00f6pfung ist Sabbat geworden!", "tokens": ["Sin\u00b7ken", "al\u00b7le", "!", "Sie", "ist", ",", "die", "Sch\u00f6p\u00b7fung", "ist", "Sab\u00b7bat", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$.", "PPER", "VAFIN", "$,", "ART", "NN", "VAFIN", "NE", "VAPP", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "O du Gedanke, Gedanke! Jahrtausende gehn noch vor\u00fcber!", "tokens": ["O", "du", "Ge\u00b7dan\u00b7ke", ",", "Ge\u00b7dan\u00b7ke", "!", "Jahr\u00b7tau\u00b7sen\u00b7de", "gehn", "noch", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,", "NN", "$.", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.23": {"text": "Eh von fern in dein heiliges Licht der Seraph hinaufblickt.", "tokens": ["Eh", "von", "fern", "in", "dein", "hei\u00b7li\u00b7ges", "Licht", "der", "Se\u00b7raph", "hin\u00b7auf\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Du! Der Sohn des Vaters, er starb! \u2026 Der Ewige denkt dich!", "tokens": ["Du", "!", "Der", "Sohn", "des", "Va\u00b7ters", ",", "er", "starb", "!", "\u2026", "Der", "E\u00b7wi\u00b7ge", "denkt", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "$.", "$(", "ART", "ADJA", "VVFIN", "PPER", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}}}}}