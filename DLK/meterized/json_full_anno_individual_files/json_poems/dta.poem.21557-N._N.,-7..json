{"dta.poem.21557": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Urewig ist des gro\u00dfen Welterhalters G\u00fcte,", "tokens": ["U\u00b7re\u00b7wig", "ist", "des", "gro\u00b7\u00dfen", "Wel\u00b7ter\u00b7hal\u00b7ters", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Urewig wechselt Herbstblattfall und Fr\u00fchlingsbl\u00fcthe,", "tokens": ["U\u00b7re\u00b7wig", "wech\u00b7selt", "Herbst\u00b7blatt\u00b7fall", "und", "Fr\u00fch\u00b7lings\u00b7bl\u00fct\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Urewig rollt der Klangstrom lyrischer Gedichte:", "tokens": ["U\u00b7re\u00b7wig", "rollt", "der", "Klangs\u00b7trom", "ly\u00b7ri\u00b7scher", "Ge\u00b7dich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Ein jedes Herz hat seine eigne Weltgeschichte.", "tokens": ["Ein", "je\u00b7des", "Herz", "hat", "sei\u00b7ne", "eig\u00b7ne", "Welt\u00b7ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}