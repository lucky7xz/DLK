{"textgrid.poem.48288": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Goodwin-Sand", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das sind die B\u00e4nke von Goodwin-Sand,", "tokens": ["Das", "sind", "die", "B\u00e4n\u00b7ke", "von", "Good\u00b7win\u00b7Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie sind nicht Meer, sie sind nicht Land,", "tokens": ["Sie", "sind", "nicht", "Meer", ",", "sie", "sind", "nicht", "Land", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie schieben sich, langsam, satt und schwer,", "tokens": ["Sie", "schie\u00b7ben", "sich", ",", "lang\u00b7sam", ",", "satt", "und", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie eine Schlange hin und her.", "tokens": ["Wie", "ei\u00b7ne", "Schlan\u00b7ge", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Schiffe, die mit dem Sturm gerungen", "tokens": ["Und", "die", "Schif\u00b7fe", ",", "die", "mit", "dem", "Sturm", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und die sch\u00e4umende Wut der Wellen bezwungen,", "tokens": ["Und", "die", "sch\u00e4u\u00b7men\u00b7de", "Wut", "der", "Wel\u00b7len", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und die gefahren \u00fcber die Welt,", "tokens": ["Und", "die", "ge\u00b7fah\u00b7ren", "\u00fc\u00b7ber", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Unzertr\u00fcmmert, unzerschellt,", "tokens": ["Un\u00b7zer\u00b7tr\u00fcm\u00b7mert", ",", "un\u00b7zer\u00b7schellt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie sehen die Heimat, sie sehen das Ziel,", "tokens": ["Sie", "se\u00b7hen", "die", "Hei\u00b7mat", ",", "sie", "se\u00b7hen", "das", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Da schiebt sich die Schlange unter den Kiel", "tokens": ["Da", "schiebt", "sich", "die", "Schlan\u00b7ge", "un\u00b7ter", "den", "Kiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und ringelt Schiff und Mannschaft hinab,", "tokens": ["Und", "rin\u00b7gelt", "Schiff", "und", "Mann\u00b7schaft", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Zugleich ihr Tod, zugleich ihr Grab.", "tokens": ["Zu\u00b7gleich", "ihr", "Tod", ",", "zu\u00b7gleich", "ihr", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die See ist still, die Ebb' ist nah,", "tokens": ["Die", "See", "ist", "still", ",", "die", "Ebb'", "ist", "nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mastspitzen ragen hier und da,", "tokens": ["Mast\u00b7spit\u00b7zen", "ra\u00b7gen", "hier", "und", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wo sie ragen in die Luft,", "tokens": ["Und", "wo", "sie", "ra\u00b7gen", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sind es Kreuze \u00fcber der Gruft;", "tokens": ["Da", "sind", "es", "Kreu\u00b7ze", "\u00fc\u00b7ber", "der", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ein Kirchhof ist's, halb Me