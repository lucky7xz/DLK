{"textgrid.poem.42013": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Still mu\u00df ich weinen: aber vergebens hofft", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still mu\u00df ich weinen: aber vergebens hofft", "tokens": ["Still", "mu\u00df", "ich", "wei\u00b7nen", ":", "a\u00b7ber", "ver\u00b7ge\u00b7bens", "hofft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "VVINF", "$.", "ADV", "ADV", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Von seinen Thr\u00e4nen freundlichen Trost das Herz;", "tokens": ["Von", "sei\u00b7nen", "Thr\u00e4\u00b7nen", "freund\u00b7li\u00b7chen", "Trost", "das", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn nicht der leise Ku\u00df der Wehmuth", "tokens": ["Denn", "nicht", "der", "lei\u00b7se", "Ku\u00df", "der", "Weh\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Haucht in die Brust mir die Rosend\u00e4mmrung.", "tokens": ["Haucht", "in", "die", "Brust", "mir", "die", "Ro\u00b7sen\u00b7d\u00e4mm\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Nein, schwarz und n\u00e4chtlich windet der bittre Schmerz", "tokens": ["Nein", ",", "schwarz", "und", "n\u00e4cht\u00b7lich", "win\u00b7det", "der", "bitt\u00b7re", "Schmerz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJD", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Kalt um des Busens k\u00e4mpfende Gluth sich her,", "tokens": ["Kalt", "um", "des", "Bu\u00b7sens", "k\u00e4mp\u00b7fen\u00b7de", "Gluth", "sich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "PRF", "PTKVZ", "$,"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und unbewegt umgraut des Tr\u00fcbsinns", "tokens": ["Und", "un\u00b7be\u00b7wegt", "um\u00b7graut", "des", "Tr\u00fcb\u00b7sinns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Br\u00fctende Wolke den zartern Schimmer,", "tokens": ["Br\u00fc\u00b7ten\u00b7de", "Wol\u00b7ke", "den", "zar\u00b7tern", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Worin durch Schmerz die Seele sich gl\u00fccklicher,", "tokens": ["Wo\u00b7rin", "durch", "Schmerz", "die", "See\u00b7le", "sich", "gl\u00fcck\u00b7li\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch bange Thr\u00e4nen sch\u00f6ner der Geist sich f\u00fchlt;", "tokens": ["Durch", "ban\u00b7ge", "Thr\u00e4\u00b7nen", "sch\u00f6\u00b7ner", "der", "Geist", "sich", "f\u00fchlt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ach, weinen mu\u00df ich, r\u00e4th des Unmuths", "tokens": ["Ach", ",", "wei\u00b7nen", "mu\u00df", "ich", ",", "r\u00e4\u00b7th", "des", "Un\u00b7muths"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVINF", "VMFIN", "PPER", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Stimme mir gleich, da\u00df ich z\u00fcrnend l\u00e4chle.", "tokens": ["Stim\u00b7me", "mir", "gleich", ",", "da\u00df", "ich", "z\u00fcr\u00b7nend", "l\u00e4ch\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sprich, was verbrach ich, da\u00df du den scharfen Pfeil", "tokens": ["Sprich", ",", "was", "ver\u00b7brach", "ich", ",", "da\u00df", "du", "den", "schar\u00b7fen", "Pfeil"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Des kalten Spottes tief in die Brust mir senkst?", "tokens": ["Des", "kal\u00b7ten", "Spot\u00b7tes", "tief", "in", "die", "Brust", "mir", "senkst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ungleichen Kampf beginnst du, drohst mit", "tokens": ["Un\u00b7glei\u00b7chen", "Kampf", "be\u00b7ginnst", "du", ",", "drohst", "mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$,", "ADV", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Waffen, die nimmer dich selbst verwunden!", "tokens": ["Waf\u00b7fen", ",", "die", "nim\u00b7mer", "dich", "selbst", "ver\u00b7wun\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Nein, muth'ge Flucht nur bleibt dem gekr\u00e4nkten Geist;", "tokens": ["Nein", ",", "muth'\u00b7ge", "Flucht", "nur", "bleibt", "dem", "ge\u00b7kr\u00e4nk\u00b7ten", "Geist", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit kr\u00e4ft'gem Mannssinn tilgt er die starke Gluth", "tokens": ["Mit", "kr\u00e4ft'\u00b7gem", "Manns\u00b7sinn", "tilgt", "er", "die", "star\u00b7ke", "Gluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und lacht des Schmerzes, wenn gewaltsam", "tokens": ["Und", "lacht", "des", "Schmer\u00b7zes", ",", "wenn", "ge\u00b7walt\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KOUS", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ringend die z\u00fcrnende Flamm' emporstrebt.", "tokens": ["Rin\u00b7gend", "die", "z\u00fcr\u00b7nen\u00b7de", "Flamm'", "em\u00b7por\u00b7strebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Nie kann Verachtung dulden der freie Sinn,", "tokens": ["Nie", "kann", "Ver\u00b7ach\u00b7tung", "dul\u00b7den", "der", "frei\u00b7e", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nie kann Verachtung dulden das Herz, das dich", "tokens": ["Nie", "kann", "Ver\u00b7ach\u00b7tung", "dul\u00b7den", "das", "Herz", ",", "das", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "VVINF", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit seiner Fl\u00fcgel k\u00fchnstem Aufschwung,", "tokens": ["Mit", "sei\u00b7ner", "Fl\u00fc\u00b7gel", "k\u00fchns\u00b7tem", "Auf\u00b7schwung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich, die dem G\u00f6ttlichen naht, geliebt hat.", "tokens": ["Dich", ",", "die", "dem", "G\u00f6tt\u00b7li\u00b7chen", "naht", ",", "ge\u00b7liebt", "hat", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "VVPP", "VAFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "D\u00fcrft' ich zu dir aufschauen, zu dir, die k\u00fchn", "tokens": ["D\u00fcrft'", "ich", "zu", "dir", "auf\u00b7schau\u00b7en", ",", "zu", "dir", ",", "die", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,", "APPR", "PPER", "$,", "PRELS", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Blick emporhebt, jeglicher Fessel z\u00fcrnt", "tokens": ["Den", "Blick", "em\u00b7por\u00b7hebt", ",", "jeg\u00b7li\u00b7cher", "Fes\u00b7sel", "z\u00fcrnt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gro\u00df im heil'gen Kranz der Freiheit", "tokens": ["Und", "gro\u00df", "im", "heil'\u00b7gen", "Kranz", "der", "Frei\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liebe dem gleichen Gem\u00fcth nur weihn kann?", "tokens": ["Lie\u00b7be", "dem", "glei\u00b7chen", "Ge\u00b7m\u00fcth", "nur", "weihn", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Ja, ich bin stolz, nicht l\u00e4ugn' ich den eignen Werth,", "tokens": ["Ja", ",", "ich", "bin", "stolz", ",", "nicht", "l\u00e4ugn'", "ich", "den", "eig\u00b7nen", "Werth", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$,", "PTKNEG", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Stolz auf das Recht, das stark in der Brust mir thront,", "tokens": ["Stolz", "auf", "das", "Recht", ",", "das", "stark", "in", "der", "Brust", "mir", "thront", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und stolzer auf den Strahl der Sch\u00f6nheit,", "tokens": ["Und", "stol\u00b7zer", "auf", "den", "Strahl", "der", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welchen ein Gott in das Herz mir senkte.", "tokens": ["Wel\u00b7chen", "ein", "Gott", "in", "das", "Herz", "mir", "senk\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Der zarten Kette, welche den Geist versch\u00f6nt,", "tokens": ["Der", "zar\u00b7ten", "Ket\u00b7te", ",", "wel\u00b7che", "den", "Geist", "ver\u00b7sch\u00f6nt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn sie ihn fesselt, schmieg' ich mich willig an:", "tokens": ["Wenn", "sie", "ihn", "fes\u00b7selt", ",", "schmieg'", "ich", "mich", "wil\u00b7lig", "an", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "$,", "VVFIN", "PPER", "PRF", "ADJD", "PTKVZ", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Doch streckt der Willk\u00fcr Spott vergebens", "tokens": ["Doch", "streckt", "der", "Will\u00b7k\u00fcr", "Spott", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber das heil'ge Gef\u00fchl den Scepter.", "tokens": ["Ue\u00b7ber", "das", "heil'\u00b7ge", "Ge\u00b7f\u00fchl", "den", "Scep\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Sprich, ist's ein Frevel, da\u00df ich unendlich dich", "tokens": ["Sprich", ",", "ist's", "ein", "Fre\u00b7vel", ",", "da\u00df", "ich", "un\u00b7end\u00b7lich", "dich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "PPER"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und alles Sch\u00f6ne innig in dir geliebt,", "tokens": ["Und", "al\u00b7les", "Sch\u00f6\u00b7ne", "in\u00b7nig", "in", "dir", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df ich in ird'scher Nacht des Himmels", "tokens": ["Da\u00df", "ich", "in", "ird'\u00b7scher", "Nacht", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freundlichem Strahle mich fromm vertraute?", "tokens": ["Freund\u00b7li\u00b7chem", "Strah\u00b7le", "mich", "fromm", "ver\u00b7trau\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Dann schreck' erz\u00fcrnt der Donner des Ewigen", "tokens": ["Dann", "schreck'", "er\u00b7z\u00fcrnt", "der", "Don\u00b7ner", "des", "E\u00b7wi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die weiche Seele, welche mit Kindessinn", "tokens": ["Die", "wei\u00b7che", "See\u00b7le", ",", "wel\u00b7che", "mit", "Kin\u00b7des\u00b7sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Bl\u00fcthenduft und in des Fr\u00fchlings", "tokens": ["Im", "Bl\u00fc\u00b7then\u00b7duft", "und", "in", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zaubergewand, in dem goldnen Strahle,", "tokens": ["Zau\u00b7ber\u00b7ge\u00b7wand", ",", "in", "dem", "gold\u00b7nen", "Strah\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Der hehr emporschwebt, und in des Mondes Licht,", "tokens": ["Der", "hehr", "em\u00b7por\u00b7schwebt", ",", "und", "in", "des", "Mon\u00b7des", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die leise Spur der ewigen Lieb' erkennt,", "tokens": ["Die", "lei\u00b7se", "Spur", "der", "e\u00b7wi\u00b7gen", "Lieb'", "er\u00b7kennt", ","], "token_info": ["word", "w