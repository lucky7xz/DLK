{"textgrid.poem.47002": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun will ich fahren in die Grube", "tokens": ["Nun", "will", "ich", "fah\u00b7ren", "in", "die", "Gru\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit leichtem Herzen, unverzagt", "tokens": ["Mit", "leich\u00b7tem", "Her\u00b7zen", ",", "un\u00b7ver\u00b7zagt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schamfrei, da der D\u00e4nenbube", "tokens": ["Und", "scham\u00b7frei", ",", "da", "der", "D\u00e4\u00b7nen\u00b7bu\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Schleswig-Holstein ist gejagt.", "tokens": ["Aus", "Schles\u00b7wig\u00b7Hol\u00b7stein", "ist", "ge\u00b7jagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da wieder predigen und lehren", "tokens": ["Da", "wie\u00b7der", "pre\u00b7di\u00b7gen", "und", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Kirchen und in Schulen darf", "tokens": ["In", "Kir\u00b7chen", "und", "in", "Schu\u00b7len", "darf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die deutsche Zunge, die mit Ehren", "tokens": ["Die", "deut\u00b7sche", "Zun\u00b7ge", ",", "die", "mit", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Th\u00fcr' hinaus die fremde warf.", "tokens": ["Zur", "Th\u00fcr'", "hin\u00b7aus", "die", "frem\u00b7de", "warf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APZR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Komm, d\u00e4nischer Schulmeister! tunken", "tokens": ["Komm", ",", "d\u00e4\u00b7ni\u00b7scher", "Schul\u00b7meis\u00b7ter", "!", "tun\u00b7ken"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df dich in Teer und Federn ganz,", "tokens": ["La\u00df", "dich", "in", "Teer", "und", "Fe\u00b7dern", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mit den \u00fcbrigen Halunken", "tokens": ["Und", "mit", "den", "\u00fcb\u00b7ri\u00b7gen", "Ha\u00b7lun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Heb' dich, flieg' hin als Eidergans!", "tokens": ["Heb'", "dich", ",", "flieg'", "hin", "als", "Ei\u00b7der\u00b7gans", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN"