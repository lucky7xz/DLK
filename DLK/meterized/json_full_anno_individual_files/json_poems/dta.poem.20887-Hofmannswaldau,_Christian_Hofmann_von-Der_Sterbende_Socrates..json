{"dta.poem.20887": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wann Argwohn und Verdrie\u00dfligkeit", "tokens": ["Wann", "Arg\u00b7wohn", "und", "Ver\u00b7drie\u00df\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns Mund und Wangen bleich gemachet/", "tokens": ["Uns", "Mund", "und", "Wan\u00b7gen", "bleich", "ge\u00b7ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So steht die Seele dann bereit/", "tokens": ["So", "steht", "die", "See\u00b7le", "dann", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und schafft da\u00df unsre Krafft erwachet;", "tokens": ["Und", "schafft", "da\u00df", "uns\u00b7re", "Krafft", "er\u00b7wa\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wiewol ihr Raht offt ist verlachet/", "tokens": ["Wie\u00b7wol", "ihr", "Raht", "offt", "ist", "ver\u00b7la\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ausgeschlagen vor der Zeit.", "tokens": ["Und", "aus\u00b7ge\u00b7schla\u00b7gen", "vor", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}