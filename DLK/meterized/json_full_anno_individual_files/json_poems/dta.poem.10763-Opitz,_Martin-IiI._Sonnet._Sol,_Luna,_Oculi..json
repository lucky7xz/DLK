{"dta.poem.10763": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "IiI. Sonnet.  \n Sol, Luna, Oculi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr beyde/ Sonn/ vnd Mon/ der Welt zwey klare Augen/", "tokens": ["Ihr", "bey\u00b7de", "/", "Sonn", "/", "vnd", "Mon", "/", "der", "Welt", "zwey", "kla\u00b7re", "Au\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$(", "NN", "$(", "KON", "NE", "$(", "ART", "NN", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was k\u00f6nnet jhr doch mehr/ dennur den grossen Saal", "tokens": ["Was", "k\u00f6n\u00b7net", "jhr", "doch", "mehr", "/", "den\u00b7nur", "den", "gros\u00b7sen", "Saal"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "$(", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "De\u00df Himmels reitten durch/ vnd scheinen vberall?", "tokens": ["De\u00df", "Him\u00b7mels", "reit\u00b7ten", "durch", "/", "vnd", "schei\u00b7nen", "vbe\u00b7rall", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$(", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Was k\u00f6nnet jhr/ als nur der Erden Safft aussaugen/", "tokens": ["Was", "k\u00f6n\u00b7net", "jhr", "/", "als", "nur", "der", "Er\u00b7den", "Safft", "aus\u00b7sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$(", "KOKOM", "ADV", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vnd geben wiederumb? Secht die Zwey Eugelein/", "tokens": ["Vnd", "ge\u00b7ben", "wie\u00b7de\u00b7rumb", "?", "Secht", "die", "Zwey", "Eu\u00b7ge\u00b7lein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "NN", "ART", "CARD", "NN", "$("], "meter": "-+-+--+-++-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Die Euch an glantz vnd krafft weit vorzu ziehen sein.", "tokens": ["Die", "Euch", "an", "glantz", "vnd", "krafft", "weit", "vor\u00b7zu", "zie\u00b7hen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "ADJD", "ADV", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Herr Bartsch hat manchen orth durchsuchet bi\u00df anher/", "tokens": ["Herr", "Bart\u00b7sch", "hat", "man\u00b7chen", "orth", "durch\u00b7su\u00b7chet", "bi\u00df", "an\u00b7her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "PIAT", "NN", "VVFIN", "APPR", "ADJA", "$("], "meter": "++--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Gereiset vber Land/ gezogen vber Meer.", "tokens": ["Ge\u00b7rei\u00b7set", "vber", "Land", "/", "ge\u00b7zo\u00b7gen", "vber", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "VVPP", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Zwey Augen hatten Ihm sein Hertze gantz benommen", "tokens": ["Zwey", "Au\u00b7gen", "hat\u00b7ten", "Ihm", "sein", "Hert\u00b7ze", "gantz", "be\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "PPER", "PPOSAT", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd in den Brand gesteckt. Es trib jhn heim der Brand", "tokens": ["Vnd", "in", "den", "Brand", "ge\u00b7steckt", ".", "Es", "trib", "jhn", "heim", "der", "Brand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da war kein retten mehr/ bi\u00df er ein Auge fandt/", "tokens": ["Da", "war", "kein", "ret\u00b7ten", "mehr", "/", "bi\u00df", "er", "ein", "Au\u00b7ge", "fandt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Damit erlescht das Fewr/ das von zwey Augen kommen.", "tokens": ["Da\u00b7mit", "er\u00b7lescht", "das", "Fewr", "/", "das", "von", "zwey", "Au\u00b7gen", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$(", "ART", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}