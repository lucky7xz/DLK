{"textgrid.poem.44445": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Erkl\u00e4rung eines Kupfers in dem Taschenbuche Vesta f\u00fcrs Jahr 1833, ein kleines M\u00e4dchen mit einer Taube, daneben eine Katze, vorstellend", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach du sch\u00f6ne, wei\u00dfe Taube,", "tokens": ["Ach", "du", "sch\u00f6\u00b7ne", ",", "wei\u00b7\u00dfe", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zitterst du gleich Espenlaube,", "tokens": ["Zit\u00b7terst", "du", "gleich", "E\u00b7spen\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmiegst dich bang mit scheuem Sinn", "tokens": ["Schmiegst", "dich", "bang", "mit", "scheu\u00b7em", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An die holde Sch\u00fctzerin?", "tokens": ["An", "die", "hol\u00b7de", "Sch\u00fct\u00b7ze\u00b7rin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl mit Recht warnt dieses Zagen! \u2013", "tokens": ["Wohl", "mit", "Recht", "warnt", "die\u00b7ses", "Za\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PDAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vieles darf der Starke wagen.", "tokens": ["Vie\u00b7les", "darf", "der", "Star\u00b7ke", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gierde lauert, Unschuld weint,", "tokens": ["Gier\u00b7de", "lau\u00b7ert", ",", "Un\u00b7schuld", "weint", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich sehe deinen Feind.", "tokens": ["Und", "ich", "se\u00b7he", "dei\u00b7nen", "Feind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einen nur der langen Reihe,", "tokens": ["Ei\u00b7nen", "nur", "der", "lan\u00b7gen", "Rei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Adler, Falke, Sperber, Weihe,", "tokens": ["Ad\u00b7ler", ",", "Fal\u00b7ke", ",", "Sper\u00b7ber", ",", "Wei\u00b7he", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Glatt und kraus, mit Streif und Stern,", "tokens": ["Glatt", "und", "kraus", ",", "mit", "Streif", "und", "Stern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle fressen T\u00e4ubchen gern.", "tokens": ["Al\u00b7le", "fres\u00b7sen", "T\u00e4ub\u00b7chen"