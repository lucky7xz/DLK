{"dta.poem.20312": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An ihren spiegel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du gl\u00e4ntzendes chrystall/ du redner ohne mund/", "tokens": ["Du", "gl\u00e4nt\u00b7zen\u00b7des", "chrys\u00b7tall", "/", "du", "red\u00b7ner", "oh\u00b7ne", "mund", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "ADJA", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rathgeber/ dessen treu und klugheit sattsam kund/", "tokens": ["Rath\u00b7ge\u00b7ber", "/", "des\u00b7sen", "treu", "und", "klug\u00b7heit", "satt\u00b7sam", "kund", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDS", "ADJD", "KON", "NN", "ADJD", "PTKVZ", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Freund/ der durch warheit blo\u00df die hertzen ihm verbindet/", "tokens": ["Freund", "/", "der", "durch", "war\u00b7heit", "blo\u00df", "die", "hert\u00b7zen", "ihm", "ver\u00b7bin\u00b7det", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "APPR", "NN", "ADV", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Du zeuge sonder list und falsche heucheley/", "tokens": ["Du", "zeu\u00b7ge", "son\u00b7der", "list", "und", "fal\u00b7sche", "heu\u00b7che\u00b7ley", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bekenn/ ob irgend auch was angenehmers sey/", "tokens": ["Be\u00b7kenn", "/", "ob", "ir\u00b7gend", "auch", "was", "an\u00b7ge\u00b7neh\u00b7mers", "sey", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ADV", "ADV", "PWS", "ADJA", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als Dorisettens pracht/ die mich so starck entz\u00fcndet?", "tokens": ["Als", "Do\u00b7ri\u00b7set\u00b7tens", "pracht", "/", "die", "mich", "so", "starck", "ent\u00b7z\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "$(", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr auge/ wei\u00df ich/ ist dir gar zu wohl bekandt/", "tokens": ["Ihr", "au\u00b7ge", "/", "wei\u00df", "ich", "/", "ist", "dir", "gar", "zu", "wohl", "be\u00b7kandt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "PPER", "$(", "VAFIN", "PPER", "ADV", "APPR", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie hat dir manchen blick in dein chrystall gesandt:", "tokens": ["Sie", "hat", "dir", "man\u00b7chen", "blick", "in", "dein", "chrys\u00b7tall", "ge\u00b7sandt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drum ist dir ihre glut auch schwerlich unverholen.", "tokens": ["Drum", "ist", "dir", "ih\u00b7re", "glut", "auch", "schwer\u00b7lich", "un\u00b7ver\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht wundre dich demnach/ wenn auch mein geist verletzt/", "tokens": ["Nicht", "wund\u00b7re", "dich", "dem\u00b7nach", "/", "wenn", "auch", "mein", "geist", "ver\u00b7letzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "PAV", "$(", "KOUS", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer kan/ wenn solcher blitz an unsre hertzen setzt/", "tokens": ["Wer", "kan", "/", "wenn", "sol\u00b7cher", "blitz", "an", "uns\u00b7re", "hert\u00b7zen", "setzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$(", "KOUS", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bey feur und flammen seyn als ausgel\u00f6schte kohlen?", "tokens": ["Bey", "feur", "und", "flam\u00b7men", "seyn", "als", "aus\u00b7ge\u00b7l\u00f6schte", "koh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "VAINF", "KOKOM", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Jedoch ich brenne gern in dieser stillen glut/", "tokens": ["Je\u00b7doch", "ich", "bren\u00b7ne", "gern", "in", "die\u00b7ser", "stil\u00b7len", "glut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ob schon sich in sich selbst verzehrt mein heisses blut/", "tokens": ["Ob", "schon", "sich", "in", "sich", "selbst", "ver\u00b7zehrt", "mein", "heis\u00b7ses", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "APPR", "PRF", "ADV", "VVPP", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "M\u00f6cht ihre wehmuth nur mich meiner noth entbinden;", "tokens": ["M\u00f6cht", "ih\u00b7re", "weh\u00b7muth", "nur", "mich", "mei\u00b7ner", "noth", "ent\u00b7bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Allein/ ihr hartes hertz/ das kein erbarmen tr\u00e4gt/", "tokens": ["Al\u00b7lein", "/", "ihr", "har\u00b7tes", "hertz", "/", "das", "kein", "er\u00b7bar\u00b7men", "tr\u00e4gt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPOSAT", "ADJA", "NN", "$(", "ART", "PIAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wird durch mein heisses flehn und seuffzen nicht bewegt/", "tokens": ["Wird", "durch", "mein", "heis\u00b7ses", "flehn", "und", "seuff\u00b7zen", "nicht", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "KON", "VVFIN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und l\u00e4st sich gegen mich ohn alle regung finden.", "tokens": ["Und", "l\u00e4st", "sich", "ge\u00b7gen", "mich", "ohn", "al\u00b7le", "re\u00b7gung", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach spiegel/ straffe du die strenge grausamkeit!", "tokens": ["Ach", "spie\u00b7gel", "/", "straf\u00b7fe", "du", "die", "stren\u00b7ge", "grau\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und r\u00e4che mich an ihr. Es ist nun einmahl zeit/", "tokens": ["Und", "r\u00e4\u00b7che", "mich", "an", "ihr", ".", "Es", "ist", "nun", "ein\u00b7mahl", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPER", "$.", "PPER", "VAFIN", "ADV", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ihr hart und kaltes hertz in heisse glut zu setzen.", "tokens": ["Ihr", "hart", "und", "kal\u00b7tes", "hertz", "in", "heis\u00b7se", "glut", "zu", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wirff ihren glantz zur\u00fcck/ der durch die augen blitzt/", "tokens": ["Wirff", "ih\u00b7ren", "glantz", "zu\u00b7r\u00fcck", "/", "der", "durch", "die", "au\u00b7gen", "blitzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKVZ", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und la\u00df dieselbe glut/ die mich und dich erhitzt/", "tokens": ["Und", "la\u00df", "die\u00b7sel\u00b7be", "glut", "/", "die", "mich", "und", "dich", "er\u00b7hitzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PDAT", "NN", "$(", "PRELS", "PPER", "KON", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ihr aug und hertze selbst durch eigne krafft verletzen.", "tokens": ["Ihr", "aug", "und", "hert\u00b7ze", "selbst", "durch", "eig\u00b7ne", "krafft", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}