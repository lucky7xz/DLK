{"dta.poem.23451": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Emancipation.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du raubest unter unsern F\u00fc\u00dfen", "tokens": ["Du", "rau\u00b7best", "un\u00b7ter", "un\u00b7sern", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns unser deutsches Vaterland:", "tokens": ["Uns", "un\u00b7ser", "deut\u00b7sches", "Va\u00b7ter\u00b7land", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "O Israel, von Gott gekehret,", "tokens": ["O", "Is\u00b7rael", ",", "von", "Gott", "ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du dich selbst zum Gott gemacht,", "tokens": ["Hast", "du", "dich", "selbst", "zum", "Gott", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bist, durch diesen Gott belehret,", "tokens": ["Und", "bist", ",", "durch", "die\u00b7sen", "Gott", "be\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Wucher, Lug und Trug bedacht.", "tokens": ["Auf", "Wu\u00b7cher", ",", "Lug", "und", "Trug", "be\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Willst du von diesem Gott nicht lassen,", "tokens": ["Willst", "du", "von", "die\u00b7sem", "Gott", "nicht", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PDAT", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nie \u00f6ffne Deutschland dir sein Ohr!", "tokens": ["Nie", "\u00f6ff\u00b7ne", "Deutschland", "dir", "sein", "Ohr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Willst du nicht deine Knechtschaft hassen,", "tokens": ["Willst", "du", "nicht", "dei\u00b7ne", "Knecht\u00b7schaft", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nie ziehst du durch der Freiheit Thor.", "tokens": ["Nie", "ziehst", "du", "durch", "der", "Frei\u00b7heit", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}