{"dta.poem.19471": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der gesch\u00e4ndete Baum.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Sie haben mit dem Beile dich zerschnitten,", "tokens": ["Sie", "ha\u00b7ben", "mit", "dem", "Bei\u00b7le", "dich", "zer\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Frevler \u2014 hast du viel dabei gelitten?", "tokens": ["Die", "Frev\u00b7ler", "hast", "du", "viel", "da\u00b7bei", "ge\u00b7lit\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich selber habe sorglich dich verbunden", "tokens": ["Ich", "sel\u00b7ber", "ha\u00b7be", "sorg\u00b7lich", "dich", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und traue: Junger Baum, du wirst gesunden!", "tokens": ["Und", "trau\u00b7e", ":", "Jun\u00b7ger", "Baum", ",", "du", "wirst", "ge\u00b7sun\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auch ich erlitt zu schier derselben Stunde", "tokens": ["Auch", "ich", "er\u00b7litt", "zu", "schier", "der\u00b7sel\u00b7ben", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PTKA", "ADJD", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von sch\u00e4rferm Messer eine tiefre Wunde.", "tokens": ["Von", "sch\u00e4r\u00b7ferm", "Mes\u00b7ser", "ei\u00b7ne", "tief\u00b7re", "Wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu untersuchen komm' ich t\u00e4glich deine", "tokens": ["Zu", "un\u00b7ter\u00b7su\u00b7chen", "komm'", "ich", "t\u00e4g\u00b7lich", "dei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "ADJD", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und unertr\u00e4glich brennen f\u00fchl' ich meine.", "tokens": ["Und", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "bren\u00b7nen", "f\u00fchl'", "ich", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Du saugest gierig ein die Kraft der Erde,", "tokens": ["Du", "sau\u00b7gest", "gie\u00b7rig", "ein", "die", "Kraft", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mir ist, als ob auch ich durchrieselt werde!", "tokens": ["Mir", "ist", ",", "als", "ob", "auch", "ich", "durc\u00b7hrie\u00b7selt", "wer\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ADV", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der frische Saft quillt aus zerschnittner Rinde", "tokens": ["Der", "fri\u00b7sche", "Saft", "quillt", "aus", "zer\u00b7schnitt\u00b7ner", "Rin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Heilsam. Mir ist, als ob auch ich's empfinde!", "tokens": ["Heil\u00b7sam", ".", "Mir", "ist", ",", "als", "ob", "auch", "ich's", "emp\u00b7fin\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ADV", "PIS", "VVFIN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.13": {"text": "Indem ich ", "tokens": ["In\u00b7dem", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Ist mir, als ob sich ", "tokens": ["Ist", "mir", ",", "als", "ob", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KOKOM", "KOUS", "PRF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Natur beginnt zu wirken und zu weben,", "tokens": ["Na\u00b7tur", "be\u00b7ginnt", "zu", "wir\u00b7ken", "und", "zu", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Ich traue: Beiden geht es nicht ans Leben!", "tokens": ["Ich", "trau\u00b7e", ":", "Bei\u00b7den", "geht", "es", "nicht", "ans", "Le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PIS", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wie viele, ", "tokens": ["Wie", "vie\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "PIS", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Wir beide prahlen noch mit unsern Narben!", "tokens": ["Wir", "bei\u00b7de", "prah\u00b7len", "noch", "mit", "un\u00b7sern", "Nar\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}