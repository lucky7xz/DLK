{"textgrid.poem.52924": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Meer, o heil'ges Meer! Nach deiner Frische,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Meer, o heil'ges Meer! Nach deiner Frische,", "tokens": ["O", "Meer", ",", "o", "heil'\u00b7ges", "Meer", "!", "Nach", "dei\u00b7ner", "Fri\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach deinem Frieden lechzet meine Seele:", "tokens": ["Nach", "dei\u00b7nem", "Frie\u00b7den", "lech\u00b7zet", "mei\u00b7ne", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So schreit um Wasser durch die Nacht der B\u00fcsche", "tokens": ["So", "schreit", "um", "Was\u00b7ser", "durch", "die", "Nacht", "der", "B\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Hindin trockne, todes-wunde Kehle.", "tokens": ["Der", "Hin\u00b7din", "trock\u00b7ne", ",", "to\u00b7des\u00b7wun\u00b7de", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mich widert's an, der T\u00e4ler und der Berge", "tokens": ["Mich", "wi\u00b7dert's", "an", ",", "der", "T\u00e4\u00b7ler", "und", "der", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Abwechselnd' Spiel und ew'ge Einerleiheit!", "tokens": ["Ab\u00b7wech\u00b7selnd'", "Spiel", "und", "ew'\u00b7ge", "Ei\u00b7ner\u00b7lei\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wer rettet mich aus diesem Bann der Zwerge", "tokens": ["Wer", "ret\u00b7tet", "mich", "aus", "die\u00b7sem", "Bann", "der", "Zwer\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dein Asyl, du Element der Freiheit?", "tokens": ["In", "dein", "A\u00b7syl", ",", "du", "E\u00b7le\u00b7ment", "der", "Frei\u00b7heit", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo aus der Brandung jauchzendem Gebr\u00fclle", "tokens": ["Wo", "aus", "der", "Bran\u00b7dung", "jauch\u00b7zen\u00b7dem", "Ge\u00b7br\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Alln\u00e4chtlich ihre Hymnen aufw\u00e4rts fliegen,", "tokens": ["All\u00b7n\u00e4cht\u00b7lich", "ih\u00b7re", "Hym\u00b7nen", "auf\u00b7w\u00e4rts", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo einst, des Zwanges ledig und der H\u00fclle,", "tokens": ["Wo", "einst", ",", "des", "Zwan\u00b7ges", "le\u00b7dig", "und", "der", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "ART", "NN", "ADJD", "KON", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die Sch\u00f6nheit selber nackt emporgestiegen!", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "sel\u00b7ber", "nackt", "em\u00b7por\u00b7ge\u00b7stie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was ist das Land und seine kurzen Lenze,", "tokens": ["Was", "ist", "das", "Land", "und", "sei\u00b7ne", "kur\u00b7zen", "Len\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wind und Frost in einer Nacht verjagen,", "tokens": ["Die", "Wind", "und", "Frost", "in", "ei\u00b7ner", "Nacht", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Nachtigallen, die um welke Kr\u00e4nze", "tokens": ["Was", "Nach\u00b7ti\u00b7gal\u00b7len", ",", "die", "um", "wel\u00b7ke", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$,", "PRELS", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und um mi\u00dfwachsne Blumen einsam klagen?", "tokens": ["Und", "um", "mi\u00df\u00b7wachs\u00b7ne", "Blu\u00b7men", "ein\u00b7sam", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Auch du tr\u00e4gst Bl\u00fcten, blendender als diese,", "tokens": ["Auch", "du", "tr\u00e4gst", "Bl\u00fc\u00b7ten", ",", "blen\u00b7den\u00b7der", "als", "die\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "NN", "$,", "ADJD", "KOKOM", "PDS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die schaumgekr\u00f6nten Wipfel deiner Wogen,", "tokens": ["Die", "schaum\u00b7ge\u00b7kr\u00f6n\u00b7ten", "Wip\u00b7fel", "dei\u00b7ner", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Sonnenlicht gr\u00fcnt ewig deine Wiese,", "tokens": ["Im", "Son\u00b7nen\u00b7licht", "gr\u00fcnt", "e\u00b7wig", "dei\u00b7ne", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begrenzt nur von des Himmels blauem Bogen.", "tokens": ["Be\u00b7grenzt", "nur", "von", "des", "Him\u00b7mels", "blau\u00b7em", "Bo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Dir rei\u00dft den Scho\u00df, den heiligen der Mutter,", "tokens": ["Dir", "rei\u00dft", "den", "Scho\u00df", ",", "den", "hei\u00b7li\u00b7gen", "der", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Eisen auf, habgierig drin zu w\u00fchlen,", "tokens": ["Kein", "Ei\u00b7sen", "auf", ",", "hab\u00b7gie\u00b7rig", "drin", "zu", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr irdisch Rindvieh bietest du kein Futter", "tokens": ["F\u00fcr", "ir\u00b7disch", "Rind\u00b7vieh", "bie\u00b7test", "du", "kein", "Fut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "NN", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und darfst der Sohle eklen Tritt nicht f\u00fchlen.", "tokens": ["Und", "darfst", "der", "Soh\u00b7le", "ek\u00b7len", "Tritt", "nicht", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Dich hemmt des Eises Joch nicht und der Br\u00fccken,", "tokens": ["Dich", "hemmt", "des", "Ei\u00b7ses", "Joch", "nicht", "und", "der", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "PTKNEG", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der D\u00e4mme lachst du, will dein Zorn erwachen,", "tokens": ["Der", "D\u00e4m\u00b7me", "lachst", "du", ",", "will", "dein", "Zorn", "er\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch schaukelst du auf deinem freien R\u00fccken", "tokens": ["Doch", "schau\u00b7kelst", "du", "auf", "dei\u00b7nem", "frei\u00b7en", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den freien Mann im kecken Schiffer-Nachen.", "tokens": ["Den", "frei\u00b7en", "Mann", "im", "ke\u00b7cken", "Schif\u00b7fer\u00b7Nach\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "O Meer, o Meer! durch deiner Bl\u00fcten Mitte", "tokens": ["O", "Meer", ",", "o", "Meer", "!", "durch", "dei\u00b7ner", "Bl\u00fc\u00b7ten", "Mit\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "FM", "NN", "$.", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An gr\u00fcnen H\u00fcgeln jach emporzuklimmen,", "tokens": ["An", "gr\u00fc\u00b7nen", "H\u00fc\u00b7geln", "jach", "em\u00b7por\u00b7zu\u00b7klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Arm und Ku\u00df der weichen Amphitrite", "tokens": ["Im", "Arm", "und", "Ku\u00df", "der", "wei\u00b7chen", "Am\u00b7phit\u00b7ri\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den h\u00fcpfenden Delphinen nachzuschwimmen,", "tokens": ["Den", "h\u00fcp\u00b7fen\u00b7den", "Del\u00b7phi\u00b7nen", "nach\u00b7zu\u00b7schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Weil unten aus dem Abgrunds klarem D\u00fcster", "tokens": ["Weil", "un\u00b7ten", "aus", "dem", "Ab\u00b7grunds", "kla\u00b7rem", "D\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Ew'gen Auge auf uns starrt und leuchtet,", "tokens": ["Des", "Ew'\u00b7gen", "Au\u00b7ge", "auf", "uns", "starrt", "und", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und z\u00fcgelloser Wellenrosse N\u00fcster", "tokens": ["Und", "z\u00fc\u00b7gel\u00b7lo\u00b7ser", "Wel\u00b7len\u00b7ros\u00b7se", "N\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit wei\u00dfem Schaum uns Haupt und Nacken feuchtet:", "tokens": ["Mit", "wei\u00b7\u00dfem", "Schaum", "uns", "Haupt", "und", "Na\u00b7cken", "feuch\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Das nenn' ich Lust und Kampf und Sieg und Leben,", "tokens": ["Das", "nenn'", "ich", "Lust", "und", "Kampf", "und", "Sieg", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das gute Rast, wann sp\u00e4t im Abenddunkeln", "tokens": ["Das", "gu\u00b7te", "Rast", ",", "wann", "sp\u00e4t", "im", "A\u00b7bend\u00b7dun\u00b7keln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Segel hochgebl\u00e4ht zum Hafen schweben,", "tokens": ["Die", "Se\u00b7gel", "hoch\u00b7ge\u00b7bl\u00e4ht", "zum", "Ha\u00b7fen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Ruder all', umspr\u00fcht von hellem Funkeln.", "tokens": ["Die", "Ru\u00b7der", "all'", ",", "um\u00b7spr\u00fcht", "von", "hel\u00b7lem", "Fun\u00b7keln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Meer, heil'ges Meer! Dir send' ich diese Gr\u00fc\u00dfe,", "tokens": ["Meer", ",", "heil'\u00b7ges", "Meer", "!", "Dir", "send'", "ich", "die\u00b7se", "Gr\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Um dich, verlornes, klagen diese Lieder;", "tokens": ["Um", "dich", ",", "ver\u00b7lor\u00b7nes", ",", "kla\u00b7gen", "die\u00b7se", "Lie\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "ADJA", "$,", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur einmal noch, bevor ich scheiden m\u00fcsse,", "tokens": ["Nur", "ein\u00b7mal", "noch", ",", "be\u00b7vor", "ich", "schei\u00b7den", "m\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zeig Gottes Spiegel mir, dein Antlitz, wieder!", "tokens": ["Zeig", "Got\u00b7tes", "Spie\u00b7gel", "mir", ",", "dein", "Ant\u00b7litz", ",", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "NN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "O Meer, o heil'ges Meer! Nach deiner Frische,", "tokens": ["O", "Meer", ",", "o", "heil'\u00b7ges", "Meer", "!", "Nach", "dei\u00b7ner", "Fri\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach deinem Frieden lechzet meine Seele:", "tokens": ["Nach", "dei\u00b7nem", "Frie\u00b7den", "lech\u00b7zet", "mei\u00b7ne", "See\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3":