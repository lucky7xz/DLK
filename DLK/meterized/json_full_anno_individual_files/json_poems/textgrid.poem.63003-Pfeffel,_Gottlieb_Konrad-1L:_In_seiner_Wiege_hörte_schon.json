{"textgrid.poem.63003": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: In seiner Wiege h\u00f6rte schon", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In seiner Wiege h\u00f6rte schon", "tokens": ["In", "sei\u00b7ner", "Wie\u00b7ge", "h\u00f6r\u00b7te", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kronprinz in dem Reich der Quadrupeden", "tokens": ["Der", "Kron\u00b7prinz", "in", "dem", "Reich", "der", "Quad\u00b7ru\u00b7pe\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Viel Arges von dem Wolf und seiner Mordsucht reden.", "tokens": ["Viel", "Ar\u00b7ges", "von", "dem", "Wolf", "und", "sei\u00b7ner", "Mord\u00b7sucht", "re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NE", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch stieg er kaum auf seiner Ahnen Thron,", "tokens": ["Auch", "stieg", "er", "kaum", "auf", "sei\u00b7ner", "Ah\u00b7nen", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So hie\u00df es den Verr\u00e4ther t\u00f6dten", "tokens": ["So", "hie\u00df", "es", "den", "Ver\u00b7r\u00e4\u00b7ther", "t\u00f6d\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und all sein Gut durch Confiscation", "tokens": ["Und", "all", "sein", "Gut", "durch", "Con\u00b7fis\u00b7ca\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zur k\u00f6niglichen Kammer schlagen.", "tokens": ["Zur", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Kam\u00b7mer", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Elephanten ward die Execution", "tokens": ["Dem", "E\u00b7le\u00b7phan\u00b7ten", "ward", "die", "Ex\u00b7e\u00b7cu\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des ernsten Urtheils aufgetragen:", "tokens": ["Des", "erns\u00b7ten", "Ur\u00b7theils", "auf\u00b7ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Allein er schlug es aus, inde\u00df die Nation,", "tokens": ["Al\u00b7lein", "er", "schlug", "es", "aus", ",", "in\u00b7de\u00df", "die", "Na\u00b7tion", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+---+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Besonders Ochs und Sch\u00f6ps, die stets am lautsten loben,", "tokens": ["Be\u00b7son\u00b7ders", "Ochs", "und", "Sch\u00f6ps", ",", "die", "stets", "am", "lauts\u00b7ten", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,", "PRELS", "ADV", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des Schachs Gerechtigkeit erhoben.", "tokens": ["Des", "Schachs", "Ge\u00b7rech\u00b7tig\u00b7keit", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbwas h\u00f6r ich? rief mit wildem Blick", "tokens": ["\u00bb", "was", "h\u00f6r", "ich", "?", "rief", "mit", "wil\u00b7dem", "Blick"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "PPER", "$.", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Majest\u00e4t: du darfst noch widerstreben?", "tokens": ["Die", "Ma\u00b7jes\u00b7t\u00e4t", ":", "du", "darfst", "noch", "wi\u00b7der\u00b7stre\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "PAV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ich will ja meiner V\u00f6lker Gl\u00fcck:", "tokens": ["Ich", "will", "ja", "mei\u00b7ner", "V\u00f6l\u00b7ker", "Gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Verw\u00fcrkte nicht der Wolf sein Leben?\u00ab", "tokens": ["Ver\u00b7w\u00fcrk\u00b7te", "nicht", "der", "Wolf", "sein", "Le\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NE", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Gut, sprach der Elephant, doch auch ein B\u00f6sewicht", "tokens": ["Gut", ",", "sprach", "der", "E\u00b7le\u00b7phant", ",", "doch", "auch", "ein", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Soll niemals ungeh\u00f6rt, soll durch das Blutgericht", "tokens": ["Soll", "nie\u00b7mals", "un\u00b7ge\u00b7h\u00f6rt", ",", "soll", "durch", "das", "Blut\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "$,", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nach dem Gesetz verurtheilt werden:", "tokens": ["Nach", "dem", "Ge\u00b7setz", "ver\u00b7urt\u00b7heilt", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Heut strafst du den Tyrann der Heerden,", "tokens": ["Heut", "strafst", "du", "den", "Ty\u00b7rann", "der", "Heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und morgen hei\u00dft vielleicht, auf des Verl\u00e4umders Rath,", "tokens": ["Und", "mor\u00b7gen", "hei\u00dft", "viel\u00b7leicht", ",", "auf", "des", "Ver\u00b7l\u00e4um\u00b7ders", "Rath", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dein Wink den treuen Hund entleiben.", "tokens": ["Dein", "Wink", "den", "treu\u00b7en", "Hund", "ent\u00b7lei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "\u00bbschweig und entferne dich! versetzt der Potentat,", "tokens": ["\u00bb", "schweig", "und", "ent\u00b7fer\u00b7ne", "dich", "!", "ver\u00b7setzt", "der", "Po\u00b7ten\u00b7tat", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Du kannst nicht mehr mein Kanzler bleiben.\u00ab", "tokens": ["Du", "kannst", "nicht", "mehr", "mein", "Kanz\u00b7ler", "blei\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Er gab die Stelle dem Rhinocero\u00df", "tokens": ["Er", "gab", "die", "Stel\u00b7le", "dem", "Rhi\u00b7no\u00b7ce\u00b7ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Und das war gleich bereit, den Gaudieb aufzureiben.", "tokens": ["Und", "das", "war", "gleich", "be\u00b7reit", ",", "den", "Gau\u00b7di\u00b7eb", "auf\u00b7zu\u00b7rei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "VVIZU", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "Der Elephant zog auf sein \u00f6des Schlo\u00df", "tokens": ["Der", "E\u00b7le\u00b7phant", "zog", "auf", "sein", "\u00f6\u00b7des", "Schlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und weinte bey den Wunderthaten", "tokens": ["Und", "wein\u00b7te", "bey", "den", "Wun\u00b7der\u00b7tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Des allzuraschen Monokraten", "tokens": ["Des", "all\u00b7zu\u00b7ra\u00b7schen", "Mo\u00b7no\u00b7kra\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Oft eine stille Thr\u00e4nenfluth.", "tokens": ["Oft", "ei\u00b7ne", "stil\u00b7le", "Thr\u00e4\u00b7nen\u00b7fluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir grauet, edler Freund, vor unsern goldnen Zeiten;", "tokens": ["Mir", "grau\u00b7et", ",", "ed\u00b7ler", "Freund", ",", "vor", "un\u00b7sern", "gold\u00b7nen", "Zei\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Gute, das ein F\u00fcrst despotisch thut,", "tokens": ["Das", "Gu\u00b7te", ",", "das", "ein", "F\u00fcrst", "des\u00b7po\u00b7tisch", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und w\u00e4r es noch so sch\u00f6n und noch so gut,", "tokens": ["Und", "w\u00e4r", "es", "noch", "so", "sch\u00f6n", "und", "noch", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Emp\u00f6rt so sehr als Grausamkeiten.", "tokens": ["Em\u00b7p\u00f6rt", "so", "sehr", "als", "Grau\u00b7sam\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In seiner Wiege h\u00f6rte schon", "tokens": ["In", "sei\u00b7ner", "Wie\u00b7ge", "h\u00f6r\u00b7te", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kronprinz in dem Reich der Quadrupeden", "tokens": ["Der", "Kron\u00b7prinz", "in", "dem", "Reich", "der", "Quad\u00b7ru\u00b7pe\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Viel Arges von dem Wolf und seiner Mordsucht reden.", "tokens": ["Viel", "Ar\u00b7ges", "von", "dem", "Wolf", "und", "sei\u00b7ner", "Mord\u00b7sucht", "re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NE", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch stieg er kaum auf seiner Ahnen Thron,", "tokens": ["Auch", "stieg", "er", "kaum", "auf", "sei\u00b7ner", "Ah\u00b7nen", "Thron", ","], "to