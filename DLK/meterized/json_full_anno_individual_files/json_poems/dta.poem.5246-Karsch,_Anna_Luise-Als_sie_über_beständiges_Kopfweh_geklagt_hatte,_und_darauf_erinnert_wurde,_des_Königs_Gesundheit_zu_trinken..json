{"dta.poem.5246": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als sie \u00fcber best\u00e4ndiges Kopfweh  \n geklagt hatte, und darauf erinnert wurde,  \n des K\u00f6nigs Gesundheit zu trinken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Du Gla\u00df! an meines Grabes Rande", "tokens": ["Du", "Gla\u00df", "!", "an", "mei\u00b7nes", "Gra\u00b7bes", "Ran\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$.", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Leer\u2019 ich dich nicht mehr aus!", "tokens": ["Leer'", "ich", "dich", "nicht", "mehr", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch stamml\u2019 ich noch, bey schon verdunkeltem Verstande:", "tokens": ["Doch", "stamml'", "ich", "noch", ",", "bey", "schon", "ver\u00b7dun\u00b7kel\u00b7tem", "Ver\u00b7stan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Es lebe Friedrich und sein Haus!", "tokens": ["Es", "le\u00b7be", "Fried\u00b7rich", "und", "sein", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}