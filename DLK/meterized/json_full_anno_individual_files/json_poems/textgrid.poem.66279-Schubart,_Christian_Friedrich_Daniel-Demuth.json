{"textgrid.poem.66279": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Demuth", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Demuth, Braut des Himmels, h\u00f6re", "tokens": ["De\u00b7muth", ",", "Braut", "des", "Him\u00b7mels", ",", "h\u00f6\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute deinen Lobgesang,", "tokens": ["Heu\u00b7te", "dei\u00b7nen", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Christenhymnen, Engelch\u00f6re", "tokens": ["Chris\u00b7ten\u00b7hym\u00b7nen", ",", "En\u00b7gelc\u00b7h\u00f6\u00b7re"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singen, G\u00f6ttin, deine Ehre", "tokens": ["Sin\u00b7gen", ",", "G\u00f6t\u00b7tin", ",", "dei\u00b7ne", "Eh\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter Himmelsharfenklang!", "tokens": ["Un\u00b7ter", "Him\u00b7mels\u00b7har\u00b7fen\u00b7klang", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Engel sind das Bild der Demuth,", "tokens": ["En\u00b7gel", "sind", "das", "Bild", "der", "De\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wenn sie vor Jehovah stehn,", "tokens": ["Wenn", "sie", "vor", "Je\u00b7ho\u00b7vah", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und vor seines Thrones Lichte", "tokens": ["Und", "vor", "sei\u00b7nes", "Thro\u00b7nes", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit bedecktem Angesichte", "tokens": ["Mit", "be\u00b7deck\u00b7tem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ehrfurchtsvoll vor\u00fcbergehn.", "tokens": ["Ehr\u00b7furchts\u00b7voll", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jene vier und zwanzig Alte", "tokens": ["Je\u00b7ne", "vier", "und", "zwan\u00b7zig", "Al\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "CARD", "KON", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beugen schweigend ihre Knie.", "tokens": ["Beu\u00b7gen", "schwei\u00b7gend", "ih\u00b7re", "Knie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unterm Donner neuer Lieder", "tokens": ["Un\u00b7term", "Don\u00b7ner", "neu\u00b7er", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Legen sie die Krone nieder;", "tokens": ["Le\u00b7gen", "sie", "die", "Kro\u00b7ne", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die Demuth lehrt es sie.", "tokens": ["Denn", "die", "De\u00b7muth", "lehrt", "es", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "In den H\u00f6hen, in den Tiefen", "tokens": ["In", "den", "H\u00f6\u00b7hen", ",", "in", "den", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beugt die ganze Sch\u00f6pfung sich.", "tokens": ["Beugt", "die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geister in des Himmels L\u00fcften,", "tokens": ["Geis\u00b7ter", "in", "des", "Him\u00b7mels", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wesen in des Mondes D\u00fcften,", "tokens": ["We\u00b7sen", "in", "des", "Mon\u00b7des", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Preisen dich, Jehovah, dich!", "tokens": ["Prei\u00b7sen", "dich", ",", "Je\u00b7ho\u00b7vah", ",", "dich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "$,", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jesus, aller Welten Erbe,", "tokens": ["Je\u00b7sus", ",", "al\u00b7ler", "Wel\u00b7ten", "Er\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er verlie\u00df des Vaters Scho\u00df.", "tokens": ["Er", "ver\u00b7lie\u00df", "des", "Va\u00b7ters", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht durch Stolz, der Gott entehret,", "tokens": ["Nicht", "durch", "Stolz", ",", "der", "Gott", "en\u00b7teh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sich gegen ihn emp\u00f6ret;", "tokens": ["Und", "sich", "ge\u00b7gen", "ihn", "em\u00b7p\u00f6\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur durch Demuth war er gro\u00df.", "tokens": ["Nur", "durch", "De\u00b7muth", "war", "er", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Satan, einst ein Sohn des Himmels,", "tokens": ["Sa\u00b7tan", ",", "einst", "ein", "Sohn", "des", "Him\u00b7mels", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trotzte Gott mit k\u00fchner Wuth.", "tokens": ["Trotz\u00b7te", "Gott", "mit", "k\u00fch\u00b7ner", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der schwindelnde Rebelle", "tokens": ["Doch", "der", "schwin\u00b7deln\u00b7de", "Re\u00b7bel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Sank von seiner Seraphsstelle", "tokens": ["Sank", "von", "sei\u00b7ner", "Se\u00b7raphs\u00b7stel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In der H\u00f6lle Schwefelgluth.", "tokens": ["In", "der", "H\u00f6l\u00b7le", "Schwe\u00b7fel\u00b7gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Jeder aufgeth\u00fcrmte Trotzer,", "tokens": ["Je\u00b7der", "auf\u00b7ge\u00b7th\u00fcrm\u00b7te", "Trot\u00b7zer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der im Stolze Satan gleicht,", "tokens": ["Der", "im", "Stol\u00b7ze", "Sa\u00b7tan", "gleicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist ein Feind von Gottes Gr\u00f6\u00dfe,", "tokens": ["Ist", "ein", "Feind", "von", "Got\u00b7tes", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er erkennt nicht seine Bl\u00f6\u00dfe,", "tokens": ["Er", "er\u00b7kennt", "nicht", "sei\u00b7ne", "Bl\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil er von der Demuth weicht.", "tokens": ["Weil", "er", "von", "der", "De\u00b7muth", "weicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Demuth ist des Mannes Harnisch,", "tokens": ["De\u00b7muth", "ist", "des", "Man\u00b7nes", "Har\u00b7nisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist des Weisen Diadem.", "tokens": ["Ist", "des", "Wei\u00b7sen", "Di\u00b7a\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Nur die sanften Christenseelen,", "tokens": ["Nur", "die", "sanf\u00b7ten", "Chris\u00b7ten\u00b7see\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mit Demuth sich verm\u00e4hlen,", "tokens": ["Die", "mit", "De\u00b7muth", "sich", "ver\u00b7m\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Sind dem Sch\u00f6pfer angenehm.", "tokens": ["Sind", "dem", "Sch\u00f6p\u00b7fer", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Demuth ist der goldne G\u00fcrtel,", "tokens": ["De\u00b7muth", "ist", "der", "gold\u00b7ne", "G\u00fcr\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der die T\u00f6chter Eva's schm\u00fcckt.", "tokens": ["Der", "die", "T\u00f6ch\u00b7ter", "E\u00b7va's", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ohne sie wird jede Sch\u00f6ne", "tokens": ["Oh\u00b7ne", "sie", "wird", "je\u00b7de", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frommen Seelen zum Geh\u00f6hne;", "tokens": ["From\u00b7men", "See\u00b7len", "zum", "Ge\u00b7h\u00f6h\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur der Demuth Reiz entz\u00fcckt.", "tokens": ["Nur", "der", "De\u00b7muth", "Reiz", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sei so reich, wie Peru's T\u00f6chter,", "tokens": ["Sei", "so", "reich", ",", "wie", "Pe\u00b7ru's", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PWAV", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4ufe Gold, wie Meeressand;", "tokens": ["H\u00e4u\u00b7fe", "Gold", ",", "wie", "Mee\u00b7res\u00b7sand", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich' den Grazien an Sch\u00f6ne,", "tokens": ["Gleich'", "den", "Gra\u00b7zi\u00b7en", "an", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fe\u00dfle alle Erdens\u00f6hne:", "tokens": ["Fe\u00df\u00b7le", "al\u00b7le", "Er\u00b7den\u00b7s\u00f6h\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne Demuth ist es Tand.", "tokens": ["Oh\u00b7ne", "De\u00b7muth", "ist", "es", "Tand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach, drum fl\u00fcstert meine Seele", "tokens": ["Ach", ",", "drum", "fl\u00fcs\u00b7tert", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PAV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Seufzer, Gott, du dir:", "tokens": ["Die\u00b7sen", "Seuf\u00b7zer", ",", "Gott", ",", "du", "dir", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NN", "$,", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht um G\u00fcter, die vergehen,", "tokens": ["Nicht", "um", "G\u00fc\u00b7ter", ",", "die", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll dich meine Seele flehen,", "tokens": ["Soll", "dich", "mei\u00b7ne", "See\u00b7le", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur um Demuth fleht sie dir.", "tokens": ["Nur", "um", "De\u00b7muth", "fleht", "sie", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.12": {"line.1": {"text": "Demuth, Braut des Himmels, h\u00f6re", "tokens": ["De\u00b7muth", ",", "Braut", "des", "Him\u00b7mels", ",", "h\u00f6\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute deinen Lobgesang,", "tokens": ["Heu\u00b7te", "dei\u00b7nen", "Lob\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Christenhymnen, Engelch\u00f6re", "tokens": ["Chris\u00b7ten\u00b7hym\u00b7nen", ",", "En\u00b7gelc\u00b7h\u00f6\u00b7re"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singen, G\u00f6ttin, deine Ehre", "tokens": ["Sin\u00b7gen", ",", "G\u00f6t\u00b7tin", ",", "dei\u00b7ne", "Eh\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "