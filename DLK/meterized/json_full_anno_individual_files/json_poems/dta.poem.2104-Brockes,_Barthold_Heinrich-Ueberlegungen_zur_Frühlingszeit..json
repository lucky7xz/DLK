{"dta.poem.2104": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ueberlegungen zur Fr\u00fchlingszeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft aber dennoch die Erkenntni\u00df der Wissensschw\u00e4ch\u2019", "tokens": ["La\u00dft", "a\u00b7ber", "den\u00b7noch", "die", "Er\u00b7kennt\u00b7ni\u00df", "der", "Wis\u00b7sens\u00b7schw\u00e4ch'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "uns nicht erschrecken.", "tokens": ["uns", "nicht", "er\u00b7schre\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wir wissen hier, so viel wir sollen, und zwar nach un-", "tokens": ["Wir", "wis\u00b7sen", "hier", ",", "so", "viel", "wir", "sol\u00b7len", ",", "und", "zwar", "nach", "un"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,", "KON", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "serm Stande gnug,", "tokens": ["serm", "Stan\u00b7de", "gnug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und klagen darum, da\u00df wir hier nicht mehr erkennen,", "tokens": ["Und", "kla\u00b7gen", "da\u00b7rum", ",", "da\u00df", "wir", "hier", "nicht", "mehr", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "nicht mit Fug.", "tokens": ["nicht", "mit", "Fug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Wir scheinen hier blo\u00df zum Bewundern von Gott in diese", "tokens": ["Wir", "schei\u00b7nen", "hier", "blo\u00df", "zum", "Be\u00b7wun\u00b7dern", "von", "Gott", "in", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPRART", "NN", "APPR", "NN", "APPR", "PDAT"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.8": {"text": "Welt gesetzt,", "tokens": ["Welt", "ge\u00b7setzt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Und, im Gesch\u00f6pf, uns sein zu freuen. Wie da\u00df man", "tokens": ["Und", ",", "im", "Ge\u00b7sch\u00f6pf", ",", "uns", "sein", "zu", "freu\u00b7en", ".", "Wie", "da\u00df", "man"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPRART", "NN", "$,", "PPER", "VAINF", "PTKZU", "VVINF", "$.", "KOKOM", "KOUS", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "sich denn nicht ergetzt!", "tokens": ["sich", "denn", "nicht", "er\u00b7getzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Wobey man doch aus der Vernunft, auch aus der Bi-", "tokens": ["Wo\u00b7bey", "man", "doch", "aus", "der", "Ver\u00b7nunft", ",", "auch", "aus", "der", "Bi"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "bel, Gr\u00fcnde nimmt,", "tokens": ["bel", ",", "Gr\u00fcn\u00b7de", "nimmt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Es hab\u2019 uns Gott nach diesem Leben zu h\u00f6h\u2019rer Wissen-", "tokens": ["Es", "hab'", "uns", "Gott", "nach", "die\u00b7sem", "Le\u00b7ben", "zu", "h\u00f6h'\u00b7rer", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "APPR", "PDAT", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "schaft bestimmt.", "tokens": ["schaft", "be\u00b7stimmt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}