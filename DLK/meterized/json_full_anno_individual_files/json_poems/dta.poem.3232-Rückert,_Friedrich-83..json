{"dta.poem.3232": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie der Schieferstift auf Schiefertafeln geht,", "tokens": ["Sieh", ",", "wie", "der", "Schie\u00b7fers\u00b7tift", "auf", "Schie\u00b7fer\u00b7ta\u00b7feln", "geht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soda\u00df die graue Schrift auf schwarzem Grunde steht;", "tokens": ["So\u00b7da\u00df", "die", "grau\u00b7e", "Schrift", "auf", "schwar\u00b7zem", "Grun\u00b7de", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Tafel und der Stift, sind sie nicht gleichentstammt?", "tokens": ["Die", "Ta\u00b7fel", "und", "der", "Stift", ",", "sind", "sie", "nicht", "glei\u00b7chent\u00b7stammt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "VAFIN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wie ist ihr Beruf verschieden und ihr Amt!", "tokens": ["Doch", "wie", "ist", "ihr", "Be\u00b7ruf", "ver\u00b7schie\u00b7den", "und", "ihr", "Amt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PPOSAT", "NN", "VVPP", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wirken beide, wie sie gleichem Grund entstammen,", "tokens": ["Doch", "wir\u00b7ken", "bei\u00b7de", ",", "wie", "sie", "glei\u00b7chem", "Grund", "ent\u00b7stam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "PWAV", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschieden wirkend, auch zu gleichem Werk zusammen.", "tokens": ["Ver\u00b7schie\u00b7den", "wir\u00b7kend", ",", "auch", "zu", "glei\u00b7chem", "Werk", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und in der Schrift ist Stift und Tafel nicht zu scheiden;", "tokens": ["Und", "in", "der", "Schrift", "ist", "Stift", "und", "Ta\u00b7fel", "nicht", "zu", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "NN", "KON", "NN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Lamm ist wie die Trift, und eins ist Thun und Leiden.", "tokens": ["Das", "Lamm", "ist", "wie", "die", "Trift", ",", "und", "eins", "ist", "Thun", "und", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,", "KON", "PIS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du trag, ob du der Stift, ob magst die Tafel seyn,", "tokens": ["Du", "trag", ",", "ob", "du", "der", "Stift", ",", "ob", "magst", "die", "Ta\u00b7fel", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "$,", "KOUS", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Deine bei zur Schrift, da\u00df sie sei sch\u00f6n und fein.", "tokens": ["Das", "Dei\u00b7ne", "bei", "zur", "Schrift", ",", "da\u00df", "sie", "sei", "sch\u00f6n", "und", "fein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "APPR", "APPRART", "NN", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}