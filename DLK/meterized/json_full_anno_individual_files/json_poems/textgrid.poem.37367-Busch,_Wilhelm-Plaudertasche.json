{"textgrid.poem.37367": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Plaudertasche", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebes Plapperm\u00e4ulchen,", "tokens": ["Du", "lie\u00b7bes", "Plap\u00b7per\u00b7m\u00e4ul\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bedenk dich erst ein Weilchen", "tokens": ["Be\u00b7denk", "dich", "erst", "ein", "Weil\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sprich nicht so geschwind.", "tokens": ["Und", "sprich", "nicht", "so", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du bist wie unsre M\u00fchle", "tokens": ["Du", "bist", "wie", "uns\u00b7re", "M\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit ihrem Fl\u00fcgelspiele", "tokens": ["Mit", "ih\u00b7rem", "Fl\u00fc\u00b7gel\u00b7spie\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im frischen Sausewind.", "tokens": ["Im", "fri\u00b7schen", "Sau\u00b7se\u00b7wind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So lang der M\u00fcller t\u00e4tig", "tokens": ["So", "lang", "der", "M\u00fcl\u00b7ler", "t\u00e4