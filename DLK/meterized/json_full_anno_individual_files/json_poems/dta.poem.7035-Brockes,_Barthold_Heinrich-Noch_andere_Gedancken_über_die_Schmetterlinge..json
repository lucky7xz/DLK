{"dta.poem.7035": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Noch andere Gedancken \u00fcber  \n die Schmetterlinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey aller dieser Wunder-Menge", "tokens": ["Bey", "al\u00b7ler", "die\u00b7ser", "Wun\u00b7der\u00b7Men\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bewundert ich insonderheit,", "tokens": ["Be\u00b7wun\u00b7dert", "ich", "in\u00b7son\u00b7der\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der regen Zunge schlancke L\u00e4nge,", "tokens": ["Der", "re\u00b7gen", "Zun\u00b7ge", "schlan\u00b7cke", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die, welches ja wol wunderbar,", "tokens": ["Die", ",", "wel\u00b7ches", "ja", "wol", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Viel l\u00e4nger, als das gantze Thierchen war:", "tokens": ["Viel", "l\u00e4n\u00b7ger", ",", "als", "das", "gant\u00b7ze", "Thier\u00b7chen", "war", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und die es doch mit solcher Fertigkeit,", "tokens": ["Und", "die", "es", "doch", "mit", "sol\u00b7cher", "Fer\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und fast unglaublicher Geschwindigkeit regierte,", "tokens": ["Und", "fast", "un\u00b7glaub\u00b7li\u00b7cher", "Ge\u00b7schwin\u00b7dig\u00b7keit", "re\u00b7gier\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und mit denselbigen der Kost nach sp\u00fcrte.", "tokens": ["Und", "mit", "den\u00b7sel\u00b7bi\u00b7gen", "der", "Kost", "nach", "sp\u00fcr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "ART", "NN", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ich dachte, wie so gro\u00df die Krafft", "tokens": ["Ich", "dach\u00b7te", ",", "wie", "so", "gro\u00df", "die", "Krafft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Solch langes Werckzeug zu bewegen.", "tokens": ["Solch", "lan\u00b7ges", "Wer\u00b7ck\u00b7zeug", "zu", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}