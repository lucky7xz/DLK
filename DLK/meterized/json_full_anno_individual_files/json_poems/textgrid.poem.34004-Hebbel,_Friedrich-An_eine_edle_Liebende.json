{"textgrid.poem.34004": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An eine edle Liebende", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du meinst in deiner Seele D\u00e4mmerweben,", "tokens": ["Du", "meinst", "in", "dei\u00b7ner", "See\u00b7le", "D\u00e4m\u00b7mer\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir sei das Tiefste so gel\u00f6s't in Liebe,", "tokens": ["Dir", "sei", "das", "Tiefs\u00b7te", "so", "ge\u00b7l\u00f6s't", "in", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df dir nichts Eig'nes zu bewahren bliebe,", "tokens": ["Da\u00df", "dir", "nichts", "Eig'\u00b7nes", "zu", "be\u00b7wah\u00b7ren", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PIS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum willst du ganz und gar dich ihm ergeben.", "tokens": ["Drum", "willst", "du", "ganz", "und", "gar", "dich", "ihm", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "KON", "ADV", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O, thu es nicht! Es giebt ein Widerstreben,", "tokens": ["O", ",", "thu", "es", "nicht", "!", "Es", "giebt", "ein", "Wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So rein von jedem selbstisch-rohen Triebe,", "tokens": ["So", "rein", "von", "je\u00b7dem", "selbstischro\u00b7hen", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sich das H\u00f6chste still zu Nichts zerriebe,", "tokens": ["Da\u00df", "sich", "das", "H\u00f6chs\u00b7te", "still", "zu", "Nichts", "zer\u00b7rie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "ADJD", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erschl\u00f6sse die\u00df ihm nicht ein ew'ges Leben.", "tokens": ["Er\u00b7schl\u00f6s\u00b7se", "die\u00df", "ihm", "nicht", "ein", "ew'\u00b7ges", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und k\u00f6nntest du, im Edelsten erglommen,", "tokens": ["Und", "k\u00f6nn\u00b7test", "du", ",", "im", "E\u00b7dels\u00b7ten", "er\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auch deines Wesens Form vor ihm vernichten \u2013", "tokens": ["Auch", "dei\u00b7nes", "We\u00b7sens", "Form", "vor", "ihm", "ver\u00b7nich\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Elemente bleiben, die sie waren!", "tokens": ["Die", "E\u00b7le\u00b7men\u00b7te", "blei\u00b7ben", ",", "die", "sie", "wa\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "So wird dein Opfer niemals ganz vollkommen,", "tokens": ["So", "wird", "dein", "Op\u00b7fer", "nie\u00b7mals", "ganz", "voll\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du kannst nicht v\u00f6llig auf dich selbst verzichten,", "tokens": ["Du", "kannst", "nicht", "v\u00f6l\u00b7lig", "auf", "dich", "selbst", "ver\u00b7zich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum sorge du, dich ganz zu offenbaren!", "tokens": ["Drum", "sor\u00b7ge", "du", ",", "dich", "ganz", "zu", "of\u00b7fen\u00b7ba\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du meinst in deiner Seele D\u00e4mmerweben,", "tokens": ["Du", "meinst", "in", "dei\u00b7ner", "See\u00b7le", "D\u00e4m\u00b7mer\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "wor