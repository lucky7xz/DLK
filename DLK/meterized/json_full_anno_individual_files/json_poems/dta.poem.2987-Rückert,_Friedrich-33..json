{"dta.poem.2987": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zuerst erschaffen sind die Zwerg' im \u00f6den Grauen", "tokens": ["Zu\u00b7erst", "er\u00b7schaf\u00b7fen", "sind", "die", "Zwer\u00b7g'", "im", "\u00f6\u00b7den", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Sch\u00f6pfung, um die Berg' und Grotten anzubauen.", "tokens": ["Der", "Sch\u00f6p\u00b7fung", ",", "um", "die", "Ber\u00b7g'", "und", "Grot\u00b7ten", "an\u00b7zu\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "ART", "NN", "KON", "NN", "VVIZU", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch sie bedr\u00e4ngten W\u00fcrm' und Drachen, und um diesen", "tokens": ["Doch", "sie", "be\u00b7dr\u00e4ng\u00b7ten", "W\u00fcrm'", "und", "Dra\u00b7chen", ",", "und", "um", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "KON", "NN", "$,", "KON", "APPR", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu steuern, wurden dann im Sturm erschaffen Riesen.", "tokens": ["Zu", "steu\u00b7ern", ",", "wur\u00b7den", "dann", "im", "Sturm", "er\u00b7schaf\u00b7fen", "Rie\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Riesen schlugen mit dem Ger die Drachen todt,", "tokens": ["Die", "Rie\u00b7sen", "schlu\u00b7gen", "mit", "dem", "Ger", "die", "Dra\u00b7chen", "todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch brachten sie vielmehr die Zwerge selbst in Noth.", "tokens": ["Doch", "brach\u00b7ten", "sie", "viel\u00b7mehr", "die", "Zwer\u00b7ge", "selbst", "in", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Zum Schutz der Zwerge sind die Menschen dann erschaffen,", "tokens": ["Zum", "Schutz", "der", "Zwer\u00b7ge", "sind", "die", "Men\u00b7schen", "dann", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Zwerge schmiedeten geschwind den Helden Waffen.", "tokens": ["Die", "Zwer\u00b7ge", "schmie\u00b7de\u00b7ten", "ge\u00b7schwind", "den", "Hel\u00b7den", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie schmieden Waffen, die sie selbst nicht k\u00f6nnen brauchen,", "tokens": ["Sie", "schmie\u00b7den", "Waf\u00b7fen", ",", "die", "sie", "selbst", "nicht", "k\u00f6n\u00b7nen", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Menschenhelden sie ins Blut der Riesen tauchen.", "tokens": ["Da\u00df", "Men\u00b7schen\u00b7hel\u00b7den", "sie", "ins", "Blut", "der", "Rie\u00b7sen", "tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Helden schlugen nun die Riesen todt, und blieben", "tokens": ["Die", "Hel\u00b7den", "schlu\u00b7gen", "nun", "die", "Rie\u00b7sen", "todt", ",", "und", "blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "ADJD", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zwerge Freunde, bis sie endlich sie vertrieben.", "tokens": ["Der", "Zwer\u00b7ge", "Freun\u00b7de", ",", "bis", "sie", "end\u00b7lich", "sie", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KOUS", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die Riesen starben und die Zwerge zogen aus,", "tokens": ["Die", "Rie\u00b7sen", "star\u00b7ben", "und", "die", "Zwer\u00b7ge", "zo\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun ist im Erdenrund der Mensch allein zuhaus.", "tokens": ["Nun", "ist", "im", "Er\u00b7den\u00b7rund", "der", "Mensch", "al\u00b7lein", "zu\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Zwerge sind zu klein, die Riesen sind zu gro\u00df,", "tokens": ["Die", "Zwer\u00b7ge", "sind", "zu", "klein", ",", "die", "Rie\u00b7sen", "sind", "zu", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das rechte Ma\u00df der Welt ist Menschengr\u00f6\u00dfe blo\u00df.", "tokens": ["Das", "rech\u00b7te", "Ma\u00df", "der", "Welt", "ist", "Men\u00b7schen\u00b7gr\u00f6\u00b7\u00dfe", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}