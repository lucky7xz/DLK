], "pos": ["ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "dem Wiege-Gast", "tokens": ["dem", "Wie\u00b7ge\u00b7Gast"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "ins Herz jubelnd,", "tokens": ["ins", "Herz", "ju\u00b7belnd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.7": {"text": "dann f\u00e4rbt sich rosig,", "tokens": ["dann", "f\u00e4rbt", "sich", "ro\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "schwingt er zur\u00fcck,", "tokens": ["schwingt", "er", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "das graue Zwielicht,", "tokens": ["das", "grau\u00b7e", "Zwie\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "und jauchzend schw\u00f6rt er", "tokens": ["und", "jauch\u00b7zend", "schw\u00f6rt", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "dem goldigen Dasein", "tokens": ["dem", "gol\u00b7di\u00b7gen", "Da\u00b7sein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "dankbare Treue.", "tokens": ["dank\u00b7ba\u00b7re", "Treu\u00b7e", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Hat ihn die eisige Hand", "tokens": ["Hat", "ihn", "die", "ei\u00b7si\u00b7ge", 