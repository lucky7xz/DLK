{"textgrid.poem.61002": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Drei Bitten", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Bitten hab' ich f\u00fcr des Himmels Ohr,", "tokens": ["Drei", "Bit\u00b7ten", "hab'", "ich", "f\u00fcr", "des", "Him\u00b7mels", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die send' ich t\u00e4glich fr\u00fch und sp\u00e4t empor:", "tokens": ["Die", "send'", "ich", "t\u00e4g\u00b7lich", "fr\u00fch", "und", "sp\u00e4t", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum ersten, da\u00df der Liebe reiner Born", "tokens": ["Zum", "ers\u00b7ten", ",", "da\u00df", "der", "Lie\u00b7be", "rei\u00b7ner", "Born"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "$,", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mir nie versieg' in Ungeduld und Zorn;", "tokens": ["Mir", "nie", "ver\u00b7sieg'", "in", "Un\u00b7ge\u00b7duld", "und", "Zorn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zum zweiten, da\u00df mir, was ich auch vernahm,", "tokens": ["Zum", "zwei\u00b7ten", ",", "da\u00df", "mir", ",", "was", "ich", "auch", "ver\u00b7nahm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "KOUS", "PPER", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Echo weck', ein Lied in Lust und Gram;", "tokens": ["Ein", "E\u00b7cho", "weck'", ",", "ein", "Lied", "in", "Lust", "und", "Gram", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zum dritten, wenn das letzte Lied verhallt,", "tokens": ["Zum", "drit\u00b7ten", ",", "wenn", "das", "letz\u00b7te", "Lied", "ver\u00b7hallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und wenn der Quell der Liebe leiser wallt,", "tokens": ["Und", "wenn", "der", "Quell", "der", "Lie\u00b7be", "lei\u00b7ser", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df dann der Tod mich schnell mit sanfter Hand", "tokens": ["Da\u00df", "dann", "der", "Tod", "mich", "schnell", "mit", "sanf\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Hin\u00fcberf\u00fchr' in jenes bessere Land,", "tokens": ["Hin\u00b7\u00fcber\u00b7f\u00fchr'", "in", "je\u00b7nes", "bes\u00b7se\u00b7re", "Land", ","], "token_info": ["word", "word", "word", "word", "word