{"dta.poem.19547": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Hesperos.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber schwarzem Tannenhange", "tokens": ["Ue\u00b7ber", "schwar\u00b7zem", "Tan\u00b7nen\u00b7han\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schimmerst mir zum Abendgange,", "tokens": ["Schim\u00b7merst", "mir", "zum", "A\u00b7bend\u00b7gan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Liebe f\u00fchl' ich neigen", "tokens": ["Ei\u00b7ne", "Lie\u00b7be", "f\u00fchl'", "ich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich in deinem Niedersteigen,", "tokens": ["Sich", "in", "dei\u00b7nem", "Nie\u00b7ders\u00b7tei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unbemerkt bist du gekommen,", "tokens": ["Un\u00b7be\u00b7merkt", "bist", "du", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der blassen Luft entglommen \u2014", "tokens": ["Aus", "der", "blas\u00b7sen", "Luft", "ent\u00b7glom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So mit ungeh\u00f6rten Tritten", "tokens": ["So", "mit", "un\u00b7ge\u00b7h\u00f6r\u00b7ten", "Trit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch die D\u00e4mm'rung hergeglitten", "tokens": ["Durch", "die", "D\u00e4m\u00b7m'\u00b7rung", "her\u00b7ge\u00b7glit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Kam die Mutter, die mir legte", "tokens": ["Kam", "die", "Mut\u00b7ter", ",", "die", "mir", "leg\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Auf die Schulter die bewegte", "tokens": ["Auf", "die", "Schul\u00b7ter", "die", "be\u00b7weg\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Hand, da\u00df ich ihr nicht verhehle,", "tokens": ["Hand", ",", "da\u00df", "ich", "ihr", "nicht", "ver\u00b7heh\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Was ich leide, was mich qu\u00e4le,", "tokens": ["Was", "ich", "lei\u00b7de", ",", "was", "mich", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und warum ich ohne Klage", "tokens": ["Und", "wa\u00b7rum", "ich", "oh\u00b7ne", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Mich verzehre, mich zernage.", "tokens": ["Mich", "ver\u00b7zeh\u00b7re", ",", "mich", "zer\u00b7na\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und ich schwieg und unter Z\u00e4hren", "tokens": ["Und", "ich", "schwieg", "und", "un\u00b7ter", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Lie\u00df sie meinen Trotz gew\u00e4hren.", "tokens": ["Lie\u00df", "sie", "mei\u00b7nen", "Trotz", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Hat sie Wohnung jetzt, die Milde,", "tokens": ["Hat", "sie", "Woh\u00b7nung", "jetzt", ",", "die", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Dort in deinem Lichtgefilde?", "tokens": ["Dort", "in", "dei\u00b7nem", "Licht\u00b7ge\u00b7fil\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Deiner Strahlen saug' ich jeden,", "tokens": ["Dei\u00b7ner", "Strah\u00b7len", "saug'", "ich", "je\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Durch das Dunkel h\u00f6r' ich reden,", "tokens": ["Durch", "das", "Dun\u00b7kel", "h\u00f6r'", "ich", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "(und mir ist als ob die k\u00fchle", "tokens": ["(", "und", "mir", "ist", "als", "ob", "die", "k\u00fch\u00b7le"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "PPER", "VAFIN", "KOKOM", "KOUS", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Hand ich auf der Schulter f\u00fchle)", "tokens": ["Hand", "ich", "auf", "der", "Schul\u00b7ter", "f\u00fch\u00b7le", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Reden nicht von Seligkeiten,", "tokens": ["Re\u00b7den", "nicht", "von", "Se\u00b7lig\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Nur Erinn'rung alter Zeiten \u2014", "tokens": ["Nur", "Er\u00b7inn'\u00b7rung", "al\u00b7ter", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Jetzt versteht sie ohne Kunde", "tokens": ["Jetzt", "ver\u00b7steht", "sie", "oh\u00b7ne", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Wer ich bin im Herzensgrunde,", "tokens": ["Wer", "ich", "bin", "im", "Her\u00b7zens\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Dies und jenes mu\u00df sie schelten,", "tokens": ["Dies", "und", "je\u00b7nes", "mu\u00df", "sie", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Andres l\u00e4\u00dft sie heiter gelten,", "tokens": ["And\u00b7res", "l\u00e4\u00dft", "sie", "hei\u00b7ter", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Und sie meint, ", "tokens": ["Und", "sie", "meint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.30": {"text": "Gebe sie sich auch zufrieden ...", "tokens": ["Ge\u00b7be", "sie", "sich", "auch", "zu\u00b7frie\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Abendstern, du eilst geschwinde!", "tokens": ["A\u00b7bends\u00b7tern", ",", "du", "eilst", "ge\u00b7schwin\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "La\u00df sie plaudern mit dem Kinde!", "tokens": ["La\u00df", "sie", "plau\u00b7dern", "mit", "dem", "Kin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Freundlich zitternd gehst du nieder ...", "tokens": ["Freund\u00b7lich", "zit\u00b7ternd", "gehst", "du", "nie\u00b7der", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Mutter, Mutter, komme ", "tokens": ["Mut\u00b7ter", ",", "Mut\u00b7ter", ",", "kom\u00b7me"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}