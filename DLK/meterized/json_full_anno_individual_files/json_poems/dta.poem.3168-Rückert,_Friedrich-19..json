{"dta.poem.3168": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie schwer entschl\u00e4gst du dich, ein gleiches andern an", "tokens": ["Wie", "schwer", "ent\u00b7schl\u00e4gst", "du", "dich", ",", "ein", "glei\u00b7ches", "an\u00b7dern", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "$,", "ART", "ADJA", "ADJA", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu thun, wie andere dir selber angethan.", "tokens": ["Zu", "thun", ",", "wie", "an\u00b7de\u00b7re", "dir", "sel\u00b7ber", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "PIS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wen man von oben dr\u00fcckt, der dr\u00fcckt nach unten weiter,", "tokens": ["Wen", "man", "von", "o\u00b7ben", "dr\u00fcckt", ",", "der", "dr\u00fcckt", "nach", "un\u00b7ten", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "ADV", "VVFIN", "$,", "PRELS", "VVFIN", "APPR", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Unterdr\u00fcckung wird dadurch auf Erden breiter.", "tokens": ["Und", "Un\u00b7ter\u00b7dr\u00fc\u00b7ckung", "wird", "da\u00b7durch", "auf", "Er\u00b7den", "brei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PAV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer in der Jugend sich durch M\u00fchsal mu\u00dfte schlagen,", "tokens": ["Wer", "in", "der", "Ju\u00b7gend", "sich", "durch", "M\u00fch\u00b7sal", "mu\u00df\u00b7te", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PRF", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den r\u00fchrts im Alter nicht, wenn sich die Jungen plagen.", "tokens": ["Den", "r\u00fchrts", "im", "Al\u00b7ter", "nicht", ",", "wenn", "sich", "die", "Jun\u00b7gen", "pla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "PTKNEG", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wen Gleichg\u00fcltigkeit gekr\u00e4nkt und Unbeachtung,", "tokens": ["Und", "wen", "Gleich\u00b7g\u00fcl\u00b7tig\u00b7keit", "ge\u00b7kr\u00e4nkt", "und", "Un\u00b7be\u00b7ach\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zieht fremdes Schicksal nicht in herzliche Betrachtung.", "tokens": ["Zieht", "frem\u00b7des", "Schick\u00b7sal", "nicht", "in", "herz\u00b7li\u00b7che", "Be\u00b7trach\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das alles ist gewi\u00df nat\u00fcrlich, doch das Heil", "tokens": ["Das", "al\u00b7les", "ist", "ge\u00b7wi\u00df", "na\u00b7t\u00fcr\u00b7lich", ",", "doch", "das", "Heil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PIS", "VAFIN", "ADV", "ADV", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Menschheit forderte das grade Gegentheil.", "tokens": ["Der", "Menschheit", "for\u00b7der\u00b7te", "das", "gra\u00b7de", "Ge\u00b7gen\u00b7theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}}}}