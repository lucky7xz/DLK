{"dta.poem.9215": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n Die Junggesellen-Noth.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Der ehstand plagt mich offt/", "tokens": ["Der", "eh\u00b7stand", "plagt", "mich", "offt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich mich unverhofft", "tokens": ["Da\u00df", "ich", "mich", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ins wesen nein verliebe/", "tokens": ["Ins", "we\u00b7sen", "nein", "ver\u00b7lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKANT", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn hab ich lange zeit/", "tokens": ["Denn", "hab", "ich", "lan\u00b7ge", "zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So denck ich allbereit/", "tokens": ["So", "denck", "ich", "all\u00b7be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach h\u00e4tt ich eine frau/ die mir die zeit vertriebe.", "tokens": ["Ach", "h\u00e4tt", "ich", "ei\u00b7ne", "frau", "/", "die", "mir", "die", "zeit", "ver\u00b7trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "2. Fr\u00fch morgens steh ich auff/", "tokens": ["Fr\u00fch", "mor\u00b7gens", "steh", "ich", "auff", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "APPR", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Und wann ich meinen lauff", "tokens": ["Und", "wann", "ich", "mei\u00b7nen", "lauff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald hie bald da betrachte/", "tokens": ["Bald", "hie", "bald", "da", "be\u00b7trach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So rumpelt mir der bauch/", "tokens": ["So", "rum\u00b7pelt", "mir", "der", "bauch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Derhalben denck ich auch/", "tokens": ["Der\u00b7hal\u00b7ben", "denck", "ich", "auch", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach h\u00e4t ich eine frau/ die mir ein s\u00fcpgen machte.", "tokens": ["Ach", "h\u00e4t", "ich", "ei\u00b7ne", "frau", "/", "die", "mir", "ein", "s\u00fcp\u00b7gen", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "3. Und wann ich meinen bart.", "tokens": ["Und", "wann", "ich", "mei\u00b7nen", "bart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Recht nach der neuen art/", "tokens": ["Recht", "nach", "der", "neu\u00b7en", "art", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Gern in die falten schraubte/", "tokens": ["Gern", "in", "die", "fal\u00b7ten", "schraub\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So kommen federn drein/", "tokens": ["So", "kom\u00b7men", "fe\u00b7dern", "drein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVINF", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da mu\u00df ich traurig seyn/", "tokens": ["Da", "mu\u00df", "ich", "trau\u00b7rig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$("], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Ach h\u00e4tt ich eine frau/ die mir im barte klaubte.", "tokens": ["Ach", "h\u00e4tt", "ich", "ei\u00b7ne", "frau", "/", "die", "mir", "im", "bar\u00b7te", "klaub\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "4. Bi\u00dfweilen bin ich kranck/", "tokens": ["Bi\u00df\u00b7wei\u00b7len", "bin", "ich", "kranck", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da lieg ich auf der banck/", "tokens": ["Da", "lieg", "ich", "auf", "der", "banck", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bete meine spr\u00fcche/", "tokens": ["Und", "be\u00b7te", "mei\u00b7ne", "spr\u00fc\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch in dergleichen qual/", "tokens": ["Doch", "in", "derg\u00b7lei\u00b7chen", "qual", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da denck ich hundertmahl/", "tokens": ["Da", "denck", "ich", "hun\u00b7dert\u00b7mahl", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach h\u00e4tt ich eine frau/ die mir im r\u00fccken strieche.", "tokens": ["Ach", "h\u00e4tt", "ich", "ei\u00b7ne", "frau", "/", "die", "mir", "im", "r\u00fc\u00b7cken", "strie\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "APPRART", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "5. Jm bette kommt ein floch/", "tokens": ["Jm", "bet\u00b7te", "kommt", "ein", "floch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ART", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der h\u00fcpfft mir gar zu hoch/", "tokens": ["Der", "h\u00fcpfft", "mir", "gar", "zu", "hoch", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und macht so krumme spr\u00fcnge/", "tokens": ["Und", "macht", "so", "krum\u00b7me", "spr\u00fcn\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich mit uberdru\u00df", "tokens": ["Da\u00df", "ich", "mit", "u\u00b7berd\u00b7ru\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von hertzen w\u00fcnschen mu\u00df/", "tokens": ["Von", "hert\u00b7zen", "w\u00fcn\u00b7schen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach h\u00e4tt ich eine frau/ die mir die thiergen fienge.", "tokens": ["Ach", "h\u00e4tt", "ich", "ei\u00b7ne", "frau", "/", "die", "mir", "die", "thier\u00b7gen", "fi\u00b7en\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}}, "stanza.6": {"line.1": {"text": "6. Jm winter w\u00e4r es zwar", "tokens": ["Jm", "win\u00b7ter", "w\u00e4r", "es", "zwar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NE", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Kein wunder/ wann ich gar", "tokens": ["Kein", "wun\u00b7der", "/", "wann", "ich", "gar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "PWAV", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich da zu todte h\u00e4rmte/", "tokens": ["Mich", "da", "zu", "tod\u00b7te", "h\u00e4rm\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch seh ich di\u00df noch an/", "tokens": ["Doch", "seh", "ich", "di\u00df", "noch", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDS", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dieweil ich w\u00fcnschen kan/", "tokens": ["Die\u00b7weil", "ich", "w\u00fcn\u00b7schen", "kan", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach h\u00e4tt ich eine frau/ die mir das bette w\u00e4rmte.", "tokens": ["Ach", "h\u00e4tt", "ich", "ei\u00b7ne", "frau", "/", "die", "mir", "das", "bet\u00b7te", "w\u00e4rm\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "PDS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "7. Wann ich in meinem sinn", "tokens": ["Wann", "ich", "in", "mei\u00b7nem", "sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Rechtschaffen b\u00f6se bin/", "tokens": ["Recht\u00b7schaf\u00b7fen", "b\u00f6\u00b7se", "bin", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meine lust nicht b\u00fcsse/", "tokens": ["Und", "mei\u00b7ne", "lust", "nicht", "b\u00fcs\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So denck ich vielerley", "tokens": ["So", "denck", "ich", "vie\u00b7ler\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PIAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch dieses auch dabey/", "tokens": ["Doch", "die\u00b7ses", "auch", "da\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach h\u00e4tt ich eine frau/ die sich erschlagen liesse.", "tokens": ["Ach", "h\u00e4tt", "ich", "ei\u00b7ne", "frau", "/", "die", "sich", "er\u00b7schla\u00b7gen", "lies\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ART", "NN", "$(", "PRELS", "PRF", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "8. In summa/ was ich thu/", "tokens": ["In", "sum\u00b7ma", "/", "was", "ich", "thu", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da kan ich nicht darzu.", "tokens": ["Da", "kan", "ich", "nicht", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als wie der hund im schilffe/", "tokens": ["Als", "wie", "der", "hund", "im", "schilf\u00b7fe", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPRART", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es ist mir alles leid/", "tokens": ["Es", "ist", "mir", "al\u00b7les", "leid", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum w\u00fcnsch ich allezeit/", "tokens": ["Drum", "w\u00fcnsch", "ich", "al\u00b7le\u00b7zeit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach h\u00e4tt ich eine frau/ die mir aus n\u00f6then h\u00fclffe.", "tokens": ["Ach", "h\u00e4tt", "ich", "ei\u00b7ne", "frau", "/", "die", "mir", "aus", "n\u00f6\u00b7then", "h\u00fclf\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "ART", "NN", "$(", "PRELS", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}