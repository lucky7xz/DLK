{"textgrid.poem.54037": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die freie Wirtschaft", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr sollt die verfluchten Tarife abbauen.", "tokens": ["Ihr", "sollt", "die", "ver\u00b7fluch\u00b7ten", "Ta\u00b7ri\u00b7fe", "ab\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+---+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr sollt auf euern Direktor vertrauen.", "tokens": ["Ihr", "sollt", "auf", "eu\u00b7ern", "Di\u00b7rek\u00b7tor", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr sollt die Schlichtungsaussch\u00fcsse verlassen.", "tokens": ["Ihr", "sollt", "die", "Schlich\u00b7tungs\u00b7aus\u00b7sch\u00fcs\u00b7se", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr sollt alles Weitere dem Chef \u00fcberlassen.", "tokens": ["Ihr", "sollt", "al\u00b7les", "Wei\u00b7te\u00b7re", "dem", "Chef", "\u00fc\u00b7ber\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Kein Betriebsrat quatsche uns mehr herein,", "tokens": ["Kein", "Be\u00b7triebs\u00b7rat", "quat\u00b7sche", "uns", "mehr", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "wir wollen freie Wirtschaftler sein!", "tokens": ["wir", "wol\u00b7len", "frei\u00b7e", "Wirt\u00b7schaft\u00b7ler", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Fort die Gruppen \u2013 sei unser Panier!", "tokens": ["Fort", "die", "Grup\u00b7pen", "\u2013", "sei", "un\u00b7ser", "Pa\u00b7nier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Na, ihr nicht.", "tokens": ["Na", ",", "ihr", "nicht", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Aber wir.", "tokens": ["A\u00b7ber", "wir", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ihr braucht keine Heime f\u00fcr eure Lungen,", "tokens": ["Ihr", "braucht", "kei\u00b7ne", "Hei\u00b7me", "f\u00fcr", "eu\u00b7re", "Lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "keine Renten und keine Versicherungen.", "tokens": ["kei\u00b7ne", "Ren\u00b7ten", "und", "kei\u00b7ne", "Ver\u00b7si\u00b7che\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ihr solltet euch allesamt was sch\u00e4men,", "tokens": ["Ihr", "soll\u00b7tet", "euch", "al\u00b7le\u00b7samt", "was", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "von dem armen Staat noch Geld zu nehmen!", "tokens": ["von", "dem", "ar\u00b7men", "Staat", "noch", "Geld", "zu", "neh\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ihr sollt nicht mehr zusammenstehn \u2013", "tokens": ["Ihr", "sollt", "nicht", "mehr", "zu\u00b7sam\u00b7men\u00b7stehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wollt ihr wohl auseinandergehn!", "tokens": ["wollt", "ihr", "wohl", "aus\u00b7ein\u00b7an\u00b7der\u00b7gehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Keine Kartelle in unserm Revier!", "tokens": ["Kei\u00b7ne", "Kar\u00b7tel\u00b7le", "in", "un\u00b7serm", "Re\u00b7vier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+---+---", "measure": "unknown.measure.tri"}, "line.8": {"text": "Ihr nicht.", "tokens": ["Ihr", "nicht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Aber wir.", "tokens": ["A\u00b7ber", "wir", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wir bilden bis in die weiteste Ferne", "tokens": ["Wir", "bil\u00b7den", "bis", "in", "die", "wei\u00b7tes\u00b7te", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Trusts, Kartelle, Verb\u00e4nde, Konzerne.", "tokens": ["Trusts", ",", "Kar\u00b7tel\u00b7le", ",", "Ver\u00b7b\u00e4n\u00b7de", ",", "Kon\u00b7zer\u00b7ne", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+---+----", "measure": "dactylic.init"}, "line.3": {"text": "Wir stehen neben den Hochofenflammen", "tokens": ["Wir", "ste\u00b7hen", "ne\u00b7ben", "den", "Hoch\u00b7o\u00b7fen\u00b7flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "in Interessengemeinschaften fest zusammen.", "tokens": ["in", "In\u00b7ter\u00b7es\u00b7sen\u00b7ge\u00b7mein\u00b7schaf\u00b7ten", "fest", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir diktieren die Preise und die Vertr\u00e4ge \u2013", "tokens": ["Wir", "dik\u00b7tie\u00b7ren", "die", "Prei\u00b7se", "und", "die", "Ver\u00b7tr\u00e4\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "kein Schutzgesetz sei uns im Wege.", "tokens": ["kein", "Schutz\u00b7ge\u00b7setz", "sei", "uns", "im", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Gut organisiert sitzen wir hier . . .", "tokens": ["Gut", "or\u00b7ga\u00b7ni\u00b7siert", "sit\u00b7zen", "wir", "hier", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "PPER", "ADV", "$.", "$.", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ihr nicht.", "tokens": ["Ihr", "nicht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Aber wir.", "tokens": ["A\u00b7ber", "wir", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Was ihr macht, ist Marxismus.", "tokens": ["Was", "ihr", "macht", ",", "ist", "Mar\u00b7xis\u00b7mus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nieder damit!", "tokens": ["Nie\u00b7der", "da\u00b7mit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PAV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wir erobern die Macht, Schritt f\u00fcr Schritt.", "tokens": ["Wir", "er\u00b7o\u00b7bern", "die", "Macht", ",", "Schritt", "f\u00fcr", "Schritt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Niemand st\u00f6rt uns. In guter Ruh", "tokens": ["Nie\u00b7mand", "st\u00f6rt", "uns", ".", "In", "gu\u00b7ter", "Ruh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "$.", "APPR", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "sehn Regierungssozialisten zu.", "tokens": ["sehn", "Re\u00b7gie\u00b7rungs\u00b7so\u00b7zi\u00b7a\u00b7lis\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Wir wollen euch einzeln. An die Gewehre!", "tokens": ["Wir", "wol\u00b7len", "euch", "ein\u00b7zeln", ".", "An", "die", "Ge\u00b7weh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$.", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Das ist die neuste Wirtschaftslehre.", "tokens": ["Das", "ist", "die", "neus\u00b7te", "Wirt\u00b7schafts\u00b7leh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Forderung ist noch nicht verk\u00fcndet,", "tokens": ["Die", "For\u00b7de\u00b7rung", "ist", "noch", "nicht", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "die ein deutscher Professor uns nicht begr\u00fcndet.", "tokens": ["die", "ein", "deut\u00b7scher", "Pro\u00b7fes\u00b7sor", "uns", "nicht", "be\u00b7gr\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "In Betrieben wirken f\u00fcr unsere Idee", "tokens": ["In", "Be\u00b7trie\u00b7ben", "wir\u00b7ken", "f\u00fcr", "un\u00b7se\u00b7re", "I\u00b7dee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "die Offiziere der alten Armee,", "tokens": ["die", "Of\u00b7fi\u00b7zie\u00b7re", "der", "al\u00b7ten", "Ar\u00b7mee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "die Stahlhelmleute, Hitlergarden . . .", "tokens": ["die", "Stahl\u00b7helm\u00b7leu\u00b7te", ",", "Hit\u00b7ler\u00b7gar\u00b7den", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$,", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr, in Kellern und in Mansarden,", "tokens": ["Ihr", ",", "in", "Kel\u00b7lern", "und", "in", "Man\u00b7sar\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "merkt ihr nicht, was mit euch gespielt wird?", "tokens": ["merkt", "ihr", "nicht", ",", "was", "mit", "euch", "ge\u00b7spielt", "wird", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "mit wessen Schwei\u00df der Gewinn erzielt wird?", "tokens": ["mit", "wes\u00b7sen", "Schwei\u00df", "der", "Ge\u00b7winn", "er\u00b7zielt", "wird", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Komme, was da kommen mag.", "tokens": ["Kom\u00b7me", ",", "was", "da", "kom\u00b7men", "mag."], "token_info": ["word", "punct", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "$,", "PRELS", "ADV", "VVFIN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Es kommt der Tag,", "tokens": ["Es", "kommt", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "da ruft der Arbeitspionier:", "tokens": ["da", "ruft", "der", "Ar\u00b7beits\u00b7pi\u00b7o\u00b7nier", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbihr nicht.", "tokens": ["\u00bb", "ihr", "nicht", "."], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "PPER", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Aber Wir. Wir. Wir.\u00ab", "tokens": ["A\u00b7ber", "Wir", ".", "Wir", ".", "Wir", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "PPER", "$.", "PPER", "$.", "PPER", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ihr sollt die verfluchten Tarife abbauen.", "tokens": ["Ihr", "sollt", "die", "ver\u00b7fluch\u00b7ten", "Ta\u00b7ri\u00b7fe", "ab\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+---+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr sollt auf euern Direktor vertrauen.", "tokens": ["Ihr", "sollt", "auf", "eu\u00b7ern", "Di\u00b7rek\u00b7tor", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr sollt die Schlichtungsaussch\u00fcsse verlassen.", "tokens": ["Ihr", "sollt", "die", "Schlich\u00b7tungs\u00b7aus\u00b7sch\u00fcs\u00b7se", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr sollt alles Weitere dem Chef \u00fcberlassen.", "tokens": ["Ihr", "sollt", "al\u00b7les", "Wei\u00b7te\u00b7re", "dem", "Chef", "\u00fc\u00b7ber\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Kein Betriebsrat quatsche uns mehr herein,", "tokens": ["Kein", "Be\u00b7triebs\u00b7rat", "quat\u00b7sche", "uns", "mehr", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "wir wollen freie Wirtschaftler sein!", "tokens": ["wir", "wol\u00b7len", "frei\u00b7e", "Wirt\u00b7schaft\u00b7ler", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Fort die Gruppen \u2013 sei unser Panier!", "tokens": ["Fort", "die", "Grup\u00b7pen", "\u2013", "sei", "un\u00b7ser", "Pa\u00b7nier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "VAFIN", "PPOSAT", "NN", "$."],