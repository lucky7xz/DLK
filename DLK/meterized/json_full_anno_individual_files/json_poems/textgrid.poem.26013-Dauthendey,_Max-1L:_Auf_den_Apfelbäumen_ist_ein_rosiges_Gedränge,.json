{"textgrid.poem.26013": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf den Apfelb\u00e4umen ist ein rosiges Gedr\u00e4nge,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den Apfelb\u00e4umen ist ein rosiges Gedr\u00e4nge,", "tokens": ["Auf", "den", "Ap\u00b7fel\u00b7b\u00e4u\u00b7men", "ist", "ein", "ro\u00b7si\u00b7ges", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Die Bl\u00fcten sind weich wie dein Nacken", "tokens": ["Die", "Bl\u00fc\u00b7ten", "sind", "weich", "wie", "dein", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und rund wie deine Wangen;", "tokens": ["Und", "rund", "wie", "dei\u00b7ne", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Apfelb\u00e4ume haben es von dir gelernt,", "tokens": ["Die", "Ap\u00b7fel\u00b7b\u00e4u\u00b7me", "ha\u00b7ben", "es", "von", "dir", "ge\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sich s\u00fc\u00df zu schm\u00fccken, sie verlernen es nie mehr.", "tokens": ["Sich", "s\u00fc\u00df", "zu", "schm\u00fc\u00b7cken", ",", "sie", "ver\u00b7ler\u00b7nen", "es", "nie", "mehr", "."], "token_inf