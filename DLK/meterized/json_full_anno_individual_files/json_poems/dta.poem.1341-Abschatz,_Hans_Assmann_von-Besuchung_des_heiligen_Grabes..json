{"dta.poem.1341": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Besuchung des heiligen Grabes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Kommt Sterbliche/ die ihr die Gr\u00e4ber scheuet/", "tokens": ["Kommt", "Sterb\u00b7li\u00b7che", "/", "die", "ihr", "die", "Gr\u00e4\u00b7ber", "scheu\u00b7et", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Besucht mit mir/ was mich im Geist erfreuet/", "tokens": ["Be\u00b7sucht", "mit", "mir", "/", "was", "mich", "im", "Geist", "er\u00b7freu\u00b7et", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$(", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und k\u00f6nt ihr nicht nach Palestina gehn/", "tokens": ["Und", "k\u00f6nt", "ihr", "nicht", "nach", "Pa\u00b7les\u00b7ti\u00b7na", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "APPR", "NE", "VVINF", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So bleibet hier in Andacht stille stehn.", "tokens": ["So", "blei\u00b7bet", "hier", "in", "An\u00b7dacht", "stil\u00b7le", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es prangt die Welt mit ausgeschm\u00fcckten Zimmern/", "tokens": ["Es", "prangt", "die", "Welt", "mit", "aus\u00b7ge\u00b7schm\u00fcck\u00b7ten", "Zim\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier blinckt Crystall und dort mu\u00df Silber schimmern/", "tokens": ["Hier", "blinckt", "Crys\u00b7tall", "und", "dort", "mu\u00df", "Sil\u00b7ber", "schim\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "ADV", "VMFIN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der HErr der Welt/ den sie gestossen au\u00df/", "tokens": ["Der", "Herr", "der", "Welt", "/", "den", "sie", "ges\u00b7tos\u00b7sen", "au\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "PPER", "VVPP", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entlehnet ihm ein schlechtes Todten-Hau\u00df.", "tokens": ["Ent\u00b7leh\u00b7net", "ihm", "ein", "schlech\u00b7tes", "Tod\u00b7ten\u00b7Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Lebens-F\u00fcrst/ durch den wir alle leben/", "tokens": ["Der", "Le\u00b7bens\u00b7F\u00fcrst", "/", "durch", "den", "wir", "al\u00b7le", "le\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "PRELS", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4st sich verschmacht ins Grab zu ruhen heben:", "tokens": ["L\u00e4st", "sich", "ver\u00b7schmacht", "ins", "Grab", "zu", "ru\u00b7hen", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVPP", "APPRART", "NN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne/ die der Sonne gab den Schein/", "tokens": ["Die", "Son\u00b7ne", "/", "die", "der", "Son\u00b7ne", "gab", "den", "Schein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Senckt sich erbla\u00dft in finstern Winckel ein.", "tokens": ["Senckt", "sich", "er\u00b7bla\u00dft", "in", "fins\u00b7tern", "Win\u00b7ckel", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}}, "stanza.4": {"line.1": {"text": "Weil Adam must aus Schuld vom Garten scheiden/", "tokens": ["Weil", "A\u00b7dam", "must", "aus", "Schuld", "vom", "Gar\u00b7ten", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "APPR", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fieng Christus auch im Garten an zu leyden.", "tokens": ["Fi\u00b7eng", "Chris\u00b7tus", "auch", "im", "Gar\u00b7ten", "an", "zu", "ley\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "APPRART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Im Garten wird er nun zur Ruhe bracht/", "tokens": ["Im", "Gar\u00b7ten", "wird", "er", "nun", "zur", "Ru\u00b7he", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil diese Schuld ist wieder gutt gemacht.", "tokens": ["Weil", "die\u00b7se", "Schuld", "ist", "wie\u00b7der", "gutt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Im Garten bl\u00fcht die edle Sarons-Blume/", "tokens": ["Im", "Gar\u00b7ten", "bl\u00fcht", "die", "ed\u00b7le", "Sa\u00b7rons\u00b7Blu\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Paradie\u00df wird uns zum Eigenthume.", "tokens": ["Das", "Pa\u00b7ra\u00b7die\u00df", "wird", "uns", "zum", "Ei\u00b7gen\u00b7thu\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Den Saamen und die Zwiebel stecken wir/", "tokens": ["Den", "Saa\u00b7men", "und", "die", "Zwie\u00b7bel", "ste\u00b7cken", "wir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch bricht heraus die sch\u00f6nste Bl\u00fcth herf\u00fcr.", "tokens": ["Doch", "bricht", "he\u00b7raus", "die", "sch\u00f6ns\u00b7te", "Bl\u00fcth", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Du stille Grufft/ in harten Fel\u00df gehauen/", "tokens": ["Du", "stil\u00b7le", "Grufft", "/", "in", "har\u00b7ten", "Fel\u00df", "ge\u00b7hau\u00b7en", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man wolte dir den Fel\u00df des Heyls vertrauen/", "tokens": ["Man", "wol\u00b7te", "dir", "den", "Fel\u00df", "des", "Heyls", "ver\u00b7trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gleichwie ihn nicht mag halten deine Klufft/", "tokens": ["Gleich\u00b7wie", "ihn", "nicht", "mag", "hal\u00b7ten", "dei\u00b7ne", "Klufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VMFIN", "VVINF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So \u00f6ffnet er auch k\u00fcnfftig unsre Grufft.", "tokens": ["So", "\u00f6ff\u00b7net", "er", "auch", "k\u00fcnff\u00b7tig", "uns\u00b7re", "Grufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Du dunckles Hau\u00df/ in festen Stein gebauet/", "tokens": ["Du", "dunck\u00b7les", "Hau\u00df", "/", "in", "fes\u00b7ten", "Stein", "ge\u00b7bau\u00b7et", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer ist/ dem noch f\u00fcr solcher Wohnung grauet?", "tokens": ["Wer", "ist", "/", "dem", "noch", "f\u00fcr", "sol\u00b7cher", "Woh\u00b7nung", "grau\u00b7et", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "ART", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es hat dich ja der Glantz der Herrligkeit/", "tokens": ["Es", "hat", "dich", "ja", "der", "Glantz", "der", "Herr\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und unser Grab zugleich mit eingeweyht.", "tokens": ["Und", "un\u00b7ser", "Grab", "zu\u00b7gleich", "mit", "ein\u00b7ge\u00b7weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Es darff ihn nicht die Schaar der W\u00e4chter h\u00fctten/", "tokens": ["Es", "darff", "ihn", "nicht", "die", "Schaar", "der", "W\u00e4ch\u00b7ter", "h\u00fct\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich will f\u00fcr mich derselben Stell erbitten/", "tokens": ["Ich", "will", "f\u00fcr", "mich", "der\u00b7sel\u00b7ben", "Stell", "er\u00b7bit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich will bey ihm mein Hertze schliessen ein/", "tokens": ["Ich", "will", "bey", "ihm", "mein", "Hert\u00b7ze", "schlies\u00b7sen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PPOSAT", "VVFIN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So werd ich auch mit ihm erwecket seyn.", "tokens": ["So", "werd", "ich", "auch", "mit", "ihm", "er\u00b7we\u00b7cket", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}