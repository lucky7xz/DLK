{"dta.poem.7031": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wein- und E\u00dfig-Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Ew\u2019ge Brunnquell aller Kr\u00e4ffte,", "tokens": ["O", "Ew'\u00b7ge", "Brunn\u00b7quell", "al\u00b7ler", "Kr\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die, in den unerforschten Gr\u00fcnden,", "tokens": ["Die", ",", "in", "den", "un\u00b7er\u00b7forschten", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jm Schoosse der Natur zu finden.", "tokens": ["Jm", "Schoos\u00b7se", "der", "Na\u00b7tur", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! la\u00df Dir doch dies denckende Gesch\u00e4ffte,", "tokens": ["Ach", "!", "la\u00df", "Dir", "doch", "dies", "den\u00b7cken\u00b7de", "Ge\u00b7sch\u00e4ff\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "ADV", "PDS", "ADJA", "NN", "$,"], "meter": "-+---+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Wenn wir von Deine G\u00f6ttlichkeit,", "tokens": ["Wenn", "wir", "von", "Dei\u00b7ne", "G\u00f6tt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In der Natur Vollkommenheit,", "tokens": ["In", "der", "Na\u00b7tur", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den hellen Wunder-Strahl und Schein", "tokens": ["Den", "hel\u00b7len", "Wun\u00b7der\u00b7Strahl", "und", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Betrachten, r\u00fchmen und erwegen,", "tokens": ["Be\u00b7trach\u00b7ten", ",", "r\u00fch\u00b7men", "und", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und fr\u00f6lich bey uns \u00fcberlegen;", "tokens": ["Und", "fr\u00f6\u00b7lich", "bey", "uns", "\u00fc\u00b7berl\u00b7e\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Aus Gnaden wolgef\u00e4llig seyn!", "tokens": ["Aus", "Gna\u00b7den", "wol\u00b7ge\u00b7f\u00e4l\u00b7lig", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}