freundlich an,", "tokens": ["Dich", "spricht", "kein", "We\u00b7sen", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du gehst die w\u00fcste Felsenbahn,", "tokens": ["Du", "gehst", "die", "w\u00fcs\u00b7te", "Fel\u00b7sen\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Klippen drohn, wo keine Blumen bl\u00fchen,", "tokens": ["Wo", "Klip\u00b7pen", "drohn", ",", "wo", "kei\u00b7ne", "Blu\u00b7men", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Sonne Strahlen hei\u00df und hei\u00dfer gl\u00fchen.", "tokens": ["Der", "Son\u00b7ne", "Strah\u00b7len", "hei\u00df", "und", "hei\u00b7\u00dfer", "gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "KON", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Liebe, die der Sch\u00f6pfung All durchklingt,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "der", "Sch\u00f6p\u00b7fung", "All", "durch\u00b7klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Schirm in Jammer und in Leiden,", "tokens": ["Der", "Schirm", "in", "Jam\u00b7mer", "und", "in", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", 