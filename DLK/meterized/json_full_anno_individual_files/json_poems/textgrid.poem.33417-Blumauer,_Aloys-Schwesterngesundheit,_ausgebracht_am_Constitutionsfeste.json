{"textgrid.poem.33417": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Schwesterngesundheit, ausgebracht am Constitutionsfeste", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund Amor, Schwestern, der, wie euch", "tokens": ["Freund", "A\u00b7mor", ",", "Schwes\u00b7tern", ",", "der", ",", "wie", "euch"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "NE", "$,", "NN", "$,", "PRELS", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht unbekannt, in alles gleich", "tokens": ["Nicht", "un\u00b7be\u00b7kannt", ",", "in", "al\u00b7les", "gleich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "APPR", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein kleines N\u00e4schen stecket,", "tokens": ["Sein", "klei\u00b7nes", "N\u00e4\u00b7schen", "ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dann die allerheimlichsten", "tokens": ["Und", "dann", "die", "al\u00b7ler\u00b7heim\u00b7lichs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Mysterien in neun Monaten,", "tokens": ["Mys\u00b7te\u00b7ri\u00b7en", "in", "neun", "Mo\u00b7na\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "CARD", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Wo nicht noch eh' entdecket,", "tokens": ["Wo", "nicht", "noch", "eh'", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schlich einst durch's allerkleinste Loch,", "tokens": ["Schlich", "einst", "durch's", "al\u00b7ler\u00b7kleins\u00b7te", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch welches je ein Amor kroch,", "tokens": ["Durch", "wel\u00b7ches", "je", "ein", "A\u00b7mor", "kroch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ganz leise auf den Zehen", "tokens": ["Ganz", "lei\u00b7se", "auf", "den", "Ze\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu uns herein, um als Spion", "tokens": ["Zu", "uns", "her\u00b7ein", ",", "um", "als", "Spi\u00b7on"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "KOUI", "KOUS", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die ganze Constitution", "tokens": ["Die", "gan\u00b7ze", "Con\u00b7sti\u00b7tu\u00b7ti\u00b7on"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Loge auszusp\u00e4hen.", "tokens": ["Der", "Lo\u00b7ge", "aus\u00b7zu\u00b7sp\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun, liebe Schwestern, h\u00f6ret an,", "tokens": ["Nun", ",", "lie\u00b7be", "Schwes\u00b7tern", ",", "h\u00f6\u00b7ret", "an", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was er f\u00fcr einen Ordensplan", "tokens": ["Was", "er", "f\u00fcr", "ei\u00b7nen", "Or\u00b7den\u00b7splan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr euch d'raus abstrahirte:", "tokens": ["F\u00fcr", "euch", "d'raus", "abstr\u00b7a\u00b7hir\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Das war ein and'rer Plan, als der,", "tokens": ["Das", "war", "ein", "an\u00b7d'\u00b7rer", "Plan", ",", "als", "der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Womit man so zum Schein bisher", "tokens": ["Wo\u00b7mit", "man", "so", "zum", "Schein", "bis\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADV", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch Schwestern nur vexirte.", "tokens": ["Euch", "Schwes\u00b7tern", "nur", "vex\u00b7ir\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er selbst will euch Jahr aus Jahr ein", "tokens": ["Er", "selbst", "will", "euch", "Jahr", "aus", "Jahr", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "NN", "APPR", "NN", "ART"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Der Meister von dem Stuhle sein,", "tokens": ["Der", "Meis\u00b7ter", "von", "dem", "Stuh\u00b7le", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und euch recht sanft regieren;", "tokens": ["Und", "euch", "recht", "sanft", "re\u00b7gie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Deputirten braucht er nicht,", "tokens": ["Den", "De\u00b7pu\u00b7tir\u00b7ten", "braucht", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So lang's ihm nicht an Kraft gebricht,", "tokens": ["So", "lang's", "ihm", "nicht", "an", "Kraft", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Hammer selbst zu f\u00fchren.", "tokens": ["Den", "Ham\u00b7mer", "selbst", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zum Bruder Ceremonier", "tokens": ["Zum", "Bru\u00b7der", "Ce\u00b7re\u00b7mo\u00b7nier"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Nimmt er den niedlichsten Abbee", "tokens": ["Nimmt", "er", "den", "nied\u00b7lichs\u00b7ten", "Ab\u00b7bee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "F\u00fcr euch zur Augenweide,", "tokens": ["F\u00fcr", "euch", "zur", "Au\u00b7gen\u00b7wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Damit, wenn Rangsucht euch entzweit,", "tokens": ["Da\u00b7mit", ",", "wenn", "Rang\u00b7sucht", "euch", "ent\u00b7zweit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er jede Sesselstreitigkeit", "tokens": ["Er", "je\u00b7de", "Ses\u00b7sel\u00b7strei\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch ein Bonmot entscheide.", "tokens": ["Durch", "ein", "Bon\u00b7mot", "ent\u00b7schei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "Dem Bruder Redner aber wird", "tokens": ["Dem", "Bru\u00b7der", "Red\u00b7ner", "a\u00b7ber", "wird"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Gnaden, wie es sich geb\u00fchrt,", "tokens": ["In", "Gna\u00b7den", ",", "wie", "es", "sich", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er seinen Abschied geben;", "tokens": ["Er", "sei\u00b7nen", "Ab\u00b7schied", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er wei\u00df ja, liebe Schwesterchen,", "tokens": ["Er", "wei\u00df", "ja", ",", "lie\u00b7be", "Schwes\u00b7ter\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Da\u00df eu're kleinen Z\u00fcngelchen", "tokens": ["Da\u00df", "eu'\u00b7re", "klei\u00b7nen", "Z\u00fcn\u00b7gel\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Des Amt's ihn \u00fcberheben.", "tokens": ["Des", "Amt's", "ihn", "\u00fc\u00b7ber\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Allein um desto weniger", "tokens": ["Al\u00b7lein", "um", "des\u00b7to", "we\u00b7ni\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "ADV"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Kann er den Bruder Secretair", "tokens": ["Kann", "er", "den", "Bru\u00b7der", "Se\u00b7cre\u00b7tair"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als unn\u00fctz dimittiren,", "tokens": ["Als", "un\u00b7n\u00fctz", "di\u00b7mit\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVINF", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Denn der hat alle H\u00e4nde voll,", "tokens": ["Denn", "der", "hat", "al\u00b7le", "H\u00e4n\u00b7de", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Er mu\u00df das grosse Protokoll", "tokens": ["Er", "mu\u00df", "das", "gros\u00b7se", "Pro\u00b7to\u00b7koll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von eu'ren K\u00fcssen f\u00fchren.", "tokens": ["Von", "eu'\u00b7ren", "K\u00fcs\u00b7sen", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und weil es gar nicht artig l\u00e4\u00dft,", "tokens": ["Und", "weil", "es", "gar", "nicht", "ar\u00b7tig", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man von Schwestern Geld erpre\u00dft,", "tokens": ["Wenn", "man", "von", "Schwes\u00b7tern", "Geld", "er\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zumal von sch\u00f6nen Kindern,", "tokens": ["Zu\u00b7mal", "von", "sch\u00f6\u00b7nen", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So weis't er den Schatzmeister an,", "tokens": ["So", "weis't", "er", "den", "Schatz\u00b7meis\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die S\u00e4cke eu'rer M\u00e4nner dann", "tokens": ["Die", "S\u00e4\u00b7cke", "eu'\u00b7rer", "M\u00e4n\u00b7ner", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Statt eu'rer auszupl\u00fcndern.", "tokens": ["Statt", "eu'\u00b7rer", "aus\u00b7zu\u00b7pl\u00fcn\u00b7dern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Auch kann er die helleuchtenden", "tokens": ["Auch", "kann", "er", "die", "hel\u00b7leuch\u00b7ten\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zwei Lichter, die in Westen steh'n,", "tokens": ["Zwei", "Lich\u00b7ter", ",", "die", "in", "Wes\u00b7ten", "steh'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht f\u00fcglich reduciren;", "tokens": ["Nicht", "f\u00fcg\u00b7lich", "re\u00b7du\u00b7ci\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die m\u00fcssen ja die Schwesternschaar", "tokens": ["Die", "m\u00fcs\u00b7sen", "ja", "die", "Schwes\u00b7tern\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Zeigen, Worten, und sogar", "tokens": ["In", "Zei\u00b7gen", ",", "Wor\u00b7ten", ",", "und", "so\u00b7gar"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Griffen exerciren.", "tokens": ["In", "Grif\u00b7fen", "ex\u00b7er\u00b7ci\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die Arbeit nimmt Herr Cypripor", "tokens": ["Die", "Ar\u00b7beit", "nimmt", "Herr", "Cyp\u00b7ri\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gemeiniglich des Nachts nur vor,", "tokens": ["Ge\u00b7mei\u00b7nig\u00b7lich", "des", "Nachts", "nur", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Profanen stets verborgen:", "tokens": ["Pro\u00b7fa\u00b7nen", "stets", "ver\u00b7bor\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er f\u00e4ngt nach Sonnenuntergang", "tokens": ["Er", "f\u00e4ngt", "nach", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie an, doch dau'rt sie nie zu lang,", "tokens": ["Sie", "an", ",", "doch", "dau'rt", "sie", "nie", "zu", "lang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und w\u00e