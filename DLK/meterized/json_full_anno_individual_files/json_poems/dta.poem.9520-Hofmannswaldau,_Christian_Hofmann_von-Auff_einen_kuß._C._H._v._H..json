{"dta.poem.9520": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff einen ku\u00df.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje z\u00fcrnst du Florida so ohne maa\u00df und ziel/", "tokens": ["Wie", "z\u00fcrnst", "du", "Flo\u00b7ri\u00b7da", "so", "oh\u00b7ne", "maa\u00df", "und", "ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "NE", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df meine zunge hat die gr\u00e4ntzen \u00fcbergangen?", "tokens": ["Da\u00df", "mei\u00b7ne", "zun\u00b7ge", "hat", "die", "gr\u00e4nt\u00b7zen", "\u00fc\u00b7ber\u00b7gan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die schuld ist nicht zu gro\u00df/ und that sie dir zu viel/", "tokens": ["Die", "schuld", "ist", "nicht", "zu", "gro\u00df", "/", "und", "that", "sie", "dir", "zu", "viel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PTKNEG", "PTKA", "ADJD", "$(", "KON", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie hast du sie denn nicht/ wie sies verdient/ gefangen?", "tokens": ["Wie", "hast", "du", "sie", "denn", "nicht", "/", "wie", "sies", "ver\u00b7dient", "/", "ge\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "$(", "KOKOM", "PDS", "VVFIN", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch da\u00df dir kundbar sey/ warum ich es gethan/", "tokens": ["Doch", "da\u00df", "dir", "kund\u00b7bar", "sey", "/", "wa\u00b7rum", "ich", "es", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$(", "PWAV", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ich die zunge dir lie\u00df schlund und gaumen lecken/", "tokens": ["Da\u00df", "ich", "die", "zun\u00b7ge", "dir", "lie\u00df", "schlund", "und", "gau\u00b7men", "le\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "ADJD", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich dachte/ weil sie mehr/ als billich/ plaudern kan/", "tokens": ["Ich", "dach\u00b7te", "/", "weil", "sie", "mehr", "/", "als", "bil\u00b7lich", "/", "plau\u00b7dern", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "$(", "KOUS", "ADJD", "$(", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie m\u00f6chte sonst aus neid mein liebes-spiel entdecken.", "tokens": ["Sie", "m\u00f6ch\u00b7te", "sonst", "aus", "neid", "mein", "lie\u00b7bes\u00b7spiel", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}