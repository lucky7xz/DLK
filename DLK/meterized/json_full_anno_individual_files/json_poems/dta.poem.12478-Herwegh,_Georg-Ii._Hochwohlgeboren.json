{"dta.poem.12478": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ii.   Hochwohlgeboren", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein guter B\u00fcrger willst du werden?", "tokens": ["Ein", "gu\u00b7ter", "B\u00fcr\u00b7ger", "willst", "du", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Pfui, Freund \u2014 Ein guter B\u00fcrger \u2014 du?", "tokens": ["Pfui", ",", "Freund", "Ein", "gu\u00b7ter", "B\u00fcr\u00b7ger", "du", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$(", "ART", "ADJA", "NN", "$(", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das also war dein Ziel auf Erden?", "tokens": ["Das", "al\u00b7so", "war", "dein", "Ziel", "auf", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem st\u00fcrmten deine Lieder zu?", "tokens": ["Dem", "st\u00fcrm\u00b7ten", "dei\u00b7ne", "Lie\u00b7der", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, nimm's zur\u00fcck, das ekle Wort!", "tokens": ["O", ",", "nim\u00b7m's", "zu\u00b7r\u00fcck", ",", "das", "ek\u00b7le", "Wort", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Wer mag sich so gemein geberden?", "tokens": ["Wer", "mag", "sich", "so", "ge\u00b7mein", "ge\u00b7ber\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nein, nein, mich rei\u00dft es weiter fort:", "tokens": ["Nein", ",", "nein", ",", "mich", "rei\u00dft", "es", "wei\u00b7ter", "fort", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich mu\u00df Geheimer-Hofrath werden!", "tokens": ["Ich", "mu\u00df", "Ge\u00b7hei\u00b7mer\u00b7Ho\u00b7frath", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Um meine Wiege sah die Amme", "tokens": ["Um", "mei\u00b7ne", "Wie\u00b7ge", "sah", "die", "Am\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schon fr\u00fche den Prophetenschein,", "tokens": ["Schon", "fr\u00fc\u00b7he", "den", "Pro\u00b7phe\u00b7ten\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in mir diese ew'ge Flamme,", "tokens": ["Und", "in", "mir", "die\u00b7se", "ew'\u00b7ge", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie kann, sie darf nicht L\u00fcge sein.", "tokens": ["Sie", "kann", ",", "sie", "darf", "nicht", "L\u00fc\u00b7ge", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PTKNEG", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bleib' du im Thal, wo dir's behagt", "tokens": ["Bleib'", "du", "im", "Thal", ",", "wo", "dir's", "be\u00b7hagt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "PWAV", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und grase mit den P\u00f6belheerden,", "tokens": ["Und", "gra\u00b7se", "mit", "den", "P\u00f6\u00b7bel\u00b7heer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In mir steht fest, was ich gesagt:", "tokens": ["In", "mir", "steht", "fest", ",", "was", "ich", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PTKVZ", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich mu\u00df Geheimer-Hofrath werden!", "tokens": ["Ich", "mu\u00df", "Ge\u00b7hei\u00b7mer\u00b7Ho\u00b7frath", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df unsre Wege so sich theilen,", "tokens": ["Da\u00df", "uns\u00b7re", "We\u00b7ge", "so", "sich", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Glaub' mir, Georg! es thut mir weh;", "tokens": ["Glaub'", "mir", ",", "Ge\u00b7org", "!", "es", "thut", "mir", "weh", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$.", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du gehst zum Bier: und ich derweilen", "tokens": ["Du", "gehst", "zum", "Bier", ":", "und", "ich", "der\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "KON", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einem Oberappellationsgerichtsvicepr\u00e4sidenten-Thee.", "tokens": ["Zu", "ei\u00b7nem", "O\u00b7berap\u00b7pel\u00b7la\u00b7ti\u00b7ons\u00b7ge\u00b7richts\u00b7vi\u00b7ce\u00b7pr\u00e4\u00b7si\u00b7den\u00b7ten\u00b7Thee", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Du hast erf\u00fcllt dein stilles Loos,", "tokens": ["Du", "hast", "er\u00b7f\u00fcllt", "dein", "stil\u00b7les", "Loos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das meine liegt noch den Beh\u00f6rden", "tokens": ["Das", "mei\u00b7ne", "liegt", "noch", "den", "Be\u00b7h\u00f6r\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "VVFIN", "ADV", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Der dunkeln Zukunft schwer im Schoo\u00df:", "tokens": ["Der", "dun\u00b7keln", "Zu\u00b7kunft", "schwer", "im", "Schoo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich mu\u00df Geheimer-Hofrath werden!", "tokens": ["Ich", "mu\u00df", "Ge\u00b7hei\u00b7mer\u00b7Ho\u00b7frath", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So Mancher hat's doch schon erreicht,", "tokens": ["So", "Man\u00b7cher", "hat's", "doch", "schon", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der h\u00f6her noch als ich gedachte,", "tokens": ["Der", "h\u00f6\u00b7her", "noch", "als", "ich", "ge\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der krummer seinen Vers vielleicht", "tokens": ["Der", "krum\u00b7mer", "sei\u00b7nen", "Vers", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und krummer seinen R\u00fccken machte.", "tokens": ["Und", "krum\u00b7mer", "sei\u00b7nen", "R\u00fc\u00b7cken", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und, trotz Gef\u00e4hrden und Beschwerden,", "tokens": ["Und", ",", "trotz", "Ge\u00b7f\u00e4hr\u00b7den", "und", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schw\u00f6r' ich's \u2014 St. Huber, h\u00f6re mich! \u2014", "tokens": ["Schw\u00f6r'", "ich's", "St.", "Hu\u00b7ber", ",", "h\u00f6\u00b7re", "mich", "!"], "token_info": ["word", "word", "punct", "abbreviation", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "$(", "NE", "NE", "$,", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ich mu\u00df Geheimer-Hofrath werden!", "tokens": ["Ich", "mu\u00df", "Ge\u00b7hei\u00b7mer\u00b7Ho\u00b7frath", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh: ein Logis im ersten Stocke,", "tokens": ["Sieh", ":", "ein", "Lo\u00b7gis", "im", "ers\u00b7ten", "Sto\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Recht weit und reich, mit Ma\u00df geheizt,", "tokens": ["Recht", "weit", "und", "reich", ",", "mit", "Ma\u00df", "ge\u00b7heizt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Kreuzchen auf dem schwarzen Rocke,", "tokens": ["Ein", "Kreuzc\u00b7hen", "auf", "dem", "schwar\u00b7zen", "Ro\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das sich kokett versteckt und spreizt,", "tokens": ["Das", "sich", "ko\u00b7kett", "ver\u00b7steckt", "und", "spreizt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADJD", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Chaischen, ein Livreechen drauf,", "tokens": ["Ein", "Chai\u00b7schen", ",", "ein", "Liv\u00b7re\u00b7e\u00b7chen", "drauf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und f\u00e4hrt's auch mit Fiacre-Pferden \u2014", "tokens": ["Und", "f\u00e4hrt's", "auch", "mit", "Fia\u00b7cre\u00b7P\u00b7fer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Bruder! die Seele geht mir auf: \u2014", "tokens": ["Bru\u00b7der", "!", "die", "See\u00b7le", "geht", "mir", "auf", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ich mu\u00df Geheimer-Hofrath werden!", "tokens": ["Ich", "mu\u00df", "Ge\u00b7hei\u00b7mer\u00b7Ho\u00b7frath", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch lebt ein Gott: Verdienst zu lohnen,", "tokens": ["Noch", "lebt", "ein", "Gott", ":", "Ver\u00b7dienst", "zu", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch steht manch ", "tokens": ["Noch", "steht", "manch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Gott theilt den F\u00fcrsten ihre Kronen,", "tokens": ["Gott", "theilt", "den", "F\u00fcrs\u00b7ten", "ih\u00b7re", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die F\u00fcrsten uns die Titel aus.", "tokens": ["Die", "F\u00fcrs\u00b7ten", "uns", "die", "Ti\u00b7tel", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gewi\u00df, gewi\u00df! ich find' es noch", "tokens": ["Ge\u00b7wi\u00df", ",", "ge\u00b7wi\u00df", "!", "ich", "find'", "es", "noch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein letztes Ziel auf dieser Erden;", "tokens": ["Mein", "letz\u00b7tes", "Ziel", "auf", "die\u00b7ser", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "W\u00e4r's nur um Voigtens Nekrolog: \u2014", "tokens": ["W\u00e4r's", "nur", "um", "Voig\u00b7tens", "Nek\u00b7ro\u00b7log", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NE", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ich mu\u00df ", "tokens": ["Ich", "mu\u00df"], "token_info": ["word", "word"], "pos": ["PPER", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}}}}}