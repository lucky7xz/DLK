{"textgrid.poem.45943": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[sieh den dunklen Schleier]", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh den dunklen Schleier der Nacht,", "tokens": ["Sieh", "den", "dunk\u00b7len", "Schlei\u00b7er", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie er sich hernieder senket,", "tokens": ["Wie", "er", "sich", "her\u00b7nie\u00b7der", "sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da des Wagens schimmernde Pracht", "tokens": ["Da", "des", "Wa\u00b7gens", "schim\u00b7mern\u00b7de", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Ph\u00f6bus nun hinab gelenket.", "tokens": ["Ph\u00f6\u00b7bus", "nun", "hin\u00b7ab", "ge\u00b7len\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sehnsucht f\u00fchret die Geliebten", "tokens": ["Sehn\u00b7sucht", "f\u00fch\u00b7ret", "die", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf des Mondes Zauberpfad,", "tokens": ["Auf", "des", "Mon\u00b7des", "Zau\u00b7ber\u00b7pfad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo sie gestern Scherze \u00fcbten,", "tokens": ["Wo", "sie", "ge\u00b7stern", "Scher\u00b7ze", "\u00fcb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Zu des Seees Glanzgestad.", "tokens": ["Zu", "des", "See\u00b7es", "Glanz\u00b7ge\u00b7stad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh, den dunklen Schleier der Nacht,", "tokens": ["Sieh", ",", "den", "dunk\u00b7len", "Schlei\u00b7er", "der", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Immer n\u00e4her aus der Ferne,", "tokens": ["Im\u00b7mer", "n\u00e4\u00b7her", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sticken nun mit lachender Pracht", "tokens": ["Sti\u00b7cken", "nun", "mit", "la\u00b7chen\u00b7der", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die geliebten Heldensterne!", "tokens": ["Die", "ge\u00b7lieb\u00b7ten", "Hel\u00b7dens\u00b7ter\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einer aber ist geschweifet,", "tokens": ["Ei\u00b7ner", "a\u00b7ber", "ist", "ge\u00b7schwei\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er ist heftig und ist mutig,", "tokens": ["Er", "ist", "hef\u00b7tig", "und", "ist", "mu\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "VAFIN", "ADJD", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Und den Mond er peitschend greifet,", "tokens": ["Und", "den", "Mond", "er", "peit\u00b7schend", "grei\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der blasse Mond wird blutig!", "tokens": ["Und", "der", "blas\u00b7se", "Mond", "wird", "blu\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh, des Monds zerschmettertes Schild", "tokens": ["Sieh", ",", "des", "Monds", "zer\u00b7schmet\u00b7ter\u00b7tes", "Schild"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In ein rotes Feld sich kehrte,", "tokens": ["In", "ein", "ro\u00b7tes", "Feld", "sich", "kehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Wappens gl\u00e4ubiges Schild", "tokens": ["Und", "des", "Wap\u00b7pens", "gl\u00e4u\u00b7bi\u00b7ges", "Schild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Schm\u00fcckt der Stern mit einem Schwerte.", "tokens": ["Schm\u00fcckt", "der", "Stern", "mit", "ei\u00b7nem", "Schwer\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Benavides, deinem Stamme", "tokens": ["Be\u00b7na\u00b7vi\u00b7des", ",", "dei\u00b7nem", "Stam\u00b7me"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist der Glanz nun angefacht!", "tokens": ["Ist", "der", "Glanz", "nun", "an\u00b7ge\u00b7facht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber sieh, des Schwertes Flamme", "tokens": ["A\u00b7ber", "sieh", ",", "des", "Schwer\u00b7tes", "Flam\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weichet und es kehrt die Nacht!", "tokens": ["Wei\u00b7chet", "und", "es", "kehrt", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh, ein feuriger Regen f\u00e4llt,", "tokens": ["Sieh", ",", "ein", "feu\u00b7ri\u00b7ger", "Re\u00b7gen", "f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und es schwebt, gleich einem Sarge", "tokens": ["Und", "es", "schwebt", ",", "gleich", "ei\u00b7nem", "Sar\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber der zornumfluteten Welt", "tokens": ["\u00dc\u00b7ber", "der", "zorn\u00b7um\u00b7flu\u00b7te\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Jetzt die gottgebaute Arche!", "tokens": ["Jetzt", "die", "gott\u00b7ge\u00b7bau\u00b7te", "Ar\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Und es fliegt der dunkle Rabe,", "tokens": ["Und", "es", "fliegt", "der", "dunk\u00b7le", "Ra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kehret hoffnungslos zur\u00fcck,", "tokens": ["Keh\u00b7ret", "hoff\u00b7nungs\u00b7los", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber mit der Friedensgabe", "tokens": ["A\u00b7ber", "mit", "der", "Frie\u00b7dens\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht die Taube Sonnenblick!", "tokens": ["Sieht", "die", "Tau\u00b7be", "Son\u00b7nen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und es spannt der Bogen des Herrn", "tokens": ["Und", "es", "spannt", "der", "Bo\u00b7gen", "des", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Seine bunte Farbenbr\u00fccke,", "tokens": ["Sei\u00b7ne", "bun\u00b7te", "Far\u00b7ben\u00b7br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tr\u00e4nen schimmern so freudig gern", "tokens": ["Tr\u00e4\u00b7nen", "schim\u00b7mern", "so", "freu\u00b7dig", "gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "In des Auges Sonnenblicke.", "tokens": ["In", "des", "Au\u00b7ges", "Son\u00b7nen\u00b7bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der Hals der Taube schimmert,", "tokens": ["Wie", "der", "Hals", "der", "Tau\u00b7be", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Locket eines Habichts Wut,", "tokens": ["Lo\u00b7cket", "ei\u00b7nes", "Ha\u00b7bichts", "Wut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, der Fried', wird der zertr\u00fcmmert,", "tokens": ["Ach", ",", "der", "Fried'", ",", "wird", "der", "zer\u00b7tr\u00fcm\u00b7mert", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "VAFIN", "ART", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Taube, du wirst Opferblut!", "tokens": ["Tau\u00b7be", ",", "du", "wirst", "Op\u00b7fer\u00b7blut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohl uns! \u00fcberm Habicht kreist", "tokens": ["Wohl", "uns", "!", "\u00fc\u00b7berm", "Ha\u00b7bicht", "kreist"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun ein Falke, doch zu leise,", "tokens": ["Nun", "ein", "Fal\u00b7ke", ",", "doch", "zu", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Habicht st\u00fcrzend zerrei\u00dft", "tokens": ["Denn", "der", "Ha\u00b7bicht", "st\u00fcr\u00b7zend", "zer\u00b7rei\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Weh! die Taube sich zur Speise.", "tokens": ["Weh", "!", "die", "Tau\u00b7be", "sich", "zur", "Spei\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Falke, lieber Falke, st\u00fcrze", "tokens": ["Fal\u00b7ke", ",", "lie\u00b7ber", "Fal\u00b7ke", ",", "st\u00fcr\u00b7ze"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADV", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf den Habicht, da\u00df ihr Weh", "tokens": ["Auf", "den", "Ha\u00b7bicht", ",", "da\u00df", "ihr", "Weh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "R\u00e4chend wenigstens sich k\u00fcrze,", "tokens": ["R\u00e4\u00b7chend", "we\u00b7nigs\u00b7tens", "sich", "k\u00fcr\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ich tot den M\u00f6rder seh!", "tokens": ["Da\u00df", "ich", "tot", "den", "M\u00f6r\u00b7der", "seh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Habicht wird zum blutigen Schild,", "tokens": ["Ha\u00b7bicht", "wird", "zum", "blu\u00b7ti\u00b7gen", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Unter ihm die arme Taube,", "tokens": ["Un\u00b7ter", "ihm", "die", "ar\u00b7me", "Tau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber der Falke steiget zum Wild", "tokens": ["A\u00b7ber", "der", "Fal\u00b7ke", "stei\u00b7get", "zum", "Wild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u00dcber des Wappens geharnischte Haube,", "tokens": ["\u00dc\u00b7ber", "des", "Wap\u00b7pens", "ge\u00b7har\u00b7nischte", "Hau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und der Falke wird zum Schwerte", "tokens": ["Und", "der", "Fal\u00b7ke", "wird", "zum", "Schwer\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Das sich flammend abw\u00e4rts kehrt,", "tokens": ["Das", "sich", "flam\u00b7mend", "ab\u00b7w\u00e4rts", "kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df der Traum erf\u00fcllet werde,", "tokens": ["Da\u00df", "der", "Traum", "er\u00b7f\u00fcl\u00b7let", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nieder in dein Herzschild f\u00e4hrt!", "tokens": ["Nie\u00b7der", "in", "dein", "Herz\u00b7schild", "f\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sieh den dunklen Schleier der Nacht,", "tokens": ["Sieh", "den", "dunk\u00b7len", "Schlei\u00b7er", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wie er sich hernieder senket,", "tokens": ["Wie", "er", "sich", "her\u00b7nie\u00b7der", "sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.