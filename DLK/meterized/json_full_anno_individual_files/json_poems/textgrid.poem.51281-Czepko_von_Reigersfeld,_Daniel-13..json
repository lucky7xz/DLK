{"textgrid.poem.51281": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Je mehr du W\u00fcrtze reibst, ie lieblicher sie schmeckt:", "tokens": ["Je", "mehr", "du", "W\u00fcrt\u00b7ze", "reibst", ",", "ie", "lieb\u00b7li\u00b7cher", "sie", "schmeckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je mehr du Feuer st\u00f6rst, ie weiter es sich streckt:", "tokens": ["Je", "mehr", "du", "Feu\u00b7er", "st\u00f6rst", ",", "ie", "wei\u00b7ter", "es", "sich", "streckt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVFIN", "$,", "ADV", "ADV", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je mehr das Schiff beschwert, ie sicherer es geht;", "tokens": ["Je", "mehr", "das", "Schiff", "be\u00b7schwert", ",", "ie", "si\u00b7che\u00b7rer", "es", "geht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Je mehr der Baum geprest, ie ruhiger er steht;", "tokens": ["Je", "mehr", "der", "Baum", "ge\u00b7prest", ",", "ie", "ru\u00b7hi\u00b7ger", "er", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", 