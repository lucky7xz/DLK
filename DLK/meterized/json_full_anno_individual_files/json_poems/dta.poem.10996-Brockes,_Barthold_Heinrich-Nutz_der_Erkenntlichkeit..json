{"dta.poem.10996": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nutz  \n der Erkenntlichkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich allhier, in sanfter Stille,", "tokens": ["Da\u00df", "ich", "all\u00b7hier", ",", "in", "sanf\u00b7ter", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sichrer Ruhe sitzen kann;", "tokens": ["Und", "sich\u00b7rer", "Ru\u00b7he", "sit\u00b7zen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seh ich, aus deiner Liebe F\u00fclle,", "tokens": ["Seh", "ich", ",", "aus", "dei\u00b7ner", "Lie\u00b7be", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als einen Stral der Gnaden, an.", "tokens": ["Als", "ei\u00b7nen", "Stral", "der", "Gna\u00b7den", ",", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach m\u00f6chte mich der Stral entz\u00fcnden!", "tokens": ["Ach", "m\u00f6ch\u00b7te", "mich", "der", "Stral", "ent\u00b7z\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00fcrd ich die gewissen Spuren", "tokens": ["So", "w\u00fcrd", "ich", "die", "ge\u00b7wis\u00b7sen", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der sonst verborgnen Gottheit sehn,", "tokens": ["Der", "sonst", "ver\u00b7borg\u00b7nen", "Got\u00b7theit", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein all erschaffend Wort verstehn,", "tokens": ["Sein", "all", "er\u00b7schaf\u00b7fend", "Wort", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "VVPP", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und, in vern\u00fcnftigem Empfinden,", "tokens": ["Und", ",", "in", "ver\u00b7n\u00fcnf\u00b7ti\u00b7gem", "Emp\u00b7fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wie alles, was er schuf, so sch\u00f6n.", "tokens": ["Wie", "al\u00b7les", ",", "was", "er", "schuf", ",", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sein\u2019 Allmacht, Liebe, Weisheit finden,", "tokens": ["Sein'", "All\u00b7macht", ",", "Lie\u00b7be", ",", "Weis\u00b7heit", "fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und ihn am w\u00fcrdigsten erh\u00f6hn!", "tokens": ["Und", "ihn", "am", "w\u00fcr\u00b7digs\u00b7ten", "er\u00b7h\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}}}}