{"dta.poem.16101": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "155.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum ich euch soviel Sinnbildliches berichte?", "tokens": ["Wa\u00b7rum", "ich", "euch", "so\u00b7viel", "Sinn\u00b7bild\u00b7li\u00b7ches", "be\u00b7rich\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Klein-Allt\u00e4gliches nur so wird zum Gedichte.", "tokens": ["Weil", "Klein\u00b7All\u00b7t\u00e4g\u00b7li\u00b7ches", "nur", "so", "wird", "zum", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ADV", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Als Sinnbild mu\u00df man es f\u00fcr etwas Gr\u00f6\u00dfres fassen;", "tokens": ["Als", "Sinn\u00b7bild", "mu\u00df", "man", "es", "f\u00fcr", "et\u00b7was", "Gr\u00f6\u00df\u00b7res", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VMFIN", "PIS", "PPER", "APPR", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Gro\u00dfes an sich selbst darf man wie's ist nur lassen.", "tokens": ["Ein", "Gro\u00b7\u00dfes", "an", "sich", "selbst", "darf", "man", "wie's", "ist", "nur", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ADV", "VMFIN", "PIS", "KOKOM", "VAFIN", "ADV",