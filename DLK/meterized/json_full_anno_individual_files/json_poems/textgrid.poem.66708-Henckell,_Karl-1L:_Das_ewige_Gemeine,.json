{"textgrid.poem.66708": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ewige Gemeine,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ewige Gemeine,", "tokens": ["Das", "e\u00b7wi\u00b7ge", "Ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wer ist ihm ganz entflohn?", "tokens": ["Wer", "ist", "ihm", "ganz", "ent\u00b7flohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir tragen alle Steine", "tokens": ["Wir", "tra\u00b7gen", "al\u00b7le", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Pharaonenbau der Konvention.", "tokens": ["Zum", "Pha\u00b7ra\u00b7o\u00b7nen\u00b7bau", "der", "Kon\u00b7ven\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "So mancher ha\u00dft die L\u00fcge,", "tokens": ["So", "man\u00b7cher", "ha\u00dft", "die", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwirft die Heuchelei,", "tokens": ["Ver\u00b7wirft", "die", "Heu\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch klagen seine Z\u00fcge", "tokens": ["Do