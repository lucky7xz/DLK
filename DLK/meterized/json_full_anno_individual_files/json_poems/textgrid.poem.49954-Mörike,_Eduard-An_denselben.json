{"textgrid.poem.49954": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An denselben", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r Er nur einmal, Herr Vetter,", "tokens": ["H\u00f6r", "Er", "nur", "ein\u00b7mal", ",", "Herr", "Vet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "$,", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was mir diese Nacht getr\u00e4umet!", "tokens": ["Was", "mir", "die\u00b7se", "Nacht", "ge\u00b7tr\u00e4u\u00b7met", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonntag war es, nach Mittage,", "tokens": ["Sonn\u00b7tag", "war", "es", ",", "nach", "Mit\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ich sah vom Fenster Seines", "tokens": ["Und", "ich", "sah", "vom", "Fens\u00b7ter", "Sei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alten gelben Gartenh\u00e4uschens,", "tokens": ["Al\u00b7ten", "gel\u00b7ben", "Gar\u00b7ten\u00b7h\u00e4usc\u00b7hens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wie die B\u00fcrgersleute ruhig", "tokens": ["Wie", "die", "B\u00fcr\u00b7gers\u00b7leu\u00b7te", "ru\u00b7hig"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor der Stadt spazierengingen.", "tokens": ["Vor", "der", "Stadt", "spa\u00b7zie\u00b7ren\u00b7gin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ich wandte mich und sah Ihn,", "tokens": ["Und", "ich", "wand\u00b7te", "mich", "und", "sah", "Ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Der im Anfang nicht zugegen,", "tokens": ["Der", "im", "An\u00b7fang", "nicht", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ernsthaft vor dem Spiegel stehen,", "tokens": ["Ernst\u00b7haft", "vor", "dem", "Spie\u00b7gel", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "In der Stellung eines Mannes,", "tokens": ["In", "der", "Stel\u00b7lung", "ei\u00b7nes", "Man\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Der sich zu balbieren trachtet.", "tokens": ["Der", "sich", "zu", "bal\u00b7bie\u00b7ren", "trach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Doch indem ich n\u00e4her trete", "tokens": ["Doch", "in\u00b7dem", "ich", "n\u00e4\u00b7her", "tre\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Mu\u00df ich voll Erstaunen sehen,", "tokens": ["Mu\u00df", "ich", "voll", "Er\u00b7stau\u00b7nen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Wie Er sich mit schwarzer Farbe", "tokens": ["Wie", "Er", "sich", "mit", "schwar\u00b7zer", "Far\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.16": {"text": "Auf Sein rundes Vollmondantlitz", "tokens": ["Auf", "Sein", "run\u00b7des", "Voll\u00b7mon\u00b7dant\u00b7litz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Einen saubern Halbkreis malte;", "tokens": ["Ei\u00b7nen", "sau\u00b7bern", "Halb\u00b7kreis", "mal\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Von der linken Schl\u00e4fe abw\u00e4rts,", "tokens": ["Von", "der", "lin\u00b7ken", "Schl\u00e4\u00b7fe", "ab\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Zwischen Mund und Kinn hindurch, und", "tokens": ["Zwi\u00b7schen", "Mund", "und", "Kinn", "hin\u00b7durch", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "KON", "NN", "PAV", "$,", "KON"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "So hinauf die rechte Backe.", "tokens": ["So", "hin\u00b7auf", "die", "rech\u00b7te", "Ba\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.21": {"text": "Jetzo mit ge\u00fcbtem Pinsel", "tokens": ["Jet\u00b7zo", "mit", "ge\u00b7\u00fcb\u00b7tem", "Pin\u00b7sel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Zeichnet' Er entlang dem Zirkel", "tokens": ["Zeich\u00b7net'", "Er", "ent\u00b7lang", "dem", "Zir\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Sch\u00f6ngeformte r\u00f6msche Ziffern,", "tokens": ["Sch\u00f6n\u00b7ge\u00b7form\u00b7te", "r\u00f6m\u00b7sche", "Zif\u00b7fern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Kunstgerecht, von eins bis zw\u00f6lfe.", "tokens": ["Kunst\u00b7ge\u00b7recht", ",", "von", "eins", "bis", "zw\u00f6l\u00b7fe", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PIS", "APPR", "CARD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Und ich dachte: ach, mein lieber", "tokens": ["Und", "ich", "dach\u00b7te", ":", "ach", ",", "mein", "lie\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "XY", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Vetter ist ein Narr geworden! \u2013", "tokens": ["Vet\u00b7ter", "ist", "ein", "Narr", "ge\u00b7wor\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "VAPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Denn Er sah mich an mit Augen,", "tokens": ["Denn", "Er", "sah", "mich", "an", "mit", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Die mich nicht zu kennen schienen.", "tokens": ["Die", "mich", "nicht", "zu", "ken\u00b7nen", "schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "\u00dcberdem stellt' Er sich f\u00f6rmlich", "tokens": ["\u00dc\u00b7ber\u00b7dem", "stellt'", "Er", "sich", "f\u00f6rm\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.30": {"text": "An das Fenster in die Sonne,", "tokens": ["An", "das", "Fens\u00b7ter", "in", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Und der Schatten Seiner Nase", "tokens": ["Und", "der", "Schat\u00b7ten", "Sei\u00b7ner", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Sollte nun die Stunde weisen.", "tokens": ["Soll\u00b7te", "nun", "die", "Stun\u00b7de", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Ach, die Leute auf der Stra\u00dfe", "tokens": ["Ach", ",", "die", "Leu\u00b7te", "auf", "der", "Stra\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Wollten fast sich Kr\u00f6pfe lachen!", "tokens": ["Woll\u00b7ten", "fast", "sich", "Kr\u00f6p\u00b7fe", "la\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PRF", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was nun dieser Traum bedeute?", "tokens": ["Was", "nun", "die\u00b7ser", "Traum", "be\u00b7deu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich will Ihn just nicht erschrecken:", "tokens": ["Ich", "will", "Ihn", "just", "nicht", "er\u00b7schre\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aber la\u00df Er Sein verdammtes", "tokens": ["A\u00b7ber", "la\u00df", "Er", "Sein", "ver\u00b7damm\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonnenuhrenmachen bleiben!", "tokens": ["Son\u00b7ne\u00b7nuh\u00b7ren\u00b7ma\u00b7chen", "blei\u00b7ben", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6r Er nur einmal, Herr Vetter,", "tokens": ["H\u00f