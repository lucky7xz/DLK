{"dta.poem.20389": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die an dem begr\u00e4bni\u00df-tage/ ihres gros-  \n sen Friedrich Wilhelms/ wehklagende Durch-  \n lauchtigste Dorothee.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ounerbittliches verh\u00e4ngni\u00df meiner jahre!", "tokens": ["Oun\u00b7er\u00b7bitt\u00b7li\u00b7ches", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "mei\u00b7ner", "jah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Du stiffter meiner noth und dieser todten-bahre!", "tokens": ["Du", "stiff\u00b7ter", "mei\u00b7ner", "noth", "und", "die\u00b7ser", "tod\u00b7ten\u00b7bah\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "PPOSAT", "NN", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vollbringest du nunmehr den allerletzten neid;", "tokens": ["Voll\u00b7brin\u00b7gest", "du", "nun\u00b7mehr", "den", "al\u00b7ler\u00b7letz\u00b7ten", "neid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was noch \u00fcbrig war an meiner traurigkeit?", "tokens": ["Und", "was", "noch", "\u00fcb\u00b7rig", "war", "an", "mei\u00b7ner", "trau\u00b7rig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Friedrich Wilhelm starb! du hast ihn mir entrissen;", "tokens": ["Mein", "Fried\u00b7rich", "Wil\u00b7helm", "starb", "!", "du", "hast", "ihn", "mir", "ent\u00b7ris\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "NE", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "War es dir nicht genug/ da\u00df ichs erleben m\u00fcssen?", "tokens": ["War", "es", "dir", "nicht", "ge\u00b7nug", "/", "da\u00df", "ichs", "er\u00b7le\u00b7ben", "m\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "ADV", "$(", "KOUS", "PIS", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hast du/ sein grab zu sehn/ mich leider auch bestimmt!", "tokens": ["Hast", "du", "/", "sein", "grab", "zu", "sehn", "/", "mich", "lei\u00b7der", "auch", "be\u00b7stimmt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "PPOSAT", "NN", "PTKZU", "VVINF", "$(", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das/ wie du lebendig/ mir ihn gestorben nimmt.", "tokens": ["Das", "/", "wie", "du", "le\u00b7ben\u00b7dig", "/", "mir", "ihn", "ge\u00b7stor\u00b7ben", "nimmt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWAV", "PPER", "ADJD", "$(", "PPER", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach nein! ach nein! zu viel der kl\u00e4glichen beschwerde!", "tokens": ["Ach", "nein", "!", "ach", "nein", "!", "zu", "viel", "der", "kl\u00e4g\u00b7li\u00b7chen", "be\u00b7schwer\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "XY", "PTKANT", "$.", "PTKA", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Zu viel/ da\u00df zweymahl ich durch ihn zur wittben werde!", "tokens": ["Zu", "viel", "/", "da\u00df", "zwey\u00b7mahl", "ich", "durch", "ihn", "zur", "witt\u00b7ben", "wer\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$(", "KOUS", "ADV", "PPER", "APPR", "PPER", "APPRART", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch seinen tod zuvor/ und nun durch diesen stein;", "tokens": ["Durch", "sei\u00b7nen", "tod", "zu\u00b7vor", "/", "und", "nun", "durch", "die\u00b7sen", "stein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$(", "KON", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der seiner aschen auch verbeut bey mir zu seyn!", "tokens": ["Der", "sei\u00b7ner", "asc\u00b7hen", "auch", "ver\u00b7beut", "bey", "mir", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "ADV", "VVFIN", "APPR", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In meiner finsterni\u00df/ da ich bi\u00dfher gesessen/", "tokens": ["In", "mei\u00b7ner", "fins\u00b7ter\u00b7ni\u00df", "/", "da", "ich", "bi\u00df\u00b7her", "ge\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da ohnmacht/ hertzeleid und angst mich abgefressen/", "tokens": ["Da", "ohn\u00b7macht", "/", "hert\u00b7ze\u00b7leid", "und", "angst", "mich", "ab\u00b7ge\u00b7fres\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "PTKVZ", "KON", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "War dieses noch mein trost/ bey meinem ungemach/", "tokens": ["War", "die\u00b7ses", "noch", "mein", "trost", "/", "bey", "mei\u00b7nem", "un\u00b7ge\u00b7mach", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df es mir meinen todt auff diesen tag versprach.", "tokens": ["Da\u00df", "es", "mir", "mei\u00b7nen", "todt", "auff", "die\u00b7sen", "tag", "ver\u00b7sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Gl\u00fcckselig mein geschick/ wenn du es vollenzogen!", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig", "mein", "ge\u00b7schick", "/", "wenn", "du", "es", "vol\u00b7len\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$(", "KOUS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wenn meine hoffnung nicht mi\u00dfg\u00fcnstig mich betrogen;", "tokens": ["Wenn", "mei\u00b7ne", "hoff\u00b7nung", "nicht", "mi\u00df\u00b7g\u00fcns\u00b7tig", "mich", "be\u00b7tro\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die meinen welcken leib/ zu der verlangten nacht/", "tokens": ["Die", "mei\u00b7nen", "wel\u00b7cken", "leib", "/", "zu", "der", "ver\u00b7lang\u00b7ten", "nacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Zum schatten und geripp/ zur leiche nicht gemacht!", "tokens": ["Zum", "schat\u00b7ten", "und", "ge\u00b7ripp", "/", "zur", "lei\u00b7che", "nicht", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KON", "ADJD", "$(", "APPRART", "ADJA", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich mu\u00df mich in mir selbst der tr\u00e4gen schwachheit sch\u00e4men/", "tokens": ["Ich", "mu\u00df", "mich", "in", "mir", "selbst", "der", "tr\u00e4\u00b7gen", "schwach\u00b7heit", "sch\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df ich nicht starck genug zu tode mich zu gr\u00e4men/", "tokens": ["Da\u00df", "ich", "nicht", "starck", "ge\u00b7nug", "zu", "to\u00b7de", "mich", "zu", "gr\u00e4\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "ADV", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da\u00df ich vor blossem schmertz nicht leicht ersterben kan;", "tokens": ["Da\u00df", "ich", "vor", "blos\u00b7sem", "schmertz", "nicht", "leicht", "ers\u00b7ter\u00b7ben", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Nach dem du solchen ri\u00df an meiner brust gethan.", "tokens": ["Nach", "dem", "du", "sol\u00b7chen", "ri\u00df", "an", "mei\u00b7ner", "brust", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Armselge Dorothee! worzu bist du erkohren?", "tokens": ["Arm\u00b7sel\u00b7ge", "Do\u00b7ro\u00b7thee", "!", "wor\u00b7zu", "bist", "du", "er\u00b7koh\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PWAV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.26": {"text": "Du weist/ was Brandenburg/ doch mehr was du verlohren;", "tokens": ["Du", "weist", "/", "was", "Bran\u00b7den\u00b7burg", "/", "doch", "mehr", "was", "du", "ver\u00b7loh\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "NE", "$(", "ADV", "ADV", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ein vaters gleicher sohn ersetzt ihm den verlust;", "tokens": ["Ein", "va\u00b7ters", "glei\u00b7cher", "sohn", "er\u00b7setzt", "ihm", "den", "ver\u00b7lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Nur dir stirbt gantz und gar was du deweinen must.", "tokens": ["Nur", "dir", "stirbt", "gantz", "und", "gar", "was", "du", "de\u00b7wei\u00b7nen", "must", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "KON", "ADV", "PWS", "PPER", "PDS", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich klage nicht die macht/ die hoheit und das gl\u00fccke/", "tokens": ["Ich", "kla\u00b7ge", "nicht", "die", "macht", "/", "die", "ho\u00b7heit", "und", "das", "gl\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "VVFIN", "$(", "ART", "NN", "KON", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df alles l\u00e4st mein f\u00fcrst mit Friedrich mir zur\u00fccke.", "tokens": ["Da\u00df", "al\u00b7les", "l\u00e4st", "mein", "f\u00fcrst", "mit", "Fried\u00b7rich", "mir", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PPOSAT", "VVFIN", "APPR", "NE", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich klage meinen herrn; nicht seinen f\u00fcrsten-saal;", "tokens": ["Ich", "kla\u00b7ge", "mei\u00b7nen", "herrn", ";", "nicht", "sei\u00b7nen", "f\u00fcrs\u00b7ten\u00b7saal", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Nicht das gekr\u00f6nte haupt; nur blo\u00df mein eh-gemahl.", "tokens": ["Nicht", "das", "ge\u00b7kr\u00f6n\u00b7te", "haupt", ";", "nur", "blo\u00df", "mein", "eh\u00b7ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$.", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mein werthes eh-gemahl/ des treue mich erlesen;", "tokens": ["Mein", "wert\u00b7hes", "eh\u00b7ge\u00b7mahl", "/", "des", "treu\u00b7e", "mich", "er\u00b7le\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Der auch so liebreich war/ als gro\u00df er ist gewesen.", "tokens": ["Der", "auch", "so", "lieb\u00b7reich", "war", "/", "als", "gro\u00df", "er", "ist", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VAFIN", "$(", "KOUS", "ADJD", "PPER", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Erweget/ welch ein schatz des Friedrich Wilhelms hertz/", "tokens": ["Er\u00b7we\u00b7get", "/", "welch", "ein", "schatz", "des", "Fried\u00b7rich", "Wil\u00b7helms", "hertz", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "PWAT", "ART", "NN", "ART", "NE", "NE", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und messet denn darnach der Dorotheen schmertz!", "tokens": ["Und", "mes\u00b7set", "denn", "dar\u00b7nach", "der", "Do\u00b7ro\u00b7theen", "schmertz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Ach lie\u00df der himmel ihn nur eintzig wieder leben!", "tokens": ["Ach", "lie\u00df", "der", "him\u00b7mel", "ihn", "nur", "eint\u00b7zig", "wie\u00b7der", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ART", "NN", "PPER", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und hatt\u2019 er sonsten nichts/ als seine gunst zugeben/", "tokens": ["Und", "hatt'", "er", "sons\u00b7ten", "nichts", "/", "als", "sei\u00b7ne", "gunst", "zu\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PIS", "$(", "KOUS", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie gl\u00fccklich w\u00fcrd\u2019 ich seyn/ auch sonder glantz und thron;", "tokens": ["Wie", "gl\u00fcck\u00b7lich", "w\u00fcrd'", "ich", "seyn", "/", "auch", "son\u00b7der", "glantz", "und", "thron", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VAINF", "$(", "ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Bey seiner eintzigen geliebtesten person;", "tokens": ["Bey", "sei\u00b7ner", "eint\u00b7zi\u00b7gen", "ge\u00b7lieb\u00b7tes\u00b7ten", "per\u00b7son", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ach h\u00e4tt\u2019 ich/ da er ja die welt verlassen wollen/", "tokens": ["Ach", "h\u00e4tt'", "ich", "/", "da", "er", "ja", "die", "welt", "ver\u00b7las\u00b7sen", "wol\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "F\u00fcr ihn; Ist es zu viel? mit ihm erblassen sollen!", "tokens": ["F\u00fcr", "ihn", ";", "Ist", "es", "zu", "viel", "?", "mit", "ihm", "er\u00b7blas\u00b7sen", "sol\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "VAFIN", "PPER", "PTKA", "PIS", "$.", "APPR", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.43": {"text": "So w\u00e4r ich in der grufft auch seine folgerin;", "tokens": ["So", "w\u00e4r", "ich", "in", "der", "grufft", "auch", "sei\u00b7ne", "fol\u00b7ge\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wie ich hier ungetrennt von ihm gewesen bin.", "tokens": ["Wie", "ich", "hier", "un\u00b7ge\u00b7trennt", "von", "ihm", "ge\u00b7we\u00b7sen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "APPR", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Allein der himmel spielt mit meinen heissen zehren/", "tokens": ["Al\u00b7lein", "der", "him\u00b7mel", "spielt", "mit", "mei\u00b7nen", "heis\u00b7sen", "zeh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und will mir meinen wunsch nach keiner art gew\u00e4hren.", "tokens": ["Und", "will", "mir", "mei\u00b7nen", "wunsch", "nach", "kei\u00b7ner", "art", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Es scheint er habe mich zu dieser qvaal versehn;", "tokens": ["Es", "scheint", "er", "ha\u00b7be", "mich", "zu", "die\u00b7ser", "qvaal", "ver\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PPER", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Wohlan! so will ich denn auch recht zu grabe gehn.", "tokens": ["Wo\u00b7hlan", "!", "so", "will", "ich", "denn", "auch", "recht", "zu", "gra\u00b7be", "gehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "VMFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Ich will mich aus der welt mit dieser leiche tragen:", "tokens": ["Ich", "will", "mich", "aus", "der", "welt", "mit", "die\u00b7ser", "lei\u00b7che", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "APPR", "PDAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Mein leben soll nichts seyn/ denn ein verl\u00e4ngtes klagen:", "tokens": ["Mein", "le\u00b7ben", "soll", "nichts", "seyn", "/", "denn", "ein", "ver\u00b7l\u00e4ng\u00b7tes", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VMFIN", "PIS", "VAINF", "$(", "KON", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Den vurpur werff ich heut zu ihm in dessen grab;", "tokens": ["Den", "vur\u00b7pur", "werff", "ich", "heut", "zu", "ihm", "in", "des\u00b7sen", "grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "PRELAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Und sterbe/ wie er stirbt/ der gantzen erden ab.", "tokens": ["Und", "ster\u00b7be", "/", "wie", "er", "stirbt", "/", "der", "gant\u00b7zen", "er\u00b7den", "ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PWAV", "PPER", "VVFIN", "$(", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Mein zimmer will ich nun zum todes-tempel machen/", "tokens": ["Mein", "zim\u00b7mer", "will", "ich", "nun", "zum", "to\u00b7des\u00b7tem\u00b7pel", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "In welchem ihn und mich mein jammer soll bewachen.", "tokens": ["In", "wel\u00b7chem", "ihn", "und", "mich", "mein", "jam\u00b7mer", "soll", "be\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "KON", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Wo sein entseelter leib in meinen schmertzen lebt/", "tokens": ["Wo", "sein", "ent\u00b7seel\u00b7ter", "leib", "in", "mei\u00b7nen", "schmert\u00b7zen", "lebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Und t\u00e4glich meine pein mich neben ihn begr\u00e4bt.", "tokens": ["Und", "t\u00e4g\u00b7lich", "mei\u00b7ne", "pein", "mich", "ne\u00b7ben", "ihn", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Hier soll mein kummer ihn und sein ged\u00e4chtni\u00df ehren;", "tokens": ["Hier", "soll", "mein", "kum\u00b7mer", "ihn", "und", "sein", "ge\u00b7d\u00e4cht\u00b7ni\u00df", "eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Ich klage nicht um trost von iemand zu begehren.", "tokens": ["Ich", "kla\u00b7ge", "nicht", "um", "trost", "von", "ie\u00b7mand", "zu", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Mich tr\u00f6stet: da\u00df mein hertz ein stetes leiden frist;", "tokens": ["Mich", "tr\u00f6s\u00b7tet", ":", "da\u00df", "mein", "hertz", "ein", "ste\u00b7tes", "lei\u00b7den", "frist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Und es dahin gedenckt/ wo mein verlangen ist.", "tokens": ["Und", "es", "da\u00b7hin", "ge\u00b7denckt", "/", "wo", "mein", "ver\u00b7lan\u00b7gen", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "VVPP", "$(", "PWAV", "PPOSAT", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Ihr zeugen meiner eh/ ihr printzen und printzessen/", "tokens": ["Ihr", "zeu\u00b7gen", "mei\u00b7ner", "eh", "/", "ihr", "print\u00b7zen", "und", "print\u00b7zes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "KOUS", "$(", "PPOSAT", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.62": {"text": "Auch euch mu\u00df ich nunmehr bey diesem fall vergessen!", "tokens": ["Auch", "euch", "mu\u00df", "ich", "nun\u00b7mehr", "bey", "die\u00b7sem", "fall", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Geburts- und nahmens-tag/ der zu erg\u00f6tzen pflag/", "tokens": ["Ge\u00b7burts", "und", "nah\u00b7mens\u00b7tag", "/", "der", "zu", "er\u00b7g\u00f6t\u00b7zen", "pflag", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "$(", "ART", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Du bist nun/ \u00fcmgekehrt/ mir ein beg\u00e4ngni\u00df-tag.", "tokens": ["Du", "bist", "nun", "/", "\u00fcm\u00b7ge\u00b7kehrt", "/", "mir", "ein", "be\u00b7g\u00e4ng\u00b7ni\u00df\u00b7tag", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "VVFIN", "$(", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Ich will/ so offt du kommst/ nur tod und sterben nennen;", "tokens": ["Ich", "will", "/", "so", "offt", "du", "kommst", "/", "nur", "tod", "und", "ster\u00b7ben", "nen\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "ADV", "ADV", "PPER", "VVFIN", "$(", "ADV", "NN", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Man soll aus meiner trau\u2019r die reine treu erkennen:", "tokens": ["Man", "soll", "aus", "mei\u00b7ner", "trau'r", "die", "rei\u00b7ne", "treu", "er\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Da\u00df/ wie ich/ in der welt/ nur meinen held geliebt;", "tokens": ["Da\u00df", "/", "wie", "ich", "/", "in", "der", "welt", "/", "nur", "mei\u00b7nen", "held", "ge\u00b7liebt", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWAV", "PPER", "$(", "APPR", "ART", "NN", "$(", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Mich auch/ bey dem verlust/ nur sein verlust betr\u00fcbt.", "tokens": ["Mich", "auch", "/", "bey", "dem", "ver\u00b7lust", "/", "nur", "sein", "ver\u00b7lust", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$(", "APPR", "ART", "NN", "$(", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.69": {"text": "Nun/ leich/ und bahre f\u00e4hrt/ ich werde nachgef\u00fchret/", "tokens": ["Nun", "/", "leich", "/", "und", "bah\u00b7re", "f\u00e4hrt", "/", "ich", "wer\u00b7de", "nach\u00b7ge\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJD", "$(", "KON", "VVFIN", "VVFIN", "$(", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Ein k\u00f6niglich gepr\u00e4ng wird um und um gesp\u00fcret/", "tokens": ["Ein", "k\u00f6\u00b7nig\u00b7lich", "ge\u00b7pr\u00e4ng", "wird", "um", "und", "um", "ge\u00b7sp\u00fc\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VAFIN", "APPR", "KON", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "So Friedrichs fr\u00f