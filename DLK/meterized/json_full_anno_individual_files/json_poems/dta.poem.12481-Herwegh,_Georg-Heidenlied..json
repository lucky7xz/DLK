{"dta.poem.12481": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Heidenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie lebten doch die Heiden", "tokens": ["Wie", "leb\u00b7ten", "doch", "die", "Hei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So herrlich und so froh!", "tokens": ["So", "herr\u00b7lich", "und", "so", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das war ein Volk von Seiden,", "tokens": ["Das", "war", "ein", "Volk", "von", "Sei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir sind ein Volk von Stroh;", "tokens": ["Wir", "sind", "ein", "Volk", "von", "Stroh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Entf\u00fchrt' ein Ochs ein sch\u00f6nes Kind", "tokens": ["Ent\u00b7f\u00fchrt'", "ein", "Ochs", "ein", "sch\u00f6\u00b7nes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zuweilen auch \u2014 doch glaubet mir:", "tokens": ["Zu\u00b7wei\u00b7len", "auch", "doch", "glau\u00b7bet", "mir", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "ADV", "VVFIN", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Sie Heiden waren nicht so blind", "tokens": ["Sie", "Hei\u00b7den", "wa\u00b7ren", "nicht", "so", "blind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht halb so blind, als wir.", "tokens": ["Nicht", "halb", "so", "blind", ",", "als", "wir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "ADJD", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Heiden, 's ist doch schade", "tokens": ["Die", "Hei\u00b7den", ",", "'s", "ist", "doch", "scha\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Um solch ", "tokens": ["Um", "solch"], "token_info": ["word", "word"], "pos": ["KOUI", "PIAT"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Sie hie\u00dfen Vier gerade", "tokens": ["Sie", "hie\u00b7\u00dfen", "Vier", "ge\u00b7ra\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nahmen F\u00fcnf f\u00fcr krumm;", "tokens": ["Und", "nah\u00b7men", "F\u00fcnf", "f\u00fcr", "krumm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "APPR", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch hatt' die Jungferschaft ein End',", "tokens": ["Auch", "hatt'", "die", "Jung\u00b7fer\u00b7schaft", "ein", "End'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sobald die Magd ein Kind gebar,", "tokens": ["So\u00b7bald", "die", "Magd", "ein", "Kind", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dieweil das N. T.", "tokens": ["Die\u00b7weil", "das", "N.", "T."], "token_info": ["word", "word", "abbreviation", "abbreviation"], "pos": ["KOUS", "ART", "NN", "NE"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Noch nicht erfunden war.", "tokens": ["Noch", "nicht", "er\u00b7fun\u00b7den", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie thaten, was sie mochten,", "tokens": ["Sie", "tha\u00b7ten", ",", "was", "sie", "moch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Frechheit war enorm;", "tokens": ["Die", "Frech\u00b7heit", "war", "en\u00b7orm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie siegten, wenn sie fochten,", "tokens": ["Sie", "sieg\u00b7ten", ",", "wenn", "sie", "foch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch ohne Uniform;", "tokens": ["Auch", "oh\u00b7ne", "U\u00b7ni\u00b7form", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie hatten keine Polizei", "tokens": ["Sie", "hat\u00b7ten", "kei\u00b7ne", "Po\u00b7li\u00b7zei"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und tranken lieber Wein, als Bier;", "tokens": ["Und", "tran\u00b7ken", "lie\u00b7ber", "Wein", ",", "als", "Bier", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie waren doch die Heiden frei,", "tokens": ["Wie", "wa\u00b7ren", "doch", "die", "Hei\u00b7den", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Heiden! \u2014 aber Ihr?", "tokens": ["Die", "Hei\u00b7den", "!", "a\u00b7ber", "Ihr", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "KON", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Und von Achill und Hektor,", "tokens": ["Und", "von", "A\u00b7chill", "und", "Hek\u00b7tor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "KON", "NE", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie's im Homerus steht,", "tokens": ["Wie's", "im", "Ho\u00b7me\u00b7rus", "steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NE", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Bis zu dem letzten Rektor", "tokens": ["Bis", "zu", "dem", "letz\u00b7ten", "Rek\u00b7tor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Universit\u00e4t,", "tokens": ["Der", "U\u00b7niv\u00b7er\u00b7si\u00b7t\u00e4t", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da gab's kein Buch in ganz Athen \u2014", "tokens": ["Da", "gab's", "kein", "Buch", "in", "ganz", "A\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "ADV", "NE", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "O schreckliche Verworfenheit!", "tokens": ["O", "schreck\u00b7li\u00b7che", "Ver\u00b7wor\u00b7fen\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man wurde vom Spazierengeh'n", "tokens": ["Man", "wur\u00b7de", "vom", "Spa\u00b7zie\u00b7ren\u00b7geh'n"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und von der Luft gescheidt.", "tokens": ["Und", "von", "der", "Luft", "ge\u00b7scheidt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie wu\u00dften sie die Tatzen", "tokens": ["Wie", "wu\u00df\u00b7ten", "sie", "die", "Tat\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Pfaffen abzuhau'n!", "tokens": ["Den", "Pfaf\u00b7fen", "ab\u00b7zu\u00b7hau'n", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die durften nur nach Spatzen,", "tokens": ["Die", "durf\u00b7ten", "nur", "nach", "Spat\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht nach den Weibern schau'n;", "tokens": ["Nicht", "nach", "den", "Wei\u00b7bern", "schau'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Den Prinzen gar erging es schlecht,", "tokens": ["Den", "Prin\u00b7zen", "gar", "er\u00b7ging", "es", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die fanden kaum ein Nachtquartier;", "tokens": ["Die", "fan\u00b7den", "kaum", "ein", "Nacht\u00b7quar\u00b7tier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie hatten doch die Heiden recht,", "tokens": ["Wie", "hat\u00b7ten", "doch", "die", "Hei\u00b7den", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Heiden! \u2014 aber Ihr?", "tokens": ["Die", "Hei\u00b7den", "!", "a\u00b7ber", "Ihr", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "KON", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Die Heiden, ach! die Heiden,", "tokens": ["Die", "Hei\u00b7den", ",", "ach", "!", "die", "Hei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die keine Christen sind,", "tokens": ["Die", "kei\u00b7ne", "Chris\u00b7ten", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie spinnen doch die Seiden", "tokens": ["Sie", "spin\u00b7nen", "doch", "die", "Sei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr manch ein Christenkind;", "tokens": ["F\u00fcr", "manch", "ein", "Chris\u00b7ten\u00b7kind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum lebe hoch das Heidenpack,", "tokens": ["Drum", "le\u00b7be", "hoch", "das", "Hei\u00b7den\u00b7pack", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und jeder \u00e4chte Heidenstrick,", "tokens": ["Und", "je\u00b7der", "\u00e4ch\u00b7te", "Hei\u00b7den\u00b7strick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Homerus mit dem Bettelsack", "tokens": ["Ho\u00b7me\u00b7rus", "mit", "dem", "Bet\u00b7tel\u00b7sack"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und ihre Republik!", "tokens": ["Und", "ih\u00b7re", "Re\u00b7pub\u00b7lik", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}