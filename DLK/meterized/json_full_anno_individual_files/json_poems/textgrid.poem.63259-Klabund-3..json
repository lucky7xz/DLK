{"textgrid.poem.63259": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hieltest mir als holde Amorette", "tokens": ["Du", "hiel\u00b7test", "mir", "als", "hol\u00b7de", "A\u00b7mo\u00b7ret\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Himmelsleiter, die ich aufw\u00e4rts stieg.", "tokens": ["Die", "Him\u00b7mels\u00b7lei\u00b7ter", ",", "die", "ich", "auf\u00b7w\u00e4rts", "stieg", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du wusstest meine Worte, als ich schwieg,", "tokens": ["Du", "wuss\u00b7test", "mei\u00b7ne", "Wor\u00b7te", ",", "als", "ich", "schwieg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schmiedetest mich an die Veilchenkette.", "tokens": ["Und", "schmie\u00b7de\u00b7test", "mich", "an", "die", "Veil\u00b7chen\u00b7ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie oft ging ich mit einer Frau zu Bette,", "tokens": ["Wie", "oft", "ging", "ich", "mit", "ei\u00b7ner", "Frau", "zu", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und es erscholl Schalmei und Mondmusik.", "tokens": ["Und", "es", "er\u00b7scholl", "Schal\u00b7mei", "und", "Mond\u00b7mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie jauchzte die Geliebte: Liebster, flieg,", "tokens": ["Wie", "jauchz\u00b7te", "die", "Ge\u00b7lieb\u00b7te", ":", "Liebs\u00b7ter", ",", "flieg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Flieg, in den Krallen mich, zur Sonnenst\u00e4tte,", "tokens": ["Flieg", ",", "in", "den", "Kral\u00b7len", "mich", ",", "zur", "Son\u00b7nen\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.3": {"line.1": {"text": "Du Adler! \u2013 Aber eine kleine Hand", "tokens": ["Du", "Ad\u00b7ler", "!", "\u2013", "A\u00b7ber", "ei\u00b7ne", "klei\u00b7ne", "Hand"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$.", "$(", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hielt mich zur\u00fcck, und ich vernahm ein Fl\u00fcstern:", "tokens": ["Hielt", "mich", "zu\u00b7r\u00fcck", ",", "und", "ich", "ver\u00b7nahm", "ein", "Fl\u00fcs\u00b7tern", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bleib bei den Weibern fest \u2013 auf festem Land!", "tokens": ["Bleib", "bei", "den", "Wei\u00b7bern", "fest", "\u2013", "auf", "fes\u00b7tem", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$(", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Sie haben Br\u00fcste! Atmen durch die N\u00fcstern!", "tokens": ["Sie", "ha\u00b7ben", "Br\u00fcs\u00b7te", "!", "At\u00b7men", "durch", "die", "N\u00fcs\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sind dem Blute blutend zugewandt ...", "tokens": ["Und", "sind", "dem", "Blu\u00b7te", "blu\u00b7tend", "zu\u00b7ge\u00b7wandt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir aber brennt ein Licht aus Himmelsl\u00fcstern.", "tokens": ["Dir", "a\u00b7ber", "brennt", "ein", "Licht", "aus", "Him\u00b7mels\u00b7l\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du hieltest mir als holde Amorette", "tokens": ["Du", "hiel\u00b7test", "mir", "als", "hol\u00b7de", "A\u00b7mo\u00b7ret\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.