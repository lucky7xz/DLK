{"dta.poem.19700": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Herr Burkhart M\u00fcnch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war Herr Burkhart M\u00fcnch bekannt               ", "tokens": ["Es", "war", "Herr", "Burk\u00b7hart", "M\u00fcnch", "be\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "NE", "NE", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als tapfrer Kriegsmann in dem Land,", "tokens": ["Als", "tapf\u00b7rer", "Kriegs\u00b7mann", "in", "dem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dem Delphin aus Frankereich", "tokens": ["Mit", "dem", "Del\u00b7phin", "aus", "Fran\u00b7ke\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NE"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Er kam mit starker Macht zugleich.", "tokens": ["Er", "kam", "mit", "star\u00b7ker", "Macht", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht weit von Basel fiel zumal", "tokens": ["Nicht", "weit", "von", "Ba\u00b7sel", "fiel", "zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "APPR", "NE", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Eidgenossen grosse Zahl,", "tokens": ["Der", "Eid\u00b7ge\u00b7nos\u00b7sen", "gros\u00b7se", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So da\u00df sein Feind f\u00fcr diesmal zwar", "tokens": ["So", "da\u00df", "sein", "Feind", "f\u00fcr", "dies\u00b7mal", "zwar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "NN", "APPR", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erleget und entflohen war.", "tokens": ["Er\u00b7le\u00b7get", "und", "ent\u00b7flo\u00b7hen", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da ritt Herr Burkhart M\u00fcnch frey fort,", "tokens": ["Da", "ritt", "Herr", "Burk\u00b7hart", "M\u00fcnch", "frey", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "NE", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort auf die Wahlstadt an den Ort,", "tokens": ["Dort", "auf", "die", "Wahl\u00b7stadt", "an", "den", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch \u00fcber todte K\u00f6rper all", "tokens": ["Auch", "\u00fc\u00b7ber", "tod\u00b7te", "K\u00f6r\u00b7per", "all"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und triumphirt mit lautem Schall.", "tokens": ["Und", "tri\u00b7um\u00b7phirt", "mit", "lau\u00b7tem", "Schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und auf der Wahlstadt einen fand,", "tokens": ["Und", "auf", "der", "Wahl\u00b7stadt", "ei\u00b7nen", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ihm zuvor war wohlbekannt,", "tokens": ["Der", "ihm", "zu\u00b7vor", "war", "wohl\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der seine Wunden schwer ertrug,", "tokens": ["Der", "sei\u00b7ne", "Wun\u00b7den", "schwer", "er\u00b7trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Alsbald er sein Visier aufschlug.", "tokens": ["Als\u00b7bald", "er", "sein", "Vi\u00b7sier", "auf\u00b7schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und sprach: \u201eSchau heut zu Tag hiebey,", "tokens": ["Und", "sprach", ":", "\u201e", "Schau", "heut", "zu", "Tag", "hie\u00b7bey", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201eda baden wir in Rosen frey.\u201c", "tokens": ["\u201e", "da", "ba\u00b7den", "wir", "in", "Ro\u00b7sen", "frey", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Solch Wort erh\u00f6rt ein Eidgeno\u00df,", "tokens": ["Solch", "Wort", "er\u00b7h\u00f6rt", "ein", "Eid\u00b7ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem diese Schmach gar sehr verdro\u00df.", "tokens": ["Dem", "die\u00b7se", "Schmach", "gar", "sehr", "ver\u00b7dro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da\u00df er zu r\u00e4chen sie gedacht:", "tokens": ["Da\u00df", "er", "zu", "r\u00e4\u00b7chen", "sie", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eich m\u00f6cht nur haben so viel Macht,", "tokens": ["\u201e", "ich", "m\u00f6cht", "nur", "ha\u00b7ben", "so", "viel", "Macht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "VAFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eweil ich doch lieg zum todt verwundt.\u201c", "tokens": ["\u201e", "weil", "ich", "doch", "lieg", "zum", "todt", "ver\u00b7wundt", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "VVFIN", "APPRART", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Also er sich ermahnt zur Stund.", "tokens": ["Al\u00b7so", "er", "sich", "er\u00b7mahnt", "zur", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Da richtet er an einem Stein", "tokens": ["Da", "rich\u00b7tet", "er", "an", "ei\u00b7nem", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich auf die Kniee ganz allein,", "tokens": ["Sich", "auf", "die", "Kni\u00b7ee", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und warf denselben scharfen Stein", "tokens": ["Und", "warf", "den\u00b7sel\u00b7ben", "schar\u00b7fen", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herrn Burkhart in den Helm hinein.", "tokens": ["Herrn", "Burk\u00b7hart", "in", "den", "Helm", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da sank Herr Burkhart unverzogen,", "tokens": ["Da", "sank", "Herr", "Burk\u00b7hart", "un\u00b7ver\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NE", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und starb an seinem Sattelbogen,", "tokens": ["Und", "starb", "an", "sei\u00b7nem", "Sat\u00b7tel\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Ro\u00df gieng mit dem Ritter durch", "tokens": ["Das", "Ro\u00df", "gieng", "mit", "dem", "Rit\u00b7ter", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bracht ihn sterbend in die Burg.", "tokens": ["Und", "bracht", "ihn", "ster\u00b7bend", "in", "die", "Burg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "\u201ewie h\u00e4ngt der Ritter auf dem Ro\u00df?", "tokens": ["\u201e", "wie", "h\u00e4ngt", "der", "Rit\u00b7ter", "auf", "dem", "Ro\u00df", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201esein Panzer ist ja rosenroth!", "tokens": ["\u201e", "sein", "Pan\u00b7zer", "ist", "ja", "ro\u00b7sen\u00b7roth", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201elegt ihn nur auf den Kirchhof fein,", "tokens": ["\u201e", "legt", "ihn", "nur", "auf", "den", "Kirch\u00b7hof", "fein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u201eda wachsen viele R\u00f6selein.\u201c", "tokens": ["\u201e", "da", "wach\u00b7sen", "vie\u00b7le", "R\u00f6\u00b7se\u00b7lein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "So ward die Ros in ihrem Blut,", "tokens": ["So", "ward", "die", "Ros", "in", "ih\u00b7rem", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die frech erwuchs mit Uebermuth,", "tokens": ["Die", "frech", "er\u00b7wuchs", "mit", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gar bald zu nicht durch fromme H\u00e4nd,", "tokens": ["Gar", "bald", "zu", "nicht", "durch", "from\u00b7me", "H\u00e4nd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Rosenbad Gott von uns wend.", "tokens": ["Das", "Ro\u00b7sen\u00b7bad", "Gott", "von", "uns", "wend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}