{"textgrid.poem.58698": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "10. Die Verkl\u00e4rung des Namens Jesu in uns", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werd' ich vernichtet sein, so wird mich Gott erh\u00f6hen,", "tokens": ["Werd'", "ich", "ver\u00b7nich\u00b7tet", "sein", ",", "so", "wird", "mich", "Gott", "er\u00b7h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VAINF", "$,", "ADV", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist es, Jesu, dir, so mu\u00df es mir geschehen;", "tokens": ["So", "ist", "es", ",", "Je\u00b7su", ",", "dir", ",", "so", "mu\u00df", "es", "mir", "ge\u00b7sche\u00b7hen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NE", "$,", "PPER", "$,", "ADV", "VMFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch nein, bleib du nur hoch, ich will in deinem Namen", "tokens": ["Doch", "nein", ",", "bleib", "du", "nur", "hoch", ",", "ich", "will", "in", "dei\u00b7nem", "Na\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKANT", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Ehrfurcht, Lieb' und Freud' mich ewig b\u00fccken. Amen.", "tokens": ["Mit", "Ehr\u00b7furcht", ",", "Lieb'", "und", "Freud'", "mich", "e\u00b7wig", "b\u00fc\u00b7cken",