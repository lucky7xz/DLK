{"dta.poem.3155": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Lichte, da\u00df es brenn', ist n\u00f6thig Fett und Docht;", "tokens": ["Dem", "Lich\u00b7te", ",", "da\u00df", "es", "brenn'", ",", "ist", "n\u00f6\u00b7thig", "Fett", "und", "Docht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PTKVZ", "$,", "VAFIN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fehlt ihm von beiden eins, so hat es nichts vermocht.", "tokens": ["Fehlt", "ihm", "von", "bei\u00b7den", "eins", ",", "so", "hat", "es", "nichts", "ver\u00b7mocht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "PIS", "$,", "ADV", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und beide m\u00fcssen rein auch aufgehn miteinander,", "tokens": ["Und", "bei\u00b7de", "m\u00fcs\u00b7sen", "rein", "auch", "auf\u00b7gehn", "mi\u00b7tein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ADV", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df seines Elements froh sei der Salamander.", "tokens": ["Da\u00df", "sei\u00b7nes", "E\u00b7le\u00b7ments", "froh", "sei", "der", "Sa\u00b7la\u00b7man\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo Docht zu kurz ist, da erstirbt im Fett der Klumpen;", "tokens": ["Wo", "Docht", "zu", "kurz", "ist", ",", "da", "er\u00b7stirbt", "im", "Fett", "der", "Klum\u00b7pen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKA", "ADJD", "VAFIN", "$,", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo das Fett ausgeht, verlodert schnell der Stumpen.", "tokens": ["Und", "wo", "das", "Fett", "aus\u00b7geht", ",", "ver\u00b7lo\u00b7dert", "schnell", "der", "Stum\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Schwindsucht ist es hier, und dort die Wassersucht,", "tokens": ["Die", "Schwind\u00b7sucht", "ist", "es", "hier", ",", "und", "dort", "die", "Was\u00b7ser\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Mi\u00dfverh\u00e4ltnisses mi\u00dfliche Doppelfrucht.", "tokens": ["Des", "Mi\u00df\u00b7ver\u00b7h\u00e4lt\u00b7nis\u00b7ses", "mi\u00df\u00b7li\u00b7che", "Dop\u00b7pel\u00b7frucht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Erfreulich leuchtet da allein des Lebens Licht,", "tokens": ["Er\u00b7freu\u00b7lich", "leuch\u00b7tet", "da", "al\u00b7lein", "des", "Le\u00b7bens", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Geist und K\u00f6rper ist im rechten Gleichgewicht.", "tokens": ["Wo", "Geist", "und", "K\u00f6r\u00b7per", "ist", "im", "rech\u00b7ten", "Gleich\u00b7ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}