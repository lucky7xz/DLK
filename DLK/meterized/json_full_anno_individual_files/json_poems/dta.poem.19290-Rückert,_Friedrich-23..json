{"dta.poem.19290": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil nicht ein gro\u00dfer F\u00fcrst im weiten L\u00e4nderbann", "tokens": ["Weil", "nicht", "ein", "gro\u00b7\u00dfer", "F\u00fcrst", "im", "wei\u00b7ten", "L\u00e4n\u00b7der\u00b7bann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In alles Einzelne sich mischen soll und kann;", "tokens": ["In", "al\u00b7les", "Ein\u00b7zel\u00b7ne", "sich", "mi\u00b7schen", "soll", "und", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "PRF", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "So meinest du, da\u00df Gott auch nur das Allgemeine", "tokens": ["So", "mei\u00b7nest", "du", ",", "da\u00df", "Gott", "auch", "nur", "das", "All\u00b7ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "NN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Welt geordnet hab', und walte nicht ins Kleine.", "tokens": ["Der", "Welt", "ge\u00b7ord\u00b7net", "hab'", ",", "und", "wal\u00b7te", "nicht", "ins", "Klei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "KON", "VVFIN", "PTKNEG", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch macht ja wohl ein F\u00fcrst auch durch sein Land die Fahrt,", "tokens": ["Doch", "macht", "ja", "wohl", "ein", "F\u00fcrst", "auch", "durch", "sein", "Land", "die", "Fahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eingreifend hier und dort mit eigner Gegenwart.", "tokens": ["Ein\u00b7grei\u00b7fend", "hier", "und", "dort", "mit", "eig\u00b7ner", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und w\u00e4r' Allgegenwart, wie Gott, auch ihm verliehn,", "tokens": ["Und", "w\u00e4r'", "All\u00b7ge\u00b7gen\u00b7wart", ",", "wie", "Gott", ",", "auch", "ihm", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$,", "PWAV", "NN", "$,", "ADV", "PPER", "VVINF", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "So braucht' er nicht die Fahrt, und alles f\u00fchr' um ihn.", "tokens": ["So", "braucht'", "er", "nicht", "die", "Fahrt", ",", "und", "al\u00b7les", "f\u00fchr'", "um", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "KON", "PIS", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Allgegenw\u00e4rtig ist Gott in den Welten nicht", "tokens": ["All\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "ist", "Gott", "in", "den", "Wel\u00b7ten", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "NN", "APPR", "ART", "NN", "PTKNEG"], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sowohl als sie vielmehr es sind in seinem Licht.", "tokens": ["So\u00b7wohl", "als", "sie", "viel\u00b7mehr", "es", "sind", "in", "sei\u00b7nem", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Er selber ist darum das Gr\u00f6ste Allgemeinste,", "tokens": ["Er", "sel\u00b7ber", "ist", "da\u00b7rum", "das", "Gr\u00f6s\u00b7te", "All\u00b7ge\u00b7meins\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil in ihm alles ist das Einzelste, das Kleinste.", "tokens": ["Weil", "in", "ihm", "al\u00b7les", "ist", "das", "Ein\u00b7zels\u00b7te", ",", "das", "Kleins\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PIS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}}}}}