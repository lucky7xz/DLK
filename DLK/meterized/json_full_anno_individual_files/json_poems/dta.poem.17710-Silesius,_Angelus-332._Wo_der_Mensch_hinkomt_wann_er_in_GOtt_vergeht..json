{"dta.poem.17710": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "332.  Wo der Mensch hinkomt/ wann er  \n in GOtt vergeht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich in GOtt vergeh/ so kom\u0304 ich wider hin", "tokens": ["Wenn", "ich", "in", "Gott", "ver\u00b7geh", "/", "so", "kom\u0304", "ich", "wi\u00b7der", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "APPR", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ich von Ewigkeit vor mir gewesen bin.", "tokens": ["Wo", "ich", "von", "E\u00b7wig\u00b7keit", "vor", "mir", "ge\u00b7we\u00b7sen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "APPR", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}