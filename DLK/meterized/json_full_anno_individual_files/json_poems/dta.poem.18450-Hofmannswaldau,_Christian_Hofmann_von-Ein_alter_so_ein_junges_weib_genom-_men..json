{"dta.poem.18450": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ein alter/ so ein junges weib genom-  \n men.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein alter/ der nicht l\u00e4ngst ein junges weib genommen/", "tokens": ["Ein", "al\u00b7ter", "/", "der", "nicht", "l\u00e4ngst", "ein", "jun\u00b7ges", "weib", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ART", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fragt einen/ wo doch sey des menschen dickste haut?", "tokens": ["Fragt", "ei\u00b7nen", "/", "wo", "doch", "sey", "des", "men\u00b7schen", "dicks\u00b7te", "haut", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$(", "PWAV", "ADV", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-----+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Dir wird sie/ sprach ein schalck/ in deiner stirn geschaut/", "tokens": ["Dir", "wird", "sie", "/", "sprach", "ein", "schalck", "/", "in", "dei\u00b7ner", "stirn", "ge\u00b7schaut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$(", "VVFIN", "ART", "NN", "$(", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sonst h\u00e4ttest du gewi\u00df schon h\u00f6rner \u00fcberkommen.", "tokens": ["Sonst", "h\u00e4t\u00b7test", "du", "ge\u00b7wi\u00df", "schon", "h\u00f6r\u00b7ner", "\u00fc\u00b7ber\u00b7kom\u00b7men", "