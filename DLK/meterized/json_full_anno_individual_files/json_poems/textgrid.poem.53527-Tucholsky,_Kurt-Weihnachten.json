ure": "iambic.tetra"}, "line.6": {"text": "Ich summe leis, ich merk es kaum,", "tokens": ["Ich", "sum\u00b7me", "leis", ",", "ich", "merk", "es", "kaum", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "die Weise meiner Jugendtage:", "tokens": ["die", "Wei\u00b7se", "mei\u00b7ner", "Ju\u00b7gend\u00b7ta\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "O Tannebaum!", "tokens": ["O", "Tan\u00b7ne\u00b7baum", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wenn ich so der Knecht Ruprecht w\u00e4re", "tokens": ["Wenn", "ich", "so", "der", "Knecht", "Ru\u00b7precht", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "NE", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und k\u00e4m in dies Brimborium", "tokens": ["und", "k\u00e4m", "in", "dies", "Brim\u00b7bo\u00b7ri\u00b7um"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDS", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u2013 bei Deutschen fruchtet keine Lehre \u2013", "tokens": ["\u2013", "bei", "Deut\u00b7schen", "fruch\u00b7tet", "kei\u00b7ne", "Leh\u00b7re", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NE", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "wei\u00df Gott! ich kehrte wieder um.", "tokens": ["wei\u00df", "Gott", "!", "ich", "kehr\u00b7te", "wie\u00b7der", "um", "."], "token_