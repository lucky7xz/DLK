{"dta.poem.17754": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "2.  An die Jungfrau Maria/ die geheime  \n Lilie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Du Edle Lilie wer findet deines gleichen?", "tokens": ["Du", "Ed\u00b7le", "Li\u00b7lie", "wer", "fin\u00b7det", "dei\u00b7nes", "glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PWS", "VVFIN", "PPOSAT", "ADJA", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Solt\u2019 er auch alles Feld im Paradei\u00df durch streichen.", "tokens": ["Solt'", "er", "auch", "al\u00b7les", "Feld", "im", "Pa\u00b7ra\u00b7dei\u00df", "durch", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "APPRART", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du gl\u00e4ntzest wie der Schnee/ wann jhn zu sch\u00f6ner Zeit", "tokens": ["Du", "gl\u00e4nt\u00b7zest", "wie", "der", "Schnee", "/", "wann", "jhn", "zu", "sch\u00f6\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$(", "PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Himmel mit dem Gold de\u00df ", "tokens": ["Der", "Him\u00b7mel", "mit", "dem", "Gold", "de\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr dir mu\u00df Sonn und Mond un\u0304 alle Stern\u2019erbleichen.", "tokens": ["F\u00fcr", "dir", "mu\u00df", "Sonn", "und", "Mond", "un\u0304", "al\u00b7le", "Stern'\u00b7er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "NN", "KON", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}