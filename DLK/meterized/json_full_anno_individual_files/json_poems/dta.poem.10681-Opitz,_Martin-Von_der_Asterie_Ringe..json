{"dta.poem.10681": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Von der Asterie Ringe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Wie dieser Ring von Golt geschmiedet ist zusammen/", "tokens": ["Wie", "die\u00b7ser", "Ring", "von", "Golt", "ge\u00b7schmie\u00b7det", "ist", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPR", "NE", "VVPP", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie dieser edle Stein scheint gleich den Fewerflammen/", "tokens": ["Wie", "die\u00b7ser", "ed\u00b7le", "Stein", "scheint", "gleich", "den", "Fe\u00b7wer\u00b7flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ist auch dein Gem\u00fcth so hart als Golt vnd Stein/", "tokens": ["So", "ist", "auch", "dein", "Ge\u00b7m\u00fcth", "so", "hart", "als", "Golt", "vnd", "Stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "KOKOM", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd dein Gesichte scheint ein helle Fackel sein.", "tokens": ["Vnd", "dein", "Ge\u00b7sich\u00b7te", "scheint", "ein", "hel\u00b7le", "Fa\u00b7ckel", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}