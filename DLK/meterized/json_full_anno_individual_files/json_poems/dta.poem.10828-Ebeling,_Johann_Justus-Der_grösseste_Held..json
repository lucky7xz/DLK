{"dta.poem.10828": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Der gr\u00f6sseste Held.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die grossen F\u00fcrsten miteinander,", "tokens": ["Die", "gros\u00b7sen", "F\u00fcrs\u00b7ten", "mi\u00b7tein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Pompejus, C\u00e4sar, Alexander,", "tokens": ["Pom\u00b7pe\u00b7jus", ",", "C\u00e4\u00b7sar", ",", "A\u00b7lex\u00b7an\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die fast die halbe Welt bekriegt,", "tokens": ["Die", "fast", "die", "hal\u00b7be", "Welt", "be\u00b7kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Land und V\u00f6lker gnug be-", "tokens": ["Und", "Land", "und", "V\u00f6l\u00b7ker", "gnug", "be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die nennt man wunderbahre Helden,", "tokens": ["Die", "nennt", "man", "wun\u00b7der\u00b7bah\u00b7re", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie uns der Zeit Geschichte melden.", "tokens": ["Wie", "uns", "der", "Zeit", "Ge\u00b7schich\u00b7te", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Allein sieht man die Thaten an,", "tokens": ["Al\u00b7lein", "sieht", "man", "die", "Tha\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "So haben sie sonst nichts gethan,", "tokens": ["So", "ha\u00b7ben", "sie", "sonst", "nichts", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als durch die Kriegrischen Soldaten,", "tokens": ["Als", "durch", "die", "Krie\u00b7gri\u00b7schen", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.10": {"text": "Verw\u00fcstet die gebauten Staaten.", "tokens": ["Ver\u00b7w\u00fcs\u00b7tet", "die", "ge\u00b7bau\u00b7ten", "Staa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Ehre war ihr gr\u00f6\u00dftes Gut,", "tokens": ["Die", "Eh\u00b7re", "war", "ihr", "gr\u00f6\u00df\u00b7tes", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Jhr Lorbeer w\u00e4chset nur im Blut;", "tokens": ["Ihr", "Lor\u00b7beer", "w\u00e4ch\u00b7set", "nur", "im", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sie sind durch Rauben, Pl\u00fcndern, Mor-", "tokens": ["Sie", "sind", "durch", "Rau\u00b7ben", ",", "Pl\u00fcn\u00b7dern", ",", "Mor"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Durch Grausamkeit zu Helden worden.", "tokens": ["Durch", "Grau\u00b7sam\u00b7keit", "zu", "Hel\u00b7den", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Sie scheinen gros, und sind doch klein,", "tokens": ["Sie", "schei\u00b7nen", "gros", ",", "und", "sind", "doch", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KON", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Weil sie offt arme Sclaven seyn,", "tokens": ["Weil", "sie", "offt", "ar\u00b7me", "Scla\u00b7ven", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die, wenn sie sich mit Ruhm bekr\u00f6nen,", "tokens": ["Die", ",", "wenn", "sie", "sich", "mit", "Ruhm", "be\u00b7kr\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Den st\u00e4rksten Leidenschaften fr\u00f6hnen.", "tokens": ["Den", "st\u00e4rks\u00b7ten", "Lei\u00b7den\u00b7schaf\u00b7ten", "fr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Was ist es, wenn des Philipps Sohn,", "tokens": ["Was", "ist", "es", ",", "wenn", "des", "Phi\u00b7lipps", "Sohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Auf Menschen Knochen baut den Thron,", "tokens": ["Auf", "Men\u00b7schen", "Kno\u00b7chen", "baut", "den", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und da er fast die Welt besieget,", "tokens": ["Und", "da", "er", "fast", "die", "Welt", "be\u00b7sie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Als Knecht in Wollust-Ketten lieget?", "tokens": ["Als", "Knecht", "in", "Wol\u00b7lust\u00b7Ket\u00b7ten", "lie\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Der gr\u00f6\u00dfte Held heist billig der,", "tokens": ["Der", "gr\u00f6\u00df\u00b7te", "Held", "heist", "bil\u00b7lig", "der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Durch dessen M\u00fch das starke Heer", "tokens": ["Durch", "des\u00b7sen", "M\u00fch", "das", "star\u00b7ke", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Der Leidenschafften wird bezwungen,", "tokens": ["Der", "Lei\u00b7den\u00b7schaff\u00b7ten", "wird", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Die in uns selbst sind eingedrungen.", "tokens": ["Die", "in", "uns", "selbst", "sind", "ein\u00b7ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Wer sich als einen Feind bek\u00e4mpft,", "tokens": ["Wer", "sich", "als", "ei\u00b7nen", "Feind", "be\u00b7k\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und seine b\u00f6se Ichheit d\u00e4mpft;", "tokens": ["Und", "sei\u00b7ne", "b\u00f6\u00b7se", "Ich\u00b7heit", "d\u00e4mpft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wer der Begierden Joch entronnen:", "tokens": ["Wer", "der", "Be\u00b7gier\u00b7den", "Joch", "ent\u00b7ron\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Der hat in Warheit mehr gewonnen,", "tokens": ["Der", "hat", "in", "War\u00b7heit", "mehr", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Als solcher der da Land und Stadt,", "tokens": ["Als", "sol\u00b7cher", "der", "da", "Land", "und", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ART", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Durch tapfer Faust bezwungen hat.", "tokens": ["Durch", "tap\u00b7fer", "Faust", "be\u00b7zwun\u00b7gen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Es ist also ein wahrer Christe,", "tokens": ["Es", "ist", "al\u00b7so", "ein", "wah\u00b7rer", "Chris\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.34": {"text": "Der gr\u00f6\u00dfste Held der seine L\u00fcste,", "tokens": ["Der", "gr\u00f6\u00df\u00b7ste", "Held", "der", "sei\u00b7ne", "L\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "In Krafft des Glaubens unterdr\u00fckt,", "tokens": ["In", "Krafft", "des", "Glau\u00b7bens", "un\u00b7ter\u00b7dr\u00fckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und seine Leidenschafft bestrikt.", "tokens": ["Und", "sei\u00b7ne", "Lei\u00b7den\u00b7schafft", "be\u00b7strikt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Die Feinde die uns immer qu\u00e4len,", "tokens": ["Die", "Fein\u00b7de", "die", "uns", "im\u00b7mer", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Das sind die Feinde unsrer Seelen;", "tokens": ["Das", "sind", "die", "Fein\u00b7de", "uns\u00b7rer", "See\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Der Teuffel, und die b\u00f6se Welt,", "tokens": ["Der", "Teuf\u00b7fel", ",", "und", "die", "b\u00f6\u00b7se", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Und das was Fleisch und Blut gef\u00e4llt.", "tokens": ["Und", "das", "was", "Fleisch", "und", "Blut", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PRELS", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Wer durch den Glauben die bezwinget,", "tokens": ["Wer", "durch", "den", "Glau\u00b7ben", "die", "be\u00b7zwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und nach der Himmels Krone ringet,", "tokens": ["Und", "nach", "der", "Him\u00b7mels", "Kro\u00b7ne", "rin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Der \u00fcbertrifft die gr\u00f6ste Macht.", "tokens": ["Der", "\u00fc\u00b7bert\u00b7rifft", "die", "gr\u00f6s\u00b7te", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Wird der vor einen Held geacht,", "tokens": ["Wird", "der", "vor", "ei\u00b7nen", "Held", "ge\u00b7acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Der gl\u00fcklich andre \u00fcberwindet;", "tokens": ["Der", "gl\u00fck\u00b7lich", "and\u00b7re", "\u00fc\u00b7berw\u00b7in\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "So ist die Warheit fest gegr\u00fcndet:", "tokens": ["So", "ist", "die", "War\u00b7heit", "fest", "ge\u00b7gr\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Da\u00df der der gr\u00f6ste Sieger heist,", "tokens": ["Da\u00df", "der", "der", "gr\u00f6s\u00b7te", "Sie\u00b7ger", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Der sich der Sclaverei entreist.", "tokens": ["Der", "sich", "der", "Scla\u00b7ve\u00b7rei", "en\u00b7treist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Je gr\u00f6\u00dfrer Feind, je gr\u00f6\u00dfrer Sieger.", "tokens": ["Je", "gr\u00f6\u00df\u00b7rer", "Feind", ",", "je", "gr\u00f6\u00df\u00b7rer", "Sie\u00b7ger", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Drum ist ein Christ der st\u00e4rkste Krieger,", "tokens": ["Drum", "ist", "ein", "Christ", "der", "st\u00e4rks\u00b7te", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Der selbst den F\u00fcrsten aller Welt,", "tokens": ["Der", "selbst", "den", "F\u00fcrs\u00b7ten", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Den Satanas zu Boden f\u00e4llt.", "tokens": ["Den", "Sa\u00b7ta\u00b7nas", "zu", "Bo\u00b7den", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Viel leichter ist die Welt bekriegen,", "tokens": ["Viel", "leich\u00b7ter", "ist", "die", "Welt", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "Als solche Reizung zu besiegen,", "tokens": ["Als", "sol\u00b7che", "Rei\u00b7zung", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.55": {"text": "Die unsern 