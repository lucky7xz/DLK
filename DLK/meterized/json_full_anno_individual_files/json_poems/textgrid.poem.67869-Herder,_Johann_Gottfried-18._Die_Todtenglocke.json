{"textgrid.poem.67869": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "18. Die Todtenglocke", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So, Liebste, lebe wohl!", "tokens": ["So", ",", "Liebs\u00b7te", ",", "le\u00b7be", "wohl", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf ewig lebe wohl!", "tokens": ["Auf", "e\u00b7wig", "le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf immer ich dich lassen,", "tokens": ["Auf", "im\u00b7mer", "ich", "dich", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun immer weinen soll!", "tokens": ["Nun", "im\u00b7mer", "wei\u00b7nen", "soll", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Todtenglocke mit Trauerschall", "tokens": ["Die", "Tod\u00b7ten\u00b7glo\u00b7cke", "mit", "Trau\u00b7er\u00b7schall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ruft: sie ist todt! sie ist nun todt!", "tokens": ["Ruft", ":", "sie", "ist", "todt", "!", "sie", "ist", "nun", "todt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VAFIN", "ADJD", "$.", "P