{"dta.poem.19139": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "CxVII.  Auf der Gr\u00e4fin 33ten Geburts-  \n Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr uns verwundes Lamm!", "tokens": ["F\u00fcr", "uns", "ver\u00b7wun\u00b7des", "Lamm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit keines Menschen Zungen,", "tokens": ["Mit", "kei\u00b7nes", "Men\u00b7schen", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach W\u00fcrdigkeit besungen:", "tokens": ["Nach", "W\u00fcr\u00b7dig\u00b7keit", "be\u00b7sun\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil sich der Adern Schlamm", "tokens": ["Weil", "sich", "der", "A\u00b7dern", "Schlamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch in die ", "tokens": ["Noch", "in", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "Und in den Gliedern zischet,", "tokens": ["Und", "in", "den", "Glie\u00b7dern", "zi\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die wie ein todter Zahn", "tokens": ["Die", "wie", "ein", "tod\u00b7ter", "Zahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Doch noch nicht abgethan.", "tokens": ["Doch", "noch", "nicht", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie w\u00e4rs, man schwiege gar,", "tokens": ["Wie", "w\u00e4rs", ",", "man", "schwie\u00b7ge", "gar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und lie\u00df\u2019 vors Geistes wittern,", "tokens": ["Und", "lie\u00df'", "vors", "Geis\u00b7tes", "wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis auf das kleinste ", "tokens": ["Bis", "auf", "das", "kleins\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und d\u00e4chten: ", "tokens": ["Und", "d\u00e4ch\u00b7ten", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Wo bliebe denn der Mund?", "tokens": ["Wo", "blie\u00b7be", "denn", "der", "Mund", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wer kan die Liebe kennen,", "tokens": ["Wer", "kan", "die", "Lie\u00b7be", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sie nicht Liebe nennen?", "tokens": ["Und", "sie", "nicht", "Lie\u00b7be", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du treuer F\u00fcrst vom Bund!", "tokens": ["Du", "treu\u00b7er", "F\u00fcrst", "vom", "Bund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie solten deine Zeugen,", "tokens": ["Wie", "sol\u00b7ten", "dei\u00b7ne", "Zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Bundes-Blute schweigen?", "tokens": ["Vom", "Bun\u00b7des\u00b7Blu\u00b7te", "schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herr! hier ist eine Magd", "tokens": ["Herr", "!", "hier", "ist", "ei\u00b7ne", "Magd"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VAFIN", "ART", "NN"], "meter": "+-++-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Von einem solchen Stande,", "tokens": ["Von", "ei\u00b7nem", "sol\u00b7chen", "Stan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der in dem Vater-Lande,", "tokens": ["Der", "in", "dem", "Va\u00b7ter\u00b7Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar selten viel besagt,", "tokens": ["Gar", "sel\u00b7ten", "viel", "be\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und dem der Geist in Schrifften,", "tokens": ["Und", "dem", "der", "Geist", "in", "Schriff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein Ehren-Mahl will stifften,", "tokens": ["Kein", "Eh\u00b7ren\u00b7Mahl", "will", "stiff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Geist, der Zweifels frey,", "tokens": ["Der", "Geist", ",", "der", "Zwei\u00b7fels", "frey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wei\u00df, was im Menschen sey.", "tokens": ["Wei\u00df", ",", "was", "im", "Men\u00b7schen", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Ja, Vater, du hast recht!Matth. 11, 25.", "tokens": ["Ja", ",", "Va\u00b7ter", ",", "du", "hast", "recht", "!", "Mat\u00b7th", ".", "11", ",", "25."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "number", "punct", "ordinal"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VAFIN", "ADJD", "$.", "NE", "$.", "CARD", "$,", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rufft dort die weise Liebe", "tokens": ["Rufft", "dort", "die", "wei\u00b7se", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Hoheit viel zu schlecht.", "tokens": ["Die", "Ho\u00b7heit", "viel", "zu", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Paul: ", "tokens": ["Und", "Paul", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NE", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Wo sind die Edle blieben?1. Cor. 1, 20.", "tokens": ["Wo", "sind", "die", "Ed\u00b7le", "blie\u00b7ben", "?", "1.", "Cor", ".", "1", ",", "20."], "token_info": ["word", "word", "word", "word", "word", "punct", "ordinal", "word", "punct", "number", "punct", "ordinal"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "VVFIN", "$.", "ADJA", "NN", "$.", "CARD", "$,", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein andrer Zeuge spricht:", "tokens": ["Ein", "an\u00b7drer", "Zeu\u00b7ge", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zwar redet auch die Schrifft,", "tokens": ["Zwar", "re\u00b7det", "auch", "die", "Schrifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von etlichen der Fetten,", "tokens": ["Von", "et\u00b7li\u00b7chen", "der", "Fet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die ihre Seele retten:", "tokens": ["Die", "ih\u00b7re", "See\u00b7le", "ret\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Theils grosse, die es trifft,", "tokens": ["Theils", "gros\u00b7se", ",", "die", "es", "trifft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sind Ammen und sind Pfleger,", "tokens": ["Sind", "Am\u00b7men", "und", "sind", "Pfle\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und GOttes letzte J\u00e4ger,", "tokens": ["Und", "Got\u00b7tes", "letz\u00b7te", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Theils bringen ihre Macht", "tokens": ["Theils", "brin\u00b7gen", "ih\u00b7re", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mit in die Salems Pracht.", "tokens": ["Mit", "in", "die", "Sa\u00b7lems", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Allein wo siehet man", "tokens": ["Al\u00b7lein", "wo", "sie\u00b7het", "man"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PWAV", "VVFIN", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Grossen, die sich lieber,", "tokens": ["Die", "Gros\u00b7sen", ",", "die", "sich", "lie\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit jenem Schmertzes-Mann,", "tokens": ["Mit", "je\u00b7nem", "Schmert\u00b7zes\u00b7Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In seine Leyden wagen,", "tokens": ["In", "sei\u00b7ne", "Ley\u00b7den", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als Ehren-Zeichen tragen,", "tokens": ["Als", "Eh\u00b7ren\u00b7Zei\u00b7chen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und lieber arm und klein,", "tokens": ["Und", "lie\u00b7ber", "arm", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als reich und m\u00e4chtig seyn.", "tokens": ["Als", "reich", "und", "m\u00e4ch\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wo ist der K\u00f6nig hin,", "tokens": ["Wo", "ist", "der", "K\u00f6\u00b7nig", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der vor des Bundes-Lade,", "tokens": ["Der", "vor", "des", "Bun\u00b7des\u00b7La\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit aufgebrachtem Pfade,", "tokens": ["Mit", "auf\u00b7ge\u00b7brach\u00b7tem", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eingekehrten Sinn,", "tokens": ["Und", "ein\u00b7ge\u00b7kehr\u00b7ten", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Chor der M\u00e4gde f\u00fchrte,", "tokens": ["Das", "Chor", "der", "M\u00e4g\u00b7de", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und seine Harffe r\u00fchrte,", "tokens": ["Und", "sei\u00b7ne", "Harf\u00b7fe", "r\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und der bey aller Schmach,", "tokens": ["Und", "der", "bey", "al\u00b7ler", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "PIAT", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Von nichts als Ehre sprach.", "tokens": ["Von", "nichts", "als", "Eh\u00b7re", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Seht doch das K\u00f6nigs-Weib,", "tokens": ["Seht", "doch", "das", "K\u00f6\u00b7nigs\u00b7Weib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Bedeckt mit Berlen-St\u00fccken", "tokens": ["Be\u00b7deckt", "mit", "Ber\u00b7len\u00b7St\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sich auf den Boden b\u00fccken!", "tokens": ["Sich", "auf", "den", "Bo\u00b7den", "b\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Purpur ziert den Leib,", "tokens": ["Der", "Pur\u00b7pur", "ziert", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Sinn ist allewege", "tokens": ["Der", "Sinn", "ist", "al\u00b7le\u00b7we\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und die den Ochus ", "tokens": ["Und", "die", "den", "O\u00b7chus"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Bleibt Mardachai ", "tokens": ["Bleibt", "Mar\u00b7dac\u00b7hai"], "token_info": ["word", "word"], "pos": ["VVFIN", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "R\u00fcckt unserm Geiste n\u00e4her!", "tokens": ["R\u00fcckt", "un\u00b7serm", "Geis\u00b7te", "n\u00e4\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und du ", "tokens": ["Und", "du"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Am Wasser Hideckel!",