{"dta.poem.6559": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indessen mu\u00df man doch gestehn,", "tokens": ["In\u00b7des\u00b7sen", "mu\u00df", "man", "doch", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Schildereyen, Bl\u00fcht\u2019 und eine Lufft, die sch\u00f6n,", "tokens": ["Da\u00df", "Schil\u00b7de\u00b7re\u00b7yen", ",", "Bl\u00fcht'", "und", "ei\u00b7ne", "Lufft", ",", "die", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "VVFIN", "KON", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja endlich alle Ding geschickt sind zu erregen", "tokens": ["Ja", "end\u00b7lich", "al\u00b7le", "Ding", "ge\u00b7schickt", "sind", "zu", "er\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "PIAT", "NN", "VVPP", "VAFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das unterschiedliche Bewegen,", "tokens": ["Das", "un\u00b7ter\u00b7schied\u00b7li\u00b7che", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Woraus bey uns die Farb\u2019 entspriesset. Aus dem Licht,", "tokens": ["Wo\u00b7raus", "bey", "uns", "die", "Fa\u00b7rb'", "ent\u00b7spries\u00b7set", ".", "Aus", "dem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPER", "ART", "NN", "VVFIN", "$.", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Das auf den Fl\u00e4chen sich ver\u00e4ndert, theilt und bricht,", "tokens": ["Das", "auf", "den", "Fl\u00e4\u00b7chen", "sich", "ver\u00b7\u00e4n\u00b7dert", ",", "theilt", "und", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "PRF", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Entspringen und entstehen", "tokens": ["Ent\u00b7sprin\u00b7gen", "und", "ent\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Farben Meng\u2019 und Zahl, durch welche wir sie sehen.", "tokens": ["Der", "Far\u00b7ben", "Meng'", "und", "Zahl", ",", "durch", "wel\u00b7che", "wir", "sie", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$,", "APPR", "PRELS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Striche, blos durchs Licht ver\u00e4ndert, die ich schau,", "tokens": ["Die", "Stri\u00b7che", ",", "blos", "durchs", "Licht", "ver\u00b7\u00e4n\u00b7dert", ",", "die", "ich", "schau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,", "PRELS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Formiren ", "tokens": ["For\u00b7mi\u00b7ren"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "und blau.", "tokens": ["und", "blau", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Ver\u00e4ndern sich die Fl\u00e4chen; werden schnell", "tokens": ["Ver\u00b7\u00e4n\u00b7dern", "sich", "die", "Fl\u00e4\u00b7chen", ";", "wer\u00b7den", "schnell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "$.", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Die Farben alsobald mehr dunckel, und mehr hell.", "tokens": ["Die", "Far\u00b7ben", "al\u00b7so\u00b7bald", "mehr", "dun\u00b7ckel", ",", "und", "mehr", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bald sind sie durch den Druck der Lufft zertrennet,", "tokens": ["Bald", "sind", "sie", "durch", "den", "Druck", "der", "Lufft", "zer\u00b7tren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Durchs Feuer sind sie auch zerst\u00f6hret und verbrennet.", "tokens": ["Durchs", "Feu\u00b7er", "sind", "sie", "auch", "zer\u00b7st\u00f6h\u00b7ret", "und", "ver\u00b7bren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}