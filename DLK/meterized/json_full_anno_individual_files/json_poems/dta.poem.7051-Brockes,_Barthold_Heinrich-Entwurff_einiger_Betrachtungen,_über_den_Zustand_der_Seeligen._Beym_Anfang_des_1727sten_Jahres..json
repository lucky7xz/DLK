{"dta.poem.7051": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sich dieses himmlische Revier", "tokens": ["Sich", "die\u00b7ses", "himm\u00b7li\u00b7sche", "Re\u00b7vier"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Diamant, Rubin, Saphier,", "tokens": ["Von", "Di\u00b7a\u00b7mant", ",", "Ru\u00b7bin", ",", "Sa\u00b7phier", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NE", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Smaragd, und Perlen vorzustellen,", "tokens": ["Sma\u00b7ragd", ",", "und", "Per\u00b7len", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist etwas zwar; Allein,", "tokens": ["Ist", "et\u00b7was", "zwar", ";", "Al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$.", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn von der edlen Steine Schein,", "tokens": ["Wenn", "von", "der", "ed\u00b7len", "Stei\u00b7ne", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihres Schimmers Glantz, wir recht ein Urthel f\u00e4llen;", "tokens": ["Und", "ih\u00b7res", "Schim\u00b7mers", "Glantz", ",", "wir", "recht", "ein", "Ur\u00b7thel", "f\u00e4l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So ist die gantze Pracht ein blosser Widerschlag", "tokens": ["So", "ist", "die", "gant\u00b7ze", "Pracht", "ein", "blos\u00b7ser", "Wi\u00b7der\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vom Sonnen-Licht,", "tokens": ["Vom", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Das blo\u00df durch das Gesicht,", "tokens": ["Das", "blo\u00df", "durch", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wann man es in gewisser Maasse sp\u00fcret,", "tokens": ["Wann", "man", "es", "in", "ge\u00b7wis\u00b7ser", "Maas\u00b7se", "sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dem, der es selten sieht, das Hertze gleichsam r\u00fchret.", "tokens": ["Dem", ",", "der", "es", "sel\u00b7ten", "sieht", ",", "das", "Hert\u00b7ze", "gleich\u00b7sam", "r\u00fch\u00b7ret", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "PDS", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}