{"dta.poem.4092": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Pause.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hab' sie umschlungen mit einem gr\u00fcnen Band \u2014", "tokens": ["Hab'", "sie", "um\u00b7schlun\u00b7gen", "mit", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Band"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich kann nicht mehr singen, mein Herz ist zu voll,", "tokens": ["Ich", "kann", "nicht", "mehr", "sin\u00b7gen", ",", "mein", "Herz", "ist", "zu", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,", "PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wei\u00df nicht, wie ich's in Reime zwingen soll.", "tokens": ["Wei\u00df", "nicht", ",", "wie", "ich's", "in", "Rei\u00b7me", "zwin\u00b7gen", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "PIS", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Meiner Sehnsucht allerhei\u00dfesten Schmerz", "tokens": ["Mei\u00b7ner", "Sehn\u00b7sucht", "al\u00b7ler\u00b7hei\u00b7\u00dfes\u00b7ten", "Schmerz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Durft' ich aushauchen in Liederscherz,", "tokens": ["Durft'", "ich", "aus\u00b7hau\u00b7chen", "in", "Lie\u00b7der\u00b7scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Und wie ich klagte so s\u00fc\u00df und fein,", "tokens": ["Und", "wie", "ich", "klag\u00b7te", "so", "s\u00fc\u00df", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Meint' ich doch, mein Leiden w\u00e4r' nicht klein:", "tokens": ["Meint'", "ich", "doch", ",", "mein", "Lei\u00b7den", "w\u00e4r'", "nicht", "klein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ei, wie gro\u00df ist wohl meines Gl\u00fcckes Last,", "tokens": ["Ei", ",", "wie", "gro\u00df", "ist", "wohl", "mei\u00b7nes", "Gl\u00fc\u00b7ckes", "Last", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "NE", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Da\u00df kein Klang auf Erden es in sich fa\u00dft?", "tokens": ["Da\u00df", "kein", "Klang", "auf", "Er\u00b7den", "es", "in", "sich", "fa\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "NN", "PPER", "APPR", "PRF", "VVFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Nun, liebe Laute, ruh' an dem Nagel hier!", "tokens": ["Nun", ",", "lie\u00b7be", "Lau\u00b7te", ",", "ruh'", "an", "dem", "Na\u00b7gel", "hier", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NE", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und weht ein L\u00fcftchen \u00fcber die Saiten dir,", "tokens": ["Und", "weht", "ein", "L\u00fcft\u00b7chen", "\u00fc\u00b7ber", "die", "Sai\u00b7ten", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und streift eine Biene mit ihren Fl\u00fcgeln dich,", "tokens": ["Und", "streift", "ei\u00b7ne", "Bie\u00b7ne", "mit", "ih\u00b7ren", "Fl\u00fc\u00b7geln", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da wird mir bange und es durchschauert mich.", "tokens": ["Da", "wird", "mir", "ban\u00b7ge", "und", "es", "durch\u00b7schau\u00b7ert", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Warum lie\u00df ich das Band auch h\u00e4ngen so lang?", "tokens": ["Wa\u00b7rum", "lie\u00df", "ich", "das", "Band", "auch", "h\u00e4n\u00b7gen", "so", "lang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ADV", "VVFIN", "ADV", "ADJD", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Oft fliegt's um die Saiten mit seufzendem Klang.", "tokens": ["Oft", "fliegt's", "um", "die", "Sai\u00b7ten", "mit", "seuf\u00b7zen\u00b7dem", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Ist es der Nachklang meiner Liebespein?", "tokens": ["Ist", "es", "der", "Nach\u00b7klang", "mei\u00b7ner", "Lie\u00b7be\u00b7spein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Soll es das Vorspiel neuer Lieder sein?", "tokens": ["Soll", "es", "das", "Vor\u00b7spiel", "neu\u00b7er", "Lie\u00b7der", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}