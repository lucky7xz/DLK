{"textgrid.poem.63863": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ist die Heimat wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist die Heimat wieder,", "tokens": ["Das", "ist", "die", "Hei\u00b7mat", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch hohe Gartenwipfel", "tokens": ["Durch", "ho\u00b7he", "Gar\u00b7ten\u00b7wip\u00b7fel"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Winkt mir Willkommen das alte Haus.", "tokens": ["Winkt", "mir", "Will\u00b7kom\u00b7men", "das", "al\u00b7te", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie sch\u00fcchtern erst", "tokens": ["Wie", "sch\u00fcch\u00b7tern", "erst"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ist aufgesprossen an allen Zweigen", "tokens": ["Ist", "auf\u00b7ge\u00b7spros\u00b7sen", "an", "al\u00b7len", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Maigr\u00fcnes Laub im herben Hauch", "tokens": ["Mai\u00b7gr\u00fc\u00b7nes", "Laub", "im", "her\u00b7ben", "Hauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des deutschen Lenzmonds!", "tokens": ["Des", "deut\u00b7schen", "Lenz\u00b7monds", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Nirgends gl\u00fcht mir entgegen,", "tokens": ["Nir\u00b7gends", "gl\u00fcht", "mir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Wie dort zu tausenden,", "tokens": ["Wie", "dort", "zu", "tau\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Nur Eine Rose,", "tokens": ["Nur", "Ei\u00b7ne", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Maasliebchen allein im Wiesengrund", "tokens": ["Maas\u00b7lieb\u00b7chen", "al\u00b7lein", "im", "Wie\u00b7sen\u00b7grund"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und zart entfaltet am Strauch", "tokens": ["Und", "zart", "ent\u00b7fal\u00b7tet", "am", "Strauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Der P\u00e4onie Knospen.", "tokens": ["Der", "P\u00e4o\u00b7nie", "Knos\u00b7pen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Doch trauen sie nicht der tr\u00fcglichen Sonne,", "tokens": ["Doch", "trau\u00b7en", "sie", "nicht", "der", "tr\u00fcg\u00b7li\u00b7chen", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Die lockend nieder\u00e4ugelt,", "tokens": ["Die", "lo\u00b7ckend", "nie\u00b7de\u00b7r\u00e4u\u00b7gelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Denn gestern erst, so h\u00f6rt' ich,", "tokens": ["Denn", "ge\u00b7stern", "erst", ",", "so", "h\u00f6rt'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.17": {"text": "Hat's wieder einmal", "tokens": ["Hat's", "wie\u00b7der", "ein\u00b7mal"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Geschneit in den M\u00fcnchner Fr\u00fchling.", "tokens": ["Ge\u00b7schneit", "in", "den", "M\u00fcnch\u00b7ner", "Fr\u00fch\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Beklommnen Herzens", "tokens": ["Be\u00b7klomm\u00b7nen", "Her\u00b7zens"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "\u00dcberschreit' ich die Schwelle.", "tokens": ["\u00dc\u00b7ber\u00b7schreit'", "ich", "die", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Werd' ich, verw\u00f6hnt", "tokens": ["Werd'", "ich", ",", "ver\u00b7w\u00f6hnt"], "token_info": ["word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "$,", "VVPP"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Durch s\u00fcdliche Sonnenglut,", "tokens": ["Durch", "s\u00fcd\u00b7li\u00b7che", "Son\u00b7nen\u00b7glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "An dieser k\u00fchlen Heimat", "tokens": ["An", "die\u00b7ser", "k\u00fch\u00b7len", "Hei\u00b7mat"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Blasses Licht mich zur\u00fcckgew\u00f6hnen?", "tokens": ["Blas\u00b7ses", "Licht", "mich", "zu\u00b7r\u00fcck\u00b7ge\u00b7w\u00f6h\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Doch da breitet am Fu\u00df der Treppe", "tokens": ["Doch", "da", "brei\u00b7tet", "am", "Fu\u00df", "der", "Trep\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Der Adorante die schlanken Arme", "tokens": ["Der", "A\u00b7do\u00b7ran\u00b7te", "die", "schlan\u00b7ken", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Aus, wie betend zur Gottheit,", "tokens": ["Aus", ",", "wie", "be\u00b7tend", "zur", "Got\u00b7theit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PWAV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df mein Eintritt gesegnet sei,", "tokens": ["Da\u00df", "mein", "Ein\u00b7tritt", "ge\u00b7seg\u00b7net", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und wie empor ich steige,", "tokens": ["Und", "wie", "em\u00b7por", "ich", "stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Begr\u00fc\u00dft mich eine vertraute Schaar,", "tokens": ["Be\u00b7gr\u00fc\u00dft", "mich", "ei\u00b7ne", "ver\u00b7trau\u00b7te", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Die meiner geharrt im langen Winter:", "tokens": ["Die", "mei\u00b7ner", "ge\u00b7harrt", "im", "lan\u00b7gen", "Win\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Erinnerungen, tr\u00fcb' und frohe,", "tokens": ["E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ",", "tr\u00fcb'", "und", "fro\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Hier angesiedelt in jedem Raum,", "tokens": ["Hier", "an\u00b7ge\u00b7sie\u00b7delt", "in", "je\u00b7dem", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Wo ich gelebt, geliebt, gelitten,", "tokens": ["Wo", "ich", "ge\u00b7lebt", ",", "ge\u00b7liebt", ",", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Jung gewesen und alt geworden", "tokens": ["Jung", "ge\u00b7we\u00b7sen", "und", "alt", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAPP", "KON", "ADJD", "VAPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Und mein redlich Herz", "tokens": ["Und", "mein", "red\u00b7lich", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "In festen H\u00e4nden gehalten.", "tokens": ["In", "fes\u00b7ten", "H\u00e4n\u00b7den", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Und in mir ruft's: Sei froh der Heimkehr!", "tokens": ["Und", "in", "mir", "ruft's", ":", "Sei", "froh", "der", "Heim\u00b7kehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$.", "VAIMP", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Hier bist du zu Haus und drunten", "tokens": ["Hier", "bist", "du", "zu", "Haus", "und", "drun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "An deinem See nur zu Gast.", "tokens": ["An", "dei\u00b7nem", "See", "nur", "zu", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Denn deines Wesens tiefste Wurzeln", "tokens": ["Denn", "dei\u00b7nes", "We\u00b7sens", "tiefs\u00b7te", "Wur\u00b7zeln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Sind z\u00e4h gesenkt in die deutsche Erde,", "tokens": ["Sind", "z\u00e4h", "ge\u00b7senkt", "in", "die", "deut\u00b7sche", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Wenn auch der Wipfel sich gern", "tokens": ["Wenn", "auch", "der", "Wip\u00b7fel", "sich", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.26": {"text": "In italischen L\u00fcften wiegt.", "tokens": ["In", "i\u00b7ta\u00b7li\u00b7schen", "L\u00fcf\u00b7ten", "wiegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Morgen aber,", "tokens": ["Mor\u00b7gen", "a\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wenn du im alten Bett zu Nacht", "tokens": ["Wenn", "du", "im", "al\u00b7ten", "Bett", "zu", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geruht und Amselgesang", "tokens": ["Ge\u00b7ruht", "und", "Am\u00b7sel\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Fr\u00fch dich lieblich ermuntert,", "tokens": ["Fr\u00fch", "dich", "lieb\u00b7lich", "er\u00b7mun\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Gehst du durch wohlbekannte", "tokens": ["Gehst", "du", "durch", "wohl\u00b7be\u00b7kann\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gassen und Pl\u00e4tze,", "tokens": ["Gas\u00b7sen", "und", "Pl\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Nicht wie da unten freilich", "tokens": ["Nicht", "wie", "da", "un\u00b7ten", "frei\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOKOM", "ADV", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von Lorbeerhecken durchduftet,", "tokens": ["Von", "Lor\u00b7beer\u00b7he\u00b7cken", "durch\u00b7duf\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Doch zum Ersatz daf\u00fcr", "tokens": ["Doch", "zum", "Er\u00b7satz", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Begegnet dir hin und wieder", "tokens": ["Be\u00b7geg\u00b7net", "dir", "hin", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Ein freundlich gr\u00fc\u00dfend Gesicht,", "tokens": ["Ein", "freund\u00b7lich", "gr\u00fc\u00b7\u00dfend", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Und Mancher stellt dich und dr\u00fcckt dir die Hand:", "tokens": ["Und", "Man\u00b7cher", "stellt", "dich", "und", "dr\u00fcckt", "dir", "die", "Hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Gr\u00fc\u00df Gott! Bist gl\u00fccklich zur\u00fcckgekehrt?", "tokens": ["Gr\u00fc\u00df", "Gott", "!", "Bist", "gl\u00fcck\u00b7lich", "zu\u00b7r\u00fcck\u00b7ge\u00b7kehrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$.", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Wir haben dich so lange vermi\u00dft.", "tokens": ["Wir", "ha\u00b7ben", "dich", "so", "lan\u00b7ge", "ver\u00b7mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Nun, hoff' ich, bleibst du ein Weilchen hier!", "tokens": ["Nun", ",", "hoff'", "ich", ",", "bleibst", "du", "ein", "Weil\u00b7chen", "hier", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ja, alte Freunde!", "tokens": ["Ja", ",", "al\u00b7te", "Freun\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$."], "meter": "-+-+-", "measu