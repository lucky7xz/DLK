"ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Aus, wie betend zur Gottheit,", "tokens": ["Aus", ",", "wie", "be\u00b7tend", "zur", "Got\u00b7theit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PWAV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df mein Eintritt gesegnet sei,", "tokens": ["Da\u00df", "mein", "Ein\u00b7tritt", "ge\u00b7seg\u00b7net", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und wie empor ich steige,", "tokens": ["Und", "wie", "em\u00b7por", "ich", "stei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Begr\u00fc\u00dft mich eine vertraute Schaar,", "tokens": ["Be\u00b7gr\u00fc\u00dft", "mich", "ei\u00b7ne", "ver\u00b7trau\u00b7te", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Die meiner geharrt im langen Winter:", "tokens": ["Die", "mei\u00b7ner", "ge\u00b7harrt", "im", "lan\u00b7gen", "Win\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Erinnerungen, tr\u00fcb' und frohe,", "tokens": ["E\u00b7rin\u00b7ne\u00b7run\u00b7gen", ",", "tr\u00fcb'", "und", "fro\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Hier angesiedelt in jedem Raum,", "tokens": ["Hier", "an\u00b7ge\u00b7sie\u00b7delt", "in", "je\u00b7dem", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Wo ich gelebt, geliebt, gelitten,", "tokens": ["Wo", "ich", "ge\u00b7lebt", ",", "ge\u00b7liebt", ",", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Jung gewesen und alt geworden", "tokens": ["Jung", "ge\u00b7we\u00b7sen", "und", "alt", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAPP", "KON", "ADJD", "VAPP"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "Und mein redlich Herz", "tokens": ["Und", "mein", "red\u00b7lich", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "In festen H\u00e4nden gehalten.", "tokens": ["In", "fes\u00b7ten", "H\u00e4n\u00b7den", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.20": {"text": "Und in mir ruft's: Sei froh der Heimkehr!", "tokens": ["Und", "in", "mir", "ruft's", ":", "Sei", "froh", "der", "Heim\u00b7kehr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$.", "VAIMP", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Hier bist du zu Haus und drunten", "tokens": ["Hier", "bist", "du", "zu", "Haus", "und", "drun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "KON", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "An deinem See nur zu Gast.", "tokens": ["An", "dei\u00b7nem", "See", "nur", "zu", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Denn deines Wesens tiefste Wurzeln", "tokens": ["Denn", "dei\u00b7nes", "We\u00b7sens", "tiefs\u00b7te", "Wur\u00b7zeln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Sind z\u00e4h gesenkt in die deutsche Erde,", "tokens": ["Sind", "z\u00e4h", "ge\u00b7senkt", "in", "die", "deut\u00b7sche", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Wenn auch der Wipfel sich gern", "tokens": ["Wenn", "auch", "der", "Wip\u00b7fel", "sich", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.26": {"text": "In italischen L\u00fcften wiegt.", "tokens": ["In", "i\u00b7ta\u00b7li\u00b7schen", "L\u00fcf\u00b7ten", "wiegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Morgen aber,", "tokens": ["Mor\u00b7gen", "a\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": 