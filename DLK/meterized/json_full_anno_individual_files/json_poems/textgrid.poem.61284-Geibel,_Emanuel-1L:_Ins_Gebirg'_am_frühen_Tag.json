{"textgrid.poem.61284": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ins Gebirg' am fr\u00fchen Tag", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ins Gebirg' am fr\u00fchen Tag", "tokens": ["Ins", "Ge\u00b7bir\u00b7g'", "am", "fr\u00fc\u00b7hen", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Schritt ich aus des Weidmanns H\u00fctte,", "tokens": ["Schritt", "ich", "aus", "des", "Weid\u00b7manns", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der Freund auf seiner Sch\u00fctte", "tokens": ["Wo", "der", "Freund", "auf", "sei\u00b7ner", "Sch\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch in tiefem Schlummer lag.", "tokens": ["Noch", "in", "tie\u00b7fem", "Schlum\u00b7mer", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ich dacht' im Morgenrot:", "tokens": ["Und", "ich", "dacht'", "im", "Mor\u00b7gen\u00b7rot", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruht dem Schlaf anheimgegeben", "tokens": ["Ruht", "dem", "Schlaf", "an\u00b7heim\u00b7ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er nicht lebend ohne Leben?", "tokens": ["Er", "nicht", "le\u00b7bend", "oh\u00b7ne", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht ein Toter ohne Tod?", "tokens": ["Nicht", "ein", "To\u00b7ter", "oh\u00b7ne", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liegt vom ird'schen Druck besiegt", "tokens": ["Liegt", "vom", "ird'\u00b7schen", "Druck", "be\u00b7siegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Willenlos nicht hier die H\u00fclle,", "tokens": ["Wil\u00b7len\u00b7los", "nicht", "hier", "die", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend halbgel\u00f6st die F\u00fclle", "tokens": ["W\u00e4h\u00b7rend", "halb\u00b7ge\u00b7l\u00f6st", "die", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Geists im All sich wiegt?", "tokens": ["Sei\u00b7nes", "Geists", "im", "All", "sich", "wiegt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dennoch braucht's nur meiner Hand", "tokens": ["Den\u00b7noch", "braucht's", "nur", "mei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Druck, und rasch vereinet", "tokens": ["Ei\u00b7nen", "Druck", ",", "und", "rasch", "ver\u00b7ei\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kn\u00fcpft sich, was so locker scheinet,", "tokens": ["Kn\u00fcpft", "sich", ",", "was", "so", "lo\u00b7cker", "schei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen Geist und Leib das Band.", "tokens": ["Zwi\u00b7schen", "Geist", "und", "Leib", "das", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Der erloschne Blick wird gl\u00fchn,", "tokens": ["Der", "er\u00b7lo\u00b7schne", "Blick", "wird", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zucken wird der Muskeln jede,", "tokens": ["Zu\u00b7cken", "wird", "der", "Mus\u00b7keln", "je\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Geist in holder Rede", "tokens": ["Und", "der", "Geist", "in", "hol\u00b7der", "Re\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den stummen Lippen spr\u00fchn.", "tokens": ["Von", "den", "stum\u00b7men", "Lip\u00b7pen", "spr\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In dies Wunder noch versenkt,", "tokens": ["In", "dies", "Wun\u00b7der", "noch", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Trat ich in die Nacht der Eichen,", "tokens": ["Trat", "ich", "in", "die", "Nacht", "der", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, sich wipfelnd, mit den reichen", "tokens": ["Die", ",", "sich", "wip\u00b7felnd", ",", "mit", "den", "rei\u00b7chen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "PRF", "VVPP", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schatten rings den See beschr\u00e4nkt.", "tokens": ["Schat\u00b7ten", "rings", "den", "See", "be\u00b7schr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Horch, da weht' es, horch, da ging", "tokens": ["Horch", ",", "da", "weht'", "es", ",", "horch", ",", "da", "ging"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "$,", "VVIMP", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leis Ger\u00e4usch im Gr\u00fcn des Haines,", "tokens": ["Leis", "Ge\u00b7r\u00e4usch", "im", "Gr\u00fcn", "des", "Hai\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fast als w\u00e4r's das Atmen eines,", "tokens": ["Fast", "als", "w\u00e4r's", "das", "At\u00b7men", "ei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "VAFIN", "ART", "NN", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welchen tiefer Schlaf befing.", "tokens": ["Wel\u00b7chen", "tie\u00b7fer", "Schlaf", "be\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Seltsam sah der See mich an,", "tokens": ["Selt\u00b7sam", "sah", "der", "See", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein stummes Auge schmachtet,", "tokens": ["Wie", "ein", "stum\u00b7mes", "Au\u00b7ge", "schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn das kranke Haupt umnachtet", "tokens": ["Wenn", "das", "kran\u00b7ke", "Haupt", "um\u00b7nach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Todverwandter Starrheit Bann.", "tokens": ["Tod\u00b7ver\u00b7wand\u00b7ter", "Star\u00b7rheit", "Bann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und durch Blume, Laub und Strauch", "tokens": ["Und", "durch", "Blu\u00b7me", ",", "Laub", "und", "Strauch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Wob es leise hin und wieder,", "tokens": ["Wob", "es", "lei\u00b7se", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie durch traumgebannte Glieder", "tokens": ["Wie", "durch", "traum\u00b7ge\u00b7bann\u00b7te", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein verlorner Seelenhauch.", "tokens": ["Ein", "ver\u00b7lor\u00b7ner", "See\u00b7len\u00b7hauch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ja, ich sp\u00fcrt' im Waldrevier,", "tokens": ["Ja", ",", "ich", "sp\u00fcrt'", "im", "Wald\u00b7re\u00b7vier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Flut ein ahnend Beben -", "tokens": ["In", "der", "Flut", "ein", "ah\u00b7nend", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "ADJD", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hier auch Leben sonder Leben,", "tokens": ["Hier", "auch", "Le\u00b7ben", "son\u00b7der", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tod, doch sonder Tod auch hier.", "tokens": ["Tod", ",", "doch", "son\u00b7der", "Tod", "auch", "hier", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und mir ward es: ", "tokens": ["Und", "mir", "ward", "es", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Aus dem Traum in dunkeln Weisen", "tokens": ["Aus", "dem", "Traum", "in", "dun\u00b7keln", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Redet ihre Sehnsucht nur.", "tokens": ["Re\u00b7det", "ih\u00b7re", "Sehn\u00b7sucht", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Aber einst erscheint der Tag,", "tokens": ["A\u00b7ber", "einst", "er\u00b7scheint", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das Wunder sich entdecket,", "tokens": ["Wo", "das", "Wun\u00b7der", "sich", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Herr zur Sprache wecket,", "tokens": ["Und", "der", "Herr", "zur", "Spra\u00b7che", "we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was in stummen Banden lag.", "tokens": ["Was", "in", "stum\u00b7men", "Ban\u00b7den", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "In das Starre wunderbar", "tokens": ["In", "das", "Star\u00b7re", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird der Geist sich dann ergie\u00dfen", "tokens": ["Wird", "der", "Geist", "sich", "dann", "er\u00b7gie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und lebendig Leben flie\u00dfen,", "tokens": ["Und", "le\u00b7ben\u00b7dig", "Le\u00b7ben", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo nur Bild und Zeichen war.", "tokens": ["Wo", "nur", "Bild", "und", "Zei\u00b7chen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KON", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Heilig Feuer mu\u00df mit Macht", "tokens": ["Hei\u00b7lig", "Feu\u00b7er", "mu\u00df", "mit", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den besiegten Stoff durchleuchten;", "tokens": ["Den", "be\u00b7sieg\u00b7ten", "Stoff", "durch\u00b7leuch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Milde Seele gl\u00fcht im Feuchten,", "tokens": ["Mil\u00b7de", "See\u00b7le", "gl\u00fcht", "im", "Feuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ros'ge D\u00e4mmrung wird die Nacht.", "tokens": ["Ros'\u00b7ge", "D\u00e4mm\u00b7rung", "wird", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Und was dumpfverworren klang,", "tokens": ["Und", "was", "dum\u00b7pf\u00b7ver\u00b7wor\u00b7ren", "klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJA", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie ein Ruf aus dunkeln Tr\u00e4umen,", "tokens": ["Wie", "ein", "Ruf", "aus", "dun\u00b7keln", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus Gestein, aus Well' und B\u00e4umen,", "tokens": ["Aus", "Ge\u00b7stein", ",", "aus", "Well'", "und", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4"