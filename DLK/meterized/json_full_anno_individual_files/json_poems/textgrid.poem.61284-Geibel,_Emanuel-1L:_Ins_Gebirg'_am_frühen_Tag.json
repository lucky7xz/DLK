keln Weisen", "tokens": ["Aus", "dem", "Traum", "in", "dun\u00b7keln", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Redet ihre Sehnsucht nur.", "tokens": ["Re\u00b7det", "ih\u00b7re", "Sehn\u00b7sucht", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Aber einst erscheint der Tag,", "tokens": ["A\u00b7ber", "einst", "er\u00b7scheint", "der", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo das Wunder sich entdecket,", "tokens": ["Wo", "das", "Wun\u00b7der", "sich", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Herr zur Sprache wecket,", "tokens": ["Und", "der", "Herr", "zur", "Spra\u00b7che", "we\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was in stummen Banden lag.", "tokens": ["Was", "in", "stum\u00b7men", "Ban\u00b7den", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "In das Starre wunderbar", "tokens": ["In", "das", "Star\u00b7re", "wun\u00b7der\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"