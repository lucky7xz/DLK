{"dta.poem.3272": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingslust.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Seht den holden Fr\u00fchling bl\u00fchn!", "tokens": ["Seht", "den", "hol\u00b7den", "Fr\u00fch\u00b7ling", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll der ungenossen fliehn?", "tokens": ["Soll", "der", "un\u00b7ge\u00b7nos\u00b7sen", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fchlt ihr niemals Fr\u00fchlingstriebe?", "tokens": ["F\u00fchlt", "ihr", "nie\u00b7mals", "Fr\u00fch\u00b7ling\u00b7strie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freunde! weg mit Ernst und Leid!", "tokens": ["Freun\u00b7de", "!", "weg", "mit", "Ernst", "und", "Leid", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "APPR", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In der frohen Bluhmenzeit", "tokens": ["In", "der", "fro\u00b7hen", "Bluh\u00b7men\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Herrsche Bacchus und die Liebe!", "tokens": ["Herr\u00b7sche", "Bac\u00b7chus", "und", "die", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die ihr heute scherzen k\u00f6nnt,", "tokens": ["Die", "ihr", "heu\u00b7te", "scher\u00b7zen", "k\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Braucht, was euch der Himmel g\u00f6nnt,", "tokens": ["Braucht", ",", "was", "euch", "der", "Him\u00b7mel", "g\u00f6nnt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wohl morgen schon entziehet!", "tokens": ["Und", "wohl", "mor\u00b7gen", "schon", "ent\u00b7zie\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Lebt ein Mensch, der wissen mag,", "tokens": ["Lebt", "ein", "Mensch", ",", "der", "wis\u00b7sen", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob f\u00fcr ihn ein Fr\u00fchlingstag", "tokens": ["Ob", "f\u00fcr", "ihn", "ein", "Fr\u00fch\u00b7lings\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus Aurorens Armen fliehet?", "tokens": ["Aus", "Au\u00b7ro\u00b7rens", "Ar\u00b7men", "flie\u00b7het", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier sind Rosen! Hier ist Wein!", "tokens": ["Hier", "sind", "Ro\u00b7sen", "!", "Hier", "ist", "Wein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$.", "ADV", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll ich ohne Freude seyn,", "tokens": ["Soll", "ich", "oh\u00b7ne", "Freu\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der alte Bacchus lachet?", "tokens": ["Wo", "der", "al\u00b7te", "Bac\u00b7chus", "la\u00b7chet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herrsche, Gott der Fr\u00f6lichkeit!", "tokens": ["Herr\u00b7sche", ",", "Gott", "der", "Fr\u00f6\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O es kommt, es kommt die Zeit,", "tokens": ["O", "es", "kommt", ",", "es", "kommt", "die", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die zur Lust uns tr\u00e4ge machet.", "tokens": ["Die", "zur", "Lust", "uns", "tr\u00e4\u00b7ge", "ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber Phyllis l\u00e4\u00dft sich sehn!", "tokens": ["A\u00b7ber", "Phyl\u00b7lis", "l\u00e4\u00dft", "sich", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seh ich Amorn mit ihr gehn?", "tokens": ["Seh", "ich", "A\u00b7morn", "mit", "ihr", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhm wird alles weichen m\u00fcssen.", "tokens": ["Jhm", "wird", "al\u00b7les", "wei\u00b7chen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiche, Wein! Wo Phyllis ist,", "tokens": ["Wei\u00b7che", ",", "Wein", "!", "Wo", "Phyl\u00b7lis", "ist", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PWAV", "NE", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Trinkt man seltner, als man k\u00fc\u00dft!", "tokens": ["Trinkt", "man", "selt\u00b7ner", ",", "als", "man", "k\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Bacchus, weg! ich will nun k\u00fcssen.", "tokens": ["Bac\u00b7chus", ",", "weg", "!", "ich", "will", "nun", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKVZ", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}