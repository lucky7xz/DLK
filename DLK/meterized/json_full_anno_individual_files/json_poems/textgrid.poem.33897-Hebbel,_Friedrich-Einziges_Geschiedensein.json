{"textgrid.poem.33897": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Einziges Geschiedensein", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlummernd im schwellenden Gr\u00fcn", "tokens": ["Schlum\u00b7mernd", "im", "schwel\u00b7len\u00b7den", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Liegst du, wo L\u00fcfte dich f\u00e4cheln!", "tokens": ["Liegst", "du", ",", "wo", "L\u00fcf\u00b7te", "dich", "f\u00e4\u00b7cheln", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "M\u00e4dchen, was spiegelt dies L\u00e4cheln,", "tokens": ["M\u00e4d\u00b7chen", ",", "was", "spie\u00b7gelt", "dies", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PDS", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Spiegelt dies zarte Ergl\u00fch'n?", "tokens": ["Spie\u00b7gelt", "dies", "zar\u00b7te", "Er\u00b7gl\u00fch'n", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, wie beschleicht es mit Schmerz", "tokens": ["Ach", ",", "wie", "be\u00b7schleicht", "es", "mit", "Schmerz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Kalt mir den innersten Frieden!", "tokens": ["Kalt", "mir", "den", "in\u00b7ners\u00b7ten", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00e4nzlich, wie nie noch, geschieden", "tokens": ["G\u00e4nz\u00b7lich", ",", "wie", "nie", "noch", ",", "ge\u00b7schie\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "PWAV", "ADV", "ADV", "$,", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "F\u00fchlt sich von deinem mein Herz.", "tokens": ["F\u00fchlt", "sich", "von", "dei\u00b7nem", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Was, wie ein g\u00f6ttlicher Hauch,", "tokens": ["Was", ",", "wie", "ein", "g\u00f6tt\u00b7li\u00b7cher", "Hauch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Jetzt dich durchzittert, das Leben,", "tokens": ["Jetzt", "dich", "durch\u00b7zit\u00b7tert", ",", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Eh' du erwachst, wird's entschweben,", "tokens": ["Eh'", "du", "er\u00b7wachst", ",", "wird's", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Nimmer erfreut es mich auch.", "tokens": ["Nim\u00b7mer", "er\u00b7freut", "es", "mich", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Schlummernd im schwellenden Gr\u00fcn", "tokens": ["Schlum\u00b7mernd", "im", "schwel\u00b7len\u00b7den", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Liegst du, wo L\u00fcfte dich f\u00e4cheln!", "tokens": ["Liegst", "du", ",", "wo", "L\u00fcf\u00b7te", "dich", "f\u00e4\u00b7cheln", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "NN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "M\u00e4dchen, was spiegelt dies L\u00e4cheln,", "tokens": ["M\u00e4d\u00b7chen", ",", "was", "spie\u00b7gelt", "dies", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PDS", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Spiegelt dies zarte Ergl\u00fch'n?", "tokens": ["Spie\u00b7gelt", "dies", "zar\u00b7te", "Er\u00b7gl\u00fch'n", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Ach, wie beschleicht es mit Schmerz", "tokens": ["Ach", ",", "wie", "be\u00b7schleicht", "es", "mit", "Schmerz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Kalt mir den innersten Frieden!", "tokens": ["Kalt", "mir", "den", "in\u00b7ners\u00b7ten", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["N