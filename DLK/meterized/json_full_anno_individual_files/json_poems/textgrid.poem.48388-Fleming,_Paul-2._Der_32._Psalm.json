{"textgrid.poem.48388": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "2. Der 32. Psalm", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie selig, selig ist ein Sterblicher zu sch\u00e4tzen,", "tokens": ["Wie", "se\u00b7lig", ",", "se\u00b7lig", "ist", "ein", "Sterb\u00b7li\u00b7cher", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dem Gott den S\u00fcndenrest fern aus den Augen setzen,", "tokens": ["dem", "Gott", "den", "S\u00fcn\u00b7den\u00b7rest", "fern", "aus", "den", "Au\u00b7gen", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ja g\u00e4nzlich schenken kan, dem seiner Gnaden Tuch", "tokens": ["ja", "g\u00e4nz\u00b7lich", "schen\u00b7ken", "kan", ",", "dem", "sei\u00b7ner", "Gna\u00b7den", "Tuch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den Wust der Fehler deckt, der Segen kriegt f\u00fcr Fluch!", "tokens": ["den", "Wust", "der", "Feh\u00b7ler", "deckt", ",", "der", "Se\u00b7gen", "kriegt", "f\u00fcr", "Fluch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sage noch einmal, da\u00df selig der zu preisen,", "tokens": ["Ich", "sa\u00b7ge", "noch", "ein\u00b7mal", ",", "da\u00df", "se\u00b7lig", "der", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "ADJD", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "dem Gott an Zorrens statt sich milde kan erweisen,", "tokens": ["dem", "Gott", "an", "Zor\u00b7rens", "statt", "sich", "mil\u00b7de", "kan", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "PRF", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "erl\u00e4\u00dft ihm Straf' und Schuld, der nur bekennet frei,", "tokens": ["er\u00b7l\u00e4\u00dft", "ihm", "Straf'", "und", "Schuld", ",", "der", "nur", "be\u00b7ken\u00b7net", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,", "PRELS", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "von allem Heucheln weit, da\u00df er ein S\u00fcnder sei.", "tokens": ["von", "al\u00b7lem", "Heu\u00b7cheln", "weit", ",", "da\u00df", "er", "ein", "S\u00fcn\u00b7der", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn als ich meine Not auch dachte zu verschweigen,", "tokens": ["Denn", "als", "ich", "mei\u00b7ne", "Not", "auch", "dach\u00b7te", "zu", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "da wolte mir f\u00fcr Angst der Beine Mark verseigen.", "tokens": ["da", "wol\u00b7te", "mir", "f\u00fcr", "Angst", "der", "Bei\u00b7ne", "Mark", "ver\u00b7sei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch die Gewissensqual entgieng mir meine Kraft,", "tokens": ["Durch", "die", "Ge\u00b7wis\u00b7sens\u00b7qual", "ent\u00b7gieng", "mir", "mei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "von deiner schweren Hand verlor ich allen Saft.", "tokens": ["von", "dei\u00b7ner", "schwe\u00b7ren", "Hand", "ver\u00b7lor", "ich", "al\u00b7len", "Saft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie wenn zu Sommerszeit die durstigen Gefilder", "tokens": ["Wie", "wenn", "zu", "Som\u00b7mers\u00b7zeit", "die", "durs\u00b7ti\u00b7gen", "Ge\u00b7fil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "der grimme Hundsstern brennt, der Auen sch\u00f6ne Bilder,", "tokens": ["der", "grim\u00b7me", "Hunds\u00b7stern", "brennt", ",", "der", "Au\u00b7en", "sch\u00f6\u00b7ne", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "die Blumen werden welk und h\u00e4ngen unter sich,", "tokens": ["die", "Blu\u00b7men", "wer\u00b7den", "welk", "und", "h\u00e4n\u00b7gen", "un\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Herr, also stund es auch umb meinen Schmuck und mich.", "tokens": ["Herr", ",", "al\u00b7so", "stund", "es", "auch", "umb", "mei\u00b7nen", "Schmuck", "und", "mich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich wil nur meine Schuld geradezu bekennen", "tokens": ["Ich", "wil", "nur", "mei\u00b7ne", "Schuld", "ge\u00b7ra\u00b7de\u00b7zu", "be\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "und deinen Geisel mich ganz unverholen nennen.", "tokens": ["und", "dei\u00b7nen", "Gei\u00b7sel", "mich", "ganz", "un\u00b7ver\u00b7ho\u00b7len", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich spreche: Sihe Herr, das ist der S\u00fcnden Knecht!", "tokens": ["Ich", "spre\u00b7che", ":", "Si\u00b7he", "Herr", ",", "das", "ist", "der", "S\u00fcn\u00b7den", "Knecht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVIMP", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "alsbald vergiebst du mir und machest mich gerecht,", "tokens": ["als\u00b7bald", "ver\u00b7giebst", "du", "mir", "und", "ma\u00b7chest", "mich", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "streichst jene Handschrift durch. Umb dieses mu\u00df ein ieder,", "tokens": ["streichst", "je\u00b7ne", "Hand\u00b7schrift", "durch", ".", "Umb", "die\u00b7ses", "mu\u00df", "ein", "ie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$.", "KOUI", "PDS", "VMFIN", "ART", "PIAT", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "ja auch die Heiligen vor dir sich b\u00fccken nieder", "tokens": ["ja", "auch", "die", "Hei\u00b7li\u00b7gen", "vor", "dir", "sich", "b\u00fc\u00b7cken", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPER", "PRF", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "und einen Fu\u00dffall tun; drumb sind sie au\u00dfer Not,", "tokens": ["und", "ei\u00b7nen", "Fu\u00df\u00b7fall", "tun", ";", "drumb", "sind", "sie", "au\u00b7\u00dfer", "Not", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$.", "PAV", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "wenn eine gro\u00dfe Flut sie gar wil haben tot", "tokens": ["wenn", "ei\u00b7ne", "gro\u00b7\u00dfe", "Flut", "sie", "gar", "wil", "ha\u00b7ben", "tot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ADV", "VMFIN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "und taucht sie unter sich. Herr, du bist mein Erretter,", "tokens": ["und", "taucht", "sie", "un\u00b7ter", "sich", ".", "Herr", ",", "du", "bist", "mein", "Er\u00b7ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PRF", "$.", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "beh\u00fcte mich f\u00fcr Angst, vertilge meine Sp\u00f6tter!", "tokens": ["be\u00b7h\u00fc\u00b7te", "mich", "f\u00fcr", "Angst", ",", "ver\u00b7til\u00b7ge", "mei\u00b7ne", "Sp\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich pf\u00e4nde dir mich ein zu sagen werthen Dank,", "tokens": ["Ich", "pf\u00e4n\u00b7de", "dir", "mich", "ein", "zu", "sa\u00b7gen", "wert\u00b7hen", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "ART", "PTKZU", "VVINF", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "zu r\u00fchmen deine Kraft durch einen Lobgesang.", "tokens": ["zu", "r\u00fch\u00b7men", "dei\u00b7ne", "Kraft", "durch", "ei\u00b7nen", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Herr, sprich zu mir: Kom her, ich wil dich unterweisen!", "tokens": ["Herr", ",", "sprich", "zu", "mir", ":", "Kom", "her", ",", "ich", "wil", "dich", "un\u00b7ter\u00b7wei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "PPER", "$.", "VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "hier ist der wahre Steg, hier kanstu zu mir reisen,", "tokens": ["hier", "ist", "der", "wah\u00b7re", "Steg", ",", "hier", "kans\u00b7tu", "zu", "mir", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "und meinen Himmel an. Kom, richte dich nach mir!", "tokens": ["und", "mei\u00b7nen", "Him\u00b7mel", "an", ".", "Kom", ",", "rich\u00b7te", "dich", "nach", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$.", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "mein klares Augenliecht sol stets dir gehen f\u00fcr", "tokens": ["mein", "kla\u00b7res", "Au\u00b7gen\u00b7liecht", "sol", "stets", "dir", "ge\u00b7hen", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ADV", "PPER", "VVINF", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "und eine Fackel sein. \u2013 Seid nicht so unverst\u00e4ndig,", "tokens": ["und", "ei\u00b7ne", "Fa\u00b7ckel", "sein", ".", "\u2013", "Seid", "nicht", "so", "un\u00b7ver\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAINF", "$.", "$(", "VAIMP", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "wie G\u00e4ul' und M\u00e4uler sein, die eh' nicht werden b\u00e4ndig,", "tokens": ["wie", "G\u00e4ul'", "und", "M\u00e4u\u00b7ler", "sein", ",", "die", "eh'", "nicht", "wer\u00b7den", "b\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VAINF", "$,", "PRELS", "ADV", "PTKNEG", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "als wenn ihr wildes Maul ein scharfer Z\u00fcgel zwingt,", "tokens": ["als", "wenn", "ihr", "wil\u00b7des", "Maul", "ein", "schar\u00b7fer", "Z\u00fc\u00b7gel", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "da\u00df ihnen Blut und Schaum durch beide Lefzen dringt;", "tokens": ["da\u00df", "ih\u00b7nen", "Blut", "und", "Schaum", "durch", "bei\u00b7de", "Lef\u00b7zen", "dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "da werden sie erst zahm. Der B\u00f6se hat viel Plagen;", "tokens": ["da", "wer\u00b7den", "sie", "erst", "zahm", ".", "Der", "B\u00f6\u00b7se", "hat", "viel", "Pla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$.", "ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "wer auf den Herren hofft, der kan von Gutem sagen.", "tokens": ["wer", "auf", "den", "Her\u00b7ren", "hofft", ",", "der", "kan", "von", "Gu\u00b7tem", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.39": {"text": "Seid, ihr Gerechten, froh, und ihr, ihr Frommen, r\u00fchmt!", "tokens": ["Seid", ",", "ihr", "Ge\u00b7rech\u00b7ten", ",", "froh", ",", "und", "ihr", ",", "ihr", "From\u00b7men", ",", "r\u00fchmt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAIMP", "$,", "PPOSAT", "NN", "$,", "ADJD", "$,", "KON", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Di\u00df ist der rechte Preis, der unserm Herren ziemt.", "tokens": ["Di\u00df", "ist", "der", "rech\u00b7te", "Preis", ",", "der", "un\u00b7serm", "Her\u00b7ren", "ziemt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie selig, selig ist ein Sterblicher zu sch\u00e4tzen,", "tokens": ["Wie", "se\u00b7lig", ",", "se\u00b7lig", "ist", "ein", "Sterb\u00b7li\u00b7cher", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dem Gott den S\u00fcndenrest fern aus den Augen setzen,", "tokens": ["dem", "Gott", "den", "S\u00fcn\u00b7den\u00b7rest", "fern", "aus", "den", "Au\u00b7gen", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ja g\u00e4nzlich schenken kan, dem seiner Gnaden Tuch", "tokens": ["ja", "g\u00e4nz\u00b7lich", "schen\u00b7ken", "kan", ",", "dem", "sei\u00b7ner", "Gna\u00b7den", "Tuch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "den Wust der Fehler deckt, der Segen kriegt f\u00fcr Fluch!", "tokens": ["den", "Wust", "der", "Feh\u00b7ler", "deckt", ",", "der", "Se\u00b7gen", "kriegt", "f\u00fcr", "Fluch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sage noch einmal, da\u00df selig der zu preisen,", "tokens": ["Ich", "sa\u00b7ge", "noch", "ein\u00b7mal", ",", "da\u00df", "se\u00b7lig", "der", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "ADJD", "ART", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "dem Gott an Zorrens statt sich milde kan erweisen,", "tokens": ["dem", "Gott", "an", "Zor\u00b7rens", "statt", "sich", "mil\u00b7de", "kan", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "PRF", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "erl\u00e4\u00dft ihm Straf' und Schuld, der nur bekennet frei,", "tokens": ["er\u00b7l\u00e4\u00dft", "ihm", "Straf'", "und", "Schuld", ",", "der", "nur", "be\u00b7ken\u00b7net", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "$,", "PRELS", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "von allem Heucheln weit, da\u00df er ein S\u00fcnder sei.", "tokens": ["von", "al\u00b7lem", "Heu\u00b7cheln", "weit", ",", "da\u00df", "er", "ein", "S\u00fcn\u00b7der", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn als ich meine Not auch dachte zu verschweigen,", "tokens": ["Denn", "als", "ich", "mei\u00b7ne", "Not", "auch", "dach\u00b7te", "zu", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "da wolte mir f\u00fcr Angst der Beine Mark verseigen.", "tokens": ["da", "wol\u00b7te", "mir", "f\u00fcr", "Angst", "der", "Bei\u00b7ne", "Mark", "ver\u00b7sei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Durch die Gewissensqual entgieng mir meine Kraft,", "tokens": ["Durch", "die", "Ge\u00b7wis\u00b7sens\u00b7qual", "ent\u00b7gieng", "mir", "mei\u00b7ne", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "von deiner schweren Hand verlor ich allen Saft.", "tokens": ["von", "dei\u00b7ner", "schwe\u00b7ren", "Hand", "ver\u00b7lor", "ich", "al\u00b7len", "Saft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie wenn zu Sommerszeit die durstigen Gefilder", "tokens": ["Wie", "wenn", "zu", "Som\u00b7mers\u00b7zeit", "die", "durs\u00b7ti\u00b7gen", "Ge\u00b7fil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "der grimme Hundsstern brennt, der Auen sch\u00f6ne Bilder,", "tokens": ["der", "grim\u00b7me", "Hunds\u00b7stern", "brennt", ",", "der", "Au\u00b7en", "sch\u00f6\u00b7ne", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "die Blumen werden welk und h\u00e4ngen unter sich,", "tokens": ["die", "Blu\u00b7men", "wer\u00b7den", "welk", "und", "h\u00e4n\u00b7gen", "un\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Herr, also stund es auch umb meinen Schmuck und mich.", "tokens": ["Herr", ",", "al\u00b7so", "stund", "es", "auch", "umb", "mei\u00b7nen", "Schmuck", "und", "mich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich wil nur meine Schuld geradezu bekennen", "tokens": ["Ich", "wil", "nur", "mei\u00b7ne", "Schuld", "ge\u00b7ra\u00b7de\u00b7zu", "be\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "und deinen Geisel mich ganz unverholen nennen.", "tokens": ["und", "dei\u00b7nen", "Gei\u00b7sel", "mich", "ganz", "un\u00b7ver\u00b7ho\u00b7len", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich spreche: Sihe Herr, das ist der S\u00fcnden Knecht!", "tokens": ["Ich", "spre\u00b7che", ":", "Si\u00b7he", "Herr", ",", "das", "ist", "der", "S\u00fcn\u00b7den", "Knecht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVIMP", "NN", "$,", "PDS", "VAFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "alsbald vergiebst du mir und machest mich gerecht,", "tokens": ["als\u00b7bald", "ver\u00b7giebst", "du", "mir", "und", "ma\u00b7chest", "mich", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "streichst jene Handschrift durch. Umb dieses mu\u00df ein ieder,", "tokens": ["streichst", "je\u00b7ne", "Hand\u00b7schrift", "durch", ".", "Umb", "die\u00b7ses", "mu\u00df", "ein", "ie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$.", "KOUI", "PDS", "VMFIN", "ART", "PIAT", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "ja auch die Heiligen vor dir sich b\u00fccken nieder", "tokens": ["ja", "auch", "die", "Hei\u00b7li\u00b7gen", "vor", "dir", "sich", "b\u00fc\u00b7cken", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPER", "PRF", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "und einen Fu\u00dffall tun; drumb sind sie au\u00dfer Not,", "tokens": ["und", "ei\u00b7nen", "Fu\u00df\u00b7fall", "tun", ";", "drumb", "sind", "sie", "au\u00b7\u00dfer", "Not", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$.", "PAV", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "wenn eine gro\u00dfe Flut sie gar wil haben tot", "tokens": ["wenn", "ei\u00b7ne", "gro\u00b7\u00dfe", "Flut", "sie", "gar", "wil", "ha\u00b7ben", "tot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ADV", "VMFIN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "und taucht sie unter sich. Herr, du bist mein Erretter,", "tokens": ["und", "taucht", "sie", "un\u00b7ter", "sich", ".", "Herr", ",", "du", "bist", "mein", "Er\u00b7ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PRF", "$.", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--++-+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "beh\u00fcte mich f\u00fcr Angst, vertilge meine Sp\u00f6tter!", "tokens": ["be\u00b7h\u00fc\u00b7te", "mich", "f\u00fcr", "Angst", ",", "ver\u00b7til\u00b7ge", "mei\u00b7ne", "Sp\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich pf\u00e4nde dir mich ein zu sagen werthen Dank,", "tokens": ["Ich", "pf\u00e4n\u00b7de", "dir", "mich", "ein", "zu", "sa\u00b7gen", "wert\u00b7hen", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PRF", "ART", "PTKZU", "VVINF", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "zu r\u00fchmen deine Kraft durch einen Lobgesang.", "tokens": ["zu", "r\u00fch\u00b7men", "dei\u00b7ne", "Kraft", "durch", "ei\u00b7nen", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Herr, sprich zu mir: Kom her, ich wil dich unterweisen!", "tokens": ["Herr", ",", "sprich", "zu", "mir", ":", "Kom", "her", ",", "ich", "wil", "dich", "un\u00b7ter\u00b7wei\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "PPER", "$.", "VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "hier ist der wahre Steg, hier kanstu zu mir reisen,", "tokens": ["hier", "ist", "der", "wah\u00b7re", "Steg", ",", "hier", "kans\u00b7tu", "zu", "mir", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "und meinen Himmel an. Kom, richte dich nach mir!", "tokens": ["und", "mei\u00b7nen", "Him\u00b7mel", "an", ".", "Kom", ",", "rich\u00b7te", "dich", "nach", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "$.", "VVFIN", "$,", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "mein klares Augenliecht sol stets dir gehen f\u00fcr", "tokens": ["mein", "kla\u00b7res", "Au\u00b7gen\u00b7liecht", "sol", "stets", "dir", "ge\u00b7hen", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ADV", "PPER", "VVINF", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "und eine Fackel sein. \u2013 Seid nicht so unverst\u00e4ndig,", "tokens": ["und", "ei\u00b7ne", "Fa\u00b7ckel", "sein", ".", "\u2013", "Seid", "nicht", "so", "un\u00b7ver\u00b7st\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAINF", "$.", "$(", "VAIMP", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "wie G\u00e4ul' und M\u00e4uler sein, die eh' nicht werden b\u00e4ndig,", "tokens": ["wie", "G\u00e4ul'", "und", "M\u00e4u\u00b7ler", "sein", ",", "die", "eh'", "nicht", "wer\u00b7den", "b\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VAINF", "$,", "PRELS", "ADV", "PTKNEG", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "als wenn ihr wildes Maul ein scharfer Z\u00fcgel zwingt,", "tokens": ["als", "wenn", "ihr", "wil\u00b7des", "Maul", "ein", "schar\u00b7fer", "Z\u00fc\u00b7gel", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "da\u00df ihnen Blut und Schaum 