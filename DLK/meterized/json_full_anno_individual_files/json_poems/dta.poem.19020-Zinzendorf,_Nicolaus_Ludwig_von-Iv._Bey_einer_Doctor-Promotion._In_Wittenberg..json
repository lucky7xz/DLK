{"dta.poem.19020": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.   Bey einer   Doctor-Promotion. In Wittenberg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Ich ha\u00df' und meide die, so beym Studiren sich", "tokens": ["Ich", "ha\u00df'", "und", "mei\u00b7de", "die", ",", "so", "beym", "Stu\u00b7di\u00b7ren", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "$,", "ADV", "APPRART", "NN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht zu dem h\u00f6chsten Punct, zu GOtt, dem Geber, neigen,", "tokens": ["Nicht", "zu", "dem", "h\u00f6chs\u00b7ten", "Punct", ",", "zu", "Gott", ",", "dem", "Ge\u00b7ber", ",", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihre K\u00fchnheit mehr, als wahre Tugend zeigen;", "tokens": ["Und", "ih\u00b7re", "K\u00fchn\u00b7heit", "mehr", ",", "als", "wah\u00b7re", "Tu\u00b7gend", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$,", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer aber GOtt verehrt, den lieb\u2019 und ehre ich.", "tokens": ["Wer", "a\u00b7ber", "Gott", "ver\u00b7ehrt", ",", "den", "lieb'", "und", "eh\u00b7re", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "VVPP", "$,", "ART", "ADJA", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn der kan, trotz der Welt, trotz allen die ihn hassen,", "tokens": ["Denn", "der", "kan", ",", "trotz", "der", "Welt", ",", "trotz", "al\u00b7len", "die", "ihn", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VMFIN", "$,", "APPR", "ART", "NN", "$,", "APPR", "PIAT", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu seiner F\u00f6rderung die sch\u00f6nste Hoffnung fassen.", "tokens": ["Zu", "sei\u00b7ner", "F\u00f6r\u00b7de\u00b7rung", "die", "sch\u00f6ns\u00b7te", "Hoff\u00b7nung", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer das Verg\u00e4ngliche nach seinem Werth verlacht,", "tokens": ["Wer", "das", "Ver\u00b7g\u00e4ng\u00b7li\u00b7che", "nach", "sei\u00b7nem", "Werth", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wer sich vom Staub erhebt, den Erden-W\u00fcrmer kauen;", "tokens": ["Wer", "sich", "vom", "Staub", "er\u00b7hebt", ",", "den", "Er\u00b7den\u00b7W\u00fcr\u00b7mer", "kau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der lernet Himmel an, auf solche Dinge schauen,", "tokens": ["Der", "ler\u00b7net", "Him\u00b7mel", "an", ",", "auf", "sol\u00b7che", "Din\u00b7ge", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "PTKVZ", "$,", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die keine Zeit verzehrt; kein Alter schimmeln macht:", "tokens": ["Die", "kei\u00b7ne", "Zeit", "ver\u00b7zehrt", ";", "kein", "Al\u00b7ter", "schim\u00b7meln", "macht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$.", "PIAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wollt\u2019 ihn die arme Erd\u2019 auch noch so gerne sch\u00e4nden;", "tokens": ["Wollt'", "ihn", "die", "ar\u00b7me", "Erd'", "auch", "noch", "so", "ger\u00b7ne", "sch\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ADV", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So steht sein Gl\u00fcck und Wohl allein in GOttes H\u00e4nden.", "tokens": ["So", "steht", "sein", "Gl\u00fcck", "und", "Wohl", "al\u00b7lein", "in", "Got\u00b7tes", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "ADV", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der ist der gro\u00dfe HErr, der theilt die Aemter aus;", "tokens": ["Der", "ist", "der", "gro\u00b7\u00dfe", "Herr", ",", "der", "theilt", "die", "A\u00b7em\u00b7ter", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.14": {"text": "Wem der sie geben will, derselbe mu\u00df sie haben.", "tokens": ["Wem", "der", "sie", "ge\u00b7ben", "will", ",", "der\u00b7sel\u00b7be", "mu\u00df", "sie", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PPER", "VVINF", "VMFIN", "$,", "PDS", "VMFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den Schatz, wornach so viel offt nur vergeblich graben,", "tokens": ["Den", "Schatz", ",", "wor\u00b7nach", "so", "viel", "offt", "nur", "ver\u00b7geb\u00b7lich", "gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Schickt er den Seinigen zur Schlafens-Zeit ins Hau\u00df.", "tokens": ["Schickt", "er", "den", "Sei\u00b7ni\u00b7gen", "zur", "Schla\u00b7fens\u00b7Zeit", "ins", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPOSS", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die diesen zum Patron und zum Bef\u00f6rdrer w\u00e4hlen,", "tokens": ["Die", "die\u00b7sen", "zum", "Pat\u00b7ron", "und", "zum", "Be\u00b7f\u00f6rd\u00b7rer", "w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "APPRART", "NN", "KON", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der \u00fcber alle herrscht, verm\u00f6gen nicht zu fehlen.", "tokens": ["Der", "\u00fc\u00b7ber", "al\u00b7le", "herrscht", ",", "ver\u00b7m\u00f6\u00b7gen", "nicht", "zu", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIS", "VVPP", "$,", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Hier schreib ich, wie mein Hertz es in der Wahrheit h\u00e4lt,", "tokens": ["Hier", "schreib", "ich", ",", "wie", "mein", "Hertz", "es", "in", "der", "Wahr\u00b7heit", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie ich mein Lebenlang vor GOtt zu wandeln suche,", "tokens": ["Wie", "ich", "mein", "Le\u00b7ben\u00b7lang", "vor", "Gott", "zu", "wan\u00b7deln", "su\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dabey das falsche Thun der Heucheley verfluche,", "tokens": ["Da\u00b7bey", "das", "fal\u00b7sche", "Thun", "der", "Heu\u00b7che\u00b7ley", "ver\u00b7flu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den Dienst der Eitelkeit, die Liebe dieser Welt.", "tokens": ["Den", "Dienst", "der", "Ei\u00b7tel\u00b7keit", ",", "die", "Lie\u00b7be", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mein Freund! sein Gl\u00fccke bl\u00fcht, er mu\u00df, bey dessen Reiffen,", "tokens": ["Mein", "Freund", "!", "sein", "Gl\u00fc\u00b7cke", "bl\u00fcht", ",", "er", "mu\u00df", ",", "bey", "des\u00b7sen", "Reif\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN", "VVFIN", "$,", "PPER", "VMFIN", "$,", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sich eintzig und allein auf GOttes G\u00fcte steiffen.", "tokens": ["Sich", "eint\u00b7zig", "und", "al\u00b7lein", "auf", "Got\u00b7tes", "G\u00fc\u00b7te", "steif\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ADV", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}