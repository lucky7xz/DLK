{"textgrid.poem.65853": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "127", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwarz galt vor Alters nicht f\u00fcr sch\u00f6n: in Worten", "tokens": ["Schwarz", "galt", "vor", "Al\u00b7ters", "nicht", "f\u00fcr", "sch\u00f6n", ":", "in", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN", "PTKNEG", "APPR", "ADJD", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchrt' es zum mindesten den Namen nicht.", "tokens": ["F\u00fchrt'", "es", "zum", "min\u00b7des\u00b7ten", "den", "Na\u00b7men", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "ART", "NN", "PTKNEG", "$."], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Doch nun ist Schwarz der Sch\u00f6nheit Farbe worden,", "tokens": ["Doch", "nun", "ist", "Schwarz", "der", "Sch\u00f6n\u00b7heit", "Far\u00b7be", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NE", "ART", "NN", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ein Bastard entstellt ihr Angesicht.", "tokens": ["Und", "ein", "Bas\u00b7tard", "ent\u00b7stellt", "ihr", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn seit sich jede Hand Natur zu sein vermi\u00dft,", "tokens": ["Denn", "seit", "sich", "je\u00b7de", "Hand", "Na\u00b7tur", "zu", "sein"