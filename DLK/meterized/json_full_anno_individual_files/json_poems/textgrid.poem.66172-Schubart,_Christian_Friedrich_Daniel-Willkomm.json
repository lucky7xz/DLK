: ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Beters Blick?", "tokens": ["Des", "Be\u00b7ters", "Blick", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ein Engel trat aus seinem Sonnenkreise,", "tokens": ["Ein", "En\u00b7gel", "trat", "aus", "sei\u00b7nem", "Son\u00b7nen\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "War dein Gef\u00e4hrte auf der Reise", "tokens": ["War", "dein", "Ge\u00b7f\u00e4hr\u00b7te", "auf", "der", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und kam mit dir zur\u00fcck.", "tokens": ["Und", "kam", "mit", "dir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als todweissagend in des Schiffes Ritzen", "tokens": ["Als", "tod\u00b7weis\u00b7sa\u00b7gend", "in", "des", "Schif\u00b7fes", "Rit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Wasser drang,", "tokens": ["Das", "Was\u00b7ser", "drang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Da hat sein Arm dich aus dem Schiff gehoben,", "tokens": ["Da", "hat", "sein", "Arm", "dich", "aus", "dem", "Schiff", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eh' es mit f\u00fcrchterlichem Toben", "tokens": ["Eh'", "es", "mit", "f\u00fcrch\u00b7ter\u00b7li\u00b7chem", "To\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Die wilde See verschlang.", "tokens": ["Die", "wil\u00b7de", "See", "ver\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Allherrscher in dem Himmel, ach! drum danken", "tokens": ["All\u00b7herr\u00b7scher", "in", "dem", "Him\u00b7mel", ",", "ach", "!", "drum", "dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "ITJ", "$.", "PAV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Thr\u00e4nen wir.", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "wir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Du, unsers F\u00fcrsten m\u00e4chtigster Erhalter,", "tokens": ["Du", ",", "un\u00b7sers", "F\u00fcrs\u00b7ten", "m\u00e4ch\u00b7tigs\u00b7ter", "Er\u00b7hal\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sieh Myriaden H\u00e4ndefalter,", "tokens": ["Sieh", "My\u00b7ria\u00b7den", "H\u00e4n\u00b7de\u00b7fal\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie alle danken dir.", "tokens": ["Sie", "al\u00b7le", "dan\u00b7ken", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir jauchzten, als des F\u00fcrsten Wagen rollte,", "tokens": ["Wir", "jauchz\u00b7ten", ",", "als", "des", "F\u00fcrs\u00b7ten", "Wa\u00b7gen", "roll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ist Er ja!", "tokens": ["Da", "ist", "Er", "ja", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "CaRL, seines Volkes Stolz! und ihre Freude,", "tokens": ["CaRL", ",", "sei\u00b7nes", "Vol\u00b7kes", "Stolz", "!", "und", "ih\u00b7re", "Freu\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "NN", "$.", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "FrANZISKA strahlt an seiner Seite!", "tokens": ["FrAN\u00b7ZIS\u00b7KA", "strahlt", "an", "sei\u00b7ner", "Sei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Heil uns! denn CARL ist da.", "tokens": ["Heil", "uns", "!", "denn", "CaRL", "ist", "da", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "KON", "NE", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ist wieder in dem Scho\u00dfe seiner Treuen", "tokens": ["Ist", "wie\u00b7der", "in", "dem", "Scho\u00b7\u00dfe", "sei\u00b7ner", "Treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Neckar, wo", "tokens": ["Am", "Ne\u00b7ckar", ",", "wo"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "PWAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Des Vaterlandes L\u00fcfte ihn bewehen,", "tokens": ["Des", "Va\u00b7ter\u00b7lan\u00b7des", "L\u00fcf\u00b7te", "ihn", "be\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Kinder ihm entgegengehen,", "tokens": ["Wo", "Kin\u00b7der", "ihm", "ent\u00b7ge\u00b7gen\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! unaussprechlich froh.", "tokens": ["Ach", "!", "un\u00b7aus\u00b7sprech\u00b7lich", "froh", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nun hast du, CARL, der V\u00f6lker viel gewogen \u2013", "tokens": ["Nun", "hast", "du", ",", "CaRL", ",", "der", "V\u00f6l\u00b7ker", "viel", "ge\u00b7wo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NE", "$,", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch fandst du eins", "tokens": ["Doch", "fandst", "du", "eins"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit deinen tiefen Menschenforscherblicken", "tokens": ["Mit", "dei\u00b7nen", "tie\u00b7fen", "Men\u00b7schen\u00b7for\u00b7scher\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So schwer auf deiner Wage nicken?", "tokens": ["So", "schwer", "auf", "dei\u00b7ner", "Wa\u00b7ge", "ni\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So treu, so gut wie deins?", "tokens": ["So", "treu", ",", "so", "gut", "wie", "deins", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Drum bleibe, CARL, in Deines Landes Mitte!", "tokens": ["Drum", "blei\u00b7be", ",", "CaRL", ",", "in", "Dei\u00b7nes", "Lan\u00b7des", "Mit\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "NE", "$,", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sieh um dich her", "tokens": ["Sieh", "um", "dich", "her"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "PRF", "APZR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die sch\u00f6ne Gruppe deiner Kinder stehen", "tokens": ["Die", "sch\u00f6\u00b7ne", "Grup\u00b7pe", "dei\u00b7ner", "Kin\u00b7der", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und dich mit Schimmerblicken flehen:", "tokens": ["Und", "dich", "mit", "Schim\u00b7mer\u00b7bli\u00b7cken", "fle\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verla\u00df uns nimmermehr!", "tokens": ["Ver\u00b7la\u00df", "uns", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Willkommen, Carl, nach einem langen Fluge", "tokens": ["Will\u00b7kom\u00b7men", ",", "Carl", ",", "nach", "ei\u00b7nem", "lan\u00b7gen", "Flu\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In fernes Land!", "tokens": ["In", "fer\u00b7nes", "Land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Willkommen hier in deiner Sueven Gauen!", "tokens": ["Will\u00b7kom\u00b7men", "hier", "in", "dei\u00b7ner", "Sue\u00b7ven", "Gau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Siehst du die Wonnez\u00e4hre thauen?", "tokens": ["Siehst", "du", "die", "Won\u00b7ne\u00b7z\u00e4h\u00b7re", "thau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und die 