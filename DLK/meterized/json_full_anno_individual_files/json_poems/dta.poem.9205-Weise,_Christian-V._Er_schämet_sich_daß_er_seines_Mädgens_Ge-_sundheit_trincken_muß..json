{"dta.poem.9205": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n Er sch\u00e4met sich/ da\u00df er seines M\u00e4dgens Ge-  \n sundheit trincken mu\u00df.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will den handel nur gestehn/", "tokens": ["Ich", "will", "den", "han\u00b7del", "nur", "ge\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe dir zu ehren/", "tokens": ["Ich", "ha\u00b7be", "dir", "zu", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf dein gesundes wolergehn/", "tokens": ["Auf", "dein", "ge\u00b7sun\u00b7des", "wo\u00b7ler\u00b7gehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein gl\u00e4\u00dfgen m\u00fcssen leeren/", "tokens": ["Ein", "gl\u00e4\u00df\u00b7gen", "m\u00fcs\u00b7sen", "lee\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch da mirs in die h\u00e4nde kam/", "tokens": ["Doch", "da", "mirs", "in", "die", "h\u00e4n\u00b7de", "kam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NE", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da kriegt ich eine grosse scham.", "tokens": ["Da", "kriegt", "ich", "ei\u00b7ne", "gros\u00b7se", "scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Es war ein guter freund darbey/", "tokens": ["Es", "war", "ein", "gu\u00b7ter", "freund", "dar\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PAV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der m\u00f6chte gerne wissen/", "tokens": ["Der", "m\u00f6ch\u00b7te", "ger\u00b7ne", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer doch das liebe m\u00e4dgen sey/", "tokens": ["Wer", "doch", "das", "lie\u00b7be", "m\u00e4d\u00b7gen", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das mir mein hertz entrissen/", "tokens": ["Das", "mir", "mein", "hertz", "ent\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und weil ich die\u00df zu hertzen nahm/", "tokens": ["Und", "weil", "ich", "die\u00df", "zu", "hert\u00b7zen", "nahm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDS", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So kriegt ich eine grosse scham.", "tokens": ["So", "kriegt", "ich", "ei\u00b7ne", "gros\u00b7se", "scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Ich bin nicht gerne/ wo man gar", "tokens": ["Ich", "bin", "nicht", "ger\u00b7ne", "/", "wo", "man", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$(", "PWAV", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu offenhertzig handelt/", "tokens": ["Zu", "of\u00b7fen\u00b7hert\u00b7zig", "han\u00b7delt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sehe lieber/ da\u00df ein paar", "tokens": ["Und", "se\u00b7he", "lie\u00b7ber", "/", "da\u00df", "ein", "paar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$(", "KOUS", "ART", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In stiller liebe wandelt/", "tokens": ["In", "stil\u00b7ler", "lie\u00b7be", "wan\u00b7delt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Darum/ als die gesundheit kam/", "tokens": ["Da\u00b7rum", "/", "als", "die", "ge\u00b7sund\u00b7heit", "kam", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "KOUS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So kriegt ich eine grosse scham.", "tokens": ["So", "kriegt", "ich", "ei\u00b7ne", "gros\u00b7se", "scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ich liebe dich/ mein s\u00fcsses kind/", "tokens": ["Ich", "lie\u00b7be", "dich", "/", "mein", "s\u00fcs\u00b7ses", "kind", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00fcsse dir die h\u00e4nde/", "tokens": ["Und", "k\u00fcs\u00b7se", "dir", "die", "h\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So offt ich durch den schnellen wind", "tokens": ["So", "offt", "ich", "durch", "den", "schnel\u00b7len", "wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die liebes-seufftzer sende/", "tokens": ["Die", "lie\u00b7bes\u00b7seufft\u00b7zer", "sen\u00b7de", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Jedoch/ als ich da\u00df gla\u00df bekam/", "tokens": ["Je\u00b7doch", "/", "als", "ich", "da\u00df", "gla\u00df", "be\u00b7kam", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "KOUS", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So kriegt ich eine grosse scham/", "tokens": ["So", "kriegt", "ich", "ei\u00b7ne", "gros\u00b7se", "scham", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Mein kind ich sch\u00e4me mich zwar nicht/", "tokens": ["Mein", "kind", "ich", "sch\u00e4\u00b7me", "mich", "zwar", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein gl\u00e4\u00dfgen aus zustechen/", "tokens": ["Ein", "gl\u00e4\u00df\u00b7gen", "aus", "zu\u00b7ste\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NE", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein hertz/ mein mund/ mein angesicht", "tokens": ["Mein", "hertz", "/", "mein", "mund", "/", "mein", "an\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird ander zeugn\u00fc\u00df sprechen/", "tokens": ["Wird", "an\u00b7der", "zeug\u00b7n\u00fc\u00df", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch/ weil mirs zu geschwinde kam/", "tokens": ["Doch", "/", "weil", "mirs", "zu", "ge\u00b7schwin\u00b7de", "kam", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "NE", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So kriegt ich eine grosse scham.", "tokens": ["So", "kriegt", "ich", "ei\u00b7ne", "gros\u00b7se", "scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "6. Dann wer sich aus der f\u00fcnstern\u00fc\u00df", "tokens": ["Dann", "wer", "sich", "aus", "der", "f\u00fcns\u00b7ter\u00b7n\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald wil der Sonne n\u00e4hen/", "tokens": ["Bald", "wil", "der", "Son\u00b7ne", "n\u00e4\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der ist am ersten ungewi\u00df/", "tokens": ["Der", "ist", "am", "ers\u00b7ten", "un\u00b7ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie und was er gesehen/", "tokens": ["Wie", "und", "was", "er", "ge\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "PWS", "PPER", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die augen sind ihm stumpff und lahm/", "tokens": ["Die", "au\u00b7gen", "sind", "ihm", "stumpff", "und", "lahm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und er kriegt eine grosse scham.", "tokens": ["Und", "er", "kriegt", "ei\u00b7ne", "gros\u00b7se", "scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "7. So lie\u00df ich in verliebter pflicht", "tokens": ["So", "lie\u00df", "ich", "in", "ver\u00b7lieb\u00b7ter", "pflicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geheime klagen fliessen/", "tokens": ["Ge\u00b7hei\u00b7me", "kla\u00b7gen", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dieweil ich meiner sonnen-liecht", "tokens": ["Die\u00b7weil", "ich", "mei\u00b7ner", "son\u00b7nen\u00b7liecht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht konte noch geniessen/", "tokens": ["Nicht", "kon\u00b7te", "noch", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und als das liebe gl\u00e4\u00dfgen kam/", "tokens": ["Und", "als", "das", "lie\u00b7be", "gl\u00e4\u00df\u00b7gen", "kam", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erschrack ich vor der grossen scham.", "tokens": ["Er\u00b7schrack", "ich", "vor", "der", "gros\u00b7sen", "scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "8. Nun Liebgen wirst du mir die hand", "tokens": ["Nun", "Lieb\u00b7gen", "wirst", "du", "mir", "die", "hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VAFIN", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit gleicher liebe dr\u00fccken/", "tokens": ["Mit", "glei\u00b7cher", "lie\u00b7be", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wirst du ohn allen unbestand", "tokens": ["Wirst", "du", "ohn", "al\u00b7len", "un\u00b7be\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auff deinen diener blicken/", "tokens": ["Auff", "dei\u00b7nen", "die\u00b7ner", "bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So wird mein hertze wieder zahm/", "tokens": ["So", "wird", "mein", "hert\u00b7ze", "wie\u00b7der", "zahm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mir vergeht die grosse scham.", "tokens": ["Und", "mir", "ver\u00b7geht", "die", "gros\u00b7se", "scham", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}