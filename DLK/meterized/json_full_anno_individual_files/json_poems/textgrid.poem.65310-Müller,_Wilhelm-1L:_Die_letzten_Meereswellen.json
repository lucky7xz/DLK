nfo": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als h\u00e4tt' es Fleisch und Blut:", "tokens": ["Als", "h\u00e4tt'", "es", "Fleisch", "und", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Je mehr sie hub das R\u00f6ckchen,", "tokens": ["Je", "mehr", "sie", "hub", "das", "R\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Je h\u00f6her stieg die Fluth.", "tokens": ["Je", "h\u00f6\u00b7her", "stieg", "die", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da fing sie an zu schreien", "tokens": ["Da", "fing", "sie", "an", "zu", "schrei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und lie\u00df die Zipfel los,", "tokens": ["Und", "lie\u00df", "die", "Zip\u00b7fel", "los", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alle Muscheln fielen", "tokens": ["Und", "al\u00b7le", "Mu\u00b7scheln", "fie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus ihrem vollen Schoo\u00df.", "tokens": ["Aus", "ih\u00b7rem", "vol\u00b7len", "Schoo\u00df", "."], 