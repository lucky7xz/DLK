{"dta.poem.19435": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "Anakreons vier und dreissigste Ode.  \n \u039c\u03b7\u03bc\u03b5 \u03a6\u03cd\u03b3\u03b7\u03c2, \u1f41\u03c1\u1ff6\u03c3\u03b1.  \n  An mein M\u00e4dchen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "Ach flieh mich nicht, erblickend", "tokens": ["Ach", "flieh", "mich", "nicht", ",", "er\u00b7bli\u00b7ckend"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "VVFIN", "PPER", "PTKNEG", "$,", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Scheitels weisse Locken!", "tokens": ["Des", "Schei\u00b7tels", "weis\u00b7se", "Lo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ach, weil dir die Blume", "tokens": ["Und", "ach", ",", "weil", "dir", "die", "Blu\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der frischen Jugend bl\u00fchet,", "tokens": ["Der", "fri\u00b7schen", "Ju\u00b7gend", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verschm\u00e4h nicht meine Liebe!", "tokens": ["Ver\u00b7schm\u00e4h", "nicht", "mei\u00b7ne", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du siehst ja, wie in Kr\u00e4nzen,", "tokens": ["Du", "siehst", "ja", ",", "wie", "in", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geflochten unter Rosen,", "tokens": ["Ge\u00b7floch\u00b7ten", "un\u00b7ter", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die weissen Liljen prangen!", "tokens": ["Die", "weis\u00b7sen", "Lil\u00b7jen", "pran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}