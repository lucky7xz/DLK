{"textgrid.poem.54138": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schnipsel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Langweilig ist noch nicht ernsthaft.", "tokens": ["Lang\u00b7wei\u00b7lig", "ist", "noch", "nicht", "ernst\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er trug sein Herz in der Hand, und er ruhte nicht, bis sie ihm aus der Hand fra\u00df.", "tokens": ["Er", "trug", "sein", "Herz", "in", "der", "Hand", ",", "und", "er", "ruh\u00b7te", "nicht", ",", "bis", "sie", "ihm", "aus", "der", "Hand", "fra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-++--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Die beste \u00dcbersetzung f\u00fcr puella publica, die mir bekannt ist, hei\u00dft: Vorfreudenm\u00e4dchen.", "tokens": ["Die", "bes\u00b7te", "\u00dc\u00b7bers\u00b7et\u00b7zung", "f\u00fcr", "pu\u00b7el\u00b7la", "pub\u00b7li\u00b7ca", ",", "die", "mir", "be\u00b7kannt", "ist", ",", "hei\u00dft", ":", "Vor\u00b7freu\u00b7den\u00b7m\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "NE", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.4": {"text": "Erwarte nichts. Heute: das ist dein Leben.", "tokens": ["Er\u00b7war\u00b7te", "nichts", ".", "Heu\u00b7te", ":", "das", "ist", "dein", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$.", "ADV", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wenn einer nichts gelernt hat \u2013: dann organisiert er.", "tokens": ["Wenn", "ei\u00b7ner", "nichts", "ge\u00b7lernt", "hat", "\u2013", ":", "dann", "or\u00b7ga\u00b7ni\u00b7siert", "er", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "VVPP", "VAFIN", "$(", "$.", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer lobt, wird selten nach seiner Aktivlegitimation gefragt.", "tokens": ["Wer", "lobt", ",", "wird", "sel\u00b7ten", "nach", "sei\u00b7ner", "Ak\u00b7tiv\u00b7le\u00b7gi\u00b7ti\u00b7ma\u00b7ti\u00b7on", "ge\u00b7fragt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Der Amerikaner h\u00e4lt sich f\u00fcr den ersten Mann der Welt, weil er kein Farbiger ist.", "tokens": ["Der", "A\u00b7me\u00b7ri\u00b7ka\u00b7ner", "h\u00e4lt", "sich", "f\u00fcr", "den", "ers\u00b7ten", "Mann", "der", "Welt", ",", "weil", "er", "kein", "Far\u00b7bi\u00b7ger", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "A