": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Der sich auf deine Blumen, in deine Saaten warf,", "tokens": ["Der", "sich", "auf", "dei\u00b7ne", "Blu\u00b7men", ",", "in", "dei\u00b7ne", "Saa\u00b7ten", "warf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und wetzt an deinen Palmen die blut'gen Hauer scharf.", "tokens": ["Und", "wetzt", "an", "dei\u00b7nen", "Pal\u00b7men", "die", "blut'\u00b7gen", "Hau\u00b7er", "scharf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Wach' auf! \u2013 hat Rolands Glocke", "tokens": ["Wach'", "auf", "!", "\u2013", "hat", "Ro\u00b7lands", "Glo\u00b7cke"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$.", "$(", "VAFIN", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hei, wie zu Gent sie dr\u00f6hnet und Br\u00fcgg' und L\u00fcttich weckt!", "tokens": ["Hei", ",", "wie", "zu", "Gent", "sie", "dr\u00f6h\u00b7net", "und", "Br\u00fcg\u00b7g'", "und", "L\u00fct\u00b7tich", "weckt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPR", "NN", "PPER", "VVFIN", "KON", "NN", "KON", "NE", "VVFIN", "$."], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Das deutet Brand! die Flamme des Aufruhrs ist erwacht;", "tokens": ["Das", "deu\u00b7tet", "Brand", "!", "die", "Flam\u00b7me", "des", "Auf\u00b7ruhrs", "ist", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$.", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sieh, wie der Franzos die Funken zur hellen Lohe facht!", "tokens": ["Sieh", ",", "wie", "der", "Fran\u00b7zos", "die", "Fun\u00b7ken", "zur", "hel\u00b7len", "Lo\u00b7he", "facht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Wach' auf, o Max, und schreite ins blutige Gericht!", "tokens": ["Wach'", "auf", ",", "o", "Max", ",", "und", "schrei\u00b7te", "ins", "blu\u00b7ti\u00b7ge", "Ge\u00b7richt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "NE", "$,", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und wecken Flanderns Rebellen und Frankreichs Meuchler dich nicht,", "tokens": ["Und", "we\u00b7cken", "Flan\u00b7derns", "Re\u00b7bel\u00b7len", "und", "Fran\u00b7kreichs", "Meuch\u00b7ler", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "KON", "NE", "NN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+---+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "So krach' es dir in die Ohren mit greller Posaunenkraft:", "tokens": ["So", "krach'", "es", "dir", "in", "die", "Oh\u00b7ren", "mit", "grel\u00b7ler", "Po\u00b7sau\u00b7nen\u00b7kraft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wach' auf, dein Sohn ist gefangen, dein Sohn ist in enger Haft!", "tokens": ["Wach'", "auf", ",", "dein", "Sohn", "ist", "ge\u00b7fan\u00b7gen", ",", "dein", "Sohn", "ist", "in", "en\u00b7ger", "Haft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Erwacht ist der Leu; ", "tokens": ["Er\u00b7wacht", "ist", "der", "Leu", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Wie ihm die M\u00e4hne lodert, wie rings das Leben erbleicht!", "tokens": ["Wie", "ihm", "die", "M\u00e4h\u00b7ne", "lo\u00b7dert", ",", "wie", "rings", "das", "Le\u00b7ben", "er\u00b7bleicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ei, du gewalt'ger Eber, der L\u00f6we packt doch gut", "tokens": ["Ei", ",", "du", "ge\u00b7walt'\u00b7ger", "E\u00b7ber", ",", "der", "L\u00f6\u00b7we", "packt", "doch", "gut"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,", "ART", "NE", "VVFIN", "ADV", "ADJD"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und d\u00fcngt jetzt seine Saaten mit deinem schwarzen Blut.", "tokens": ["Und", "d\u00fcngt", "jetzt", "sei\u00b7ne", "Saa\u00b7ten", "mit", "dei\u00b7nem", "schwar\u00b7zen", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.12": {"line.1": {"text": "Ei, Gent, die M\u00f6rser donnern doch lauter als dein Roland,", "tokens": ["Ei", ",", "Gent", ",", "die", "M\u00f6r\u00b7ser", "don\u00b7nern", "doch", "lau\u00b7ter", "als", "dein", "Ro\u00b7land", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "PIAT", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gelt, Franzmann, hast beim Heizen die Finger dir verbrannt?", "tokens": ["Gelt", ",", "Franz\u00b7mann", ",", "hast", "beim", "Hei\u00b7zen", "die", "Fin\u00b7ger", "dir", "ver\u00b7brannt", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "VAFIN", "APPRART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gelt, meuterisches Flandern, der Aar holt doch sein Kind,", "tokens": ["Gelt", ",", "meu\u00b7te\u00b7ri\u00b7sches", "Flan\u00b7dern", ",", "der", "Aar", "holt", "doch", "sein", "Kind", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zum sichern Felsenhorste tr\u00e4gt er's durch Sturm und Wind!", "tokens": ["Zum", "si\u00b7chern", "Fel\u00b7sen\u00b7hors\u00b7te", "tr\u00e4gt", "er's", "durch", "Sturm", "und", "Wind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PIS", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}}}}}