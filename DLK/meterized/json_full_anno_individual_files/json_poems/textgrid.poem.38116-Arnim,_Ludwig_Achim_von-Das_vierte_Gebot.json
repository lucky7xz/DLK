{"textgrid.poem.38116": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das vierte Gebot", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Land zu Frankereiche", "tokens": ["Im", "Land", "zu", "Fran\u00b7ke\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein alter Konig sa\u00df,", "tokens": ["Ein", "al\u00b7ter", "Ko\u00b7nig", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der all sein Land und Reiche", "tokens": ["Der", "all", "sein", "Land", "und", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "KON", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An seinen Sohn da gab.", "tokens": ["An", "sei\u00b7nen", "Sohn", "da", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das war aus Alters Schw\u00e4che,", "tokens": ["Das", "war", "aus", "Al\u00b7ters", "Schw\u00e4\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df er sich des verwandt,", "tokens": ["Da\u00df", "er", "sich", "des", "ver\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Sohn th\u00e4t ihm versprechen,", "tokens": ["Der", "Sohn", "th\u00e4t", "ihm", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich n\u00e4hre dich zur Hand.", "tokens": ["Ich", "n\u00e4h\u00b7re", "dich", "zur", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Sohn gar bald sich nahme", "tokens": ["Der", "Sohn", "gar", "bald", "sich", "nah\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "PRF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Hausfrau minniglich", "tokens": ["Ein", "Haus\u00b7frau", "min\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die war dem Vater grame,", "tokens": ["Die", "war", "dem", "Va\u00b7ter", "gra\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sprach also kl\u00e4gelich:", "tokens": ["Sprach", "al\u00b7so", "kl\u00e4\u00b7ge\u00b7lich", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der alt Mann thut stets husten,", "tokens": ["Der", "alt", "Mann", "thut", "stets", "hus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Bei Tisch, das graut mir sehr,", "tokens": ["Bei", "Tisch", ",", "das", "graut", "mir", "sehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PDS", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und nimmt mir Essens Lusten,", "tokens": ["Und", "nimmt", "mir", "Es\u00b7sens", "Lus\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Macht mir die Zunge schwer.", "tokens": ["Macht", "mir", "die", "Zun\u00b7ge", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Der Sohn th\u00e4t ihren Willen,", "tokens": ["Der", "Sohn", "th\u00e4t", "ih\u00b7ren", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lie\u00df auch den Vater sein", "tokens": ["Lie\u00df", "auch", "den", "Va\u00b7ter", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da legen in der Stillen", "tokens": ["Da", "le\u00b7gen", "in", "der", "Stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Unter die Stiege hinein.", "tokens": ["Un\u00b7ter", "die", "Stie\u00b7ge", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Ein Bett darinnen stunde,", "tokens": ["Ein", "Bett", "da\u00b7rin\u00b7nen", "stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Heu und auch von Stroh,", "tokens": ["Von", "Heu", "und", "auch", "von", "Stroh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Recht als ein andrer Hunde", "tokens": ["Recht", "als", "ein", "an\u00b7drer", "Hun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel Jahre lag er so.", "tokens": ["Viel", "Jah\u00b7re", "lag", "er", "so", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Konigin th\u00e4t sich legen,", "tokens": ["Die", "Ko\u00b7ni\u00b7gin", "th\u00e4t", "sich", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gebahr ein Sohne gut,", "tokens": ["Ge\u00b7bahr", "ein", "Soh\u00b7ne", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der ward ein stolzer Degen,", "tokens": ["Der", "ward", "ein", "stol\u00b7zer", "De\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und h\u00e4tt ein frommen Muth.", "tokens": ["Und", "h\u00e4tt", "ein", "from\u00b7men", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Als der die Sach erkannte,", "tokens": ["Als", "der", "die", "Sach", "er\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bracht er zu aller Stund", "tokens": ["Bracht", "er", "zu", "al\u00b7ler", "Stund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seim Anherrn Spei\u00df und Tranke,", "tokens": ["Seim", "An\u00b7herrn", "Spei\u00df", "und", "Tran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was er nur finden kunt.", "tokens": ["Was", "er", "nur", "fin\u00b7den", "kunt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er bat ihn an eim Tage", "tokens": ["Er", "bat", "ihn", "an", "eim", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um eine Ro\u00dfdeck alt,", "tokens": ["Um", "ei\u00b7ne", "Ro\u00df\u00b7deck", "alt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er nit kalt da lage,", "tokens": ["Da\u00df", "er", "nit", "kalt", "da", "la\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der fromm J\u00fcngling lief bald.", "tokens": ["Der", "fromm", "J\u00fcng\u00b7ling", "lief", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "--+-+-", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "Da er zum Ro\u00dfstall kame,", "tokens": ["Da", "er", "zum", "Ro\u00df\u00b7stall", "ka\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Ro\u00dfdeck, die war gut,", "tokens": ["Ein", "Ro\u00df\u00b7deck", ",", "die", "war", "gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er von dem Pferd da nahme,", "tokens": ["Er", "von", "dem", "Pferd", "da", "nah\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerri\u00df sie mit Unmuth.", "tokens": ["Zer\u00b7ri\u00df", "sie", "mit", "Un\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Sein Vater ihn da fraget:", "tokens": ["Sein", "Va\u00b7ter", "ihn", "da", "fra\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ihm die Ro\u00dfdeck th\u00e4t:", "tokens": ["Was", "ihm", "die", "Ro\u00df\u00b7deck", "th\u00e4t", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbich bring sie halb, er saget", "tokens": ["\u00bb", "ich", "bring", "sie", "halb", ",", "er", "sa\u00b7get"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Deim Vater an sein Bett.", "tokens": ["Deim", "Va\u00b7ter", "an", "sein", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Halbtheil ich behalte", "tokens": ["Das", "Halb\u00b7theil", "ich", "be\u00b7hal\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr dich, wenn du da ruhst,", "tokens": ["F\u00fcr", "dich", ",", "wenn", "du", "da", "ruhst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo deinen Vater alte,", "tokens": ["Wo", "dei\u00b7nen", "Va\u00b7ter", "al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du jezt versperren thust.\u00ab", "tokens": ["Du", "jezt", "ver\u00b7sper\u00b7ren", "thust", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "VVINF", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Im Land zu Frankereiche", "tokens": ["Im", "Land", "zu", "Fran\