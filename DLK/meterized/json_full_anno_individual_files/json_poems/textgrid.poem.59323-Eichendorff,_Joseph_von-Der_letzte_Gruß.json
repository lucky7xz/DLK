{"textgrid.poem.59323": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der letzte Gru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kam vom Walde hernieder,", "tokens": ["Ich", "kam", "vom", "Wal\u00b7de", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da stand noch das alte Haus,", "tokens": ["Da", "stand", "noch", "das", "al\u00b7te", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mein Liebchen, sie schaute wieder", "tokens": ["Mein", "Lieb\u00b7chen", ",", "sie", "schau\u00b7te", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie sonst zum Fenster hinaus.", "tokens": ["Wie", "sonst", "zum", "Fens\u00b7ter", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Sie hat einen andern genommen,", "tokens": ["Sie", "hat", "ei\u00b7nen", "an\u00b7dern", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ich war drau\u00dfen in Schlacht und Sieg,", "tokens": ["Ich", "war", "drau\u00b7\u00dfen", "in", "Schlacht", "und", "Sieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nun ist alles anders gekommen,", "tokens": ["Nun", "ist", "al\u00b7les", "an\u00b7ders", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ich wollt, 's w\u00e4r wieder erst Krieg.", "tokens": ["Ich", "wollt", ",", "'s", "w\u00e4r", "wie\u00b7der", "erst", "Krieg", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Am Wege dort spielte ihr Kindlein,", "tokens": ["Am", "We\u00b7ge", "dort", "spiel\u00b7te", "ihr", "Kin\u00b7dlein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das glich ihr recht auf ein Haar,", "tokens": ["Das", "glich", "ihr", "recht", "auf", "ein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich k\u00fc\u00dft's auf sein rotes M\u00fcndlein:", "tokens": ["Ich", "k\u00fc\u00dft's", "auf", "sein", "ro\u00b7tes", "M\u00fcnd\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbgott segne dich immerdar!\u00ab", "tokens": ["\u00bb", "gott", "seg\u00b7ne", "dich", "im\u00b7mer\u00b7dar", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie aber schaute erschrocken", "tokens": ["Sie", "a\u00b7ber", "schau\u00b7te", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch lange Zeit nach mir hin,", "tokens": ["Noch", "lan\u00b7ge", "Zeit", "nach", "mir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und sch\u00fcttelte sinnend die Locken", "tokens": ["Und", "sch\u00fct\u00b7tel\u00b7te", "sin\u00b7nend", "die", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und wu\u00dfte nicht, wer ich bin. \u2013", "tokens": ["Und", "wu\u00df\u00b7te", "nicht", ",", "wer", "ich", "bin", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VAFIN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Da droben hoch stand ich am Baume,", "tokens": ["Da", "dro\u00b7ben", "hoch", "stand", "ich", "am", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da rauschten die W\u00e4lder so sacht,", "tokens": ["Da", "rauschten", "die", "W\u00e4l\u00b7der", "so", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Mein Waldhorn, das klang wie im Traume", "tokens": ["Mein", "Wald\u00b7horn", ",", "das", "klang", "wie", "im", "Trau\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Hin\u00fcber die ganze Nacht.", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "die", "gan\u00b7ze", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und als die V\u00f6gelein sangen", "tokens": ["Und", "als", "die", "V\u00f6\u00b7ge\u00b7lein", "san\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fr\u00fchmorgens, sie weinte so sehr,", "tokens": ["Fr\u00fch\u00b7mor\u00b7gens", ",", "sie", "wein\u00b7te", "so", "sehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich aber war weit schon gegangen,", "tokens": ["Ich", "a\u00b7ber", "war", "weit", "schon", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Nun sieht sie mich nimmermehr!", "tokens": ["Nun", "sieht", "sie", "mich", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "wo