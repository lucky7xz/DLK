{"textgrid.poem.60689": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie h\u00e4ufig narrt uns Selbstbetrug!", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie h\u00e4ufig narrt uns Selbstbetrug!", "tokens": ["Wie", "h\u00e4u\u00b7fig", "narrt", "uns", "Selbst\u00b7be\u00b7trug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Toren gibt es stets genug,", "tokens": ["Der", "To\u00b7ren", "gibt", "es", "stets", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Schatten haschen gleich \u00c4sopus Hund,", "tokens": ["Die", "Schat\u00b7ten", "ha\u00b7schen", "gleich", "\u00c4\u00b7so\u00b7pus", "Hund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NE", "NN", "$,"], "meter": "-+-+-+---+", "measure": "zehnsilber"}, "line.4": {"text": "Der jenen Knochen, den er trug,", "tokens": ["Der", "je\u00b7nen", "Kno\u00b7chen", ",", "den", "er", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergr\u00f6\u00dfert schaut in eines andern Mund,", "tokens": ["Ver\u00b7gr\u00f6\u00b7\u00dfert", "schaut", "in", "ei\u00b7nes", "an\u00b7dern", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der ihm als Spiegelbild aus eines Flusses Grund", "tokens": ["Der", "ihm", "als", "Spie\u00b7gel\u00b7bild", "aus", "ei\u00b7nes", "Flus\u00b7ses", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "KOUS", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Entgegenfletscht. Er springt und schnappt hinein.", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7fletscht", ".", "Er", "springt", "und", "schnappt", "hin\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der eigne Knochen sinkt, der fremde war nur Schein", "tokens": ["Der", "eig\u00b7ne", "Kno\u00b7chen", "sinkt", ",", "der", "frem\u00b7de", "war", "nur", "Schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ART", "ADJA", "VAFIN", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und schwindet jenem echten gleich", "tokens": ["Und", "schwin\u00b7det", "je\u00b7nem", "ech\u00b7ten", "gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDAT", "ADJA", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Im aufgew\u00fchlten Wellenreich.", "tokens": ["Im", "auf\u00b7ge\u00b7w\u00fchl\u00b7ten", "Wel\u00b7len\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit M\u00fche nur entkam der Gierige der Gefahr,", "tokens": ["Mit", "M\u00fc\u00b7he", "nur", "ent\u00b7kam", "der", "Gie\u00b7ri\u00b7ge", "der", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "