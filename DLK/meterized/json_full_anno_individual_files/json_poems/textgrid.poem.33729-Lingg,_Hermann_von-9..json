{"textgrid.poem.33729": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was zu erleben w\u00e4re wohl das Beste?", "tokens": ["Was", "zu", "er\u00b7le\u00b7ben", "w\u00e4\u00b7re", "wohl", "das", "Bes\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einherzuziehn auf stolzem Siegeswagen,", "tokens": ["Ein\u00b7her\u00b7zu\u00b7ziehn", "auf", "stol\u00b7zem", "Sie\u00b7ges\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nachdem man einen kecken Feind geschlagen,", "tokens": ["Nach\u00b7dem", "man", "ei\u00b7nen", "ke\u00b7cken", "Feind", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der unsrer Freiheit bestes Blut erpre\u00dfte?", "tokens": ["Der", "uns\u00b7rer", "Frei\u00b7heit", "bes\u00b7tes", "Blut", "er\u00b7pre\u00df\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Troph\u00e4en, Siegsgepr\u00e4ng' und Siegesfeste?", "tokens": ["Trop\u00b7h\u00e4\u00b7en", ",", "Siegs\u00b7ge\u00b7pr\u00e4ng'", "und", "Sie\u00b7ges\u00b7fes\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "KON", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Doch wenn ich m\u00fc\u00dfte deshalb dir entsagen,", "tokens": ["Doch", "wenn", "ich", "m\u00fc\u00df\u00b7te", "des\u00b7halb", "dir", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "PAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie dann? \u2013 Ich will die kleine Schwalbe fragen,", "tokens": ["Wie", "dann", "?", "\u2013", "Ich", "will", "die", "klei\u00b7ne", "Schwal\u00b7be", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "$(", "PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die so vergn\u00fcglich lebt in ihrem Neste.", "tokens": ["Die", "so", "ver\u00b7gn\u00fcg\u00b7lich", "lebt", "in", "ih\u00b7rem", "Nes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie eng ist unser Dasein! Unser Wille", "tokens": ["Wie", "eng", "ist", "un\u00b7ser", "Da\u00b7sein", "!", "Un\u00b7ser", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat nur die Wahl, in hohem Stolz entweder", "tokens": ["Hat", "nur", "die", "Wahl", ",", "in", "ho\u00b7hem", "Stolz", "ent\u00b7we\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "APPR", "ADJA", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Gl\u00fcck zu suchen, oder in der Stille!", "tokens": ["Sein", "Gl\u00fcck", "zu", "su\u00b7chen", ",", "o\u00b7der", "in", "der", "Stil\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer mit dem Degen, wer mit Wort und Feder", "tokens": ["Wer", "mit", "dem", "De\u00b7gen", ",", "wer", "mit", "Wort", "und", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "$,", "PWS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Kampf f\u00fchrt, lass' das Traumbild der Idylle!", "tokens": ["Den", "Kampf", "f\u00fchrt", ",", "lass'", "das", "Traum\u00b7bild", "der", "I\u00b7dyl\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Bl\u00fctenschmuck prangt nicht zugleich die Zeder.", "tokens": ["Mit", "Bl\u00fc\u00b7ten\u00b7schmuck", "prangt", "nicht", "zu\u00b7gleich", "die", "Ze\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was zu erleben w\u00e4re wohl das Beste?", "tokens": ["Was", "zu", "er\u00b7le\u00b7ben", "w\u00e4\u00b7re", "wohl", "das", "Bes\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKZU", "VVINF", "VAFIN", "ADV", "ART", "NN", "$."], "