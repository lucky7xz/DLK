{"dta.poem.5274": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sonderlich scheint ihre Glut", "tokens": ["Son\u00b7der\u00b7lich", "scheint", "ih\u00b7re", "Glut"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Herrlich auf der klaren Fluth,", "tokens": ["Herr\u00b7lich", "auf", "der", "kla\u00b7ren", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als im Berg-Cristallnen Spiegel,", "tokens": ["Als", "im", "Ber\u00b7gCris\u00b7tall\u00b7nen", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da wo ihr beflammtes Bild", "tokens": ["Da", "wo", "ihr", "be\u00b7flamm\u00b7tes", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhre glatte Fl\u00e4che f\u00fcllt.", "tokens": ["Ih\u00b7re", "glat\u00b7te", "Fl\u00e4\u00b7che", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn auch ihre rege H\u00fcgel", "tokens": ["Wenn", "auch", "ih\u00b7re", "re\u00b7ge", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Hin und wieder sich erh\u00f6hn,", "tokens": ["Hin", "und", "wie\u00b7der", "sich", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht man pl\u00f6tzlich auf den Spitzen", "tokens": ["Sieht", "man", "pl\u00f6tz\u00b7lich", "auf", "den", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Tausend kleine Lichter blitzen,", "tokens": ["Tau\u00b7send", "klei\u00b7ne", "Lich\u00b7ter", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schnell entstehn und schnell vergehn;", "tokens": ["Schnell", "ent\u00b7stehn", "und", "schnell", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Jhre nimmer stille Wellen", "tokens": ["Ih\u00b7re", "nim\u00b7mer", "stil\u00b7le", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Scheinen darum sanft zu schwellen,", "tokens": ["Schei\u00b7nen", "da\u00b7rum", "sanft", "zu", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Um dem menschlichen Gesicht", "tokens": ["Um", "dem", "menschli\u00b7chen", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Sonnen-Spiegel vorzustellen.", "tokens": ["Son\u00b7nen\u00b7Spie\u00b7gel", "vor\u00b7zu\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ja mich deucht ob sagten sie:", "tokens": ["Ja", "mich", "deucht", "ob", "sag\u00b7ten", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "KOUS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Sucht bey uns, als der Copie,", "tokens": ["Sucht", "bey", "uns", ",", "als", "der", "Co\u00b7pie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Aller Sch\u00f6nheit Urbild nicht:", "tokens": ["Al\u00b7ler", "Sch\u00f6n\u00b7heit", "Ur\u00b7bild", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Droben ist das wahre Licht.", "tokens": ["Dro\u00b7ben", "ist", "das", "wah\u00b7re", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Seht, wie dort die Rehe springen!", "tokens": ["Seht", ",", "wie", "dort", "die", "Re\u00b7he", "sprin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "H\u00f6rt, wie hier die V\u00f6gel singen!", "tokens": ["H\u00f6rt", ",", "wie", "hier", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wie so hell, wie rein, wie klar!", "tokens": ["Wie", "so", "hell", ",", "wie", "rein", ",", "wie", "klar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "H\u00f6rt, wie sie die T\u00f6ne kr\u00e4useln!", "tokens": ["H\u00f6rt", ",", "wie", "sie", "die", "T\u00f6\u00b7ne", "kr\u00e4u\u00b7seln", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "H\u00f6rt der lauen Winde s\u00e4useln!", "tokens": ["H\u00f6rt", "der", "lau\u00b7en", "Win\u00b7de", "s\u00e4u\u00b7seln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "H\u00f6rt der Bienen muntre Schaar!", "tokens": ["H\u00f6rt", "der", "Bie\u00b7nen", "mun\u00b7tre", "Schaar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "H\u00f6rt, wie \u00fcber glatte Kiesel,", "tokens": ["H\u00f6rt", ",", "wie", "\u00fc\u00b7ber", "glat\u00b7te", "Kie\u00b7sel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Mit sanft murmelndem Geriesel,", "tokens": ["Mit", "sanft", "mur\u00b7meln\u00b7dem", "Ge\u00b7rie\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Das geschwinde B\u00e4chlein eilet,", "tokens": ["Das", "ge\u00b7schwin\u00b7de", "B\u00e4ch\u00b7lein", "ei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und bald einen gelben Sand,", "tokens": ["Und", "bald", "ei\u00b7nen", "gel\u00b7ben", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Bald ein gr\u00fcn-bebl\u00fchmtes Land", "tokens": ["Bald", "ein", "gr\u00fcn\u00b7be\u00b7bl\u00fchm\u00b7tes", "Land"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Mit besch\u00e4umten Wirbeln theilet!", "tokens": ["Mit", "be\u00b7sch\u00e4um\u00b7ten", "Wir\u00b7beln", "thei\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}