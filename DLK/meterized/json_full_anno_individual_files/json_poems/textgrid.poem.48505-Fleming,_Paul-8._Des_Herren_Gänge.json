{"textgrid.poem.48505": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. Des Herren G\u00e4nge", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, erstlich f\u00fchrt man dich f\u00fcr Hannas sein Gerichte.", "tokens": ["Herr", ",", "erst\u00b7lich", "f\u00fchrt", "man", "dich", "f\u00fcr", "Han\u00b7nas", "sein", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PIS", "PRF", "APPR", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hernach bek\u00f6mmt sein Schw\u00e4hr dich balde zu Gesichte,", "tokens": ["Her\u00b7nach", "be\u00b7k\u00f6mmt", "sein", "Schw\u00e4hr", "dich", "bal\u00b7de", "zu", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "worauf dich Pontius auf seinen Richtplatz r\u00fcckt", "tokens": ["wo\u00b7rauf", "dich", "Pon\u00b7ti\u00b7us", "auf", "sei\u00b7nen", "Richt\u00b7platz", "r\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NE", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und zu Herodes Burg von dannen ab verschickt,", "tokens": ["und", "zu", "He\u00b7ro\u00b7des", "Burg", "von", "dan\u00b7nen", "ab", "ver\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "APPR", "ADV", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "der dich ihm wieder giebt nach sattsamen Verspotten.", "tokens": ["der", "dich", "ihm", "wie\u00b7der", "giebt", "nach", "satt\u00b7sa\u00b7men", "Ver\u00b7spot\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Drauf mustu mitte fort mit deinen Henkersrotten,", "tokens": ["Drauf", "mus\u00b7tu", "mit\u00b7te", "fort", "mit", "dei\u00b7nen", "Hen\u00b7kers\u00b7rot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ADV", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "die dich so fleischen ab, bald auf die Sch\u00e4delst\u00e4t'", "tokens": ["die", "dich", "so", "flei\u00b7schen", "ab", ",", "bald", "auf", "die", "Sch\u00e4\u00b7del\u00b7st\u00e4t'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVFIN", "PTKVZ", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und an das Kreuze selbst, das dir den Tod ant\u00e4t',", "tokens": ["und", "an", "das", "Kreu\u00b7ze", "selbst", ",", "das", "dir", "den", "Tod", "an\u00b7t\u00e4t'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "und von dem Kreuz' ins Grab. O rechte Leidensstege,", "tokens": ["und", "von", "dem", "Kreuz'", "ins", "Grab", ".", "O", "rech\u00b7te", "Lei\u00b7denss\u00b7te\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPRART", "NN", "$.", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.10": {"text": "o G\u00e4nge voller Angst, o harte, schwere Wege!", "tokens": ["o", "G\u00e4n\u00b7ge", "vol\u00b7ler", "Angst", ",", "o", "har\u00b7te", ",", "schwe\u00b7re", "We\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "NN", "ADJA", "NN", "$,", "FM", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Herr, erstlich f\u00fchrt man dich f\u00fcr Hannas sein Gerichte.", "tokens": ["Herr", ",", "erst\u00b7lich", "f\u00fchrt", "man", "dich", "f\u00fcr", "Han\u00b7nas", "sein", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PIS", "PRF", "APPR", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hernach bek\u00f6mmt sein Schw\u00e4hr dich balde zu Gesichte,", "tokens": ["Her\u00b7nach", "be\u00b7k\u00f6mmt", "sein", "Schw\u00e4hr", "dich", "bal\u00b7de", "zu", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "worauf dich Pontius auf seinen Richtplatz r\u00fcckt", "tokens": ["wo\u00b7rauf", "dich", "Pon\u00b7ti\u00b7us", "auf", "sei\u00b7nen", "Richt\u00b7platz", "r\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NE", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure"