{"dta.poem.9583": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie sagte: sie w\u00fcste nicht/ was Venus  \n und Cupido sey.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn/ Amor/ wirstu doch solch einen hochmuth schw\u00e4chen/", "tokens": ["Wenn", "/", "A\u00b7mor", "/", "wirs\u00b7tu", "doch", "solch", "ei\u00b7nen", "hoch\u00b7muth", "schw\u00e4\u00b7chen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "NE", "$(", "VAFIN", "ADV", "PIAT", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Leidstu/ da\u00df Solime dir trotzet ins gesicht?", "tokens": ["Leids\u00b7tu", "/", "da\u00df", "So\u00b7li\u00b7me", "dir", "trot\u00b7zet", "ins", "ge\u00b7sicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KOUS", "NN", "PPER", "VVFIN", "APPRART", "VVPP", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "H\u00f6r\u2019/ wie ver\u00e4chtlich sie zu deinem nachtheil spricht/", "tokens": ["H\u00f6r'", "/", "wie", "ver\u00b7\u00e4cht\u00b7lich", "sie", "zu", "dei\u00b7nem", "nacht\u00b7heil", "spricht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie kenne weder dich/ noch deine mutter nicht:", "tokens": ["Sie", "ken\u00b7ne", "we\u00b7der", "dich", "/", "noch", "dei\u00b7ne", "mut\u00b7ter", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "$(", "ADV", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "L\u00e4stu so l\u00e4sterlich mit worten auf dich stechen?", "tokens": ["L\u00e4s\u00b7tu", "so", "l\u00e4s\u00b7ter\u00b7lich", "mit", "wor\u00b7ten", "auf", "dich", "ste\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "APPR", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn wird einst die gedult in deiner seele brechen?", "tokens": ["Wenn", "wird", "einst", "die", "ge\u00b7dult", "in", "dei\u00b7ner", "see\u00b7le", "bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auff! waffne deinen leib! Es heischet noth und pflicht/", "tokens": ["Auff", "!", "waff\u00b7ne", "dei\u00b7nen", "leib", "!", "Es", "hei\u00b7schet", "noth", "und", "pflicht", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "NN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Solch eine frevel-that an Solimen zu r\u00e4chen.", "tokens": ["Solch", "ei\u00b7ne", "fre\u00b7vel\u00b7that", "an", "So\u00b7li\u00b7men", "zu", "r\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Greiff deinen bogen an. Nimm pfeil und blitz zur hand/", "tokens": ["Greiff", "dei\u00b7nen", "bo\u00b7gen", "an", ".", "Nimm", "pfeil", "und", "blitz", "zur", "hand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "PTKVZ", "$.", "NE", "NE", "KON", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "La\u00df deine fackeln stehn in lichterlohem brand/", "tokens": ["La\u00df", "dei\u00b7ne", "fa\u00b7ckeln", "stehn", "in", "lich\u00b7ter\u00b7lo\u00b7hem", "brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Heb\u2019 an ihr hertze drauff zu martern und zu brennen/", "tokens": ["Heb'", "an", "ihr", "hert\u00b7ze", "drauff", "zu", "mar\u00b7tern", "und", "zu", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "PAV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Geu\u00df immer frisches \u00f6hl zu diesen flammen hin/", "tokens": ["Geu\u00df", "im\u00b7mer", "fri\u00b7sches", "\u00f6hl", "zu", "die\u00b7sen", "flam\u00b7men", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "APPR", "PDAT", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So lange/ bi\u00df sie beugt den hart-versteinten sinn/", "tokens": ["So", "lan\u00b7ge", "/", "bi\u00df", "sie", "beugt", "den", "har\u00b7tver\u00b7stein\u00b7ten", "sinn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und deine mutter lernt nebst deinem nahmen kennen.", "tokens": ["Und", "dei\u00b7ne", "mut\u00b7ter", "lernt", "nebst", "dei\u00b7nem", "nah\u00b7men", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}