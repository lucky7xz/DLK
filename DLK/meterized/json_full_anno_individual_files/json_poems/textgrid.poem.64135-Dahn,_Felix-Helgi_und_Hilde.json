{"textgrid.poem.64135": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Helgi und Hilde", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mir den Vater erschlagen", "tokens": ["Du", "hast", "mir", "den", "Va\u00b7ter", "er\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und schlugst mir den Bruder dazu,", "tokens": ["Und", "schlugst", "mir", "den", "Bru\u00b7der", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PAV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und dennoch in ewigen Tagen", "tokens": ["Und", "den\u00b7noch", "in", "e\u00b7wi\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mein Liebster, mein alles bist du.", "tokens": ["Mein", "Liebs\u00b7ter", ",", "mein", "al\u00b7les", "bist", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "PIS", "VAFIN", "PPER", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es liegen so m\u00fcde vom Fechten", "tokens": ["Es", "lie\u00b7gen", "so", "m\u00fc\u00b7de", "vom", "Fech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die erschlagenen Helden zu Hauf:", "tokens": ["Die", "er\u00b7schla\u00b7ge\u00b7nen", "Hel\u00b7den", "zu", "