{"textgrid.poem.64307": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "26. Stimme der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Selinde! denn mit Engelstimme", "tokens": ["Mei\u00b7ne", "Se\u00b7lin\u00b7de", "!", "denn", "mit", "En\u00b7gel\u00b7stim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ADV", "APPR", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Singt die Liebe mir zu: Sie wird die Deine!", "tokens": ["Singt", "die", "Lie\u00b7be", "mir", "zu", ":", "Sie", "wird", "die", "Dei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "PPOSAT", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wird die Meine! Himmel und Erde schwinden,", "tokens": ["Wird", "die", "Mei\u00b7ne", "!", "Him\u00b7mel", "und", "Er\u00b7de", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPOSAT", "$.", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Meine Selinde!", "tokens": ["Mei\u00b7ne", "Se\u00b7lin\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Thr\u00e4nen der Sehnsucht, die auf blassen Wangen", "tokens": ["Thr\u00e4\u00b7nen", "der", "Sehn\u00b7sucht", ",", "die", "auf", "blas\u00b7sen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN"