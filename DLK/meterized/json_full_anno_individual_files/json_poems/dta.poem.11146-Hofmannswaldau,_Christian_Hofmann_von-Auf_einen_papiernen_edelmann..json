{"dta.poem.11146": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen papiernen edelmann.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man siehet, da\u00df dein schild nur schwartze farben weist,", "tokens": ["Man", "sie\u00b7het", ",", "da\u00df", "dein", "schild", "nur", "schwart\u00b7ze", "far\u00b7ben", "weist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPOSAT", "ADJD", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du mehr tint\u2019 als blut vors vaterland vergeust.", "tokens": ["Weil", "du", "mehr", "tint'", "als", "blut", "vors", "va\u00b7ter\u00b7land", "ver\u00b7geust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KOKOM", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du kommst von Sebulon, drum prangen deine fahnen", "tokens": ["Du", "kommst", "von", "Se\u00b7bu\u00b7lon", ",", "drum", "pran\u00b7gen", "dei\u00b7ne", "fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$,", "PAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit federn, aber nicht mit v\u00e4terlichen ahnen.", "tokens": ["Mit", "fe\u00b7dern", ",", "a\u00b7ber", "nicht", "mit", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "ah\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "ADV", "PT