{"dta.poem.20852": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wann durch der G\u00f6tter Glut und Flammen/", "tokens": ["Wann", "durch", "der", "G\u00f6t\u00b7ter", "Glut", "und", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und eine nicht bekante Macht/", "tokens": ["Und", "ei\u00b7ne", "nicht", "be\u00b7kan\u00b7te", "Macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PTKNEG", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Seelen Kr\u00e4fften gantz zusammen", "tokens": ["Der", "See\u00b7len", "Kr\u00e4ff\u00b7ten", "gantz", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind in den Kl\u00f6\u00df des Leibes bracht/", "tokens": ["Sind", "in", "den", "Kl\u00f6\u00df", "des", "Lei\u00b7bes", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sp\u00fchrt man wie durch Freundschaffts Wercke/", "tokens": ["So", "sp\u00fchrt", "man", "wie", "durch", "Freund\u00b7schaffts", "Wer\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So unser Geist nicht an sich h\u00e4lt/", "tokens": ["So", "un\u00b7ser", "Geist", "nicht", "an", "sich", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gar vieler Wei\u00dfheit/ Macht und St\u00e4rcke/", "tokens": ["Gar", "vie\u00b7ler", "Wei\u00df\u00b7heit", "/", "Macht", "und", "St\u00e4r\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In Eil zusammen sich gesellt.", "tokens": ["In", "Eil", "zu\u00b7sam\u00b7men", "sich", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}