{"dta.poem.18717": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Glaubst du denn von Mund zu Ohr", "tokens": ["Glaubst", "du", "denn", "von", "Mund", "zu", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey ein redlicher Gewinnst?", "tokens": ["Sey", "ein", "red\u00b7li\u00b7cher", "Ge\u00b7winnst", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueberliefrung, o! du Thor!", "tokens": ["Ue\u00b7berl\u00b7ief\u00b7rung", ",", "o", "!", "du", "Thor", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "$.", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist auch wohl ein Hirngespinnst.", "tokens": ["Ist", "auch", "wohl", "ein", "Hirn\u00b7ge\u00b7spinnst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun geht erst das Urtheil an.", "tokens": ["Nun", "geht", "erst", "das", "Ur\u00b7theil", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Dich vermag aus Glaubensketten", "tokens": ["Dich", "ver\u00b7mag", "aus", "Glau\u00b7bens\u00b7ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der Verstand allein zu retten,", "tokens": ["Der", "Ver\u00b7stand", "al\u00b7lein", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dem du schon Verzicht gethan.", "tokens": ["Dem", "du", "schon", "Ver\u00b7zicht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}