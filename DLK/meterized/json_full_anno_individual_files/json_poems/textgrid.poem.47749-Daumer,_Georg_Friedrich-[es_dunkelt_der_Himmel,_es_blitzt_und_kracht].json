{"textgrid.poem.47749": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[es dunkelt der Himmel, es blitzt und kracht]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es dunkelt der Himmel, es blitzt und kracht \u2013", "tokens": ["Es", "dun\u00b7kelt", "der", "Him\u00b7mel", ",", "es", "blitzt", "und", "kracht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nur schnell die Fenster zugemacht,", "tokens": ["Nur", "schnell", "die", "Fens\u00b7ter", "zu\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df nicht der Teufel in's Haus herein", "tokens": ["Da\u00df", "nicht", "der", "Teu\u00b7fel", "in's", "Haus", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich fl\u00fcchte, der entsetzliche,", "tokens": ["Sich", "fl\u00fcch\u00b7te", ",", "der", "ent\u00b7setz\u00b7li\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Gott verfolgt mit aller Macht", "tokens": ["Den", "Gott", "ver\u00b7folgt", "mit", "al\u00b7ler", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wir mit ihm verloren sei'n!", "tokens": ["Und", "wir", "mit", "ihm", "ver\u00b7lo\u00b7ren", "sei'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es dunkelt der Himmel, es blitzt und kracht \u2013", "tokens": ["Es", "dun\u00b7kelt", "der", "Him\u00b7mel", ",", "es", "blitzt", "und", "kracht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nur schnell die Fenster zugemacht,", "tokens": ["Nur", "schnell", "die", "Fens\u00b7ter", "zu\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df nicht der Teufel in's Haus herein", "tokens": ["Da\u00df", "nicht", "der", "Teu\u00b7fel", "in's", "Haus", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich fl\u00fcchte, der entsetzliche,", "tokens": ["Sich", "fl\u00fcch\u00b7te", ",", "der", "ent\u00b7setz\u00b7li\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Gott verfolgt mit aller Macht", "tokens": ["Den", "Gott", "ver\u00b7folgt", "mit", "al\u00b7ler", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN"], "meter"