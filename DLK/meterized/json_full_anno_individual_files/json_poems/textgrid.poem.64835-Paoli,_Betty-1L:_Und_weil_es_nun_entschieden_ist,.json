{"textgrid.poem.64835": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und weil es nun entschieden ist,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und weil es nun entschieden ist,", "tokens": ["Und", "weil", "es", "nun", "ent\u00b7schie\u00b7den", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df mir auf immerdar entrissen,", "tokens": ["Da\u00df", "mir", "auf", "im\u00b7mer\u00b7dar", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was sich ersetzt nicht, noch vergi\u00dft,", "tokens": ["Was", "sich", "er\u00b7setzt", "nicht", ",", "noch", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "PTKNEG", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und einst gekannt, nicht mehr zu missen.", "tokens": ["Und", "einst", "ge\u00b7kannt", ",", "nicht", "mehr", "zu", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So sei mein k\u00f6nigliches Haupt, \u2013", "tokens": ["So", "sei", "mein", "k\u00f6\u00b7nig\u00b7li\u00b7ches", "Haupt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umstrahlt einst von der Krone Prangen \u2013", "tokens": ["Um\u00b7strahlt", "einst", "von", "der", "Kro\u00b7ne", "Pran\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun ihm das Diadem geraubt,", "tokens": ["Nun", "ihm", "das", "Di\u00b7a\u00b7dem", "ge\u00b7raubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von keinem Kranz je mehr umfangen!", "tokens": ["Von", "kei\u00b7nem", "Kranz", "je", "mehr", "um\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Umsonst lockt mich mit Schmeichelton", "tokens": ["Um\u00b7sonst", "lockt", "mich", "mit", "Schmei\u00b7chel\u00b7ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "NE"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Die Lust, der heitre Glanz des Lebens;", "tokens": ["Die", "Lust", ",", "der", "heit\u00b7re", "Glanz", "des", "Le\u00b7bens", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein stolzer Sinn spricht ihnen Hohn:", "tokens": ["Mein", "stol\u00b7zer", "Sinn", "spricht", "ih\u00b7nen", "Hohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Lust und Glanz, ihr lockt vergebens!", "tokens": ["O", "Lust", "und", "Glanz", ",", "ihr", "lockt", "ver\u00b7ge\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df, es schien euch sch\u00f6n genug", "tokens": ["Ich", "wei\u00df", ",", "es", "schien", "euch", "sch\u00f6n", "ge\u00b7nug"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein unbezwungen Herz zu knicken,", "tokens": ["Mein", "un\u00b7be\u00b7zwun\u00b7gen", "Herz", "zu", "kni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und dort in euerm Siegeszug", "tokens": ["Und", "dort", "in", "eu\u00b7erm", "Sie\u00b7ges\u00b7zug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich als gefangen zu erblicken.", "tokens": ["Mich", "als", "ge\u00b7fan\u00b7gen", "zu", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich aber, wissend fest und klar,", "tokens": ["Ich", "a\u00b7ber", ",", "wis\u00b7send", "fest", "und", "klar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2"