{"textgrid.poem.62384": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Der Durlacher Thurm", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es lacht die gr\u00fcne Wiese,", "tokens": ["Es", "lacht", "die", "gr\u00fc\u00b7ne", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es lockt der Sonnenstrahl;", "tokens": ["Es", "lockt", "der", "Son\u00b7nen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom H\u00fcgel schaut ein Riese", "tokens": ["Vom", "H\u00fc\u00b7gel", "schaut", "ein", "Rie\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins liebe gr\u00fcne Thal.", "tokens": ["Ins", "lie\u00b7be", "gr\u00fc\u00b7ne", "Thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein edler Heldenschatten", "tokens": ["Ein", "ed\u00b7ler", "Hel\u00b7den\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat sich der Thurm gebaut,", "tokens": ["Hat", "sich", "der", "Thurm", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rings die hellen Matten", "tokens": ["Und", "rings", "die", "hel\u00b7len", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich liebend angetraut.", "tokens": ["Sich", "lie\u00b7bend", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es stehn die alten W\u00e4chter", "tokens": ["Es", "stehn", "die", "al\u00b7ten", "W\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort wo die Wolken ziehn,", "tokens": ["Dort", "wo", "die", "Wol\u00b7ken", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schauen die Geschlechter", "tokens": ["Und", "schau\u00b7en", "die", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erstehen und verbl\u00fchn.", "tokens": ["Er\u00b7ste\u00b7hen", "und", "ver\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "KON", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Monden ziehn vor\u00fcber,", "tokens": ["Die", "Mon\u00b7den", "ziehn", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor\u00fcber manches Jahr,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie denken immer tr\u00fcber", "tokens": ["Sie", "den\u00b7ken", "im\u00b7mer", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An das, was vormals war.", "tokens": ["An", "das", ",", "was", "vor\u00b7mals", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch steigt nach jener Mauer", "tokens": ["Doch", "steigt", "nach", "je\u00b7ner", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein z\u00e4rtlich liebend Paar,", "tokens": ["Ein", "z\u00e4rt\u00b7lich", "lie\u00b7bend", "Paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Heldenliebestrauer!", "tokens": ["O", "Hel\u00b7den\u00b7lie\u00b7bes\u00b7trau\u00b7er", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann wirst du mild und klar.", "tokens": ["Dann", "wirst", "du", "mild", "und", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann scheinen die Gestalten", "tokens": ["Dann", "schei\u00b7nen", "die", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Liebenden erhellt", "tokens": ["Der", "Lie\