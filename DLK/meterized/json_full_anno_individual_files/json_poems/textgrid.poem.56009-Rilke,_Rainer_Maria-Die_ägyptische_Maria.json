{"textgrid.poem.56009": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Die \u00e4gyptische Maria", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit sie damals, betthei\u00df, als die Hure", "tokens": ["Seit", "sie", "da\u00b7mals", ",", "bett\u00b7hei\u00df", ",", "als", "die", "Hu\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00fcbern Jordan floh und, wie ein Grab", "tokens": ["\u00fc\u00b7bern", "Jor\u00b7dan", "floh", "und", ",", "wie", "ein", "Grab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "KON", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "gebend, stark und unvermischt das pure", "tokens": ["ge\u00b7bend", ",", "stark", "und", "un\u00b7ver\u00b7mischt", "das", "pu\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "ADJD", "KON", "ADJD", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Herz der Ewigkeit zu trinken gab,", "tokens": ["Herz", "der", "E\u00b7wig\u00b7keit", "zu", "trin\u00b7ken", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "wuchs ihr fr\u00fches Hingegebensein", "tokens": ["wuchs", "ihr", "fr\u00fc\u00b7hes", "Hin\u00b7ge\u00b7ge\u00b7ben\u00b7sein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measur