{"textgrid.poem.55319": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Epirrhema", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00fcsset im Naturbetrachten", "tokens": ["M\u00fcs\u00b7set", "im", "Na\u00b7tur\u00b7be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Immer eins wie alles achten;", "tokens": ["Im\u00b7mer", "eins", "wie", "al\u00b7les", "ach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts ist drinnen, nichts ist drau\u00dfen:", "tokens": ["Nichts", "ist", "drin\u00b7nen", ",", "nichts", "ist", "drau\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "$,", "PIS", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn was innen, das ist au\u00dfen.", "tokens": ["Denn", "was", "in\u00b7nen", ",", "das", "ist", "au\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So ergreifet ohne S\u00e4umnis", "tokens": ["So", "er\u00b7grei\u00b7fet", "oh\u00b7ne", "S\u00e4um\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heilig \u00f6ffentlich Geheimnis.", "tokens": ["Hei\u00b7lig", "\u00f6f\u00b7fent\u00b7lich", "Ge\u00b7heim\u00b7nis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJD", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Freuet euch des wahren Scheins,", "tokens": ["Freu\u00b7et", "euch", "des", "wah\u00b7ren", "Scheins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch des ernsten Spieles:", "tokens": ["Euch", "des", "erns\u00b7ten", "Spie\u00b7les", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kein Lebendiges ist ein Eins,", "tokens": ["Kein", "Le\u00b7ben\u00b7di\u00b7ges", "ist", "ein", "Eins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Immer ist's ein Vieles.", "tokens": ["Im\u00b7mer", "ist's", "ein", "Vie\u00b7les", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "M\u00fcsset im Naturbetrachten", "tokens": ["M\u00fcs\u00b7set", "im", "Na\u00b7tur\u00b7be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Immer eins wie alles achten;", "tokens": ["Im\u00b7mer", "eins", "wie", "al\u00b7les", "ach\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts ist drinnen, nichts ist drau\u00dfen:", "tokens": ["Nichts", "ist", "drin\u00b7nen", ",", "nichts", "ist", "drau\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word"