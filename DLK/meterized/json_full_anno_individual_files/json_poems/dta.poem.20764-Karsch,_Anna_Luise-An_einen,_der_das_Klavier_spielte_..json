{"dta.poem.20764": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An einen, der das Klavier spielte .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was h\u00f6r ich? Ist Apoll zu dir herabgestiegen               ", "tokens": ["Was", "h\u00f6r", "ich", "?", "Ist", "A\u00b7poll", "zu", "dir", "her\u00b7ab\u00b7ge\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VAFIN", "NE", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ordnet deiner Triller Lauf?", "tokens": ["Und", "ord\u00b7net", "dei\u00b7ner", "Tril\u00b7ler", "Lauf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So lernt ein junger Adler fliegen", "tokens": ["So", "lernt", "ein", "jun\u00b7ger", "Ad\u00b7ler", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Alten nach, der oft herauf", "tokens": ["Dem", "Al\u00b7ten", "nach", ",", "der", "oft", "her\u00b7auf"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu Ph\u00f6bus Wagen flog, und dem Olympus nah", "tokens": ["Zu", "Ph\u00f6\u00b7bus", "Wa\u00b7gen", "flog", ",", "und", "dem", "O\u00b7lym\u00b7pus", "nah"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,", "KON", "ART", "NE", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den Vogel Jupiters, den Pfau der Juno sah.", "tokens": ["Den", "Vo\u00b7gel", "Ju\u00b7pi\u00b7ters", ",", "den", "Pfau", "der", "Ju\u00b7no", "sah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$,", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}