{"dta.poem.11564": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Lied eines Verliebten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Weckt mich mein Herz, an dich zu denken", "tokens": ["Weckt", "mich", "mein", "Herz", ",", "an", "dich", "zu", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da doch gesunde Jugend schlafen mag.", "tokens": ["Da", "doch", "ge\u00b7sun\u00b7de", "Ju\u00b7gend", "schla\u00b7fen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hell ist mein Aug' um Mitternacht,", "tokens": ["Hell", "ist", "mein", "Aug'", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heller als fr\u00fche Morgenglocken:", "tokens": ["Hel\u00b7ler", "als", "fr\u00fc\u00b7he", "Mor\u00b7gen\u00b7glo\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wann h\u00e4tt'st du je am Tage mein gedacht?", "tokens": ["Wann", "h\u00e4tt'st", "du", "je", "am", "Ta\u00b7ge", "mein", "ge\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "W\u00e4r' ich ein Fischer, st\u00fcnd' ich auf,", "tokens": ["W\u00e4r'", "ich", "ein", "Fi\u00b7scher", ",", "st\u00fcnd'", "ich", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00fcge mein Netz hinab zum Flusse,", "tokens": ["Tr\u00fc\u00b7ge", "mein", "Netz", "hin\u00b7ab", "zum", "Flus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Tr\u00fcg' herzlich froh die Fische zum Verkauf.", "tokens": ["Tr\u00fcg'", "herz\u00b7lich", "froh", "die", "Fi\u00b7sche", "zum", "Ver\u00b7kauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In der M\u00fchle, bei Licht, der M\u00fcllerknecht", "tokens": ["In", "der", "M\u00fch\u00b7le", ",", "bei", "Licht", ",", "der", "M\u00fcl\u00b7ler\u00b7knecht"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "NN", "$,", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Tummelt sich, alle G\u00e4nge klappern;", "tokens": ["Tum\u00b7melt", "sich", ",", "al\u00b7le", "G\u00e4n\u00b7ge", "klap\u00b7pern", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "PIAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So r\u00fcstig Treiben w\u00e4r' mir eben recht!", "tokens": ["So", "r\u00fcs\u00b7tig", "Trei\u00b7ben", "w\u00e4r'", "mir", "e\u00b7ben", "recht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Weh, aber ich! o armer Tropf!", "tokens": ["Weh", ",", "a\u00b7ber", "ich", "!", "o", "ar\u00b7mer", "Tropf", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df auf dem Lager mich m\u00fc\u00dfig gr\u00e4men,", "tokens": ["Mu\u00df", "auf", "dem", "La\u00b7ger", "mich", "m\u00fc\u00b7\u00dfig", "gr\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein ungeb\u00e4rdig Mutterkind im Kopf!", "tokens": ["Ein", "un\u00b7ge\u00b7b\u00e4r\u00b7dig", "Mut\u00b7ter\u00b7kind", "im", "Kopf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}