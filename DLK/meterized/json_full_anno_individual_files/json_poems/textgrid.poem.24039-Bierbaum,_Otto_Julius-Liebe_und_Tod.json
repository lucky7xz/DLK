{"textgrid.poem.24039": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Liebe und Tod", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwischen Rosenranken steht der kleine Gott,", "tokens": ["Zwi\u00b7schen", "Ro\u00b7sen\u00b7ran\u00b7ken", "steht", "der", "klei\u00b7ne", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Nackt im Fleische seiner s\u00fc\u00dfen Lust", "tokens": ["Nackt", "im", "Flei\u00b7sche", "sei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Vor dem Haus, dem er sein Gl\u00fcck beschert.", "tokens": ["Vor", "dem", "Haus", ",", "dem", "er", "sein", "Gl\u00fcck", "be\u00b7schert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Kommt die Todesg\u00f6ttin, gr\u00fcnlich wei\u00df", "tokens": ["Kommt", "die", "To\u00b7des\u00b7g\u00f6t\u00b7tin", ",", "gr\u00fcn\u00b7lich", "wei\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Ueberschleiert, lakeneingeh\u00fcllt,", "tokens": ["Ue\u00b7ber\u00b7schlei\u00b7ert", ",", "la\u00b7ken\u00b7ein\u00b7ge\u00b7h\u00fcllt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hebt den Arm zum Thor und will hinein.", "tokens": ["Hebt", "den", "Arm", "zum", "Thor", "und", "will", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "KON", "VMFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbach, in meine Rosen schreite nicht!\u00ab", "tokens": ["\u00bb", "ach", ",", "in", "mei\u00b7ne", "Ro\u00b7sen", "schrei\u00b7te", "nicht", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wehrt der Gott, \u00bbich rankte sie ums Haus,", "tokens": ["Wehrt", "der", "Gott", ",", "\u00bb", "ich", "rank\u00b7te", "sie", "ums", "Haus", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "$(", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn es heimt junghei\u00dfe Liebe drin.\u00ab", "tokens": ["Denn", "es", "heimt", "jung\u00b7hei\u00b7\u00dfe", "Lie\u00b7be", "drin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Doch die G\u00f6ttin mit gesenktem Haupt", "tokens": ["Doch", "die", "G\u00f6t\u00b7tin", "mit", "ge\u00b7senk\u00b7tem", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Hebt den starken Arm ... Die Th\u00fcre kreischt,", "tokens": ["Hebt", "den", "star\u00b7ken", "Arm", "...", "Die", "Th\u00fc\u00b7re", "kreischt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Rosen, eben aufgebl\u00fcht,", "tokens": ["Und", "die", "Ro\u00b7sen", ",", "e\u00b7ben", "auf\u00b7ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Fallen ab vom Stamm.", "tokens": ["Fal\u00b7len", "ab", "vom", "Stamm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Die Stille klagt.", "tokens": ["Die", "Stil\u00b7le", "klagt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "In die nackten Rosenranken weint der Gott.", "tokens": ["In", "die", "nack\u00b7ten", "Ro\u00b7sen\u00b7ran\u00b7ken", "weint", "der", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Zwischen Rosenranken steht der kleine Gott,", "tokens": ["Zwi\u00b7schen", "Ro\u00b7sen\u00b7ran\u00b7ken", "steht", "der", "klei\u00b7ne", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Nackt im Fleische seiner s\u00fc\u00dfen Lust", "tokens": ["N