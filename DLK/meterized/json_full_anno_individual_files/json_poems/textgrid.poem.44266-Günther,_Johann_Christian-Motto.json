{"textgrid.poem.44266": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "Motto", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So aber sucht man ihm die Wege vorzuschreiben:", "tokens": ["So", "a\u00b7ber", "sucht", "man", "ihm", "die", "We\u00b7ge", "vor\u00b7zu\u00b7schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem soll ich Rolands Stamm in einen Vierzling treiben,", "tokens": ["Dem", "soll", "ich", "Ro\u00b7lands", "Stamm", "in", "ei\u00b7nen", "Vierz\u00b7ling", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "NE", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der fodert auf ein Blat den ganzen Dichterkram ...", "tokens": ["Der", "fo\u00b7dert", "auf", "ein", "Blat", "den", "gan\u00b7zen", "Dich\u00b7ter\u00b7kram", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem soll ich den Papa in jede Strophe thun,", "tokens": ["Dem", "soll", "ich", "den", "Pa\u00b7pa", "in", "je\u00b7de", "Stro\u00b7phe", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der bettelt, geht und kommt und kan vor Angst nicht ruhn,", "tokens": ["Der", "bet\u00b7telt", ",", "geht", "und", "kommt", "und", "kan", "vor", "Angst", "nicht", "ruhn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "KON", "VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bis da\u00df ich Flavien erbermlich vorgeleyret,", "tokens": ["Bis", "da\u00df", "ich", "Fla\u00b7vi\u00b7en", "er\u00b7berm\u00b7lich", "vor\u00b7ge\u00b7ley\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie, da sie gestern sp\u00e4t das Sonntagszinn gescheuret,", "tokens": ["Wie", ",", "da", "sie", "ge\u00b7stern", "sp\u00e4t", "das", "Sonn\u00b7tags\u00b7zinn", "ge\u00b7scheu\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Ihr aufgestreifter Arm die Schwanenhaut entbl\u00f6st", "tokens": ["Ihr", "auf\u00b7ge\u00b7streif\u00b7ter", "Arm", "die", "Schwa\u00b7nen\u00b7haut", "ent\u00b7bl\u00f6st"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und ihm dadurch die Milch der Hofnung eingefl\u00f6st,", "tokens": ["Und", "ihm", "da\u00b7durch", "die", "Milch", "der", "Hof\u00b7nung", "ein\u00b7ge\u00b7fl\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Daher in seiner Brust ein neuer Aetna brennte,", "tokens": ["Da\u00b7her", "in", "sei\u00b7ner", "Brust", "ein", "neu\u00b7er", "A\u00b7et\u00b7na", "brenn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Dem selbst ihr Sch\u00fc\u00dfelfa\u00df die Glut nicht l\u00f6schen k\u00f6nte.", "tokens": ["Dem", "selbst", "ihr", "Sch\u00fc\u00b7\u00dfel\u00b7fa\u00df", "die", "Glut", "nicht", "l\u00f6\u00b7schen", "k\u00f6n\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So aber sucht man ihm die Wege vorzuschreiben:", "tokens": ["So", "a\u00b7ber", "sucht", "man", "ihm", "die", "We\u00b7ge", "vor\u00b7zu\u00b7schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem soll ich Rolands Stamm in einen Vierzling treiben,", "tokens": ["Dem", "soll", "ich", "Ro\u00b7lands", "Stamm", "in", "ei\u00b7nen", "Vierz\u00b7ling", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "NE", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der fodert auf ein Blat den ganzen Dichterkram ...", "tokens": ["