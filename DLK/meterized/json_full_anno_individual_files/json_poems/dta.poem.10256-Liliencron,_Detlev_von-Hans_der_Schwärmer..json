{"dta.poem.10256": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Hans der Schw\u00e4rmer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Hans T\u00f6ffel liebt Sch\u00f6n Doris sehr,             ", "tokens": ["Hans", "T\u00f6f\u00b7fel", "liebt", "Sch\u00f6n", "Do\u00b7ris", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NE", "NE", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00f6n Doris Hans T\u00f6ffel vielleicht noch mehr.", "tokens": ["Sch\u00f6n", "Do\u00b7ris", "Hans", "T\u00f6f\u00b7fel", "viel\u00b7leicht", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Doch seine Liebe, ich wei\u00df nicht wie,", "tokens": ["Doch", "sei\u00b7ne", "Lie\u00b7be", ",", "ich", "wei\u00df", "nicht", "wie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ist zu scheu, zu sch\u00fcchtern, zu viel Elegie.", "tokens": ["Ist", "zu", "scheu", ",", "zu", "sch\u00fcch\u00b7tern", ",", "zu", "viel", "E\u00b7le\u00b7gie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "$,", "PTKZU", "VVINF", "$,", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Im Kreise liest er Gedichte vor,", "tokens": ["Im", "Krei\u00b7se", "liest", "er", "Ge\u00b7dich\u00b7te", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sch\u00f6n Doris steht unten am Gartenthor:", "tokens": ["Sch\u00f6n", "Do\u00b7ris", "steht", "un\u00b7ten", "am", "Gar\u00b7ten\u00b7thor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ach, k\u00e4m\u2019 er doch frisch zu mir hergesprungen,", "tokens": ["Ach", ",", "k\u00e4m'", "er", "doch", "frisch", "zu", "mir", "her\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Wie wollt\u2019 ich ihn herzen, den lieben Jungen.", "tokens": ["Wie", "wollt'", "ich", "ihn", "her\u00b7zen", ",", "den", "lie\u00b7ben", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Am andern Abend, der bl\u00f6de Thor,", "tokens": ["Am", "an\u00b7dern", "A\u00b7bend", ",", "der", "bl\u00f6\u00b7de", "Thor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hans T\u00f6ffel tr\u00e4gt wieder Gedichte vor.", "tokens": ["Hans", "T\u00f6f\u00b7fel", "tr\u00e4gt", "wie\u00b7der", "Ge\u00b7dich\u00b7te", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sch\u00f6n Doris das wirklich sehr verdrie\u00dft,", "tokens": ["Sch\u00f6n", "Do\u00b7ris", "das", "wirk\u00b7lich", "sehr", "ver\u00b7drie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df er immer weiter und weiter liest.", "tokens": ["Da\u00df", "er", "im\u00b7mer", "wei\u00b7ter", "und", "wei\u00b7ter", "liest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Sie schleicht sich hinaus, er gewahrt es nicht,", "tokens": ["Sie", "schleicht", "sich", "hin\u00b7aus", ",", "er", "ge\u00b7wahrt", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APZR", "$,", "PPER", "VVPP", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Just sagt er von Heine ein herrlich Gedicht.", "tokens": ["Just", "sagt", "er", "von", "Hei\u00b7ne", "ein", "herr\u00b7lich", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NE", "ART", "ADJD", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Sch\u00f6n Doris steht unten in Rosend\u00fcften", "tokens": ["Sch\u00f6n", "Do\u00b7ris", "steht", "un\u00b7ten", "in", "Ro\u00b7sen\u00b7d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.8": {"text": "Und h\u00e4tte so gern seinen Arm um die H\u00fcften.", "tokens": ["Und", "h\u00e4t\u00b7te", "so", "gern", "sei\u00b7nen", "Arm", "um", "die", "H\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Am andern Abend ist gro\u00dfes Fest,", "tokens": ["Am", "an\u00b7dern", "A\u00b7bend", "ist", "gro\u00b7\u00dfes", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Viel Menschen sind eng aneinander gepre\u00dft.", "tokens": ["Viel", "Men\u00b7schen", "sind", "eng", "an\u00b7ein\u00b7an\u00b7der", "ge\u00b7pre\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Heut mu\u00df er\u2019s doch endlich sehn der Poet,", "tokens": ["Heut", "mu\u00df", "er's", "doch", "end\u00b7lich", "sehn", "der", "Po\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn Sch\u00f6n Doris sacht aus der Th\u00fcre geht.", "tokens": ["Wenn", "Sch\u00f6n", "Do\u00b7ris", "sacht", "aus", "der", "Th\u00fc\u00b7re", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Potz Tausend, er merkt es und merkt es auch nicht,", "tokens": ["Potz", "Tau\u00b7send", ",", "er", "merkt", "es", "und", "merkt", "es", "auch", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Er spricht und verzapft gar ein eigen Gedicht.", "tokens": ["Er", "spricht", "und", "ver\u00b7zapft", "gar", "ein", "ei\u00b7gen", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und unten im stillen, dunklen Garten", "tokens": ["Und", "un\u00b7ten", "im", "stil\u00b7len", ",", "dunk\u00b7len", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mu\u00df Sch\u00f6n Doris vergeblich, vergeblich warten.", "tokens": ["Mu\u00df", "Sch\u00f6n", "Do\u00b7ris", "ver\u00b7geb\u00b7lich", ",", "ver\u00b7geb\u00b7lich", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "NE", "NE", "ADJD", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Am andern Abend, beim heiligen Gral,", "tokens": ["Am", "an\u00b7dern", "A\u00b7bend", ",", "beim", "hei\u00b7li\u00b7gen", "Gral", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sch\u00f6n Doris fehlt im Gesellschaftssaal.", "tokens": ["Sch\u00f6n", "Do\u00b7ris", "fehlt", "im", "Ge\u00b7sell\u00b7schafts\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und ist auch Hans T\u00f6ffel mein Freund und mir wert \u2014", "tokens": ["Und", "ist", "auch", "Hans", "T\u00f6f\u00b7fel", "mein", "Freund", "und", "mir", "wert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "NE", "NE", "PPOSAT", "NN", "KON", "PPER", "ADJD", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die Katze schl\u00e4ft unten am Feuerherd,", "tokens": ["Die", "Kat\u00b7ze", "schl\u00e4ft", "un\u00b7ten", "am", "Feu\u00b7er\u00b7herd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Beim K\u00e4tzchen steht sinnend Sch\u00f6n Doris und sehnt,", "tokens": ["Beim", "K\u00e4tz\u00b7chen", "steht", "sin\u00b7nend", "Sch\u00f6n", "Do\u00b7ris", "und", "sehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "NE", "NE", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ihr K\u00f6pfchen an meiner Schulter lehnt.", "tokens": ["Ihr", "K\u00f6pf\u00b7chen", "an", "mei\u00b7ner", "Schul\u00b7ter", "lehnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und h\u00e4tt\u2019 ich auch eine Legion Verdammer,", "tokens": ["Und", "h\u00e4tt'", "ich", "auch", "ei\u00b7ne", "Le\u00b7gi\u00b7on", "Ver\u00b7dam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Zu s\u00fc\u00df war die Stunde bei ihr in der Kammer.", "tokens": ["Zu", "s\u00fc\u00df", "war", "die", "Stun\u00b7de", "bei", "ihr", "in", "der", "Kam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}}}}