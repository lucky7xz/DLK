, "sehn\u00b7lich", "mit", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In mir nach Ruhe strebt.", "tokens": ["In", "mir", "nach", "Ru\u00b7he", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Bey deiner K\u00f6rner Essen", "tokens": ["Bey", "dei\u00b7ner", "K\u00f6r\u00b7ner", "Es\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Wasser, h\u00fcpftest du;", "tokens": ["Und", "Was\u00b7ser", ",", "h\u00fcpf\u00b7test", "du", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel wird mir zugemessen:", "tokens": ["Viel", "wird", "mir", "zu\u00b7ge\u00b7mes\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich fordre mehr dazu.", "tokens": ["Ich", "ford\u00b7re", "mehr", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Das Gl\u00fcck, das ich schon habe,", "tokens": ["Das", "Gl\u00fcck", ",", "das", "ich", "schon", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist meinem Geist zu klein.", "tokens": ["Ist", "mei\u00b7nem", "Geist", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr ihn mu\u00df \u00fcberm Grabe", "tokens": ["F\u00fcr", "ihn", "mu\u00df", "\u00fc\u00b7berm", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehr Gl\u00fcck, mehr Ruhe seyn.", "tokens": ["Mehr", "Gl\u00fcck", ",", "mehr", "Ru\u00b7he", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}