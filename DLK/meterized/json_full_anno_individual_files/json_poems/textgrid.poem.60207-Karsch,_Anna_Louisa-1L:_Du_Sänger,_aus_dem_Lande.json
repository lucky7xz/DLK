{"textgrid.poem.60207": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du S\u00e4nger, aus dem Lande", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du S\u00e4nger, aus dem Lande", "tokens": ["Du", "S\u00e4n\u00b7ger", ",", "aus", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das feinen Zucker zeugt,", "tokens": ["Das", "fei\u00b7nen", "Zu\u00b7cker", "zeugt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erstarrt liegst du im Sande,", "tokens": ["Er\u00b7starrt", "liegst", "du", "im", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und deine Kehle schweigt!", "tokens": ["Und", "dei\u00b7ne", "Keh\u00b7le", "schweigt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dir klopfte viele Tage", "tokens": ["Dir", "klopf\u00b7te", "vie\u00b7le", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit ungest\u00fcmem Schmerz", "tokens": ["Mit", "un\u00b7ge\u00b7st\u00fc\u00b7mem", "Schmerz"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wiederhohltem Schlage", "tokens": ["Und", "wie\u00b7der\u00b7hohl\u00b7tem", "Schla\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Tod aus kleine Herz!", "tokens": ["Der", "Tod", "aus", "klei\u00b7ne", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In tiefer Todes-Stille", "tokens": ["In", "tie\u00b7fer", "To\u00b7des\u00b7Stil\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Befand dein H\u00e4uschen sich,", "tokens": ["Be\u00b7fand", "dein", "H\u00e4usc\u00b7hen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df, auch der kleinste Wille,", "tokens": ["Da\u00df", ",", "auch", "der", "kleins\u00b7te", "Wil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Singen dir entwich.", "tokens": ["Zum", "Sin\u00b7gen", "dir", "ent\u00b7wich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mit kl\u00e4glichem Geschreye", "tokens": ["Mit", "kl\u00e4g\u00b7li\u00b7chem", "Ge\u00b7schre\u00b7ye"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im andern Bauer rief", "tokens": ["Im", "an\u00b7dern", "Bau\u00b7er", "rief"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich deines Freundes Treue,", "tokens": ["Dich", "dei\u00b7nes", "Freun\u00b7des", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn fr\u00fch noch alles schlief.", "tokens": ["Wenn", "fr\u00fch", "noch", "al\u00b7les", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du starbst, geliebter Kleiner,", "tokens": ["Du", "starbst", ",", "ge\u00b7lieb\u00b7ter", "Klei\u00b7ner", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von deiner Frau beklagt!", "tokens": ["Von", "dei\u00b7ner", "Frau", "be\u00b7klagt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da von den V\u00f6geln keiner", "tokens": ["Da", "von", "den", "V\u00f6\u00b7geln", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach deinem Grabe fragt,", "tokens": ["Nach", "dei\u00b7nem", "Gra\u00b7be", "fragt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Da weint sie bittre Z\u00e4hren,", "tokens": ["Da", "weint", "sie", "bitt\u00b7re", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu kostbar, Vogel, dir!", "tokens": ["Zu", "kost\u00b7bar", ",", "Vo\u00b7gel", ",", "dir", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "NE", "$,", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn W\u00fcrmer mich verzehren,", "tokens": ["Wenn", "W\u00fcr\u00b7mer", "mich", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weint sie auch \u00fcber mir.", "tokens": ["Weint", "sie", "auch", "\u00fc\u00b7ber", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auf meine Asche nieder", "tokens": ["Auf", "mei\u00b7ne", "A\u00b7sche", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weint meiner Freunde Leid;", "tokens": ["Weint", "mei\u00b7ner", "Freun\u00b7de", "Leid", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie klagen meine Lieder", "tokens": ["Sie", "kla\u00b7gen", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz voll Z\u00e4rtlichkeit.", "tokens": ["Mein", "Herz", "voll", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich singe, wie du sangest", "tokens": ["Ich", "sin\u00b7ge", ",", "wie", "du", "san\u00b7gest"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach t\u00e4glichem Gebrauch,", "tokens": ["Nach", "t\u00e4g\u00b7li\u00b7chem", "Ge\u00b7brauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was du itzt erlangest,", "tokens": ["Und", "was", "du", "itzt", "er\u00b7lan\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erlang ich k\u00fcnftig auch.", "tokens": ["Er\u00b7lang", "ich", "k\u00fcnf\u00b7tig", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Den Staub auf dich gebreitet,", "tokens": ["Den", "Staub", "auf", "dich", "ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wirft man auch \u00fcber mich,", "tokens": ["Wirft", "man", "auch", "\u00fc\u00b7ber", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Grab, mehr ausgeweitet", "tokens": ["Mein", "Grab", ",", "mehr", "aus\u00b7ge\u00b7wei\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als deines, \u00f6fnet sich", "tokens": ["Als", "dei\u00b7nes", ",", "\u00f6f\u00b7net", "sich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPOSAT", "$,", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Den C\u00f6rper zu empfangen,", "tokens": ["Den", "C\u00f6r\u00b7per", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den jetzt ein Geist belebt,", "tokens": ["Den", "jetzt", "ein", "Geist", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sehnlich mit Verlangen,", "tokens": ["Der", "sehn\u00b7lich", "mit", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In mir nach Ruhe strebt.", "tokens": ["In", "mir", "nach", "Ru\u00b7he", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Bey deiner K\u00f6rner Essen", "tokens": ["Bey", "dei\u00b7ner", "K\u00f6r\u00b7ner", "Es\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Wasser, h\u00fcpftest du;", "tokens": ["Und", "Was\u00b7ser", ",", "h\u00fcpf\u00b7test", "du", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel wird mir zugemessen:", "tokens": ["Viel", "wird", "mir", "zu\u00b7ge\u00b7mes\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich fordre mehr dazu.", "tokens": ["Ich", "ford\u00b7re", "mehr", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Das Gl\u00fcck, das ich schon habe,", "tokens": ["Das", "Gl\u00fcck", ",", "das", "ich", "schon", "ha\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist meinem Geist zu klein.", "tokens": ["Ist", "mei\u00b7nem", "Geist", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr ihn mu\u00df \u00fcberm Grabe", "tokens": ["F\u00fcr", "ihn", "mu\u00df", "\u00fc\u00b7berm", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mehr Gl\u00fcck, mehr Ruhe seyn.", "tokens": ["Mehr", "Gl\u00fcck", ",", "mehr", "Ru\u00b7he", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Du S\u00e4nger, aus dem Lande", "tokens": ["Du", "S\u00e4n\u00b7ger", ",", "aus", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das feinen Zucker zeugt,", "tokens": ["Das", "fei\u00b7nen", "Zu\u00b7cker", "zeugt", ","], "token_info": ["word", "word", "word", "word", "pu