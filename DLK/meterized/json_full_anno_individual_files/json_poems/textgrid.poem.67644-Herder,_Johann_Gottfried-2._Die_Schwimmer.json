{"textgrid.poem.67644": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2. Die Schwimmer", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Leben ist ein st\u00fcrmisch Meer;", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "st\u00fcr\u00b7misch", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir schweben hin, wir schweben her,", "tokens": ["Wir", "schwe\u00b7ben", "hin", ",", "wir", "schwe\u00b7ben", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir streben schwer durchs Leben!", "tokens": ["Wir", "stre\u00b7ben", "schwer", "durchs", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Thor, so wirf die B\u00fcrden schwer,", "tokens": ["O", "Thor", ",", "so", "wirf", "die", "B\u00fcr\u00b7den", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sorgenb\u00fcrden wirf ins Meer!", "tokens": ["Die", "Sor\u00b7gen\u00b7b\u00fcr\u00b7den", "wirf", "ins", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie leichter, nacket sterben!", "to