{"textgrid.poem.42756": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[\u00bbOh\u00ab, rief ein Glas Burgunder]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bboh\u00ab, rief ein Glas Burgunder,", "tokens": ["\u00bb", "oh", "\u00ab", ",", "rief", "ein", "Glas", "Bur\u00b7gun\u00b7der", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$(", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "\u00bboh, Mond, du g\u00f6ttliches Wunder!", "tokens": ["\u00bb", "oh", ",", "Mond", ",", "du", "g\u00f6tt\u00b7li\u00b7ches", "Wun\u00b7der", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du gie\u00dft aus silberner Schale", "tokens": ["Du", "gie\u00dft", "aus", "sil\u00b7ber\u00b7ner", "Scha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das liebestaumelnde, fahle,", "tokens": ["Das", "lie\u00b7be\u00b7stau\u00b7meln\u00b7de", ",", "fah\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Trunkene Licht wie sengende Glut", "tokens": ["Trun\u00b7ke\u00b7ne", "Licht", "wie", "sen\u00b7gen\u00b7de", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KOKOM", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Hin \u00fcber da