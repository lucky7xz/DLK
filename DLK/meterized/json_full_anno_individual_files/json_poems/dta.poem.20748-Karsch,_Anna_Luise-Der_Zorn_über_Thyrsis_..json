{"dta.poem.20748": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der Zorn \u00fcber Thyrsis .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bei dem Apoll schw\u00f6r ich: mich hat nicht Thyrsis lieb,                 ", "tokens": ["Bei", "dem", "A\u00b7poll", "schw\u00f6r", "ich", ":", "mich", "hat", "nicht", "Thyr\u00b7sis", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "PTKNEG", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er lauschet auf mein Lied begierig wie ein Dieb,", "tokens": ["Er", "lau\u00b7schet", "auf", "mein", "Lied", "be\u00b7gie\u00b7rig", "wie", "ein", "Dieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verspricht zu kommen her und wei\u00df nicht Wort zu", "tokens": ["Ver\u00b7spricht", "zu", "kom\u00b7men", "her", "und", "wei\u00df", "nicht", "Wort", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "PTKVZ", "KON", "VVFIN", "PTKNEG", "NN", "PTKZU"], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.4": {"text": "halten,", "tokens": ["hal\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Bald nenn ich aufgebracht ihn zornig einen kalten", "tokens": ["Bald", "nenn", "ich", "auf\u00b7ge\u00b7bracht", "ihn", "zor\u00b7nig", "ei\u00b7nen", "kal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Empfindungslosen Freund und einen harten Mann;", "tokens": ["Emp\u00b7fin\u00b7dungs\u00b7lo\u00b7sen", "Freund", "und", "ei\u00b7nen", "har\u00b7ten", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bald nehm ichs heimlich vor, mein Herz halb abzu-", "tokens": ["Bald", "nehm", "ichs", "heim\u00b7lich", "vor", ",", "mein", "Herz", "halb", "ab\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "trennen", "tokens": ["tren\u00b7nen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Von dem Undankbaren, nicht mehr f\u00fcr ihn zu", "tokens": ["Von", "dem", "Un\u00b7dank\u00b7ba\u00b7ren", ",", "nicht", "mehr", "f\u00fcr", "ihn", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PTKNEG", "ADV", "APPR", "PPER", "PTKZU"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "brennen,", "tokens": ["bren\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Und \u00e4rgern soll er sich alsdann,", "tokens": ["Und", "\u00e4r\u00b7gern", "soll", "er", "sich", "als\u00b7dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn ihn, den oft mein Lied zu G\u00f6ttern hat erhoben,", "tokens": ["Wenn", "ihn", ",", "den", "oft", "mein", "Lied", "zu", "G\u00f6t\u00b7tern", "hat", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADV", "PPOSAT", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn ihn nicht mein Gesang wird loben.", "tokens": ["Wenn", "ihn", "nicht", "mein", "Ge\u00b7sang", "wird", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Doch jezt emp\u00f6ret sich in mir mein Herz zu sehr:", "tokens": ["Doch", "jezt", "em\u00b7p\u00f6\u00b7ret", "sich", "in", "mir", "mein", "Herz", "zu", "sehr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "PPER", "PPOSAT", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Graf! sag ihm nichts von Zorn, ich z\u00fcrne schon nicht", "tokens": ["Graf", "!", "sag", "ihm", "nichts", "von", "Zorn", ",", "ich", "z\u00fcr\u00b7ne", "schon", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "PPER", "PIS", "APPR", "NN", "$,", "PPER", "VVFIN", "ADV", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "mehr.", "tokens": ["mehr", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}}}}}