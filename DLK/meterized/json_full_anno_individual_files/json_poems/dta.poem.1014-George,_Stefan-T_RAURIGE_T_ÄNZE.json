{"dta.poem.1014": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ob schwerer nebel in den w&#228;ldern h&#228;ngt               ", "tokens": ["Ob", "schwe\u00b7rer", "ne\u00b7bel", "in", "den", "w", "&#228;", "ldern", "h", "&#228;", "ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "ADJA", "NN", "APPR", "ART", "NE", "$(", "VVINF", "VVFIN", "$(", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Du sollst im weiterschreiten drum nicht zaudern               ", "tokens": ["Du", "sollst", "im", "wei\u00b7ter\u00b7schrei\u00b7ten", "drum", "nicht", "zau\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "VVFIN", "PAV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sprich mit den bleichen bildern ohne schaudern               ", "tokens": ["Sprich", "mit", "den", "blei\u00b7chen", "bil\u00b7dern", "oh\u00b7ne", "schau\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN", "APPR", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon regen sie sich sacht hinangedr&#228;ngt             ", "tokens": ["Schon", "re\u00b7gen", "sie", "sich", "sacht", "hin\u00b7an\u00b7ge\u00b7dr", "&#228;", "ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ADV", "ADJA", "PPER", "PRF", "VVFIN", "ADV", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn gras und furche auf dem pfad versteinen               ", "tokens": ["Wenn", "gras", "und", "fur\u00b7che", "auf", "dem", "pfad", "ver\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "ADJA", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geh&#228;ufter reif die wipfel beugt versteh               ", "tokens": ["Geh", "&#228;", "uf\u00b7ter", "reif", "die", "wip\u00b7fel", "beugt", "ver\u00b7steh"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "ADJD", "ADJD", "ART", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu lauschen auf der winterwinde weh               ", "tokens": ["Zu", "lau\u00b7schen", "auf", "der", "win\u00b7ter\u00b7win\u00b7de", "weh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die mit den welken einsamkeiten weinen             ", "tokens": ["Die", "mit", "den", "wel\u00b7ken", "ein\u00b7sam\u00b7kei\u00b7ten", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So h&#228;ltst du immer wach die m&#252;de stirn               ", "tokens": ["So", "h", "&#228;", "ltst", "du", "im\u00b7mer", "wach", "die", "m", "&#252;", "de", "stirn"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "VVFIN", "PPER", "ADV", "ADJD", "ART", "FM", "FM", "FM", "FM"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und gleitest nicht herab von steiler b\u00f6sche               ", "tokens": ["Und", "glei\u00b7test", "nicht", "her\u00b7ab", "von", "stei\u00b7ler", "b\u00f6\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ob auch das matt erhellte ziel verl\u00f6sche               ", "tokens": ["Ob", "auch", "das", "matt", "er\u00b7hell\u00b7te", "ziel", "ver\u00b7l\u00f6\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJD", "VVFIN", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und &#252;ber dir das einzige gestirn.             ", "tokens": ["Und", "&#252;", "ber", "dir", "das", "ein\u00b7zi\u00b7ge", "ge\u00b7stirn", "."], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}}}}}