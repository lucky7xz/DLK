{"textgrid.poem.63313": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "B\u00fcrgerliches Weihnachtsidyll", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was bringt der Weihnachtsmann Emilien?", "tokens": ["Was", "bringt", "der", "Weih\u00b7nachts\u00b7mann", "E\u00b7mi\u00b7li\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Strau\u00df von Rosmarin und Lilien.", "tokens": ["Ein", "Strau\u00df", "von", "Ros\u00b7ma\u00b7rin", "und", "Li\u00b7li\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie geht so flei\u00dfig auf den Strich.", "tokens": ["Sie", "geht", "so", "flei\u00b7\u00dfig", "auf", "den", "Strich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Tochter Zions, freue dich!", "tokens": ["O", "Toch\u00b7ter", "Zi\u00b7ons", ",", "freu\u00b7e", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch sieh, was wird sie bleich wie Flieder?", "tokens": ["Doch", "sieh", ",", "was", "wird", "sie", "bleich", "wie", "Flie\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "VAFIN", "PPER", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+