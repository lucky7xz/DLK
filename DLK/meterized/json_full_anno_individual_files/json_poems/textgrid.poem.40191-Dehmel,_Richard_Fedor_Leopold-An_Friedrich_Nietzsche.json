{"textgrid.poem.40191": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "An Friedrich Nietzsche", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und es kam die Zeit,", "tokens": ["Und", "es", "kam", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "da\u00df Zarathustra abermals", "tokens": ["da\u00df", "Za\u00b7ra\u00b7thust\u00b7ra", "a\u00b7ber\u00b7mals"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NE", "ADV"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "aus seiner H\u00f6hle niederstieg vom Berge;", "tokens": ["aus", "sei\u00b7ner", "H\u00f6h\u00b7le", "nie\u00b7ders\u00b7tieg", "vom", "Ber\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und viel Volkes k\u00fc\u00dfte seine Spuren ...", "tokens": ["und", "viel", "Vol\u00b7kes", "k\u00fc\u00df\u00b7te", "sei\u00b7ne", "Spu\u00b7ren", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der J\u00fcnger aber, der ihn liebte,", "tokens": ["Der", "J\u00fcn\u00b7ger", "a\u00b7ber", ",", "der", "ihn", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "stand von ferne,", "tokens": ["stand", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "und der Meister kannte ihn nicht.", "tokens": ["und", "der", "Meis\u00b7ter", "kann\u00b7te", "ihn", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und der J\u00fcnger trat zu ihm und sprach:", "tokens": ["Und", "der", "J\u00fcn\u00b7ger", "trat", "zu", "ihm", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "KON", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Meister, was soll ich thun,", "tokens": ["Meis\u00b7ter", ",", "was", "soll", "ich", "thun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "da\u00df ich selig werde? \u2013", "tokens": ["da\u00df", "ich", "se\u00b7lig", "wer\u00b7de", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Zarathustra aber wandte sich", "tokens": ["Za\u00b7ra\u00b7thust\u00b7ra", "a\u00b7ber", "wand\u00b7te", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "und schaute hinter sich,", "tokens": ["und", "schau\u00b7te", "hin\u00b7ter", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "und seine Augen wurden weit,", "tokens": ["und", "sei\u00b7ne", "Au\u00b7gen", "wur\u00b7den", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "und sagte Antwort:", "tokens": ["und", "sag\u00b7te", "Ant\u00b7wort", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Folge mir nach! \u2013", "tokens": ["Fol\u00b7ge", "mir", "nach", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Da ward der J\u00fcnger sehend", "tokens": ["Da", "ward", "der", "J\u00fcn\u00b7ger", "se\u00b7hend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "und verstand den Meister", "tokens": ["und", "ver\u00b7stand", "den", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "und folgte ihm", "tokens": ["und", "folg\u00b7te", "ihm"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "und verlie\u00df ihn ...", "tokens": ["und", "ver\u00b7lie\u00df", "ihn", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Da er aber seines Weges wanderte,", "tokens": ["Da", "er", "a\u00b7ber", "sei\u00b7nes", "We\u00b7ges", "wan\u00b7der\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.21": {"text": "ward er traurig", "tokens": ["ward", "er", "trau\u00b7rig"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "und sprach also zu seiner Sehnsucht:", "tokens": ["und", "sprach", "al\u00b7so", "zu", "sei\u00b7ner", "Sehn\u00b7sucht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wahrlich! viele sind,", "tokens": ["Wahr\u00b7lich", "!", "vie\u00b7le", "sind", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "PIS", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.24": {"text": "deren Zunge trieft vom Namen Zarathustras,", "tokens": ["de\u00b7ren", "Zun\u00b7ge", "trieft", "vom", "Na\u00b7men", "Za\u00b7ra\u00b7thust\u00b7ras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELAT", "NN", "VVFIN", "APPRART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.25": {"text": "und im Herzen beten sie zum Gotte Tamtam;", "tokens": ["und", "im", "Her\u00b7zen", "be\u00b7ten", "sie", "zum", "Got\u00b7te", "Tam\u00b7tam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "APPRART", "NN", "NE", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.26": {"text": "wahrlich, ", "tokens": ["wahr\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Seinen Adler sahen sie fliegen,", "tokens": ["Sei\u00b7nen", "Ad\u00b7ler", "sa\u00b7hen", "sie", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "welcher hei\u00dft \u2013 der Wille zur Macht", "tokens": ["wel\u00b7cher", "hei\u00dft", "\u2013", "der", "Wil\u00b7le", "zur", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAT", "VVFIN", "$(", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.29": {"text": "\u00fcber die Kleinen;", "tokens": ["\u00fc\u00b7ber", "die", "Klei\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "und seine Schlange n\u00e4hrten sie an ihrer Brust,", "tokens": ["und", "sei\u00b7ne", "Schlan\u00b7ge", "n\u00e4hr\u00b7ten", "sie", "an", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "die Schlange Klugheit.", "tokens": ["die", "Schlan\u00b7ge", "Klug\u00b7heit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.32": {"text": "Aber seiner Sonne ist ihr Auge blind,", "tokens": ["A\u00b7ber", "sei\u00b7ner", "Son\u00b7ne", "ist", "ihr", "Au\u00b7ge", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.33": {"text": "welche hei\u00dft \u2013 der Wille zur Macht", "tokens": ["wel\u00b7che", "hei\u00dft", "\u2013", "der", "Wil\u00b7le", "zur", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$(", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.34": {"text": "\u00fcber den Einen:", "tokens": ["\u00fc\u00b7ber", "den", "Ei\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "den Gott Ich.", "tokens": ["den", "Gott", "Ich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.36": {"text": "Wiedergeburten feiern sie", "tokens": ["Wie\u00b7der\u00b7ge\u00b7bur\u00b7ten", "fei\u00b7ern", "sie"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "und Wiedertaufen ihrer G\u00f6tzen,", "tokens": ["und", "Wie\u00b7der\u00b7tau\u00b7fen", "ih\u00b7rer", "G\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "aber Keiner wu\u00dfte noch", "tokens": ["a\u00b7ber", "Kei\u00b7ner", "wu\u00df\u00b7te", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "sich selber zu befruchten", "tokens": ["sich", "sel\u00b7ber", "zu", "be\u00b7fruch\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "und seinem Samen jubelnd sich zu kreuzigen.", "tokens": ["und", "sei\u00b7nem", "Sa\u00b7men", "ju\u00b7belnd", "sich", "zu", "kreu\u00b7zi\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Der Du Deine neue S\u00fcnde lehrtest,", "tokens": ["Der", "Du", "Dei\u00b7ne", "neu\u00b7e", "S\u00fcn\u00b7de", "lehr\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "habe Dank! o d\u00fcrft' ich dir", "tokens": ["ha\u00b7be", "Dank", "!", "o", "d\u00fcrft'", "ich", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$.", "FM", "VMFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "dein letztes Wort vom Munde k\u00fcssen,", "tokens": ["dein", "letz\u00b7tes", "Wort", "vom", "Mun\u00b7de", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "du l\u00e4chelnder Priester des zeugenden Todes!", "tokens": ["du", "l\u00e4\u00b7cheln\u00b7der", "Pries\u00b7ter", "des", "zeu\u00b7gen\u00b7den", "To\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.45": {"text": "und mancher Art", "tokens": ["und", "man\u00b7cher", "Art"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.46": {"text": "sind die Sonnenpfeile", "tokens": ["sind", "die", "Son\u00b7nen\u00b7pfei\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.47": {"text": "und Blumengifte", "tokens": ["und", "Blu\u00b7men\u00b7gif\u00b7te"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.48": {"text": "des zeugenden Todes!", "tokens": ["des", "zeu\u00b7gen\u00b7den", "To\u00b7des", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.49": {"text": "Ach, da\u00df du Manchem auch ", "tokens": ["Ach", ",", "da\u00df", "du", "Man\u00b7chem", "auch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPER", "PIS", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und es kam die Zeit,", "tokens": ["Und", "es", "kam", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "da\u00df Zarathustra abermals", "tokens": ["da\u00df", "Za\u00b7ra\u00b7thust\u00b7ra", "a\u00b7ber\u00b7mals"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "NE", "ADV"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "aus seiner H\u00f6hle niederstieg vom Berge;", "tokens": ["aus", "sei\u00b7ner", "H\u00f6h\u00b7le", "nie\u00b7ders\u00b7tieg", "vom", "Ber\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und viel Volkes k\u00fc\u00dfte seine Spuren ...", "tokens": ["und", "viel", "Vol\u00b7kes", "k\u00fc\u00df\u00b7te", "sei\u00b7ne", "Spu\u00b7ren", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der J\u00fcnger aber, der ihn liebte,", "tokens": ["Der", "J\u00fcn\u00b7ger", "a\u00b7ber", ",", "der", "ihn", "lieb\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", 