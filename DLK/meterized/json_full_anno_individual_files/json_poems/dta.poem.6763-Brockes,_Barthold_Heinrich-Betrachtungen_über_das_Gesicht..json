{"dta.poem.6763": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch ein' Erfahrung, die gemein,", "tokens": ["Noch", "ein'", "Er\u00b7fah\u00b7rung", ",", "die", "ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heisst uns Anmerckungen und Schl\u00fcsse machen.", "tokens": ["Heisst", "uns", "An\u00b7mer\u00b7ckun\u00b7gen", "und", "Schl\u00fcs\u00b7se", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wir finden, da\u00df wir offt betrogen seyn,", "tokens": ["Wir", "fin\u00b7den", ",", "da\u00df", "wir", "offt", "be\u00b7tro\u00b7gen", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn wir auf einen Vorwurf sehn,", "tokens": ["Wenn", "wir", "auf", "ei\u00b7nen", "Vor\u00b7wurf", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df sodann verschiedne Sachen", "tokens": ["Und", "da\u00df", "so\u00b7dann", "ver\u00b7schied\u00b7ne", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dazwischen stehn;", "tokens": ["Da\u00b7zwi\u00b7schen", "stehn", ";"], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "So scheinet es, als ob die Weite", "tokens": ["So", "schei\u00b7net", "es", ",", "als", "ob", "die", "Wei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Sich noch vergr\u00f6sserte: hingegen", "tokens": ["Sich", "noch", "ver\u00b7gr\u00f6s\u00b7ser\u00b7te", ":", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PRF", "ADV", "VVFIN", "$.", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Befindet man, da\u00df wir uns auch zu irren pflegen,", "tokens": ["Be\u00b7fin\u00b7det", "man", ",", "da\u00df", "wir", "uns", "auch", "zu", "ir\u00b7ren", "pfle\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn, zwischen uns und den von fern gesehnen H\u00f6hen,", "tokens": ["Wenn", ",", "zwi\u00b7schen", "uns", "und", "den", "von", "fern", "ge\u00b7seh\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "PPER", "KON", "ART", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sich flache Felder und in Gr\u00fcnden", "tokens": ["Sich", "fla\u00b7che", "Fel\u00b7der", "und", "in", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJA", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Vertieffte Th\u00e4ler finden;", "tokens": ["Ver\u00b7tieff\u00b7te", "Th\u00e4\u00b7ler", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}