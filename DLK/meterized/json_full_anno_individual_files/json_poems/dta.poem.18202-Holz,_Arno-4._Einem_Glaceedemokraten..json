{"dta.poem.18202": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "4.  \n  Einem Glaceedemokraten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist wie ich ein ", "tokens": ["Du", "bist", "wie", "ich", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "KOKOM", "PPER", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und f\u00fchrst das Elend aus der Gasse", "tokens": ["Und", "f\u00fchrst", "das", "E\u00b7lend", "aus", "der", "Gas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch deine Lieder in den Salon.", "tokens": ["Durch", "dei\u00b7ne", "Lie\u00b7der", "in", "den", "Sa\u00b7lon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Du h\u00fcllst sie in Gold und Purpur ein,", "tokens": ["Du", "h\u00fcllst", "sie", "in", "Gold", "und", "Pur\u00b7pur", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nun wird die Armuth unsterblich sein.", "tokens": ["Nun", "wird", "die", "Ar\u00b7muth", "uns\u00b7terb\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df, du liebst es hoch zu Rosse", "tokens": ["Ich", "wei\u00df", ",", "du", "liebst", "es", "hoch", "zu", "Ros\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu sch\u00fctteln den Speer deiner Poesie,", "tokens": ["Zu", "sch\u00fct\u00b7teln", "den", "Speer", "dei\u00b7ner", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Drum duftet sie auch nie nach der Gosse", "tokens": ["Drum", "duf\u00b7tet", "sie", "auch", "nie", "nach", "der", "Gos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und stinkt betr\u00e4chtlich nach Patschuli.", "tokens": ["Und", "stinkt", "be\u00b7tr\u00e4cht\u00b7lich", "nach", "Pat\u00b7schu\u00b7li", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Famos! schon wird vor Bewundrung stumm", "tokens": ["Fa\u00b7mos", "!", "schon", "wird", "vor", "Be\u00b7wund\u00b7rung", "stumm"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VAFIN", "APPR", "NN", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Das h\u00f6here T\u00f6chterpublikum.", "tokens": ["Das", "h\u00f6\u00b7he\u00b7re", "T\u00f6ch\u00b7ter\u00b7pub\u00b7li\u00b7kum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Vergn\u00fcglich hockst du hinterm Ofen,", "tokens": ["Ver\u00b7gn\u00fcg\u00b7lich", "hockst", "du", "hin\u00b7term", "O\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Fortschritts Ziel hast du entdeckt", "tokens": ["Des", "Fort\u00b7schritts", "Ziel", "hast", "du", "ent\u00b7deckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und so zu sagen mit deinen Strophen", "tokens": ["Und", "so", "zu", "sa\u00b7gen", "mit", "dei\u00b7nen", "Stro\u00b7phen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKZU", "VVINF", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den wei\u00dfen Mohren schwarz geleckt \u2014", "tokens": ["Den", "wei\u00b7\u00dfen", "Moh\u00b7ren", "schwarz", "ge\u00b7leckt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Lied, das die rothe Rache prei\u00dft,", "tokens": ["Kein", "Lied", ",", "das", "die", "ro\u00b7the", "Ra\u00b7che", "prei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kein Aufschrei, der uns das Herz zerrei\u00dft!", "tokens": ["Kein", "Auf\u00b7schrei", ",", "der", "uns", "das", "Herz", "zer\u00b7rei\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ich w\u00fcrde dir gern ein Kr\u00f6nchen kleistern,", "tokens": ["Ich", "w\u00fcr\u00b7de", "dir", "gern", "ein", "Kr\u00f6n\u00b7chen", "kleis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du wei\u00dft, ich bin kein Nihilist;", "tokens": ["Du", "wei\u00dft", ",", "ich", "bin", "kein", "Ni\u00b7hi\u00b7list", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch kann ich mich nicht recht begeistern,", "tokens": ["Doch", "kann", "ich", "mich", "nicht", "recht", "be\u00b7geis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dieweil es mir mitunter ist:", "tokens": ["Die\u00b7weil", "es", "mir", "mi\u00b7tun\u00b7ter", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als lachte durch jedes Hungergedicht", "tokens": ["Als", "lach\u00b7te", "durch", "je\u00b7des", "Hun\u00b7ger\u00b7ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dein wohlgen\u00e4hrtes Prostmahlzeitsgesicht!", "tokens": ["Dein", "wohl\u00b7ge\u00b7n\u00e4hr\u00b7tes", "Prost\u00b7mahl\u00b7zeits\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}