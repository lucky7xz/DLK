{"dta.poem.4244": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Feld-Bluhmen am Wasser.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich an einem Wasser-Graben, der, hohe Felder in der", "tokens": ["Als", "ich", "an", "ei\u00b7nem", "Was\u00b7ser\u00b7Gra\u00b7ben", ",", "der", ",", "ho\u00b7he", "Fel\u00b7der", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "$,", "ADJA", "NN", "APPR", "ART"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Mitte,", "tokens": ["Mit\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "In einem lang- geraden Strich, so weit man sehen kann,", "tokens": ["In", "ei\u00b7nem", "lang", "ge\u00b7ra\u00b7den", "Strich", ",", "so", "weit", "man", "se\u00b7hen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "ADJA", "NN", "$,", "ADV", "ADJD", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "durchschnitte,", "tokens": ["durch\u00b7schnit\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Der rein und klar, als ein Krystall, so recht mit Flei\u00df poliert,", "tokens": ["Der", "rein", "und", "klar", ",", "als", "ein", "Krys\u00b7tall", ",", "so", "recht", "mit", "Flei\u00df", "po\u00b7liert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "$,", "KOUS", "ART", "NN", "$,", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "mich setzte,", "tokens": ["mich", "setz\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und an dem Erd- und Himmels-Spiegel des klaren Wassers", "tokens": ["Und", "an", "dem", "Erd", "und", "Him\u00b7mels\u00b7Spie\u00b7gel", "des", "kla\u00b7ren", "Was\u00b7sers"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "TRUNC", "KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "mich ergetzte;", "tokens": ["mich", "er\u00b7getz\u00b7te", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Ward ich, zu meiner Lust Vermehrung, am Ufer einer Bluh-", "tokens": ["Ward", "ich", ",", "zu", "mei\u00b7ner", "Lust", "Ver\u00b7meh\u00b7rung", ",", "am", "U\u00b7fer", "ei\u00b7ner", "Bluh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,", "APPRART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "men Schaar,", "tokens": ["men", "Schaar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Von ungez\u00e4hlter Farb\u2019 und Art, mit inniglicher Lust,", "tokens": ["Von", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter", "Fa\u00b7rb'", "und", "Art", ",", "mit", "in\u00b7nig\u00b7li\u00b7cher", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "gewahr.", "tokens": ["ge\u00b7wahr", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Zu unterst war am jungen Schilf ein Gr\u00fcn, das unbe-", "tokens": ["Zu", "un\u00b7terst", "war", "am", "jun\u00b7gen", "Schilf", "ein", "Gr\u00fcn", ",", "das", "un\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "VAFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "schreiblich- sch\u00f6n,", "tokens": ["schreib\u00b7lich", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["TRUNC", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Zumahl vom Sonnen-Licht durchstrahlet, ein recht durch-", "tokens": ["Zu\u00b7mahl", "vom", "Son\u00b7nen\u00b7Licht", "durch\u00b7strah\u00b7let", ",", "ein", "recht", "durch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,", "ART", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "l\u00e4uchtig Gr\u00fcn zu sehn,", "tokens": ["l\u00e4uch\u00b7tig", "Gr\u00fcn", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "An diesem stand, in hellem Purpur, manch Bl\u00fchmchen, das", "tokens": ["An", "die\u00b7sem", "stand", ",", "in", "hel\u00b7lem", "Pur\u00b7pur", ",", "manch", "Bl\u00fch\u00b7mchen", ",", "das"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "PDAT", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "PIAT", "NN", "$,", "PRELS"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Levcojen gleich,", "tokens": ["Lev\u00b7co\u00b7jen", "gleich", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Dort war ein Platz an Silber- weissen und riechenden", "tokens": ["Dort", "war", "ein", "Platz", "an", "Sil\u00b7ber", "weis\u00b7sen", "und", "rie\u00b7chen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "TRUNC", "VVINF", "KON", "ADJA"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Camillen reich.", "tokens": ["Ca\u00b7mil\u00b7len", "reich", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die r\u00f6htlich- gelben Wucher-Bluhmen, der Grase-Bluhmen", "tokens": ["Die", "r\u00f6ht\u00b7lich", "gel\u00b7ben", "Wu\u00b7cher\u00b7Bluh\u00b7men", ",", "der", "Gra\u00b7se\u00b7Bluh\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "TRUNC", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "g\u00fcldner Glanz,", "tokens": ["g\u00fcld\u00b7ner", "Glanz", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Die Schwefel- gelben Jrides, der holde weiss\u2019 und rohte", "tokens": ["Die", "Schwe\u00b7fel", "gel\u00b7ben", "Jri\u00b7des", ",", "der", "hol\u00b7de", "weiss'", "und", "roh\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "KON", "ADJA"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Klee,", "tokens": ["Klee", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Formierten \u00f6fters hin und wieder, aus manchem Theil, ein", "tokens": ["For\u00b7mier\u00b7ten", "\u00f6f\u00b7ters", "hin", "und", "wie\u00b7der", ",", "aus", "man\u00b7chem", "Theil", ",", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "PTKVZ", "KON", "ADV", "$,", "APPR", "PIAT", "NN", "$,", "ART"], "meter": "-+-+-+-+--+---", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "buntes Ganz,", "tokens": ["bun\u00b7tes", "Ganz", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Bey welchen ich denn auch zugleich, bey gelblich- gr\u00fcnen", "tokens": ["Bey", "wel\u00b7chen", "ich", "denn", "auch", "zu\u00b7gleich", ",", "bey", "gelb\u00b7lich", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "PPER", "ADV", "ADV", "ADV", "$,", "APPR", "TRUNC", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wasser-Linsen,", "tokens": ["Was\u00b7ser\u00b7Lin\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Ein B\u00fcschel dunkel- gr\u00fcner Binsen,", "tokens": ["Ein", "B\u00fc\u00b7schel", "dun\u00b7kel", "gr\u00fc\u00b7ner", "Bin\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "TRUNC", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Aus klarem Wasser, ragen seh\u2019.", "tokens": ["Aus", "kla\u00b7rem", "Was\u00b7ser", ",", "ra\u00b7gen", "seh'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jm tausendfachen Gr\u00fcnen schimmert die liebliche Vergi\u00df", "tokens": ["Jm", "tau\u00b7send\u00b7fa\u00b7chen", "Gr\u00fc\u00b7nen", "schim\u00b7mert", "die", "lieb\u00b7li\u00b7che", "Ver\u00b7gi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "mein nicht;", "tokens": ["mein", "nicht", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Vor allen andern Bluhmen aber fiel mir in mein ger\u00fchrt", "tokens": ["Vor", "al\u00b7len", "an\u00b7dern", "Bluh\u00b7men", "a\u00b7ber", "fiel", "mir", "in", "mein", "ge\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "VVPP"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Gesicht,", "tokens": ["Ge\u00b7sicht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "In einer dunkel-rohten Gluht, als wie ein Feuer, hie und", "tokens": ["In", "ei\u00b7ner", "dun\u00b7kel\u00b7roh\u00b7ten", "Gluht", ",", "als", "wie", "ein", "Feu\u00b7er", ",", "hie", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "ADV", "KON"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "da,", "tokens": ["da", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Und \u00fcbertraf fast all\u2019an Schein, der einfach wilde, rohte", "tokens": ["Und", "\u00fc\u00b7bert\u00b7raf", "fast", "all'\u00b7an", "Schein", ",", "der", "ein\u00b7fach", "wil\u00b7de", ",", "roh\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "$,", "PRELS", "ADV", "ADJA", "$,", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Mah.", "tokens": ["Mah", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Durch den von so verschiednen Farben gemischten Schim-", "tokens": ["Durch", "den", "von", "so", "ver\u00b7schied\u00b7nen", "Far\u00b7ben", "ge\u00b7mischten", "Schim"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADV", "ADJA", "NN", "ADJA", "TRUNC"], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "mer, Glanz und Pracht", "tokens": ["mer", ",", "Glanz", "und", "Pracht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ward jeder Mensch, der menschlich sieht, ergetzet und recht", "tokens": ["Ward", "je\u00b7der", "Mensch", ",", "der", "menschlich", "sieht", ",", "er\u00b7get\u00b7zet", "und", "recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,", "VVFIN", "KON", "NN"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "angelacht;", "tokens": ["an\u00b7ge\u00b7lacht", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ja, was die Sch\u00f6nheit noch vermehrt, war, da die Fluht so", "tokens": ["Ja", ",", "was", "die", "Sch\u00f6n\u00b7heit", "noch", "ver\u00b7mehrt", ",", "war", ",", "da", "die", "Fluht", "so"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PRELS", "ART", "NN", "ADV", "ADJD", "$,", "VAFIN", "$,", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "klar, so rein,", "tokens": ["klar", ",", "so", "rein", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Der, von so tausendfacher Sch\u00f6nheit, so hell- geformte", "tokens": ["Der", ",", "von", "so", "tau\u00b7send\u00b7fa\u00b7cher", "Sch\u00f6n\u00b7heit", ",", "so", "hell", "ge\u00b7form\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "ADV", "ADJA", "NN", "$,", "ADV", "TRUNC", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Wiederschein,", "tokens": ["Wie\u00b7der\u00b7schein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Wo, auf des himmlischen Sapphirs, und oft auf einem", "tokens": ["Wo", ",", "auf", "des", "himm\u00b7li\u00b7schen", "Sap\u00b7phirs", ",", "und", "oft", "auf", "ei\u00b7nem"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "gr\u00fcnen Grunde,", "tokens": ["gr\u00fc\u00b7nen", "Grun\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Der Bluhmen und der Kr\u00e4uter Sch\u00f6nheit, versch\u00f6nert und", "tokens": ["Der", "Bluh\u00b7men", "und", "der", "Kr\u00e4u\u00b7ter", "Sch\u00f6n\u00b7heit", ",", "ver\u00b7sch\u00f6\u00b7nert", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$,", "VVPP", "KON"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "verdoppelt stunde.", "tokens": ["ver\u00b7dop\u00b7pelt", "stun\u00b7de", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJA", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Und kurz: ein sch\u00f6n- und holder Vorwurf, als dieser bunte", "tokens": ["Und", "kurz", ":", "ein", "sch\u00f6n", "und", "hol\u00b7der", "Vor\u00b7wurf", ",", "als", "die\u00b7ser", "bun\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "ART", "TRUNC", "KON", "ADJA", "NN", "$,", "KOUS", "PDAT", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Platz uns wiese,", "tokens": ["Platz", "uns", "wie\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Ist man, von Edens Auen selber, und vom verlohrnen", "tokens": ["Ist", "man", ",", "von", "E\u00b7dens", "Au\u00b7en", "sel\u00b7ber", ",", "und", "vom", "ver\u00b7lohr\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PIS", "$,", "APPR", "NE", "NN", "ADV", "$,", "KON", "APPRART", "ADJA"], "meter": "+--+-+-+-++-+-", "measure": "iambic.septa.invert"}, "line.16": {"text": "Paradiese,", "tokens": ["Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", 