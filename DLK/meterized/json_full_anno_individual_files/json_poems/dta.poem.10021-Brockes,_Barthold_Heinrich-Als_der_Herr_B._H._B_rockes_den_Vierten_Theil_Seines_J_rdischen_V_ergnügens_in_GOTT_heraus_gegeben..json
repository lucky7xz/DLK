{"dta.poem.10021": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Als der  \n Herr  B. H. B rockes  \n den  \n Vierten Theil  \n Seines  \n  J rdischen  V ergn\u00fcgens  \n in GOTT  \n heraus gegeben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Dicht-Kunst! lehre mich die\u00dfmahl,", "tokens": ["Auf", "Dicht\u00b7Kunst", "!", "leh\u00b7re", "mich", "die\u00df\u00b7mahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den sch\u00f6nsten Thon von deinen Spielen,", "tokens": ["Den", "sch\u00f6ns\u00b7ten", "Thon", "von", "dei\u00b7nen", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verdopple der Gedancken Zahl,", "tokens": ["Ver\u00b7dopp\u00b7le", "der", "Ge\u00b7dan\u00b7cken", "Zahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df mich dein edles Feuer f\u00fchlen;", "tokens": ["La\u00df", "mich", "dein", "ed\u00b7les", "Feu\u00b7er", "f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wirff Krafft und Reitzung in die Gluth,", "tokens": ["Wirff", "Krafft", "und", "Reit\u00b7zung", "in", "die", "Gluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es soll kein anfgesch\u00e4umtes Blut,", "tokens": ["Es", "soll", "kein", "anf\u00b7ge\u00b7sch\u00e4um\u00b7tes", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das schw\u00fclstig braust, hier opffern gehen.", "tokens": ["Das", "schw\u00fcls\u00b7tig", "braust", ",", "hier", "opf\u00b7fern", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "$,", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nein, nein, es mu\u00df durch deine Gunst,", "tokens": ["Nein", ",", "nein", ",", "es", "mu\u00df", "durch", "dei\u00b7ne", "Gunst", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Hohe deiner starcken Kunst,", "tokens": ["Das", "Ho\u00b7he", "dei\u00b7ner", "star\u00b7cken", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In allen meinen Zeilen stehen.", "tokens": ["In", "al\u00b7len", "mei\u00b7nen", "Zei\u00b7len", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es gilt hier deinem gr\u00f6sten Sohn,", "tokens": ["Es", "gilt", "hier", "dei\u00b7nem", "gr\u00f6s\u00b7ten", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Kiel will deinen ", "tokens": ["Mein", "Kiel", "will", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie? ja mich daucht, ich h\u00f6re schon", "tokens": ["Wie", "?", "ja", "mich", "daucht", ",", "ich", "h\u00f6\u00b7re", "schon"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "ADV", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Anmuts-volle Laute klingen.", "tokens": ["Die", "An\u00b7muts\u00b7vol\u00b7le", "Lau\u00b7te", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich reitzt, mich locket, mich erweckt", "tokens": ["Mich", "reitzt", ",", "mich", "lo\u00b7cket", ",", "mich", "er\u00b7weckt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Thon, der nach der Gottheit schmeckt,", "tokens": ["Ein", "Thon", ",", "der", "nach", "der", "Got\u00b7theit", "schmeckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich sehe dich, du bist bem\u00fcht,", "tokens": ["Ich", "se\u00b7he", "dich", ",", "du", "bist", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Kelch, worin dein Nectar gl\u00fcht,", "tokens": ["Den", "Kelch", ",", "wo\u00b7rin", "dein", "Nec\u00b7tar", "gl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit reichen Str\u00f6men einzuschencken.", "tokens": ["Mit", "rei\u00b7chen", "Str\u00f6\u00b7men", "ein\u00b7zu\u00b7schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Allein, was l\u00e4ssest du mich hier,", "tokens": ["Al\u00b7lein", ",", "was", "l\u00e4s\u00b7sest", "du", "mich", "hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Holdsel\u2019ge Sch\u00f6ne, doch erblicken?", "tokens": ["Hold\u00b7sel'\u00b7ge", "Sch\u00f6\u00b7ne", ",", "doch", "er\u00b7bli\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was stellst du meinen Augen f\u00fcr?", "tokens": ["Was", "stellst", "du", "mei\u00b7nen", "Au\u00b7gen", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie? will mich gar dein Tranck entz\u00fccken?", "tokens": ["Wie", "?", "will", "mich", "gar", "dein", "Tranck", "ent\u00b7z\u00fc\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sehe hier ein Paradie\u00df,", "tokens": ["Ich", "se\u00b7he", "hier", "ein", "Pa\u00b7ra\u00b7die\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So, wie es sich einst dorten wies,", "tokens": ["So", ",", "wie", "es", "sich", "einst", "dor\u00b7ten", "wies", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich kann hier Berge, Feld und Auen,", "tokens": ["Ich", "kann", "hier", "Ber\u00b7ge", ",", "Feld", "und", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Sonnen Licht, der Sterne Pracht,", "tokens": ["Der", "Son\u00b7nen", "Licht", ",", "der", "Ster\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ohn\u2019 alle Dunckelheit und Nacht,", "tokens": ["Ohn'", "al\u00b7le", "Dun\u00b7ckel\u00b7heit", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit aufgekl\u00e4rten Blicken, schauen.", "tokens": ["Mit", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ten", "Bli\u00b7cken", ",", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer ist der Mann, der dorten steht,", "tokens": ["Wer", "ist", "der", "Mann", ",", "der", "dor\u00b7ten", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den ein erhabner Glantz \u00fcmgeben?", "tokens": ["Den", "ein", "er\u00b7hab\u00b7ner", "Glantz", "\u00fcm\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den ein dreyfacher Schmuck erh\u00f6ht,", "tokens": ["Den", "ein", "drey\u00b7fac\u00b7her", "Schmuck", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um den die reinen Geister schweben?", "tokens": ["Um", "den", "die", "rei\u00b7nen", "Geis\u00b7ter", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er h\u00e4lt in seiner rechten Hand", "tokens": ["Er", "h\u00e4lt", "in", "sei\u00b7ner", "rech\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Glas, mit dieser Schrift: Verstand.", "tokens": ["Ein", "Glas", ",", "mit", "die\u00b7ser", "Schrift", ":", "Ver\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PDAT", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er sieht dadurch die tieffsten H\u00f6hen,", "tokens": ["Er", "sieht", "da\u00b7durch", "die", "tieffs\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Geist, der nur allein sich regt,", "tokens": ["Sein", "Geist", ",", "der", "nur", "al\u00b7lein", "sich", "regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Scheint, da der Leib sich nie bewegt,", "tokens": ["Scheint", ",", "da", "der", "Leib", "sich", "nie", "be\u00b7wegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Selbst in die Gottheit einzugehen.", "tokens": ["Selbst", "in", "die", "Got\u00b7theit", "ein\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kann ich hier ", "tokens": ["Kann", "ich", "hier"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "In diesem Angesichte finden?", "tokens": ["In", "die\u00b7sem", "An\u00b7ge\u00b7sich\u00b7te", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Strahl, so aus demselben bricht,", "tokens": ["Der", "Strahl", ",", "so", "aus", "dem\u00b7sel\u00b7ben", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heisst allen Zweifel schon verschwinden;", "tokens": ["Heisst", "al\u00b7len", "Zwei\u00b7fel", "schon", "ver\u00b7schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Um allen Jrrthum weg zu ziehn,", "tokens": ["Um", "al\u00b7len", "Jrr\u00b7thum", "weg", "zu", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie spricht, Er setzet sich und mahlet;", "tokens": ["Sie", "spricht", ",", "Er", "set\u00b7zet", "sich", "und", "mah\u00b7let", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er zeigt des Sch\u00f6pfers Eigenschafft,", "tokens": ["Er", "zeigt", "des", "Sch\u00f6p\u00b7fers", "Ei\u00b7gen\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Dessen Liebe, Huld und Krafft", "tokens": ["Wie", "Des\u00b7sen", "Lie\u00b7be", ",", "Huld", "und", "Krafft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Aus allen seinen Wercken strahlet.", "tokens": ["Aus", "al\u00b7len", "sei\u00b7nen", "Wer\u00b7cken", "strah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Andacht nimmt, was Er gesetzt,", "tokens": ["Die", "An\u00b7dacht", "nimmt", ",", "was", "Er", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4sst es selbst dem Sch\u00f6pfer wissen,", "tokens": ["Und", "l\u00e4sst", "es", "selbst", "dem", "Sch\u00f6p\u00b7fer", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Liebe, so dadurch ergetzt,", "tokens": ["Die", "Lie\u00b7be", ",", "so", "da\u00b7durch", "er\u00b7getzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umarmet Jhn mit holden K\u00fcssen.", "tokens": ["Um\u00b7ar\u00b7met", "Jhn", "mit", "hol\u00b7den", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Engel bringt den vierten Crantz", "tokens": ["Ein", "En\u00b7gel", "bringt", "den", "vier\u00b7ten", "Crantz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von einem Sternen-gleichen Glantz,", "tokens": ["Von", "ei\u00b7nem", "Ster\u00b7nen\u00b7glei\u00b7chen", "Glantz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und spricht: Dein Jrdisches Vergn\u00fcgen", "tokens": ["Und", "spricht", ":", "Dein", "Jr\u00b7di\u00b7sches", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des vierten Theils, das nun vollbracht,", "tokens": ["Des", "vier\u00b7ten", "Theils", ",", "das", "nun", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hat Dich der Ehren werth gemacht,", "tokens": ["Hat", "Dich", "der", "Eh\u00b7ren", "werth", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die\u00df Vorrecht Dir noch beyzuf\u00fcgen.", "tokens": ["Die\u00df", "Vor\u00b7recht", "Dir", "noch", "bey\u00b7zu\u00b7f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Es eilt der reinen Geister Schaar,", "tokens": ["Es", "eilt", "der", "rei\u00b7nen", "Geis\u00b7ter", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhm Palm und Lorbeer hinzureichen,", "tokens": ["Jhm", "Palm", "und", "Lor\u00b7beer", "hin\u00b7zu\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie stellen sich mit Freuden dar,", "tokens": ["Sie", "stel\u00b7len", "sich", "mit", "Freu\u00b7den", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und keiner will dem Eifer weichen.", "tokens": ["Und", "kei\u00b7ner", "will", "dem", "Ei\u00b7fer", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich komme selber, Andachts voll,", "tokens": ["Ich", "kom\u00b7me", "sel\u00b7ber", ",", "An\u00b7dachts", "voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch meinen Ehr-Furchts-reichen Zoll", "tokens": ["Auch", "mei\u00b7nen", "Ehr\u00b7Furchts\u00b7rei\u00b7chen", "Zoll"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bey ihrem Opffer auszustreuen.", "tokens": ["Bey", "ih\u00b7rem", "Opf\u00b7fer", "aus\u00b7zu\u00b7streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier aber fliehet alles fort,", "tokens": ["Hier", "a\u00b7ber", "flie\u00b7het", "al\u00b7les", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und lehrt mich, einen heilgen Ort", "tokens": ["Und", "lehrt", "mich", ",", "ei\u00b7nen", "heil\u00b7gen", "Ort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht unbedachtsam zu entweihen.", "tokens": ["Nicht", "un\u00b7be\u00b7dacht\u00b7sam", "zu", "ent\u00b7wei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}