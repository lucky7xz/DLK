{"dta.poem.3371": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Orpheus Hymne  \n  an den Mond .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6re mich, K\u00f6nigin, G\u00f6ttin, du freundliche", "tokens": ["H\u00f6\u00b7re", "mich", ",", "K\u00f6\u00b7ni\u00b7gin", ",", "G\u00f6t\u00b7tin", ",", "du", "freund\u00b7li\u00b7che"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "NN", "$,", "PPER", "ADJA"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.2": {"text": "Sch\u00f6ne des Himmels,", "tokens": ["Sch\u00f6\u00b7ne", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Luftdurchwandlerin, Dunkelerleuchterin, Sterne-", "tokens": ["Luft\u00b7durch\u00b7wand\u00b7le\u00b7rin", ",", "Dun\u00b7ke\u00b7ler\u00b7leuch\u00b7te\u00b7rin", ",", "Ster\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "TRUNC"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "regentin,", "tokens": ["re\u00b7gen\u00b7tin", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "---", "measure": "unknown.measure.zero"}, "line.5": {"text": "Nimmererm\u00fcdende Pilgerin, nimmer verl\u00f6schende", "tokens": ["Nim\u00b7me\u00b7rer\u00b7m\u00fc\u00b7den\u00b7de", "Pil\u00b7ge\u00b7rin", ",", "nim\u00b7mer", "ver\u00b7l\u00f6\u00b7schen\u00b7de"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJA"], "meter": "+--+--+--+--+--", "measure": "diphilius"}, "line.6": {"text": "Fackel,", "tokens": ["Fa\u00b7ckel", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Ewig neugeboren, und ewig alternd und sterbend,", "tokens": ["E\u00b7wig", "neu\u00b7ge\u00b7bo\u00b7ren", ",", "und", "e\u00b7wig", "al\u00b7ternd", "und", "ster\u00b7bend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "$,", "KON", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "M\u00e4nnliche, Fr\u00e4uliche, Gl\u00e4nzende, Herrliche, Schim-", "tokens": ["M\u00e4nn\u00b7li\u00b7che", ",", "Fr\u00e4u\u00b7li\u00b7che", ",", "Gl\u00e4n\u00b7zen\u00b7de", ",", "Herr\u00b7li\u00b7che", ",", "Schim"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "mergelockte,", "tokens": ["mer\u00b7ge\u00b7lock\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.10": {"text": "Stralenverspenderin, Rossetummlerin, Mutter der", "tokens": ["Stra\u00b7len\u00b7ver\u00b7spen\u00b7de\u00b7rin", ",", "Ros\u00b7se\u00b7tumm\u00b7le\u00b7rin", ",", "Mut\u00b7ter", "der"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "ART"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.11": {"text": "Zeiten,", "tokens": ["Zei\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Alles belauschende W\u00e4chterin, T\u00e4nzerin himmli-", "tokens": ["Al\u00b7les", "be\u00b7lau\u00b7schen\u00b7de", "W\u00e4ch\u00b7te\u00b7rin", ",", "T\u00e4n\u00b7ze\u00b7rin", "himm\u00b7li"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "NN", "TRUNC"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.13": {"text": "scher T\u00e4nze,", "tokens": ["scher", "T\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Die du wandelst so gern in stillem traulichen", "tokens": ["Die", "du", "wan\u00b7delst", "so", "gern", "in", "stil\u00b7lem", "trau\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ADV", "ADV", "APPR", "PIS", "VVINF"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Dunkel,", "tokens": ["Dun\u00b7kel", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Die du leuchtest so gern, so gern erquickest und", "tokens": ["Die", "du", "leuch\u00b7test", "so", "gern", ",", "so", "gern", "er\u00b7qui\u00b7ckest", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ADV", "ADV", "$,", "ADV", "ADV", "VVFIN", "KON"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "segnest,", "tokens": ["seg\u00b7nest", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Die du f\u00fchrst im Triumph der Sterne jauchzende", "tokens": ["Die", "du", "f\u00fchrst", "im", "Tri\u00b7umph", "der", "Ster\u00b7ne", "jauch\u00b7zen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "NN"], "meter": "--+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Reigen,", "tokens": ["Rei\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Lichtumg\u00fcrtet, umwallt vom weiten silbernen", "tokens": ["Lich\u00b7tum\u00b7g\u00fcr\u00b7tet", ",", "um\u00b7wallt", "vom", "wei\u00b7ten", "sil\u00b7ber\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Mantel \u2014", "tokens": ["Man\u00b7tel"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Komm, Holdselige, komm' in deiner Sch\u00f6ne, du", "tokens": ["Komm", ",", "Hold\u00b7se\u00b7li\u00b7ge", ",", "komm'", "in", "dei\u00b7ner", "Sch\u00f6\u00b7ne", ",", "du"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gute,", "tokens": ["Gu\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Und mit mildem Strahl erfreue den flehenden Waller.", "tokens": ["Und", "mit", "mil\u00b7dem", "Strahl", "er\u00b7freu\u00b7e", "den", "fle\u00b7hen\u00b7den", "Wal\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}