{"dta.poem.21001": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxV .  \n Eines Bastart-Kindes.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Wo meine Mutter liegt/ da bin ich auch begraben/", "tokens": ["Wo", "mei\u00b7ne", "Mut\u00b7ter", "liegt", "/", "da", "bin", "ich", "auch", "be\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wolte nechst bey ihr mein Leichbegr\u00e4bn\u00fcs haben/", "tokens": ["Ich", "wol\u00b7te", "nechst", "bey", "ihr", "mein", "Leich\u00b7be\u00b7gr\u00e4b\u00b7n\u00fcs", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht unlieb hett\u2019 ich mich zum Vater hinverf\u00fcgt/", "tokens": ["Nicht", "un\u00b7lieb", "hett'", "ich", "mich", "zum", "Va\u00b7ter", "hin\u00b7ver\u00b7f\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wuste wo er lag/ und wei\u00df nicht wo er liegt.", "tokens": ["Ich", "wus\u00b7te", "wo", "er", "lag", "/", "und", "wei\u00df", "nicht", "wo", "er", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWAV", "PPER", "VVFIN", "$(", "KON", "VVFIN", "PTKNEG", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}