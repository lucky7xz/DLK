{"textgrid.poem.58647": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vernunft und Fleisch und die sich mehr als meine Freunde stellen,", "tokens": ["Ver\u00b7nunft", "und", "Fleisch", "und", "die", "sich", "mehr", "als", "mei\u00b7ne", "Freun\u00b7de", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "PRELS", "PRF", "PIAT", "KOKOM", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Die passen auf und locken sehr und meinen mich zu f\u00e4llen,", "tokens": ["Die", "pas\u00b7sen", "auf", "und", "lo\u00b7cken", "sehr", "und", "mei\u00b7nen", "mich", "zu", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Weil ich als Feinde sie bestreit', so suchen sie auch Rach',", "tokens": ["Weil", "ich", "als", "Fein\u00b7de", "sie", "be\u00b7streit'", ",", "so", "su\u00b7chen", "sie", "auch", "Rach'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Ihr Schmeicheln ist Betrug und List, ich frage nichts darnach.", "tokens": ["Ihr", "Schmei\u00b7cheln", "ist", "Be\u00b7trug", "und", "List", ",", "ich", "fra\u00b7ge", "nichts", "dar\u00b7nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Zwar bin ich blind und schwach in mir und k\u00f6nnt' so leichte fallen,", "tokens": ["Zwar", "bin", "ich", "blind", "und", "schwach", "in", "mir", "und", "k\u00f6nnt'", "so", "leich\u00b7te", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "PPER", "KON", "VMFIN", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Doch du willst bei mir sein, mein Herr, mein starker Held, in allen;", "tokens": ["Doch", "du", "willst", "bei", "mir", "sein", ",", "mein", "Herr", ",", "mein", "star\u00b7ker", "Held", ",", "in", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPER", "VAINF", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Bist du mit deiner Kraft mir nah, h\u00e4ltst du mich bei der Hand,", "tokens": ["Bist", "du", "mit", "dei\u00b7ner", "Kraft", "mir", "nah", ",", "h\u00e4ltst", "du", "mich", "bei", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Wie nichts ist dann der Feinde Macht, sie fallen selbst mit 