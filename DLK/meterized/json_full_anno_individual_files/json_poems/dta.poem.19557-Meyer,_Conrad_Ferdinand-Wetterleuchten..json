{"dta.poem.19557": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Wetterleuchten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Im Garten schritt ich durch die Lenzesnacht.", "tokens": ["Im", "Gar\u00b7ten", "schritt", "ich", "durch", "die", "Len\u00b7zes\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Jahres erste Blitze loderten.", "tokens": ["Des", "Jah\u00b7res", "ers\u00b7te", "Blit\u00b7ze", "lo\u00b7der\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die jungen Bl\u00fcten glommen feuerroth", "tokens": ["Die", "jun\u00b7gen", "Bl\u00fc\u00b7ten", "glom\u00b7men", "feu\u00b7er\u00b7roth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und blichen wieder dann. Ein sch\u00f6nes Spiel,", "tokens": ["Und", "bli\u00b7chen", "wie\u00b7der", "dann", ".", "Ein", "sch\u00f6\u00b7nes", "Spiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Davor ich stille hielt. Da sah ich ", "tokens": ["Da\u00b7vor", "ich", "stil\u00b7le", "hielt", ".", "Da", "sah", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PPER", "VVFIN", "VVFIN", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einem Bl\u00fctenzweige spieltest du,", "tokens": ["Mit", "ei\u00b7nem", "Bl\u00fc\u00b7ten\u00b7zwei\u00b7ge", "spiel\u00b7test", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die jung gebliebne Todte! Durch die Hast", "tokens": ["Die", "jung", "ge\u00b7blieb\u00b7ne", "Tod\u00b7te", "!", "Durch", "die", "Hast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Flucht der Zeit zur\u00fcck erkannt' ich dich,", "tokens": ["Und", "Flucht", "der", "Zeit", "zu\u00b7r\u00fcck", "er\u00b7kannt'", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "PTKVZ", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die just des Himmels Feuer \u00fcberglomm.", "tokens": ["Die", "just", "des", "Him\u00b7mels", "Feu\u00b7er", "\u00fc\u00b7berg\u00b7lomm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ergl\u00fchend standest du, wie dazumal,", "tokens": ["Er\u00b7gl\u00fc\u00b7hend", "stan\u00b7dest", "du", ",", "wie", "da\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da dich das erste Liebeswort erschreckt,", "tokens": ["Da", "dich", "das", "ers\u00b7te", "Lie\u00b7bes\u00b7wort", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Du Ungeb\u00e4ndigte, du Fl\u00fcchtende!", "tokens": ["Du", "Un\u00b7ge\u00b7b\u00e4n\u00b7dig\u00b7te", ",", "du", "Fl\u00fcch\u00b7ten\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Dann mit den Bl\u00fcten wieder blichest du.", "tokens": ["Dann", "mit", "den", "Bl\u00fc\u00b7ten", "wie\u00b7der", "bli\u00b7chest", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}