line.3": {"text": "Gar manche ers\u00e4uft oder bricht das Genick,", "tokens": ["Gar", "man\u00b7che", "er\u00b7s\u00e4uft", "o\u00b7der", "bricht", "das", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die lebenden lassen die toten zur\u00fcck.", "tokens": ["Die", "le\u00b7ben\u00b7den", "las\u00b7sen", "die", "to\u00b7ten", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.18": {"line.1": {"text": "Es haben diese K\u00e4uze", "tokens": ["Es", "ha\u00b7ben", "die\u00b7se", "K\u00e4u\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gar f\u00fcrchterliche Schn\u00e4uze;", "tokens": ["Gar", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Schn\u00e4u\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie tragen die K\u00f6pfe geschoren egal,", "tokens": ["Sie", "tra\u00b7gen", "die", "K\u00f6p\u00b7fe", "ge\u00b7scho\u00b7ren", "e\u00b7gal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "ADV", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ganz radikal, ganz rattenkahl.", "tokens": ["Ganz", "ra\u00b7di\u00b7kal", ",", "ganz", "rat\u00b7ten\u00b7kahl", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Die radikale Rotte", "tokens": ["Die", "ra\u00b7di\u00b7ka\u00b7le", "Rot\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wei\u00df nichts von einem Gotte.", "tokens": ["Wei\u00df", "nichts", "von", "ei\u00b7nem", "Got\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie lassen nicht taufen ihre Brut,", "tokens": ["Sie", "las\u00b7sen", "nicht", "tau\u00b7fen", "ih\u00b7re", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Weiber sind Gemeindegut.", "tokens": ["Die", "Wei\u00b7ber", "sind", "Ge\u00b7mein\u00b7de\u00b7gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Der sinnliche Rattenhaufen,", "tokens": ["Der", "sinn\u00b7li\u00b7che", "Rat\u00b7ten\u00b7hau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er will nur fressen und saufen,", "tokens": ["Er", "will", "nur", "fres\u00b7sen", "und", "sau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er denkt nicht, w\u00e4hrend er s\u00e4uft und fri\u00dft,", "tokens": ["Er", "denkt", "nicht", ",", "w\u00e4h\u00b7rend", "er", "s\u00e4uft", "und", "fri\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df unsre Seele unsterblich ist.", "tokens": ["Da\u00df", "uns\u00b7re", "See\u00b7le", "uns\u00b7terb\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.21": {"line.1": {"text": "So eine wilde Ratze,", "tokens": ["So", "ei\u00b7ne", "wil\u00b7de", "Rat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die f\u00fcrchtet nicht H\u00f6lle, nicht Katze;", "tokens": ["Die", "f\u00fcrch\u00b7tet", "nicht", "H\u00f6l\u00b7le", ",", "nicht", "Kat\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie hat kein Gut, sie hat kein Geld", "tokens": ["Sie", "hat", "kein", "Gut", ",", "sie", "hat", "kein", "Geld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcnscht aufs neue zu teilen die Welt.", "tokens": ["Und", "w\u00fcnscht", "aufs", "neu\u00b7e", "zu", "tei\u00b7len", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.22": {"line.1": {"text": "Die Wanderratten, o wehe!", "tokens": ["Die", "Wan\u00b7der\u00b7rat\u00b7ten", ",", "o", "we\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie sind schon in der N\u00e4he.", "tokens": ["Sie", "sind", "schon", "in", "der", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie r\u00fccken heran, ich h\u00f6re schon", "tokens": ["Sie", "r\u00fc\u00b7cken", "he\u00b7ran", ",", "ich", "h\u00f6\u00b7re", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr Pfeifen \u2013 die Zahl ist Legion.", "tokens": ["Ihr", "Pfei\u00b7fen", "\u2013", "die", "Zahl", "ist", "Le\u00b7gi\u00b7on", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.23": {"line.1": {"text": "O wehe! wir sind verloren,", "tokens": ["O", "we\u00b7he", "!", "wir", "sind", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie sind schon vor den Toren!", "tokens": ["Sie", "sind", "schon", "vor", "den", "To\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der B\u00fcrgermeister und Senat,", "tokens": ["Der", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "und", "Se\u00b7nat", ","], "