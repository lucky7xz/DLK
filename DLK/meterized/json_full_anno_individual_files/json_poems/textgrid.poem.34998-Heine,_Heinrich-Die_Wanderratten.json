{"textgrid.poem.34998": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Wanderratten", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gibt zwei Sorten Ratten:", "tokens": ["Es", "gibt", "zwei", "Sor\u00b7ten", "Rat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hungrigen und satten.", "tokens": ["Die", "hung\u00b7ri\u00b7gen", "und", "sat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die satten bleiben vergn\u00fcgt zu Haus,", "tokens": ["Die", "sat\u00b7ten", "blei\u00b7ben", "ver\u00b7gn\u00fcgt", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die hungrigen aber wandern aus.", "tokens": ["Die", "hung\u00b7ri\u00b7gen", "a\u00b7ber", "wan\u00b7dern", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie wandern viel tausend Meilen,", "tokens": ["Sie", "wan\u00b7dern", "viel", "tau\u00b7send", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ganz ohne Rasten und Weilen,", "tokens": ["Ganz", "oh\u00b7ne", "Ras\u00b7ten", "und", "Wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gradaus in ihrem grimmigen Lauf,", "tokens": ["Gra\u00b7daus", "in", "ih\u00b7rem", "grim\u00b7mi\u00b7gen", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Nicht Wind noch Wetter h\u00e4lt sie auf.", "tokens": ["Nicht", "Wind", "noch", "Wet\u00b7ter", "h\u00e4lt", "sie", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie klimmen wohl \u00fcber die H\u00f6hen,", "tokens": ["Sie", "klim\u00b7men", "wohl", "\u00fc\u00b7ber", "die", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie schwimmen wohl durch die Seen;", "tokens": ["Sie", "schwim\u00b7men", "wohl", "durch", "die", "Seen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gar manche ers\u00e4uft oder bricht das Genick,", "tokens": ["Gar", "man\u00b7che", "er\u00b7s\u00e4uft", "o\u00b7der", "bricht", "das", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die lebenden lassen die toten zur\u00fcck.", "tokens": ["Die", "le\u00b7ben\u00b7den", "las\u00b7sen", "die", "to\u00b7ten", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Es haben diese K\u00e4uze", "tokens": ["Es", "ha\u00b7ben", "die\u00b7se", "K\u00e4u\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gar f\u00fcrchterliche Schn\u00e4uze;", "tokens": ["Gar", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Schn\u00e4u\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie tragen die K\u00f6pfe geschoren egal,", "tokens": ["Sie", "tra\u00b7gen", "die", "K\u00f6p\u00b7fe", "ge\u00b7scho\u00b7ren", "e\u00b7gal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "ADV", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ganz radikal, ganz rattenkahl.", "tokens": ["Ganz", "ra\u00b7di\u00b7kal", ",", "ganz", "rat\u00b7ten\u00b7kahl", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die radikale Rotte", "tokens": ["Die", "ra\u00b7di\u00b7ka\u00b7le", "Rot\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wei\u00df nichts von einem Gotte.", "tokens": ["Wei\u00df", "nichts", "von", "ei\u00b7nem", "Got\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie lassen nicht taufen ihre Brut,", "tokens": ["Sie", "las\u00b7sen", "nicht", "tau\u00b7fen", "ih\u00b7re", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Weiber sind Gemeindegut.", "tokens": ["Die", "Wei\u00b7ber", "sind", "Ge\u00b7mein\u00b7de\u00b7gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der sinnliche Rattenhaufen,", "tokens": ["Der", "sinn\u00b7li\u00b7che", "Rat\u00b7ten\u00b7hau\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Er will nur fressen und saufen,", "tokens": ["Er", "will", "nur", "fres\u00b7sen", "und", "sau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er denkt nicht, w\u00e4hrend er s\u00e4uft und fri\u00dft,", "tokens": ["Er", "denkt", "nicht", ",", "w\u00e4h\u00b7rend", "er", "s\u00e4uft", "und", "fri\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df unsre Seele unsterblich ist.", "tokens": ["Da\u00df", "uns\u00b7re", "See\u00b7le", "uns\u00b7terb\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "So eine wilde Ratze,", "tokens": ["So", "ei\u00b7ne", "wil\u00b7de", "Rat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die f\u00fcrchtet nicht H\u00f6lle, nicht Katze;", "tokens": ["Die", "f\u00fcrch\u00b7tet", "nicht", "H\u00f6l\u00b7le", ",", "nicht", "Kat\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie hat kein Gut, sie hat kein Geld", "tokens": ["Sie", "hat", "kein", "Gut", ",", "sie", "hat", "kein", "Geld"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcnscht aufs neue zu teilen die Welt.", "tokens": ["Und", "w\u00fcnscht", "aufs", "neu\u00b7e", "zu", "tei\u00b7len", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Die Wanderratten, o wehe!", "tokens": ["Die", "Wan\u00b7der\u00b7rat\u00b7ten", ",", "o", "we\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "FM", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie sind schon in der N\u00e4he.", "tokens": ["Sie", "sind", "schon", "in", "der", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie r\u00fccken heran, ich h\u00f6re schon", "tokens": ["Sie", "r\u00fc\u00b7cken", "he\u00b7ran", ",", "ich", "h\u00f6\u00b7re", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr Pfeifen \u2013 die Zahl ist Legion.", "tokens": ["Ihr", "Pfei\u00b7fen", "\u2013", "die", "Zahl", "ist", "Le\u00b7gi\u00b7on", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "O wehe! wir sind verloren,", "tokens": ["O", "we\u00b7he", "!", "wir", "sind", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie sind schon vor den Toren!", "tokens": ["Sie", "sind", "schon", "vor", "den", "To\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der B\u00fcrgermeister und Senat,", "tokens": ["Der", "B\u00fcr\u00b7ger\u00b7meis\u00b7ter", "und", "Se\u00b7nat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sch\u00fctteln die K\u00f6pfe, und keiner wei\u00df Rat.", "tokens": ["Sie", "sch\u00fct\u00b7teln", "die", "K\u00f6p\u00b7fe", ",", "und", "kei\u00b7ner", "wei\u00df", "Rat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PIS", "VVFIN", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.10": {"line.1": {"text": "Die B\u00fcrgerschaft greift zu den Waffen,", "tokens": ["Die", "B\u00fcr\u00b7ger\u00b7schaft", "greift", "zu", "den", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Glocken l\u00e4uten die Pfaffen.", "tokens": ["Die", "Glo\u00b7cken", "l\u00e4u\u00b7ten", "die", "Pfaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gef\u00e4hrdet ist das Palladium", "tokens": ["Ge\u00b7f\u00e4hr\u00b7det", "ist", "das", "Pal\u00b7la\u00b7di\u00b7um"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Des sittlichen Staats, das Eigentum.", "tokens": ["Des", "sitt\u00b7li\u00b7chen", "Staats", ",", "das", "Ei\u00b7gen\u00b7tum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Nicht Glockengel\u00e4ute, nicht Pfaffengebete,", "tokens": ["Nicht", "Glo\u00b7cken\u00b7ge\u00b7l\u00e4u\u00b7te", ",", "nicht", "Pfaf\u00b7fen\u00b7ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nicht hochwohlweise Senatsdekrete,", "tokens": ["Nicht", "hoch\u00b7wohl\u00b7wei\u00b7se", "Se\u00b7nats\u00b7de\u00b7kre\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Auch nicht Kanonen, viel Hundertpf\u00fcnder,", "tokens": ["Auch", "nicht", "Ka\u00b7no\u00b7nen", ",", "viel", "Hun\u00b7dert\u00b7pf\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie helfen euch heute, ihr lieben Kinder!", "tokens": ["Sie", "hel\u00b7fen", "euch", "heu\u00b7te", ",", "ihr", "lie\u00b7ben", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.12": {"line.1": {"text": "Heut helfen euch nicht die Wortgespinste", "tokens": ["Heut", "hel\u00b7fen", "euch", "nicht", "die", "Wort\u00b7ge\u00b7spins\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der abgelebten Redek\u00fcnste.", "tokens": ["Der", "ab\u00b7ge\u00b7leb\u00b7ten", "Re\u00b7de\u00b7k\u00fcns\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man f\u00e4ngt nicht Ratten mit Syllogismen,", "tokens": ["Man", "f\u00e4ngt", "nicht", "Rat\u00b7ten", "mit", "Syl\u00b7lo\u00b7gis\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie springen \u00fcber die feinsten Sophismen.", "tokens": ["Sie", "sprin\u00b7gen", "\u00fc\u00b7ber", "die", "feins\u00b7ten", "So\u00b7phis\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "Im hungrigen Magen Eingang finden", "tokens": ["Im", "hung\u00b7ri\u00b7gen", "Ma\u00b7gen", "Ein\u00b7gang", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur Suppenlogik mit Kn\u00f6delgr\u00fcnden,", "tokens": ["Nur", "Sup\u00b7pen\u00b7lo\u00b7gik", "mit", "Kn\u00f6\u00b7del\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nur Argumente von Rinderbraten,", "tokens": ["Nur", "Ar\u00b7gu\u00b7men\u00b7te", "von", "Rin\u00b7der\u00b7bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Begleitet mit G\u00f6ttinger Wurstzitaten.", "tokens": ["Be\u00b7glei\u00b7tet", "mit", "G\u00f6t\u00b7tin\u00b7ger", "Wurst\u00b7zi\u00b7ta\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.14": {"line.1": {"text": "Ein schweigender Stockfisch, in Butter gesotten,", "tokens": ["Ein", "schwei\u00b7gen\u00b7der", "Stock\u00b7fisch", ",", "in", "But\u00b7ter", "ge\u00b7sot\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Behaget den radikalen Rotten", "tokens": ["Be\u00b7ha\u00b7get", "den", "ra\u00b7di\u00b7ka\u00b7len", "Rot\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Viel besser als ein Mirabeau", "tokens": ["Viel", "bes\u00b7ser", "als", "ein", "Mi\u00b7ra\u00b7bea\u00b7u"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und alle Redner seit Cicero.", "tokens": ["Und", "al\u00b7le", "Red\u00b7ner", "seit", "Ci\u00b7ce\u00b7ro", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Es gibt zwei Sorten Ratten:", "tokens": ["Es", "gibt", "zwei", "Sor\u00b7ten", "Rat\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "CARD", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die hungrigen und satten.", "tokens": ["Die", "hung\u00b7ri\u00b7gen", "und", "sat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die satten bleiben vergn\u00fcgt zu Haus,", "tokens": ["Die", "sat\u00b7ten", "blei\u00b7ben", "ver\u00b7gn\u00fcgt", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die hungrigen aber wandern aus.", "tokens": ["Die", "hung\u00b7ri\u00b7gen", "a\u00b7ber", "wan\u00b7dern", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+", "mea