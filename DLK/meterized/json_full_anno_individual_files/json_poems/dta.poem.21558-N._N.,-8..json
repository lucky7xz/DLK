{"dta.poem.21558": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Dichter und kein Papagei", "tokens": ["Ich", "bin", "ein", "Dich\u00b7ter", "und", "kein", "Pa\u00b7pa\u00b7gei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und lieb es drum, in ", "tokens": ["Und", "lieb", "es", "drum", ",", "in"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "PPER", "PAV", "$,", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und doch mi\u00dff\u00e4llt an ihr mir dreierlei,", "tokens": ["Und", "doch", "mi\u00df\u00b7f\u00e4llt", "an", "ihr", "mir", "drei\u00b7er\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und dieses Factum kann ich nicht verdauen.", "tokens": ["Und", "die\u00b7ses", "Fac\u00b7tum", "kann", "ich", "nicht", "ver\u00b7dau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die junge Dame weint sich nicht mehr \u201eblind\u201c,", "tokens": ["Die", "jun\u00b7ge", "Da\u00b7me", "weint", "sich", "nicht", "mehr", "\u201e", "blind", "\u201c", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKNEG", "ADV", "$(", "ADJD", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die jungen Herrn sind meistens eitle Sch\u00f6pse,", "tokens": ["Die", "jun\u00b7gen", "Herrn", "sind", "meis\u00b7tens", "eit\u00b7le", "Sch\u00f6p\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und \u2014 ", "tokens": ["Und"], "token_info": ["word", "punct"], "pos": ["KON", "$("], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Noch seltner heute als die echten M\u00f6pse.", "tokens": ["Noch", "selt\u00b7ner", "heu\u00b7te", "als", "die", "ech\u00b7ten", "M\u00f6p\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}