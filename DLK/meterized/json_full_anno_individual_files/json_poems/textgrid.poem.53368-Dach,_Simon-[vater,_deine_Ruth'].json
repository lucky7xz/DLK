{"textgrid.poem.53368": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[vater, deine Ruth']", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vater, deine Ruth'", "tokens": ["Va\u00b7ter", ",", "dei\u00b7ne", "Ruth'"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hab ich gnug geschmecket,", "tokens": ["Hab", "ich", "gnug", "ge\u00b7schme\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deines Eiffers Glut,", "tokens": ["Dei\u00b7nes", "Eif\u00b7fers", "Glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hat mich stets erschrecket,", "tokens": ["Hat", "mich", "stets", "er\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vmb mein Leiden weist", "tokens": ["Vmb", "mein", "Lei\u00b7den", "weist"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Du erst allermeist.", "tokens": ["Du", "erst", "al\u00b7ler\u00b7meist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nunmehr wil es auch", "tokens": ["Nun\u00b7mehr", "wil", "es", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mit mir Abend werden,", "tokens": ["Mit", "mir", "A\u00b7bend", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie ein d\u00fcnner Rauch", "tokens": ["Wie", "ein", "d\u00fcn\u00b7ner", "Rauch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Auffsteigt von der Erden,", "tokens": ["Auffs\u00b7teigt", "von", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wie ein Dampff entsteht,", "tokens": ["Wie", "ein", "Dampff", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Aber bald vergeht.", "tokens": ["A\u00b7ber", "bald", "ver\u00b7geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Also nehm' ich ab,", "tokens": ["Al\u00b7so", "nehm'", "ich", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Meine krancke Glieder", "tokens": ["Mei\u00b7ne", "kran\u00b7cke", "Glie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Eilen in das Grab,", "tokens": ["Ei\u00b7len", "in", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Alles legt sich nieder:", "tokens": ["Al\u00b7les", "legt", "sich", "nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ich bin alt und matt", "tokens": ["Ich", "bin", "alt", "und", "matt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "----+", "measure": "unknown.measure.single"}, "line.6": {"text": "Vnd des Lebens satt.", "tokens": ["Vnd", "des", "Le\u00b7bens", "satt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Aber weist du dich", "tokens": ["A\u00b7ber", "weist", "du", "dich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Mein nicht anzumassen?", "tokens": ["Mein", "nicht", "an\u00b7zu\u00b7mas\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "PTKNEG", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wirst du jetzund mich", "tokens": ["Wirst", "du", "je\u00b7tzund", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "H\u00fclff- und Trostlo\u00df lassen?", "tokens": ["H\u00fclff", "und", "Trost\u00b7lo\u00df", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wird dein Wort allein", "tokens": ["Wird", "dein", "Wort", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Mir nicht Warheit seyn?", "tokens": ["Mir", "nicht", "War\u00b7heit", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Ich wil Euch, sprichst du,", "tokens": ["Ich", "wil", "Euch", ",", "sprichst", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Seyn in b\u00f6sen Tagen,", "tokens": ["Seyn", "in", "b\u00f6\u00b7sen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auffenthalt und Rhu,", "tokens": ["Auf\u00b7fent\u00b7halt", "und", "Rhu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Euch im Alter tragen,", "tokens": ["Euch", "im", "Al\u00b7ter", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ewer grawes Haar", "tokens": ["E\u00b7wer", "gra\u00b7wes", "Haar"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Retten aus Gefahr.", "tokens": ["Ret\u00b7ten", "aus", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Di\u00df vollbring an mir", "tokens": ["Di\u00df", "voll\u00b7bring", "an", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Die ich m\u00fchsam lebe,", "tokens": ["Die", "ich", "m\u00fch\u00b7sam", "le\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tag und Nacht zu dir", "tokens": ["Tag", "und", "Nacht", "zu", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Meine H\u00e4nd' auffhebe,", "tokens": ["Mei\u00b7ne", "H\u00e4nd'", "auff\u00b7he\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vnd ohn unterla\u00df", "tokens": ["Vnd", "ohn", "un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bin von weinen na\u00df.", "tokens": ["Bin", "von", "wei\u00b7nen", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "VVINF", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wie ein Wandersmann", "tokens": ["Wie", "ein", "Wan\u00b7ders\u00b7mann"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Nach der Herberg eilet,", "tokens": ["Nach", "der", "Her\u00b7berg", "ei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sieht kein Wetter an,", "tokens": ["Sieht", "kein", "Wet\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Nirgends sich verweilet,", "tokens": ["Nir\u00b7gends", "sich", "ver\u00b7wei\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Also sehn ich mich", "tokens": ["Al\u00b7so", "sehn", "ich", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Auch zu schawen dich.", "tokens": ["Auch", "zu", "scha\u00b7wen", "dich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Vnterdessen steh", "tokens": ["Vn\u00b7ter\u00b7des\u00b7sen", "steh"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hie mir, Herr, zur Seiten,", "tokens": ["Hie", "mir", ",", "Herr", ",", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hilff mir alles Weh", "tokens": ["Hilff", "mir", "al\u00b7les", "Weh"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Fein getrost bestreiten,", "tokens": ["Fein", "ge\u00b7trost", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Tilge meine Schuld", "tokens": ["Til\u00b7ge", "mei\u00b7ne", "Schuld"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Vnd verleyh Gedult.", "tokens": ["Vnd", "ver\u00b7leyh", "Ge\u00b7dult", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Ist es dann dein Will'", "tokens": ["Ist", "es", "dann", "dein", "Will'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Vnd ich sol verscheiden,", "tokens": ["Vnd", "ich", "sol", "ver\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ey so nimm mich still", "tokens": ["Ey", "so", "nimm", "mich", "still"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVIMP", "PPER", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hin aus diesem Leiden,", "tokens": ["Hin", "aus", "die\u00b7sem", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Stell die wahre Rhu", "tokens": ["Stell", "die", "wah\u00b7re", "Rhu"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Mir im Himmel zu.", "tokens": ["Mir", "im", "Him\u00b7mel", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Da an keine Qual", "tokens": ["Da", "an", "kei\u00b7ne", "Qual"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Mehr gedacht wird werden,", "tokens": ["Mehr", "ge\u00b7dacht", "wird", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da kein Thr\u00e4nen-Thal", "tokens": ["Da", "kein", "Thr\u00e4\u00b7nen\u00b7Thal"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Vnd kein Angst-Geberden,", "tokens": ["Vnd", "kein", "Angst\u00b7Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sondern Frewd allein", "tokens": ["Son\u00b7dern", "Frewd", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Wird ohn Ende seyn.", "tokens": ["Wird", "ohn", "En\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Da wil ich dir Danck", "tokens": ["Da", "wil", "ich", "dir", "Danck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Mit den Engeln geben", "tokens": ["Mit", "den", "En\u00b7geln", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Durch der Stimmen Klang", "tokens": ["Durch", "der", "Stim\u00b7men", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Deinen Ruhm erheben", "tokens": ["Dei\u00b7nen", "Ruhm", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der du ewig Gott", "tokens": ["Der", "du", "e\u00b7wig", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Bist, Herr Zebaoth!", "tokens": ["Bist", ",", "Herr", "Ze\u00b7bao\u00b7th", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Vater, deine Ruth'", "tokens": ["Va\u00b7ter", ",", "dei\u00b7ne", "Ruth'"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Hab ich gnug geschmecket,", "tokens": ["Hab", "ich", "gnug", "ge\u00b7schme\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deines Eiffers Glut,", "tokens": ["Dei\u00b7nes", "Eif\u00b7fers", "Glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hat mich stets erschrecket,", "tokens": ["Hat", "mich", "stets", "er\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Vmb mein Leiden weist", "tokens": ["Vmb", "mein", "Lei\u00b7den", "weist"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Du erst allermeist.", "tokens": ["Du", "erst", "al\u00b7ler\u00b7meist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Nunmehr wil es auch", "tokens": ["Nun\u00b7mehr", "wil", "es", "auch"], "token_info": 