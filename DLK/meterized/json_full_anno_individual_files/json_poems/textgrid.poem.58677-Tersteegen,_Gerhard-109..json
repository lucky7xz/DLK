{"textgrid.poem.58677": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "109.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder seinen Weg nach seinem Lichte gehet,", "tokens": ["Ein", "je\u00b7der", "sei\u00b7nen", "Weg", "nach", "sei\u00b7nem", "Lich\u00b7te", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unterschied ist gro\u00df bei denen, die auch fromm;", "tokens": ["Der", "Un\u00b7ter\u00b7schied", "ist", "gro\u00df", "bei", "de\u00b7nen", ",", "die", "auch", "fromm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "PRELS", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das priesterlich Geschlecht, das vor dem Herren stehet,", "tokens": ["Das", "pries\u00b7ter\u00b7lich", "Ge\u00b7schlecht", ",", "das", "vor", "dem", "Her\u00b7ren", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das innerliche Volk hat gar kein Eigentum.", "tokens": ["Das", "in\u00b7ner\u00b7li\u00b7che", "Volk", "hat", "gar", "kein", "Ei\u00b7gen\u00b7tum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ihr Erbteil ist der Herr, dem sie sich ganz ergeben,", "tokens": ["Ihr", "Erb\u00b7teil", "ist", "der", "Herr", ",", "dem", "sie", "sich", "ganz", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und den besitzen sie im Grunde wesentlich,", "tokens": ["Und", "den", "be\u00b7sit\u00b7zen", "sie", "im", "Grun\u00b7de", "we\u00b7sent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie haben au\u00dfer Gott in nichts sonst Lust noch Leben,", "tokens": ["Sie", "ha\u00b7ben", "au\u00b7\u00dfer", "Gott", "in", "nichts", "sonst", "Lust", "noch", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "PIS", "ADV", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nur Eines ist ihr Teil, das bleibet ewiglich.", "tokens": ["Nur", "Ei\u00b7nes", "ist", "ihr", "Teil", ",", "das", "blei\u00b7bet", "e\u00b7wig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein jeder seinen Weg nach seinem Lichte gehet,", "tokens": ["Ein", "je\u00b7der", "sei\u00b7nen", "Weg", "nach", "sei\u00b7nem", "Lich\u00b7te", "ge\u00b7het", ","], "token_info": ["word", "word", "word", "wor