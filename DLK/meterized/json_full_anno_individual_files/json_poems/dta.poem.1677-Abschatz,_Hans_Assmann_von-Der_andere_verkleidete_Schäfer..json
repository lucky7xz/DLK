{"dta.poem.1677": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der andere verkleidete Sch\u00e4fer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ne Wilden/ seynd gelinder/ ", "tokens": ["Sch\u00f6\u00b7ne", "Wil\u00b7den", "/", "seynd", "ge\u00b7lin\u00b7der", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernt verliebt und freundlich seyn/", "tokens": ["Lernt", "ver\u00b7liebt", "und", "freund\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KON", "ADJD", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebt beliebte Sch\u00e4fers-Kinder/", "tokens": ["Liebt", "be\u00b7lieb\u00b7te", "Sch\u00e4\u00b7fer\u00b7sKin\u00b7der", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr m\u00fcst euch doch finden drein.", "tokens": ["Ihr", "m\u00fcst", "euch", "doch", "fin\u00b7den", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Unsre Hertzen sind ergeben", "tokens": ["Uns\u00b7re", "Hert\u00b7zen", "sind", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu empfinden s\u00fcsse Brunst/", "tokens": ["Zu", "emp\u00b7fin\u00b7den", "s\u00fcs\u00b7se", "Brunst", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum wolt ihr widerstreben/", "tokens": ["Wa\u00b7rum", "wolt", "ihr", "wi\u00b7der\u00b7stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo doch alle M\u00fch umsunst?", "tokens": ["Wo", "doch", "al\u00b7le", "M\u00fch", "um\u00b7sunst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Geht und sucht die Lust beyzeiten", "tokens": ["Geht", "und", "sucht", "die", "Lust", "bey\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die aus reiner Lieb entspringt/", "tokens": ["Die", "aus", "rei\u00b7ner", "Lieb", "ent\u00b7springt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eh euch tausend Bitterkeiten", "tokens": ["Eh", "euch", "tau\u00b7send", "Bit\u00b7ter\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eur verlebtes Alter bringt.", "tokens": ["Eur", "ver\u00b7leb\u00b7tes", "Al\u00b7ter", "bringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Leben und ohn Liebe leben", "tokens": ["Le\u00b7ben", "und", "ohn", "Lie\u00b7be", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist bey Leben seyn halb todt/", "tokens": ["Ist", "bey", "Le\u00b7ben", "seyn", "halb", "todt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PPOSAT", "ADJD", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasst uns Hertz um Hertze geben/", "tokens": ["Lasst", "uns", "Hertz", "um", "Hert\u00b7ze", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "VVFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So habt ihr und wir nicht Noth.", "tokens": ["So", "habt", "ihr", "und", "wir", "nicht", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KON", "PPER", "PTKNEG", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}