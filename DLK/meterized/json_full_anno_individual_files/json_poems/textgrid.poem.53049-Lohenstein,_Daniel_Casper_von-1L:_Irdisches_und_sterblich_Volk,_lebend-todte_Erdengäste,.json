{"textgrid.poem.53049": {"metadata": {"author": {"name": "Lohenstein, Daniel Casper von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Irdisches und sterblich Volk, lebend-todte Erdeng\u00e4ste,", "genre": "verse", "period": "N.A.", "pub_year": 1659, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Irdisches und sterblich Volk, lebend-todte Erdeng\u00e4ste,", "tokens": ["Ir\u00b7di\u00b7sches", "und", "sterb\u00b7lich", "Volk", ",", "le\u00b7ben\u00b7dtod\u00b7te", "Er\u00b7den\u00b7g\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJD", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ihr Verw\u00fcrflinge des Himmels, ihr Gespenster dieser Welt,", "tokens": ["Ihr", "Ver\u00b7w\u00fcrf\u00b7lin\u00b7ge", "des", "Him\u00b7mels", ",", "ihr", "Ge\u00b7spens\u00b7ter", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "PDAT", "NN", "$,"], "meter": "+-++--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Denen Nichts, als falsche Waare, Nichts, als Rauch und Wind gef\u00e4llt,", "tokens": ["De\u00b7nen", "Nichts", ",", "als", "fal\u00b7sche", "Waa\u00b7re", ",", "Nichts", ",", "als", "Rauch", "und", "Wind", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "KOUS", "ADJA", "NN", "$,", "PIS", "$,", "KOUS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Thoren, klettert und besteigt die bequalmten Ehren\u00e4ste,", "tokens": ["Tho\u00b7ren", ",", "klet\u00b7tert", "und", "be\u00b7steigt", "die", "be\u00b7qualm\u00b7ten", "Eh\u00b7re\u00b7n\u00e4s\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Setzt euch S\u00e4ulen von Porphyr, mauert euch aus Gold Pal\u00e4ste,", "tokens": ["Setzt", "euch", "S\u00e4u\u00b7len", "von", "Por\u00b7phyr", ",", "mau\u00b7ert", "euch", "aus", "Gold", "Pa\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NE", "$,", "VVFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Festigt Tempel euch aus Marmor, der der Zeit die Wage h\u00e4lt,", "tokens": ["Fes\u00b7tigt", "Tem\u00b7pel", "euch", "aus", "Mar\u00b7mor", ",", "der", "der", "Zeit", "die", "Wa\u00b7ge", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "APPR", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Rafft zu euch mit gicht'gen Klauen er verdammten Klumpen Geld,", "tokens": ["Rafft", "zu", "euch", "mit", "gicht'\u00b7gen", "Klau\u00b7en", "er", "ver\u00b7damm\u00b7ten", "Klum\u00b7pen", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "ADJA", "NN", "PPER", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Macht euch euer stolzes Lob durch gelehrte Schriften feste;", "tokens": ["Macht", "euch", "eu\u00b7er", "stol\u00b7zes", "Lob", "durch", "ge\u00b7lehr\u00b7te", "Schrif\u00b7ten", "fes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "ADJA", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber wi\u00dft, wenn das Verh\u00e4ngni\u00df euer Lebensgarn rei\u00dft ab,", "tokens": ["A\u00b7ber", "wi\u00dft", ",", "wenn", "das", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "eu\u00b7er", "Le\u00b7bens\u00b7garn", "rei\u00dft", "ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Schwindet Wissenschaft und Kunst, Sch\u00e4tze, Reichthum, Ehr' und Titel,", "tokens": ["Schwin\u00b7det", "Wis\u00b7sen\u00b7schaft", "und", "Kunst", ",", "Sch\u00e4t\u00b7ze", ",", "Reicht\u00b7hum", ",", "Ehr'", "und", "Ti\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Und ihr nehmet Nichts mit euch, als den nackten Sterbekittel,", "tokens": ["Und", "ihr", "neh\u00b7met", "Nichts", "mit", "euch", ",", "als", "den", "nack\u00b7ten", "Ster\u00b7be\u00b7kit\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIS", "APPR", "PPER", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wo ihr auch noch aus dem allen noch erschwitzet Sarg und Grab!", "tokens": ["Wo", "ihr", "auch", "noch", "aus", "