{"textgrid.poem.52504": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wandrer! Erklimme sie nicht mehr, die Alpenh\u00f6hen \u2013 sie sind, ach!", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wandrer! Erklimme sie nicht mehr, die Alpenh\u00f6hen \u2013 sie sind, ach!", "tokens": ["Wand\u00b7rer", "!", "Er\u00b7klim\u00b7me", "sie", "nicht", "mehr", ",", "die", "Al\u00b7pen\u00b7h\u00f6\u00b7hen", "\u2013", "sie", "sind", ",", "ach", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "ART", "NN", "$(", "PPER", "VAFIN", "$,", "ITJ", "$."], "meter": "+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Blutig von Frevlern entehrt, \u00fcber mit Schande bedeckt:", "tokens": ["Blu\u00b7tig", "von", "Frev\u00b7lern", "ent\u00b7ehrt", ",", "\u00fc\u00b7ber", "mit", "Schan\u00b7de", "be\u00b7deckt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVPP", "$,", "APPR", "APPR", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wandrer! Besuche nicht mehr die Alpenthale, \u2013 wo vormals", "tokens": ["Wand\u00b7rer", "!", "Be\u00b7su\u00b7che", "nicht", "mehr", "die", "Al\u00b7pen\u00b7tha\u00b7le", ",", "\u2013", "wo", "vor\u00b7mals"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "PTKNEG", "ADV", "ART", "NN", "$,", "$(", "PWAV", "ADV"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Schallete Friedensgejauchz, t\u00f6nte des Hirten Schalmei,", "tokens": ["Schal\u00b7le\u00b7te", "Frie\u00b7dens\u00b7ge\u00b7jauchz", ",", "t\u00f6n\u00b7te", "des", "Hir\u00b7ten", "Schal\u00b7mei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+--++--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und der Herde Geklingel, und frommer Liebe Ges\u00e4nge:", "tokens": ["Und", "der", "Her\u00b7de", "Ge\u00b7klin\u00b7gel", ",", "und", "from\u00b7mer", "Lie\u00b7be", "Ge\u00b7s\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,", "KON", "ADJA", "NN", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Heulet itzt Hunger und Schmerz; \u00e4chzt nur das Elend verwaist.", "tokens": ["Heu\u00b7let", "itzt", "Hun\u00b7ger", "und", "Schmerz", ";", "\u00e4chzt", "nur", "das", "E\u00b7lend", "ver\u00b7waist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$.", "VVFIN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ungl\u00fcckselige Schweiz! Dich konnte dein Genius nicht mehr", "tokens": ["Un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7ge", "Schweiz", "!", "Dich", "konn\u00b7te", "dein", "Ge\u00b7nius", "nicht", "mehr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "ADV"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.8": {"text": "Retten, wie stark er dich liebt', weil das Verh\u00e4ngnis es wehrt.", "tokens": ["Ret\u00b7ten", ",", "wie", "stark", "er", "dich", "liebt'", ",", "weil", "das", "Ver\u00b7h\u00e4ng\u00b7nis", "es", "wehrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-++-+-+--+", "measure": "iambic.septa.invert"}, "line.9": {"text": "Aber er wollte dich doch im Bild noch erhalten, Geliebte!", "tokens": ["A\u00b7ber", "er", "woll\u00b7te", "dich", "doch", "im", "Bild", "noch", "er\u00b7hal\u00b7ten", ",", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "VVINF", "$,", "NN", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Und er begeisterte ", "tokens": ["Und", "er", "be\u00b7geis\u00b7ter\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Hessens beseeleter Hand entstr\u00f6mten die hehren Gestalten,", "tokens": ["Hes\u00b7sens", "be\u00b7see\u00b7le\u00b7ter", "Hand", "ent\u00b7str\u00f6m\u00b7ten", "die", "heh\u00b7ren", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Wie in das Aug' sie gestrahlt, treu, wie das Herz sie gef\u00fchlt,", "tokens": ["Wie", "in", "das", "Aug'", "sie", "ge\u00b7strahlt", ",", "treu", ",", "wie", "das", "Herz", "sie", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "VVPP", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Als das eidesgen\u00f6ssische Land in seinen Reizen noch bl\u00fchte,", "tokens": ["Als", "das", "ei\u00b7des\u00b7ge\u00b7n\u00f6s\u00b7si\u00b7sche", "Land", "in", "sei\u00b7nen", "Rei\u00b7zen", "noch", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+--+-+-+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Als es noch unentweiht, w\u00fcrdig den Wandrer entz\u00fcckt!", "tokens": ["Als", "es", "noch", "un\u00b7ent\u00b7weiht", ",", "w\u00fcr\u00b7dig", "den", "Wand\u00b7rer", "ent\u00b7z\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "---+-+---+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Denn des Bildenden Seele verd\u00fcsterte nichts von der schwarzen", "tokens": ["Denn", "des", "Bil\u00b7den\u00b7den", "See\u00b7le", "ver\u00b7d\u00fcs\u00b7ter\u00b7te", "nichts", "von", "der", "schwar\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "PIS", "APPR", "ART", "ADJA"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.16": {"text": "Ahnung der Tr\u00fcbsal, die bald \u00fcber Helvetien kam.", "tokens": ["Ah\u00b7nung", "der", "Tr\u00fcb\u00b7sal", ",", "die", "bald", "\u00fc\u00b7ber", "Hel\u00b7ve\u00b7ti\u00b7en", "kam", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Wenn aus zerri\u00dfnem Gew\u00f6lk er die Alpenh\u00fcgel begl\u00e4nzte,", "tokens": ["Wenn", "aus", "zer\u00b7ri\u00df\u00b7nem", "Ge\u00b7w\u00f6lk", "er", "die", "Al\u00b7pen\u00b7h\u00fc\u00b7gel", "be\u00b7gl\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Wallete wonniglich warm schlagend die biedere Brust,", "tokens": ["Wal\u00b7le\u00b7te", "won\u00b7nig\u00b7lich", "warm", "schla\u00b7gend", "die", "bie\u00b7de\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJD", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.19": {"text": "Und da sah er Gesichte von paradiesischen Tagen,", "tokens": ["Und", "da", "sah", "er", "Ge\u00b7sich\u00b7te", "von", "pa\u00b7ra\u00b7die\u00b7si\u00b7schen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Da\u00df seines Blickes Flamm' alle Farben verkl\u00e4rt! \u2013", "tokens": ["Da\u00df", "sei\u00b7nes", "Bli\u00b7ckes", "Flamm'", "al\u00b7le", "Far\u00b7ben", "ver\u00b7kl\u00e4rt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Fremdling! in ", "tokens": ["Fremd\u00b7ling", "!", "in"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$.", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Willst du das Schweizerland sehn, wie es noch Schweizerland war.", "tokens": ["Willst", "du", "das", "Schwei\u00b7zer\u00b7land", "sehn", ",", "wie", "es", "noch", "Schwei\u00b7zer\u00b7land", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,", "PWAV", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+--+-++", "measure": "iambic.septa.relaxed"}, "line.23": {"text": "Und dann weine dem Bildner die Thr\u00e4ne der dankenden Sehnsucht;", "tokens": ["Und", "dann", "wei\u00b7ne", "dem", "Bild\u00b7ner", "die", "Thr\u00e4\u00b7ne", "der", "dan\u00b7ken\u00b7den", "Sehn\u00b7sucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.24": {"text": "Siehe! der Fr\u00fchling bekr\u00e4nzt ihn nicht \u2013 bekleidet", "tokens": ["Sie\u00b7he", "!", "der", "Fr\u00fch\u00b7ling", "be\u00b7kr\u00e4nzt", "ihn", "nicht", "\u2013", "be\u00b7klei\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "$.", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Ach! Ihm schwanden die hohen Gebilde der t\u00e4uschenden Hoffnung,", "tokens": ["Ach", "!", "Ihm", "schwan\u00b7den", "die", "ho\u00b7hen", "Ge\u00b7bil\u00b7de", "der", "t\u00e4u\u00b7schen\u00b7den", "Hoff\u00b7nung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.26": {"text": "Und er bebte zu sehn, Schweiz, deine Schande, dein Weh,", "tokens": ["Und", "er", "beb\u00b7te", "zu", "sehn", ",", "Schweiz", ",", "dei\u00b7ne", "Schan\u00b7de", ",", "dein", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKZU", "VVINF", "$,", "NE", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.27": {"text": "Was der Verrat erzeugt, was mordende Raubsucht geboren; \u2013", "tokens": ["Was", "der", "Ver\u00b7rat", "er\u00b7zeugt", ",", "was", "mor\u00b7den\u00b7de", "Raub\u00b7sucht", "ge\u00b7bo\u00b7ren", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,", "PWS", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Sah aus der Alpenburg noch zitternde Unschuld verjagt; \u2013", "tokens": ["Sah", "aus", "der", "Al\u00b7pen\u00b7burg", "noch", "zit\u00b7tern\u00b7de", "Un\u00b7schuld", "ver\u00b7jagt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.29": {"text": "Und da wurd' es dunkel der Seele des f\u00fchlenden K\u00fcnstlers,", "tokens": ["Und", "da", "wurd'", "es", "dun\u00b7kel", "der", "See\u00b7le", "des", "f\u00fch\u00b7len\u00b7den", "K\u00fcnst\u00b7lers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Da\u00df ihm der Pinsel entfiel, \u2013 brach das erschrockene Herz!", "tokens": ["Da\u00df", "ihm", "der", "Pin\u00b7sel", "ent\u00b7fiel", ",", "\u2013", "brach", "das", "er\u00b7schro\u00b7cke\u00b7ne", "Herz", "!"], "to