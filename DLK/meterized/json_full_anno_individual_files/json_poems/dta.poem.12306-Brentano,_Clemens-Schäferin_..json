{"dta.poem.12306": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4ferin .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schlaf allhie,               ", "tokens": ["Ich", "schlaf", "all\u00b7hie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Bey meinem Vieh,", "tokens": ["Bey", "mei\u00b7nem", "Vieh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ich schlaf im Moos,", "tokens": ["Ich", "schlaf", "im", "Moos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dem Gl\u00fcck im Schoos;", "tokens": ["Dem", "Gl\u00fcck", "im", "Schoos", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dein Schlo\u00df ich schau,", "tokens": ["Dein", "Schlo\u00df", "ich", "schau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Es liegt vor mir,", "tokens": ["Es", "liegt", "vor", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zu sagen schier,", "tokens": ["Zu", "sa\u00b7gen", "schier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Wie k\u00fchler Thau.", "tokens": ["Wie", "k\u00fch\u00b7ler", "Thau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Kommt Morgenroth", "tokens": ["Kommt", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word"], "pos": ["VVIMP", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So lob ich Gott,", "tokens": ["So", "lob", "ich", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das Feldgeschrey", "tokens": ["Das", "Feld\u00b7ge\u00b7schrey"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wird jubelnd neu", "tokens": ["Wird", "ju\u00b7belnd", "neu"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Beym goldnen Lohn,", "tokens": ["Beym", "gold\u00b7nen", "Lohn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Die Morgenstund", "tokens": ["Die", "Mor\u00b7gen\u00b7stund"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Hat Gold im Mund,", "tokens": ["Hat", "Gold", "im", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Baut mir den Thron.", "tokens": ["Baut", "mir", "den", "Thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Vom Schlo\u00df ich zieh,", "tokens": ["Vom", "Schlo\u00df", "ich", "zieh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Zu dir ich flieh,", "tokens": ["Zu", "dir", "ich", "flieh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Lieb Sch\u00e4ferin,", "tokens": ["Lieb", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Nach deinem Sinn", "tokens": ["Nach", "dei\u00b7nem", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mein Scepter wird", "tokens": ["Mein", "Scep\u00b7ter", "wird"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ein Hirtenstab,", "tokens": ["Ein", "Hir\u00b7ten\u00b7stab", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und was ich hab,", "tokens": ["Und", "was", "ich", "hab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Dich Sch\u00e4frin ziert.", "tokens": ["Dich", "Sch\u00e4f\u00b7rin", "ziert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich Sch\u00e4ferin,", "tokens": ["Ich", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Mit leichtem Sinn,", "tokens": ["Mit", "leich\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sing ruhig fort", "tokens": ["Sing", "ru\u00b7hig", "fort"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKVZ"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "Mein sinnig Wort:", "tokens": ["Mein", "sin\u00b7nig", "Wort", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ein jeder bleib", "tokens": ["Ein", "je\u00b7der", "bleib"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Bey seiner Heerd,", "tokens": ["Bey", "sei\u00b7ner", "Heerd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Den K\u00f6nig ehrt", "tokens": ["Den", "K\u00f6\u00b7nig", "ehrt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Kein Sch\u00e4ferweib.", "tokens": ["Kein", "Sch\u00e4\u00b7fer\u00b7weib", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}