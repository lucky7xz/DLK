{"dta.poem.20997": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxI.  \n Eines Schlaffs\u00fcchtigen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt ein fauler Leib/ der aus dem Tage Nacht/", "tokens": ["Hier", "liegt", "ein", "fau\u00b7ler", "Leib", "/", "der", "aus", "dem", "Ta\u00b7ge", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ART", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aus dem Leben Tod/ durch Schlaffen hat ge-", "tokens": ["Und", "aus", "dem", "Le\u00b7ben", "Tod", "/", "durch", "Schlaf\u00b7fen", "hat", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$(", "APPR", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "macht.", "tokens": ["macht", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Aus allzugrosser Furcht/ da\u00df man ihn nicht erwecket", "tokens": ["Aus", "all\u00b7zu\u00b7gros\u00b7ser", "Furcht", "/", "da\u00df", "man", "ihn", "nicht", "er\u00b7we\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$(", "KOUS", "PIS", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So hat er sich alhier in diese Grufft verstecket.", "tokens": ["So", "hat", "er", "sich", "al\u00b7hier", "in", "die\u00b7se", "Grufft", "ver\u00b7ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}