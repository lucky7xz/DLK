{"textgrid.poem.61390": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwei Reiter reiten vom K\u00f6nigsschlo\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Reiter reiten vom K\u00f6nigsschlo\u00df,", "tokens": ["Zwei", "Rei\u00b7ter", "rei\u00b7ten", "vom", "K\u00f6\u00b7nigs\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie reiten hinab zum Strande;", "tokens": ["Sie", "rei\u00b7ten", "hin\u00b7ab", "zum", "Stran\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In hohen L\u00fcften pfeift der Wind,", "tokens": ["In", "ho\u00b7hen", "L\u00fcf\u00b7ten", "pfeift", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wellen sch\u00e4umen zu Lande.", "tokens": ["Die", "Wel\u00b7len", "sch\u00e4u\u00b7men", "zu", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der K\u00f6nig spricht zum Pagen sein,", "tokens": ["Der", "K\u00f6\u00b7nig", "spricht", "zum", "Pa\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er spricht's in finsterem Mute:", "tokens": ["Er", "spricht's", "in", "fins\u00b7te\u00b7rem", "Mu\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbwer gab das R\u00f6slein dir, Gesell,", "tokens": ["\u00bb", "wer", "gab", "das", "R\u00f6s\u00b7lein", "dir", ",", "Ge\u00b7sell", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ART", "NN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das R\u00f6slein auf deinem Hute?\u00ab", "tokens": ["Das", "R\u00f6s\u00b7lein", "auf", "dei\u00b7nem", "Hu\u00b7te", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbdas R\u00f6slein gab die Mutter mir,", "tokens": ["\u00bb", "das", "R\u00f6s\u00b7lein", "gab", "die", "Mut\u00b7ter", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sie mich lie\u00df in Sorgen!", "tokens": ["Da", "sie", "mich", "lie\u00df", "in", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich stell's in Wasser jede Nacht,", "tokens": ["Ich", "stell's", "in", "Was\u00b7ser", "je\u00b7de", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So bl\u00fcht es jeden Morgen.\u00ab", "tokens": ["So", "bl\u00fcht", "es", "je\u00b7den", "Mor\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie reiten entlang an der blauen Bucht,", "tokens": ["Sie", "rei\u00b7ten", "ent\u00b7lang", "an", "der", "blau\u00b7en", "Bucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Woge murrt eint\u00f6nig,", "tokens": ["Die", "Wo\u00b7ge", "murrt", "ein\u00b7t\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die M\u00f6wen fliegen kreischend auf,", "tokens": ["Die", "M\u00f6\u00b7wen", "flie\u00b7gen", "krei\u00b7schend", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum andern fragt der K\u00f6nig:", "tokens": ["Zum", "an\u00b7dern", "fragt", "der", "K\u00f6\u00b7nig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbwes ist die Locke, die ich sah", "tokens": ["\u00bb", "wes", "ist", "die", "Lo\u00b7cke", ",", "die", "ich", "sah"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An deine Brust geschlungen,", "tokens": ["An", "dei\u00b7ne", "Brust", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da dir vorhin vom scharfen Ritt", "tokens": ["Da", "dir", "vor\u00b7hin", "vom", "schar\u00b7fen", "Ritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Reitwams aufgesprungen?\u00ab", "tokens": ["Das", "Reit\u00b7wams", "auf\u00b7ge\u00b7sprun\u00b7gen", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbdas ist meiner Schwester lichtbraun Haar,", "tokens": ["\u00bb", "das", "ist", "mei\u00b7ner", "Schwes\u00b7ter", "licht\u00b7braun", "Haar", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "So fein und weich wie Seiden!", "tokens": ["So", "fein", "und", "weich", "wie", "Sei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es duftet s\u00fc\u00df wie Rosen\u00f6l,", "tokens": ["Es", "duf\u00b7tet", "s\u00fc\u00df", "wie", "Ro\u00b7sen\u00b7\u00f6l", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie weinte drauf beim Scheiden.\u00ab", "tokens": ["Sie", "wein\u00b7te", "drauf", "beim", "Schei\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie reiten hinauf den Felsensteig;", "tokens": ["Sie", "rei\u00b7ten", "hin\u00b7auf", "den", "Fel\u00b7sen\u00b7steig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Am Pfad sind eingeschnitten", "tokens": ["Am", "Pfad", "sind", "ein\u00b7ge\u00b7schnit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Blutrunen aus uralter Zeit,", "tokens": ["Blut\u00b7ru\u00b7nen", "aus", "ur\u00b7al\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der K\u00f6nig fragt zum dritten:", "tokens": ["Der", "K\u00f6\u00b7nig", "fragt", "zum", "drit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbsag' an und rede die Wahrheit mir,", "tokens": ["\u00bb", "sag'", "an", "und", "re\u00b7de", "die", "Wahr\u00b7heit", "mir", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "KON", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gesell, es gilt dein Leben,", "tokens": ["Ge\u00b7sell", ",", "es", "gilt", "dein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer hat den Ring am Finger dir,", "tokens": ["Wer", "hat", "den", "Ring", "am", "Fin\u00b7ger", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den goldnen Ring gegeben?\u00ab", "tokens": ["Den", "gold\u00b7nen", "Ring", "ge\u00b7ge\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbdie mir den Ring am Finger gab,", "tokens": ["\u00bb", "die", "mir", "den", "Ring", "am", "Fin\u00b7ger", "gab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gab mir ihr Herz desgleichen;", "tokens": ["Gab", "mir", "ihr", "Herz", "des\u00b7glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist die allersch\u00f6nste Maid", "tokens": ["Das", "ist", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Maid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In allen deinen Reichen.\u00ab", "tokens": ["In", "al\u00b7len", "dei\u00b7nen", "Rei\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Des K\u00f6nigs Stirn wird rot wie Blut,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Stirn", "wird", "rot", "wie", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Augen zornig ihm brennen;", "tokens": ["Die", "Au\u00b7gen", "zor\u00b7nig", "ihm", "bren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbder Ring ist meines Kindes Ring!", "tokens": ["\u00bb", "der", "Ring", "ist", "mei\u00b7nes", "Kin\u00b7des", "Ring", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Blinken mu\u00df ich kennen.", "tokens": ["Sein", "Blin\u00b7ken", "mu\u00df", "ich", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und wagtest du in frecher Lust", "tokens": ["Und", "wag\u00b7test", "du", "in", "fre\u00b7cher", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um ihren Leib zu werben,", "tokens": ["Um", "ih\u00b7ren", "Leib", "zu", "wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So dauert dein jungfrisch Leben mich nicht,", "tokens": ["So", "dau\u00b7ert", "dein", "jung\u00b7frisch", "Le\u00b7ben", "mich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Des Todes mu\u00dft du sterben.\u00ab", "tokens": ["Des", "To\u00b7des", "mu\u00dft", "du", "ster\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er zieht hervor sein scharfes Schwert,", "tokens": ["Er", "zieht", "her\u00b7vor", "sein", "schar\u00b7fes", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er st\u00f6\u00dft es durchs Herz dem Gesellen;", "tokens": ["Er", "st\u00f6\u00dft", "es", "durchs", "Herz", "dem", "Ge\u00b7sel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das Blut flie\u00dft \u00fcber den Runenstein", "tokens": ["Das", "Blut", "flie\u00dft", "\u00fc\u00b7ber", "den", "Ru\u00b7nen\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hinunter in die Wellen.", "tokens": ["Hin\u00b7un\u00b7ter", "in", "die", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er wirft den Leichnam in die Flut:", "tokens": ["Er", "wirft", "den", "Leich\u00b7nam", "in", "die", "Flut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbund steht so hoch dein Sinnen,", "tokens": ["\u00bb", "und", "steht", "so", "hoch", "dein", "Sin\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So magst du um die K\u00f6nigin jetzt", "tokens": ["So", "magst", "du", "um", "die", "K\u00f6\u00b7ni\u00b7gin", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Wassernixen minnen!\u00ab", "tokens": ["Der", "Was\u00b7ser\u00b7ni\u00b7xen", "min\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Den Strand entlang zum K\u00f6nigsschlo\u00df", "tokens": ["Den", "Strand", "ent\u00b7lang", "zum", "K\u00f6\u00b7nigs\u00b7schlo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPO", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heimreitet ein d\u00fcsterer Reiter;", "tokens": ["Heim\u00b7rei\u00b7tet", "ein", "d\u00fcs\u00b7te\u00b7rer", "Rei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Hinaus ins Meer die Leiche schwimmt,", "tokens": ["Hin\u00b7aus", "ins", "Meer", "die", "Lei\u00b7che", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART