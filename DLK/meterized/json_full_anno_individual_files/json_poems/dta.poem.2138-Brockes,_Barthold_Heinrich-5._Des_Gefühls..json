{"dta.poem.2138": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5. Des Gef\u00fchls.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Os\u00fc\u00dfer Sinn, der allgemein,", "tokens": ["O\u00b7s\u00fc\u00b7\u00dfer", "Sinn", ",", "der", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In welchem Riechen, H\u00f6ren, Sehen", "tokens": ["In", "wel\u00b7chem", "Rie\u00b7chen", ",", "H\u00f6\u00b7ren", ",", "Se\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "VVFIN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Schmecken eigentlich bestehen", "tokens": ["Und", "Schme\u00b7cken", "ei\u00b7gent\u00b7lich", "be\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und recht darinn vereinet seyn!", "tokens": ["Und", "recht", "da\u00b7rinn", "ver\u00b7ei\u00b7net", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch dich wird noch insonderheit", "tokens": ["Durch", "dich", "wird", "noch", "in\u00b7son\u00b7der\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ganz besondrer Lieblichkeit", "tokens": ["Mit", "ganz", "be\u00b7sond\u00b7rer", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Gott auf unsre Lust gezielet,", "tokens": ["Von", "Gott", "auf", "uns\u00b7re", "Lust", "ge\u00b7zie\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Denn unser ganzer K\u00f6rper f\u00fchlet.", "tokens": ["Denn", "un\u00b7ser", "gan\u00b7zer", "K\u00f6r\u00b7per", "f\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch scheinest du mit unsrer Hand", "tokens": ["Doch", "schei\u00b7nest", "du", "mit", "uns\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jm ganz besonderem Verband", "tokens": ["Jm", "ganz", "be\u00b7son\u00b7de\u00b7rem", "Ver\u00b7band"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und in der regen Finger Spitzen,", "tokens": ["Und", "in", "der", "re\u00b7gen", "Fin\u00b7ger", "Spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Als deinem Aufenthalt, zu sitzen.", "tokens": ["Als", "dei\u00b7nem", "Auf\u00b7ent\u00b7halt", ",", "zu", "sit\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Wodurch wir offenbar entdecken,", "tokens": ["Wo\u00b7durch", "wir", "of\u00b7fen\u00b7bar", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df in der Nerven \u00e4u\u00dfern Ecken", "tokens": ["Da\u00df", "in", "der", "Ner\u00b7ven", "\u00e4u\u00b7\u00dfern", "E\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Des Geistes Kr\u00e4fte sich erstrecken:", "tokens": ["Des", "Geis\u00b7tes", "Kr\u00e4f\u00b7te", "sich", "er\u00b7stre\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wie wir, wenn linde Winde k\u00fchlen,", "tokens": ["Wie", "wir", ",", "wenn", "lin\u00b7de", "Win\u00b7de", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "KOUS", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Auch wenn uns laue W\u00e4rme schmeichelt,", "tokens": ["Auch", "wenn", "uns", "lau\u00b7e", "W\u00e4r\u00b7me", "schmei\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Von beyden angenehm gestreichelt,", "tokens": ["Von", "bey\u00b7den", "an\u00b7ge\u00b7nehm", "ge\u00b7strei\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "An allen \u00e4u\u00dfern Theilen f\u00fchlen.", "tokens": ["An", "al\u00b7len", "\u00e4u\u00b7\u00dfern", "Thei\u00b7len", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Man mu\u00df in dir auch nicht vergessen,", "tokens": ["Man", "mu\u00df", "in", "dir", "auch", "nicht", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die Lust der Liebe zu ermessen,", "tokens": ["Die", "Lust", "der", "Lie\u00b7be", "zu", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die, wenn man sie nur nach der Pflicht,", "tokens": ["Die", ",", "wenn", "man", "sie", "nur", "nach", "der", "Pflicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIS", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Die vorgeschrieben ist, verricht,", "tokens": ["Die", "vor\u00b7ge\u00b7schrie\u00b7ben", "ist", ",", "ver\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Mit Recht ein Wundergut zu nennen,", "tokens": ["Mit", "Recht", "ein", "Wun\u00b7der\u00b7gut", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Die, da\u00df das menschliche Geschlechte", "tokens": ["Die", ",", "da\u00df", "das", "menschli\u00b7che", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Auf Erden fortgepflanzt seyn m\u00f6chte,", "tokens": ["Auf", "Er\u00b7den", "fort\u00b7ge\u00b7pflanzt", "seyn", "m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Uns Gott auf Erden wollen g\u00f6nnen.", "tokens": ["Uns", "Gott", "auf", "Er\u00b7den", "wol\u00b7len", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Auf! la\u00dft uns denn im F\u00fchlen, Schmecken,", "tokens": ["Auf", "!", "la\u00dft", "uns", "denn", "im", "F\u00fch\u00b7len", ",", "Schme\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "$.", "VVIMP", "PPER", "ADV", "APPRART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Jm H\u00f6ren, Riechen, Sehn, entdecken,", "tokens": ["Jm", "H\u00f6\u00b7ren", ",", "Rie\u00b7chen", ",", "Sehn", ",", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Was sich darinn f\u00fcr Wunder f\u00fcgen;", "tokens": ["Was", "sich", "da\u00b7rinn", "f\u00fcr", "Wun\u00b7der", "f\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PAV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und wie uns, auf so manche Weise,", "tokens": ["Und", "wie", "uns", ",", "auf", "so", "man\u00b7che", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "$,", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Der Sch\u00f6pfer suche zu vergn\u00fcgen.", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "su\u00b7che", "zu", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Gebraucht sie denn zu seinem Preise!", "tokens": ["Ge\u00b7braucht", "sie", "denn", "zu", "sei\u00b7nem", "Prei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}