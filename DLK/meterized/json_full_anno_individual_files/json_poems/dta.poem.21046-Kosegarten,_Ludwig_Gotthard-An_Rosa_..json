{"dta.poem.21046": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "An Rosa .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Warum bist du nicht hier, meine Geliebteste,               ", "tokens": ["Wa\u00b7rum", "bist", "du", "nicht", "hier", ",", "mei\u00b7ne", "Ge\u00b7lieb\u00b7tes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dass mich g\u00fcrte dein Arm, dass mich dein H\u00e4ndedruck", "tokens": ["Dass", "mich", "g\u00fcr\u00b7te", "dein", "Arm", ",", "dass", "mich", "dein", "H\u00e4n\u00b7de\u00b7druck"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Labe, dass du mich pressest", "tokens": ["La\u00b7be", ",", "dass", "du", "mich", "pres\u00b7sest"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "An dein schlagendes Schwesterherz.", "tokens": ["An", "dein", "schla\u00b7gen\u00b7des", "Schwes\u00b7ter\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Rosa, bist du mir hold? Rosa, so hold, wie ich,", "tokens": ["Ro\u00b7sa", ",", "bist", "du", "mir", "hold", "?", "Ro\u00b7sa", ",", "so", "hold", ",", "wie", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "PPER", "ADJD", "$.", "NE", "$,", "ADV", "ADJD", "$,", "PWAV", "PPER", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "War dir keiner, und wird keiner dir wieder seyn", "tokens": ["War", "dir", "kei\u00b7ner", ",", "und", "wird", "kei\u00b7ner", "dir", "wie\u00b7der", "seyn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PIS", "$,", "KON", "VAFIN", "PIS", "PPER", "ADV", "VAINF"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Von den S\u00f6hnen der Erde,", "tokens": ["Von", "den", "S\u00f6h\u00b7nen", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Von den S\u00f6hnen Elisiums.", "tokens": ["Von", "den", "S\u00f6h\u00b7nen", "E\u00b7lis\u00b7i\u00b7ums", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "W\u00e4rmer, Rosa, f\u00fcrwahr, w\u00e4rmer und z\u00e4rtlicher", "tokens": ["W\u00e4r\u00b7mer", ",", "Ro\u00b7sa", ",", "f\u00fcr\u00b7wahr", ",", "w\u00e4r\u00b7mer", "und", "z\u00e4rt\u00b7li\u00b7cher"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "ADV", "$,", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "K\u00f6nnte nimmer f\u00fcr dich schlagen mein f\u00fchlend Herz,", "tokens": ["K\u00f6nn\u00b7te", "nim\u00b7mer", "f\u00fcr", "dich", "schla\u00b7gen", "mein", "f\u00fch\u00b7lend", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VVFIN", "PPOSAT", "CARD", "NN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": " h\u00e4tt' Ein Schooss uns geboren,", "tokens": ["h\u00e4tt'", "Ein", "Scho\u00b7oss", "uns", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tt' uns einerley Brust ges\u00e4ugt.", "tokens": ["H\u00e4tt'", "uns", "ei\u00b7ner\u00b7ley", "Brust", "ge\u00b7s\u00e4ugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Matte labet der Quell, M\u00fcde der Abendstern,", "tokens": ["Mat\u00b7te", "la\u00b7bet", "der", "Quell", ",", "M\u00fc\u00b7de", "der", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+--++-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Irre Wandrer der Mond, Kranke das Morgenroth;", "tokens": ["Ir\u00b7re", "Wand\u00b7rer", "der", "Mond", ",", "Kran\u00b7ke", "das", "Mor\u00b7gen\u00b7roth", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Mich erlabet, Geliebte,", "tokens": ["Mich", "er\u00b7la\u00b7bet", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Dein Umfangen am kr\u00e4ftigsten.", "tokens": ["Dein", "Um\u00b7fan\u00b7gen", "am", "kr\u00e4f\u00b7tigs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Warum bist du nicht hier, meine Vertrauteste,", "tokens": ["Wa\u00b7rum", "bist", "du", "nicht", "hier", ",", "mei\u00b7ne", "Ver\u00b7trau\u00b7tes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dass dich g\u00fcrte mein Arm, dass ich dir s\u00fcssen Gruss", "tokens": ["Dass", "dich", "g\u00fcr\u00b7te", "mein", "Arm", ",", "dass", "ich", "dir", "s\u00fcs\u00b7sen", "Gruss"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PPER", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Lispl' und feurig dich dr\u00fccke", "tokens": ["Lispl'", "und", "feu\u00b7rig", "dich", "dr\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "An mein schlagendes Bruderherz.", "tokens": ["An", "mein", "schla\u00b7gen\u00b7des", "Bru\u00b7der\u00b7herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}