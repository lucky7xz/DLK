{"textgrid.poem.35686": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "10. Die Schlummernde", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eingewiegt von Nachtigallent\u00f6nen", "tokens": ["Ein\u00b7ge\u00b7wiegt", "von", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schlummert sie, die K\u00f6nigin der Sch\u00f6nen!", "tokens": ["Schlum\u00b7mert", "sie", ",", "die", "K\u00f6\u00b7ni\u00b7gin", "der", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Frischer bl\u00fcht der Thron der K\u00f6nigin;", "tokens": ["Fri\u00b7scher", "bl\u00fcht", "der", "Thron", "der", "K\u00f6\u00b7ni\u00b7gin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Weste wehn ihr Opferd\u00fcfte hin.", "tokens": ["Wes\u00b7te", "wehn", "ihr", "Op\u00b7fer\u00b7d\u00fcf\u00b7te", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "L\u00e4chle sanft! Mit hohen Engelmienen", "tokens": ["L\u00e4ch\u00b7le", "sanft", "!", "Mit", "ho\u00b7hen", "En\u00b7gel\u00b7mie\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJD", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist die That des Tages dir erschienen.", "tokens": ["Ist", "die", "That", "des", "Ta\u00b7ges", "dir", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Strecke froh die sch\u00f6nen H\u00e4nd' empor!", "tokens": ["Stre\u00b7cke", "froh", "die", "sch\u00f6\u00b7nen", "H\u00e4nd'", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Denn sie h\u00e4lt dir ihre Palme vor.", "tokens": ["Denn", "sie", "h\u00e4lt", "dir", "ih\u00b7re", "Pal\u00b7me", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber, war' s ein zitterndes Verlangen?", "tokens": ["A\u00b7ber", ",", "wa\u00b7r'", "s", "ein", "zit\u00b7tern\u00b7des", "Ver\u00b7lan\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "L\u00e4chelt Lieb' auf diesen Rosenwangen?", "tokens": ["L\u00e4\u00b7chelt", "Lieb'", "auf", "die\u00b7sen", "Ro\u00b7sen\u00b7wan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und war ich's, der dir, o L\u00e4chelnde", "tokens": ["Und", "war", "ich's", ",", "der", "dir", ",", "o", "L\u00e4\u00b7cheln\u00b7de"], "token_info": ["word", "word", "word", "punct", "word"