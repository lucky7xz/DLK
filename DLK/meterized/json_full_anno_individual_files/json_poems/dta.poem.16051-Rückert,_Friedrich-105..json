{"dta.poem.16051": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "105.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dich tr\u00e4gt Erinnerung zu deiner Kindheit Schwelle,", "tokens": ["Dich", "tr\u00e4gt", "E\u00b7rin\u00b7ne\u00b7rung", "zu", "dei\u00b7ner", "Kind\u00b7heit", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den vollen lauten Strom zur\u00fcck zur stillen Quelle.", "tokens": ["Den", "vol\u00b7len", "lau\u00b7ten", "Strom", "zu\u00b7r\u00fcck", "zur", "stil\u00b7len", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dort aber angelangt, begehrst du weiter nur", "tokens": ["Dort", "a\u00b7ber", "an\u00b7ge\u00b7langt", ",", "be\u00b7gehrst", "du", "wei\u00b7ter", "nur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "$,", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu dringen, und verlierst im Dunkel bald die Spur.", "tokens": ["Zu", "drin\u00b7gen", ",", "und", "ver\u00b7lierst", "im", "Dun\u00b7kel", "bald", "die", "Spur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und nur die Sternenschrift im Dunkeln kanst du lesen:", "tokens": ["Und", "nur", "die", "Ster\u00b7nen\u00b7schrift", "im", "Dun\u00b7keln", "kanst", "du", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du warest eh du warst, und bleibst wann du gewesen.", "tokens": ["Du", "wa\u00b7rest", "eh", "du", "warst", ",", "und", "bleibst", "wann", "du", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "VAFIN", "$,", "KON", "VVFIN", "PWAV", "PPER", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Alswie aus einem Traum erwachtest du, geboren,", "tokens": ["Als\u00b7wie", "aus", "ei\u00b7nem", "Traum", "er\u00b7wach\u00b7test", "du", ",", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und fandest eine Welt, wie eine du verloren.", "tokens": ["Und", "fan\u00b7dest", "ei\u00b7ne", "Welt", ",", "wie", "ei\u00b7ne", "du", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du sahest sie vor dir sich wechselnd umgestalten,", "tokens": ["Du", "sa\u00b7hest", "sie", "vor", "dir", "sich", "wech\u00b7selnd", "um\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lerntest deine Kraft im Kampf mit ihr entfalten.", "tokens": ["Und", "lern\u00b7test", "dei\u00b7ne", "Kraft", "im", "Kampf", "mit", "ihr", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Sovieles kam und gieng; la\u00df alles gehn und schwinden!", "tokens": ["So\u00b7vie\u00b7les", "kam", "und", "gieng", ";", "la\u00df", "al\u00b7les", "gehn", "und", "schwin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "$.", "VVFIN", "PIS", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wirst dich anders stets, und stets denselben finden.", "tokens": ["Du", "wirst", "dich", "an\u00b7ders", "stets", ",", "und", "stets", "den\u00b7sel\u00b7ben", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "$,", "KON", "ADV", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}