{"dta.poem.5577": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auch Plato, dessen Witz der Menschheit sich entrissen,", "tokens": ["Auch", "Pla\u00b7to", ",", "des\u00b7sen", "Witz", "der", "Menschheit", "sich", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PRELAT", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Hat alles ins Gesetz des H\u00f6chsten Geists gesiellt.", "tokens": ["Hat", "al\u00b7les", "ins", "Ge\u00b7setz", "des", "H\u00f6chs\u00b7ten", "Geists", "ge\u00b7siellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es zeigt uns eine Welt, die geistig ist, sein Wissen,", "tokens": ["Es", "zeigt", "uns", "ei\u00b7ne", "Welt", ",", "die", "geis\u00b7tig", "ist", ",", "sein", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als den Entwurf von einer sichtbar\u2019n Welt.", "tokens": ["Als", "den", "Ent\u00b7wurf", "von", "ei\u00b7ner", "sicht\u00b7ba\u00b7r'n", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter":