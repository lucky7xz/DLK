{"textgrid.poem.38745": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was will die Angst an meiner Seele?", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was will die Angst an meiner Seele?", "tokens": ["Was", "will", "die", "Angst", "an", "mei\u00b7ner", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was fl\u00fcchten die Gedanken fort?", "tokens": ["Was", "fl\u00fcch\u00b7ten", "die", "Ge\u00b7dan\u00b7ken", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohin ich fliehe und mich qu\u00e4le,", "tokens": ["Wo\u00b7hin", "ich", "flie\u00b7he", "und", "mich", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entdeck' ich keinen sichern Ort:", "tokens": ["Ent\u00b7deck'", "ich", "kei\u00b7nen", "si\u00b7chern", "Ort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Fu\u00df gehemmt, mein Athem schwer,", "tokens": ["Mein", "Fu\u00df", "ge\u00b7hemmt", ",", "mein", "A\u00b7them", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Brust so voll, das Herz so leer.", "tokens": ["Die", "Brust", "so", "voll", ",", "das", "Herz", "so", "leer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich will mich tiefer, tiefer gr\u00fcnden,", "tokens": ["Ich", "will", "mich", "tie\u00b7fer", ",", "tie\u00b7fer", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unsicher wird die Sicherheit,", "tokens": ["Un\u00b7si\u00b7cher", "wird", "die", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die Kr\u00e4ft' erblinden und entz\u00fcnden", "tokens": ["Die", "Kr\u00e4ft'", "er\u00b7blin\u00b7den", "und", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich ringend nach der Ewigkeit,", "tokens": ["Sich", "rin\u00b7gend", "nach", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Seele Wurzel streckt sich weit,", "tokens": ["Der", "See\u00b7le", "Wur\u00b7zel", "streckt", "sich", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will greifen aus der Zeitlichkeit.", "tokens": ["Will", "grei\u00b7fen", "aus", "der", "Zeit\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da kommen Strahlen an, die bunten,", "tokens": ["Da", "kom\u00b7men", "Strah\u00b7len", "an", ",", "die", "bun\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus alten Reichen ohne Licht,", "tokens": ["Aus", "al\u00b7ten", "Rei\u00b7chen", "oh\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es murren dumpf Gew\u00e4sser unten.", "tokens": ["Es", "mur\u00b7ren", "dumpf", "Ge\u00b7w\u00e4s\u00b7ser", "un\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entgegen streckt sich ein Gesicht,", "tokens": ["Ent\u00b7ge\u00b7gen", "streckt", "sich", "ein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Wie bang, wie schwer, es winkt und lockt,", "tokens": ["Wie", "bang", ",", "wie", "schwer", ",", "es", "winkt", "und", "lockt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Herze bebt, der Athem stockt.", "tokens": ["Das", "Her\u00b7ze", "bebt", ",", "der", "A\u00b7them", "stockt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbgieb dich gefangen, sey gefangen,", "tokens": ["\u00bb", "gieb", "dich", "ge\u00b7fan\u00b7gen", ",", "sey", "ge\u00b7fan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "PTKVZ", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich thue auf mein stilles Reich.", "tokens": ["Ich", "thue", "auf", "mein", "stil\u00b7les", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich kenne dich, dein starr Verlangen,", "tokens": ["Ich", "ken\u00b7ne", "dich", ",", "dein", "starr", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein steinern Herz biet' ich dir gleich,", "tokens": ["Mein", "stei\u00b7nern", "Herz", "biet'", "ich", "dir", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Manch Edelstein, manch g\u00fclden St\u00fcck", "tokens": ["Manch", "E\u00b7del\u00b7stein", ",", "manch", "g\u00fcl\u00b7den", "St\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Giebt dir den kalten Liebesblick.", "tokens": ["Giebt", "dir", "den", "kal\u00b7ten", "Lie\u00b7bes\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Von hier die bunten Pflanzen stammen,", "tokens": ["Von", "hier", "die", "bun\u00b7ten", "Pflan\u00b7zen", "stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von hier nimmt Baum und Gras die Kost;", "tokens": ["Von", "hier", "nimmt", "Baum", "und", "Gras", "die", "Kost", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "NE", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier schlummern sie die ewgen Flammen,", "tokens": ["Hier", "schlum\u00b7mern", "sie", "die", "ew\u00b7gen", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die dir erzeugen s\u00fc\u00dfen Most.", "tokens": ["Die", "dir", "er\u00b7zeu\u00b7gen", "s\u00fc\u00b7\u00dfen", "Most", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Berge wie das w\u00fcste Meer,", "tokens": ["Die", "Ber\u00b7ge", "wie", "das", "w\u00fcs\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie liegen in mir gro\u00df und schwer.", "tokens": ["Sie", "lie\u00b7gen", "in", "mir", "gro\u00df", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Steig nieder hier mit deinen Sinnen,", "tokens": ["Steig", "nie\u00b7der", "hier", "mit", "dei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein steinern Herze steigt in dich;", "tokens": ["Mein", "stei\u00b7nern", "Her\u00b7ze", "steigt", "in", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So magst du von mir abgewinnen,", "tokens": ["So", "magst", "du", "von", "mir", "ab\u00b7ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was mir zur Last und f\u00fcrchterlich.", "tokens": ["Was", "mir", "zur", "Last", "und", "f\u00fcrch\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O la\u00df es werden deine Lust,", "tokens": ["O", "la\u00df", "es", "wer\u00b7den", "dei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was mir beschwert die volle Brust.\u00ab", "tokens": ["Was", "mir", "be\u00b7schwert", "die", "vol\u00b7le", "Brust", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ha! folg' ich ihm? bleib' ich zur\u00fccke?", "tokens": ["Ha", "!", "fol\u00b7g'", "ich", "ihm", "?", "bleib'", "ich", "zu\u00b7r\u00fc\u00b7cke", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "PPER", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mich treibt die Angst zur\u00fcck und vor.", "tokens": ["Mich", "treibt", "die", "Angst", "zu\u00b7r\u00fcck", "und", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stimme ruft mir all mein Gl\u00fccke,", "tokens": ["Die", "Stim\u00b7me", "ruft", "mir", "all", "mein", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die fernsten W\u00fcnsche in mein Ohr;", "tokens": ["Die", "ferns\u00b7ten", "W\u00fcn\u00b7sche", "in", "mein", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entrissen von den s\u00fc\u00dfen T\u00f6nen", "tokens": ["Ent\u00b7ris\u00b7sen", "von", "den", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schau' ich krystallene Sirenen.", "tokens": ["Schau'", "ich", "krys\u00b7tal\u00b7le\u00b7ne", "Si\u00b7re\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Was will die Angst an meiner Seele?", "tokens": ["Was", "will", "die", "Angst", "an", "mei\u00b7ner", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was fl\u00fcchten die Gedanken fort?", "tokens": ["Was", "fl\u00fcch\u00b7ten", "die", "Ge\u00b7dan\u00b7ken", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohin ich fliehe und mich qu\u00e4le,", "tokens": ["Wo\u00b7hin", "ich", "flie\u00b7he", "und", "mich", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entdeck' ich keinen sichern Ort:", "tokens": ["Ent\u00b7deck'", "ich", "kei\u00b7nen", "si\u00b7chern", "Ort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Fu\u00df gehemmt, mein Athem schwer,", "tokens": ["Mein", "Fu\u00df", "ge\u00b7hemmt", ",", "mein", "A\u00b7them", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PPOS