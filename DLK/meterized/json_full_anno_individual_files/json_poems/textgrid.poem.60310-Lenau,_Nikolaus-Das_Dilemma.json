{"textgrid.poem.60310": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Das Dilemma", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er streckt dir sein Dilemma stracks entgegen;", "tokens": ["Er", "streckt", "dir", "sein", "Di\u00b7lem\u00b7ma", "stracks", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ists eine Gabel, logisch mich zu spie\u00dfen?", "tokens": ["Ists", "ei\u00b7ne", "Ga\u00b7bel", ",", "lo\u00b7gisch", "mich", "zu", "spie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADJD", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sinds Arme zwei, die Wahrheit einzuschlie\u00dfen? \u2013", "tokens": ["Sinds", "Ar\u00b7me", "zwei", ",", "die", "Wahr\u00b7heit", "ein\u00b7zu\u00b7schlie\u00b7\u00dfen", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "CARD", "$,", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So zweifelst du, versch\u00fcchtert und verlegen.", "tokens": ["So", "zwei\u00b7felst", "du", ",", "ver\u00b7sch\u00fcch\u00b7tert", "und", "ver\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mich aber mahnt der Zweizack dieses Weisen", "tokens": ["Mich", "a\u00b7ber", "mahnt", "der", "Zwei\u00b7zack", "die\u00b7ses", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An eine Fahrt auf mondbestrahlten Bahnen;", "tokens": ["An", "ei\u00b7ne", "Fahrt", "auf", "mond\u00b7bes\u00b7trahl\u00b7ten", "Bah\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Fuhrwerk wars, wie bei den Altgermanen", "tokens": ["Ein", "Fuhr\u00b7werk", "wars", ",", "wie", "bei", "den", "Alt\u00b7ger\u00b7ma\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein schlichter K\u00f6nig pflegt' umherzureisen.", "tokens": ["Ein", "schlich\u00b7ter", "K\u00f6\u00b7nig", "pflegt'", "um\u00b7her\u00b7zu\u00b7rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sacht ging es fort auf heugewohntem Wagen,", "tokens": ["Sacht", "ging", "es", "fort", "auf", "heu\u00b7ge\u00b7wohn\u00b7tem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Bauer lie\u00df die Ochsen langsam schreiten;", "tokens": ["Der", "Bau\u00b7er", "lie\u00df", "die", "Och\u00b7sen", "lang\u00b7sam", "schrei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Nacht ist sch\u00f6n, und durch die Seele gleiten", "tokens": ["Die", "Nacht", "ist", "sch\u00f6n", ",", "und", "durch", "die", "See\u00b7le", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Bilder mit idyllischem Behagen.", "tokens": ["Die", "Bil\u00b7der", "mit", "i\u00b7dyl\u00b7li\u00b7schem", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ha! zwischen des Gespannes H\u00f6rnern leuchtet", "tokens": ["Ha", "!", "zwi\u00b7schen", "des", "Ge\u00b7span\u00b7nes", "H\u00f6r\u00b7nern", "leuch\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Das Horn des Mondes, scheinbar eingefangen,", "tokens": ["Das", "Horn", "des", "Mon\u00b7des", ",", "schein\u00b7bar", "ein\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie zwischen des Dilemmas beiden Stangen", "tokens": ["Wie", "zwi\u00b7schen", "des", "Di\u00b7lem\u00b7mas", "bei\u00b7den", "Stan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PIAT", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Himmelslicht dir eingeschlos