{"textgrid.poem.40199": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Blick in das All", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben uns gesucht und nicht gefunden:", "tokens": ["Wir", "ha\u00b7ben", "uns", "ge\u00b7sucht", "und", "nicht", "ge\u00b7fun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "KON", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wir sahen G\u00f6tter, weil wir ", "tokens": ["wir", "sa\u00b7hen", "G\u00f6t\u00b7ter", ",", "weil", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wir hatten uns durch eigne Kraft gebunden,", "tokens": ["Wir", "hat\u00b7ten", "uns", "durch", "eig\u00b7ne", "Kraft", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "da\u00df wir die eigne Kraft nicht f\u00fchlten ", "tokens": ["da\u00df", "wir", "die", "eig\u00b7ne", "Kraft", "nicht", "f\u00fchl\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Geist war leiblos aus dem All geschwunden:", "tokens": ["Der", "Geist", "war", "lei\u00b7blos", "aus", "dem", "All", "ge\u00b7schwun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "er sollte g\u00f6ttlich ", "tokens": ["er", "soll\u00b7te", "g\u00f6tt\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Wir wollten nur am Wesen uns begeistern,", "tokens": ["Wir", "woll\u00b7ten", "nur", "am", "We\u00b7sen", "uns", "be\u00b7geis\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und konnten dennoch nicht die Form bemeistern.", "tokens": ["und", "konn\u00b7ten", "den\u00b7noch", "nicht", "die", "Form", "be\u00b7meis\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und machtlos f\u00fchlte sich auf seiner H\u00f6he", "tokens": ["Und", "macht\u00b7los", "f\u00fchl\u00b7te", "sich", "auf", "sei\u00b7ner", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Menschengeist, der G\u00f6ttergeist sich gab,", "tokens": ["der", "Men\u00b7schen\u00b7geist", ",", "der", "G\u00f6t\u00b7ter\u00b7geist", "sich", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df er aus ", "tokens": ["da\u00df", "er", "aus"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "sein Hoheits", "tokens": ["sein", "Ho\u00b7heits"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Es zwang ihn nieder ein gewaltig Wehe,", "tokens": ["Es", "zwang", "ihn", "nie\u00b7der", "ein", "ge\u00b7wal\u00b7tig", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "verzweifelnd wies zum Staub er sich hinab;", "tokens": ["ver\u00b7zwei\u00b7felnd", "wies", "zum", "Staub", "er", "sich", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPRART", "NN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "zum Wesen ward der Dinge ", "tokens": ["zum", "We\u00b7sen", "ward", "der", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "und Wahrheit schien ihm, da\u00df er ewig ", "tokens": ["und", "Wahr\u00b7heit", "schien", "ihm", ",", "da\u00df", "er", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch aus der Selbstbez\u00e4hmung umgeboren", "tokens": ["Doch", "aus", "der", "Selbst\u00b7be\u00b7z\u00e4h\u00b7mung", "um\u00b7ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zu neuem ", "tokens": ["zu", "neu\u00b7em"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "und was er an erhabnem ", "tokens": ["und", "was", "er", "an", "er\u00b7hab\u00b7nem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "gewann als ", "tokens": ["ge\u00b7wann", "als"], "token_info": ["word", "word"], "pos": ["VVFIN", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "denn Demut hob ihn aus der Schwachheit Haft:", "tokens": ["denn", "De\u00b7mut", "hob", "ihn", "aus", "der", "Schwach\u00b7heit", "Haft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "der Geist im Leib vom Geist ins All verbunden,", "tokens": ["der", "Geist", "im", "Leib", "vom", "Geist", "ins", "All", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "APPRART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "so hat im All der Geist sich erst ", "tokens": ["so", "hat", "im", "All", "der", "Geist", "sich", "erst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "PRF", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er sah, wie in der endlos gro\u00dfen Kette", "tokens": ["Er", "sah", ",", "wie", "in", "der", "end\u00b7los", "gro\u00b7\u00dfen", "Ket\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Formen endlos gro\u00df ein Sehnen wohnt,", "tokens": ["der", "For\u00b7men", "end\u00b7los", "gro\u00df", "ein", "Seh\u00b7nen", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sich selbst zu heben in der Wesenwette,", "tokens": ["sich", "selbst", "zu", "he\u00b7ben", "in", "der", "We\u00b7sen\u00b7wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in der die Form der ", "tokens": ["in", "der", "die", "Form", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "ART"], "meter": "++-+-", "measure": "iambic.di"}, "line.5": {"text": "Und aus ", "tokens": ["Und", "aus"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "ein Streben, das sich endlich nicht ", "tokens": ["ein", "Stre\u00b7ben", ",", "das", "sich", "end\u00b7lich", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "der Menschheit ", "tokens": ["der", "Menschheit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "es b\u00fcrgt auch f\u00fcr des Ringens Vollerf\u00fcllung!", "tokens": ["es", "b\u00fcrgt", "auch", "f\u00fcr", "des", "Rin\u00b7gens", "Vol\u00b7ler\u00b7f\u00fcl\u00b7lung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "verschlossen ewig sei das R\u00e4tselbuch", "tokens": ["ver\u00b7schlos\u00b7sen", "e\u00b7wig", "sei", "das", "R\u00e4t\u00b7sel\u00b7buch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der Allkraft, deren Pulse in uns walten!", "tokens": ["der", "All\u00b7kraft", ",", "de\u00b7ren", "Pul\u00b7se", "in", "uns", "wal\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "ein Wahn, da\u00df uns die L\u00f6sung nur ein Fluch,", "tokens": ["ein", "Wahn", ",", "da\u00df", "uns", "die", "L\u00f6\u00b7sung", "nur", "ein", "Fluch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "da\u00df unsrer Geistesm\u00e4chte ", "tokens": ["da\u00df", "uns\u00b7rer", "Geis\u00b7tes\u00b7m\u00e4ch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "der Formwelt drohe den Zusammenbruch!", "tokens": ["der", "Form\u00b7welt", "dro\u00b7he", "den", "Zu\u00b7sam\u00b7men\u00b7bruch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"