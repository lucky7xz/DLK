{"textgrid.poem.52772": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich der Menschheit Jammer fa\u00dft,", "tokens": ["Wenn", "dich", "der", "Menschheit", "Jam\u00b7mer", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "So lasse dir zum Troste sagen,", "tokens": ["So", "las\u00b7se", "dir", "zum", "Tros\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Seelenlebensschiffsbalast", "tokens": ["Der", "See\u00b7len\u00b7le\u00b7bens\u00b7schiffs\u00b7ba\u00b7last"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df dich nach dem Polarstern tragen \u2013", "tokens": ["Mu\u00df", "dich", "nach", "dem", "Po\u00b7lars\u00b7tern", "tra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Felsenwahrheit wird sobald", "tokens": ["Die", "Fel\u00b7sen\u00b7wahr\u00b7heit", "wird", "so\u00b7bald"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht fallen vor der Nacht des Schn\u00f6den,", "tokens": ["Nicht", "fal\u00b7len", "vor", "der", "Nacht", "des", "Schn\u00f6\u00b7den", ","], "token_info": ["word", "wor