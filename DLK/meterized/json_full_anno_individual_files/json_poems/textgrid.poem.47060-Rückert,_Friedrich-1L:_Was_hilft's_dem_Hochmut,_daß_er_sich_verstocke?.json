{"textgrid.poem.47060": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was hilft's dem Hochmut, da\u00df er sich verstocke?", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hilft's dem Hochmut, da\u00df er sich verstocke?", "tokens": ["Was", "hilft's", "dem", "Hoch\u00b7mut", ",", "da\u00df", "er", "sich", "ver\u00b7sto\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Macht der Liebe wird ihn doch erfassen;", "tokens": ["Die", "Macht", "der", "Lie\u00b7be", "wird", "ihn", "doch", "er\u00b7fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ist kein andres Mittel ihr gelassen,", "tokens": ["Und", "ist", "kein", "and\u00b7res", "Mit\u00b7tel", "ihr", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So w\u00e4hlt sie sich den Hammer einer Glocke.", "tokens": ["So", "w\u00e4hlt", "sie", "sich", "den", "Ham\u00b7mer", "ei\u00b7ner", "Glo\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Glocke drau\u00dfen in dem h\u00f6chsten Stocke", "tokens": ["Die", "Glo\u00b7cke", "drau\u00b7\u00dfen", "in", "dem", "h\u00f6chs\u00b7ten", "Sto\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Turms, an dem vorbei sonst meine Stra\u00dfen", "tokens": ["Des", "Turms", ",", "an", "dem", "vor\u00b7bei", "sonst", "mei\u00b7ne", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zum Hause gingen, das ich jetzt will hassen,", "tokens": ["Zum", "Hau\u00b7se", "gin\u00b7gen", ",", "das", "ich", "jetzt", "will", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PRELS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dringt ein zu mir, da\u00df sie mich wieder locke.", "tokens": ["Dringt", "ein", "zu", "mir", ",", "da\u00df", "sie", "mich", "wie\u00b7der", "lo\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "PPER", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O die du nur die christliche Gemeinde", "tokens": ["O", "die", "du", "nur", "die", "christ\u00b7li\u00b7che", "Ge\u00b7mein\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PRELS", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Berufen solltest zu des Tempels Stufen,", "tokens": ["Be\u00b7ru\u00b7fen", "soll\u00b7test", "zu", "des", "Tem\u00b7pels", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat statt der Andacht Liebe dich gedungen?", "tokens": ["Hat", "statt", "der", "An\u00b7dacht", "Lie\u00b7be", "dich", "ge\u00b7dun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wer sch\u00fctzt mich gegen so verbundne Feinde,", "tokens": ["Wer", "sch\u00fctzt", "mich", "ge\u00b7gen", "so", "ver\u00b7bund\u00b7ne", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ferne Liebe, wo ihr selbst zum Rufen", "tokens": ["Wenn", "fer\u00b7ne", "Lie\u00b7be", ",", "wo", "ihr", "selbst", "zum", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht reicht die Stimme, borgt Gebetes Zungen?", "tokens": ["Nicht", "reicht", "die", "Stim\u00b7me", ",", "borgt", "Ge\u00b7be\u00b7tes", "Zun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was hilft's dem Hochmut, da\u00df er sich verstocke?", "tokens": ["Was", "hilft's", "dem", "Hoch\u00b7mut", ",", "da\u00df", "er", "sich", "ver\u00b7sto\u00b7cke", "?"], "token_info": ["word",