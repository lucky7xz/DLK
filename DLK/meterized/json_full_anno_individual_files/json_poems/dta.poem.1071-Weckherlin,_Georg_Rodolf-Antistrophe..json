{"dta.poem.1071": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vnd wa la\u00df ich die Dapferkeit/", "tokens": ["Vnd", "wa", "la\u00df", "ich", "die", "Dap\u00b7fer\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "VVIMP", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dadurch du Mars nit nur vo\u0303 stam\u0303e\u0303/", "tokens": ["Da\u00b7durch", "du", "Mars", "nit", "nur", "v\u00f5", "stam\u0303\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "PTKNEG", "ADV", "XY", "XY", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So\u0303dern an st\u00e4rck/ muht/ munderkeit", "tokens": ["S\u00f5\u00b7dern", "an", "st\u00e4rck", "/", "muht", "/", "mun\u00b7der\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "APPR", "NN", "$(", "VVFIN", "$(", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ein wahrer Mars an that vnd namen?", "tokens": ["Ein", "wah\u00b7rer", "Mars", "an", "that", "vnd", "na\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie oft hat dein schr\u00f6ckliches schwert", "tokens": ["Wie", "oft", "hat", "dein", "schr\u00f6ck\u00b7li\u00b7ches", "schwert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPOSAT", "ADJA", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Mit Martialischen buchstaben", "tokens": ["Mit", "Mar\u00b7ti\u00b7a\u00b7li\u00b7schen", "buch\u00b7sta\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Deinen namen so klar vnd wert", "tokens": ["Dei\u00b7nen", "na\u00b7men", "so", "klar", "vnd", "wert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "KON", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Auf Jberische h\u00e4ut gegraben?", "tokens": ["Auf", "Jbe\u00b7ri\u00b7sche", "h\u00e4ut", "ge\u00b7gra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Also das die Cimbrische Cron", "tokens": ["Al\u00b7so", "das", "die", "Cim\u00b7bri\u00b7sche", "Cron"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Wolt sich (zu deiner Tugent lohn/", "tokens": ["Wolt", "sich", "(", "zu", "dei\u00b7ner", "Tu\u00b7gent", "lohn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "$(", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Vnd jhrem prei\u00df) vmbsunst bem\u00fchen/", "tokens": ["Vnd", "jhrem", "prei\u00df", ")", "vmbsunst", "be\u00b7m\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Dich deinem K\u00f6nig zu entziehen.", "tokens": ["Dich", "dei\u00b7nem", "K\u00f6\u00b7nig", "zu", "ent\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}