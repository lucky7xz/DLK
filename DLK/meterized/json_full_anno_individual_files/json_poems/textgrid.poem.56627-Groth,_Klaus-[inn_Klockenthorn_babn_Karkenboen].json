{"textgrid.poem.56627": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "[inn Klockenthorn babn Karkenboen]", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Inn Klockenthorn babn Karkenb\u0153n", "tokens": ["Inn", "Klo\u00b7cken\u00b7thorn", "babn", "Kar\u00b7ken\u00b7b\u0153n"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dar geit en Rad mit isern T\u00e4hn,", "tokens": ["Dar", "geit", "en", "Rad", "mit", "i\u00b7sern", "T\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un wenn de ole, de dowe, de K\u00fcster ni weer,", "tokens": ["Un", "wenn", "de", "o\u00b7le", ",", "de", "do\u00b7we", ",", "de", "K\u00fcs\u00b7ter", "ni", "weer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "NE", "$,", "NE", "NE", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "So keem der keen starbens Minsch, vunne ganze Eer.", "tokens": ["So", "keem", "der", "ke\u00b7en", "star\u00b7bens", "Minsch", ",", "vun\u00b7ne", "gan\u00b7ze", "E\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ART", "VVFIN", "ADJA", "NN", "$,", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "De M\u00f6llerbursch sitt ganz alleen,", "tokens": ["De", "M\u00f6l\u00b7ler\u00b7bursch", "sitt", "ganz", "al\u00b7le\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "He s\u00fcht in dat Rad, dat drift de Steen", "tokens": ["He", "s\u00fcht", "in", "dat", "Rad", ",", "dat", "drift", "de", "Steen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "NE", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Un wenn de ole, de dowe, de K\u00fcster sin Dochder ni weer,", "tokens": ["Un", "wenn", "de", "o\u00b7le", ",", "de", "do\u00b7we", ",", "de", "K\u00fcs\u00b7ter", "sin", "Doch\u00b7der", "ni", "weer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "NE", "$,", "NE", "NE", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Wat kunn der so lusti sik dreihn oppe ganze Eer?", "tokens": ["Wat", "kunn", "der", "so", "lus\u00b7ti", "sik", "dreihn", "op\u00b7pe", "gan\u00b7ze", "E\u00b7er", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Inn Klockenthorn babn Karkenb\u0153n", "tokens": ["Inn", "Klo\u00b7cken\u00b7thorn", "babn", "Kar\u00b7ken\u00b7b\u0153n"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dar geit en Rad mit isern T\u00e4hn,", "tokens": ["Dar", "geit", "en", "Rad", "mit", "i