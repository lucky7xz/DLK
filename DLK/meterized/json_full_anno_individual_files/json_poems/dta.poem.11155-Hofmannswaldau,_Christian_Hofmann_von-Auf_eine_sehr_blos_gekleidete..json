{"dta.poem.11155": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine sehr blos gekleidete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die wahrheit pflegt man sonst gantz nackt und blos zu kleiden;", "tokens": ["Die", "wahr\u00b7heit", "pflegt", "man", "sonst", "gantz", "nackt", "und", "blos", "zu", "klei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "ADV", "ADJD", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Euch beyde mu\u00df man doch darinnen unterscheiden,", "tokens": ["Euch", "bey\u00b7de", "mu\u00df", "man", "doch", "da\u00b7rin\u00b7nen", "un\u00b7ter\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VMFIN", "PIS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df jener bl\u00f6se zeigt, was sie rechtschaffen ist;", "tokens": ["Da\u00df", "je\u00b7ner", "bl\u00f6\u00b7se", "zeigt", ",", "was", "sie", "recht\u00b7schaf\u00b7fen", "ist", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du aber nackt verr\u00e4thst das, was du nicht mehr bist.", "tokens": ["Du", "a\u00b7ber", "nackt", "ver\u00b7r\u00e4thst", "das", ",", "was", "du", "nicht", "mehr", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVFIN", "PDS", "$,", "PWS", "PPER", "PTKNEG", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}