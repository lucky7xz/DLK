{"textgrid.poem.31116": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "230.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weiland war f\u00fcrs Vaterland Gut und Blut gelassen;", "tokens": ["Wei\u00b7land", "war", "f\u00fcrs", "Va\u00b7ter\u00b7land", "Gut", "und", "Blut", "ge\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "ADJD", "KON", "NN", "VVPP", "$."], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Gut, nicht Blut wird ietzt verthan, aber nur zum hassen.", "tokens": ["Gut", ",", "nicht", "Blut", "wird", "ietzt", "ver\u00b7than", ",", "a\u00b7ber", "nur", "zum", "has\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PTKNEG", "NN", "VAFIN", "ADV", "VVPP", "$,", "ADV", "ADV", "APPRART", "VVINF", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Man verreiset grosses Geld; was man bringt zur\u00fccke,", "tokens": ["Man", "ver\u00b7rei\u00b7set", "gros\u00b7ses", "Geld", ";", "was", "man", "bringt", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJA", "NN", "$.", "PWS", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Braucht man, das man schimpffen kan redlich deutsch geschicke.", "tokens": ["Braucht", "man", ",", "das", "man", "schimpf\u00b7fen", "kan", "red\u00b7lich", "deutsch", "ge\u00b7schi\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "po