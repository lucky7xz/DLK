{"dta.poem.931": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N ACH   D ER   L ESE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ja heil und dank dir die den segen brachte!               ", "tokens": ["Ja", "heil", "und", "dank", "dir", "die", "den", "se\u00b7gen", "brach\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJD", "KON", "APPR", "PPER", "ART", "ART", "PPOSS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du schl&#228;fertest das immer laute pochen               ", "tokens": ["Du", "schl", "&#228;", "fer\u00b7test", "das", "im\u00b7mer", "lau\u00b7te", "po\u00b7chen"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ART", "ADV", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit der erwartung deiner Teure sachte               ", "tokens": ["Mit", "der", "er\u00b7war\u00b7tung", "dei\u00b7ner", "Teu\u00b7re", "sach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In diesen glanzerf&#252;llten sterbewochen             ", "tokens": ["In", "die\u00b7sen", "glan\u00b7zerf", "&#252;", "ll\u00b7ten", "ster\u00b7be\u00b7wo\u00b7chen"], "token_info": ["word", "word", "word", "XML_entity", "word", "word"], "pos": ["APPR", "PDAT", "VVFIN", "$(", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du kamest und wir halten uns umschlungen               ", "tokens": ["Du", "ka\u00b7mest", "und", "wir", "hal\u00b7ten", "uns", "um\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich werde sanfte worte f&#252;r dich lernen               ", "tokens": ["Ich", "wer\u00b7de", "sanf\u00b7te", "wor\u00b7te", "f", "&#252;", "r", "dich", "ler\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "NE", "$(", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ganz als glichest du der Einen Fernen               ", "tokens": ["Und", "ganz", "als", "gli\u00b7chest", "du", "der", "Ei\u00b7nen", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "VVFIN", "PPER", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich loben auf den sonnen-wanderungen.             ", "tokens": ["Dich", "lo\u00b7ben", "auf", "den", "son\u00b7nen\u00b7wan\u00b7de\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}