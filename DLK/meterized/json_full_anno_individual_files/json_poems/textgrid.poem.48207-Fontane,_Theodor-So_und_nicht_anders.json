{"textgrid.poem.48207": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "So und nicht anders", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen k\u00fcmmerten mich nicht viel,", "tokens": ["Die", "Men\u00b7schen", "k\u00fcm\u00b7mer\u00b7ten", "mich", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich mied den Markt, ich mied den Schwarm,", "tokens": ["Ich", "mied", "den", "Markt", ",", "ich", "mied", "den", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Andre sind reich, ich bin arm.", "tokens": ["And\u00b7re", "sind", "reich", ",", "ich", "bin", "arm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Andre regierten (regieren noch),", "tokens": ["And\u00b7re", "re\u00b7gier\u00b7ten", "(", "re\u00b7gie\u00b7ren", "noch", ")", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "$(", "VVFIN", "ADV", "$(", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ich stand unten und ging durchs Joch.", "tokens": ["Ich", "stand", "un\u00b7ten", "und", "ging", "durchs", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Entsagen und l\u00e4cheln bei Dem\u00fctigungen,", "tokens": ["Ent\u00b7sa\u00b7gen", "und", "l\u00e4\u00b7cheln", "bei", "De\u00b7m\u00fc\u00b7ti\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das ist die Kunst, die ", "tokens": ["Das", "ist", "die", "Kunst", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und doch, w\u00e4r's in die Wahl mir gegeben,", "tokens": ["Und", "doch", ",", "w\u00e4r's", "in", "die", "Wahl", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VAFIN", "APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ich f\u00fchrte noch einmal dasselbe Leben.", "tokens": ["Ich", "f\u00fchr\u00b7te", "noch", "ein\u00b7mal", "das\u00b7sel\u00b7be", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PDAT", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Und sollt' ich noch einmal die Tage beginnen,", "tokens": ["Und", "sollt'", "ich", "noch", "ein\u00b7mal", "die", "Ta\u00b7ge", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich w\u00fcrde denselben Faden spinnen.", "tokens": ["Ich