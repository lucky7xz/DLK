{"textgrid.poem.66047": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Auf einem andern Stern", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Purpurdecke deines Zeltes hebt", "tokens": ["Die", "Pur\u00b7pur\u00b7de\u00b7cke", "dei\u00b7nes", "Zel\u00b7tes", "hebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ganz langsam eine schmale wei\u00dfe Hand,", "tokens": ["Ganz", "lang\u00b7sam", "ei\u00b7ne", "schma\u00b7le", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und meine K\u00f6nigin, im Rosenschmuck", "tokens": ["Und", "mei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ",", "im", "Ro\u00b7sen\u00b7schmuck"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der schlaferquickten Jugend, gr\u00fc\u00dft den Tag.", "tokens": ["Der", "schla\u00b7fer\u00b7quick\u00b7ten", "Ju\u00b7gend", ",", "gr\u00fc\u00dft", "den", "Tag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Seit gestern weilen wir auf diesem Stern,", "tokens": ["Seit", "ge\u00b7stern", "wei\u00b7len", "wir", "auf", "die\u00b7sem", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Millionenmal millionen Meilen weit", "tokens": ["Mil\u00b7li\u00b7o\u00b7nen\u00b7mal", "mil\u00b7li\u00b7o\u00b7nen", "Mei\u00b7len", "weit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJD"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Entr\u00fcckt der Erde. Als ich von dir ging,", "tokens": ["Ent\u00b7r\u00fcckt", "der", "Er\u00b7de", ".", "Als", "ich", "von", "dir", "ging", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Stand \u00fcber mir der blasse Erdenmond,", "tokens": ["Stand", "\u00fc\u00b7ber", "mir", "der", "blas\u00b7se", "Er\u00b7den\u00b7mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und eines W\u00e4chters harte Stimme wies", "tokens": ["Und", "ei\u00b7nes", "W\u00e4ch\u00b7ters", "har\u00b7te", "Stim\u00b7me", "wies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von deines stillen Gartens Gitter mich,", "tokens": ["Von", "dei\u00b7nes", "stil\u00b7len", "Gar\u00b7tens", "Git\u00b7ter", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vermutend den gesuchten \u00c4pfeldieb.", "tokens": ["Ver\u00b7mu\u00b7tend", "den", "ge\u00b7such\u00b7ten", "\u00c4p\u00b7fel\u00b7dieb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Seit gestern weilen wir auf diesem Stern,", "tokens": ["Seit", "ge\u00b7stern", "wei\u00b7len", "wir", "auf", "die\u00b7sem", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und eine Nacht, der selbst der Wettgesang", "tokens": ["Und", "ei\u00b7ne", "Nacht", ",", "der", "selbst", "der", "Wett\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von vielen hundert Nachtigallen nichts", "tokens": ["Von", "vie\u00b7len", "hun\u00b7dert", "Nach\u00b7ti\u00b7gal\u00b7len", "nichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "CARD", "NN", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vom Zauber ihres tiefen Schweigens nahm,", "tokens": ["Vom", "Zau\u00b7ber", "ih\u00b7res", "tie\u00b7fen", "Schwei\u00b7gens", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bracht uns Vergessen. Mi\u00dfverst\u00e4ndnis, Stolz", "tokens": ["Bracht", "uns", "Ver\u00b7ges\u00b7sen", ".", "Mi\u00df\u00b7ver\u00b7st\u00e4nd\u00b7nis", ",", "Stolz"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "NN", "$.", "NN", "$,", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Und jede Kluft, die Menschennarrheit schuf,", "tokens": ["Und", "je\u00b7de", "Kluft", ",", "die", "Men\u00b7schen\u00b7nar\u00b7rheit", "schuf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Blieb hinter uns, und die Erinnerung starb.", "tokens": ["Blieb", "hin\u00b7ter", "uns", ",", "und", "die", "E\u00b7rin\u00b7ne\u00b7rung", "starb", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "Die Purpurdecke deines Zeltes hebt", "tokens": ["Die", "Pur\u00b7pur\u00b7de\u00b7cke", "dei\u00b7nes", "Zel\u00b7tes", "hebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ganz langsam eine schmale wei\u00dfe Hand,", "tokens": ["Ganz", "lang\u00b7sam", "ei\u00b7ne", "schma\u00b7le", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und meine K\u00f6nigin, im Rosenschmuck", "tokens": ["Und", "mei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", ",", "im", "Ro\u00b7sen\u00b7schmuck"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der schlaferquickten Jugend, gr\u00fc\u00dft den Tag.", "tokens": ["Der", "schla\u00b7fer\u00b7quick\u00b7ten", "Ju\u00b7gend", ",", "gr\u00fc\u00dft", "den", "Tag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie bist du sch\u00f6n im vollen Morgenglanz", "tokens": ["Wie", "bist", "du", "sch\u00f6n", "im", "vol\u00b7len", "Mor\u00b7gen\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der sieben Sonnen, die, ein reicher Ring,", "tokens": ["Der", "sie\u00b7ben", "Son\u00b7nen", ",", "die", ",", "ein", "rei\u00b7cher", "Ring", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,", "PRELS", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hier unseres Gl\u00fcckes Wiegenbett umstehn.", "tokens": ["Hier", "un\u00b7se\u00b7res", "Gl\u00fc\u00b7ckes", "Wie\u00b7gen\u00b7bett", "um\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Schneewei\u00dfe Seide, lose aufgerafft", "tokens": ["Schnee\u00b7wei\u00b7\u00dfe", "Sei\u00b7de", ",", "lo\u00b7se", "auf\u00b7ge\u00b7rafft"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.9": {"text": "Von goldnen Spangen, h\u00fcllt den schlanken Leib,", "tokens": ["Von", "gold\u00b7nen", "Span\u00b7gen", ",", "h\u00fcllt", "den", "schlan\u00b7ken", "Leib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und nicht der kleinste Zierat weiter st\u00f6rt", "tokens": ["Und", "nicht", "der", "kleins\u00b7te", "Zie\u00b7rat", "wei\u00b7ter", "st\u00f6rt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der zarten Formen keuschen Linienflu\u00df.", "tokens": ["Der", "zar\u00b7ten", "For\u00b7men", "keu\u00b7schen", "Li\u00b7ni\u00b7en\u00b7flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Seit gestern weilen wir auf diesem Stern,", "tokens": ["Seit", "ge\u00b7stern", "wei\u00b7len", "wir", "auf", "die\u00b7sem", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und niemals ist ein sch\u00f6nerer Morgen wohl", "tokens": ["Und", "nie\u00b7mals", "ist", "ein", "sch\u00f6\u00b7ne\u00b7rer", "Mor\u00b7gen", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf eine sch\u00f6nere Nacht, wo auch, gefolgt.", "tokens": ["Auf", "ei\u00b7ne", "sch\u00f6\u00b7ne\u00b7re", "Nacht", ",", "wo", "auch", ",", "ge\u00b7folgt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ADV", "$,", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den sieben Sonnen wich die Siebenzahl", "tokens": ["Den", "sie\u00b7ben", "Son\u00b7nen", "wich", "die", "Sie\u00b7ben\u00b7zahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der sanften Silbermonde, die das Amt", "tokens": ["Der", "sanf\u00b7ten", "Sil\u00b7ber\u00b7mon\u00b7de", ",", "die", "das", "Amt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der W\u00e4chter vor dem Liebeszelt versahn", "tokens": ["Der", "W\u00e4ch\u00b7ter", "vor", "dem", "Lie\u00b7bes\u00b7zelt", "ver\u00b7sahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und bla\u00df und bl\u00e4sser wurden, st\u00fcndlich mehr,", "tokens": ["Und", "bla\u00df", "und", "bl\u00e4s\u00b7ser", "wur\u00b7den", ",", "st\u00fcnd\u00b7lich", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Vor Neid und Neugier. Doch das dichte Tuch", "tokens": ["Vor", "Neid", "und", "Neu\u00b7gier", ".", "Doch", "das", "dich\u00b7te", "Tuch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Des Purpurdaches wehrte jedem Blick,", "tokens": ["Des", "Pur\u00b7pur\u00b7da\u00b7ches", "wehr\u00b7te", "je\u00b7dem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Selbst jeder Laut verfing im schweren Stoff", "tokens": ["Selbst", "je\u00b7der", "Laut", "ver\u00b7fing", "im", "schwe\u00b7ren", "Stoff"], "token_i