{"dta.poem.8845": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und ob von euch ich nicht betrogen werde?", "tokens": ["Und", "ob", "von", "euch", "ich", "nicht", "be\u00b7tro\u00b7gen", "wer\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPER", "PPER", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich staune, da\u00df ich, da mein Lenz entwichen,", "tokens": ["Ich", "stau\u00b7ne", ",", "da\u00df", "ich", ",", "da", "mein", "Lenz", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Bl\u00fcthenstaub noch \u00fcberflogen werde;", "tokens": ["Vom", "Bl\u00fc\u00b7then\u00b7staub", "noch", "\u00fc\u00b7berf\u00b7lo\u00b7gen", "wer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich zweifelte, da ich gespielt den Kalten,", "tokens": ["Ich", "zwei\u00b7fel\u00b7te", ",", "da", "ich", "ge\u00b7spielt", "den", "Kal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ob ein Gem\u00fcth mir noch gewogen werde?", "tokens": ["Ob", "ein", "Ge\u00b7m\u00fcth", "mir", "noch", "ge\u00b7wo\u00b7gen", "wer\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch wei\u00df ich euch kein s\u00fc\u00df Geschw\u00e4tz zu bieten,", "tokens": ["Doch", "wei\u00df", "ich", "euch", "kein", "s\u00fc\u00df", "Ge\u00b7schw\u00e4tz", "zu", "bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PIAT", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das uns zu z\u00e4rtlichen Eklogen werde:", "tokens": ["Das", "uns", "zu", "z\u00e4rt\u00b7li\u00b7chen", "Ek\u00b7lo\u00b7gen", "wer\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zum Himmel trozt mein Lebensbaum und harret,", "tokens": ["Zum", "Him\u00b7mel", "trozt", "mein", "Le\u00b7bens\u00b7baum", "und", "har\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ob er zur Laube noch gebogen werde;", "tokens": ["Ob", "er", "zur", "Lau\u00b7be", "noch", "ge\u00b7bo\u00b7gen", "wer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wer meiner Fahrt Gef\u00e4hrte, sey gew\u00e4rtig,", "tokens": ["Wer", "mei\u00b7ner", "Fahrt", "Ge\u00b7f\u00e4hr\u00b7te", ",", "sey", "ge\u00b7w\u00e4r\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Da\u00df er ein Spiel der falschen Wogen werde!", "tokens": ["Da\u00df", "er", "ein", "Spiel", "der", "fal\u00b7schen", "Wo\u00b7gen", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}