{"dta.poem.4069": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Aber inde\u00df wir schaun und die m\u00e4chtige Freude", "tokens": ["A\u00b7ber", "in\u00b7de\u00df", "wir", "schaun", "und", "die", "m\u00e4ch\u00b7ti\u00b7ge", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "ADJD", "KON", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "durchwandeln,", "tokens": ["durch\u00b7wan\u00b7deln", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Fliehet der Weg und der Tag uns, wie den", "tokens": ["Flie\u00b7het", "der", "Weg", "und", "der", "Tag", "uns", ",", "wie", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "ART", "NN", "PPER", "$,", "PWAV", "ART"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Trunkenen, hin.", "tokens": ["Trun\u00b7ke\u00b7nen", ",", "hin", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Denn mit heiligem Laub umkr\u00e4nzt erhebet die", "tokens": ["Denn", "mit", "hei\u00b7li\u00b7gem", "Laub", "um\u00b7kr\u00e4nzt", "er\u00b7he\u00b7bet", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "VVFIN", "ART"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.6": {"text": "Stadt schon,", "tokens": ["Stadt", "schon", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Die gepriesene, dort, leuchtend ihr priesterlich", "tokens": ["Die", "ge\u00b7prie\u00b7se\u00b7ne", ",", "dort", ",", "leuch\u00b7tend", "ihr", "pries\u00b7ter\u00b7lich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADV", "$,", "VVPP", "PPER", "ADJD"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Haupt.", "tokens": ["Haupt", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Herrlich steht sie, und h\u00e4lt den Rebenstab und die", "tokens": ["Herr\u00b7lich", "steht", "sie", ",", "und", "h\u00e4lt", "den", "Re\u00b7bens\u00b7tab", "und", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ART", "NN", "KON", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Tanne", "tokens": ["Tan\u00b7ne"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Hoch in den seligen purpurnen Wolken empor.", "tokens": ["Hoch", "in", "den", "se\u00b7li\u00b7gen", "pur\u00b7pur\u00b7nen", "Wol\u00b7ken", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.12": {"text": "Sey uns hold, dem Gast und dem Sohn, o F\u00fcr-", "tokens": ["Sey", "uns", "hold", ",", "dem", "Gast", "und", "dem", "Sohn", ",", "o", "F\u00fcr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ART", "NN", "KON", "ART", "NN", "$,", "FM", "TRUNC"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "stin der Heimath,", "tokens": ["stin", "der", "Hei\u00b7math", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "Gl\u00fcckliches Stuttgart! nimm freundlich den", "tokens": ["Gl\u00fcck\u00b7li\u00b7ches", "Stutt\u00b7gart", "!", "nimm", "freund\u00b7lich", "den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "VVIMP", "ADJD", "ART"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.15": {"text": "Fremdling mir auf!", "tokens": ["Fremd\u00b7ling", "mir", "auf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Immer hast du Gesang mit Fl\u00f6ten und Saiten", "tokens": ["Im\u00b7mer", "hast", "du", "Ge\u00b7sang", "mit", "Fl\u00f6\u00b7ten", "und", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "gebilligt,", "tokens": ["ge\u00b7bil\u00b7ligt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Wie ich glaub', und des Lieds kindlich Geschw\u00e4tz,", "tokens": ["Wie", "ich", "glaub'", ",", "und", "des", "Lieds", "kind\u00b7lich", "Ge\u00b7schw\u00e4tz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KON", "ART", "NN", "ADJD", "NN", "$,"], "meter": "--+---+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "und der M\u00fchn", "tokens": ["und", "der", "M\u00fchn"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "S\u00fc\u00dfe Vergessenheit bei gegenw\u00e4rtigem Geiste,", "tokens": ["S\u00fc\u00b7\u00dfe", "Ver\u00b7ges\u00b7sen\u00b7heit", "bei", "ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7gem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.21": {"text": "Drum erfreuest du auch gerne den S\u00e4ngern das", "tokens": ["Drum", "er\u00b7freu\u00b7est", "du", "auch", "ger\u00b7ne", "den", "S\u00e4n\u00b7gern", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ART"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Herz.", "tokens": ["Herz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.23": {"text": "Aber ihr, ihr Gr\u00f6\u00dferen auch, ihr Frohen, die", "tokens": ["A\u00b7ber", "ihr", ",", "ihr", "Gr\u00f6\u00b7\u00dfe\u00b7ren", "auch", ",", "ihr", "Fro\u00b7hen", ",", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "ADV", "$,", "PPOSAT", "NN", "$,", "PRELS"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "allzeit", "tokens": ["all\u00b7zeit"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+", "measure": "iambic.single"}, "line.25": {"text": "Leben und walten, erkannt, oder gewaltiger auch,", "tokens": ["Le\u00b7ben", "und", "wal\u00b7ten", ",", "er\u00b7kannt", ",", "o\u00b7der", "ge\u00b7wal\u00b7ti\u00b7ger", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,", "VVPP", "$,", "KON", "ADJA", "ADV", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.26": {"text": "Wenn ihr wirket und schafft in heiliger Nacht und", "tokens": ["Wenn", "ihr", "wir\u00b7ket", "und", "schafft", "in", "hei\u00b7li\u00b7ger", "Nacht", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "KON"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "alleinherrscht,", "tokens": ["al\u00b7lein\u00b7herrscht", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "Und allm\u00e4hlig emporziehet ein ahnendes Volk,", "tokens": ["Und", "all\u00b7m\u00e4h\u00b7lig", "em\u00b7por\u00b7zie\u00b7het", "ein", "ah\u00b7nen\u00b7des", "Volk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Bis die J\u00fcnglinge sich der V\u00e4ter droben erinnern,", "tokens": ["Bis", "die", "J\u00fcng\u00b7lin\u00b7ge", "sich", "der", "V\u00e4\u00b7ter", "dro\u00b7ben", "e\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-++-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "M\u00fcndig und hell vor euch steht der besonnene", "tokens": ["M\u00fcn\u00b7dig", "und", "hell", "vor", "euch", "steht", "der", "be\u00b7son\u00b7ne\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPR", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.31": {"text": "Mensch.", "tokens": ["Mensch", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Engel des Vaterlands! o ihr, vor denen das Auge,", "tokens": ["En\u00b7gel", "des", "Va\u00b7ter\u00b7lands", "!", "o", "ihr", ",", "vor", "de\u00b7nen", "das", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "FM", "PPER", "$,", "APPR", "PRELS", "ART", "NN", "$,"], "meter": "+--+-+--+---+-", "measure": "iambic.penta.invert"}, "line.33": {"text": "Sey's auch stark, und das Knie bricht dem", "tokens": ["Sey's", "auch", "stark", ",", "und", "das", "Knie", "bricht", "dem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJD", "$,", "KON", "ART", "NN", "VVFIN", "ART"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.34": {"text": "vereinzelten Mann,", "tokens": ["ver\u00b7ein\u00b7zel\u00b7ten", "Mann", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "Da\u00df er halten sich mu\u00df an die Freund' und bitten", "tokens": ["Da\u00df", "er", "hal\u00b7ten", "sich", "mu\u00df", "an", "die", "Freund'", "und", "bit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "VMFIN", "APPR", "ART", "NN", "KON", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.36": {"text": "die Theuern,", "tokens": ["die", "Theu\u00b7ern", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Da\u00df sie tragen mit ihm all die begl\u00fcckende Last,", "tokens": ["Da\u00df", "sie", "tra\u00b7gen", "mit", "ihm", "all", "die", "be\u00b7gl\u00fc\u00b7cken\u00b7de", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPER", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Habt, o G\u00fctige, Dank f\u00fcr den und alle die Andern,", "tokens": ["Habt", ",", "o", "G\u00fc\u00b7ti\u00b7ge", ",", "Dank", "f\u00fcr", "den", "und", "al\u00b7le", "die", "An\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "NN", "$,", "NN", "APPR", "ART", "KON", "PIS", "ART", "ADJA", "$,"], "meter": "+-+--++--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Die mein Leben, mein Gut unten den Sterb-", "tokens": ["Die", "mein", "Le\u00b7ben", ",", "mein", "Gut", "un\u00b7ten", "den", "Ster\u00b7b"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ADV", "ART", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.40": {"text": "lichen sind.", "tokens": ["li\u00b7chen", "sind", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}