{"dta.poem.18822": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn der K\u00f6rper ein Kerker ist,", "tokens": ["Wenn", "der", "K\u00f6r\u00b7per", "ein", "Ker\u00b7ker", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Warum nur der Kerker so durstig ist?", "tokens": ["Wa\u00b7rum", "nur", "der", "Ker\u00b7ker", "so", "durs\u00b7tig", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Seele befindet sich wohl darinnen", "tokens": ["See\u00b7le", "be\u00b7fin\u00b7det", "sich", "wohl", "da\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PRF", "ADV", "ADV"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und bliebe gern vergn\u00fcgt bey Sinnen;", "tokens": ["Und", "blie\u00b7be", "gern", "ver\u00b7gn\u00fcgt", "bey", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun aber soll eine Flasche Wein", "tokens": ["Nun", "a\u00b7ber", "soll", "ei\u00b7ne", "Fla\u00b7sche", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Frisch eine nach der andern herein.", "tokens": ["Frisch", "ei\u00b7ne", "nach", "der", "an\u00b7dern", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "APPR", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Seele will\u2019s nicht l\u00e4nger tragen,", "tokens": ["See\u00b7le", "will's", "nicht", "l\u00e4n\u00b7ger", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie an der Th\u00fcre in St\u00fccke schlagen.", "tokens": ["Sie", "an", "der", "Th\u00fc\u00b7re", "in", "St\u00fc\u00b7cke", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}