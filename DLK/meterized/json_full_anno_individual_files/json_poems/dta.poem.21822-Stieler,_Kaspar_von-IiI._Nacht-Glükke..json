{"dta.poem.21822": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Nacht-Gl\u00fckke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.85", "no:0.14"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Wjllkommen F\u00fcrstinn aller N\u00e4chte!", "tokens": ["Wjll\u00b7kom\u00b7men", "F\u00fcrs\u00b7tinn", "al\u00b7ler", "N\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Printz der Silber-Knechte/", "tokens": ["Printz", "der", "Sil\u00b7ber\u00b7Knech\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "willkommen/ Mohn/ aus d\u00fcstrer Bahn", "tokens": ["will\u00b7kom\u00b7men", "/", "Mohn", "/", "aus", "d\u00fcst\u00b7rer", "Bahn"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$(", "NN", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "vom Ozean!", "tokens": ["vom", "O\u00b7ze\u00b7an", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Di\u00df ist die Nacht/ die tausend Tagen", "tokens": ["Di\u00df", "ist", "die", "Nacht", "/", "die", "tau\u00b7send", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "ART", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Trozz kan sagen:", "tokens": ["Trozz", "kan", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "weil mein Schazz", "tokens": ["weil", "mein", "Schazz"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "hier in Priapus Plazz\u2019", "tokens": ["hier", "in", "Pria\u00b7pus", "Plaz\u00b7z'"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "erscheinen wird/ zu stillen meine Pein.", "tokens": ["er\u00b7schei\u00b7nen", "wird", "/", "zu", "stil\u00b7len", "mei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VAFIN", "$(", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wer wird/ wie ich/ wol so begl\u00fckket sein?", "tokens": ["Wer", "wird", "/", "wie", "ich", "/", "wol", "so", "be\u00b7gl\u00fck\u00b7ket", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "PWAV", "PPER", "$(", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Beneidet himmlische Laternen/", "tokens": ["Be\u00b7nei\u00b7det", "himm\u00b7li\u00b7sche", "La\u00b7ter\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wei\u00df-geflammte Sternen/", "tokens": ["wei\u00df\u00b7ge\u00b7flamm\u00b7te", "Ster\u00b7nen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "mit einem sch\u00e4len Angesicht\u2019", "tokens": ["mit", "ei\u00b7nem", "sch\u00e4\u00b7len", "An\u00b7ge\u00b7sicht'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ach! mich nur nicht.", "tokens": ["ach", "!", "mich", "nur", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "kein Mensch/ als ihr nur m\u00f6get wissen/", "tokens": ["kein", "Mensch", "/", "als", "ihr", "nur", "m\u00f6\u00b7get", "wis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "wie w", "tokens": ["wie", "w"], "token_info": ["word", "word"], "pos": ["PWAV", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "alle Welt", "tokens": ["al\u00b7le", "Welt"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "hat seine Ruh bestellt/", "tokens": ["hat", "sei\u00b7ne", "Ruh", "be\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "wir beyde nur/ ich und mein Kind/ sind wach/", "tokens": ["wir", "bey\u00b7de", "nur", "/", "ich", "und", "mein", "Kind", "/", "sind", "wach", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "$(", "PPER", "KON", "PPOSAT", "NN", "$(", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "und/ Flammen/ ihr an Bronteus Wolken-dach\u2019", "tokens": ["und", "/", "Flam\u00b7men", "/", "ihr", "an", "Bron\u00b7teus", "Wol\u00b7ken\u00b7da\u00b7ch'"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$(", "NN", "$(", "PPER", "APPR", "NE", "NE"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Es seuselt Zefyr au\u00df dem Weste", "tokens": ["Es", "seu\u00b7selt", "Ze\u00b7fyr", "au\u00df", "dem", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "durch Pomonen \u00e4ste/", "tokens": ["durch", "Po\u00b7mo\u00b7nen", "\u00e4s\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "es seufzet sein verliebter Wind", "tokens": ["es", "seuf\u00b7zet", "sein", "ver\u00b7lieb\u00b7ter", "Wind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nach meinem Kind\u2019", "tokens": ["nach", "mei\u00b7nem", "Kind'"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich seh es gerne da\u00df er spielet", "tokens": ["Ich", "seh", "es", "ger\u00b7ne", "da\u00df", "er", "spie\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und sie k\u00fchlet/", "tokens": ["und", "sie", "k\u00fch\u00b7let", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "weil sie mir", "tokens": ["weil", "sie", "mir"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "folgt durch die Garten-T\u00fchr/", "tokens": ["folgt", "durch", "die", "Gar\u00b7ten\u00b7T\u00fchr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "und doppelt den geschwinden Liebes-tritt.", "tokens": ["und", "dop\u00b7pelt", "den", "ge\u00b7schwin\u00b7den", "Lie\u00b7bes\u00b7tritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Bring/ West/ sie bald und tausend K\u00fcsse mit!", "tokens": ["Bring", "/", "West", "/", "sie", "bald", "und", "tau\u00b7send", "K\u00fcs\u00b7se", "mit", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$(", "PPER", "ADV", "KON", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was werd\u2019 ich wenn sie k\u00f6mmt gegangen/", "tokens": ["Was", "werd'", "ich", "wenn", "sie", "k\u00f6mmt", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "KOUS", "PPER", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "an-doch erstlichst-fangen/", "tokens": ["an\u00b7doch", "erst\u00b7lichst\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "K\u00fc\u00df\u2019 ich die Hand/ den Brust/ den Mund", "tokens": ["K\u00fc\u00df'", "ich", "die", "Hand", "/", "den", "Brust", "/", "den", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zur selben Stund\u2019?", "tokens": ["zur", "sel\u00b7ben", "Stund'", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Ich werd\u2019 (ich wei\u00df) kein Wort nicht machen/", "tokens": ["Ich", "werd'", "(", "ich", "wei\u00df", ")", "kein", "Wort", "nicht", "ma\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PPER", "VVFIN", "$(", "PIAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "die an Zier", "tokens": ["die", "an", "Zier"], "token_info": ["word", "word", "word"], "pos": ["ART", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "den G\u00f6ttern gehen f\u00fcr", "tokens": ["den", "G\u00f6t\u00b7tern", "ge\u00b7hen", "f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "und auff di\u00df Sch\u00f6nchen sein gewendet an/", "tokens": ["und", "auff", "di\u00df", "Sch\u00f6n\u00b7chen", "sein", "ge\u00b7wen\u00b7det", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDS", "NN", "PPOSAT", "VVPP", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "erstaunen mich/ da\u00df ich nicht reden kan.", "tokens": ["er\u00b7stau\u00b7nen", "mich", "/", "da\u00df", "ich", "nicht", "re\u00b7den", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Komm/ Flora/ streue dein Verm\u00fcgen", "tokens": ["Komm", "/", "Flo\u00b7ra", "/", "streu\u00b7e", "dein", "Ver\u00b7m\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$(", "NE", "$(", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "darhin/ wo wir liegen!", "tokens": ["dar\u00b7hin", "/", "wo", "wir", "lie\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Es soll ein bunter Rosen-hauf\u2019", "tokens": ["Es", "soll", "ein", "bun\u00b7ter", "Ro\u00b7sen\u00b7hauf'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "uns nehmen auff/", "tokens": ["uns", "neh\u00b7men", "auff", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "und/ Venus du sollst in den Myrten", "tokens": ["und", "/", "Ve\u00b7nus", "du", "sollst", "in", "den", "Myr\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "NE", "PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "uns bewirten/", "tokens": ["uns", "be\u00b7wir\u00b7ten", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "bi\u00df das Blut", "tokens": ["bi\u00df", "das", "Blut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "der R\u00f6ht\u2019 herf\u00fcr sich tuht.", "tokens": ["der", "R\u00f6ht'", "her\u00b7f\u00fcr", "sich", "tuht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Was Schein ist das? die Schatten werden klar.", "tokens": ["Was", "Schein", "ist", "das", "?", "die", "Schat\u00b7ten", "wer\u00b7den", "klar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "PDS", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Still! Lauten-klang/ mein Liebchen ist schon dar.", "tokens": ["Still", "!", "Lau\u00b7ten\u00b7klang", "/", "mein", "Lieb\u00b7chen", "ist", "schon", "dar", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "NN", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}