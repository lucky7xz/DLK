{"textgrid.poem.35666": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "29. An G\u00f6ckingk", "genre": "verse", "period": "N.A.", "pub_year": 1780, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welche Hexe, ge\u00fcbt, zur Walpurgsgala", "tokens": ["Wel\u00b7che", "He\u00b7xe", ",", "ge\u00b7\u00fcbt", ",", "zur", "Wal\u00b7purgs\u00b7ga\u00b7la"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "$,", "VVPP", "$,", "APPRART", "NN"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Meister Satans auf Bock und Ofengabel", "tokens": ["Meis\u00b7ter", "Sa\u00b7tans", "auf", "Bock", "und", "O\u00b7fen\u00b7ga\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NE", "KON", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Hinzureiten; vor Lust aus ihrem Nachttopf", "tokens": ["Hin\u00b7zu\u00b7rei\u00b7ten", ";", "vor", "Lust", "aus", "ih\u00b7rem", "Nacht\u00b7topf"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ungewitter zu gie\u00dfen; Fl\u00f6h' und Wanzen,", "tokens": ["Un\u00b7ge\u00b7wit\u00b7ter", "zu", "gie\u00b7\u00dfen", ";", "Fl\u00f6h'", "und", "Wan\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$.", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "M\u00e4us' und Ratzen in unbekreuzte H\u00e4user", "tokens": ["M\u00e4us'", "und", "Rat\u00b7zen", "in", "un\u00b7be\u00b7kreuz\u00b7te", "H\u00e4u\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Frommer Leute zu bannen; Saatenfelder", "tokens": ["From\u00b7mer", "Leu\u00b7te", "zu", "ban\u00b7nen", ";", "Saa\u00b7ten\u00b7fel\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$.", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Kahl zu hexen; und nachts die Kuh des Nachbars", "tokens": ["Kahl", "zu", "he\u00b7xen", ";", "und", "nachts", "die", "Kuh", "des", "Nach\u00b7bars"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$.", "KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Durch den St\u00e4nder zu melken, da\u00df die Viehmagd", "tokens": ["Durch", "den", "St\u00e4n\u00b7der", "zu", "mel\u00b7ken", ",", "da\u00df", "die", "Vieh\u00b7magd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Voll Verwunderung Blut statt Milch herauszerrt:", "tokens": ["Voll", "Ver\u00b7wun\u00b7de\u00b7rung", "Blut", "statt", "Milch", "her\u00b7aus\u00b7zerrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Welch trief\u00e4ugichtes, schieles, ausgest\u00e4uptes,", "tokens": ["Welch", "trie\u00b7f\u00e4u\u00b7gich\u00b7tes", ",", "schie\u00b7les", ",", "aus\u00b7ge\u00b7st\u00e4up\u00b7tes", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "L\u00e4ngst f\u00fcr Galgen und Rad und Strang und Holzsto\u00df", "tokens": ["L\u00e4ngst", "f\u00fcr", "Gal\u00b7gen", "und", "Rad", "und", "Strang", "und", "Holz\u00b7sto\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Reifgewordenes Weib erfand das Posthorn,", "tokens": ["Reif\u00b7ge\u00b7wor\u00b7de\u00b7nes", "Weib", "er\u00b7fand", "das", "Post\u00b7horn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Welches m\u00e4chtiger noch an Zaubert\u00f6nen,", "tokens": ["Wel\u00b7ches", "m\u00e4ch\u00b7ti\u00b7ger", "noch", "an", "Zau\u00b7ber\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "ADV", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Als des Hamelschen Ratzenf\u00e4ngers Pfeife,", "tokens": ["Als", "des", "Ha\u00b7mel\u00b7schen", "Rat\u00b7zen\u00b7f\u00e4n\u00b7gers", "Pfei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Allen dichtrischen Aberwitz und Unsinn,", "tokens": ["Al\u00b7len", "dich\u00b7tri\u00b7schen", "A\u00b7ber\u00b7witz", "und", "Un\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.16": {"text": "Der im heiligen r\u00f6mschen Reich nur aufkeimt,", "tokens": ["Der", "im", "hei\u00b7li\u00b7gen", "r\u00f6m\u00b7schen", "Reich", "nur", "auf\u00b7keimt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Mir herbannt! Denn so oft des Schreckenhornes", "tokens": ["Mir", "her\u00b7bannt", "!", "Denn", "so", "oft", "des", "Schre\u00b7cken\u00b7hor\u00b7nes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Taratantara t\u00f6nt; k\u00f6mmt Ode, Volkslied,", "tokens": ["Ta\u00b7ra\u00b7tan\u00b7ta\u00b7ra", "t\u00f6nt", ";", "k\u00f6mmt", "O\u00b7de", ",", "Volks\u00b7lied", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Epigramm und Idyll', Epistel, Fabel,", "tokens": ["E\u00b7pig\u00b7ramm", "und", "I\u00b7dyll'", ",", "E\u00b7pis\u00b7tel", ",", "Fa\u00b7bel", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Elegie und Ballad', und aller Mi\u00dfwachs,", "tokens": ["E\u00b7le\u00b7gie", "und", "Bal\u00b7lad'", ",", "und", "al\u00b7ler", "Mi\u00df\u00b7wachs", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Der auf sandiger Heid', in kalten S\u00fcmpfen,", "tokens": ["Der", "auf", "san\u00b7di\u00b7ger", "Heid'", ",", "in", "kal\u00b7ten", "S\u00fcmp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Oder brennendem Miste wild hervorscho\u00df:", "tokens": ["O\u00b7der", "bren\u00b7nen\u00b7dem", "Mis\u00b7te", "wild", "her\u00b7vor\u00b7scho\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.23": {"text": "Kommt im Sturme dahergesaust, und wuchert", "tokens": ["Kommt", "im", "Stur\u00b7me", "da\u00b7her\u00b7ge\u00b7saust", ",", "und", "wu\u00b7chert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "VVFIN", "$,", "KON", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Durch die Beete des sch\u00f6nen Blumengartens,", "tokens": ["Durch", "die", "Bee\u00b7te", "des", "sch\u00f6\u00b7nen", "Blu\u00b7men\u00b7gar\u00b7tens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Wo, erm\u00fcdet von Arbeit, Deutschlands M\u00e4nner", "tokens": ["Wo", ",", "er\u00b7m\u00fc\u00b7det", "von", "Ar\u00b7beit", ",", "Deutschlands", "M\u00e4n\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "VVFIN", "APPR", "NN", "$,", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.26": {"text": "Und rotwangichte Frau'n in lauer D\u00e4mmrung", "tokens": ["Und", "rot\u00b7wan\u00b7gich\u00b7te", "Frau'n", "in", "lau\u00b7er", "D\u00e4mm\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Atmend unter Gesang und Lachen wandeln.", "tokens": ["At\u00b7mend", "un\u00b7ter", "Ge\u00b7sang", "und", "La\u00b7chen", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "G\u00e4te, raufe mit mir das geile Unkraut!", "tokens": ["G\u00e4\u00b7te", ",", "rau\u00b7fe", "mit", "mir", "das", "gei\u00b7le", "Un\u00b7kraut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "Hurtig, G\u00f6ckingk, du rechts; ich g\u00e4te linksum!", "tokens": ["Hur\u00b7tig", ",", "G\u00f6\u00b7ckingk", ",", "du", "rechts", ";", "ich", "g\u00e4\u00b7te", "link\u00b7sum", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "PPER", "ADV", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.30": {"text": "Hier die Quecke von Trink- und Liebesliedern,", "tokens": ["Hier", "die", "Que\u00b7cke", "von", "Trink", "und", "Lie\u00b7bes\u00b7lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.31": {"text": "Dort elegischen Wermut, Odentollwurz,", "tokens": ["Dort", "e\u00b7le\u00b7gi\u00b7schen", "Wer\u00b7mut", ",", "O\u00b7den\u00b7toll\u00b7wurz", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "Und Saudisteln des Minn'- und Bardensanges,", "tokens": ["Und", "Sau\u00b7dis\u00b7teln", "des", "Minn'", "und", "Bar\u00b7den\u00b7san\u00b7ges", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Taube Nesseln des Epigramms, und langen", "tokens": ["Tau\u00b7be", "Nes\u00b7seln", "des", "E\u00b7pig\u00b7ramms", ",", "und", "lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "ART", "NN", "$,", "KON", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "Epistolischen H\u00fchnerschwarm, des Volkslieds", "tokens": ["E\u00b7pis\u00b7to\u00b7li\u00b7schen", "H\u00fch\u00b7ner\u00b7schwarm", ",", "des", "Volks\u00b7lieds"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "Pofist, und der Balladen Teufelsabbi\u00df!", "tokens": ["Po\u00b7fist", ",", "und", "der", "Bal\u00b7la\u00b7den", "Teu\u00b7fels\u00b7ab\u00b7bi\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Hurtig! nicht in den Steig, dort hintern Dornbusch", "tokens": ["Hur\u00b7tig", "!", "nicht", "in", "den", "Steig", ",", "dort", "hin\u00b7tern", "Dorn\u00b7busch"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$.", "PTKNEG", "APPR", "ART", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.37": {"text": "Hingeschleudert den ekelhaften Unrat,", "tokens": ["Hin\u00b7ge\u00b7schleu\u00b7dert", "den", "e\u00b7kel\u00b7haf\u00b7ten", "Un\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "Aufgeh\u00e4uft und verbrannt mit Pech und Schwefel!", "tokens": ["Auf\u00b7ge\u00b7h\u00e4uft", "und", "ver\u00b7brannt", "mit", "Pech", "und", "Schwe\u00b7fel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.39": {"text": "Ha! dann stehen wir fern mit Hopfenstangen,", "tokens": ["Ha", "!", "dann", "ste\u00b7hen", "wir", "fern", "mit", "Hop\u00b7fen\u00b7stan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.40": {"text": "Abgewandt, und die Nase fest zuhaltend,", "tokens": ["Ab\u00b7ge\u00b7wandt", ",", "und", "die", "Na\u00b7se", "fest", "zu\u00b7hal\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.41": {"text": "Stehn, und sch\u00fcren die Glut; indes der dicke", "tokens": ["Stehn", ",", "und", "sch\u00fc\u00b7ren", "die", "Glut", ";", "in\u00b7des", "der", "di\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "VVFIN", "ART", "NN", "$.", "ADV", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.42": {"text": "Pestaushauchende schwarze Qualm hoch aufsteigt,", "tokens": ["Pest\u00b7aus\u00b7hau\u00b7chen\u00b7de", "schwar\u00b7ze", "Qualm", "hoch", "auf\u00b7steigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.43": {"text": "Der noch stinkender, als Tobias Fischdampf,", "tokens": ["Der", "noch", "stin\u00b7ken\u00b7der", ",", "als", "To\u00b7bias", "Fischdampf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "KOUS", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.44": {"text": "Alle Teufel verscheucht, und, weht ein Teilchen", "tokens": ["Al\u00b7le", "Teu\u00b7fel", "ver\u00b7scheucht", ",", "und", ",", "weht", "ein", "Teil\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KON", "$,", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.45": {"text": "Ihr ins Maul, die verw\u00fcnschte Hexe kitzelt,", "tokens": ["Ihr", "ins", "Maul", ",", "die", "ver\u00b7w\u00fcnschte", "He\u00b7xe", "kit\u00b7zelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--++", "measure": "anapaest.tri.plus"}, "line.46": {"text": "Da\u00df sie hustend die schwarze Seel' herausw\u00fcrgt!", "tokens": ["Da\u00df", "sie", "hus\u00b7tend", "die", "schwar\u00b7ze", "Seel'", "her\u00b7aus\u00b7w\u00fcrgt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.47": {"text": "Aber, Freund, in den Winkeln la\u00df des Unkrauts", "tokens": ["A\u00b7ber", ",", "Freund", ",", "in", "den", "Win\u00b7keln", "la\u00df", "des", "Un\u00b7krauts"], "token_info": ["word", "punct", "word", "punct", "w