{"textgrid.poem.29024": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was denckstu, lieber Gott, wann ietzund deine Christen", "tokens": ["Was", "dencks\u00b7tu", ",", "lie\u00b7ber", "Gott", ",", "wann", "ie\u00b7tzund", "dei\u00b7ne", "Chris\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PDS", "$,", "ADV", "NN", "$,", "PWAV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In deinem Hause dir nach ihres Ohres L\u00fcsten", "tokens": ["In", "dei\u00b7nem", "Hau\u00b7se", "dir", "nach", "ih\u00b7res", "Oh\u00b7res", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bestellen Sang und Klang? Die krause Melodey", "tokens": ["Be\u00b7stel\u00b7len", "Sang", "und", "Klang", "?", "Die", "krau\u00b7se", "Me\u00b7lo\u00b7dey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird angestimmt zum Tantz und s\u00fcsser Buhlerey;", "tokens": ["Wird", "an\u00b7ge\u00b7stimmt", "zum", "Tantz", "und", "s\u00fcs\u00b7ser", "Buh\u00b7le\u00b7rey", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "APPRART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Andacht acht man nicht. Der geilen Brunst Gefieder", "tokens": ["Die", "An\u00b7dacht", "acht", "man", "nicht", ".", "Der", "gei\u00b7len", "Brunst", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "CARD", "PIS", "PTKNEG", "$.", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erw\u00e4chst und steigt empor durch unsre freche Lieder;", "tokens": ["Er\u00b7w\u00e4chst", "und", "steigt", "em\u00b7por", "durch", "uns\u00b7re", "fre\u00b7che", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der stille Geist ersitzt: Wir h\u00f6ren viel Geschrey;", "tokens": ["Der", "stil\u00b7le", "Geist", "er\u00b7sitzt", ":", "Wir", "h\u00f6\u00b7ren", "viel", "Ge\u00b7schrey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Einfalt wei\u00df nicht viel, obs s\u00fc\u00df, obs sauer sey,", "tokens": ["Die", "Ein\u00b7falt", "wei\u00df", "nicht", "viel", ",", "obs", "s\u00fc\u00df", ",", "obs", "sau\u00b7er", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$,", "KOUS", "ADJD", "$,", "KOUS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Obs Thier, obs Menschen sind, die ohne Sinn so klingen,", "tokens": ["Obs", "Thier", ",", "obs", "Men\u00b7schen", "sind", ",", "die", "oh\u00b7ne", "Sinn", "so", "klin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "NN", "VAFIN", "$,", "PRELS", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ob seuffzen einer soll, ob einer so soll springen.", "tokens": ["Ob", "seuff\u00b7zen", "ei\u00b7ner", "soll", ",", "ob", "ei\u00b7ner", "so", "soll", "sprin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJD", "$,", "KOUS", "ART", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man wiegert den Discant; man br\u00fcllet den Tenor;", "tokens": ["Man", "wie\u00b7gert", "den", "Dis\u00b7cant", ";", "man", "br\u00fcl\u00b7let", "den", "Te\u00b7nor", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$.", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Man billt den Contrapunct; man heult den Alt hervor;", "tokens": ["Man", "billt", "den", "Con\u00b7tra\u00b7punct", ";", "man", "heult", "den", "Alt", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "$.", "PIS", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man brummt den tieffen Bass, und wann es wol soll klingen,", "tokens": ["Man", "brummt", "den", "tief\u00b7fen", "Bass", ",", "und", "wann", "es", "wol", "soll", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "PWAV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So klingt es ohne Wort, wil keine Meinung bringen;", "tokens": ["So", "klingt", "es", "oh\u00b7ne", "Wort", ",", "wil", "kei\u00b7ne", "Mei\u00b7nung", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man wei\u00df nicht, ob es Danck, man wei\u00df nicht, ob es Preis,", "tokens": ["Man", "wei\u00df", "nicht", ",", "ob", "es", "Danck", ",", "man", "wei\u00df", "nicht", ",", "ob", "es", "Preis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "NN", "$,", "PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Man wei\u00df nicht, obs Gebet und was es sonsten hei\u00df.", "tokens": ["Man", "wei\u00df", "nicht", ",", "obs", "Ge\u00b7bet", "und", "was", "es", "sons\u00b7ten", "hei\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "NN", "KON", "PWS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was denckstu, lieber Gott, wann wir so sehr uns regen", "tokens": ["Was", "dencks\u00b7tu", ",", "lie\u00b7ber", "Gott", ",", "wann", "wir", "so", "sehr", "uns", "re\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDS", "$,", "ADV", "NN", "$,", "PWAV", "PPER", "ADV