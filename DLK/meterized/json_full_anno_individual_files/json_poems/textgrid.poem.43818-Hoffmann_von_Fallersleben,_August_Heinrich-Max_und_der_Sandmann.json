{"textgrid.poem.43818": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Max und der Sandmann", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Max, willst du noch nicht schlafen?", "tokens": ["Max", ",", "willst", "du", "noch", "nicht", "schla\u00b7fen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Begieb dich doch zur Ruh!", "tokens": ["Be\u00b7gieb", "dich", "doch", "zur", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bist gewi\u00df recht m\u00fcde,", "tokens": ["Du", "bist", "ge\u00b7wi\u00df", "recht", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Augen fallen dir zu.", "tokens": ["Die", "Au\u00b7gen", "fal\u00b7len", "dir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "\u00bbo nein, ich will nicht schlafen,", "tokens": ["\u00bb", "o", "nein", ",", "ich", "will", "nicht", "schla\u00b7fen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKANT", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich will noch munter sein;", "tokens": ["Ich", "will", "noch", "mun\u00b7ter", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erst soll der W\u00e4chter blasen,", "tokens": ["Erst", "soll", "der", "W\u00e4ch\u00b7ter", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht eher nick' ich ein.\u00ab", "tokens": ["Nicht", "e\u00b7her", "nick'", "ich", "ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Sandmann wird schon kommen,", "tokens": ["Der", "Sand\u00b7mann", "wird", "schon", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er bleibet l\u00e4nger nicht,", "tokens": ["Er", "blei\u00b7bet", "l\u00e4n\u00b7ger", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wird Schlafk\u00f6rner streuen", "tokens": ["Er", "wird", "Schlaf\u00b7k\u00f6r\u00b7ner", "streu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir in das Angesicht.", "tokens": ["Dir", "in", "das", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbder Sandmann soll's mal wagen!", "tokens": ["\u00bb", "der", "Sand\u00b7mann", "soll's", "mal", "wa\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er komme mal heran!", "tokens": ["Er", "kom\u00b7me", "mal", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er wird sich heute wundern,", "tokens": ["Er", "wird", "sich", "heu\u00b7te", "wun\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heut steh' ich meinen Mann.\u00ab \u2013", "tokens": ["Heut", "steh'", "ich", "mei\u00b7nen", "Mann", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Sandmann ist gekommen,", "tokens": ["Der", "Sand\u00b7mann", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch ehe man's sich gedacht,", "tokens": ["Noch", "e\u00b7he", "man's", "sich", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PRF", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und Maxel ist eingeschlafen:", "tokens": ["Und", "Ma\u00b7xel", "ist", "ein\u00b7ge\u00b7schla\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "VVPP", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Nun, Maxel,