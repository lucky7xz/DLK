{"dta.poem.10830": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Anrede an diejenigen die mit  \n Seidenen Kleidern prangen, und sich  \n dadurch einen Vorzug anma\u00dfen  \n wollen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr die ihr eurer Sch\u00f6nheit Glanz von\nSchimmerreichen Kleidern borgt,", "tokens": ["Ihr", "die", "ihr", "eu\u00b7rer", "Sch\u00f6n\u00b7heit", "Glanz", "von", "Schim\u00b7mer\u00b7rei\u00b7chen", "Klei\u00b7dern", "borgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und mehr f\u00fcr euren Leibes Schmuk; als", "tokens": ["Und", "mehr", "f\u00fcr", "eu\u00b7ren", "Lei\u00b7bes", "Schmuk", ";", "als"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "NN", "$.", "KOUS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr prangt bei euren eitlen Wahn ach! leider nur", "tokens": ["Ihr", "prangt", "bei", "eu\u00b7ren", "eit\u00b7len", "Wahn", "ach", "!", "lei\u00b7der", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "ITJ", "$.", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Dekken eurer Schande sind, und euch gar kei-", "tokens": ["Die", "Dek\u00b7ken", "eu\u00b7rer", "Schan\u00b7de", "sind", ",", "und", "euch", "gar", "kei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "$,", "KON", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Kleider sind zum Zeichen uns, da\u00df wir entbl\u00f6\u00df-", "tokens": ["Die", "Klei\u00b7der", "sind", "zum", "Zei\u00b7chen", "uns", ",", "da\u00df", "wir", "ent\u00b7bl\u00f6\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "PPER", "$,", "KOUS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und da\u00df die S\u00fcnde uns geschw\u00e4cht, bei der verdor-", "tokens": ["Und", "da\u00df", "die", "S\u00fcn\u00b7de", "uns", "ge\u00b7schw\u00e4cht", ",", "bei", "der", "ver\u00b7dor"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie th\u00f6richt ist es, wenn ein Dieb mit seidnen Strik-", "tokens": ["Wie", "th\u00f6\u00b7richt", "ist", "es", ",", "wenn", "ein", "Dieb", "mit", "seid\u00b7nen", "Stri\u00b7k"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn es vor ihm ein Zeichen w\u00e4r, er solte an den-", "tokens": ["Wenn", "es", "vor", "ihm", "ein", "Zei\u00b7chen", "w\u00e4r", ",", "er", "sol\u00b7te", "an", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VAFIN", "$,", "PPER", "VMFIN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jhr prangt mit einem seidnen Tuch, das mannig-", "tokens": ["Ihr", "prangt", "mit", "ei\u00b7nem", "seid\u00b7nen", "Tuch", ",", "das", "man\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "PPOSAT", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Allein denkt nur von wem ihr habt dazu den ersten", "tokens": ["Al\u00b7lein", "denkt", "nur", "von", "wem", "ihr", "habt", "da\u00b7zu", "den", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PWS", "PPER", "VAFIN", "PAV", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein Wurm hat ihn vor sich gemacht, um sich in", "tokens": ["Ein", "Wurm", "hat", "ihn", "vor", "sich", "ge\u00b7macht", ",", "um", "sich", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "PRF", "VVPP", "$,", "KOUI", "PRF", "APPR"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "Als in sein eigenes Gespinst, vor Luft und Wetter zu", "tokens": ["Als", "in", "sein", "ei\u00b7ge\u00b7nes", "Ge\u00b7spinst", ",", "vor", "Luft", "und", "Wet\u00b7ter", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "PTKZU"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Gebrauchet immer dies Geweb, der Sch\u00f6pfer g\u00f6n-", "tokens": ["Ge\u00b7brau\u00b7chet", "im\u00b7mer", "dies", "Ge\u00b7web", ",", "der", "Sch\u00f6p\u00b7fer", "g\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PDS", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und h\u00fcllet euch in ein Gewand das euren Stand ge-", "tokens": ["Und", "h\u00fcl\u00b7let", "euch", "in", "ein", "Ge\u00b7wand", "das", "eu\u00b7ren", "Stand", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Stolziret nicht mit seidner Tracht; sonst werden wir", "tokens": ["Stol\u00b7zi\u00b7ret", "nicht", "mit", "seid\u00b7ner", "Tracht", ";", "sonst", "wer\u00b7den", "wir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ein Thore prangt mit einem Kleid, das l\u00e4ngst", "tokens": ["Ein", "Tho\u00b7re", "prangt", "mit", "ei\u00b7nem", "Kleid", ",", "das", "l\u00e4ngst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}