{"dta.poem.17100": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "201.  GOtt gibt gern grosse Gaben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott/ weil Er gro\u00df ist/ gibt am liebsten grosse Gaben.", "tokens": ["Gott", "/", "weil", "Er", "gro\u00df", "ist", "/", "gibt", "am", "liebs\u00b7ten", "gros\u00b7se", "Ga\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$(", "VVFIN", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ach da\u00df wir arme nur so kleine Hertzen haben!", "tokens": ["Ach", "da\u00df", "wir", "ar\u00b7me", "nur", "so", "klei\u00b7ne", "Hert\u00b7zen", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ADJA", "ADV", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}