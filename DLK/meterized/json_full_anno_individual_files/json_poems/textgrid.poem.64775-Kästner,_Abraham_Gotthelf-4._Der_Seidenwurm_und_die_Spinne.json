{"textgrid.poem.64775": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "4. Der Seidenwurm und die Spinne", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Raupen edelste, die Weberinn der Seide,", "tokens": ["Der", "Rau\u00b7pen", "e\u00b7dels\u00b7te", ",", "die", "We\u00b7be\u00b7rinn", "der", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spann sich ihr Grab zu eines F\u00fcrsten Kleide;", "tokens": ["Spann", "sich", "ihr", "Grab", "zu", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "Klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "PPOSAT", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht weit von ihr hing an der schwarzen Wand", "tokens": ["Nicht", "weit", "von", "ihr", "hing", "an", "der", "schwar\u00b7zen", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "APPR", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die K\u00fcnstlerin, die Pallas \u00fcberwand.", "tokens": ["Die", "K\u00fcnst\u00b7le\u00b7rin", ",", "die", "Pal\u00b7las", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch war von ihr nicht ganz der alte Stolz entwichen,", "tokens": ["Noch", "war", "von", "ihr", "nicht", "g