e darf erheben,", "tokens": ["Und", "wenn", "sich", "mein", "Ge\u00b7spinnst", "auf", "Thro\u00b7ne", "darf", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "PPOSAT", "NN", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So lern' ich wohl von dir nicht Fliegennetze weben.", "tokens": ["So", "lern'", "ich", "wohl", "von", "dir", "nicht", "Flie\u00b7gen\u00b7net\u00b7ze", "we\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Abstrakte Logiker, merkt euch den Unterricht,", "tokens": ["A\u00b7bstrak\u00b7te", "Lo\u00b7gi\u00b7ker", ",", "merkt", "euch", "den", "Un\u00b7ter\u00b7richt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}