{"dta.poem.19730": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Nachtigall .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jungfrau merk auf meinen Schall,               ", "tokens": ["Jung\u00b7frau", "merk", "auf", "mei\u00b7nen", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich bin die Frau Nachtigall,", "tokens": ["Ich", "bin", "die", "Frau", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Schwing mich \u00fcber ein hohes Haus,", "tokens": ["Schwing", "mich", "\u00fc\u00b7ber", "ein", "ho\u00b7hes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ein wackrer Herr, der schickt mich aus,", "tokens": ["Ein", "wack\u00b7rer", "Herr", ",", "der", "schickt", "mich", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er schickt euch einen sch\u00f6nen Gru\u00df.", "tokens": ["Er", "schickt", "euch", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun h\u00f6rt, was ich noch sagen mu\u00df.", "tokens": ["Nun", "h\u00f6rt", ",", "was", "ich", "noch", "sa\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er sah im Blumengarten euch,", "tokens": ["Er", "sah", "im", "Blu\u00b7men\u00b7gar\u00b7ten", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Lieb entbrannt sein Herze gleich,", "tokens": ["In", "Lieb", "ent\u00b7brannt", "sein", "Her\u00b7ze", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAINF", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Viel Gut und Ehr hat er umsonst,", "tokens": ["Viel", "Gut", "und", "Ehr", "hat", "er", "um\u00b7sonst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil nichts freut als eure Gunst,", "tokens": ["Weil", "nichts", "freut", "als", "eu\u00b7re", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nehmt diesen Ring doch von ihm an,", "tokens": ["Nehmt", "die\u00b7sen", "Ring", "doch", "von", "ihm", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er sich wieder freuen kann.", "tokens": ["Da\u00df", "er", "sich", "wie\u00b7der", "freu\u00b7en", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geh\u00f6ret hab ich deinen Schall,", "tokens": ["Ge\u00b7h\u00f6\u00b7ret", "hab", "ich", "dei\u00b7nen", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und da\u00df du bist Frau Nachtigall,", "tokens": ["Und", "da\u00df", "du", "bist", "Frau", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwingst dich \u00fcber ein hohes Hau\u00df,", "tokens": ["Schwingst", "dich", "\u00fc\u00b7ber", "ein", "ho\u00b7hes", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ein wackrer Herr, der schickt dich aus,", "tokens": ["Ein", "wack\u00b7rer", "Herr", ",", "der", "schickt", "dich", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schickt mir einen sch\u00f6nen Gru\u00df,", "tokens": ["Und", "schickt", "mir", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun h\u00f6re, was ich sagen mu\u00df.", "tokens": ["Nun", "h\u00f6\u00b7re", ",", "was", "ich", "sa\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Ring steck ich an Finger hier,", "tokens": ["Den", "Ring", "steck", "ich", "an", "Fin\u00b7ger", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schick die Rose ihm daf\u00fcr,", "tokens": ["Und", "schick", "die", "Ro\u00b7se", "ihm", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es war die Rose meine Lust,", "tokens": ["Es", "war", "die", "Ro\u00b7se", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich trug sie wohl an meiner Brust,", "tokens": ["Ich", "trug", "sie", "wohl", "an", "mei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zwar hat sie einen Dorn, der sticht,", "tokens": ["Zwar", "hat", "sie", "ei\u00b7nen", "Dorn", ",", "der", "sticht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch treue Lieb f\u00fcrcht Dornen nicht.", "tokens": ["Doch", "treu\u00b7e", "Lieb", "f\u00fcrcht", "Dor\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}