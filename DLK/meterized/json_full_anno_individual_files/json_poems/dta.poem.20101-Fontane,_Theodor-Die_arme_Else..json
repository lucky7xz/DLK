{"dta.poem.20101": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Die arme Else.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter spricht: \u201elieb Else mein,", "tokens": ["Die", "Mut\u00b7ter", "spricht", ":", "\u201e", "lieb", "El\u00b7se", "mein", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADJD", "NE", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du mu\u00dft nicht lange w\u00e4hlen;", "tokens": ["Du", "mu\u00dft", "nicht", "lan\u00b7ge", "w\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man lebt sich in einander ein,", "tokens": ["Man", "lebt", "sich", "in", "ein\u00b7an\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch ohne Liebesqu\u00e4len;", "tokens": ["Auch", "oh\u00b7ne", "Lie\u00b7bes\u00b7qu\u00e4\u00b7len", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Manch\u2019 Eine nahm schon ihren Mann,", "tokens": ["Man\u00b7ch'", "Ei\u00b7ne", "nahm", "schon", "ih\u00b7ren", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Da\u00df sie nicht sitzen bliebe,", "tokens": ["Da\u00df", "sie", "nicht", "sit\u00b7zen", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und d\u00fcnkte sich im Himmel dann,", "tokens": ["Und", "d\u00fcnk\u00b7te", "sich", "im", "Him\u00b7mel", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und alles ohne Liebe.\u201c", "tokens": ["Und", "al\u00b7les", "oh\u00b7ne", "Lie\u00b7be", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Jung-Else h\u00f6rt\u2019s und schlo\u00df das Band,", "tokens": ["Jung\u00b7El\u00b7se", "h\u00f6rt's", "und", "schlo\u00df", "das", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ewge am Altare,", "tokens": ["Das", "ew\u00b7ge", "am", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es nahm, zur Nacht, des Gatten Hand", "tokens": ["Es", "nahm", ",", "zur", "Nacht", ",", "des", "Gat\u00b7ten", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kranz aus ihrem Haare;", "tokens": ["Den", "Kranz", "aus", "ih\u00b7rem", "Haa\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr war zu Sinn, als ob der Tod", "tokens": ["Ihr", "war", "zu", "Sinn", ",", "als", "ob", "der", "Tod"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "KOKOM", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie auf die Schlachtbank triebe, \u2014", "tokens": ["Sie", "auf", "die", "Schlacht\u00b7bank", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie gab ihr Alles nach \u2014 Gebot,", "tokens": ["Sie", "gab", "ihr", "Al\u00b7les", "nach", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "APPR", "$(", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und alles ohne Liebe.", "tokens": ["Und", "al\u00b7les", "oh\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Mann ist schlecht, er liebt das Spiel,", "tokens": ["Der", "Mann", "ist", "schlecht", ",", "er", "liebt", "das", "Spiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und guten Trunk nicht minder,", "tokens": ["Und", "gu\u00b7ten", "Trunk", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Weib zu Hause weint zu viel,", "tokens": ["Sein", "Weib", "zu", "Hau\u00b7se", "weint", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ewig schrein die Kinder;", "tokens": ["Und", "e\u00b7wig", "schrein", "die", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sp\u00e4t kommt er heim, er kost, er \u2014 schl\u00e4gt,", "tokens": ["Sp\u00e4t", "kommt", "er", "heim", ",", "er", "kost", ",", "er", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "PPER", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nachgiebig ", "tokens": ["Nach\u00b7gie\u00b7big"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Sie tr\u00e4gt\u2019s, wie nur die Liebe tr\u00e4gt,", "tokens": ["Sie", "tr\u00e4gt's", ",", "wie", "nur", "die", "Lie\u00b7be", "tr\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und alles ohne Liebe.", "tokens": ["Und", "al\u00b7les", "oh\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie w\u00fcnscht\u2019 sich oft: \u201ees w\u00e4r\u2019 vorbei\u201c,", "tokens": ["Sie", "w\u00fcnscht'", "sich", "oft", ":", "\u201e", "es", "w\u00e4r'", "vor\u00b7bei", "\u201c", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "$.", "$(", "PPER", "VAFIN", "ADV", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn nicht die Kinder w\u00e4ren;", "tokens": ["Wenn", "nicht", "die", "Kin\u00b7der", "w\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So aber sucht sie, stets auf\u2019s Neu,", "tokens": ["So", "a\u00b7ber", "sucht", "sie", ",", "stets", "auf's", "Neu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gatten zu bekehren;", "tokens": ["Den", "Gat\u00b7ten", "zu", "be\u00b7keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie schmeichelt ihm, und ob er dann", "tokens": ["Sie", "schmei\u00b7chelt", "ihm", ",", "und", "ob", "er", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auch kalt bei Seit\u2019 sie schiebe,", "tokens": ["Auch", "kalt", "bei", "Seit'", "sie", "schie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie nennt ihn: ihren ", "tokens": ["Sie", "nennt", "ihn", ":", "ih\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und alles ohne Liebe.", "tokens": ["Und", "al\u00b7les", "oh\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}