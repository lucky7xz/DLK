t": "Und da die Wahrheit wir verirrend fanden,", "tokens": ["Und", "da", "die", "Wahr\u00b7heit", "wir", "ver\u00b7ir\u00b7rend", "fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So sey'n vergessen jene Gr\u00e4uelthaten:", "tokens": ["So", "sey'n", "ver\u00b7ges\u00b7sen", "je\u00b7ne", "Gr\u00e4u\u00b7elt\u00b7ha\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es steht die Blume zwischen jungen Saaten.", "tokens": ["Es", "steht", "die", "Blu\u00b7me", "zwi\u00b7schen", "jun\u00b7gen", "Saa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Wenn auch der alte, hohe Baum verdorben,", "tokens": ["Wenn", "auch", "der", "al\u00b7te", ",", "ho\u00b7he", "Baum", "ver\u00b7dor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der eine Welt im Schatten konnte wahren,", "tokens": ["Der", "ei\u00b7ne", "Welt", "im", "Schat\u00b7ten", "konn\u00b7te", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn auch der Glanz von ehedem erstorben,", "tokens": ["Wenn", "auch", "der", "Glanz", "von", "e\u00b7he\u00b7dem", "ers\u00b7tor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerst\u00fcckt ein Reich, das trozte tausend Jahren,", "tokens": ["Zer\u00b7st\u00fcckt", "ein", "Reich", ",", "das", "troz\u00b7te", "tau\u00b7send", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "ADJA", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So ward daf\u00fcr ein geistiges erworben,", "tokens": ["So", "ward", "da\u00b7f\u00fcr", "ein", "geis\u00b7ti\u00b7ges", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PAV", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und immer sch\u00f6ner wird sich's offenbaren,", "tokens": ["Und", "im\u00b7mer", "sch\u00f6\u00b7ner", "wird", "sich's", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und fehlt ein Kaiser dieses Reiches Throne,", "tokens": ["Und", "fehlt", "ein", "Kai\u00b7ser", "die\u00b7ses", "Rei\u00b7ches", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So nimm von uns, die du verdienst, die Krone!", "tokens": ["So", "nimm", "von", "uns", ",", "die", "du", "ver\u00b7dienst", ",", "die", "Kro\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos":