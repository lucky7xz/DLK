{"dta.poem.8773": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Prolog an Goethe", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Zuerst geneigt, sie gr\u00fc\u00dfend aufgenommen,", "tokens": ["Zu\u00b7erst", "ge\u00b7neigt", ",", "sie", "gr\u00fc\u00b7\u00dfend", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du magst dich noch einmal an ihn gew\u00f6hnen,", "tokens": ["Du", "magst", "dich", "noch", "ein\u00b7mal", "an", "ihn", "ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Du siehst ihn wieder dir entgegen kommen,", "tokens": ["Du", "siehst", "ihn", "wie\u00b7der", "dir", "ent\u00b7ge\u00b7gen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit frohem Klang der Zeiten Drang versch\u00f6nen,", "tokens": ["Mit", "fro\u00b7hem", "Klang", "der", "Zei\u00b7ten", "Drang", "ver\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vielleicht von innerlichem Schmerz beklommen:", "tokens": ["Viel\u00b7leicht", "von", "in\u00b7ner\u00b7li\u00b7chem", "Schmerz", "be\u00b7klom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Viel mu\u00df ein solcher Geist von solchen Gaben,", "tokens": ["Viel", "mu\u00df", "ein", "sol\u00b7cher", "Geist", "von", "sol\u00b7chen", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "PIAT", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn er um Leichtsinn buhlt, gelitten haben.", "tokens": ["Wenn", "er", "um", "Leicht\u00b7sinn", "buhlt", ",", "ge\u00b7lit\u00b7ten", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Im Kampfe mu\u00df er sich entgegen wagen", "tokens": ["Im", "Kamp\u00b7fe", "mu\u00df", "er", "sich", "ent\u00b7ge\u00b7gen", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "PRF", "PTKVZ", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der eignen Liebe, wie dem fremden Hasse;", "tokens": ["Der", "eig\u00b7nen", "Lie\u00b7be", ",", "wie", "dem", "frem\u00b7den", "Has\u00b7se", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn einem Solchen Liebe zu versagen,", "tokens": ["Denn", "ei\u00b7nem", "Sol\u00b7chen", "Lie\u00b7be", "zu", "ver\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist eine Wollust f\u00fcr die stumpfe Masse,", "tokens": ["Ist", "ei\u00b7ne", "Wol\u00b7lust", "f\u00fcr", "die", "stump\u00b7fe", "Mas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Dies und Jenes wird herbeygetragen,", "tokens": ["Und", "Dies", "und", "Je\u00b7nes", "wird", "her\u00b7bey\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "KON", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df man ihn stets bey seiner Schw\u00e4che fasse,", "tokens": ["Da\u00df", "man", "ihn", "stets", "bey", "sei\u00b7ner", "Schw\u00e4\u00b7che", "fas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und fehlen ihm, so leiht man ihm Gebrechen,", "tokens": ["Und", "feh\u00b7len", "ihm", ",", "so", "leiht", "man", "ihm", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PIS", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihm, der zu gro\u00df ist, um zu widersprechen.", "tokens": ["Ihm", ",", "der", "zu", "gro\u00df", "ist", ",", "um", "zu", "wi\u00b7der\u00b7spre\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PTKA", "ADJD", "VAFIN", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das mochte Hafis wohl im Geist bedenken,", "tokens": ["Das", "moch\u00b7te", "Ha\u00b7fis", "wohl", "im", "Geist", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lie\u00df getrost des Lebens St\u00fcrme rollen:", "tokens": ["Und", "lie\u00df", "ge\u00b7trost", "des", "Le\u00b7bens", "St\u00fcr\u00b7me", "rol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn in Befriedigung wir uns versenken,", "tokens": ["Wenn", "in", "Be\u00b7frie\u00b7di\u00b7gung", "wir", "uns", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entgehn wir eigner Qual und fremdem Grollen:", "tokens": ["Ent\u00b7gehn", "wir", "eig\u00b7ner", "Qual", "und", "frem\u00b7dem", "Grol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Beym Wein im Becher, bey dem Ku\u00df des Schenken,", "tokens": ["Beym", "Wein", "im", "Be\u00b7cher", ",", "bey", "dem", "Ku\u00df", "des", "Schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bey Liedern, die melodisch ihm entquollen,", "tokens": ["Bey", "Lie\u00b7dern", ",", "die", "me\u00b7lo\u00b7disch", "ihm", "ent\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Empfand er stets im Herzen sich ges\u00fcnder,", "tokens": ["Emp\u00b7fand", "er", "stets", "im", "Her\u00b7zen", "sich", "ge\u00b7s\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wiewohl sie schrien: Es ist ein gro\u00dfer S\u00fcnder!", "tokens": ["Wie\u00b7wohl", "sie", "schri\u00b7en", ":", "Es", "ist", "ein", "gro\u00b7\u00dfer", "S\u00fcn\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Er schuf inde\u00df durch Bilder oder Spr\u00fcche", "tokens": ["Er", "schuf", "in\u00b7de\u00df", "durch", "Bil\u00b7der", "o\u00b7der", "Spr\u00fc\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Netz, worin die Herzen man erbeutet,", "tokens": ["Ein", "Netz", ",", "wo\u00b7rin", "die", "Her\u00b7zen", "man", "er\u00b7beu\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Gartenbeet erquickender Ger\u00fcche,", "tokens": ["Ein", "Gar\u00b7ten\u00b7beet", "er\u00b7qui\u00b7cken\u00b7der", "Ge\u00b7r\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem jede falsche Nessel ausgereutet,", "tokens": ["Dem", "je\u00b7de", "fal\u00b7sche", "Nes\u00b7sel", "aus\u00b7ge\u00b7reu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und einen Himmel ohne Wolkenbr\u00fcche,", "tokens": ["Und", "ei\u00b7nen", "Him\u00b7mel", "oh\u00b7ne", "Wol\u00b7ken\u00b7br\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo jeder Stern auf eine Blume deutet:", "tokens": ["Wo", "je\u00b7der", "Stern", "auf", "ei\u00b7ne", "Blu\u00b7me", "deu\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und so verglichest du dir ihn bescheiden,", "tokens": ["Und", "so", "ver\u00b7gli\u00b7chest", "du", "dir", "ihn", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In That und Sinn, im Streben und im Leiden.", "tokens": ["In", "That", "und", "Sinn", ",", "im", "Stre\u00b7ben", "und", "im", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was hast du nicht erlitten und erfahren!", "tokens": ["Was", "hast", "du", "nicht", "er\u00b7lit\u00b7ten", "und", "er\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie theuer mu\u00dftest du den Ruhm erkaufen!", "tokens": ["Wie", "theu\u00b7er", "mu\u00df\u00b7test", "du", "den", "Ruhm", "er\u00b7kau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verkannt von ferne hausenden Barbaren,", "tokens": ["Ver\u00b7kannt", "von", "fer\u00b7ne", "hau\u00b7sen\u00b7den", "Bar\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Schwarm der Gecken l\u00e4stig \u00fcberlaufen,", "tokens": ["Vom", "Schwarm", "der", "Ge\u00b7cken", "l\u00e4s\u00b7tig", "\u00fc\u00b7berl\u00b7au\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Uebelwollenden zu ganzen Schaaren,", "tokens": ["Die", "Ue\u00b7bel\u00b7wol\u00b7len\u00b7den", "zu", "gan\u00b7zen", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Mi\u00dfverstehenden zu ganzen Haufen,", "tokens": ["Die", "Mi\u00df\u00b7ver\u00b7ste\u00b7hen\u00b7den", "zu", "gan\u00b7zen", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wenn ich alles insgesammt erw\u00e4hne,", "tokens": ["Und", "wenn", "ich", "al\u00b7les", "ins\u00b7ge\u00b7sammt", "er\u00b7w\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Krittler freche, wenn auch stumpfe Z\u00e4hne.", "tokens": ["Der", "Kritt\u00b7ler", "fre\u00b7che", ",", "wenn", "auch", "stump\u00b7fe", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "KOUS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und wie du sonst, in jugendlichen Tagen,", "tokens": ["Und", "wie", "du", "sonst", ",", "in", "ju\u00b7gend\u00b7li\u00b7chen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie reich besch\u00fcttet hast mit Bl\u00fcthenflocken,", "tokens": ["Sie", "reich", "be\u00b7sch\u00fct\u00b7tet", "hast", "mit", "Bl\u00fc\u00b7then\u00b7flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sie, zu feig die sch\u00f6ne Last zu tragen,", "tokens": ["Und", "sie", ",", "zu", "feig", "die", "sch\u00f6\u00b7ne", "Last", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PTKA", "ADJD", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich zeigten neidisch halb und halb erschrocken:", "tokens": ["Sich", "zeig\u00b7ten", "nei\u00b7disch", "halb", "und", "halb", "er\u00b7schro\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADJD", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So sehn wir jezt sie noch hervor sich wagen,", "tokens": ["So", "sehn", "wir", "jezt", "sie", "noch", "her\u00b7vor", "sich", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "ADV", "PTKVZ", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um Schmach zu bieten deinen Silberlocken;", "tokens": ["Um", "Schmach", "zu", "bie\u00b7ten", "dei\u00b7nen", "Sil\u00b7berl\u00b7o\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch dies Geschlecht vermag dich nicht zu hemmen,", "tokens": ["Doch", "dies", "Ge\u00b7schlecht", "ver\u00b7mag", "dich", "nicht", "zu", "hem\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es mu\u00df die Welt sich dir entgegenstemmen.", "tokens": ["Es", "mu\u00df", "die", "Welt", "sich", "dir", "ent\u00b7ge\u00b7gens\u00b7tem\u00b7men", "."], "token_info"