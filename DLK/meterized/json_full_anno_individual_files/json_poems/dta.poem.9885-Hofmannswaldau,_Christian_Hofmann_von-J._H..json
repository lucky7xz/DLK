{"dta.poem.9885": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "J. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will itzt meinen geist nicht auf der erden/", "tokens": ["Ich", "will", "itzt", "mei\u00b7nen", "geist", "nicht", "auf", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil seine fl\u00fcgel nach den sternen ziehn/", "tokens": ["Weil", "sei\u00b7ne", "fl\u00fc\u00b7gel", "nach", "den", "ster\u00b7nen", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was sterne liebt/ kan zwar nicht sternen werden/", "tokens": ["Was", "ster\u00b7ne", "liebt", "/", "kan", "zwar", "nicht", "ster\u00b7nen", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$(", "VMFIN", "ADV", "PTKNEG", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil nesseln nicht so wie die rosen bl\u00fchn:", "tokens": ["Weil", "nes\u00b7seln", "nicht", "so", "wie", "die", "ro\u00b7sen", "bl\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKNEG", "ADV", "KOKOM", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch kan dein hoher sinn mich/ Flora/ tragen", "tokens": ["Doch", "kan", "dein", "ho\u00b7her", "sinn", "mich", "/", "Flo\u00b7ra", "/", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "$(", "NE", "$(", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dahin/ wo mir das gl\u00fcck sonst will absagen.", "tokens": ["Da\u00b7hin", "/", "wo", "mir", "das", "gl\u00fcck", "sonst", "will", "ab\u00b7sa\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "PWAV", "PPER", "ART", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Magnet zieht eisen an/ und du mein hertze", "tokens": ["Mag\u00b7net", "zieht", "ei\u00b7sen", "an", "/", "und", "du", "mein", "hert\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$(", "KON", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich leg es dir zu treuen h\u00e4nden hin;", "tokens": ["Ich", "leg", "es", "dir", "zu", "treu\u00b7en", "h\u00e4n\u00b7den", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denck nicht da\u00df ich mit bunten farben schertze/", "tokens": ["Denck", "nicht", "da\u00df", "ich", "mit", "bun\u00b7ten", "far\u00b7ben", "schert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du siehest mein gesicht/ doch nicht den sinn:", "tokens": ["Du", "sie\u00b7hest", "mein", "ge\u00b7sicht", "/", "doch", "nicht", "den", "sinn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVPP", "$(", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn m\u00f6gligkeit mir will ihn lassen mahlen/", "tokens": ["Wenn", "m\u00f6g\u00b7lig\u00b7keit", "mir", "will", "ihn", "las\u00b7sen", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VMFIN", "PPER", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So wirst du liebe auch mit liebe zahlen.", "tokens": ["So", "wirst", "du", "lie\u00b7be", "auch", "mit", "lie\u00b7be", "zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schau ich dich g\u00f6ttin an und dein gesichte/", "tokens": ["Schau", "ich", "dich", "g\u00f6t\u00b7tin", "an", "und", "dein", "ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "NE", "PTKVZ", "KON", "PPOSAT", "ADJA", "$("], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "So seh ich mehr als klar des himmels zier/", "tokens": ["So", "seh", "ich", "mehr", "als", "klar", "des", "him\u00b7mels", "zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "KOKOM", "ADJD", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie deiner augen paar den glantz vernichte/", "tokens": ["Wie", "dei\u00b7ner", "au\u00b7gen", "paar", "den", "glantz", "ver\u00b7nich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PIAT", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den sonn und monde brauchet f\u00fcr und f\u00fcr;", "tokens": ["Den", "sonn", "und", "mon\u00b7de", "brau\u00b7chet", "f\u00fcr", "und", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wie du holtz und berg und thal entz\u00fcndest/", "tokens": ["Und", "wie", "du", "holtz", "und", "berg", "und", "thal", "ent\u00b7z\u00fcn\u00b7dest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "KON", "NE", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo du/ was zunder nur und schwefel/ findest.", "tokens": ["Wo", "du", "/", "was", "zun\u00b7der", "nur", "und", "schwe\u00b7fel", "/", "fin\u00b7dest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "$(", "PWS", "ADV", "ADV", "KON", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Seh ich die weisse hand und deine glieder/", "tokens": ["Seh", "ich", "die", "weis\u00b7se", "hand", "und", "dei\u00b7ne", "glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So mu\u00df der alabast zu grabe gehn.", "tokens": ["So", "mu\u00df", "der", "a\u00b7la\u00b7bast", "zu", "gra\u00b7be", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der wangen liebligkeit giebt mir kaum wieder/", "tokens": ["Der", "wan\u00b7gen", "lieb\u00b7lig\u00b7keit", "giebt", "mir", "kaum", "wie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Da\u00df ich vermag auf meinem fu\u00df zu stehn;", "tokens": ["Da\u00df", "ich", "ver\u00b7mag", "auf", "mei\u00b7nem", "fu\u00df", "zu", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PPOSAT", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Weil milch und purpur hier verm\u00e4hlet lieget/", "tokens": ["Weil", "milch", "und", "pur\u00b7pur", "hier", "ver\u00b7m\u00e4h\u00b7let", "lie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "ADJD", "ADV", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wei\u00df ich/ da\u00df deine pracht zur sonnen flieget.", "tokens": ["Wei\u00df", "ich", "/", "da\u00df", "dei\u00b7ne", "pracht", "zur", "son\u00b7nen", "flie\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dein sch\u00f6ner mund sticht weit corallen abe/", "tokens": ["Dein", "sch\u00f6\u00b7ner", "mund", "sticht", "weit", "co\u00b7ral\u00b7len", "a\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rubin der siehet ihn als mutter an/", "tokens": ["Ru\u00b7bin", "der", "sie\u00b7het", "ihn", "als", "mut\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "VVFIN", "PPER", "KOUS", "NN", "PTKVZ", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Es stirbet deine pracht auf seinem grabe/", "tokens": ["Es", "stir\u00b7bet", "dei\u00b7ne", "pracht", "auf", "sei\u00b7nem", "gra\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil dein mund selbsten sich nicht k\u00fcssen kan;", "tokens": ["Weil", "dein", "mund", "selbs\u00b7ten", "sich", "nicht", "k\u00fcs\u00b7sen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erweichst du deinen sinn und dein gem\u00fcthe/", "tokens": ["Er\u00b7weichst", "du", "dei\u00b7nen", "sinn", "und", "dein", "ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So seh ich meinen wunsch in sch\u00f6nster bl\u00fcthe.", "tokens": ["So", "seh", "ich", "mei\u00b7nen", "wunsch", "in", "sch\u00f6ns\u00b7ter", "bl\u00fc\u00b7the", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADJD", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Was sind die br\u00fcste? ambrosiner kuchen/", "tokens": ["Was", "sind", "die", "br\u00fcs\u00b7te", "?", "am\u00b7bro\u00b7si\u00b7ner", "ku\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "$.", "NN", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die doch vor dich nicht selbst gebacken seyn;", "tokens": ["Die", "doch", "vor", "dich", "nicht", "selbst", "ge\u00b7ba\u00b7cken", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie w\u00fcrden mit der zeit dir selber fluchen/", "tokens": ["Sie", "w\u00fcr\u00b7den", "mit", "der", "zeit", "dir", "sel\u00b7ber", "flu\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum lade einen gast zu diesen ein/", "tokens": ["Drum", "la\u00b7de", "ei\u00b7nen", "gast", "zu", "die\u00b7sen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADV", "APPR", "PDAT", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "D\u00f6rfft ich mit einem blick dieselben r\u00fchren/", "tokens": ["D\u00f6rfft", "ich", "mit", "ei\u00b7nem", "blick", "die\u00b7sel\u00b7ben", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "PDAT", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So w\u00fcrdest du mich aus mir selber f\u00fchren.", "tokens": ["So", "w\u00fcr\u00b7dest", "du", "mich", "aus", "mir", "sel\u00b7ber", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Di\u00df/ was ich hier gesagt/ ist wohl zu r\u00fchmen/", "tokens": ["Di\u00df", "/", "was", "ich", "hier", "ge\u00b7sagt", "/", "ist", "wohl", "zu", "r\u00fch\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "PWS", "PPER", "ADV", "VVPP", "$(", "VAFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch mehr dein hoher geist der in dir lebt;", "tokens": ["Doch", "mehr", "dein", "ho\u00b7her", "geist", "der", "in", "dir", "lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "ART", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die tugend will dir stets dein haar bebl\u00fcmen/", "tokens": ["Die", "tu\u00b7gend", "will", "dir", "stets", "dein", "haar", "be\u00b7bl\u00fc\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sie vom sch\u00f6nsten golde hat gewebt;", "tokens": ["Die", "sie", "vom", "sch\u00f6ns\u00b7ten", "gol\u00b7de", "hat", "ge\u00b7webt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie mu\u00df dir immerfort zu dienste stehen/", "tokens": ["Sie", "mu\u00df", "dir", "im\u00b7mer\u00b7fort", "zu", "diens\u00b7te", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn du kanst anders nicht als mit ihr gehen.", "tokens": ["Denn", "du", "kanst", "an\u00b7ders", "nicht", "als", "mit", "ihr", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PTKNEG", "KOKOM", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ich opffre meinen sinn dir/ Flora/ eigen/", "tokens": ["Ich", "opff\u00b7re", "mei\u00b7nen", "sinn", "dir", "/", "Flo\u00b7ra", "/", "ei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "$(", "NE", "$(", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nim ihn auch/ wie du wilt/ zu diensten hin/", "tokens": ["Nim", "ihn", "auch", "/", "wie", "du", "wilt", "/", "zu", "diens\u00b7ten", "hin", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "$(", "PWAV", "PPER", "VMFIN", "$(", "PTKZU", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es soll das tieffe meer eh gantz verseigen/", "tokens": ["Es", "soll", "das", "tief\u00b7fe", "meer", "eh", "gantz", "ver\u00b7sei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word"