{"dta.poem.8532": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Geplagt .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weh dem Knaben, der zwei Herrinnen hat!", "tokens": ["Weh", "dem", "Kna\u00b7ben", ",", "der", "zwei", "Her\u00b7rin\u00b7nen", "hat", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "CARD", "NN", "VAFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Verloren ist er, verloren!", "tokens": ["Ver\u00b7lo\u00b7ren", "ist", "er", ",", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ruft die Stimme und ruft sie dort:", "tokens": ["Ruft", "die", "Stim\u00b7me", "und", "ruft", "sie", "dort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "\u201ekomm, binde mir die Sandalen!", "tokens": ["\u201e", "komm", ",", "bin\u00b7de", "mir", "die", "San\u00b7da\u00b7len", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Gib den Schleier; \u2014 nun eile fort,", "tokens": ["Gib", "den", "Schlei\u00b7er", ";", "nun", "ei\u00b7le", "fort", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$.", "$(", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vom Markte Narde zu holen!\u201c", "tokens": ["Vom", "Mark\u00b7te", "Nar\u00b7de", "zu", "ho\u00b7len", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Durch die Menge irrt er umher", "tokens": ["Durch", "die", "Men\u00b7ge", "irrt", "er", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wie ein armer verscheuchter Vogel,", "tokens": ["Wie", "ein", "ar\u00b7mer", "ver\u00b7scheuch\u00b7ter", "Vo\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Wie ein armes zerrissnes Gewand,", "tokens": ["Wie", "ein", "ar\u00b7mes", "zer\u00b7riss\u00b7nes", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Geflickt von tausend H\u00e4nden.", "tokens": ["Ge\u00b7flickt", "von", "tau\u00b7send", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wehe dem Knaben, der zwei Herrinnen hat!", "tokens": ["We\u00b7he", "dem", "Kna\u00b7ben", ",", "der", "zwei", "Her\u00b7rin\u00b7nen", "hat", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "CARD", "NN", "VAFIN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Verloren ist er, verloren!", "tokens": ["Ver\u00b7lo\u00b7ren", "ist", "er", ",", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}