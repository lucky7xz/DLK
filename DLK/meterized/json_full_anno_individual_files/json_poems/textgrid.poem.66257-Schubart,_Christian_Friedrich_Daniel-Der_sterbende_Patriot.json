{"textgrid.poem.66257": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Der sterbende Patriot", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Todtengr\u00e4ber, schaufle mir ein Grab.", "tokens": ["Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", ",", "schauf\u00b7le", "mir", "ein", "Grab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Immer tiefer", "tokens": ["Im\u00b7mer", "tie\u00b7fer"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Sinkt mein liebes Vaterland hinab.", "tokens": ["Sinkt", "mein", "lie\u00b7bes", "Va\u00b7ter\u00b7land", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Todtengr\u00e4ber, schaufle mir ein Grab.", "tokens": ["Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", ",", "schauf\u00b7le", "mir", "ein", "Grab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "In den alten Eichenw\u00e4ldern stand", "tokens": ["In", "den", "al\u00b7ten", "Ei\u00b7chen\u00b7w\u00e4l\u00b7dern", "stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Einst die ", "tokens": ["Einst", "die"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "S