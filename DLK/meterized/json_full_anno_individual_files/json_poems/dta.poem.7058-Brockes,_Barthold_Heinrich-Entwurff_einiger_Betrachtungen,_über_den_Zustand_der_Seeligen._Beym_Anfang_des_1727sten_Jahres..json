{"dta.poem.7058": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bevor wir nun ins Heilige zu gehen", "tokens": ["Be\u00b7vor", "wir", "nun", "ins", "Hei\u00b7li\u00b7ge", "zu", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Uns unterstehen;", "tokens": ["Uns", "un\u00b7ter\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "So m\u00fcssen wir vorher die irrigen Jdeen,", "tokens": ["So", "m\u00fcs\u00b7sen", "wir", "vor\u00b7her", "die", "ir\u00b7ri\u00b7gen", "Jdeen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Von allem fleischlichen, und eitelen Ergetzen,", "tokens": ["Von", "al\u00b7lem", "fleischli\u00b7chen", ",", "und", "ei\u00b7te\u00b7len", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie etwan Mahomet gehabt, bey Seite setzen.", "tokens": ["Wie", "et\u00b7wan", "Ma\u00b7ho\u00b7met", "ge\u00b7habt", ",", "bey", "Sei\u00b7te", "set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "VAPP", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}}}}