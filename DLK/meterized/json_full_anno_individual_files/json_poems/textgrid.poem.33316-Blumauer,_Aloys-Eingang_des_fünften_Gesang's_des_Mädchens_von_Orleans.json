{"textgrid.poem.33316": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Eingang des f\u00fcnften Gesang's des M\u00e4dchens von Orleans", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Freunde! fangen wir ein christlich Leben an!", "tokens": ["O", "Freun\u00b7de", "!", "fan\u00b7gen", "wir", "ein", "christ\u00b7lich", "Le\u00b7ben", "an", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir k\u00f6nnen zu nichts Kl\u00fcger'm uns entschliessen;", "tokens": ["Wir", "k\u00f6n\u00b7nen", "zu", "nichts", "Kl\u00fc\u00b7ger'm", "uns", "ent\u00b7schlies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIS", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fr\u00fch oder sp\u00e4t wird's doch geschehen m\u00fcssen.", "tokens": ["Fr\u00fch", "o\u00b7der", "sp\u00e4t", "wird's", "doch", "ge\u00b7sche\u00b7hen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "ADV", "VVINF", "VMINF", "$."], "meter": "+--++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ich selbst hing einst den lockern Burschen an,", "tokens": ["Ich", "selbst", "hing", "einst", "den", "lo\u00b7ckern", "Bur\u00b7schen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die kein Gesetz als ihre L\u00fcste kannten,", "tokens": ["Die", "kein", "Ge\u00b7setz", "als", "ih\u00b7re", "L\u00fcs\u00b7te", "kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KOKOM", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Oft auf den Ball, und nie zur Messe rannten,", "tokens": ["Oft", "auf", "den", "Ball", ",", "und", "nie", "zur", "Mes\u00b7se", "rann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KON", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die, ach; get\u00e4uscht vom jugendlichen Wahn,", "tokens": ["Die", ",", "ach", ";", "ge\u00b7t\u00e4uscht", "vom", "ju\u00b7gend\u00b7li\u00b7chen", "Wahn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ITJ", "$.", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nur Gasterei'n und Freudenm\u00e4dchen liebten,", "tokens": ["Nur", "Gas\u00b7te\u00b7rei'n", "und", "Freu\u00b7den\u00b7m\u00e4d\u00b7chen", "lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und ihren Witz an Gottesdienern \u00fcbten.", "tokens": ["Und", "ih\u00b7ren", "Witz", "an", "Got\u00b7tes\u00b7die\u00b7nern", "\u00fcb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Doch was geschieht? Der b\u00f6se Knochenmann", "tokens": ["Doch", "was", "ge\u00b7schieht", "?", "Der", "b\u00f6\u00b7se", "Kno\u00b7chen\u00b7mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mit hohler Nas' und f\u00fcrchterlicher Hiype", "tokens": ["Mit", "hoh\u00b7ler", "Nas'", "und", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "Hiy\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Schlie\u00dft unsern Witzlingen die Lippe:", "tokens": ["Schlie\u00dft", "un\u00b7sern", "Witz\u00b7lin\u00b7gen", "die", "Lip\u00b7pe", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Ein hitzig Fieber, an dem Styr erzeugt,", "tokens": ["Ein", "hit\u00b7zig", "Fie\u00b7ber", ",", "an", "dem", "Styr", "er\u00b7zeugt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Von Atropos", "tokens": ["Von", "A\u00b7tro\u00b7pos"], "token_info": ["word", "word"], "pos": ["APPR", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Verr\u00fccket nun ihr Hirnchen. Gegenw\u00e4rtig", "tokens": ["Ver\u00b7r\u00fc\u00b7cket", "nun", "ihr", "Hirn\u00b7chen", ".", "Ge\u00b7gen\u00b7w\u00e4r\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Sind Priester und Notar; die W\u00e4rterin", "tokens": ["Sind", "Pries\u00b7ter", "und", "No\u00b7tar", ";", "die", "W\u00e4r\u00b7te\u00b7rin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Fr\u00e4gt ungescheut: Herr, sind sie reisefertig?", "tokens": ["Fr\u00e4gt", "un\u00b7ge\u00b7scheut", ":", "Herr", ",", "sind", "sie", "rei\u00b7se\u00b7fer\u00b7tig", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "NN", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wo wollen sie mit ihrem Leichnam hin? \u2013", "tokens": ["Wo", "wol\u00b7len", "sie", "mit", "ih\u00b7rem", "Leich\u00b7nam", "hin", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Nun kommt den Herr'n die Reue ungebeten,", "tokens": ["Nun", "kommt", "den", "Herr'n", "die", "Reu\u00b7e", "un\u00b7ge\u00b7be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Obgleich zu sp\u00e4t; der in den Todesn\u00f6then", "tokens": ["Ob\u00b7gleich", "zu", "sp\u00e4t", ";", "der", "in", "den", "To\u00b7des\u00b7n\u00f6\u00b7then"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PTKA", "ADJD", "$.", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Fri\u00dft Lukaszettel, trinkt Walburgis Oel,", "tokens": ["Fri\u00dft", "Lu\u00b7kas\u00b7zet\u00b7tel", ",", "trinkt", "Wal\u00b7bur\u00b7gis", "O\u00b7el", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.22": {"text": "Und der verlobt sich nach Maria Zell.", "tokens": ["Und", "der", "ver\u00b7lobt", "sich", "nach", "Ma\u00b7ria", "Zell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PRF", "APPR", "NE", "NE", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Man betet, badet im geweihtem Thaue", "tokens": ["Man", "be\u00b7tet", ",", "ba\u00b7det", "im", "ge\u00b7weih\u00b7tem", "Thau\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.24": {"text": "Den Kranken, psalmodirt und pl\u00e4rrt Latein;", "tokens": ["Den", "Kran\u00b7ken", ",", "psal\u00b7mo\u00b7dirt", "und", "pl\u00e4rrt", "La\u00b7tein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Allein, umsonst: schon harrt mit off'ner Klaue", "tokens": ["Al\u00b7lein", ",", "um\u00b7sonst", ":", "schon", "harrt", "mit", "off'\u00b7ner", "Klau\u00b7e"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Am Fu\u00df des Betts der b\u00f6se Satan sein.", "tokens": ["Am", "Fu\u00df", "des", "Betts", "der", "b\u00f6\u00b7se", "Sa\u00b7tan", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und wie das Seelchen d