{"textgrid.poem.66321": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es starb einmal ein B\u00e4uerlein;", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es starb einmal ein B\u00e4uerlein;", "tokens": ["Es", "starb", "ein\u00b7mal", "ein", "B\u00e4u\u00b7er\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Engel, hell, wie Sonnenschein,", "tokens": ["Sein", "En\u00b7gel", ",", "hell", ",", "wie", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einem g\u00fcldnen Stabe wies", "tokens": ["Mit", "ei\u00b7nem", "g\u00fcld\u00b7nen", "Sta\u00b7be", "wies"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies B\u00e4uerlein zum Paradies.", "tokens": ["Dies", "B\u00e4u\u00b7er\u00b7lein", "zum", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ging an den bestimmten Ort", "tokens": ["Es", "ging", "an", "den", "be\u00b7stimm\u00b7ten", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf einer Morgenr\u00f6the fort,", "tokens": ["Auf", "ei\u00b7ner", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kam an das Thor von Diamant", "tokens": ["Kam", "an", "das", "Thor", "von", "Di\u00b7a\u00b7mant"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und klopfte sittsam mit der Hand.", "tokens": ["Und", "klopf\u00b7te", "sitt\u00b7sam", "mit", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "St. ", "tokens": ["St."], "token_info": ["abbreviation"], "pos": ["NE"]}, "line.2": {"text": "Und schrie: \u00bbNun, wer ist wieder hier?\u00ab", "tokens": ["Und", "schrie", ":", "\u00bb", "Nun", ",", "wer", "ist", "wie\u00b7der", "hier", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "$,", "PWS", "VAFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbich bin ein armer Bauersmann,", "tokens": ["\u00bb", "ich", "bin", "ein", "ar\u00b7mer", "Bau\u00b7ers\u00b7mann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der auf der Erde nichts gethan,", "tokens": ["Der", "auf", "der", "Er\u00b7de", "nichts", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als seine Felder angebaut,", "tokens": ["Als", "sei\u00b7ne", "Fel\u00b7der", "an\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einem Weibe sich getraut,", "tokens": ["Mit", "ei\u00b7nem", "Wei\u00b7be", "sich", "ge\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die mir zum Stecken und zum Stab", "tokens": ["Die", "mir", "zum", "Ste\u00b7cken", "und", "zum", "Stab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Dutzend derbe Buben gab.", "tokens": ["Ein", "Dut\u00b7zend", "der\u00b7be", "Bu\u00b7ben", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In meinem Leben gab ich gern", "tokens": ["In", "mei\u00b7nem", "Le\u00b7ben", "gab", "ich", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Steuern meinem gn\u00e4d'gen Herrn;", "tokens": ["Die", "Steu\u00b7ern", "mei\u00b7nem", "gn\u00e4d'\u00b7gen", "Herrn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich glaubte, was der Pfarrer sprach,", "tokens": ["Ich", "glaub\u00b7te", ",", "was", "der", "Pfar\u00b7rer", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kam treulich seinen Lehren nach", "tokens": ["Kam", "treu\u00b7lich", "sei\u00b7nen", "Leh\u00b7ren", "nach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und zahlt' ihn redlich, wie mich d\u00e4ucht,", "tokens": ["Und", "zahlt'", "ihn", "red\u00b7lich", ",", "wie", "mich", "d\u00e4ucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "F\u00fcr seine Predigt, Bet' und Beicht.", "tokens": ["F\u00fcr", "sei\u00b7ne", "Pre\u00b7digt", ",", "Bet'", "und", "Beicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich starb. Er salbte mich mit Oel;", "tokens": ["Ich", "starb", ".", "Er", "salb\u00b7te", "mich", "mit", "O\u00b7el", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ein Engelein wies meine Seel'", "tokens": ["Ein", "En\u00b7ge\u00b7lein", "wies", "mei\u00b7ne", "Seel'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Zu dir ins Paradies herauf:", "tokens": ["Zu", "dir", "ins", "Pa\u00b7ra\u00b7dies", "her\u00b7auf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "O heil'ger ", "tokens": ["O", "heil'\u00b7ger"], "token_info": ["word", "word"], "pos": ["NE", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Nun \u00f6ffnete die Pforte sich,", "tokens": ["Nun", "\u00f6ff\u00b7ne\u00b7te", "die", "Pfor\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "St. ", "tokens": ["St."], "token_info": ["abbreviation"], "pos": ["NE"]}, "line.3": {"text": "Du guter Mann verdienst gewi\u00df", "tokens": ["Du", "gu\u00b7ter", "Mann", "ver\u00b7dienst", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Pl\u00e4tzchen in dem Paradies.", "tokens": ["Ein", "Pl\u00e4tz\u00b7chen", "in", "dem", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du sollst's auch haben: aber heut,", "tokens": ["Du", "sollst's", "auch", "ha\u00b7ben", ":", "a\u00b7ber", "heut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VAFIN", "$.", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein B\u00e4uerlein, fehlt mir die Zeit.", "tokens": ["Mein", "B\u00e4u\u00b7er\u00b7lein", ",", "fehlt", "mir", "die", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Wir feiern heut ein gro\u00dfes Fest,", "tokens": ["Wir", "fei\u00b7ern", "heut", "ein", "gro\u00b7\u00dfes", "Fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das mich an dich nicht denken l\u00e4\u00dft.", "tokens": ["Das", "mich", "an", "dich", "nicht", "den\u00b7ken", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PPER", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Geh dort in jene Laube hin,", "tokens": ["Geh", "dort", "in", "je\u00b7ne", "Lau\u00b7be", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gew\u00f6lbt von himmlischem Jasmin,", "tokens": ["Ge\u00b7w\u00f6lbt", "von", "himm\u00b7li\u00b7schem", "Jas\u00b7min", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und warte, bis ich komme, da,", "tokens": ["Und", "war\u00b7te", ",", "bis", "ich", "kom\u00b7me", ",", "da", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Beim Nektar und Ambrosia!\u00ab", "tokens": ["Beim", "Nek\u00b7tar", "und", "A\u00b7mbro\u00b7sia", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "KON", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Das B\u00e4uerlein sprach: Habe Dank!", "tokens": ["Das", "B\u00e4u\u00b7er\u00b7lein", "sprach", ":", "Ha\u00b7be", "Dank", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Setzt' sich auf eine Veilchenbank,", "tokens": ["Setzt'", "sich", "auf", "ei\u00b7ne", "Veil\u00b7chen\u00b7bank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wartete, bis ", "tokens": ["Und", "war\u00b7te\u00b7te", ",", "bis"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Erhabne Stille herrschte tief.", "tokens": ["Er\u00b7hab\u00b7ne", "Stil\u00b7le", "herrschte", "tief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Doch pl\u00f6tzlich sprang das goldne Thor,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "sprang", "das", "gold\u00b7ne", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ganze Himmel war ", "tokens": ["Der", "gan\u00b7ze", "Him\u00b7mel", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es schwammen s\u00fc\u00dfe Symphonie'n", "tokens": ["Es", "schwam\u00b7men", "s\u00fc\u00b7\u00dfe", "Sym\u00b7pho\u00b7nie'n"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch den entz\u00fcckten Himmel hin;", "tokens": ["Durch", "den", "ent\u00b7z\u00fcck\u00b7ten", "Him\u00b7mel", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Schatten eines Priesters schwebt'", "tokens": ["Der", "Schat\u00b7ten", "ei\u00b7nes", "Pries\u00b7ters", "schwebt'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Herauf, vom Lobesang erbebt'", "tokens": ["Her\u00b7auf", ",", "vom", "Lo\u00b7be\u00b7sang", "er\u00b7bebt'"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKVZ", "$,", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Himmel: \u00bb", "tokens": ["Der", "Him\u00b7mel", ":", "\u00bb"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.8": {"line.1": {"text": "Mit Abraham und Isaak sa\u00df", "tokens": ["Mit", "Ab\u00b7ra\u00b7ham", "und", "I\u00b7saak", "sa\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Selige zu Tisch und a\u00df", "tokens": ["Der", "Se\u00b7li\u00b7ge", "zu", "Tisch", "und", "a\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das erstemal Ambrosia;", "tokens": ["Das", "ers\u00b7te\u00b7mal", "A\u00b7mbro\u00b7sia", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADV", "NE", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Sang laut der Seraphimen Chor", "tokens": ["Sang", "laut", "der", "Se\u00b7ra\u00b7phi\u00b7men", "Chor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um des entz\u00fcckten Priesters Ohr.", "tokens": ["Um", "des", "ent\u00b7z\u00fcck\u00b7ten", "Pries\u00b7ters", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und erst am Himmelsabend kam", "tokens": ["Und", "erst", "am", "Him\u00b7mel\u00b7sa\u00b7bend", "kam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "St. ", "tokens": ["St."], "token_info": ["abbreviation"], "pos": ["NE"]}, "line.9": {"text": "Mit sich den armen Bauersmann,", "tokens": ["Mit", "sich", "den", "ar\u00b7men", "Bau\u00b7ers\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und wies ihm auch sein Pl\u00e4tzchen an.", "tokens": ["Und", "wies", "ihm", "auch", "sein", "Pl\u00e4tz\u00b7chen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Bauer fa\u00dfte wieder Muth", "tokens": ["Der", "Bau\u00b7er", "fa\u00df\u00b7te", "wie\u00b7der", "Muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprach: \u00bbHerr ", "tokens": ["Und", "sprach", ":", "\u00bb",