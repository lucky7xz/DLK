{"dta.poem.6003": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "da:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So kommt uns unsre Kugel hier,", "tokens": ["So", "kommt", "uns", "uns\u00b7re", "Ku\u00b7gel", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Erd und Fluht gemacht, nicht anders f\u00fcr,", "tokens": ["Aus", "Erd", "und", "Fluht", "ge\u00b7macht", ",", "nicht", "an\u00b7ders", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "PTKNEG", "ADV", "APPR", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als w\u00e4re sie in der so grossen Welt", "tokens": ["Als", "w\u00e4\u00b7re", "sie", "in", "der", "so", "gros\u00b7sen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur wie ein Mittel-Punct gestellt.", "tokens": ["Nur", "wie", "ein", "Mit\u00b7tel\u00b7Punct", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Es blendet uns auch hier der Schein;", "tokens": ["Es", "blen\u00b7det", "uns", "auch", "hier", "der", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn wir aufs neue nur ergr\u00fcnden,", "tokens": ["Wenn", "wir", "aufs", "neu\u00b7e", "nur", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Ordnung Trefflichkeit, so unsern Geist", "tokens": ["Der", "Ord\u00b7nung", "Treff\u00b7lich\u00b7keit", ",", "so", "un\u00b7sern", "Geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die unermessliche Machine weist;", "tokens": ["Die", "un\u00b7er\u00b7mess\u00b7li\u00b7che", "Ma\u00b7chi\u00b7ne", "weist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So werden wir gar bald den Jrrthum finden.", "tokens": ["So", "wer\u00b7den", "wir", "gar", "bald", "den", "Jrr\u00b7thum", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}