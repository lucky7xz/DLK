{"dta.poem.16217": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "271.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor allen Thieren, die dem Menschen \u00e4hnlich scheinen,", "tokens": ["Vor", "al\u00b7len", "Thie\u00b7ren", ",", "die", "dem", "Men\u00b7schen", "\u00e4hn\u00b7lich", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat dis der Mensch voraus, zu l\u00e4cheln und zu weinen.", "tokens": ["Hat", "dis", "der", "Mensch", "vo\u00b7raus", ",", "zu", "l\u00e4\u00b7cheln", "und", "zu", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "PTKVZ", "$,", "PTKZU", "VVFIN", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch L\u00e4cheln suchet er und Weinen \u00fcbers Thier", "tokens": ["Durch", "L\u00e4\u00b7cheln", "su\u00b7chet", "er", "und", "Wei\u00b7nen", "\u00fc\u00b7bers", "Thier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hin\u00fcber, o Natur, den Weg zur\u00fcck zu dir.", "tokens": ["Hin\u00b7\u00fc\u00b7ber", ",", "o", "Na\u00b7tur", ",", "den", "Weg", "zu\u00b7r\u00fcck", "zu", "dir", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$,", "ART", "NN", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn deine Blume auch, sie l\u00e4chelt und sie weint,", "tokens": ["Denn", "dei\u00b7ne", "Blu\u00b7me", "auch", ",", "sie", "l\u00e4\u00b7chelt", "und", "sie", "weint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$,", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie dein Thau benetzt, wenn sie dein Licht bescheint.", "tokens": ["Wenn", "sie", "dein", "Thau", "be\u00b7netzt", ",", "wenn", "sie", "dein", "Licht", "be\u00b7scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dein Weinen das Gew\u00f6lk, dein L\u00e4cheln ist die Sonne,", "tokens": ["Dein", "Wei\u00b7nen", "das", "Ge\u00b7w\u00f6lk", ",", "dein", "L\u00e4\u00b7cheln", "ist", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein L\u00e4chelweinen ist wie unsres Wehmuthswonne.", "tokens": ["Dein", "L\u00e4\u00b7chel\u00b7wei\u00b7nen", "ist", "wie", "uns\u00b7res", "Weh\u00b7muths\u00b7won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du, weil wir weinen, weinst: wir l\u00e4cheln, weil du lachst;", "tokens": ["Du", ",", "weil", "wir", "wei\u00b7nen", ",", "weinst", ":", "wir", "l\u00e4\u00b7cheln", ",", "weil", "du", "lachst", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "PPER", "VVINF", "$,", "VVFIN", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir machen vor und nach dir alles, wie du's machst.", "tokens": ["Wir", "ma\u00b7chen", "vor", "und", "nach", "dir", "al\u00b7les", ",", "wie", "du's", "machst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "APPR", "PPER", "PIS", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}