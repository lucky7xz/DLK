{"textgrid.poem.41302": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrift des Herrn Sextils", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ruht der Herr Sextil, das Bild erfahrner M\u00e4nner,", "tokens": ["Hier", "ruht", "der", "Herr", "Sex\u00b7til", ",", "das", "Bild", "er\u00b7fahr\u00b7ner", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Leser jeder Stirn, und der Aspecten Kenner.", "tokens": ["Der", "Le\u00b7ser", "je\u00b7der", "Stirn", ",", "und", "der", "A\u00b7spec\u00b7ten", "Ken\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der sechste Carl verschied, und kein Komet erschien,", "tokens": ["Der", "sechs\u00b7te", "Carl", "ver\u00b7schied", ",", "und", "kein", "Ko\u00b7met", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "$,", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein Nordlicht streift' umher, und beides \u00e4rgert' ihn.", "tokens": ["Kein", "Nord\u00b7licht", "streift'", "um\u00b7her", ",", "und", "bei\u00b7des", "\u00e4r\u00b7gert'", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,", "KON", "PIS", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch seine Frau ward krank, zu vieler Mi\u00dfvergn\u00fcgen;", "tokens": ["Doch", "sei\u00b7ne", "Frau", "ward", "krank", ",", "zu", "vie\u00b7ler", "Mi\u00df\u00b7ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da sah er einen Stern durch seinen Garten fliegen.", "tokens": ["Da", "sah", "er", "ei\u00b7nen", "Stern", "durch", "sei\u00b7nen", "Gar\u00b7ten", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach! sprach er, voller Furcht, die kaum sich schildern l\u00e4\u00dft:", "tokens": ["Ach", "!", "sprach", "er", ",", "vol\u00b7ler", "Furcht", ",", "die", "kaum", "sich", "schil\u00b7dern", "l\u00e4\u00dft", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "PRELS", "ADV", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Stirbt nicht mein sch\u00f6nes Weib, so k\u00f6mmt uns doch die Pest.", "tokens": ["Stirbt", "nicht", "mein", "sch\u00f6\u00b7nes", "Weib", ",", "so", "k\u00f6mmt", "uns", "doch", "die", "Pest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sein sch\u00f6nes Weib genas: die Pest blieb aus dem Lande.", "tokens": ["Sein", "sch\u00f6\u00b7nes", "Weib", "ge\u00b7nas", ":", "die", "Pest", "blieb", "aus", "dem", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "$.", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Halt! rief er, dieser Stern droht Schiffbeck mit dem Brande.", "tokens": ["Halt", "!", "rief", "er", ",", "die\u00b7ser", "Stern", "droht", "Schiff\u00b7beck", "mit", "dem", "Bran\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVFIN", "PPER", "$,", "PDAT", "NN", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Brand erfolgte nicht, und endlich fiel ihm ein:", "tokens": ["Der", "Brand", "er\u00b7folg\u00b7te", "nicht", ",", "und", "end\u00b7lich", "fiel", "ihm", "ein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "KON", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich erb' in kurzer Zeit: es mu\u00df ein Gl\u00fccksstern sein!", "tokens": ["Ich", "erb'", "in", "kur\u00b7zer", "Zeit", ":", "es", "mu\u00df", "ein", "Gl\u00fccks\u00b7stern", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"},