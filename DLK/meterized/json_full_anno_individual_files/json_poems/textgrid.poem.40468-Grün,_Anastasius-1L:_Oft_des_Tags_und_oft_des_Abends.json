{"textgrid.poem.40468": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft des Tags und oft des Abends", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft des Tags und oft des Abends", "tokens": ["Oft", "des", "Tags", "und", "oft", "des", "A\u00b7bends"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wall' ich an das Ziel der Sehnsucht,", "tokens": ["Wall'", "ich", "an", "das", "Ziel", "der", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.3": {"text": "Aus der Stadt durchtobten Stra\u00dfen", "tokens": ["Aus", "der", "Stadt", "durch\u00b7tob\u00b7ten", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Vorstadt still're Welt.", "tokens": ["In", "der", "Vor\u00b7stadt", "still'\u00b7re", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ueber unsres Stromes Br\u00fccke", "tokens": ["Ue\u00b7ber", "uns\u00b7res", "Stro\u00b7mes", "Br\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieh' ich hin mit raschem Schritte,", "tokens": ["Zieh'", "ich", "hin", "mit", "ra\u00b7schem", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wie ein Geist so still und schweigsam", "tokens": ["Wie", "ein", "Geist", "so", "still", "und", "schweig\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch den l\u00e4rmend lauten Schwarm.", "tokens": ["Durch", "den", "l\u00e4r\u00b7mend", "lau\u00b7ten", "Schwarm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dann rechts? \u2013 ach nein, zur Linken!", "tokens": ["Und", "dann", "rechts", "?", "\u2013", "ach", "nein", ",", "zur", "Lin\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "$(", "XY", "PTKANT", "$,", "APPRART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Seht, kaum wei\u00df ich mehr es selber;", "tokens": ["Seht", ",", "kaum", "wei\u00df", "ich", "mehr", "es", "sel\u00b7ber", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann grad fort? \u2013 ach nein, zur Rechten,", "tokens": ["Dann", "grad", "fort", "?", "\u2013", "ach", "nein", ",", "zur", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$.", "$(", "XY", "PTKANT", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.rela