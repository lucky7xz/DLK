{"textgrid.poem.54243": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geliebtes Engels-Kind! dein unempfindlich Wesen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebtes Engels-Kind! dein unempfindlich Wesen,", "tokens": ["Ge\u00b7lieb\u00b7tes", "En\u00b7gels\u00b7Kind", "!", "dein", "un\u00b7emp\u00b7find\u00b7lich", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mir dein kaltes Hertz und Auge gibt zu lesen,", "tokens": ["Das", "mir", "dein", "kal\u00b7tes", "Hertz", "und", "Au\u00b7ge", "gibt", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPOSAT", "ADJA", "NN", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verzehrt mir Marck und Bluth aus Adern und Gebein,", "tokens": ["Ver\u00b7zehrt", "mir", "Marck", "und", "Bluth", "aus", "A\u00b7dern", "und", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So da\u00df ich leider! mu\u00df ein halber Mensche seyn.", "tokens": ["So", "da\u00df", "ich", "lei\u00b7der", "!", "mu\u00df", "ein", "hal\u00b7ber", "Men\u00b7sche", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "$.", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So oft die Leidenschafft mich heist mit dir besprechen,", "tokens": ["So", "oft", "die", "Lei\u00b7den\u00b7schafft", "mich", "heist", "mit", "dir", "be\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So suchst du das Gespr\u00e4ch mit List zu unterbrechen.", "tokens": ["So", "suchst", "du", "das", "Ge\u00b7spr\u00e4ch", "mit", "List", "zu", "un\u00b7ter\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Hertz ist Felsen gleich und dein verstockter Sinn", "tokens": ["Dein", "Hertz", "ist", "Fel\u00b7sen", "gleich", "und", "dein", "ver\u00b7stock\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADV", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Macht, da\u00df ich meiner nicht, wie vor, mehr m\u00e4chtig bin.", "tokens": ["Macht", ",", "da\u00df", "ich", "mei\u00b7ner", "nicht", ",", "wie", "vor", ",", "mehr", "m\u00e4ch\u00b7tig", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "PTKNEG", "$,", "PWAV", "PTKVZ", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du merckst wohl meinen Schmertz, und doch mu\u00df ich mich qu\u00e4len,", "tokens": ["Du", "merckst", "wohl", "mei\u00b7nen", "Schmertz", ",", "und", "doch", "mu\u00df", "ich", "mich", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "KON", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Sehnsucht r\u00fchret mir das innerste der Seelen.", "tokens": ["Die", "Sehn\u00b7sucht", "r\u00fch\u00b7ret", "mir", "das", "in\u00b7ners\u00b7te", "der", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Mein Hertz verwelckt dabey wie d\u00fcrres Espen-Laub,", "tokens": ["Mein", "Hertz", "ver\u00b7welckt", "da\u00b7bey", "wie", "d\u00fcr\u00b7res", "Es\u00b7pen\u00b7Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und dennoch bleibest du bey meinen Bitten taub.", "tokens": ["Und", "den\u00b7noch", "blei\u00b7best", "du", "bey", "mei\u00b7nen", "Bit\u00b7ten", "taub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr Sterne! helfft mir doch ihr Hertze mit erbitten,", "tokens": ["Ihr", "Ster\u00b7ne", "!", "helfft", "mir", "doch", "ihr", "Hert\u00b7ze", "mit", "er\u00b7bit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "PPER", "VVFIN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ihr wist, was ich bi\u00dfher um selbige gelitten.", "tokens": ["Ihr", "wist", ",", "was", "ich", "bi\u00df\u00b7her", "um", "sel\u00b7bi\u00b7ge", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "PPER", "ADV", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.15": {"text": "Wiewohl es ist umsonst, ihr seyd so schwach als ich,", "tokens": ["Wie\u00b7wohl", "es", "ist", "um\u00b7sonst", ",", "ihr", "seyd", "so", "schwach", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ihr harter Sinn verlacht euch ebenfals wie mich.", "tokens": ["Ihr", "har\u00b7ter", "Sinn", "ver\u00b7lacht", "euch", "e\u00b7ben\u00b7fals", "wie", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach strenge Flavia, behertzge dein Beginnen,", "tokens": ["Ach", "stren\u00b7ge", "Fla\u00b7via", ",", "be\u00b7hertz\u00b7ge", "dein", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NE", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Du siehst das herbe Na\u00df der Thr\u00e4nen hefftig rinnen,", "tokens": ["Du", "siehst", "das", "her\u00b7be", "Na\u00df", "der", "Thr\u00e4\u00b7nen", "heff\u00b7tig", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und doch wilst du dabey ein Scyth und Barbar seyn;", "tokens": ["Und", "doch", "wilst", "du", "da\u00b7bey", "ein", "Scyth", "und", "Bar\u00b7bar", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PAV", "ART", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Verbleibt denn deine Brust noch immer Stahl und Stein?", "tokens": ["Ver\u00b7bleibt", "denn", "dei\u00b7ne", "Brust", "noch", "im\u00b7mer", "Stahl", "und", "Stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PPOSAT", "NN", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Besinne dich, mein Kind, la\u00df H\u00e4rt und K\u00e4lte schwinden,", "tokens": ["Be\u00b7sin\u00b7ne", "dich", ",", "mein", "Kind", ",", "la\u00df", "H\u00e4rt", "und", "K\u00e4l\u00b7te", "schwin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVIMP", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du kanst leicht auf der Welt wohl keinen Menschen finden,", "tokens": ["Du", "kanst", "leicht", "auf", "der", "Welt", "wohl", "kei\u00b7nen", "Men\u00b7schen", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "ART", "NN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der dich so z\u00e4rtlich liebt, mehr als sein Auge hegt,", "tokens": ["Der", "dich", "so", "z\u00e4rt\u00b7lich", "liebt", ",", "mehr", "als", "sein", "Au\u00b7ge", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,", "PIAT", "KOKOM", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und sich zu deinen Fu\u00df mit gr\u00f6ssrer Ehrfurcht legt.", "tokens": ["Und", "sich", "zu", "dei\u00b7nen", "Fu\u00df", "mit", "gr\u00f6ss\u00b7rer", "Ehr\u00b7furcht", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Verstelle dich nur nicht, als k\u00f6ntest du nicht lieben,", "tokens": ["Ver\u00b7stel\u00b7le", "dich", "nur", "nicht", ",", "als", "k\u00f6n\u00b7test", "du", "nicht", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "In deiner Augen Paar steht warlich was geschrieben,", "tokens": ["In", "dei\u00b7ner", "Au\u00b7gen", "Paar", "steht", "war\u00b7lich", "was", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Woraus ein iederman mehr als zu deutlich schaut,", "tokens": ["Wo\u00b7raus", "ein", "ie\u00b7der\u00b7man", "mehr", "als", "zu", "deut\u00b7lich", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIS", "PIS", "KOKOM", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Liebe habe dir ihr Wappen anvertraut.", "tokens": ["Die", "Lie\u00b7be", "ha\u00b7be", "dir", "ihr", "Wap\u00b7pen", "an\u00b7ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "H\u00f6r auf, Annehmlichste, mich fernerweit zu qu\u00e4len,", "tokens": ["H\u00f6r", "auf", ",", "An\u00b7nehm\u00b7lichs\u00b7te", ",", "mich", "fer\u00b7ner\u00b7weit", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NE", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Die Gro\u00dfmuth ist ja sonst das Merckmahl sch\u00f6ner Seelen,", "tokens": ["Die", "Gro\u00df\u00b7muth", "ist", "ja", "sonst", "das", "Merck\u00b7mahl", "sch\u00f6\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "O la\u00df doch selbige dir auch zur Seiten stehn,", "tokens": ["O", "la\u00df", "doch", "sel\u00b7bi\u00b7ge", "dir", "auch", "zur", "Sei\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJA", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Damit ich mit Triumph kan in dein Hertze gehn.", "tokens": ["Da\u00b7mit", "ich", "mit", "Tri\u00b7umph", "kan", "in", "dein", "Hert\u00b7ze", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Hoffnung scheinet mich noch immer zu begleiten,", "tokens": ["Die", "Hoff\u00b7nung", "schei\u00b7net", "mich", "noch", "im\u00b7mer", "zu", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Obgleich mein Hertze will mit Furcht und Zweifel streiten.", "tokens": ["Ob\u00b7gleich", "mein", "Hert\u00b7ze", "will", "mit", "Furcht", "und", "Zwei\u00b7fel", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "VVFIN", "VMFIN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Erbarm, O Sch\u00f6nste! dich, ertheile mir Bericht,", "tokens": ["Er\u00b7barm", ",", "O", "Sch\u00f6ns\u00b7te", "!", "dich", ",", "er\u00b7thei\u00b7le", "mir", "Be\u00b7richt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "NN", "$.", "PPER", "$,", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ob mir nunmehr dein Mund ein gn\u00e4dig Urthel spricht.", "tokens": ["Ob", "mir", "nun\u00b7mehr", "dein", "Mund", "ein", "gn\u00e4\u00b7dig", "Ur\u00b7thel", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Fehl ich, so will ich mich in mein Verh\u00e4ngni\u00df schicken,", "tokens": ["Fehl", "ich", ",", "so", "will", "ich", "mich", "in", "mein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "schi\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und nunmehr weiter nicht nach etwas andern blicken,", "tokens": ["Und", "nun\u00b7mehr", "wei\u00b7ter", "nicht", "nach", "et\u00b7was", "an\u00b7dern", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PTKNEG", "APPR", "PIAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Vielleicht kan dir, wie mir, dergleichen auch geschehn,", "tokens": ["Viel\u00b7leicht", "kan", "dir", ",", "wie", "mir", ",", "derg\u00b7lei\u00b7chen", "auch", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PWAV", "PPER", "$,", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wenn sich dein Augen-Paar was liebes ausersehn.", "tokens": ["Wenn", "sich", "dein", "Au\u00b7gen\u00b7Paar", "was", "lie\u00b7bes", "au\u00b7ser\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PWS", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Geliebtes Engels-Kind! dein unempfindlich Wesen,", "tokens": ["Ge\u00b7lieb\u00b7tes", "En\u00b7gels\u00b7Kind", "!", "dein", "un\u00b7emp\u00b7find\u00b7lich", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das mir dein kaltes Hertz und Auge gibt zu lesen,", "tokens": ["Das", "mir", "dein", "kal\u00b7tes", "Hertz", "und", "Au\u00b7ge", "gibt", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPOSAT", "ADJA", "NN", "KON", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verzehrt mir Marck und Bluth aus Adern und Gebein,", "tokens": ["Ver\u00b7zehrt", "mir", "Marck", "und", "Bluth", "aus", "A\u00b7dern", "und", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So da\u00df ich leider! mu\u00df ein halber Mensche seyn.", "tokens": ["So", "da\u00df", "ich", "lei\u00b7der", "!", "mu\u00df", "ein", "hal\u00b7ber", "Men\u00b7sche", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "$.", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So oft die Leidenschafft mich heist mit dir besprechen,", "tokens": ["So", "oft", "die", "Lei\u00b7den\u00b7schafft", "mich", "heist", "mit", "dir", "be\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So suchst du das Gespr\u00e4ch mit List zu unterbrechen.", "tokens": ["So", "suchst", "du", "das", "Ge\u00b7spr\u00e4ch", "mit", "List", "zu", "un\u00b7ter\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein Hertz ist Felsen gleich und dein verstockter Sinn", "tokens": ["Dein", "Hertz", "ist", "Fel\u00b7sen", "gleich", "und", "dein", "ver\u00b7stock\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADV", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Macht, da\u00df ich meiner nicht, wie vor, mehr m\u00e4chtig bin.", "tokens": ["Macht", ",", "da\u00df", "ich", "mei\u00b7ner", "nicht", ",", "wie", "vor", ",", "mehr", "m\u00e4ch\u00b7tig", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "PTKNEG", "$,", "PWAV", "PTKVZ", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du merckst wohl meinen Schmertz, und doch mu\u00df ich mich qu\u00e4len,", "tokens": ["Du", "merckst", "wohl", "mei\u00b7nen", "Schmertz", ",", "und", "doch", "mu\u00df", "ich", "mich", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "KON", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Sehnsucht r\u00fchret mir das innerste der Seelen.", "tokens": ["Die", "Sehn\u00b7sucht", "r\u00fch\u00b7ret", "mir", "das", "in\u00b7ners\u00b7te", "der", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Mein Hertz verwelckt dabey wie d\u00fcrres Espen-Laub,", "tokens": ["Mein", "Hertz", "ver\u00b7welckt", "da\u00b7bey", "wie", "d\u00fcr\u00b7res", "Es\u00b7pen\u00b7Laub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und dennoch bleibest du bey meinen Bitten taub.", "tokens": ["Und", "den\u00b7noch", "blei\u00b7best", "du", "bey", "mei\u00b7nen", "Bit\u00b7ten", "taub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ihr Sterne! helfft mir doch ihr Hertze mit erbitten,", "tokens": ["Ihr", "Ster\u00b7ne", "!", "helfft", "mir", "doch", "ihr", "Hert\u00b7ze", "mit", "er\u00b7bit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "PPER", "VVFIN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ihr wist, was ich bi\u00dfher um selbige gelitten.", "tokens": ["Ihr", "wist", ",", "was", "ich", "bi\u00df\u00b7her", "um", "sel\u00b7bi\u00b7ge", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWS", "PPER", "ADV", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.15": {"text": "Wiewohl es ist umsonst, ihr seyd so schwach als ich,", "tokens": ["Wie\u00b7wohl", "es", "ist", "um\u00b7sonst", ",", "ihr", "seyd", "so", "schwach", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ihr harter Sinn verlacht euch ebenfals wie mich.", "tokens": ["Ihr", "har\u00b7ter", "Sinn", "ver\u00b7lacht", "euch", "e\u00b7ben\u00b7fals", "wie", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach strenge Flavia, behertzge dein Beginnen,", "tokens": ["Ach", "stren\u00b7ge", "Fla\u00b7via", ",", "be\u00b7hertz\u00b7ge", "dein", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "NE", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Du siehst das herbe Na\u00df der Thr\u00e4nen hefftig rinnen,", "tokens": ["Du", "siehst", "das", "her\u00b7be", "Na\u00df", "der", "Thr\u00e4\u0