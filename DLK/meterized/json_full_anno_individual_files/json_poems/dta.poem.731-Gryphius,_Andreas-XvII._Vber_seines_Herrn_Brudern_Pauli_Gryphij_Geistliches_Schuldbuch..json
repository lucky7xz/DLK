{"dta.poem.731": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XvII.  Vber seines Herrn Brudern  Pauli  \n Gryphij  Geistliches Schuldbuch.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "De\u00df strengen Richters Buch/ das Buch voll grauser ", "tokens": ["De\u00df", "stren\u00b7gen", "Rich\u00b7ters", "Buch", "/", "das", "Buch", "voll", "grau\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "ART", "NN", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "S\u00fcnden ", "tokens": ["S\u00fcn\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Voll vbermachter ", "tokens": ["Voll", "vber\u00b7mach\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJD", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Ein jeder ", "tokens": ["Ein", "je\u00b7der"], "token_info": ["word", "word"], "pos": ["ART", "PIAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Der hochergrimbte Gott/ sich rechtlich ab wil finden/", "tokens": ["Der", "ho\u00b7cher\u00b7grimb\u00b7te", "Gott", "/", "sich", "recht\u00b7lich", "ab", "wil", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PRF", "ADJD", "PTKVZ", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Buch f\u00fcr welchem mir mu\u00df Geist vud Leben schwinden", "tokens": ["Das", "Buch", "f\u00fcr", "wel\u00b7chem", "mir", "mu\u00df", "Geist", "vud", "Le\u00b7ben", "schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PRELS", "PPER", "VMFIN", "NN", "NE", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr dessen anblick nur sinckt alle ", "tokens": ["F\u00fcr", "des\u00b7sen", "an\u00b7blick", "nur", "sinckt", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "VVFIN", "ADV", "VVFIN", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Herr Bruder weist vns hier ew'r wolberedter Mundt; ", "tokens": ["Herr", "Bru\u00b7der", "weist", "vns", "hier", "ew'r", "wol\u00b7be\u00b7red\u00b7ter", "Mundt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit diesem der vns kan von solcher noth entbinden.", "tokens": ["Mit", "die\u00b7sem", "der", "vns", "kan", "von", "sol\u00b7cher", "noth", "ent\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ART", "PPER", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O wol dem/ der bey zeit nimbt seine sach in acht/", "tokens": ["O", "wol", "dem", "/", "der", "bey", "zeit", "nimbt", "sei\u00b7ne", "sach", "in", "acht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "$(", "ART", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "CARD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eh' er wird citirt/ jhm den zum Freunde macht/ ", "tokens": ["Der", "eh'", "er", "wird", "ci\u00b7tirt", "/", "jhm", "den", "zum", "Freun\u00b7de", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PPER", "VAFIN", "VVPP", "$(", "PPER", "ART", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der diese s", "tokens": ["Der", "die\u00b7se", "s"], "token_info": ["word", "word", "word"], "pos": ["ART", "PDAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wol euch/ der wolgemeiut/ was wol f\u00fcr vus entdeckt/", "tokens": ["Wol", "euch", "/", "der", "wol\u00b7ge\u00b7mei\u00b7ut", "/", "was", "wol", "f\u00fcr", "vus", "ent\u00b7deckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$(", "ART", "NN", "$(", "PWS", "ADV", "APPR", "NE", "VVPP", "$("], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Jhr/ dessen ernst erquickt/ die eigne wollust schreckt", "tokens": ["Ihr", "/", "des\u00b7sen", "ernst", "er\u00b7quickt", "/", "die", "eig\u00b7ne", "wol\u00b7lust", "schreckt"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$(", "PDS", "ADJD", "VVPP", "$(", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die jhr die f\u00fchrt zu Gott/ die Gott ins leben f\u00fchret.", "tokens": ["Die", "jhr", "die", "f\u00fchrt", "zu", "Gott", "/", "die", "Gott", "ins", "le\u00b7ben", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "VVFIN", "APPR", "NN", "$(", "ART", "NN", "APPRART", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}