{"dta.poem.9732": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auff die hochzeit des Churf\u00fcrstl. Brandenb.  \n hoff- und cammer-gerichts-raths Hn.  \n BEWERTS mit der Jungfer  \n Lehmannin.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich nahm mir neulich vor die hochzeit zu besingen/", "tokens": ["Ich", "nahm", "mir", "neu\u00b7lich", "vor", "die", "hoch\u00b7zeit", "zu", "be\u00b7sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und den verlangten tag/ da ihr geehrten zwey", "tokens": ["Und", "den", "ver\u00b7lang\u00b7ten", "tag", "/", "da", "ihr", "ge\u00b7ehr\u00b7ten", "zwey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "KOUS", "PPOSAT", "ADJA", "CARD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Einander saget zu gunst/ lieb und ew\u2019ge treu;", "tokens": ["Ein\u00b7an\u00b7der", "sa\u00b7get", "zu", "gunst", "/", "lieb", "und", "ew'\u00b7ge", "treu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$(", "ADJD", "KON", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein/ es war umsonst/ mir wolte nichts gelingen/", "tokens": ["Al\u00b7lein", "/", "es", "war", "um\u00b7sonst", "/", "mir", "wol\u00b7te", "nichts", "ge\u00b7lin\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VAFIN", "ADV", "$(", "PPER", "VMFIN", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die freude lie\u00df sich nicht in enge reimen zwingen/", "tokens": ["Die", "freu\u00b7de", "lie\u00df", "sich", "nicht", "in", "en\u00b7ge", "rei\u00b7men", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "ADJA", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich schrieb und \u00e4nderte wohl zehnmal eine reih/", "tokens": ["Ich", "schrieb", "und", "\u00e4n\u00b7der\u00b7te", "wohl", "zehn\u00b7mal", "ei\u00b7ne", "reih", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bi\u00df ich voll ungedult auffsprang/ und rieff: es sey!", "tokens": ["Bi\u00df", "ich", "voll", "un\u00b7ge\u00b7dult", "auff\u00b7sprang", "/", "und", "rieff", ":", "es", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADJD", "VVFIN", "$(", "KON", "VVFIN", "$.", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich kan unm\u00f6glich heut ein wort zu wege bringen.", "tokens": ["Ich", "kan", "un\u00b7m\u00f6g\u00b7lich", "heut", "ein", "wort", "zu", "we\u00b7ge", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Apollo sah\u2019 an mir den unmuths-vollen sinn;", "tokens": ["A\u00b7pol\u00b7lo", "sah'", "an", "mir", "den", "un\u00b7muths\u00b7vol\u00b7len", "sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was? schalt er/ setzstu denn nicht eine zeile hin?", "tokens": ["Was", "?", "schalt", "er", "/", "setz\u00b7stu", "denn", "nicht", "ei\u00b7ne", "zei\u00b7le", "hin", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$.", "VVFIN", "PPER", "$(", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach! sch\u00e4me dich/ und la\u00df dir nimmer das belieben!", "tokens": ["Ach", "!", "sch\u00e4\u00b7me", "dich", "/", "und", "la\u00df", "dir", "nim\u00b7mer", "das", "be\u00b7lie\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$(", "KON", "VVIMP", "PPER", "ADV", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Begreiffe dich und nim die feder noch zur hand/", "tokens": ["Be\u00b7greif\u00b7fe", "dich", "und", "nim", "die", "fe\u00b7der", "noch", "zur", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVIMP", "ART", "NN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der wille wird gar offt auch vor die that erkannt:", "tokens": ["Der", "wil\u00b7le", "wird", "gar", "offt", "auch", "vor", "die", "that", "er\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "APPR", "ART", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Schreib ihre nahmen nur/ so hastu gnug geschrieben.", "tokens": ["Schreib", "ih\u00b7re", "nah\u00b7men", "nur", "/", "so", "has\u00b7tu", "gnug", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "ADV", "$(", "ADV", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}