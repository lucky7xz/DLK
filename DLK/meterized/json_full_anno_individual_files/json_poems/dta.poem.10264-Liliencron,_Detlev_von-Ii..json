{"dta.poem.10264": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Und wieder stand ich unter unsern Ulmen,", "tokens": ["Und", "wie\u00b7der", "stand", "ich", "un\u00b7ter", "un\u00b7sern", "Ul\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch nicht mit dir. Allein sah ich hinaus", "tokens": ["Doch", "nicht", "mit", "dir", ".", "Al\u00b7lein", "sah", "ich", "hin\u00b7aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "PPER", "$.", "ADV", "VVFIN", "PPER", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In lichten Fr\u00fchlingstag: Der Junge pfiff", "tokens": ["In", "lich\u00b7ten", "Fr\u00fch\u00b7lings\u00b7tag", ":", "Der", "Jun\u00b7ge", "pfiff"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein lustig Liedchen seinen K\u00fchen; gl\u00e4nzend", "tokens": ["Ein", "lus\u00b7tig", "Lied\u00b7chen", "sei\u00b7nen", "K\u00fc\u00b7hen", ";", "gl\u00e4n\u00b7zend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "NN", "PPOSAT", "NN", "$.", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Licht umkreisten Kr\u00e4hen hohe B\u00e4ume,", "tokens": ["Im", "Licht", "um\u00b7kreis\u00b7ten", "Kr\u00e4\u00b7hen", "ho\u00b7he", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In blauer Luft schaut\u2019 ich am Horizont", "tokens": ["In", "blau\u00b7er", "Luft", "schaut'", "ich", "am", "Ho\u00b7ri\u00b7zont"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die M\u00fchle schnell im Wind die Fl\u00fcgel drehn.", "tokens": ["Die", "M\u00fch\u00b7le", "schnell", "im", "Wind", "die", "Fl\u00fc\u00b7gel", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und doch, ich sah nur graue Todesnebel,", "tokens": ["Und", "doch", ",", "ich", "sah", "nur", "grau\u00b7e", "To\u00b7des\u00b7ne\u00b7bel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und teilnahmlos kehrt\u2019 ich zur\u00fcck ins Haus.", "tokens": ["Und", "teil\u00b7nahm\u00b7los", "kehrt'", "ich", "zu\u00b7r\u00fcck", "ins", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}