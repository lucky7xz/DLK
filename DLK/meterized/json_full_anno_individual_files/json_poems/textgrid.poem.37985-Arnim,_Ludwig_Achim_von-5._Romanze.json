{"textgrid.poem.37985": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "5. Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig lag in seinem Bett,", "tokens": ["Der", "K\u00f6\u00b7nig", "lag", "in", "sei\u00b7nem", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Nachts seltsam Gedanken h\u00e4tt,", "tokens": ["Des", "Nachts", "selt\u00b7sam", "Ge\u00b7dan\u00b7ken", "h\u00e4tt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine Gedanken gingen ein", "tokens": ["Und", "sei\u00b7ne", "Ge\u00b7dan\u00b7ken", "gin\u00b7gen", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In seiner Base Schlafk\u00e4mmerlein,", "tokens": ["In", "sei\u00b7ner", "Ba\u00b7se", "Schlaf\u00b7k\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und immer schwerer kamen wieder,", "tokens": ["Und", "im\u00b7mer", "schwe\u00b7rer", "ka\u00b7men", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Bienen ziehn vom Schw\u00e4rmen nieder.", "tokens": ["Wie", "Bie\u00b7nen", "ziehn", "vom", "Schw\u00e4r\u00b7men", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am Morgen schickt er seinen Zwerg,", "tokens": ["Am", "Mor\u00b7gen", "schickt", "er", "sei\u00b7nen", "Zwerg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Peter Herrn von Stauffenberg:", "tokens": ["Zu", "Pe\u00b7ter", "Herrn", "von", "Stauf\u00b7fen\u00b7berg", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdie Base mein von hoher Art,", "tokens": ["\u00bb", "die", "Ba\u00b7se", "mein", "von", "ho\u00b7her", "Art", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die F\u00fcrstin, jung und reich und zart,", "tokens": ["Die", "F\u00fcrs\u00b7tin", ",", "jung", "und", "reich", "und", "zart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die will ich geben euch zum Weib,", "tokens": ["Die", "will", "ich", "ge\u00b7ben", "euch", "zum", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihrem K\u00e4rntnerland und Leut.\u00ab", "tokens": ["Mit", "ih\u00b7rem", "K\u00e4rnt\u00b7ner\u00b7land", "und", "Leut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Kein Wort kam aus den Ritters Mund,", "tokens": ["Kein", "Wort", "kam", "aus", "den", "Rit\u00b7ters", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erschrocken stand er da zur Stund:", "tokens": ["Er\u00b7schro\u00b7cken", "stand", "er", "da", "zur", "Stund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbmein Red halt mir f\u00fcr keinen Spott,", "tokens": ["\u00bb", "mein", "Red", "halt", "mir", "f\u00fcr", "kei\u00b7nen", "Spott", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nimm hiemit zu Zeugen Gott,", "tokens": ["Und", "nimm", "hie\u00b7mit", "zu", "Zeu\u00b7gen", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df es mein ewger Ernst f\u00fcrwahr,", "tokens": ["Da\u00df", "es", "mein", "ew\u00b7ger", "Ernst", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df euer die F\u00fcrstin ganz und gar.\u00ab", "tokens": ["Da\u00df", "eu\u00b7er", "die", "F\u00fcrs\u00b7tin", "ganz", "und", "gar", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPOSAT", "ART", "NN", "ADV", "KON", "ADV", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Herr Peter sprach mit gro\u00dfen Treuen,", "tokens": ["Herr", "Pe\u00b7ter", "sprach", "mit", "gro\u00b7\u00dfen", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der hohe Lohn k\u00f6nnt ihn nicht freuen,", "tokens": ["Der", "ho\u00b7he", "Lohn", "k\u00f6nnt", "ihn", "nicht", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie er der Meerfey schon verlobt,", "tokens": ["Wie", "er", "der", "Meer\u00b7fey", "schon", "ver\u00b7lobt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Untreu sey der Tod gelobt,", "tokens": ["Der", "Un\u00b7treu", "sey", "der", "Tod", "ge\u00b7lobt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst sey er frey von Noth und Leid,", "tokens": ["Sonst", "sey", "er", "frey", "von", "Noth", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Gut und Geld von ihr erfreut.", "tokens": ["Mit", "Gut", "und", "Geld", "von", "ihr", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbweh eurer Seele an dem Ort,", "tokens": ["\u00bb", "weh", "eu\u00b7rer", "See\u00b7le", "an", "dem", "Ort", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sie ist verloren hier und dort,", "tokens": ["Sie", "ist", "ver\u00b7lo\u00b7ren", "hier", "und", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seht Gottes Auge nimmermehr,", "tokens": ["Seht", "Got\u00b7tes", "Au\u00b7ge", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihr euch nicht von ihr abkehrt;", "tokens": ["Wenn", "ihr", "euch", "nicht", "von", "ihr", "ab\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Sollt ihr 'nen Geist zum Weibe haben,", "tokens": ["Sollt", "ihr", "'nen", "Geist", "zum", "Wei\u00b7be", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nie werden euch die Kinder laben.", "tokens": ["Nie", "wer\u00b7den", "euch", "die", "Kin\u00b7der", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dem Teufel seyd ihr zugesellt,", "tokens": ["Dem", "Teu\u00b7fel", "seyd", "ihr", "zu\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr armer Mann! Ihr theurer Held!\u00ab", "tokens": ["Ihr", "ar\u00b7mer", "Mann", "!", "Ihr", "theu\u00b7rer", "Held", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So sprach der Bischof und der K\u00f6nig,", "tokens": ["So", "sprach", "der", "Bi\u00b7schof", "und", "der", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Ritter sagt darauf zum K\u00f6nig:", "tokens": ["Der", "Rit\u00b7ter", "sagt", "da\u00b7rauf", "zum", "K\u00f6\u00b7nig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbes geht mir tief zu meinem Herzen,", "tokens": ["\u00bb", "es", "geht", "mir", "tief", "zu", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Gottes Gnad will nicht verscherzen.\u00ab", "tokens": ["Und", "Got\u00b7tes", "Gnad", "will", "nicht", "ver\u00b7scher\u00b7zen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "NN", "VMFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Herr Peter ward verlobt sogleich,", "tokens": ["Herr", "Pe\u00b7ter", "ward", "ver\u00b7lobt", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VAFIN", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Gold und edlen Steinen reich,", "tokens": ["An", "Gold", "und", "ed\u00b7len", "Stei\u00b7nen", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O heller Glanz der Jungfrau fein,", "tokens": ["O", "hel\u00b7ler", "Glanz", "der", "Jung\u00b7frau", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wem strahlet er mit Freudenschein.", "tokens": ["Wem", "strah\u00b7let", "er", "mit", "Freu\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach Stauffenberg sie ziehen fort,", "tokens": ["Nach", "Stauf\u00b7fen\u00b7berg", "sie", "zie\u00b7hen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu feyern ihre Hochzeit dort!", "tokens": ["Zu", "fey\u00b7ern", "ih\u00b7re", "Hoch\u00b7zeit", "dort", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihr d\u00fcstren W\u00e4lder auf dem Wege,", "tokens": ["Ihr", "d\u00fcst\u00b7ren", "W\u00e4l\u00b7der", "auf", "dem", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was streckt die Aeste ihr entgegen,", "tokens": ["Was", "streckt", "die", "A\u00b7es\u00b7te", "ihr", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Viel froher Schaaren ziehen ja,", "tokens": ["Viel", "fro\u00b7her", "Schaa\u00b7ren", "zie\u00b7hen", "ja", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit hellem Klange fern und nah,", "tokens": ["Mit", "hel\u00b7lem", "Klan\u00b7ge", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit bunten B\u00e4ndern, Scherz und Streit,", "tokens": ["Mit", "bun\u00b7ten", "B\u00e4n\u00b7dern", ",", "Scherz", "und", "Streit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist alles Lust, ist alles Freud.", "tokens": ["Ist", "al\u00b7les", "Lust", ",", "ist", "al\u00b7les", "Freud", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "$,", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der K\u00f6nig lag in seinem Bett,", "tokens": ["Der", "K\u00f6\u00b7nig", "lag", "in", "sei\u00b7nem", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Nachts seltsam Gedanken h\u00e4tt,", "tokens": ["Des", "Nachts", "selt\u00b7sam", "Ge\u00b7dan\u00b7ken", "h\u00e4tt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine Gedanken gingen ein", "tokens": ["Und", "sei\u00b7ne", "Ge\u00b7dan\u00b7ken", "gin\u00b7gen", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In seiner Base Schlafk\u00e4mmerlein,", "tokens": ["In", "sei\u00b7ner", "Ba\u00b7se", "Schlaf\u00b7k\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und immer schwerer kamen wieder,", "tokens": ["Und", "im\u00b7mer", "schwe\u00b7rer", "ka\u00b7men", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Bienen ziehn vom Schw\u00e4rmen nieder.", "tokens": ["Wie", "Bie\u00b7nen", "ziehn", "v