{"textgrid.poem.43933": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eleonore lies ihr Herze", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eleonore lies ihr Herze", "tokens": ["E\u00b7leo\u00b7no\u00b7re", "lies", "ihr", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht l\u00e4nger unempfindlich seyn,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "un\u00b7emp\u00b7find\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie r\u00e4umt es nach so langem Schmerze", "tokens": ["Sie", "r\u00e4umt", "es", "nach", "so", "lan\u00b7gem", "Schmer\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem wohlbekandten Dichter ein", "tokens": ["Dem", "wohl\u00b7be\u00b7kand\u00b7ten", "Dich\u00b7ter", "ein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und lies ihn unter Schwur und K\u00fc\u00dfen", "tokens": ["Und", "lies", "ihn", "un\u00b7ter", "Schwur", "und", "K\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Anfang ihrer Neigung wi\u00dfen.", "tokens": ["Den", "An\u00b7fang", "ih\u00b7rer", "Nei\u00b7gung", "wi\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie nahm ihn in die treuen Armen", "tokens": ["Sie", "nahm", "ihn", "in", "die", "treu\u00b7en", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprach bey z\u00e4rtlicher Gewalt:", "tokens": ["Und", "sprach", "bey", "z\u00e4rt\u00b7li\u00b7cher", "Ge\u00b7walt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat ja der Himmel ein Erbarmen,", "tokens": ["Hat", "ja", "der", "Him\u00b7mel", "ein", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So g\u00f6nnt er mir den Aufenthalt,", "tokens": ["So", "g\u00f6nnt", "er", "mir", "den", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis da\u00df ich in dem sanften Grabe", "tokens": ["Bis", "da\u00df", "ich", "in", "dem", "sanf\u00b7ten", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Ziel der Angst erlanget habe.", "tokens": ["Das", "Ziel", "der", "Angst", "er\u00b7lan\u00b7get", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drauf schwieg sie mit verwandten Blicken", "tokens": ["Drauf", "schwieg", "sie", "mit", "ver\u00b7wand\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und strich des Dichters Angesicht,", "tokens": ["Und", "strich", "des", "Dich\u00b7ters", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erg\u00f6zt ihn durch ein H\u00e4ndedr\u00fccken", "tokens": ["Er\u00b7g\u00f6zt", "ihn", "durch", "ein", "H\u00e4n\u00b7de\u00b7dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach von neuem: Ach, mein Licht!", "tokens": ["Und", "sprach", "von", "neu\u00b7em", ":", "Ach", ",", "mein", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "$.", "ITJ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, wird auch dieses mein Verbinden", "tokens": ["Ach", ",", "wird", "auch", "die\u00b7ses", "mein", "Ver\u00b7bin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "PDAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Herz best\u00e4ndig rein erfinden?", "tokens": ["Dein", "Herz", "be\u00b7st\u00e4n\u00b7dig", "rein", "er\u00b7fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bedencke nur, wie viel ich wage", "tokens": ["Be\u00b7den\u00b7cke", "nur", ",", "wie", "viel", "ich", "wa\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PIS", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was ich deinetwegen thu!", "tokens": ["Und", "was", "ich", "dei\u00b7net\u00b7we\u00b7gen", "thu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich eile mit Gefahr und Plage", "tokens": ["Ich", "ei\u00b7le", "mit", "Ge\u00b7fahr", "und", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach deinen sch\u00f6nen Lippen zu", "tokens": ["Nach", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Lip\u00b7pen", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und breche dir allein zu Liebe", "tokens": ["Und", "bre\u00b7che", "dir", "al\u00b7lein", "zu", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Ketten meiner ersten Triebe.", "tokens": ["Die", "Ket\u00b7ten", "mei\u00b7ner", "ers\u00b7ten", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich habe nichts als dein Gem\u00fcthe,", "tokens": ["Ich", "ha\u00b7be", "nichts", "als", "dein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Worauf ich mich verla\u00dfen kan;", "tokens": ["Wo\u00b7rauf", "ich", "mich", "ver\u00b7la\u00b7\u00dfen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verl\u00e4st mich jemahls de\u00dfen G\u00fcte,", "tokens": ["Ver\u00b7l\u00e4st", "mich", "je\u00b7mahls", "de\u00b7\u00dfen", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist es ganz um mich gethan,", "tokens": ["So", "ist", "es", "ganz", "um", "mich", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So werd ich allen auf der Erden", "tokens": ["So", "werd", "ich", "al\u00b7len", "auf", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein M\u00e4hrchen und ein Greuel werden.", "tokens": ["Ein", "M\u00e4hr\u00b7chen", "und", "ein", "Greu\u00b7el", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dies sagte sie mit na\u00dfen Wangen", "tokens": ["Dies", "sag\u00b7te", "sie", "mit", "na\u00b7\u00dfen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zog ihn eilends br\u00fcnstig fort", "tokens": ["Und", "zog", "ihn", "ei\u00b7lends", "br\u00fcns\u00b7tig", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchrte sein best\u00fcrzt Verlangen", "tokens": ["Und", "f\u00fchr\u00b7te", "sein", "be\u00b7st\u00fcrzt", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An den schon oft besuchten Ort,", "tokens": ["An", "den", "schon", "oft", "be\u00b7such\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo nichts als Graus und Nacht regieret", "tokens": ["Wo", "nichts", "als", "Graus", "und", "Nacht", "re\u00b7gie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "KOKOM", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Tod und Stille triumphieret.", "tokens": ["Und", "Tod", "und", "Stil\u00b7le", "tri\u00b7um\u00b7phie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Hier fing sie br\u00fcnstig an zu weinen", "tokens": ["Hier", "fing", "sie", "br\u00fcns\u00b7tig", "an", "zu", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rief: Ihr Todten zeuget mir,", "tokens": ["Und", "rief", ":", "Ihr", "Tod\u00b7ten", "zeu\u00b7get", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey meiner Eltern Leichensteinen", "tokens": ["Bey", "mei\u00b7ner", "El\u00b7tern", "Lei\u00b7chen\u00b7stei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihrer Asche schw\u00f6r ich dir,", "tokens": ["Und", "ih\u00b7rer", "A\u00b7sche", "schw\u00f6r", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df mich dein Herz allein vergn\u00fcge,", "tokens": ["Da\u00df", "mich", "dein", "Herz", "al\u00b7lein", "ver\u00b7gn\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bis da\u00df es hier versammlet liege.", "tokens": ["Bis", "da\u00df", "es", "hier", "ver\u00b7samm\u00b7let", "lie\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du wirst die Redligkeit erkennen", "tokens": ["Du", "wirst", "die", "Red\u00b7lig\u00b7keit", "er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und, bin ich gleich ein armes Kind,", "tokens": ["Und", ",", "bin", "ich", "gleich", "ein", "ar\u00b7mes", "Kind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir ewig deine Seele g\u00f6nnen.", "tokens": ["Mir", "e\u00b7wig", "dei\u00b7ne", "See\u00b7le", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich weis zwar, wie die M\u00e4nner sind;", "tokens": ["Ich", "weis", "zwar", ",", "wie", "die", "M\u00e4n\u00b7ner", "sind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ADV", "$,", "PWAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus Liebe glaub ich deinen Schw\u00fcren,", "tokens": ["Aus", "Lie\u00b7be", "glaub", "ich", "dei\u00b7nen", "Schw\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie werden mich wohl nicht verf\u00fchren.", "tokens": ["Sie", "wer\u00b7den", "mich", "wohl", "nicht", "ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Dichter trocknet' ihre Thr\u00e4nen", "tokens": ["Der", "Dich\u00b7ter", "trock\u00b7net'", "ih\u00b7re", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit tausend warmen K\u00fc\u00dfen ab,", "tokens": ["Mit", "tau\u00b7send", "war\u00b7men", "K\u00fc\u00b7\u00dfen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und als das weich- und stumme Sehnen", "tokens": ["Und", "als", "das", "weich", "und", "stum\u00b7me", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm endlich Zeit zur Antwort gab,", "tokens": ["Ihm", "end\u00b7lich", "Zeit", "zur", "Ant\u00b7wort", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So zog er die geliebten Glieder", "tokens": ["So", "zog", "er", "die", "ge\u00b7lieb\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit diesem Trost ins Gra\u00df darnieder:", "tokens": ["Mit", "die\u00b7sem", "Trost", "ins", "Gra\u00df", "dar\u00b7nie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Komm her, du Nahrung meiner Flammen,", "tokens": ["Komm", "her", ",", "du", "Nah\u00b7rung", "mei\u00b7ner", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punc