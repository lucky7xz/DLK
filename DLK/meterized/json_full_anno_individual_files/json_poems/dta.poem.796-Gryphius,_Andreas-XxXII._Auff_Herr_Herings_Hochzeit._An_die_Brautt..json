{"dta.poem.796": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.  \n Auff Herr Herings Hochzeit. An die Brautt.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ob zwar die sch\u00f6ne zeit/ der Erden newes leben/ ", "tokens": ["Ob", "zwar", "die", "sch\u00f6\u00b7ne", "zeit", "/", "der", "Er\u00b7den", "ne\u00b7wes", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Den Menschen newe lust/ den B\u00e4umen newe zier.", "tokens": ["Den", "Men\u00b7schen", "ne\u00b7we", "lust", "/", "den", "B\u00e4u\u00b7men", "ne\u00b7we", "zier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+---+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Vnd Fr\u00fcchte wider schenckt/ doch trawrt jhr f\u00fcr vnd f\u00fcr/", "tokens": ["Vnd", "Fr\u00fcch\u00b7te", "wi\u00b7der", "schenckt", "/", "doch", "trawrt", "jhr", "f\u00fcr", "vnd", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "APPR", "KON", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd wolt Euch Jungfraw Brautt zu keiner Lust erheben!", "tokens": ["Vnd", "wolt", "Euch", "Jung\u00b7fraw", "Brautt", "zu", "kei\u00b7ner", "Lust", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie da\u00df man auch doch siht in stetten schmertzen schweben?", "tokens": ["Wie", "da\u00df", "man", "auch", "doch", "siht", "in", "stet\u00b7ten", "schmert\u00b7zen", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "ADV", "ADV", "VVFIN", "APPR", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo r\u00fchrt dis Vbel her? Mich d\u00fcnckt/ ich mercke schier/", "tokens": ["Wo", "r\u00fchrt", "dis", "Vbel", "her", "?", "Mich", "d\u00fcnckt", "/", "ich", "mer\u00b7cke", "schier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDS", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den vrsprung aller pein; dir/ dir Cupido dir/", "tokens": ["Den", "vr\u00b7sprung", "al\u00b7ler", "pein", ";", "dir", "/", "dir", "Cu\u00b7pi\u00b7do", "dir", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$.", "PPER", "$(", "PPER", "NE", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schreibt man die plage zu! Doch kan ich rettung geben/", "tokens": ["Schreibt", "man", "die", "pla\u00b7ge", "zu", "!", "Doch", "kan", "ich", "ret\u00b7tung", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKVZ", "$.", "KON", "VMFIN", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So sprach Er; vnd warff Pfeil vnd Fackel au\u00df der hand", "tokens": ["So", "sprach", "Er", ";", "vnd", "warff", "Pfeil", "vnd", "Fa\u00b7ckel", "au\u00df", "der", "hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "VVFIN", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd wagt sich auff die See der Mutter Vaterland", "tokens": ["Vnd", "wagt", "sich", "auff", "die", "See", "der", "Mut\u00b7ter", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als da er (was jhr w\u00fcnd scht/) den Hering hat gefangen.", "tokens": ["Als", "da", "er", "(", "was", "jhr", "w\u00fcnd", "scht", "/", ")", "den", "He\u00b7ring", "hat", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "$(", "PWS", "PPER", "ADJD", "VVFIN", "$(", "$(", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "O mehr denn frembder fall/ Sol di\u00df ein mittel seyn/", "tokens": ["O", "mehr", "denn", "fremb\u00b7der", "fall", "/", "Sol", "di\u00df", "ein", "mit\u00b7tel", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJA", "NN", "$(", "VMFIN", "PDS", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Seuch vnd Feber bringt! Ja schrie Er/ di\u00df allein", "tokens": ["Was", "Seuch", "vnd", "Fe\u00b7ber", "bringt", "!", "Ja", "schrie", "Er", "/", "di\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN", "$.", "PTKANT", "VVFIN", "PPER", "$(", "PDS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist was die Krancke sucht vnd was sie sol empfangen.", "tokens": ["Ist", "was", "die", "Kran\u00b7cke", "sucht", "vnd", "was", "sie", "sol", "emp\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "VVFIN", "KON", "PWS", "PPER", "VMFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}