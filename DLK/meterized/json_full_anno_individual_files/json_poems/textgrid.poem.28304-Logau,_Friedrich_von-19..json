{"textgrid.poem.28304": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist Gl\u00fccke wo und was, so halt ich mir f\u00fcr Gl\u00fccke,", "tokens": ["Ist", "Gl\u00fc\u00b7cke", "wo", "und", "was", ",", "so", "halt", "ich", "mir", "f\u00fcr", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PWAV", "KON", "PWS", "$,", "ADV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann ich mein eigen bin, da\u00df ich kein dienstbar Ohr", "tokens": ["Wann", "ich", "mein", "ei\u00b7gen", "bin", ",", "da\u00df", "ich", "kein", "dienst\u00b7bar", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Um weg verkauffte Pflicht darff recken hoch empor", "tokens": ["Um", "weg", "ver\u00b7kauff\u00b7te", "Pflicht", "darff", "re\u00b7cken", "hoch", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "ADJA", "NN", "VMFIN", "VVINF", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und horchen auff Befehl. Da\u00df mich der Neid ber\u00fccke,", "tokens": ["Und", "hor\u00b7chen", "auff", "Be\u00b7fehl", ".", "Da\u00df", "mich", "der", "Neid", "be\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da bin ich Sorgen-los; die schmale st\u00fcrtze-Br\u00fccke,", "tokens": ["Da", "bin", "ich", "Sor\u00b7gen\u00b7los", ";", "die", "schma\u00b7le", "st\u00fcrt\u00b7ze\u00b7Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Darauff nach Gunst man zeucht, die bringt mir nicht Gefahr;", "tokens": ["Dar\u00b7auff", "nach", "Gunst", "man", "zeucht", ",", "die", "bringt", "mir", "nicht", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NE", "PIS", "VVFIN", "$,", "PRELS", "VVFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich stehe, wo ich steh und bleibe, wo ich war.", "tokens": ["Ich", "ste\u00b7he", ",", "wo", "ich", "steh", "und", "blei\u00b7be", ",", "wo", "ich", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Ehre scheinlich Gifft, de\u00df Hofes Meisterst\u00fccke", "tokens": ["Der", "Eh\u00b7re", "schein\u00b7lich", "Gifft", ",", "de\u00df", "Ho\u00b7fes", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was gehen die mich an? Gut! da\u00df mir das vergn\u00fcgen", "tokens": ["Was", "ge\u00b7hen", "die", "mich", "an", "?", "Gut", "!", "da\u00df", "mir", "das", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "PPER", "PTKVZ", "$.", "NN", "$.", "KOUS", "PPER", "PDS", "VVINF"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "F\u00fcr grosse W\u00fcrde gilt! mir ist ja noch so wol,", "tokens": ["F\u00fcr", "gros\u00b7se", "W\u00fcr\u00b7de", "gilt", "!", "mir", "ist", "ja", "noch", "so", "wol", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als dem der Wanst zerschw\u00fcllt, dieweil er Hoffart voll;", "tokens": ["Als", "dem", "der", "Wanst", "zer\u00b7schw\u00fcllt", ",", "die\u00b7weil", "er", "Hof\u00b7fart", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer biegen sich nicht kan, bleibt, wann er f\u00e4llet, liegen.", "tokens": ["Wer", "bie\u00b7gen", "sich", "nicht", "kan", ",", "bleibt", ",", "wann", "er", "f\u00e4l\u00b7let", ",", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PTKNEG", "VMFIN", "$,", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nach Purpur tracht ich nicht; ich neme weit daf\u00f