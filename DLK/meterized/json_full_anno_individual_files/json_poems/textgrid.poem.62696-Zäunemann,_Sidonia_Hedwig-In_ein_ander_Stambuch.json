{"textgrid.poem.62696": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "In ein ander Stambuch", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort sagt ein Cazique: Gold ist der Christen Gott!", "tokens": ["Dort", "sagt", "ein", "Ca\u00b7zi\u00b7que", ":", "Gold", "ist", "der", "Chris\u00b7ten", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ja wohl! Es wird sein Dienst durch Heucheley verehret;", "tokens": ["Ja", "wohl", "!", "Es", "wird", "sein", "Dienst", "durch", "Heu\u00b7che\u00b7ley", "ver\u00b7eh\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn Geitz den G\u00f6tzendienst aufs m\u00e4chtigste vermehret,", "tokens": ["Wenn", "Geitz", "den", "G\u00f6t\u00b7zen\u00b7dienst", "aufs", "m\u00e4ch\u00b7tigs\u00b7te", "ver\u00b7meh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So dr\u00e4nget sich zu ihm der Diebe starke Rott.", "tokens": ["So", "dr\u00e4n\u00b7get", "sich", "zu", "ihm", "der", "Die\u00b7be", "star\u00b7ke", "Rott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vor seinem Haus dem Kasten", "tokens": ["Vor", "sei\u00b7nem", "Haus", "dem", "Kas\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Will man mit Andacht rasten.", "tokens": ["Will", "man", "mit", "An\u00b7dacht", "ras\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ist das nicht Schand und Spott/", "tokens": ["Ist", "das", "nicht", "Schand", "und", "Spott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wenn dort ein Heyde sagt: Gold ist der Christen Gott.", "tokens": ["Wenn", "dort", "ein", "Hey\u00b7de", "sagt", ":", "Gold", "ist", "der", "Chris\u00b7ten", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$.", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Dort sagt ein Cazique: Gold ist der Christen Gott!", "tokens": ["Dort", "sagt", "ein", "Ca\u00b7zi\u00b7que", ":", "Gold", "ist", "der", "Chris\u00b7ten", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ja wohl! Es wird sein Dienst durch Heucheley verehret;", "tokens": ["Ja", "wohl", "!", "Es", "wird", "sein", "Dienst", "