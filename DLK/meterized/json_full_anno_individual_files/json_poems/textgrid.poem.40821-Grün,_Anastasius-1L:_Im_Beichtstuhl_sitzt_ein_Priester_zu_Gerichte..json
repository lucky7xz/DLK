{"textgrid.poem.40821": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Beichtstuhl sitzt ein Priester zu Gerichte.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Beichtstuhl sitzt ein Priester zu Gerichte.", "tokens": ["Im", "Beicht\u00b7stuhl", "sitzt", "ein", "Pries\u00b7ter", "zu", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Glaubt nicht des J\u00fcnglings ros'gem Angesichte!", "tokens": ["Glaubt", "nicht", "des", "J\u00fcng\u00b7lings", "ros'\u00b7gem", "An\u00b7ge\u00b7sich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Eisfeld ist sein Herz, das kalte, rauhe,", "tokens": ["Ein", "Eis\u00b7feld", "ist", "sein", "Herz", ",", "das", "kal\u00b7te", ",", "rau\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Spiegel, drin sich nur der Himmel schaue!", "tokens": ["Ein", "Spie\u00b7gel", ",", "drin", "sich", "nur", "der", "Him\u00b7mel", "schau\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PRF", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und eine W\u00fcste ist's, die schrankenlose,", "tokens": ["Und", "ei\u00b7ne", "W\u00fcs\u00b7te", "ist's", ",", "die", "schran\u00b7ken\u00b7lo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die \u00f6de, kahle, ohne Quell und Rose,", "tokens": ["Die", "\u00f6\u00b7de", ",", "kah\u00b7le", ",", "oh\u00b7ne", "Quell", "und", "Ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "VVFIN", "$,", "KOUI", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Draus nur die Pyramide \u00bbGott\u00ab sich hebet,", "tokens": ["Draus", "nur", "die", "Py\u00b7ra\u00b7mi\u00b7de", "\u00bb", "Gott", "\u00ab", "sich", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "$(", "NN", "$(", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch einsam, d\u00fcster, grau und unbelebet.", "tokens": ["Doch", "ein\u00b7sam", ",", "d\u00fcs\u00b7ter", ",", "grau", "und", "un\u00b7be\u00b7le\u00b7bet", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein lockig M\u00e4gdlein kniet zu seinen F\u00fc\u00dfen,", "tokens": ["Ein", "lo\u00b7ckig", "M\u00e4gd\u00b7lein", "kniet", "zu", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Herz ihm ganz und reuig aufzuschlie\u00dfen;", "tokens": ["Ihr", "Herz", "ihm", "ganz", "und", "reu\u00b7ig", "auf\u00b7zu\u00b7schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drin hat die S\u00fcnd' ein G\u00e4rtlein, ein gar sch\u00f6nes,", "tokens": ["Drin", "hat", "die", "S\u00fcnd'", "ein", "G\u00e4rt\u00b7lein", ",", "ein", "gar", "sch\u00f6\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,", "ART", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Voll Rosenhecken und voll Quellget\u00f6nes.", "tokens": ["Voll", "Ro\u00b7sen\u00b7he\u00b7cken", "und", "voll", "Quell\u00b7ge\u00b7t\u00f6\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun ihre Worte den Bericht beginnen", "tokens": ["Nun", "ih\u00b7re", "Wor\u00b7te", "den", "Be\u00b7richt", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und von den ros'gen Lippen lispelnd rinnen,", "tokens": ["Und", "von", "den", "ros'\u00b7gen", "Lip\u00b7pen", "lis\u00b7pelnd", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da wird es ihm, als ries'le eine Quelle", "tokens": ["Da", "wird", "es", "ihm", ",", "als", "ries'\u00b7le", "ei\u00b7ne", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KOUS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch seinen W\u00fcstensand ganz frisch und helle.", "tokens": ["Durch", "sei\u00b7nen", "W\u00fcs\u00b7ten\u00b7sand", "ganz", "frisch", "und", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wie sie fl\u00fcsternd spricht von sel'gen Lauben,", "tokens": ["Und", "wie", "sie", "fl\u00fcs\u00b7ternd", "spricht", "von", "sel'\u00b7gen", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVPP", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da mochte wohl mit Fug der Arme glauben,", "tokens": ["Da", "moch\u00b7te", "wohl", "mit", "Fug", "der", "Ar\u00b7me", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es habe Lenz mit seinen Rosen allen", "tokens": ["Es", "ha\u00b7be", "Lenz", "mit", "sei\u00b7nen", "Ro\u00b7sen", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Gletscher bombardirend \u00fcberfallen.", "tokens": ["Den", "Glet\u00b7scher", "bom\u00b7bar\u00b7di\u00b7rend", "\u00fc\u00b7berf\u00b7al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Das M\u00e4dchen schritt ents\u00fchnt schon l\u00e4ngst von hinnen,", "tokens": ["Das", "M\u00e4d\u00b7chen", "schritt", "ent\u00b7s\u00fchnt", "schon", "l\u00e4ngst", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ADV", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er lehnt im Stuhle noch in tiefem Sinnen,", "tokens": ["Er", "lehnt", "im", "Stuh\u00b7le", "noch", "in", "tie\u00b7fem", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ums\u00e4uselt still von keimenden Gedanken;", "tokens": ["Um\u00b7s\u00e4u\u00b7selt", "still", "von", "kei\u00b7men\u00b7den", "Ge\u00b7dan\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Pyramide, ach, beginnt zu wanken!", "tokens": ["Die", "Py\u00b7ra\u00b7mi\u00b7de", ",", "ach", ",", "be\u00b7ginnt", "zu", "wan\u00b7ken", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ITJ", "$,", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und aus den wiedergr\u00fcnen W\u00fcstenschollen", "tokens": ["Und", "aus", "den", "wie\u00b7der\u00b7gr\u00fc\u00b7nen", "W\u00fcs\u00b7ten\u00b7schol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist Bl\u00fcth' an Bl\u00fcth' und Zweig an Zweig entquollen,", "tokens": ["Ist", "Bl\u00fcth'", "an", "Bl\u00fcth'", "und", "Zweig", "an", "Zweig", "ent\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Laube k\u00fchl und lind sein Haupt umd\u00fcsternd,", "tokens": ["Als", "Lau\u00b7be", "k\u00fchl", "und", "lind", "sein", "Haupt", "um\u00b7d\u00fcs\u00b7ternd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Viel s\u00fc\u00dfe, heil'ge Wonnem\u00e4rchen fl\u00fcsternd.", "tokens": ["Viel", "s\u00fc\u00b7\u00dfe", ",", "heil'\u00b7ge", "Won\u00b7ne\u00b7m\u00e4r\u00b7chen", "fl\u00fcs\u00b7ternd", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und an den Zweigen gaukelnd auf und nieder,", "tokens": ["Und", "an", "den", "Zwei\u00b7gen", "gau\u00b7kelnd", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Singt eine Nachtigall gar seltne Lieder:", "tokens": ["Singt", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall", "gar", "selt\u00b7ne", "Lie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist sein Herz! \u2013 Wenn Nachtigallen schlagen,", "tokens": ["Es", "ist", "sein", "Herz", "!", "\u2013", "Wenn", "Nach\u00b7ti\u00b7gal\u00b7len", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$.", "$(", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer wei\u00df, ist's Jauchzen, ist's ein stilles Klagen?", "tokens": ["Wer", "wei\u00df", ",", "ist's", "Jauch\u00b7zen", ",", "ist's", "ein", "stil\u00b7les", "Kla\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VAFIN", "NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Im Beichtstuhl sitzt ein Priester zu Gerichte.", "tokens": ["Im", "Beicht\u00b7stuhl", "sitzt", "ein", "Pries\u00b7ter", "zu", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Glaubt nicht des J\u00fcnglings ros'gem Angesichte!", "tokens": ["Glaubt", "nicht", "des", "J\u00fcng\u00b7lings", "ros'\u00b7gem", "An\u00b7ge\u00b7sich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Eisfeld ist sein Herz, das kalte, rauhe,", "tokens": ["Ein", "Eis\u00b7feld", "ist", "sein", "Herz", ",", "das", "kal\u00b7te", ",", "rau\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ART", "ADJA