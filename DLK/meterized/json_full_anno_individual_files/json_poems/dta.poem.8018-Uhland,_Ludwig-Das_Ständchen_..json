{"dta.poem.8018": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das St\u00e4ndchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Was wecken aus dem Schlummer mich", "tokens": ["Was", "we\u00b7cken", "aus", "dem", "Schlum\u00b7mer", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr s\u00fc\u00dfe Kl\u00e4nge doch?", "tokens": ["F\u00fcr", "s\u00fc\u00b7\u00dfe", "Kl\u00e4n\u00b7ge", "doch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Mutter, sieh! wer mag es seyn,", "tokens": ["O", "Mut\u00b7ter", ",", "sieh", "!", "wer", "mag", "es", "seyn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "$.", "PWS", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In sp\u00e4ter Stunde noch?", "tokens": ["In", "sp\u00e4\u00b7ter", "Stun\u00b7de", "noch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201eich h\u00f6re nichts, ich sehe nichts,", "tokens": ["\u201e", "ich", "h\u00f6\u00b7re", "nichts", ",", "ich", "se\u00b7he", "nichts", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PIS", "$,", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O schlummre fort so lind!", "tokens": ["O", "schlumm\u00b7re", "fort", "so", "lind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man bringt dir keine St\u00e4ndchen jetzt,", "tokens": ["Man", "bringt", "dir", "kei\u00b7ne", "St\u00e4nd\u00b7chen", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du armes, krankes Kind!\u201c", "tokens": ["Du", "ar\u00b7mes", ",", "kran\u00b7kes", "Kind", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist nicht irdische Musik,", "tokens": ["Es", "ist", "nicht", "ir\u00b7di\u00b7sche", "Mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mich so freudig macht;", "tokens": ["Was", "mich", "so", "freu\u00b7dig", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich rufen Engel mit Gesang.", "tokens": ["Mich", "ru\u00b7fen", "En\u00b7gel", "mit", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Mutter, gute Nacht!", "tokens": ["O", "Mut\u00b7ter", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}