{"textgrid.poem.46618": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[als ich vor'gen Herbst mit euern]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich vor'gen Herbst mit euern", "tokens": ["Als", "ich", "vor'\u00b7gen", "Herbst", "mit", "eu\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aeltern Br\u00fcdern ausgezogen", "tokens": ["A\u00b7el\u00b7tern", "Br\u00fc\u00b7dern", "aus\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu der Weinles' Abenteuern,", "tokens": ["Zu", "der", "Wein\u00b7les'", "A\u00b7bent\u00b7eu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zu Hause bei der Mutter", "tokens": ["Und", "zu", "Hau\u00b7se", "bei", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Euch zwei Kleinste mu\u00dfte lassen;", "tokens": ["Euch", "zwei", "Kleins\u00b7te", "mu\u00df\u00b7te", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dachten wir bei jeder reifsten", "tokens": ["Dach\u00b7ten", "wir", "bei", "je\u00b7der", "reifs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sch\u00f6nsten Traube, die wir pfl\u00fcckten \u2013", "tokens": ["Sch\u00f6ns\u00b7ten", "Trau\u00b7be", ",", "die", "wir", "pfl\u00fcck\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder, recht zu sagen, dacht' ich,", "tokens": ["O\u00b7der", ",", "recht", "zu", "sa\u00b7gen", ",", "dacht'", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weil an nichts die Knaben denken \u2013", "tokens": ["Weil", "an", "nichts", "die", "Kna\u00b7ben", "den\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIS", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wie euch w\u00fcrd' ein Beerchen schmecken,", "tokens": ["Wie", "euch", "w\u00fcrd'", "ein", "Beer\u00b7chen", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wenn ihr jetzt daheim es h\u00e4ttet;", "tokens": ["Wenn", "ihr", "jetzt", "da\u00b7heim", "es", "h\u00e4t\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPER", "VAFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "K\u00f6nnt' ich's in den Mund euch stecken!", "tokens": ["K\u00f6nnt'", "ich's", "in", "den", "Mund", "euch", "ste\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und dann von der Lese brachten", "tokens": ["Und", "dann", "von", "der", "Le\u00b7se", "brach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Wir die sch\u00f6nsten, nicht die reifsten,", "tokens": ["Wir", "die", "sch\u00f6ns\u00b7ten", ",", "nicht", "die", "reifs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "$,", "PTKNEG", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Weil die reifsten sich nicht halten", "tokens": ["Weil", "die", "reifs\u00b7ten", "sich", "nicht", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "VVFIN", "PRF", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und sich nicht verf\u00fchren lassen,", "tokens": ["Und", "sich", "nicht", "ver\u00b7f\u00fch\u00b7ren", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Brachten wir die sch\u00f6nsten Tr\u00e4ubchen", "tokens": ["Brach\u00b7ten", "wir", "die", "sch\u00f6ns\u00b7ten", "Tr\u00e4ub\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Euch mit heim, ihr sch\u00f6nsten T\u00e4ubchen.", "tokens": ["Euch", "mit", "heim", ",", "ihr", "sch\u00f6ns\u00b7ten", "T\u00e4ub\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "O wie mir das Beerchen schmeckte,", "tokens": ["O", "wie", "mir", "das", "Beer\u00b7chen", "schmeck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Das ich den Mund euch steckte!", "tokens": ["Das", "ich", "den", "Mund", "euch", "steck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Und als ob ein Kern der Traube", "tokens": ["Und", "als", "ob", "ein", "Kern", "der", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOKOM", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Vor mir auf den Boden fiele,", "tokens": ["Vor", "mir", "auf", "den", "Bo\u00b7den", "fie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Wurzeln schl\u00fcg' und w\u00fcrd' ein Weinstock,", "tokens": ["Wur\u00b7zeln", "schl\u00fcg'", "und", "w\u00fcrd'", "ein", "Wein\u00b7stock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "So versetzte mich mein Glaube", "tokens": ["So", "ver\u00b7setz\u00b7te", "mich", "mein", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "In die Zukunft goldner Zeiten,", "tokens": ["In", "die", "Zu\u00b7kunft", "gold\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "In die sch\u00f6nste Rebenlaube,", "tokens": ["In", "die", "sch\u00f6ns\u00b7te", "Re\u00b7ben\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wo ihr stundet mir zur Seiten,", "tokens": ["Wo", "ihr", "stun\u00b7det", "mir", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Pfl\u00fcckend aus dem Laub die Traube,", "tokens": ["Pfl\u00fc\u00b7ckend", "aus", "dem", "Laub", "die", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Pressend Wein, und mir kredenzend,", "tokens": ["Pres\u00b7send", "Wein", ",", "und", "mir", "kre\u00b7den\u00b7zend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "PPER", "ADJD", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.30": {"text": "Und mit Rebenlaub mich kr\u00e4nzend,", "tokens": ["Und", "mit", "Re\u00b7ben\u00b7laub", "mich", "kr\u00e4n\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Beide gro\u00df und hochgestaltet,", "tokens": ["Bei\u00b7de", "gro\u00df", "und", "hoch\u00b7ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Bl\u00fchend jugendlich entfaltet,", "tokens": ["Bl\u00fc\u00b7hend", "ju\u00b7gend\u00b7lich", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Du, mein M\u00e4dchen, eine Hebe,", "tokens": ["Du", ",", "mein", "M\u00e4d\u00b7chen", ",", "ei\u00b7ne", "He\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Du, mein Kn\u00e4bchen, ein Ephebe,", "tokens": ["Du", ",", "mein", "Kn\u00e4b\u00b7chen", ",", "ein", "E\u00b7phe\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Oder du mein Ganymedchen", "tokens": ["O\u00b7der", "du", "mein", "Ga\u00b7ny\u00b7med\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Sie mein Ganymedes-M\u00e4dchen.", "tokens": ["Sie", "mein", "Ga\u00b7ny\u00b7me\u00b7des\u00b7M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "O der Aussicht goldner Weiten!", "tokens": ["O", "der", "Aus\u00b7sicht", "gold\u00b7ner", "Wei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Und nun seid ihr mir entrissen,", "tokens": ["Und", "nun", "seid", "ihr", "mir", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Soll ich meine Zukunft missen?", "tokens": ["Soll", "ich", "mei\u00b7ne", "Zu\u00b7kunft", "mis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Was bedarf es ferner Zeiten?", "tokens": ["Was", "be\u00b7darf", "es", "fer\u00b7ner", "Zei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Von des Lebens Thau begossen,", "tokens": ["Von", "des", "Le\u00b7bens", "Thau", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Uebernacht emporgeschossen,", "tokens": ["Ue\u00b7ber\u00b7nacht", "em\u00b7por\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Was mein Ferngesicht bedeute,", "tokens": ["Was", "mein", "Fern\u00b7ge\u00b7sicht", "be\u00b7deu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Seid ihr schon geworden heute.", "tokens": ["Seid", "ihr", "schon", "ge\u00b7wor\u00b7den", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "VAPP", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Ja, dies Jahr noch will ich sehen", "tokens": ["Ja", ",", "dies", "Jahr", "noch", "will", "ich", "se\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PDS", "NN", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Euch mir so zur Seite stehen,", "tokens": ["Euch", "mir", "so", "zur", "Sei\u00b7te", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "In der Laube mir kredenzend,", "tokens": ["In", "der", "Lau\u00b7be", "mir", "kre\u00b7den\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.48": {"text": "Und mit L\u00e4cheln mich bekr\u00e4nzend,", "tokens": ["Und", "mit", "L\u00e4\u00b7cheln", "mich", "be\u00b7kr\u00e4n\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Du, mein M\u00e4dchen, eine Hebe,", "tokens": ["Du", ",", "mein", "M\u00e4d\u00b7chen", ",", "ei\u00b7ne", "He\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Du, mein Kn\u00e4bchen, ein Ephebe,", "tokens": ["Du", ",", "mein", "Kn\u00e4b\u00b7chen", ",", "ein", "E\u00b7phe\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Oder du mein Ganymedchen,", "tokens": ["O\u00b7der", "du", "mein", "Ga\u00b7ny\u00b7med\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Du mein Ganymeda-M\u00e4dchen.", "tokens": ["Du", "mein", "Ga\u00b7ny\u00b7me\u00b7da\u00b7M\u00e4d\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Als ich vor'gen Herbst mit euern", "tokens": ["Als", "ich", "vor'\u00b7gen", "Herbst", "mit", "eu\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aeltern Br\u00fcdern ausgezogen", "tokens": ["A\u00b7el\u00b7tern", "Br\u00fc\u00b7dern", "aus\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu der Weinles' Abenteuern,", "tokens": ["Zu", "der", "Wein\u00b7les'", "A\u00b7bent\u00b7eu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zu Hause bei der Mutter", "tokens": ["Und", "zu", "Hau\u00b7se", "bei", "der", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Euch zwei Kleinste mu\u00dfte lassen;", "tokens": ["Euch", "zwei", "Kleins\u00b7te", "mu\u00df\u00b7te", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dachten wir bei jeder reifsten", "tokens": ["Dach\u00b7ten", "wir", "bei", "je\u00b7der", "reifs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sch\u00f6nsten Traube, die wir pfl\u00fcckten \u2013", "tokens": ["Sch\u00f6ns\u00b7ten", "Trau\u00b7be", ",", "die", "wir", "pfl\u00fcck\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder, recht zu sagen, dacht' ich,", "tokens": ["O\u00b7der", ",", "recht", "zu", "sa\u00b7gen", ",", "dacht'", "ich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weil an nichts die Knaben denken \u2013", "tokens": ["Weil", "an", "nichts", "die", "Kna\u00b7ben", "den\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIS", "ART", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Wie euch w\u00fcrd' ein Beerchen schmecken,", "tokens": ["Wie", "euch", "w\u00fcrd'", "ein", "Beer\u00b7chen", "schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wenn ihr jetzt daheim es h\u00e4ttet;", "tokens": ["Wenn", "ihr", "jetzt", "da\u00b7heim", "es", "h\u00e4t\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPER", "VAFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.12": {"text": "K\u00f6nnt' ich's in den Mund euch stecken!", "tokens": ["K\u00f6nnt'", "ich's", "in", "den", "Mund", "euch", "ste\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und dann von der Lese brachten", "tokens": ["Und", "dann", "von", "der", "Le\u00b7se", "brach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Wir die sch\u00f6nsten, nicht die reifsten,", "tokens": ["Wir", "die", "sch\u00f6ns\u00b7ten", ",", "nicht", "die", "reifs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "$,", "PTKNEG", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Weil die reifsten sich nicht halten", "tokens": ["Weil", "die", "reifs\u00b7ten", "sich", "nicht", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "VVFIN", "PRF", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und sich nicht verf\u00fchren lassen,", "tokens": ["Und", "sich", "nicht", "ver\u00b7f\u00fch\u00b7ren", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Brachten wir die sch\u00f6nsten Tr\u00e4ubchen", "tokens": ["Brach\u00b7ten", "wir", "die", "sch\u00f6ns\u00b7ten", "Tr\u00e4ub\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Euch mit heim, ihr sch\u00f6nsten T\u00e4ubchen.", "tokens": ["Euch", "mit", "heim", ",", "ihr", "sch\u00f6ns\u00b7ten", "T\u00e4ub\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "O wie mir das Beerchen schmeckte,", "tokens": ["O", "wie", "mir", "das", "Beer\u00b7chen", "schmeck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Das ich den Mund euch steckte!", "tokens": ["Das", "ich", "den", "Mund", "euch", "steck\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Und als ob ein Kern der Traube", "tokens": ["Und", "als", "ob", "ein", "Kern", "der", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOKOM", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Vor mir auf den Boden fiele,", "tokens": ["Vor", "mir", "auf", "den", "Bo\u00b7den", "fie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Wurzeln schl\u00fcg' und w\u00fcrd' ein Weinstock,", "tokens": ["Wur\u00b7zeln", "schl\u00fcg'", "und", "w\u00fcrd'", "ein", "Wein\u00b7stock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "So versetzte mich mein Glaube", "tokens": ["So", "ver\u00b7setz\u00b7te", "mich", "mein", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "In die Zukunft goldner Zeiten,", "tokens": ["In", "die", "Zu\u00b7kunft", "gold\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "In die sch\u00f6nste Rebenlaube,", "tokens": ["In", "die", "sch\u00f6ns\u00b7te", "Re\u00b7ben\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Wo ihr stundet mir zur Seiten,", "tokens": ["Wo", "ihr", "stun\u00b7det", "mir", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Pfl\u00fcckend aus dem Laub die Traube,", "tokens": ["Pfl\u00fc\u00b7ckend", "aus", "dem", "Laub", "die", "Trau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Pressend Wein, und mir kredenzend,", "tokens": ["Pres\u00b7send", "Wein", ",", "und", "mir", "kre\u00b7den\u00b7zend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "PPER", "ADJD", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.30": {"text": "Und mit Rebenlaub mich kr\u00e4nzend,", "tokens": ["U