{"dta.poem.15001": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Was uns bleibt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn der G\u00f6tter Stimme tr\u00fcgt,", "tokens": ["Wenn", "der", "G\u00f6t\u00b7ter", "Stim\u00b7me", "tr\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Menschheit Wunden sich nicht r\u00e4chen,", "tokens": ["Wenn", "der", "Menschheit", "Wun\u00b7den", "sich", "nicht", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn das heiligste Vertrauen l\u00fcgt,", "tokens": ["Wenn", "das", "hei\u00b7ligs\u00b7te", "Ver\u00b7trau\u00b7en", "l\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn umsonst die aufgeblitzte Jugend", "tokens": ["Wenn", "um\u00b7sonst", "die", "auf\u00b7ge\u00b7blitz\u00b7te", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Um des Vaterlandes Kerker st\u00fcrmt,", "tokens": ["Um", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Ker\u00b7ker", "st\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und des Volkes Spartergleiche Tugend", "tokens": ["Und", "des", "Vol\u00b7kes", "Spar\u00b7ter\u00b7glei\u00b7che", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Fruchtlos Leichen \u00fcber Leichen th\u00fcrmt? \u2014", "tokens": ["Frucht\u00b7los", "Lei\u00b7chen", "\u00fc\u00b7ber", "Lei\u00b7chen", "th\u00fcrmt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Was uns bleibt, wenn wir trotz unserm Rechte", "tokens": ["Was", "uns", "bleibt", ",", "wenn", "wir", "trotz", "un\u00b7serm", "Rech\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Knirschend vor dem falschen Gl\u00fccke stehn,", "tokens": ["Knir\u00b7schend", "vor", "dem", "fal\u00b7schen", "Gl\u00fc\u00b7cke", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Und des W\u00fcthrichs feile Henkersknechte", "tokens": ["Und", "des", "W\u00fcth\u00b7richs", "fei\u00b7le", "Hen\u00b7kers\u00b7knech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Mordend durch der Freiheit Tempel gehn? \u2014", "tokens": ["Mor\u00b7dend", "durch", "der", "Frei\u00b7heit", "Tem\u00b7pel", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Was uns bleibt, wenn unser Blut vergebens", "tokens": ["Was", "uns", "bleibt", ",", "wenn", "un\u00b7ser", "Blut", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Auf des Vaterlandes Grab verraucht,", "tokens": ["Auf", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Grab", "ver\u00b7raucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Und der Freiheit Stern, der ", "tokens": ["Und", "der", "Frei\u00b7heit", "Stern", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "NN", "$,", "PRELS"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "An dem deutschen Himmel niedertaucht? \u2014", "tokens": ["An", "dem", "deut\u00b7schen", "Him\u00b7mel", "nie\u00b7der\u00b7taucht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Was uns bleibt? R\u00fchmt nicht des Wissens Bronnen,", "tokens": ["Was", "uns", "bleibt", "?", "R\u00fchmt", "nicht", "des", "Wis\u00b7sens", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "NE", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Nicht der K\u00fcnste friedensreichen Strand;", "tokens": ["Nicht", "der", "K\u00fcns\u00b7te", "frie\u00b7dens\u00b7rei\u00b7chen", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "F\u00fcr die Knechte giebt es keine Sonnen,", "tokens": ["F\u00fcr", "die", "Knech\u00b7te", "giebt", "es", "kei\u00b7ne", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Und die Kunst verlangt ein Vaterland!", "tokens": ["Und", "die", "Kunst", "ver\u00b7langt", "ein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Aller G\u00f6tter Stimmen sind verklungen,", "tokens": ["Al\u00b7ler", "G\u00f6t\u00b7ter", "Stim\u00b7men", "sind", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Vor dem Jammerton der Sklaverei, \u2014", "tokens": ["Vor", "dem", "Jam\u00b7mer\u00b7ton", "der", "Skla\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Und Homer, er h\u00e4tte nie gesungen,", "tokens": ["Und", "Ho\u00b7mer", ",", "er", "h\u00e4t\u00b7te", "nie", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Doch sein Griechenland war frei! \u2014", "tokens": ["Doch", "sein", "Grie\u00b7chen\u00b7land", "war", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Was uns bleibt? ein christliches Ertragen,", "tokens": ["Was", "uns", "bleibt", "?", "ein", "christ\u00b7li\u00b7ches", "Er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Wo des Dulders feige Thr\u00e4ne thaut? \u2014", "tokens": ["Wo", "des", "Dul\u00b7ders", "fei\u00b7ge", "Thr\u00e4\u00b7ne", "thaut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Soll ich selbst den Altar mir zerschlagen,", "tokens": ["Soll", "ich", "selbst", "den", "Al\u00b7tar", "mir", "zer\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "Den ich mir im Herzen aufgebaut?", "tokens": ["Den", "ich", "mir", "im", "Her\u00b7zen", "auf\u00b7ge\u00b7baut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Soll ich das f\u00fcr Gottes Finger halten,", "tokens": ["Soll", "ich", "das", "f\u00fcr", "Got\u00b7tes", "Fin\u00b7ger", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.29": {"text": "Wo der Menschheit Engel Rache schrein?", "tokens": ["Wo", "der", "Menschheit", "En\u00b7gel", "Ra\u00b7che", "schrein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Wo die Teufel teuflisch walten,", "tokens": ["Wo", "die", "Teu\u00b7fel", "teuf\u00b7lisch", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Das kann nur ein Sieg der H\u00f6lle seyn! \u2014", "tokens": ["Das", "kann", "nur", "ein", "Sieg", "der", "H\u00f6l\u00b7le", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ART", "NN", "ART", "NN", "VAINF", "$.", "$("], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.32": {"text": "Bleibt uns nichts? \u2014 Fliehn alle gute Engel", "tokens": ["Bleibt", "uns", "nichts", "?", "Fliehn", "al\u00b7le", "gu\u00b7te", "En\u00b7gel"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "$.", "$(", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Mit verwandtem Angesicht?", "tokens": ["Mit", "ver\u00b7wand\u00b7tem", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Brechen aller Hoffnung Bl\u00fcthenstengel,", "tokens": ["Bre\u00b7chen", "al\u00b7ler", "Hoff\u00b7nung", "Bl\u00fc\u00b7thens\u00b7ten\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.35": {"text": "Weil des Sieges Palme bricht?", "tokens": ["Weil", "des", "Sie\u00b7ges", "Pal\u00b7me", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Kann der Arm kein rettend Kreuz umarmen,", "tokens": ["Kann", "der", "Arm", "kein", "ret\u00b7tend", "Kreuz", "um\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PIAT", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "In der h\u00f6chsten letzten Noth,", "tokens": ["In", "der", "h\u00f6chs\u00b7ten", "letz\u00b7ten", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "M\u00fcssen wir verzweifeln und verjammern,", "tokens": ["M\u00fcs\u00b7sen", "wir", "ver\u00b7zwei\u00b7feln", "und", "ver\u00b7jam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.39": {"text": "Giebt es keine Freiheit, als den Tod?", "tokens": ["Giebt", "es", "kei\u00b7ne", "Frei\u00b7heit", ",", "als", "den", "Tod", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "Doch! \u2014 wir sehn's im Aufschwung unserer Jugend,", "tokens": ["Doch", "!", "wir", "sehn's", "im", "Auf\u00b7schwung", "un\u00b7se\u00b7rer", "Ju\u00b7gend", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$.", "$(", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "In des ganzen Volkes Heldengeist,", "tokens": ["In", "des", "gan\u00b7zen", "Vol\u00b7kes", "Hel\u00b7den\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.42": {"text": "Ja! es giebt noch eine ", "tokens": ["Ja", "!", "es", "giebt", "noch", "ei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "ADV", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.43": {"text": "Die allm\u00e4chtig einst die Ketten rei\u00dft.", "tokens": ["Die", "all\u00b7m\u00e4ch\u00b7tig", "einst", "die", "Ket\u00b7ten", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.44": {"text": "Wenn auch jetzt in den bezwungnen Hallen", "tokens": ["Wenn", "auch", "jetzt", "in", "den", "be\u00b7zwung\u00b7nen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "Tyranney der Freiheit Tempel bricht, \u2014", "tokens": ["Ty\u00b7ran\u00b7ney", "der", "Frei\u00b7heit", "Tem\u00b7pel", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "NN", "VVFIN", "$,", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Deutsches Volk, du konntest fallen,", "tokens": ["Deut\u00b7sches", "Volk", ",", "du", "konn\u00b7test", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Und noch lebt der Hoffnung Himmelsfunken!", "tokens": ["Und", "noch", "lebt", "der", "Hoff\u00b7nung", "Him\u00b7mels\u00b7fun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Muthig vorw\u00e4rts durch das falsche Gl\u00fcck!", "tokens": ["Mut\u00b7hig", "vor\u00b7w\u00e4rts", "durch", "das", "fal\u00b7sche", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.49": {"text": "Doch der Morgen bringt ihn uns zur\u00fcck.", "tokens": ["Doch", "der", "Mor\u00b7gen", "bringt", "ihn", "uns", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.50": {"text": "La\u00df die blut'gen Wolken treiben,", "tokens": ["La\u00df", "die", "blut'\u00b7gen", "Wol\u00b7ken", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "Der ist in der Huth des Herrn!", "tokens": ["Der", "ist", "in", "der", "Huth", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.52": {"text": "Mag die H\u00f6lle drohn und schnauben,", "tokens": ["Mag", "die", "H\u00f6l\u00b7le", "drohn", "und", "schnau\u00b7ben", ","], "token_info": ["word", "word", "word"