{"dta.poem.460": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Liebsten Mund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der sch\u00f6nste Ort/ der Platz wo Venus lachet/", "tokens": ["Ich", "bin", "der", "sch\u00f6ns\u00b7te", "Ort", "/", "der", "Platz", "wo", "Ve\u00b7nus", "la\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "PWAV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sitz wo Lieblichkeit in Purpur-Rosen wachet/", "tokens": ["Der", "Sitz", "wo", "Lieb\u00b7lich\u00b7keit", "in", "Pur\u00b7pur\u00b7Ro\u00b7sen", "wa\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der s\u00fc\u00dften L\u00fcste-Thron von Rosen auffgef\u00fchrt/", "tokens": ["Der", "s\u00fc\u00df\u00b7ten", "L\u00fcs\u00b7te\u00b7Thron", "von", "Ro\u00b7sen", "auff\u00b7ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den ein Albaster Feld voll sch\u00f6nster Liljen ziert.", "tokens": ["Den", "ein", "Al\u00b7bas\u00b7ter", "Feld", "voll", "sch\u00f6ns\u00b7ter", "Lil\u00b7jen", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von der Natur werd ich zum Lieben angefrischet/", "tokens": ["Von", "der", "Na\u00b7tur", "werd", "ich", "zum", "Lie\u00b7ben", "an\u00b7ge\u00b7fri\u00b7schet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "APPRART", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch krieg ich nimmer satt/ und wer mich einst erwischet/", "tokens": ["Doch", "krieg", "ich", "nim\u00b7mer", "satt", "/", "und", "wer", "mich", "einst", "er\u00b7wi\u00b7schet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$(", "KON", "PWS", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der l\u00e4chset stets nach mir/ der/ welcher mich geschmeckt/", "tokens": ["Der", "l\u00e4ch\u00b7set", "stets", "nach", "mir", "/", "der", "/", "wel\u00b7cher", "mich", "ge\u00b7schmeckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "PPER", "$(", "ART", "$(", "PWS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich immer wieder hin nach meiner Anmuth streckt.", "tokens": ["Sich", "im\u00b7mer", "wie\u00b7der", "hin", "nach", "mei\u00b7ner", "An\u00b7muth", "streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich bin der Sammel-Platz wo ", "tokens": ["Ich", "bin", "der", "Sam\u00b7mel\u00b7Platz", "wo"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PWAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die den verliebten Geist mit Himmels ", "tokens": ["Die", "den", "ver\u00b7lieb\u00b7ten", "Geist", "mit", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der Wollust reiche Quell der Ort wo beyder Geist", "tokens": ["Der", "Wol\u00b7lust", "rei\u00b7che", "Quell", "der", "Ort", "wo", "bey\u00b7der", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "ART", "NN", "PWAV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In eine ", "tokens": ["In", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Ein jeder w\u00fcnschet sich/ auf meiner Au zu weiden/", "tokens": ["Ein", "je\u00b7der", "w\u00fcn\u00b7schet", "sich", "/", "auf", "mei\u00b7ner", "Au", "zu", "wei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "$(", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Man nennet mich die Th\u00fcr/ zu Zucker-s\u00fcssen Freuden/", "tokens": ["Man", "nen\u00b7net", "mich", "die", "Th\u00fcr", "/", "zu", "Zu\u00b7cke\u00b7rs\u00fcs\u00b7sen", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$(", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Seelen k\u00fchln bey mir den heissen Liebes-Brand/", "tokens": ["Die", "See\u00b7len", "k\u00fchln", "bey", "mir", "den", "heis\u00b7sen", "Lie\u00b7bes\u00b7Brand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und schmecken/ was besiegt/ den theursten ", "tokens": ["Und", "schme\u00b7cken", "/", "was", "be\u00b7siegt", "/", "den", "theurs\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "$(", "PWS", "VVFIN", "$(", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}