{"textgrid.poem.49172": {"metadata": {"author": {"name": "Lautensack, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom \u00dcbermut einer T\u00e4nzerin zur Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So hantiert ein st\u00e4mm'ger Fischer", "tokens": ["So", "han\u00b7tiert", "ein", "st\u00e4m\u00b7m'\u00b7ger", "Fi\u00b7scher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "tief geb\u00fcckt in seiner Zill'n", "tokens": ["tief", "ge\u00b7b\u00fcckt", "in", "sei\u00b7ner", "Zill'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "an sein'm Netz \u2013", "tokens": ["an", "sein'm", "Netz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Die Beine breit \u2013", "tokens": ["Die", "Bei\u00b7ne", "breit", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "mit den Schuhen schier ausgleitend", "tokens": ["mit", "den", "Schu\u00b7hen", "schier", "aus\u00b7glei\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "rechts und links die Seitenw\u00e4nd'", "tokens": ["rechts", "und", "links", "die", "Sei\u00b7ten\u00b7w\u00e4nd'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "schier hochgleitend \u2013", "tokens": ["schier", "hoch\u00b7glei\u00b7tend", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "und die ganze Zill'n, die wackelt dabei wie ein einzig's Brett \u2013", "tokens": ["und", "die", "gan\u00b7ze", "Zill'n", ",", "die", "wa\u00b7ckelt", "da\u00b7bei", "wie", "ein", "ein\u00b7zig's", "Brett", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "PAV", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "so wie ich jetz', jetz' und jetz'", "tokens": ["so", "wie", "ich", "jetz'", ",", "jetz'", "und", "jetz'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "$,", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "von dem pflasterten Boden der Besinnung verlassen", "tokens": ["von", "dem", "pflas\u00b7ter\u00b7ten", "Bo\u00b7den", "der", "Be\u00b7sin\u00b7nung", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "und die gache B\u00f6schung der Gesittung einfach rrrrrunterg'saust", "tokens": ["und", "die", "ga\u00b7che", "B\u00f6\u00b7schung", "der", "Ge\u00b7sit\u00b7tung", "ein\u00b7fach", "rrrrr\u00b7un\u00b7ter\u00b7g's\u00b7aust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVFIN"], "meter": "--+-+---+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "in dein'm Bett steh \u2013", "tokens": ["in", "dein'm", "Bett", "steh", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "in ... dein'm ... Bett \u2013", "tokens": ["in", "...", "dein'm", "...", "Bett", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "$(", "PPOSAT", "$(", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Segel mein Korsett,", "tokens": ["Se\u00b7gel", "mein", "Kor\u00b7sett", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "So war ich dir noch nie zu Will'n.", "tokens": ["So", "war", "ich", "dir", "noch", "nie", "zu", "Will'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So hantiert ein st\u00e4mm'ger Fischer", "tokens": ["So", "han\u00b7tiert", "ein", "st\u00e4m\u00b7m'\u00b7ger", "Fi\u00b7scher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "tief geb\u00fcckt in seiner Zill'n", "tokens": ["tief", "ge\u00b7b\u00fcckt", "in", "sei\u00b7ner", "Zill'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "an sein'm Netz \u2013", "tokens": ["an", "sein'm", "Netz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Die Beine breit \u2013", "tokens": ["Die", "Bei\u00b7ne", "breit", "\u2013"], "token_info": ["word", "word", "word", "