{"dta.poem.10186": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zuf\u00e4llige Gedancken  \n auf  \n  S. T.  Herrn  Doctoris  FABRICII  \n Himmlisches  V ergn\u00fcgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich glaube, grosser Mann, es sey Dein Nahme, blo\u00df", "tokens": ["Ich", "glau\u00b7be", ",", "gros\u00b7ser", "Mann", ",", "es", "sey", "Dein", "Nah\u00b7me", ",", "blo\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu diesem Zweck allein, so weit ber\u00fchmt und gro\u00df,", "tokens": ["Zu", "die\u00b7sem", "Zweck", "al\u00b7lein", ",", "so", "weit", "be\u00b7r\u00fchmt", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "$,", "ADV", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So gar in Ost und West, in S\u00fcden und in Norden,", "tokens": ["So", "gar", "in", "Ost", "und", "West", ",", "in", "S\u00fc\u00b7den", "und", "in", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch Den, der alles wirckt, und alles ordnet, worden;", "tokens": ["Durch", "Den", ",", "der", "al\u00b7les", "wirckt", ",", "und", "al\u00b7les", "ord\u00b7net", ",", "wor\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "PIS", "VVFIN", "$,", "KON", "PIS", "VVFIN", "$,", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Damit er manchem Geist, der gern den Sch\u00f6pfer priese,", "tokens": ["Da\u00b7mit", "er", "man\u00b7chem", "Geist", ",", "der", "gern", "den", "Sch\u00f6p\u00b7fer", "prie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Jhm, nach Seiner Gr\u00f6ss\u2019 und Allmacht, Ehr erwiese,", "tokens": ["Und", "Jhm", ",", "nach", "Sei\u00b7ner", "Gr\u00f6ss'", "und", "All\u00b7macht", ",", "Ehr", "er\u00b7wie\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sich aber dessen kaum bishero d\u00fcrft\u2019 erk\u00fchnen,", "tokens": ["Sich", "a\u00b7ber", "des\u00b7sen", "kaum", "bis\u00b7he\u00b7ro", "d\u00fcrft'", "er\u00b7k\u00fch\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PDS", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Durch Seine Gr\u00f6sse m\u00f6gt zum Schutz und Schilde die-", "tokens": ["Durch", "Sei\u00b7ne", "Gr\u00f6s\u00b7se", "m\u00f6gt", "zum", "Schutz", "und", "Schil\u00b7de", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "APPRART", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es wird, GOTT Lob! durch Dich nunmehr der Teutschen", "tokens": ["Es", "wird", ",", "GoTT", "Lob", "!", "durch", "Dich", "nun\u00b7mehr", "der", "Teut\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "NE", "NN", "$.", "APPR", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Sch\u00f6pfer, als ein GOTT, recht G\u00f6ttlich vorgestellt.", "tokens": ["Der", "Sch\u00f6p\u00b7fer", ",", "als", "ein", "GoTT", ",", "recht", "G\u00f6tt\u00b7lich", "vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du zeigst, wie auch bey uns, GOTT k\u00f6nn\u2019 ein Sch\u00f6pfer", "tokens": ["Du", "zeigst", ",", "wie", "auch", "bey", "uns", ",", "GoTT", "k\u00f6nn'", "ein", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "APPR", "PPER", "$,", "NE", "VMFIN", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Von Millionen Sonn- und Millionen Erden.", "tokens": ["Von", "Mil\u00b7lion\u00b7en", "Sonn", "und", "Mil\u00b7lion\u00b7en", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "TRUNC", "KON", "NN", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Ich sehe Deinen Ruhm dahero gleichsam an,", "tokens": ["Ich", "se\u00b7he", "Dei\u00b7nen", "Ruhm", "da\u00b7he\u00b7ro", "gleich\u00b7sam", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PAV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als einer Glocken Thon, die dar\u00fcm hoch erhoben,", "tokens": ["Als", "ei\u00b7ner", "Glo\u00b7cken", "Thon", ",", "die", "da\u00b7r\u00fcm", "hoch", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "PRELS", "PAV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Damit man fern und nah ihr schallen h\u00f6ren kann,", "tokens": ["Da\u00b7mit", "man", "fern", "und", "nah", "ihr", "schal\u00b7len", "h\u00f6\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "KON", "ADJD", "PPER", "VVINF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie sie zum heilgen Dienst, zum dancken, ", "tokens": ["Wie", "sie", "zum", "heil\u00b7gen", "Dienst", ",", "zum", "dan\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "$,", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Mit lautem th\u00f6nen rufft. Wie wird nicht dieser Klang", "tokens": ["Mit", "lau\u00b7tem", "th\u00f6\u00b7nen", "rufft", ".", "Wie", "wird", "nicht", "die\u00b7ser", "Klang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$.", "PWAV", "VAFIN", "PTKNEG", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Des grossen Sch\u00f6pfers Ruhm durch manchen Lob-Ge-", "tokens": ["Des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Ruhm", "durch", "man\u00b7chen", "Lob\u00b7Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "In, bis dahero stumm-, ietzt durch Dich lauten, Ch\u00f6ren,", "tokens": ["In", ",", "bis", "da\u00b7he\u00b7ro", "stum\u00b7m", ",", "ietzt", "durch", "Dich", "lau\u00b7ten", ",", "Ch\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "$,", "KON", "PAV", "TRUNC", "$,", "ADV", "APPR", "PPER", "VVINF", "$,", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Von heilger Furcht und Lust entflammter Hertzen mehren!", "tokens": ["Von", "heil\u00b7ger", "Furcht", "und", "Lust", "ent\u00b7flamm\u00b7ter", "Hert\u00b7zen", "meh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "O h\u00f6chst begl\u00fcckter Mann! schon hier halb seelger Geist,", "tokens": ["O", "h\u00f6chst", "be\u00b7gl\u00fcck\u00b7ter", "Mann", "!", "schon", "hier", "halb", "seel\u00b7ger", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$.", "ADV", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Durch den der Sch\u00f6pfer selbst uns Seine Gr\u00f6ss\u2019 er-", "tokens": ["Durch", "den", "der", "Sch\u00f6p\u00b7fer", "selbst", "uns", "Sei\u00b7ne", "Gr\u00f6ss'", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "PPER", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Da GOTT durch Deinen Ruhm selbst Seinen Ruhm ver-", "tokens": ["Da", "GoTT", "durch", "Dei\u00b7nen", "Ruhm", "selbst", "Sei\u00b7nen", "Ruhm", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Was Wunder, da\u00df die Welt Dir so viel Ehr erweis\u2019t?", "tokens": ["Was", "Wun\u00b7der", ",", "da\u00df", "die", "Welt", "Dir", "so", "viel", "Ehr", "er\u00b7weis't", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}}}}