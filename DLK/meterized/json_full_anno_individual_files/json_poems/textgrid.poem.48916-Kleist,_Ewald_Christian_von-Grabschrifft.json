{"textgrid.poem.48916": {"metadata": {"author": {"name": "Kleist, Ewald Christian von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft", "genre": "verse", "period": "N.A.", "pub_year": 1737, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Witz, Einsicht, Wissenschafft, Geschmack, Bescheidenheit,", "tokens": ["Witz", ",", "Ein\u00b7sicht", ",", "Wis\u00b7sen\u00b7schafft", ",", "Ge\u00b7schmack", ",", "Be\u00b7schei\u00b7den\u00b7heit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Menschenlieb und Tapferkeit,", "tokens": ["Und", "Men\u00b7schen\u00b7lieb", "und", "Tap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und alle Tugenden, vereint mit allen Gaben,", "tokens": ["Und", "al\u00b7le", "Tu\u00b7gen\u00b7den", ",", "ver\u00b7eint", "mit", "al\u00b7len", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Besa\u00df der, den man hier begraben.", "tokens": ["Be\u00b7sa\u00df", "der", ",", "den", "man", "hier", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "$,", "PRELS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er starb f\u00fcrs Vaterland, er starb mit Heldenmuth.", "tokens": ["Er", "starb", "f\u00fcrs", "Va\u00b7ter\u00b7land", ",", "er", "starb", "mit", "Hel\u00b7den