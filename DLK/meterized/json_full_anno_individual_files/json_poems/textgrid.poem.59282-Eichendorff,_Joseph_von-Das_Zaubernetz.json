{"textgrid.poem.59282": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Zaubernetz", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fraue, in den blauen Tagen", "tokens": ["Frau\u00b7e", ",", "in", "den", "blau\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast ein Netz du ausgehangen,", "tokens": ["Hast", "ein", "Netz", "du", "aus\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zart gewebt aus seidnen Haaren,", "tokens": ["Zart", "ge\u00b7webt", "aus", "seid\u00b7nen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfen Worten, wei\u00dfen Armen.", "tokens": ["S\u00fc\u00b7\u00dfen", "Wor\u00b7ten", ",", "wei\u00b7\u00dfen", "Ar\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die blauen Augen sprachen,", "tokens": ["Und", "die", "blau\u00b7en", "Au\u00b7gen", "spra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da ich waldw\u00e4rts wollte jagen:", "tokens": ["Da", "ich", "wald\u00b7w\u00e4rts", "woll\u00b7te", "ja\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbzieh mir, Sch\u00f6ner, nicht von dannen!\u00ab", "tokens": ["\u00bb", "zieh", "mir", ",", "Sch\u00f6\u00b7ner", ",", "nicht", "von", "dan\u00b7nen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "APPR", "ADV", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, da war ich dein Gefangner!", "tokens": ["Ach", ",", "da", "war", "ich", "dein", "Ge\u00b7fang\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6rst du nun den Fr\u00fchling laden? \u2013", "tokens": ["H\u00f6rst", "du", "nun", "den", "Fr\u00fch\u00b7ling", "la\u00b7den", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "J\u00e4gers Waldhorn geht im Walde,", "tokens": ["J\u00e4\u00b7gers", "Wald\u00b7horn", "geht", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lockend gr\u00fc\u00dfen bunte Flaggen,", "tokens": ["Lo\u00b7ckend", "gr\u00fc\u00b7\u00dfen", "bun\u00b7te", "Flag\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem S\u00e4nger alle fragen.", "tokens": ["Nach", "dem", "S\u00e4n\u00b7ger", "al\u00b7le", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, von euch, ihr Fr\u00fchlingsfahnen,", "tokens": ["Ach", ",", "von", "euch", ",", "ihr", "Fr\u00fch\u00b7lings\u00b7fah\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann ich, wie von dir, nicht lassen!", "tokens": ["Kann", "ich", ",", "wie", "von", "dir", ",", "nicht", "las\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "APPR", "PPER", "$,", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reisen in den blauen Tagen", "tokens": ["Rei\u00b7sen", "in", "den", "blau\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df der S\u00e4nger mit dem Klange.", "tokens": ["Mu\u00df", "der", "S\u00e4n\u00b7ger", "mit", "dem", "Klan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Fl\u00fcgel hat, den du gefangen \u2013", "tokens": ["Fl\u00fc\u00b7gel", "hat", ",", "den", "du", "ge\u00b7fan\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Schlingen m\u00fcssen lassen", "tokens": ["Al\u00b7le", "Schlin\u00b7gen", "m\u00fcs\u00b7sen", "las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er wird dir weggetragen,", "tokens": ["Und", "er", "wird", "dir", "weg\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn die ersten Lerchen sangen.", "tokens": ["Wenn", "die", "ers\u00b7ten", "Ler\u00b7chen", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Liebst du, treu dem alten Sange", "tokens": ["Liebst", "du", ",", "treu", "dem", "al\u00b7ten", "San\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie dem S\u00e4nger, mich wahrhaftig:", "tokens": ["Wie", "dem", "S\u00e4n\u00b7ger", ",", "mich", "wahr\u00b7haf\u00b7tig", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PPER", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df dein Schlo\u00df, den sch\u00f6nen Garten,", "tokens": ["La\u00df", "dein", "Schlo\u00df", ",", "den", "sch\u00f6\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchr dich heim in Waldesprachten!", "tokens": ["F\u00fchr", "dich", "heim", "in", "Wal\u00b7des\u00b7prach\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf dem Zelter sollst du prangen,", "tokens": ["Auf", "dem", "Zel\u00b7ter", "sollst", "du", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die sch\u00f6nen Glieder schlanke", "tokens": ["Um", "die", "sch\u00f6\u00b7nen", "Glie\u00b7der", "schlan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seide, himmelblau, gespannet,", "tokens": ["Sei\u00b7de", ",", "him\u00b7mel\u00b7blau", ",", "ge\u00b7span\u00b7net", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als ein s\u00fc\u00dfgeschm\u00fcckter Knabe.", "tokens": ["Als", "ein", "s\u00fc\u00df\u00b7ge\u00b7schm\u00fcck\u00b7ter", "Kna\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Und der J\u00e4ger sieht uns fahren,", "tokens": ["Und", "der", "J\u00e4\u00b7ger", "sieht", "uns", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und er l\u00e4\u00dft das Wild, das Jagen,", "tokens": ["Und", "er", "l\u00e4\u00dft", "das", "Wild", ",", "das", "Ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will nun ewig mit uns wandern", "tokens": ["Will", "nun", "e\u00b7wig", "mit", "uns", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem frischen H\u00f6rnerklange.", "tokens": ["Mit", "dem", "fri\u00b7schen", "H\u00f6r\u00b7ner\u00b7klan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wer von uns verf\u00fchrt den andern,", "tokens": ["Wer", "von", "uns", "ver\u00b7f\u00fchrt", "den", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob es deine Augen taten,", "tokens": ["Ob", "es", "dei\u00b7ne", "Au\u00b7gen", "ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Laut', des J\u00e4gers Blasen? \u2013", "tokens": ["Mei\u00b7ne", "Laut'", ",", "des", "J\u00e4\u00b7gers", "Bla\u00b7sen", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, wir k\u00f6nnen's nicht erraten;", "tokens": ["Ach", ",", "wir", "k\u00f6n\u00b7nen's", "nicht", "er\u00b7ra\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Aber um uns drei zusammen", "tokens": ["A\u00b7ber", "um", "uns", "drei", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird der Lenz im gr\u00fcnen Walde", "tokens": ["Wird", "der", "Lenz", "im", "gr\u00fc\u00b7nen", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl ein Zaubernetze schlagen,", "tokens": ["Wohl", "ein", "Zau\u00b7ber\u00b7net\u00b7ze", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem noch keiner je entgangen.", "tokens": ["Dem", "noch", "kei\u00b7ner", "je", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Fraue, in den blauen Tagen", "tokens": ["Frau\u00b7e", ",", "in", "den", "blau\u00b7en", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast ein Netz du ausgehangen,", "tokens": ["Hast", "ein", "Netz", "du", "aus\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zart gewebt aus seidnen Haaren,", "tokens": ["Zart", "ge\u00b7webt", "aus", "seid\u00b7nen", 