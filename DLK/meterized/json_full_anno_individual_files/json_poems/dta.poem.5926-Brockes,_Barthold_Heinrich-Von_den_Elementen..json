{"dta.poem.5926": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und also ohn uns zu bequemen,", "tokens": ["Und", "al\u00b7so", "ohn", "uns", "zu", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Gleichheits-Klumpen anzunehmen;", "tokens": ["Den", "Gleich\u00b7heits\u00b7Klum\u00b7pen", "an\u00b7zu\u00b7neh\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einen ", "tokens": ["Noch", "ei\u00b7nen"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "In seinem ungewissen Eilen;", "tokens": ["In", "sei\u00b7nem", "un\u00b7ge\u00b7wis\u00b7sen", "Ei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Noch die Verwendungen und \u00f6ffters Wiederkehren", "tokens": ["Noch", "die", "Ver\u00b7wen\u00b7dun\u00b7gen", "und", "\u00f6ff\u00b7ters", "Wie\u00b7der\u00b7keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "ADJA", "NN"], "meter": "+---+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Des Aufbehalts der Elementen;", "tokens": ["Des", "Auf\u00b7be\u00b7halts", "der", "E\u00b7le\u00b7men\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-----", "measure": "unknown.measure.di"}, "line.7": {"text": "Kan man durch unsre leichte Lehren,", "tokens": ["Kan", "man", "durch", "uns\u00b7re", "leich\u00b7te", "Leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df alles deutlicher begreiffen und erkl\u00e4ren.", "tokens": ["Da\u00df", "al\u00b7les", "deut\u00b7li\u00b7cher", "be\u00b7greif\u00b7fen", "und", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}