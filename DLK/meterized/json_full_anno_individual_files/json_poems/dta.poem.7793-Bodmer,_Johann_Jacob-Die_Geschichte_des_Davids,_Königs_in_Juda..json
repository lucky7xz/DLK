{"dta.poem.7793": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil nun die Michal eilt um ihrer Schwester willen,", "tokens": ["Weil", "nun", "die", "Mic\u00b7hal", "eilt", "um", "ih\u00b7rer", "Schwes\u00b7ter", "wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bitt sie Zeruja soll ihr W\u00fcnschen doch erf\u00fcllen,", "tokens": ["Bitt", "sie", "Ze\u00b7ru\u00b7ja", "soll", "ihr", "W\u00fcn\u00b7schen", "doch", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und fahren mit ihr hin, die ist alsbald bereit,", "tokens": ["Und", "fah\u00b7ren", "mit", "ihr", "hin", ",", "die", "ist", "als\u00b7bald", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "PRELS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man spannt den Wagen an, da Michal ihre Zeit", "tokens": ["Man", "spannt", "den", "Wa\u00b7gen", "an", ",", "da", "Mic\u00b7hal", "ih\u00b7re", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KOUS", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jmmittelst bringet zu, des Landes s\u00fcsse Leben", "tokens": ["Jm\u00b7mit\u00b7telst", "brin\u00b7get", "zu", ",", "des", "Lan\u00b7des", "s\u00fcs\u00b7se", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hie anzuschauen recht, davon dann wird gegeben", "tokens": ["Hie", "an\u00b7zu\u00b7schau\u00b7en", "recht", ",", "da\u00b7von", "dann", "wird", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVIZU", "ADJD", "$,", "PAV", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Von ihr ein solcher Schlu\u00df, da\u00df sie den Hof veracht,", "tokens": ["Von", "ihr", "ein", "sol\u00b7cher", "Schlu\u00df", ",", "da\u00df", "sie", "den", "Hof", "ver\u00b7acht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "PIAT", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wann sie die Dienstbarkeit mit dieser Ruh betracht.", "tokens": ["Wann", "sie", "die", "Dienst\u00b7bar\u00b7keit", "mit", "die\u00b7ser", "Ruh", "be\u00b7tracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Hof der ist ein Band der unsern Willen zwinget,", "tokens": ["Der", "Hof", "der", "ist", "ein", "Band", "der", "un\u00b7sern", "Wil\u00b7len", "zwin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ART", "NN", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und \u00fcberg\u00fcldte Qual f\u00fcr reine Lust uns bringet.", "tokens": ["Und", "\u00fc\u00b7berg\u00b7\u00fcld\u00b7te", "Qual", "f\u00fcr", "rei\u00b7ne", "Lust", "uns", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es dienet alles da, es dienet Herr und Knecht,", "tokens": ["Es", "die\u00b7net", "al\u00b7les", "da", ",", "es", "die\u00b7net", "Herr", "und", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$,", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Knechte Dienst uns selbst die freye Herrschafft schw\u00e4cht.", "tokens": ["Der", "Knech\u00b7te", "Dienst", "uns", "selbst", "die", "frey\u00b7e", "Herr\u00b7schafft", "schw\u00e4cht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es mu\u00df oft unser Will nach ihrem Willen gehen,", "tokens": ["Es", "mu\u00df", "oft", "un\u00b7ser", "Will", "nach", "ih\u00b7rem", "Wil\u00b7len", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und dem der mich bedient mu\u00df ich zu Dienste stehen.", "tokens": ["Und", "dem", "der", "mich", "be\u00b7dient", "mu\u00df", "ich", "zu", "Diens\u00b7te", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PRELS", "PPER", "VVPP", "VMFIN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich bin des Knechtes Knecht, und hab den Willen nicht,", "tokens": ["Ich", "bin", "des", "Knech\u00b7tes", "Knecht", ",", "und", "hab", "den", "Wil\u00b7len", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "KON", "VAFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df was derselbe will, ich ohne M\u00fch verricht.", "tokens": ["Da\u00df", "was", "der\u00b7sel\u00b7be", "will", ",", "ich", "oh\u00b7ne", "M\u00fch", "ver\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PDAT", "VMFIN", "$,", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So waren Michals Wort, Zeruja wollt bestreiten,", "tokens": ["So", "wa\u00b7ren", "Mic\u00b7hals", "Wort", ",", "Ze\u00b7ru\u00b7ja", "wollt", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$,", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Hof der gienge f\u00fcr, und w\u00e4re auch bey weiten", "tokens": ["Der", "Hof", "der", "gien\u00b7ge", "f\u00fcr", ",", "und", "w\u00e4\u00b7re", "auch", "bey", "wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "APPR", "$,", "KON", "VAFIN", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Geprie\u00dfner als das Land. Doch Michal blieb dabey,", "tokens": ["Ge\u00b7prie\u00df\u00b7ner", "als", "das", "Land", ".", "Doch", "Mic\u00b7hal", "blieb", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "$.", "KON", "NE", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bey Hofe lebt man streng, im Felde lebt man frey.", "tokens": ["Bey", "Ho\u00b7fe", "lebt", "man", "streng", ",", "im", "Fel\u00b7de", "lebt", "man", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Hierauf geht nun die Reis nach der verla\u00dfnen H\u00f6len,", "tokens": ["Hier\u00b7auf", "geht", "nun", "die", "Reis", "nach", "der", "ver\u00b7la\u00df\u00b7nen", "H\u00f6\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Allwo Thalmais sich und Merob hefftig qu\u00e4len,", "tokens": ["All\u00b7wo", "Thal\u00b7mais", "sich", "und", "Me\u00b7rob", "heff\u00b7tig", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PRF", "KON", "NE", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.23": {"text": "Weil sie als sie erwacht, die Michal funden nicht,", "tokens": ["Weil", "sie", "als", "sie", "er\u00b7wacht", ",", "die", "Mic\u00b7hal", "fun\u00b7den", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "PPER", "VVPP", "$,", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und finden keinen auch der ihnen was bericht,", "tokens": ["Und", "fin\u00b7den", "kei\u00b7nen", "auch", "der", "ih\u00b7nen", "was", "be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADV", "ART", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wo ihre Schwester sey. Von David seinem Knaben,", "tokens": ["Wo", "ih\u00b7re", "Schwes\u00b7ter", "sey", ".", "Von", "Da\u00b7vid", "sei\u00b7nem", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "$.", "APPR", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der weil sein Herr war aus, die Aufsicht muste haben", "tokens": ["Der", "weil", "sein", "Herr", "war", "aus", ",", "die", "Auf\u00b7sicht", "mus\u00b7te", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "KOUS", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$,", "ART", "NN", "VMFIN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Auf die geliebte Heerd, erfahren sie zuletzt,", "tokens": ["Auf", "die", "ge\u00b7lieb\u00b7te", "Heerd", ",", "er\u00b7fah\u00b7ren", "sie", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Was ihre Sorg vermehrt und sie in Unruh setzt;", "tokens": ["Was", "ih\u00b7re", "Sorg", "ver\u00b7mehrt", "und", "sie", "in", "Un\u00b7ruh", "setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "KON", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df nemlich Michal sey mit Davld auf dem Wagen", "tokens": ["Da\u00df", "nem\u00b7lich", "Mic\u00b7hal", "sey", "mit", "Davld", "auf", "dem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NE", "VAFIN", "APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Gefahren in das Feld. Was will man hievon sagen?", "tokens": ["Ge\u00b7fah\u00b7ren", "in", "das", "Feld", ".", "Was", "will", "man", "hie\u00b7von", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "PWS", "VMFIN", "PIS", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Spricht Merob gantz best\u00fcrtzt; Indem schaut sie sich um,", "tokens": ["Spricht", "Me\u00b7rob", "gantz", "be\u00b7st\u00fcrtzt", ";", "In\u00b7dem", "schaut", "sie", "sich", "um", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "VVPP", "$.", "KOUS", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Und schallt ihr f\u00fcr das Ohr der Michal ihre Stimm,", "tokens": ["Und", "schallt", "ihr", "f\u00fcr", "das", "Ohr", "der", "Mic\u00b7hal", "ih\u00b7re", "Stimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Sie eilet nach dem Ton, und sieht den Wagen kommen,", "tokens": ["Sie", "ei\u00b7let", "nach", "dem", "Ton", ",", "und", "sieht", "den", "Wa\u00b7gen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "In welchem sie nun kaum Zeruja mit vernommen,", "tokens": ["In", "wel\u00b7chem", "sie", "nun", "kaum", "Ze\u00b7ru\u00b7ja", "mit", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "NE", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da wird sie hoch erfreut und eilet zu ihr an,", "tokens": ["Da", "wird", "sie", "hoch", "er\u00b7freut", "und", "ei\u00b7let", "zu", "ihr", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Zeruja ebenfalls sich nicht mehr halten kan,", "tokens": ["Ze\u00b7ru\u00b7ja", "e\u00b7ben\u00b7falls", "sich", "nicht", "mehr", "hal\u00b7ten", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PRF", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.37": {"text": "Als sie die Merob sieht. Gleichwie sich streckt das Eisen", "tokens": ["Als", "sie", "die", "Me\u00b7rob", "sieht", ".", "Gleich\u00b7wie", "sich", "streckt", "das", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NE", "VVFIN", "$.", "KON", "PRF", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Nach dem Magnetenstein, so will sich hier erweisen", "tokens": ["Nach", "dem", "Mag\u00b7ne\u00b7ten\u00b7stein", ",", "so", "will", "sich", "hier", "er\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VMFIN", "PRF", "ADV", "VVINF"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Auch Wechselsweise Lieb. Zeruja und Merob", "tokens": ["Auch", "Wech\u00b7sels\u00b7wei\u00b7se", "Lieb", ".", "Ze\u00b7ru\u00b7ja", "und", "Me\u00b7rob"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "NN", "$.", "NE", "KON", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die streiten wer da k\u00f6nn einander siegen ob,", "tokens": ["Die", "strei\u00b7ten", "wer", "da", "k\u00f6nn", "ein\u00b7an\u00b7der", "sie\u00b7gen", "ob", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PWS", "ADV", "VMFIN", "PRF", "VVFIN", "KOUS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "In Zeigung wahrer Freud; Zeruja war geschicket", "tokens": ["In", "Zei\u00b7gung", "wah\u00b7rer", "Freud", ";", "Ze\u00b7ru\u00b7ja", "war", "ge\u00b7schi\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "$.", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Hiezu weil sie es wust, doch Merob gantz verr\u00fccket,", "tokens": ["Hie\u00b7zu", "weil", "sie", "es", "wust", ",", "doch", "Me\u00b7rob", "gantz", "ver\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "PPER", "VVFIN", "$,", "ADV", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Weil die\u00df ihr widerfuhr ohn da\u00df sie es gedacht,", "tokens": ["Weil", "die\u00df", "ihr", "wi\u00b7der\u00b7fuhr", "ohn", "da\u00df", "sie", "es", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPER", "VVFIN", "APPR", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Drum wurde Wechselswei\u00df best\u00fcrtzte Ruh gebracht.", "tokens": ["Drum", "wur\u00b7de", "Wech\u00b7sels\u00b7wei\u00df", "be\u00b7st\u00fcrtz\u00b7te", "Ruh", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Thalmais hatt ihr Theil auch mit an diesen Freuden,", "tokens": ["Thal\u00b7mais", "hatt", "ihr", "Theil", "auch", "mit", "an", "die\u00b7sen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.46": {"text": "Und David dessen Hertz schon vieles must erleiden,", "tokens": ["Und", "Da\u00b7vid", "des\u00b7sen", "Hertz", "schon", "vie\u00b7les", "must", "er\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PRELAT", "NN", "ADV", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Sah Merob an best\u00fcrtzt, die er nebst Thalmais", "tokens": ["Sah", "Me\u00b7rob", "an", "be\u00b7st\u00fcrtzt", ",", "die", "er", "nebst", "Thal\u00b7mais"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "APZR", "VVPP", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Aufs h\u00f6flichste empf\u00e4ht, er wei\u00df nun nicht gewi\u00df", "tokens": ["Aufs", "h\u00f6f\u00b7lichs\u00b7te", "em\u00b7pf\u00e4ht", ",", "er", "wei\u00df", "nun", "nicht", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Wer die sey so ihn liebt. Die Merob sich erweiset", "tokens": ["Wer", "die", "sey", "so", "ihn", "liebt", ".", "Die", "Me\u00b7rob", "sich", "er\u00b7wei\u00b7set"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PDS", "VAFIN", "ADV", "PPER", "VVFIN", "$.", "ART", "NN", "PRF", "VVFIN"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.50": {"text": "Mit Michal gleicher G\u00fct, er jede bey sich preiset.", "tokens": ["Mit", "Mic\u00b7hal", "glei\u00b7cher", "G\u00fct", ",", "er", "je\u00b7de", "bey", "sich", "prei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,", "PPER", "PIAT", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Wann er schaut Merob an, so ist ihr Wunderschein", "tokens": ["Wann", "er", "schaut", "Me\u00b7rob", "an", ",", "so", "ist", "ihr", "Wun\u00b7der\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "NE", "PTKVZ", "$,", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "--++-+-+-+-+", "measure": "anapaest.init"}, "line.52": {"text": "So starck da\u00df der vermag sein Hertz zu nehmen ein.", "tokens": ["So", "starck", "da\u00df", "der", "ver\u00b7mag", "sein", "Hertz", "zu", "neh\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "ART", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Erblickt er Michal dann ist ihr liebreiches Wesen", "tokens": ["Er\u00b7blickt", "er", "Mic\u00b7hal", "dann", "ist", "ihr", "lieb\u00b7rei\u00b7ches", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.54": {"text": "Also f\u00fcr seinen Sinn dermassen auserlesen,", "tokens": ["Al\u00b7so", "f\u00fcr", "sei\u00b7nen", "Sinn", "der\u00b7mas\u00b7sen", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PDS", "VVINF", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.55": {"text": "Da\u00df er bey sich gedenckt, ach wann es Michal w\u00e4r!", "tokens": ["Da\u00df", "er", "bey", "sich", "ge\u00b7denckt", ",", "ach", "wann", "es", "Mic\u00b7hal", "w\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "VVPP", "$,", "XY", "PWAV", "PPER", "NE", "VAFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.56": {"text": "Sieht er denn Merob an, w\u00fcnscht er die ja so sehr.", "tokens": ["Sieht", "er", "denn", "Me\u00b7rob", "an", ",", "w\u00fcnscht", "er", "die", "ja", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NE", "PTKVZ", "$,", "VVFIN", "PPER", "ART", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.57": {"text": "Er meinet aus dem Ton der Stimmen wollt er h\u00f6ren,", "tokens": ["Er", "mei\u00b7net", "aus", "dem", "Ton", "der", "Stim\u00b7men", "wollt", "er", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Was sein verliebter Sinn zu wissen that begehren.", "tokens": ["Was", "sein", "ver\u00b7lieb\u00b7ter", "Sinn", "zu", "wis\u00b7sen", "that", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}