{"dta.poem.2704": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die erl\u00f6sende Christ-Nacht  \n  Bey Beerdigung Hn.   M.   B.   D.   E. hertzge-  \n liebten T\u00f6chterleins Judith den 30. De-  \n cembr. 1677.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht/ so uns das Heil und Leben hat gebohren/", "tokens": ["Die", "Nacht", "/", "so", "uns", "das", "Heil", "und", "Le\u00b7ben", "hat", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "ART", "NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dr\u00fcckt dir/ o zartes Kind/ die matten Augen zu/", "tokens": ["Dr\u00fcckt", "dir", "/", "o", "zar\u00b7tes", "Kind", "/", "die", "mat\u00b7ten", "Au\u00b7gen", "zu", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "FM", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Nacht von Ewigkeit zum Frieden auserkohren/", "tokens": ["Die", "Nacht", "von", "E\u00b7wig\u00b7keit", "zum", "Frie\u00b7den", "au\u00b7ser\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bringt nach der Kranckheit-Weh\u2019 dich auch in Fried", "tokens": ["Bringt", "nach", "der", "Kranck\u00b7heit\u00b7Weh'", "dich", "auch", "in", "Fried"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verla\u00df dein sieches Bett/ geh aus der engen Wiegen/", "tokens": ["Ver\u00b7la\u00df", "dein", "sie\u00b7ches", "Bett", "/", "geh", "aus", "der", "en\u00b7gen", "Wie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein Heiland ladet dich zu feiner Wiegen ein/", "tokens": ["Dein", "Hei\u00b7land", "la\u00b7det", "dich", "zu", "fei\u00b7ner", "Wie\u00b7gen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Hier siehst du deinen GOtt und deinen Bruder liegen/", "tokens": ["Hier", "siehst", "du", "dei\u00b7nen", "Gott", "und", "dei\u00b7nen", "Bru\u00b7der", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Windeln binden ihn/ auff da\u00df du frey kanst seyn.", "tokens": ["Die", "Win\u00b7deln", "bin\u00b7den", "ihn", "/", "auff", "da\u00df", "du", "frey", "kanst", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$(", "APPR", "KOUS", "PPER", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den nichts begreiffen kan/ der alles ist in allen/", "tokens": ["Den", "nichts", "be\u00b7greif\u00b7fen", "kan", "/", "der", "al\u00b7les", "ist", "in", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$(", "ART", "PIS", "VAFIN", "APPR", "PIAT", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wirfft deiner D\u00fcrfftigkeit jetzt neuen Reichthum bey/", "tokens": ["Wirfft", "dei\u00b7ner", "D\u00fcr\u00b7ff\u00b7tig\u00b7keit", "jetzt", "neu\u00b7en", "Reicht\u00b7hum", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "ADJA", "NN", "APPR", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wird GOtt und Mensch zugleich den Menschen zu gefallen/", "tokens": ["Wird", "Gott", "und", "Mensch", "zu\u00b7gleich", "den", "Men\u00b7schen", "zu", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und legt die Majest\u00e4t auff schlechtes Stroh und Heu.", "tokens": ["Und", "legt", "die", "Ma\u00b7jes\u00b7t\u00e4t", "auff", "schlech\u00b7tes", "Stroh", "und", "Heu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach hochbegl\u00fcckte Nacht/ f\u00fcr der die Morgenr\u00f6the", "tokens": ["Ach", "hoch\u00b7be\u00b7gl\u00fcck\u00b7te", "Nacht", "/", "f\u00fcr", "der", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADJA", "NN", "$(", "APPR", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In ihrem Purpur-Glantz und Rosen-Schein erbleicht!", "tokens": ["In", "ih\u00b7rem", "Pur\u00b7pur\u00b7Glantz", "und", "Ro\u00b7sen\u00b7Schein", "er\u00b7bleicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ists m\u00f6glich da\u00df die Nacht des Tages Stralen t\u00f6dte/", "tokens": ["Ists", "m\u00f6g\u00b7lich", "da\u00df", "die", "Nacht", "des", "Ta\u00b7ges", "Stra\u00b7len", "t\u00f6d\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und da\u00df der Sonnen Rad vor diesen Fackeln weicht?", "tokens": ["Und", "da\u00df", "der", "Son\u00b7nen", "Rad", "vor", "die\u00b7sen", "Fa\u00b7ckeln", "weicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich seh\u2019 das lichte Heer der g\u00fcldnen Sterne schimmern/", "tokens": ["Ich", "seh'", "das", "lich\u00b7te", "Heer", "der", "g\u00fcld\u00b7nen", "Ster\u00b7ne", "schim\u00b7mern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und wie ihr ewig Feur die Freuden-Blicke zeigt/", "tokens": ["Und", "wie", "ihr", "e\u00b7wig", "Feur", "die", "Freu\u00b7den\u00b7Bli\u00b7cke", "zeigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich seh\u2019 wie die Natur aus allen ihren Zimmern", "tokens": ["Ich", "seh'", "wie", "die", "Na\u00b7tur", "aus", "al\u00b7len", "ih\u00b7ren", "Zim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Lufft/ Erde/ See und Gluth/ zu dieser Nacht sich neigt.", "tokens": ["Lufft", "/", "Er\u00b7de", "/", "See", "und", "Gluth", "/", "zu", "die\u00b7ser", "Nacht", "sich", "neigt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "$(", "APPR", "PDAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach hoch gew\u00fcnschte Nacht/ die uns der H\u00f6llen Schrecken", "tokens": ["Ach", "hoch", "ge\u00b7w\u00fcnschte", "Nacht", "/", "die", "uns", "der", "H\u00f6l\u00b7len", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADJD", "ADJA", "NN", "$(", "PRELS", "PPER", "ART", "NN", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Des Todes Finstern\u00fc\u00df mit ihrem Glantz vertreibt!", "tokens": ["Des", "To\u00b7des", "Fins\u00b7ter\u00b7n\u00fc\u00df", "mit", "ih\u00b7rem", "Glantz", "ver\u00b7treibt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nacht/ derer Klarheit kan die Todten aufferwecken/", "tokens": ["Nacht", "/", "de\u00b7rer", "Klar\u00b7heit", "kan", "die", "Tod\u00b7ten", "auf\u00b7fer\u00b7we\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PDS", "NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.24": {"text": "Und die uns zu dem Licht/ das unumbschrieben/ schreibt.", "tokens": ["Und", "die", "uns", "zu", "dem", "Licht", "/", "das", "u\u00b7numbsc\u00b7hrie\u00b7ben", "/", "schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "PPER", "APPR", "ART", "NN", "$(", "ART", "ADJA", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Es sey die Nacht erfreut den B\u00fcrgern in Jdumen/", "tokens": ["Es", "sey", "die", "Nacht", "er\u00b7freut", "den", "B\u00fcr\u00b7gern", "in", "Jdu\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Da Judiths Helden-Arm den Holofern enthaupt/", "tokens": ["Da", "Ju\u00b7diths", "Hel\u00b7den\u00b7Arm", "den", "Ho\u00b7lo\u00b7fern", "ent\u00b7haupt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ach/ Judith/ mit mehr Schmuck und Himmels-sch\u00f6nen Blumen", "tokens": ["Ach", "/", "Ju\u00b7dith", "/", "mit", "mehr", "Schmuck", "und", "Him\u00b7mels\u00b7sch\u00f6\u00b7nen", "Blu\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$(", "NE", "$(", "APPR", "PIAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Hat dich dein Br\u00e4utigam in dieser Nacht belaubt.", "tokens": ["Hat", "dich", "dein", "Br\u00e4u\u00b7ti\u00b7gam", "in", "die\u00b7ser", "Nacht", "be\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wie solte doch dein Mund des Todes Wermuth schmecken?", "tokens": ["Wie", "sol\u00b7te", "doch", "dein", "Mund", "des", "To\u00b7des", "Wer\u00b7muth", "schme\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da das gebohrne Kind dir Lebens-Zucker bringt:", "tokens": ["Da", "das", "ge\u00b7bohr\u00b7ne", "Kind", "dir", "Le\u00b7bens\u00b7Zu\u00b7cker", "bringt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wie kan der Fl\u00fcgel dich der Finstern\u00fc\u00df bedecken?", "tokens": ["Wie", "kan", "der", "Fl\u00fc\u00b7gel", "dich", "der", "Fins\u00b7ter\u00b7n\u00fc\u00df", "be\u00b7de\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Nun neuer Klarheit Blitz in deine Augen dringt.", "tokens": ["Nun", "neu\u00b7er", "Klar\u00b7heit", "Blitz", "in", "dei\u00b7ne", "Au\u00b7gen", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Auff Judith! bi\u00df erfreut/ dein Heyland hat bescheret/", "tokens": ["Auff", "Ju\u00b7dith", "!", "bi\u00df", "er\u00b7freut", "/", "dein", "Hey\u00b7land", "hat", "be\u00b7sche\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "ADV", "ADJD", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dein Bruder theilt mit dir den Schatz der Ewigkeit/", "tokens": ["Dein", "Bru\u00b7der", "theilt", "mit", "dir", "den", "Schatz", "der", "E\u00b7wig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was nicht der Himmel hat/ was nicht die Welt gewehret/", "tokens": ["Was", "nicht", "der", "Him\u00b7mel", "hat", "/", "was", "nicht", "die", "Welt", "ge\u00b7weh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VAFIN", "$(", "PWS", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Was keine Zeit sonst giebt/ das schenckt dir diese Zeit.", "tokens": ["Was", "kei\u00b7ne", "Zeit", "sonst", "giebt", "/", "das", "schenckt", "dir", "die\u00b7se", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ADV", "VVFIN", "$(", "PDS", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Verlangst du theures Gold? Sein Glantz ist mehr als g\u00fclden.", "tokens": ["Ver\u00b7langst", "du", "theu\u00b7res", "Gold", "?", "Sein", "Glantz", "ist", "mehr", "als", "g\u00fcl\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$.", "PPOSAT", "NN", "VAFIN", "PIS", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Liebst du den Perlen-Schmuck? die Perl ist ohne Fleck.", "tokens": ["Liebst", "du", "den", "Per\u00b7len\u00b7Schmuck", "?", "die", "Perl", "ist", "oh\u00b7ne", "Fleck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Gef\u00e4llt dir denn ein Bild? hier wil sich der abbilden/", "tokens": ["Ge\u00b7f\u00e4llt", "dir", "denn", "ein", "Bild", "?", "hier", "wil", "sich", "der", "ab\u00b7bil\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$.", "ADV", "VMFIN", "PRF", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der durch Geburt und Tod ist dein Erl\u00f6sungs-Zweck.", "tokens": ["Der", "durch", "Ge\u00b7burt", "und", "Tod", "ist", "dein", "Er\u00b7l\u00f6\u00b7sungs\u00b7Zweck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "AP