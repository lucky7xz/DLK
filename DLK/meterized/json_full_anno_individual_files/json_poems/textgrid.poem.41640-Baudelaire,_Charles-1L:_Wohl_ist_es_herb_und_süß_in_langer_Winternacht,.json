{"textgrid.poem.41640": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl ist es herb und s\u00fc\u00df in langer Winternacht,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl ist es herb und s\u00fc\u00df in langer Winternacht,", "tokens": ["Wohl", "ist", "es", "herb", "und", "s\u00fc\u00df", "in", "lan\u00b7ger", "Win\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann durch den tr\u00fcben Rauch die Flammen flackernd dringen,", "tokens": ["Wann", "durch", "den", "tr\u00fc\u00b7ben", "Rauch", "die", "Flam\u00b7men", "fla\u00b7ckernd", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu lauschen, wie Erinnern fern erwacht", "tokens": ["Zu", "lau\u00b7schen", ",", "wie", "E\u00b7rin\u00b7nern", "fern", "er\u00b7wacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Heim Klang der Glocken, die im Nebelmeere singen.", "tokens": ["Heim", "Klang", "der", "Glo\u00b7cken", ",", "die", "im", "Ne\u00b7bel\u00b7mee\u00b7re", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gl\u00fcckselge Glocke siegender Gewalt!", "tokens": ["Gl\u00fcck\u00b7sel\u00b7ge", "Glo\u00b7cke", "sie\u00b7gen\u00b7der", "Ge\u00b7walt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Von der trotz ihres Alters \u00fcber Welten", "tokens": ["Von", "der", "trotz", "ih\u00b7res", "Al\u00b7ters", "\u00fc\u00b7ber", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stark und getreu der heilge Ruf erschallt,", "tokens": ["Stark", "und", "ge\u00b7treu", "der", "heil\u00b7ge", "Ruf", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dem grauen Krieger gleich, der wacht in den Gezelten.", "tokens": ["Dem", "grau\u00b7en", "Krie\u00b7ger", "gleich", ",", "der", "wacht", "in", "den", "Ge\u00b7zel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Jedoch mein Herz zersprang, und wenn sein gramvoll Lied", "tokens": ["Je\u00b7doch", "mein", "Herz", "zer\u00b7sprang", ",", "und", "wenn", "sein", "gram\u00b7voll", "Lied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,", "KON", "KOUS", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In tiefer Pein die Luft der kalten Nacht durchzieht,", "tokens": ["In", "tie\u00b7fer", "Pein", "die", "Luft", "der", "kal\u00b7ten", "Nacht", "durch\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So gleicht sein schwacher Ruf dem bangen R\u00f6cheln dessen,", "tokens": ["So", "gleicht", "sein", "schwa\u00b7cher", "Ruf", "dem", "ban\u00b7gen", "R\u00f6\u00b7cheln", "des\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "PDS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den man an einem See von dunklem Blut vergessen,", "tokens": ["Den", "man", "an", "ei\u00b7nem", "See", "von", "dunk\u00b7lem", "Blut", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Leichen ganz bedeckt, in f\u00fcrchterlichem Krampf,", "tokens": ["Von", "Lei\u00b7chen", "ganz", "be\u00b7deckt", ",", "in", "f\u00fcrch\u00b7ter\u00b7li\u00b7chem", "Krampf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und der nun reglos stirbt trotz ungeheurem Kampf.", "tokens": ["Und", "der", "nun", "reg\u00b7los", "stirbt", "trotz", "un\u00b7ge\u00b7heu\u00b7rem", "Kampf", "."], "token_info": ["word", "word", "word", "wor