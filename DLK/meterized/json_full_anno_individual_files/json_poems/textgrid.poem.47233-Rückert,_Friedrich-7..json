{"textgrid.poem.47233": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchmessen habt ihr l\u00e4ngst Gebirg und Wogen", "tokens": ["Durch\u00b7mes\u00b7sen", "habt", "ihr", "l\u00e4ngst", "Ge\u00b7birg", "und", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "NN", "KON", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mit Wanderschritt und mit des Schiffes Kiele;", "tokens": ["Mit", "Wan\u00b7der\u00b7schritt", "und", "mit", "des", "Schif\u00b7fes", "Kie\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur noch gekommen seid ihr nicht zum Ziele,", "tokens": ["Nur", "noch", "ge\u00b7kom\u00b7men", "seid", "ihr", "nicht", "zum", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo auf der Erde steht der Himmelsbogen!", "tokens": ["Wo", "auf", "der", "Er\u00b7de", "steht", "der", "Him\u00b7mels\u00b7bo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr habt dem Vogel auch sein Recht entzogen,", "tokens": ["Ihr", "habt", "dem", "Vo\u00b7gel", "auch", "sein", "Recht", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word"