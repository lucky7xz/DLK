{"dta.poem.10003": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  Vergn\u00fcgungen der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mutter der weisen Betrachtung, du Sch\u00f6pferin ern-\nster Gedanken,", "tokens": ["Mut\u00b7ter", "der", "wei\u00b7sen", "Be\u00b7trach\u00b7tung", ",", "du", "Sch\u00f6p\u00b7fe\u00b7rin", "ern", "ster", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PPER", "NN", "TRUNC", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Deren Grotte sich hoch auf Teneriffs Gipfel gew\u00f6lbet,", "tokens": ["De\u00b7ren", "Grot\u00b7te", "sich", "hoch", "auf", "Te\u00b7ne\u00b7riffs", "Gip\u00b7fel", "ge\u00b7w\u00f6l\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "APPR", "NN", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Wo oft mitten in schrecklicher Nacht der heulende Sturm-", "tokens": ["Wo", "oft", "mit\u00b7ten", "in", "schreck\u00b7li\u00b7cher", "Nacht", "der", "heu\u00b7len\u00b7de", "Stur\u00b7m"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Vom wildstr\u00f6menden Regen und prasselnden Hagel be-", "tokens": ["Vom", "wild\u00b7str\u00f6\u00b7men\u00b7den", "Re\u00b7gen", "und", "pras\u00b7seln\u00b7den", "Ha\u00b7gel", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "KON", "ADJA", "NN", "TRUNC"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Dein hinhorchendes Ohr ergetzt; indem du, erheitert,", "tokens": ["Dein", "hin\u00b7hor\u00b7chen\u00b7des", "Ohr", "er\u00b7getzt", ";", "in\u00b7dem", "du", ",", "er\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$.", "KOUS", "PPER", "$,", "VVPP", "$,"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Mitten im Aufruhr, versenkt in tiefe Gedanken dich", "tokens": ["Mit\u00b7ten", "im", "Auf\u00b7ruhr", ",", "ver\u00b7senkt", "in", "tie\u00b7fe", "Ge\u00b7dan\u00b7ken", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "PPER"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Oder indem in der Nacht ein Schleyer trauriger Wolken", "tokens": ["O\u00b7der", "in\u00b7dem", "in", "der", "Nacht", "ein", "Schle\u00b7yer", "trau\u00b7ri\u00b7ger", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Alle Gestirne verbirgt, bis bald vom ruhigen Himmel", "tokens": ["Al\u00b7le", "Ge\u00b7stir\u00b7ne", "ver\u00b7birgt", ",", "bis", "bald", "vom", "ru\u00b7hi\u00b7gen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "$,", "KOUS", "ADV", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Cynthia traurig und bla\u00df von ihrem silbernen Wagen", "tokens": ["Cyn\u00b7thia", "trau\u00b7rig", "und", "bla\u00df", "von", "ih\u00b7rem", "sil\u00b7ber\u00b7nen", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Nieder zum Ocean schaut, da du voll Tiefsinn indessen", "tokens": ["Nie\u00b7der", "zum", "O\u00b7cean", "schaut", ",", "da", "du", "voll", "Tief\u00b7sinn", "in\u00b7des\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "NN", "VVINF"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Unverwandt mit dem starrenden Blick auf das Sternen-", "tokens": ["Un\u00b7ver\u00b7wandt", "mit", "dem", "star\u00b7ren\u00b7den", "Blick", "auf", "das", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "TRUNC"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Angeheftet, dich ganz in frommer Entz\u00fcckung verlierest;", "tokens": ["An\u00b7ge\u00b7hef\u00b7tet", ",", "dich", "ganz", "in", "from\u00b7mer", "Ent\u00b7z\u00fc\u00b7ckung", "ver\u00b7lie\u00b7rest", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.13": {"text": "Obgleich, mit verwirrtem Ger\u00e4usch, die brausenden", "tokens": ["Ob\u00b7gleich", ",", "mit", "ver\u00b7wirr\u00b7tem", "Ge\u00b7r\u00e4usch", ",", "die", "brau\u00b7sen\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Unter dir wallen, und heisres Gemurmel die Felsen", "tokens": ["Un\u00b7ter", "dir", "wal\u00b7len", ",", "und", "heis\u00b7res", "Ge\u00b7mur\u00b7mel", "die", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVINF", "$,", "KON", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.15": {"text": "Wo du, begl\u00fcckt, und in dich gekehrt, den tobenden", "tokens": ["Wo", "du", ",", "be\u00b7gl\u00fcckt", ",", "und", "in", "dich", "ge\u00b7kehrt", ",", "den", "to\u00b7ben\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "$,", "VVPP", "$,", "KON", "APPR", "PPER", "VVPP", "$,", "ART", "NN"], "meter": "+--+--+-+-+--", "measure": "dactylic.di.plus"}, "line.16": {"text": "Des emp\u00f6rten Oceans h\u00f6rst; fern von dem Get\u00fcmmel,", "tokens": ["Des", "em\u00b7p\u00f6r\u00b7ten", "O\u00b7ceans", "h\u00f6rst", ";", "fern", "von", "dem", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Fern von den Freuden der Menschen, und mit den himm-", "tokens": ["Fern", "von", "den", "Freu\u00b7den", "der", "Men\u00b7schen", ",", "und", "mit", "den", "him\u00b7m"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,", "KON", "APPR", "ART", "TRUNC"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.18": {"text": "Unterhaltungen pflegst: \u2014 O! leite mich, m\u00e4chtige", "tokens": ["Un\u00b7ter\u00b7hal\u00b7tun\u00b7gen", "pflegst", ":", "O", "!", "lei\u00b7te", "mich", ",", "m\u00e4ch\u00b7ti\u00b7ge"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "$.", "$(", "ITJ", "$.", "VVFIN", "PPER", "$,", "ADJA"], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Zu dem heiligen Dunkel, mit meiner Seele, harmo-", "tokens": ["Zu", "dem", "hei\u00b7li\u00b7gen", "Dun\u00b7kel", ",", "mit", "mei\u00b7ner", "See\u00b7le", ",", "har\u00b7mo"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "TRUNC"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Unter den einsamen Gang von alten verfallnen Ge-", "tokens": ["Un\u00b7ter", "den", "ein\u00b7sa\u00b7men", "Gang", "von", "al\u00b7ten", "ver\u00b7fall\u00b7nen", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}, "line.21": {"text": "Zu den d\u00e4mmernden Zellen und Lauben, und traurigen", "tokens": ["Zu", "den", "d\u00e4m\u00b7mern\u00b7den", "Zel\u00b7len", "und", "Lau\u00b7ben", ",", "und", "trau\u00b7ri\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN", "$,", "KON", "VVINF"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.22": {"text": "Wo die Melancholey ihr werthe Gedanken hinausdenkt,", "tokens": ["Wo", "die", "Me\u00b7lan\u00b7cho\u00b7ley", "ihr", "wert\u00b7he", "Ge\u00b7dan\u00b7ken", "hin\u00b7aus\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.23": {"text": "Und am liebsten verweilt. Die lachenden Scenen des", "tokens": ["Und", "am", "liebs\u00b7ten", "ver\u00b7weilt", ".", "Die", "la\u00b7chen\u00b7den", "Sce\u00b7nen", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKA", "ADJD", "VVPP", "$.", "ART", "ADJA", "NN", "ART"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.24": {"text": "Wenn um ihn her die Gratien scherzen, und Liebesg\u00f6tter", "tokens": ["Wenn", "um", "ihn", "her", "die", "Gra\u00b7ti\u00b7en", "scher\u00b7zen", ",", "und", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ADV", "ART", "NN", "VVINF", "$,", "KON", "NN"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "Jhn umtanzen, und Blumen und Bl\u00fcthen, Ambrosia", "tokens": ["Jhn", "um\u00b7tan\u00b7zen", ",", "und", "Blu\u00b7men", "und", "Bl\u00fc\u00b7then", ",", "A\u00b7mbro\u00b7sia"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVINF", "$,", "KON", "NN", "KON", "NN", "$,", "NE"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Unter ihm mit verschwendrischer Hand auf Fluren", "tokens": ["Un\u00b7ter", "ihm", "mit", "ver\u00b7schwend\u00b7ri\u00b7scher", "Hand", "auf", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "ADJA", "NN", "APPR", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.27": {"text": "R\u00fchren l\u00e4nger mich nicht; ich w\u00fcnsche mir nicht mehr,", "tokens": ["R\u00fch\u00b7ren", "l\u00e4n\u00b7ger", "mich", "nicht", ";", "ich", "w\u00fcn\u00b7sche", "mir", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.28": {"text": "Deine balsamischen L\u00fcfte zu athmen. Jhr gr\u00fcnenden", "tokens": ["Dei\u00b7ne", "bal\u00b7sa\u00b7mi\u00b7schen", "L\u00fcf\u00b7te", "zu", "ath\u00b7men", ".", "Ihr", "gr\u00fc\u00b7nen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$.", "PPOSAT", "ADJA"], "meter": "+-+-+-+--+--+--", "measure": "trochaic.hexa.relaxed"}, "line.29": {"text": "Und ihr Wiesen, du bl\u00fchender Hain, um welchen der", "tokens": ["Und", "ihr", "Wie\u00b7sen", ",", "du", "bl\u00fc\u00b7hen\u00b7der", "Hain", ",", "um", "wel\u00b7chen", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,", "KOUI", "PWAT", "ART"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Murmelnd sich schlie\u00dft, gehabt euch wohl! Ich folge", "tokens": ["Mur\u00b7melnd", "sich", "schlie\u00dft", ",", "ge\u00b7habt", "euch", "wohl", "!", "Ich", "fol\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PRF", "VVFIN", "$,", "VAPP", "PPER", "ADV", "$.", "PPER", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}