{"textgrid.poem.52538": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das flaue Herz", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du lie\u00dfest mich in Beth\u00f6rung,", "tokens": ["Du", "lie\u00b7\u00dfest", "mich", "in", "Be\u00b7th\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hinschmachten stets auf's Neue,", "tokens": ["Hin\u00b7schmach\u00b7ten", "stets", "auf's", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nahmst deines Wesens Verehrung", "tokens": ["Nahmst", "dei\u00b7nes", "We\u00b7sens", "Ver\u00b7eh\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "L\u00e4chelnd hin ohne Scheue,", "tokens": ["L\u00e4\u00b7chelnd", "hin", "oh\u00b7ne", "Scheu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Sahst meiner Seele Verst\u00f6rung,", "tokens": ["Sahst", "mei\u00b7ner", "See\u00b7le", "Ver\u00b7st\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Und f\u00fchltest nimmer Reue.", "tokens": ["Und", "f\u00fchl\u00b7test", "nim\u00b7mer", "Reu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sprich, Weib, ist es Verschw\u00f6rung?", "tokens": ["Sprich", ",", "Weib", ",", "ist", "es", "Ver\u00b7schw\u00f6\u00b7rung", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sprich, ist es Herzensfl\u00e4ue?", "tokens": ["Sprich", ",", "ist", "es", "Her\u00b7zens\u00b7fl\u00e4u\u00b7e", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "O gibt es ", "tokens": ["O", "gibt", "es"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "F\u00fcr so viel Liebe und Treue!", "tokens": ["F\u00fcr", "so", "viel", "Lie\u00b7be", "und", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Du lie\u00dfest mich in Beth\u00f6rung,", "tokens": ["Du", "lie\u00b7\u00dfest", "mich", "in", "Be\u00b7th\u00f6\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Hinschmachten stets auf's Neue,", "tokens": ["Hin\u00b7schmach\u00b7ten", "stets", "auf's", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nahmst deines Wesens Verehrung", "tokens": ["Nahmst", "dei\u00b7nes", "We\u00b7sens", "Ver\u00b7eh\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PP