{"textgrid.poem.54180": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schnipsel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas f\u00e4llt Ihnen ein! Ich habe f\u00fcr einen Bandwurm und drei unm\u00fcndige Kinder zu sorgen!\u00ab", "tokens": ["\u00bb", "was", "f\u00e4llt", "Ih\u00b7nen", "ein", "!", "Ich", "ha\u00b7be", "f\u00fcr", "ei\u00b7nen", "Band\u00b7wurm", "und", "drei", "un\u00b7m\u00fcn\u00b7di\u00b7ge", "Kin\u00b7der", "zu", "sor\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "ART", "NN", "KON", "CARD", "ADJA", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "--+-+-+--+-+--+-+--+--+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Pessimist. \u00bbIch werde also eines Tages sterben. Nat\u00fcrlich \u2013 das kann auch nur mir passieren!\u00ab", "tokens": ["Der", "Pes\u00b7si\u00b7mist", ".", "\u00bb", "Ich", "wer\u00b7de", "al\u00b7so", "ei\u00b7nes", "Ta\u00b7ges", "ster\u00b7ben", ".", "Na\u00b7t\u00fcr\u00b7lich", "\u2013", "das", "kann", "auch", "nur", "mir", "pas\u00b7sie\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "PPER", "VAFIN", "ADV", "ART", "NN", "VVINF", "$.", "ADV", "$(", "PDS", "VMFIN", "ADV", "ADV", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-+--+--+-+-+--", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Wie schlafen die Leute \u2013?", "tokens": ["Wie", "schla\u00b7fen", "die", "Leu\u00b7te", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$(", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Eine Frau, alleinim Pyjama", "tokens": ["Ei\u00b7ne", "Frau", ",", "al\u00b7lei\u00b7nim", "Py\u00b7ja\u00b7ma"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Eine Frau, nicht alleinim Nachthemd", "tokens": ["Ei\u00b7ne", "Frau", ",", "nicht", "al\u00b7lei\u00b7nim", "Nacht\u00b7hemd"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PTKNEG", "APPRART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ein Mann, alleinNachthemd", "tokens": ["Ein", "Mann", ",", "al\u00b7lein", "Nacht\u00b7hemd"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ein Mann, nicht alleinPyjama.", "tokens": ["Ein", "Mann", ",", "nicht", "al\u00b7lein", "Py\u00b7ja\u00b7ma", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Kriege habe ich einmal diesen Satz geh\u00f6rt: \u00bbDie Bohnensuppe ist das Klavier des kleinen Mannes.\u00ab", "tokens": ["Im", "Krie\u00b7ge", "ha\u00b7be", "ich", "ein\u00b7mal", "die\u00b7sen", "Satz", "ge\u00b7h\u00f6rt", ":", "\u00bb", "Die", "Boh\u00b7nen\u00b7sup\u00b7pe", "ist", "das", "Kla\u00b7vier", "des", "klei\u00b7nen", "Man\u00b7nes", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos"