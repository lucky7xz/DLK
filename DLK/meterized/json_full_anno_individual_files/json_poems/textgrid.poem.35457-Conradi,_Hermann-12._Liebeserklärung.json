{"textgrid.poem.35457": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "12. Liebeserkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So hast du denn auch mich bezwungen,", "tokens": ["So", "hast", "du", "denn", "auch", "mich", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich verachtet grenzenlos!", "tokens": ["Die", "ich", "ver\u00b7ach\u00b7tet", "gren\u00b7zen\u00b7los", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu deinem Liebsten mich gedungen \u2013", "tokens": ["Zu", "dei\u00b7nem", "Liebs\u00b7ten", "mich", "ge\u00b7dun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gezerrt zu deinem feilen Scho\u00df!", "tokens": ["Ge\u00b7zerrt", "zu", "dei\u00b7nem", "fei\u00b7len", "Scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In deinen Armen lieg' ich st\u00fcndlich,", "tokens": ["In", "dei\u00b7nen", "Ar\u00b7men", "lieg'", "ich", "st\u00fcnd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und deine M\u00e4tzchen lehrst du mich \u2013", "tokens": ["Un