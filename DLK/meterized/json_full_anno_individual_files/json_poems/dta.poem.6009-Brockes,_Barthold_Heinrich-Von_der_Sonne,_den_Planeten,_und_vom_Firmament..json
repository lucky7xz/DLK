{"dta.poem.6009": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Meinung ist weit leichter, sichrer, kl\u00e4rer.", "tokens": ["Die", "Mei\u00b7nung", "ist", "weit", "leich\u00b7ter", ",", "sich\u00b7rer", ",", "kl\u00e4\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "ADJD", "$,", "ADJA", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie ward zuerst gesetzt, durch sehr ber\u00fchmte Lehrer.", "tokens": ["Sie", "ward", "zu\u00b7erst", "ge\u00b7setzt", ",", "durch", "sehr", "be\u00b7r\u00fchm\u00b7te", "Leh\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Schul in Samos schon hat auch also gedacht,", "tokens": ["Die", "Schul", "in", "Sa\u00b7mos", "schon", "hat", "auch", "al\u00b7so", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "ADV", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-----+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Da\u00df es nohtwendig sey, zur Ordnung unsrer Welt,", "tokens": ["Da\u00df", "es", "noht\u00b7wen\u00b7dig", "sey", ",", "zur", "Ord\u00b7nung", "uns\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Da\u00df das, so sie beseelt, erw\u00e4rmt und helle macht;", "tokens": ["Da\u00df", "das", ",", "so", "sie", "be\u00b7seelt", ",", "er\u00b7w\u00e4rmt", "und", "hel\u00b7le", "macht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "ADV", "PPER", "VVPP", "$,", "VVPP", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jm Mittel-Punct sey hingestellt.", "tokens": ["Jm", "Mit\u00b7tel\u00b7Punct", "sey", "hin\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}