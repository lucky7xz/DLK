{"textgrid.poem.65838": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "112", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein liebend Mitleid schlie\u00dft die Wunde wieder,", "tokens": ["Dein", "lie\u00b7bend", "Mit\u00b7leid", "schlie\u00dft", "die", "Wun\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die in die Stirn mir grub des P\u00f6bels Dienst.", "tokens": ["Die", "in", "die", "Stirn", "mir", "grub", "des", "P\u00f6\u00b7bels", "Dienst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was k\u00fcmmert mich mein Leumund f\u00fcr und wider,", "tokens": ["Was", "k\u00fcm\u00b7mert", "mich", "mein", "Leu\u00b7mund", "f\u00fcr", "und", "wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Du bist die Welt mir. Deinem Mund best\u00e4ndig", "tokens": ["Du", "bist", "die", "Welt", "mir", ".", "Dei\u00b7nem", "Mund", "be\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "$.", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vertrau' ich all mein Lob- und Tadelsrecht.", "tokens": ["Ver\u00b7trau'", "ich", "all", "mein", "Lob", "und", "Ta\u00b7dels\u00b7recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "PPOSAT", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcr niemand bin ich sonst, niemand f\u00fcr mich lebendig,", "tokens": ["F\u00fcr", "nie\u00b7mand", "bin", "ich", "sonst", ",", "nie\u00b7mand", "f\u00fcr", "mich", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ADV", "$,", "PIS", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der mir den eh'rnen Sinn links oder rechts bewegt.", "tokens": ["Der", "mir", "den", "eh'r\u00b7nen", "Sinn", "links", "o\u00b7der", "rechts", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADV", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In tiefsten Abgrund werf' ich alle Sorgen", "tokens": ["In", "tiefs\u00b7ten", "Ab\u00b7grund", "wer\u00b7f'", "ich", "al\u00b7le", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Um Menschengunst. Mein taubes Otterohr", "tokens": ["Um", "Men\u00b7schen\u00b7gunst", ".", "Mein", "tau\u00b7bes", "Ot\u00b7te\u00b7rohr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "NN", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wir