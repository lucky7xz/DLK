{"textgrid.poem.62597": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken \u00fcber den Epheu", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Epheu stellet mir,", "tokens": ["Der", "E\u00b7pheu", "stel\u00b7let", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das Bild von einem wahren Christen f\u00fcr.", "tokens": ["Das", "Bild", "von", "ei\u00b7nem", "wah\u00b7ren", "Chris\u00b7ten", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Epheu w\u00e4chst nicht auf der Erde, wie der Klee;", "tokens": ["Der", "E\u00b7pheu", "w\u00e4chst", "nicht", "auf", "der", "Er\u00b7de", ",", "wie", "der", "Klee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er flicht sich in die H\u00f6h,", "tokens": ["Er", "flicht", "sich", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und schlingt sich um die hohen B\u00e4ume.", "tokens": ["Und", "schlingt", "sich", "um", "die", "ho\u00b7hen", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So macht es auch ein \u00e4chtes Christen-Kind,", "tokens": ["So", "macht", "es", "auch", "ein", "\u00e4ch\u00b7tes", "Chris\u00b7ten\u00b7Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das sein Vergn\u00fcgen nicht auf dieser Erde findt:", "tokens": ["Das", "sein", "Ver\u00b7gn\u00fc\u00b7gen", "nicht", "auf", "die\u00b7ser", "Er\u00b7de", "findt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PTKNEG", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es schwingt sich in die H\u00f6h zu Gott hinauf,", "tokens": ["Es", "schwingt", "sich", "in", "die", "H\u00f6h", "zu", "Gott", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und bleibt daselbst nach seines Worts Verlangen", "tokens": ["Und", "bleibt", "da\u00b7selbst", "nach", "sei\u00b7nes", "Worts", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ganz feste an ihm hangen,", "tokens": ["Ganz", "fes\u00b7te", "an", "ihm", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und f\u00fchrt im Himmel seinen Lauf.", "tokens": ["Und", "f\u00fchrt", "im", "