{"dta.poem.10": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das gute Gewissen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was kan uns auf dieser Erden,", "tokens": ["Was", "kan", "uns", "auf", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Alle Leiden, und Beschwerden,", "tokens": ["Al\u00b7le", "Lei\u00b7den", ",", "und", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Plagen, alle Noth", "tokens": ["Al\u00b7le", "Pla\u00b7gen", ",", "al\u00b7le", "Noth"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja! den f\u00fcrchterlichen Todt,", "tokens": ["Ja", "!", "den", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Todt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Bitterkeit vers\u00fcssen?", "tokens": ["Al\u00b7le", "Bit\u00b7ter\u00b7keit", "ver\u00b7s\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ist gut in b\u00f6sen Zeiten,", "tokens": ["Was", "ist", "gut", "in", "b\u00f6\u00b7sen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei der Pest von falschen Leuten,", "tokens": ["Bei", "der", "Pest", "von", "fal\u00b7schen", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn man unter Mesech wohnt,", "tokens": ["Wenn", "man", "un\u00b7ter", "Me\u00b7sech", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da man Guts mit B\u00f6sen lohnt,", "tokens": ["Da", "man", "Guts", "mit", "B\u00f6\u00b7sen", "lohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ist gut vor Judas-K\u00fcssen?", "tokens": ["Was", "ist", "gut", "vor", "Ju\u00b7das\u00b7K\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was braucht man vor die Verl\u00e4umden,", "tokens": ["Was", "braucht", "man", "vor", "die", "Ver\u00b7l\u00e4um\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die uns offenbahr anfeinden,", "tokens": ["Die", "uns", "of\u00b7fen\u00b7bahr", "an\u00b7fein\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder die uns Nezze stelln,", "tokens": ["O\u00b7der", "die", "uns", "Nez\u00b7ze", "stelln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um uns heimlich zu beschnelln,", "tokens": ["Um", "uns", "heim\u00b7lich", "zu", "be\u00b7schnelln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vor der Neider Schlangen-Bissen?", "tokens": ["Vor", "der", "Nei\u00b7der", "Schlan\u00b7gen\u00b7Bis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn man in betr\u00fcbten Stande;", "tokens": ["Wenn", "man", "in", "be\u00b7tr\u00fcb\u00b7ten", "Stan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn der Krankheit harte Bande", "tokens": ["Wenn", "der", "Krank\u00b7heit", "har\u00b7te", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen siechen K\u00f6rper qu\u00e4ln,", "tokens": ["Ei\u00b7nen", "sie\u00b7chen", "K\u00f6r\u00b7per", "qu\u00e4ln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was kan man alsdenn erw\u00e4hln,", "tokens": ["Was", "kan", "man", "als\u00b7denn", "er\u00b7w\u00e4hln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sich zu einen sanften K\u00fcssen?", "tokens": ["Sich", "zu", "ei\u00b7nen", "sanf\u00b7ten", "K\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was kan uns Vergn\u00fcgen geben,", "tokens": ["Was", "kan", "uns", "Ver\u00b7gn\u00fc\u00b7gen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn wir aus dem eitlen Leben,", "tokens": ["Wenn", "wir", "aus", "dem", "eit\u00b7len", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu der Ewigkeit fortgehn;", "tokens": ["Zu", "der", "E\u00b7wig\u00b7keit", "fort\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn wir Christi Richtstuhl sehn,", "tokens": ["Wenn", "wir", "Chris\u00b7ti", "Richt\u00b7stuhl", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Davor wir erscheinen m\u00fcssen?", "tokens": ["Da\u00b7vor", "wir", "er\u00b7schei\u00b7nen", "m\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVINF", "VMINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}