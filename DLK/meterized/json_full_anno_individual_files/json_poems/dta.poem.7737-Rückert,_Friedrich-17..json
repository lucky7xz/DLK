{"dta.poem.7737": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Quelle, wenn du hier bew\u00e4ssert hast den Garten,", "tokens": ["O", "Quel\u00b7le", ",", "wenn", "du", "hier", "be\u00b7w\u00e4s\u00b7sert", "hast", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Flie\u00df nur dem n\u00e4chsten zu, der durstig auch wird warten.", "tokens": ["Flie\u00df", "nur", "dem", "n\u00e4chs\u00b7ten", "zu", ",", "der", "durs\u00b7tig", "auch", "wird", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "PTKVZ", "$,", "PRELS", "ADJD", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil \u00fcbern Berg das Licht des Morgens uns gekommen,", "tokens": ["Weil", "\u00fc\u00b7bern", "Berg", "das", "Licht", "des", "Mor\u00b7gens", "uns", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "ART", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "R\u00fchmt sich der stolze Berg, es sei von ihm entglommen.", "tokens": ["R\u00fchmt", "sich", "der", "stol\u00b7ze", "Berg", ",", "es", "sei", "von", "ihm", "ent\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Sonn' auch prahle nicht, da\u00df sie die Welt erhelle;", "tokens": ["Die", "Sonn'", "auch", "prah\u00b7le", "nicht", ",", "da\u00df", "sie", "die", "Welt", "er\u00b7hel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sch\u00f6pfet auch ihr Licht nur aus verborgnem Quelle.", "tokens": ["Sie", "sch\u00f6p\u00b7fet", "auch", "ihr", "Licht", "nur", "aus", "ver\u00b7borg\u00b7nem", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Lehrer, den du lernst, war eines Lehrers Lerner;", "tokens": ["Der", "Leh\u00b7rer", ",", "den", "du", "lernst", ",", "war", "ei\u00b7nes", "Leh\u00b7rers", "Ler\u00b7ner", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist nur einen Grad vom ersten Lehrer ferner.", "tokens": ["Du", "bist", "nur", "ei\u00b7nen", "Grad", "vom", "ers\u00b7ten", "Leh\u00b7rer", "fer\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nicht das gedeiht zumeist, was man gepflegt mit Flei\u00df;", "tokens": ["Nicht", "das", "ge\u00b7deiht", "zu\u00b7meist", ",", "was", "man", "ge\u00b7pflegt", "mit", "Flei\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "VVFIN", "ADV", "$,", "PRELS", "PIS", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stets das Lebendigste w\u00e4chst ohne da\u00df mans wei\u00df.", "tokens": ["Stets", "das", "Le\u00b7ben\u00b7digs\u00b7te", "w\u00e4chst", "oh\u00b7ne", "da\u00df", "mans", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "KOUS", "PIS", "VVFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Drum wechselt Tag und Nacht, weil bald Nachtthaubefeuchtung", "tokens": ["Drum", "wech\u00b7selt", "Tag", "und", "Nacht", ",", "weil", "bald", "Nachtt\u00b7hau\u00b7be\u00b7feuch\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "NN", "KON", "NN", "$,", "KOUS", "ADV", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Leben n\u00f6thig hat, bald Morgensonnerleuchtung.", "tokens": ["Das", "Le\u00b7ben", "n\u00f6\u00b7thig", "hat", ",", "bald", "Mor\u00b7gen\u00b7son\u00b7ner\u00b7leuch\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAFIN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Drum, weil er in der Nacht verga\u00df die alten Lieder,", "tokens": ["Drum", ",", "weil", "er", "in", "der", "Nacht", "ver\u00b7ga\u00df", "die", "al\u00b7ten", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Singt sie mit neuer Lust der Vogel t\u00e4glich wieder.", "tokens": ["Singt", "sie", "mit", "neu\u00b7er", "Lust", "der", "Vo\u00b7gel", "t\u00e4g\u00b7lich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "ART", "NE", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Erinnrung d\u00e4mmert mir, da\u00df ich schon einst so sang,", "tokens": ["E\u00b7rinn\u00b7rung", "d\u00e4m\u00b7mert", "mir", ",", "da\u00df", "ich", "schon", "einst", "so", "sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer neu Gef\u00fchl liegt in dem alten Klang.", "tokens": ["Und", "im\u00b7mer", "neu", "Ge\u00b7f\u00fchl", "liegt", "in", "dem", "al\u00b7ten", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}