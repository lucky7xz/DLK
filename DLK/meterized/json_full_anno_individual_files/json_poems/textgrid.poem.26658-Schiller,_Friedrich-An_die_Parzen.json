{"textgrid.poem.26658": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Parzen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht ins Gew\u00fchl der rauschenden Redouten", "tokens": ["Nicht", "ins", "Ge\u00b7w\u00fchl", "der", "rau\u00b7schen\u00b7den", "Re\u00b7dou\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Wo Stutzerwitz sich wunderherrlich sprei\u00dft", "tokens": ["Wo", "Stut\u00b7zer\u00b7witz", "sich", "wun\u00b7der\u00b7herr\u00b7lich", "sprei\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und leichter als das Netz der fliegenden Bajouten", "tokens": ["Und", "leich\u00b7ter", "als", "das", "Netz", "der", "flie\u00b7gen\u00b7den", "Ba\u00b7jou\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Tugend junger Sch\u00f6nen rei\u00dft; \u2013", "tokens": ["Die", "Tu\u00b7gend", "jun\u00b7ger", "Sch\u00f6\u00b7nen", "rei\u00dft", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht vor die schmeichlerische Toilette,", "tokens": ["Nicht", "vor", "die", "schmeich\u00b7le\u00b7ri\u00b7sche", "To\u00b7i\u00b7let\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wovor die Eitelkeit, als ihrem G\u00f6tzen, kniet,", "tokens": ["Wo\u00b7vor", "die", "Ei\u00b7tel\u00b7keit", ",", "als", "ih\u00b7rem", "G\u00f6t\u00b7zen", ",", "kniet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und oft in w\u00e4rmere Gebete", "tokens": ["Und", "oft", "in", "w\u00e4r\u00b7me\u00b7re", "Ge\u00b7be\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als zu dem Himmel selbst entgl\u00fcht;", "tokens": ["Als", "zu", "dem", "Him\u00b7mel", "selbst", "ent\u00b7gl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht hinter der Gardinen listgen Schleier,", "tokens": ["Nicht", "hin\u00b7ter", "der", "Gar\u00b7di\u00b7nen", "list\u00b7gen", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo heuchlerische Nacht das Aug der Welt betr\u00fcgt", "tokens": ["Wo", "heuch\u00b7le\u00b7ri\u00b7sche", "Nacht", "das", "Aug", "der", "Welt", "be\u00b7tr\u00fcgt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Herzen, kalt im Sonnenfeuer,", "tokens": ["Und", "Her\u00b7zen", ",", "kalt", "im", "Son\u00b7nen\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In gl\u00fchende Begierden wiegt,", "tokens": ["In", "gl\u00fc\u00b7hen\u00b7de", "Be\u00b7gier\u00b7den", "wiegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo wir die Weisheit ", "tokens": ["Wo", "wir", "die", "Weis\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die k\u00fchnlich Ph\u00f6bus' Strahlen trinkt,", "tokens": ["Die", "k\u00fchn\u00b7lich", "Ph\u00f6\u00b7bus'", "Strah\u00b7len", "trinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo M\u00e4nner gleich den Knaben diebisch naschen,", "tokens": ["Wo", "M\u00e4n\u00b7ner", "gleich", "den", "Kna\u00b7ben", "die\u00b7bisch", "na\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Plato von den Sph\u00e4ren sinkt \u2013", "tokens": ["Und", "Pla\u00b7to", "von", "den", "Sph\u00e4\u00b7ren", "sinkt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu dir \u2013 zu dir, du einsames Geschwister,", "tokens": ["Zu", "dir", "\u2013", "zu", "dir", ",", "du", "ein\u00b7sa\u00b7mes", "Ge\u00b7schwis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch T\u00f6chtern des Geschickes, flieht", "tokens": ["Euch", "T\u00f6ch\u00b7tern", "des", "Ge\u00b7schi\u00b7ckes", ",", "flieht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei meiner Laute leiserem Geflister", "tokens": ["Bei", "mei\u00b7ner", "Lau\u00b7te", "lei\u00b7se\u00b7rem", "Ge\u00b7flis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schwerm\u00fctig s\u00fc\u00df mein Minnelied.", "tokens": ["Schwer\u00b7m\u00fc\u00b7tig", "s\u00fc\u00df", "mein", "Min\u00b7ne\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ihr einzigen, f\u00fcr die noch kein Sonett gegirret,", "tokens": ["Ihr", "ein\u00b7zi\u00b7gen", ",", "f\u00fcr", "die", "noch", "kein", "So\u00b7nett", "ge\u00b7gir\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "APPR", "ART", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um deren Geld kein Wucherer noch warb,", "tokens": ["Um", "de\u00b7ren", "Geld", "kein", "Wu\u00b7che\u00b7rer", "noch", "warb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Stutzer noch Klag-Arien geschwirret,", "tokens": ["Kein", "Stut\u00b7zer", "noch", "Klag\u00b7\u00b7A\u00b7ri\u00b7en", "ge\u00b7schwir\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Kein Sch\u00e4fer noch arkadisch starb.", "tokens": ["Kein", "Sch\u00e4\u00b7fer", "noch", "ar\u00b7ka\u00b7disch", "starb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die ihr den Nervenfaden unsers Lebens", "tokens": ["Die", "ihr", "den", "Ner\u00b7ven\u00b7fa\u00b7den", "un\u00b7sers", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch weiche Finger sorgsam treibt,", "tokens": ["Durch", "wei\u00b7che", "Fin\u00b7ger", "sorg\u00b7sam", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis unterm Klang der Schere sich vergebens", "tokens": ["Bis", "un\u00b7term", "Klang", "der", "Sche\u00b7re", "sich", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "ART", "NN", "PRF", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die zarte Spinnewebe str\u00e4ubt.", "tokens": ["Die", "zar\u00b7te", "Spin\u00b7ne\u00b7we\u00b7be", "str\u00e4ubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da\u00df du auch mir den Lebensfaden spinntest,", "tokens": ["Da\u00df", "du", "auch", "mir", "den", "Le\u00b7bens\u00b7fa\u00b7den", "spinn\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00fc\u00df ich, o Klotho, deine Hand; \u2013", "tokens": ["K\u00fc\u00df", "ich", ",", "o", "Klo\u00b7tho", ",", "dei\u00b7ne", "Hand", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NE", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df du noch nicht den jungen Faden trenntest,", "tokens": ["Da\u00df", "du", "noch", "nicht", "den", "jun\u00b7gen", "Fa\u00b7den", "trenn\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nimm, Lachesis, dies Blumenband.", "tokens": ["Nimm", ",", "La\u00b7che\u00b7sis", ",", "dies", "Blu\u00b7men\u00b7band", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "PDS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Oft hast du Dornen an den Faden,", "tokens": ["Oft", "hast", "du", "Dor\u00b7nen", "an", "den", "Fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch \u00f6fter Rosen drangereiht,", "tokens": ["Noch", "\u00f6f\u00b7ter", "Ro\u00b7sen", "dran\u00b7ge\u00b7reiht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Dorn' und Rosen an dem Faden", "tokens": ["F\u00fcr", "Dorn'", "und", "Ro\u00b7sen", "an", "dem", "Fa\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei, Klotho, dir dies Lied geweiht.", "tokens": ["Sei", ",", "Klo\u00b7tho", ",", "dir", "dies", "Lied", "ge\u00b7weiht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NE", "$,", "PPER", "PDS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Oft haben st\u00fcrmende Affekte", "tokens": ["Oft", "ha\u00b7ben", "st\u00fcr\u00b7men\u00b7de", "Af\u00b7fek\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Den weichen Zwirn herumgezerrt,", "tokens": ["Den", "wei\u00b7chen", "Zwirn", "her\u00b7um\u00b7ge\u00b7zerrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oft riesenm\u00e4\u00dfige Projekte", "tokens": ["Oft", "rie\u00b7sen\u00b7m\u00e4\u00b7\u00dfi\u00b7ge", "Pro\u00b7jek\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Fadens freien Schwung gesperrt;", "tokens": ["Des", "Fa\u00b7dens", "frei\u00b7en", "Schwung", "ge\u00b7sperrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Oft in woll\u00fcstig s\u00fc\u00dfer Stunde", "tokens": ["Oft", "in", "wol\u00b7l\u00fcs\u00b7tig", "s\u00fc\u00b7\u00dfer", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War mir der Faden fast zu fein,", "tokens": ["War", "mir", "der", "Fa\u00b7den", "fast", "zu", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch \u00f6fter an der Schwermut Schauerschlunde", "tokens": ["Noch", "\u00f6f\u00b7ter", "an", "der", "Schwer\u00b7mut", "Schau\u00b7er\u00b7schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00dft er zu fest gesponnen sein:", "tokens": ["Mu\u00dft", "er", "zu", "fest", "ge\u00b7spon\u00b7nen", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKA", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Dies, Klotho, und noch andre L\u00fcgen", "tokens": ["Dies", ",", "Klo\u00b7tho", ",", "und", "noch", "and\u00b7re", "L\u00fc\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "$,", "NE", "$,", "KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bitt ich dir itzt mit Tr\u00e4nen ab,", "tokens": ["Bitt", "ich", "dir", "itzt", "mit", "Tr\u00e4\u00b7nen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun soll mir auch fortan gen\u00fcgen,", "tokens": ["Nun", "soll", "mir", "auch", "for\u00b7tan", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was mir die weise Klotho gab.", "tokens": ["Was", "mir", "die", "wei\u00b7se", "Klo\u00b7tho", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Nur la\u00df an Rosen nie die Schere klirren,", "tokens": ["Nur", "la\u00df", "an", "Ro\u00b7sen", "nie", "die", "Sche\u00b7re", "klir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "APPR", "NN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An Dornen nur \u2013 doch wie du willst.", "tokens": ["An", "Dor\u00b7nen", "nur", "\u2013", "doch", "wie", "du", "willst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$(", "KON", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df, wenn du willst, die Totenschere klirren,", "tokens": ["La\u00df", ",", "wenn", "du", "willst", ",", "die", "To\u00b7ten\u00b7sche\u00b7re", "klir\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "VMFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn du dies ", "tokens": ["Wenn", "du", "dies"], "token_info": ["word", "word", "word"], 