{"textgrid.poem.24017": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kind, das Bette ist bereit,", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kind, das Bette ist bereit,", "tokens": ["Kind", ",", "das", "Bet\u00b7te", "ist", "be\u00b7reit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lege dich nun nieder", "tokens": ["Le\u00b7ge", "dich", "nun", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und thu ab dein schwarzes Kleid,", "tokens": ["Und", "thu", "ab", "dein", "schwar\u00b7zes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Rock und Hemd und Mieder.", "tokens": ["Rock", "und", "Hemd", "und", "Mie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Eva, Eva, Evalein,", "tokens": ["E\u00b7va", ",", "E\u00b7va", ",", "E\u00b7val\u00b7ein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Lasse dich beschauen!", "tokens": ["Las\u00b7se", "dich", "be\u00b7schau\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ist das wirklich Alles mein?", "tokens": ["Ist", "das", "wirk\u00b7lich", "Al\u00b7les", "mein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADJD", "PIS", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf ich michs getrauen?", "tokens": ["Darf", "ich", "michs", "ge\u00b7trau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Pst! Sie spielt die Schl\u00e4ferin.", "tokens": ["Pst", "!", "Sie", "spielt", "die", "Schl\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leise und verstohlen", "tokens": ["Lei\u00b7se", "und", "ver\u00b7stoh\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schleich ich mich zur Vase hin,", "tokens": ["Schleich", "ich", "mich", "zur", "Va\u00b7se", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rosen herzuholen.", "tokens": ["Ro\u00b7sen", "her\u00b7zu\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Und ich \u00fcbersch\u00fctte sie,", "tokens": ["Und", "ich", "\u00fc\u00b7ber\u00b7sch\u00fct\u00b7te", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Brust und Leib und Lenden,", "tokens": ["Brust", "und", "Leib", "und", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ich sinke in die Knie", "tokens": ["Und", "ich", "sin\u00b7ke", "in", "die", "Knie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit erhobnen H\u00e4nden.", "tokens": ["Mit", "er\u00b7hob\u00b7nen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Der noch nie ich am Altar", "tokens": ["Der", "noch", "nie", "ich", "am", "Al\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines Gottes kniete,", "tokens": ["Ei\u00b7nes", "Got\u00b7tes", "knie\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Meine Rosen bring ich dar", "tokens": ["Mei\u00b7ne", "Ro\u00b7sen", "bring", "ich", "dar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir, oh Aphrodite.", "tokens": ["Dir", ",", "oh", "A\u00b7phro\u00b7di\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "FM", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gottlos lief ich kreuz und quer", "tokens": ["Gott\u00b7los", "lief", "ich", "kreuz", "und", "quer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit beschwerten Sinnen", "tokens": ["Mit", "be\u00b7schwer\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Hinter leeren Schatten her,", "tokens": ["Hin\u00b7ter", "lee\u00b7ren", "Schat\u00b7ten", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbwahrheit\u00ab zu gewinnen.", "tokens": ["\u00bb", "wahr\u00b7heit", "\u00ab", "zu", "ge\u00b7win\u00b7nen", "."], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NN", "$(", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Nichts gewann ich und verlor", "tokens": ["Nichts", "ge\u00b7wann", "ich", "und", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine besten Tage,", "tokens": ["Mei\u00b7ne", "bes\u00b7ten", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Denn sie raunten mir ins Ohr", "tokens": ["Denn", "sie", "raun\u00b7ten", "mir", "ins", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer neue Frage.", "tokens": ["Im\u00b7mer", "neu\u00b7e", "Fra\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Oh die Schatten! Hin und her!", "tokens": ["Oh", "die", "Schat\u00b7ten", "!", "Hin", "und", "her", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "NN", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die verw\u00fcnschten Spinnen:", "tokens": ["Die", "ver\u00b7w\u00fcnschten", "Spin\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Doch ich folge nun nicht mehr", "tokens": ["Doch", "ich", "fol\u00b7ge", "nun", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKNEG", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Diesen Fragerinnen.", "tokens": ["Die\u00b7sen", "Fra\u00b7ge\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Dir, die keine Fragen wei\u00df,", "tokens": ["Dir", ",", "die", "kei\u00b7ne", "Fra\u00b7gen", "wei\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die nur lacht: ich gebe!,", "tokens": ["Die", "nur", "lacht", ":", "ich", "ge\u00b7be", "!", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "VVFIN", "$.", "PPER", "VVFIN", "$.", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dir str\u00f6mt meine Andacht hei\u00df:", "tokens": ["Dir", "str\u00f6mt", "mei\u00b7ne", "An\u00b7dacht", "hei\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nheit, sieh, ich lebe!", "tokens": ["Sch\u00f6n\u00b7heit", ",", "sieh", ",", "ich", "le\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Liebliche, oh nimm mich hin,", "tokens": ["Lieb\u00b7li\u00b7che", ",", "oh", "nimm", "mich", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Da\u00df ich neu erwarme;", "tokens": ["Da\u00df", "ich", "neu", "er\u00b7war\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Aphrodite, Schenkerin,", "tokens": ["A\u00b7phro\u00b7di\u00b7te", ",", "Schen\u00b7ke\u00b7rin", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nimm mich in die Arme.", "tokens": ["Nimm", "mich", "in", "die", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Und mein s\u00fc\u00dfes M\u00e4dchen lacht", "tokens": ["Und", "mein", "s\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen", "lacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rosend\u00fcftetrunken.", "tokens": ["Ro\u00b7sen\u00b7d\u00fcf\u00b7te\u00b7trun\u00b7ken", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"},