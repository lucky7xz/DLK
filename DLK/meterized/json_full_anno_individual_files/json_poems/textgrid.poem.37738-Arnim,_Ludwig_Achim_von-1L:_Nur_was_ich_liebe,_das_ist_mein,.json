, "line.3": {"text": "Ist mein die Perl, geh\u00f6rt die Schal auch mir,", "tokens": ["Ist", "mein", "die", "Perl", ",", "ge\u00b7h\u00f6rt", "die", "Schal", "auch", "mir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ART", "NN", "$,", "VVFIN", "ART", "NN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was ist sie ohne dich, die ihre Zier.", "tokens": ["Was", "ist", "sie", "oh\u00b7ne", "dich", ",", "die", "ih\u00b7re", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "PPER", "$,", "PRELS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und was ich habe, das ist dein,", "tokens": ["Und", "was", "ich", "ha\u00b7be", ",", "das", "ist", "dein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "$,", "PDS", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kann es ohne dich nicht leiden,", "tokens": ["Ich", "kann", "es", "oh\u00b7ne", "dich", "nicht", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df ihn fliehn, den lichten gr\u00fcnen Hayn,", "tokens": ["Ich", "mu\u00df", "ihn", "fliehn", ",", "den", "lich\u00b7ten", "gr\u00fc\u00b7nen", "Hayn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der liebe Waldhorn Klang wird m