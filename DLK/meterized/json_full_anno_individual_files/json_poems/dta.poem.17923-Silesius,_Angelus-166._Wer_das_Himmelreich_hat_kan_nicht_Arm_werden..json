{"dta.poem.17923": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "166.  Wer das Himmelreich hat kan nicht  \n Arm werden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Das Reich Gotts ist in un\u00df. Hastu schon hier auf Erden", "tokens": ["Das", "Reich", "Gotts", "ist", "in", "un\u00df", ".", "Has\u00b7tu", "schon", "hier", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VAFIN", "APPR", "NE", "$.", "NE", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein gantzes Reich in dir/ was f\u00fcrchstu arm zuwerden?", "tokens": ["Ein", "gant\u00b7zes", "Reich", "in", "dir", "/", "was", "f\u00fcrch\u00b7stu", "arm", "zu\u00b7wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "$(", "PWS", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}