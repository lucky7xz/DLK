{"textgrid.poem.25286": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf allen Forsten, Wiesengr\u00fcnden/", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf allen Forsten, Wiesengr\u00fcnden/", "tokens": ["Auf", "al\u00b7len", "Fors\u00b7ten", ",", "Wie\u00b7sen\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf meines Grames Heimat/ lagert Nacht.", "tokens": ["Auf", "mei\u00b7nes", "Gra\u00b7mes", "Hei\u00b7mat", "/", "la\u00b7gert", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur droben, droben jene Fernen", "tokens": ["Nur", "dro\u00b7ben", ",", "dro\u00b7ben", "je\u00b7ne", "Fer\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verkl\u00e4ren sich, entz\u00fcnden", "tokens": ["Ver\u00b7kl\u00e4\u00b7ren", "sich", ",", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die wundervollste Silberpracht", "tokens": ["Die", "wun\u00b7der\u00b7volls\u00b7te", "Sil\u00b7ber\u00b7pracht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Funkelsternen.", "tokens": ["Von", "Fun\u00b7kels\u00b7ter\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "O Sternenhimmel/", "tokens": ["O", "Ster\u00b7nen\u00b7him\u00b7mel", "/"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Du Weltengewimmel!", "tokens": ["Du", "Wel\u00b7ten\u00b7ge\u00b7wim\u00b7mel", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Ihr dunkelblauen", "tokens": ["Ihr", "dun\u00b7kel\u00b7blau\u00b7en"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Lichtbes\u00e4ten Auen", "tokens": ["Licht\u00b7be\u00b7s\u00e4\u00b7ten", "Au\u00b7en"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Der Ewigkeit!", "tokens": ["Der", "E\u00b7wig\u00b7keit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Euch tief zu schauen", "tokens": ["Euch", "tief", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ist Seligkeit,", "tokens": ["Ist", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ist k\u00fchler Trost", "tokens": ["Ist", "k\u00fch\u00b7ler", "Trost"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "F\u00fcr diese brennenden Wunden/", "tokens": ["F\u00fcr", "die\u00b7se", "bren\u00b7nen\u00b7den", "Wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Die mir, erbost", "tokens": ["Die", "mir", ",", "er\u00b7bost"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "PPER", "$,", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Gleich kl\u00e4ffenden Hunden,", "tokens": ["Gleich", "kl\u00e4f\u00b7fen\u00b7den", "Hun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Die Menschenmeute schlug, um nun", "tokens": ["Die", "Men\u00b7schen\u00b7meu\u00b7te", "schlug", ",", "um", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUI", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mit sattem Hasse auszuruhn ...", "tokens": ["Mit", "sat\u00b7tem", "Has\u00b7se", "aus\u00b7zu\u00b7ruhn", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Sternenhimmel/", "tokens": ["O", "Ster\u00b7nen\u00b7him\u00b7mel", "/"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Du Weltengewimmel!", "tokens": ["Du", "Wel\u00b7ten\u00b7ge\u00b7wim\u00b7mel", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Milchstra\u00dfe, ungeheuer, breit,", "tokens": ["Milch\u00b7stra\u00b7\u00dfe", ",", "un\u00b7ge\u00b7heu\u00b7er", ",", "breit", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vielbuchtig wie ein ausgetretener Strom", "tokens": ["Viel\u00b7buch\u00b7tig", "wie", "ein", "aus\u00b7ge\u00b7tre\u00b7te\u00b7ner", "Strom"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Durchquerst du die Unendlichkeit/", "tokens": ["Durch\u00b7querst", "du", "die", "Un\u00b7end\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Welle an Welle,", "tokens": ["Wel\u00b7le", "an", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Nebel an Nebel/", "tokens": ["Ne\u00b7bel", "an", "Ne\u00b7bel", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Jede Welle ein Lichtermeer,", "tokens": ["Je\u00b7de", "Wel\u00b7le", "ein", "Lich\u00b7ter\u00b7meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Jeder Nebel ein Weltenheer.", "tokens": ["Je\u00b7der", "Ne\u00b7bel", "ein", "Wel\u00b7ten\u00b7heer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "An des Lichtstroms Ufern bl\u00fchn", "tokens": ["An", "des", "Licht\u00b7stroms", "U\u00b7fern", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Gro\u00dfe Sterne, schwefelflammenblau.", "tokens": ["Gro\u00b7\u00dfe", "Ster\u00b7ne", ",", "schwe\u00b7fel\u00b7flam\u00b7men\u00b7blau", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Manche funkeln rot und gr\u00fcn", "tokens": ["Man\u00b7che", "fun\u00b7keln", "rot", "und", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie besonnter Blumentau.", "tokens": ["Wie", "be\u00b7sonn\u00b7ter", "Blu\u00b7men\u00b7tau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Sternschnuppen spr\u00fchn/", "tokens": ["Stern\u00b7schnup\u00b7pen", "spr\u00fchn", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Leuchtk\u00e4fer auf dunkler Flur.", "tokens": ["Leucht\u00b7k\u00e4\u00b7fer", "auf", "dunk\u00b7ler", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "G\u00f6ttergleich auf hehren Thronen,", "tokens": ["G\u00f6t\u00b7ter\u00b7gleich", "auf", "heh\u00b7ren", "Thro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Blitzen mit den Kronen", "tokens": ["Blit\u00b7zen", "mit", "den", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Jupiter, Sirius, Arktur.", "tokens": ["Ju\u00b7pi\u00b7ter", ",", "Si\u00b7rius", ",", "Ark\u00b7tur", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Zum Polarstern, seit \u00c4onen,", "tokens": ["Zum", "Po\u00b7lars\u00b7tern", ",", "seit", "\u00c4\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Zielt der Wagen wie gebannt.", "tokens": ["Zielt", "der", "Wa\u00b7gen", "wie", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Von Demant", "tokens": ["Von", "De\u00b7mant"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+--", "measure": "dactylic.init"}, "line.22": {"text": "Flammt Orions G\u00fcrtelbild.", "tokens": ["Flammt", "O\u00b7rions", "G\u00fcr\u00b7tel\u00b7bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Gemma, reizend, m\u00e4dchenmild,", "tokens": ["Gem\u00b7ma", ",", "rei\u00b7zend", ",", "m\u00e4d\u00b7chen\u00b7mild", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Regenbogenbunt sich malend,", "tokens": ["Re\u00b7gen\u00b7bo\u00b7gen\u00b7bunt", "sich", "ma\u00b7lend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Winkt dem Mars/ der fackelrot,", "tokens": ["Winkt", "dem", "Mars", "/", "der", "fa\u00b7ckel\u00b7rot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Schlachten sinnend loht.", "tokens": ["Schlach\u00b7ten", "sin\u00b7nend", "loht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.27": {"text": "Alle Schwestern \u00fcberstrahlend", "tokens": ["Al\u00b7le", "Schwes\u00b7tern", "\u00fc\u00b7bers\u00b7trah\u00b7lend"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Taucht der Liebe Stern mit Schneegefunkel", "tokens": ["Taucht", "der", "Lie\u00b7be", "Stern", "mit", "Schnee\u00b7ge\u00b7fun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Aus des Forstes ernstem Dunkel.", "tokens": ["Aus", "des", "Fors\u00b7tes", "erns\u00b7tem", "Dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie feierliche, leise", "tokens": ["Und", "wie", "fei\u00b7er\u00b7li\u00b7che", ",", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "ADJA", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hingehauchte Harfenweise", "tokens": ["Hin\u00b7ge\u00b7hauch\u00b7te", "Har\u00b7fen\u00b7wei\u00b7se"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6r ich nun die Sterne klingen/", "tokens": ["H\u00f6r", "ich", "nun", "die", "Ster\u00b7ne", "klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich im Auge/ sinnen, singen:", "tokens": ["Mich", "im", "Au\u00b7ge", "/", "sin\u00b7nen", ",", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$(", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbsei still und lausche/ lauschend gleite", "tokens": ["\u00bb", "sei", "still", "und", "lau\u00b7sche", "/", "lau\u00b7schend", "glei\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VAFIN", "ADJD", "KON", "ADJA", "$(", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zum k\u00fchlen Rasen/ breit', breite", "tokens": ["Zum", "k\u00fch\u00b7len", "Ra\u00b7sen", "/", "breit'", ",", "brei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "$(", "VVFIN", "$,", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Arme andachtsvoll empor!", "tokens": ["Die", "Ar\u00b7me", "an\u00b7dachts\u00b7voll", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Dunkelblau, in Silberschauer", "tokens": ["In", "Dun\u00b7kel\u00b7blau", ",", "in", "Sil\u00b7ber\u00b7schau\u00b7er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df taumlig deine Augen sinken", "tokens": ["La\u00df", "taum\u00b7lig", "dei\u00b7ne", "Au\u00b7gen", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADJD", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dieser Kr\u00e4nkung letzte Trauer", "tokens": ["Und", "die\u00b7ser", "Kr\u00e4n\u00b7kung", "letz\u00b7te", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In unserm Ruhemeer ertrinken!", "tokens": ["In", "un\u00b7serm", "Ru\u00b7he\u00b7meer", "er\u00b7trin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Menschentorheit wund gesteinigt,", "tokens": ["Von", "Men\u00b7schen\u00b7tor\u00b7heit", "wund", "ge\u00b7stei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Im Strahlenquell gesund gereinigt,", "tokens": ["Im", "Strah\u00b7len\u00b7quell", "ge\u00b7sund", "ge\u00b7rei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sollst du ein Heil der Erden,", "tokens": ["Sollst", "du", "ein", "Heil", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ein stiller Weiser werden.", "tokens": ["Ein", "stil\u00b7ler", "Wei\u00b7ser", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Sei nur getreu der Sehnsucht,", "tokens": ["Sei", "nur", "ge\u00b7treu", "der", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Die um den Fri