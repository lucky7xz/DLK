an", "ihm", "vor\u00b7bei", ",", "zur", "T\u00fcr", "hin\u00b7aus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "PPER", "PTKVZ", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Greis jedoch sprach dieses: Sagt, o sagt, warum", "tokens": ["Der", "Greis", "je\u00b7doch", "sprach", "die\u00b7ses", ":", "Sagt", ",", "o", "sagt", ",", "wa\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PDAT", "$.", "VVFIN", "$,", "FM", "VVFIN", "$,", "PWAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verlasset ihr mich, Holde, Musen\u00e4hnliche?", "tokens": ["Ver\u00b7las\u00b7set", "ihr", "mich", ",", "Hol\u00b7de", ",", "Mu\u00b7sen\u00b7\u00e4hn\u00b7li\u00b7che", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.13": {"text": "Und jene M\u00e4dchen, scheidend schon, erwiderten:", "tokens": ["Und", "je\u00b7ne", "M\u00e4d\u00b7chen", ",", "schei\u00b7dend", "schon", ",", "er\u00b7wi\u00b7der\u00b7ten", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "ADJD", "ADV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.14": {"text": "Wir wollen nicht den Untergang Athens beschaun!", "tokens": ["Wir", "wol\u00b7len", "nicht", "den", "Un\u00b7ter\u00b7gang", "A\u00b7thens", "be\u00b7schaun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "NE", "VVINF", "$."], "meter": "-+-+-+-+-