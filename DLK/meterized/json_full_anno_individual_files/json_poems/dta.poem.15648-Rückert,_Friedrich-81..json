{"dta.poem.15648": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "81.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mond kehrt unverwandt ein gleiches Angesicht", "tokens": ["Der", "Mond", "kehrt", "un\u00b7ver\u00b7wandt", "ein", "glei\u00b7ches", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Erde zu, doch sie siehts in verschiednem Licht.", "tokens": ["Der", "Er\u00b7de", "zu", ",", "doch", "sie", "siehts", "in", "ver\u00b7schied\u00b7nem", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Da\u00df wechselnd ab und zu du nehmen siehst die Hellung,", "tokens": ["Da\u00df", "wech\u00b7selnd", "ab", "und", "zu", "du", "neh\u00b7men", "siehst", "die", "Hel\u00b7lung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PTKVZ", "KON", "APPR", "PPER", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Liegt nicht am Gegenstand, nur an der Gegenstellung.", "tokens": ["Liegt", "nicht", "am", "Ge\u00b7gen\u00b7stand", ",", "nur", "an", "der", "Ge\u00b7gen\u00b7stel\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+