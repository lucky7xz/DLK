{"dta.poem.10326": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kam von meiner Herrin Haus,", "tokens": ["Ich", "kam", "von", "mei\u00b7ner", "Her\u00b7rin", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wandelt' in Wahnsinn und Mitternachtgraus.", "tokens": ["Und", "wan\u00b7delt'", "in", "Wahn\u00b7sinn", "und", "Mit\u00b7ter\u00b7nacht\u00b7graus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und wie ich am Kirchhof vor\u00fcber gehn will,", "tokens": ["Und", "wie", "ich", "am", "Kirch\u00b7hof", "vor\u00b7\u00fc\u00b7ber", "gehn", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPRART", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Da winken die Gr\u00e4ber ernst und still.", "tokens": ["Da", "win\u00b7ken", "die", "Gr\u00e4\u00b7ber", "ernst", "und", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Da winkt's von des Spielmanns Leichenstein;", "tokens": ["Da", "winkt's", "von", "des", "Spiel\u00b7manns", "Lei\u00b7chen\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das war der flimmernde Mondesschein.", "tokens": ["Das", "war", "der", "flim\u00b7mern\u00b7de", "Mon\u00b7des\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da lispelt's: Lieb Bruder, ich komme gleich!", "tokens": ["Da", "lis\u00b7pelt's", ":", "Lieb", "Bru\u00b7der", ",", "ich", "kom\u00b7me", "gleich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "NN", "NN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da steigt's aus dem Grabe nebelbleich.", "tokens": ["Da", "steigt's", "aus", "dem", "Gra\u00b7be", "ne\u00b7bel\u00b7bleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Spielmann war's, der entstiegen jetzt,", "tokens": ["Der", "Spiel\u00b7mann", "wa\u00b7r's", ",", "der", "ent\u00b7stie\u00b7gen", "jetzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "VVFIN", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und hoch auf den Leichenstein sich setzt.", "tokens": ["Und", "hoch", "auf", "den", "Lei\u00b7chen\u00b7stein", "sich", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In die Saiten der Zither greift er schnell,", "tokens": ["In", "die", "Sai\u00b7ten", "der", "Zi\u00b7ther", "greift", "er", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und singt dabei recht hohl und grell:", "tokens": ["Und", "singt", "da\u00b7bei", "recht", "hohl", "und", "grell", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ei! kennt Ihr noch das alte Lied,", "tokens": ["Ei", "!", "kennt", "Ihr", "noch", "das", "al\u00b7te", "Lied", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das einst so wild die Brust durchgl\u00fcht,", "tokens": ["Das", "einst", "so", "wild", "die", "Brust", "durch\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Saiten dumpf und tr\u00fcbe?", "tokens": ["Ihr", "Sai\u00b7ten", "dumpf", "und", "tr\u00fc\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Engel, die nennen es Himmelsfreud,", "tokens": ["Die", "En\u00b7gel", ",", "die", "nen\u00b7nen", "es", "Him\u00b7mels\u00b7freud", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Teufel, die nennen es H\u00f6llenleid,", "tokens": ["Die", "Teu\u00b7fel", ",", "die", "nen\u00b7nen", "es", "H\u00f6l\u00b7len\u00b7leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Menschen, die nennen es: Liebe!", "tokens": ["Die", "Men\u00b7schen", ",", "die", "nen\u00b7nen", "es", ":", "Lie\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Kaum t\u00f6nte des letzten Wortes Schall,", "tokens": ["Kaum", "t\u00f6n\u00b7te", "des", "letz\u00b7ten", "Wor\u00b7tes", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da thaten sich auf die Gr\u00e4ber all';", "tokens": ["Da", "tha\u00b7ten", "sich", "auf", "die", "Gr\u00e4\u00b7ber", "all'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "PIS", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Viel Luftgestalten dringen hervor,", "tokens": ["Viel", "Luft\u00b7ge\u00b7stal\u00b7ten", "drin\u00b7gen", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und umschweben den Spielmann und schrillen im Chor:", "tokens": ["Und", "um\u00b7schwe\u00b7ben", "den", "Spiel\u00b7mann", "und", "schril\u00b7len", "im", "Chor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.7": {"line.1": {"text": "Liebe! Liebe! deine Macht", "tokens": ["Lie\u00b7be", "!", "Lie\u00b7be", "!", "dei\u00b7ne", "Macht"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat uns hier zu Bett gebracht,", "tokens": ["Hat", "uns", "hier", "zu", "Bett", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Augen zugemacht, \u2014", "tokens": ["Und", "die", "Au\u00b7gen", "zu\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, was rufst du in der Nacht?", "tokens": ["Ei", ",", "was", "rufst", "du", "in", "der", "Nacht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "So heult es verworren, und \u00e4chzet und girrt,", "tokens": ["So", "heult", "es", "ver\u00b7wor\u00b7ren", ",", "und", "\u00e4ch\u00b7zet", "und", "girrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und brauset und sauset, und kr\u00e4chzet und klirrt;", "tokens": ["Und", "brau\u00b7set", "und", "sau\u00b7set", ",", "und", "kr\u00e4ch\u00b7zet", "und", "klirrt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und der tolle Schwarm den Spielmann umschweift,", "tokens": ["Und", "der", "tol\u00b7le", "Schwarm", "den", "Spiel\u00b7mann", "um\u00b7schweift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und der Spielmann wild in die Saiten greift:", "tokens": ["Und", "der", "Spiel\u00b7mann", "wild", "in", "die", "Sai\u00b7ten", "greift", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Bravo! bravo! immer toll!", "tokens": ["Bra\u00b7vo", "!", "bra\u00b7vo", "!", "im\u00b7mer", "toll", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ITJ", "$.", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seyd willkommen!", "tokens": ["Seyd", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Habt vernommen", "tokens": ["Habt", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word"], "pos": ["VAFIN", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Da\u00df mein Zauberwort erscholl,", "tokens": ["Da\u00df", "mein", "Zau\u00b7ber\u00b7wort", "er\u00b7scholl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Liegt man doch jahraus, jahrein,", "tokens": ["Liegt", "man", "doch", "ja\u00b7hraus", ",", "ja\u00b7hrein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "$,", "PTKANT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4uschenstill im K\u00e4mmerlein;", "tokens": ["M\u00e4u\u00b7schen\u00b7still", "im", "K\u00e4m\u00b7mer\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft uns heute lustig seyn!", "tokens": ["La\u00dft", "uns", "heu\u00b7te", "lus\u00b7tig", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Vergunst, \u2014", "tokens": ["Mit", "Ver\u00b7gunst", ","], "token_info": ["word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Seht erst zu, sind wir allein? \u2014", "tokens": ["Seht", "erst", "zu", ",", "sind", "wir", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Narren waren wir im Leben,", "tokens": ["Nar\u00b7ren", "wa\u00b7ren", "wir", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit toller Wuth ergeben", "tokens": ["Und", "mit", "tol\u00b7ler", "Wuth", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Einer tollen Liebesbrunst.", "tokens": ["Ei\u00b7ner", "tol\u00b7len", "Lie\u00b7bes\u00b7brunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Kurzweil soll uns heut nicht fehlen,", "tokens": ["Kurz\u00b7weil", "soll", "uns", "heut", "nicht", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Jeder soll hier treu erz\u00e4hlen,", "tokens": ["Je\u00b7der", "soll", "hier", "treu", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Was ihn weiland hergebracht,", "tokens": ["Was", "ihn", "wei\u00b7land", "her\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie gehetzt,", "tokens": ["Wie", "ge\u00b7hetzt", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Wie zerfetzt", "tokens": ["Wie", "zer\u00b7fetzt"], "token_info": ["word", "word"], "pos": ["PWAV", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Ihn die tolle Liebesjagd.", "tokens": ["Ihn", "die", "tol\u00b7le", "Lie\u00b7bes\u00b7jagd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Da h\u00fcpft aus dem Kreise, so leicht, wie der Wind,", "tokens": ["Da", "h\u00fcpft", "aus", "dem", "Krei\u00b7se", ",", "so", "leicht", ",", "wie", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein mageres Wesen, das summend beginnt:", "tokens": ["Ein", "ma\u00b7ge\u00b7res", "We\u00b7sen", ",", "das", "sum\u00b7mend", "be\u00b7ginnt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.12": {"line.1": {"text": "Ich war ein Schneidergeselle,", "tokens": ["Ich", "war", "ein", "Schnei\u00b7der\u00b7ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit Nadel und mit Scheer';", "tokens": ["Mit", "Na\u00b7del", "und", "mit", "Scheer'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich war so flink und schnelle", "tokens": ["Ich", "war", "so", "flink", "und", "schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Nadel und mit Scheer'.", "tokens": ["Mit", "Na\u00b7del", "und", "mit", "Scheer'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da kam die Meisterstochter", "tokens": ["Da", "kam", "die", "Meis\u00b7ter\u00b7stoch\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit Nadel und mit Scheer';", "tokens": ["Mit", "Na\u00b7del", "und", "mit", "Scheer'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und hat mir in's Herz gestochen", "tokens": ["Und", "hat", "mir", "in's", "Herz", "ge\u00b7sto\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Mit Nadel und mit Scheer'.", "tokens": ["Mit", "Na\u00b7del", "und", "mit", "Scheer'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Da lachten die Geister im lustigen Chor;", "tokens": ["Da", "lach\u00b7ten", "die", "Geis\u00b7ter", "im", "lus\u00b7ti\u00b7gen", "Chor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein Zweiter trat still und ernst hervor:", "tokens": ["Ein", "Zwei\u00b7ter", "trat", "still", "und", "ernst", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Den Rinaldo Rinaldini,", "tokens": ["Den", "Ri\u00b7nal\u00b7do", "Ri\u00b7nal\u