{"dta.poem.13134": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewer wu\u00dft etwas von euch, nachdem ihr in dem Grab,", "tokens": ["\u201e", "wer", "wu\u00dft", "et\u00b7was", "von", "euch", ",", "nach\u00b7dem", "ihr", "in", "dem", "Grab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PIS", "APPR", "PPER", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201eund etwan ich davon nichts aufgezeichnet hab?", "tokens": ["\u201e", "und", "et\u00b7wan", "ich", "da\u00b7von", "nichts", "auf\u00b7ge\u00b7zeich\u00b7net", "hab", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "PPER", "PAV", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eh\u00e4tt meine Zunge nichts gelehret, stets geschwiegen,", "tokens": ["\u201e", "h\u00e4tt", "mei\u00b7ne", "Zun\u00b7ge", "nichts", "ge\u00b7leh\u00b7ret", ",", "stets", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "PIS", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "40\u201dWie w\u00e4r das Alterthum bi\u00df zu dem Tag gestiegen?", "tokens": ["\"", "Wie", "w\u00e4r", "das", "Al\u00b7ter\u00b7thum", "bi\u00df", "zu", "dem", "Tag", "ge\u00b7stie\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "ART", "NN", "APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edie Thaten, denen es an meinem Kiel gefehlt,", "tokens": ["\u201e", "die", "Tha\u00b7ten", ",", "de\u00b7nen", "es", "an", "mei\u00b7nem", "Kiel", "ge\u00b7fehlt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eh\u00e4lt die Vergessenheit in Finsternis verh\u00f6hlt.", "tokens": ["\u201e", "h\u00e4lt", "die", "Ver\u00b7ges\u00b7sen\u00b7heit", "in", "Fins\u00b7ter\u00b7nis", "ver\u00b7h\u00f6hlt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "\u201eso gro\u00df ein Feld-Herr war, prangt er nicht in den Schriften,", "tokens": ["\u201e", "so", "gro\u00df", "ein", "Feld\u00b7Herr", "war", ",", "prangt", "er", "nicht", "in", "den", "Schrif\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "ART", "NN", "VAFIN", "$,", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "\u201eso deckt ihn Rost und Staub in unbekannten Gr\u00fcften.", "tokens": ["\u201e", "so", "deckt", "ihn", "Rost", "und", "Staub", "in", "un\u00b7be\u00b7kann\u00b7ten", "Gr\u00fcf\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}