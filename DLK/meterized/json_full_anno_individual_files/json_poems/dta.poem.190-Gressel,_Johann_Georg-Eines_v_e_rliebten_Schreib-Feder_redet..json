{"dta.poem.190": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Eines v e rliebten Schreib-Feder redet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was vielen heimlich ist/ das kriege ich zu wissen/", "tokens": ["Was", "vie\u00b7len", "heim\u00b7lich", "ist", "/", "das", "krie\u00b7ge", "ich", "zu", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VAFIN", "$(", "PDS", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch mich wird manche Noht und grosser Schmertz", "tokens": ["Durch", "mich", "wird", "man\u00b7che", "Noht", "und", "gros\u00b7ser", "Schmertz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "NN", "KON", "ADJA", "NN"], "meter": "+----+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "beklagt;", "tokens": ["be\u00b7klagt", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Ich sage was man offt sich nicht zu sagen wagt/", "tokens": ["Ich", "sa\u00b7ge", "was", "man", "offt", "sich", "nicht", "zu", "sa\u00b7gen", "wagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "PIS", "ADV", "PRF", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und jenes macht entz\u00fcckt/ was ich gesetzt vom K\u00fcssen.", "tokens": ["Und", "je\u00b7nes", "macht", "ent\u00b7z\u00fcckt", "/", "was", "ich", "ge\u00b7setzt", "vom", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "VVPP", "$(", "PWS", "PPER", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Aus meiner R\u00f6hre pflegt der Wollust-Quell zu fliessen/", "tokens": ["Aus", "mei\u00b7ner", "R\u00f6h\u00b7re", "pflegt", "der", "Wol\u00b7lust\u00b7Quell", "zu", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So da\u00df die todte Schrifft so Aug\u2019 als Hertz behagt.", "tokens": ["So", "da\u00df", "die", "tod\u00b7te", "Schrifft", "so", "Aug'", "als", "Hertz", "be\u00b7hagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "ADV", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein stummer Mund viel mehr als ein Beredter sagt/", "tokens": ["Mein", "stum\u00b7mer", "Mund", "viel", "mehr", "als", "ein", "Be\u00b7red\u00b7ter", "sagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PIS", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Durch mich kan sich ein Paar an fernen Orten gr\u00fcssen;", "tokens": ["Durch", "mich", "kan", "sich", "ein", "Paar", "an", "fer\u00b7nen", "Or\u00b7ten", "gr\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Durch mich entz\u00fcnden sich gar offt die kalten Hertzen/", "tokens": ["Durch", "mich", "ent\u00b7z\u00fcn\u00b7den", "sich", "gar", "offt", "die", "kal\u00b7ten", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PRF", "ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Mein Wesen zeuget Lust mit Gluth beseelten Schertzen/", "tokens": ["Mein", "We\u00b7sen", "zeu\u00b7get", "Lust", "mit", "Gluth", "be\u00b7seel\u00b7ten", "Schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mein unberedter Mund auch spr\u00f6de Hertzen z\u00e4hmt/", "tokens": ["Mein", "un\u00b7be\u00b7red\u00b7ter", "Mund", "auch", "spr\u00f6\u00b7de", "Hert\u00b7zen", "z\u00e4hmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich mu\u00df den Liebenden die erste Labsahl zeigen/", "tokens": ["Ich", "mu\u00df", "den", "Lie\u00b7ben\u00b7den", "die", "ers\u00b7te", "Lab\u00b7sahl", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich s\u00e4tze was der Mund sich vorzubringen sch\u00e4mt/", "tokens": ["Ich", "s\u00e4t\u00b7ze", "was", "der", "Mund", "sich", "vor\u00b7zu\u00b7brin\u00b7gen", "sch\u00e4mt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWS", "ART", "NN", "PRF", "VVIZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und rede vom Papier wenn Zung und Redner schweigen.", "tokens": ["Und", "re\u00b7de", "vom", "Pa\u00b7pier", "wenn", "Zung", "und", "Red\u00b7ner", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}