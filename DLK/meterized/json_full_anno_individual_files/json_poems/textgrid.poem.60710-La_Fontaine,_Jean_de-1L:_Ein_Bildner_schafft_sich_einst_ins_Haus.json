ART", "NN", "$.", "$(", "VVFIN", "PPER", "$.", "$(", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der in der Hand den Donner h\u00e4lt?", "tokens": ["Der", "in", "der", "Hand", "den", "Don\u00b7ner", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erzittert, Menschen! Schweige, Spott!", "tokens": ["Er\u00b7zit\u00b7tert", ",", "Men\u00b7schen", "!", "Schwei\u00b7ge", ",", "Spott", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "NN", "$.", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn vor euch tritt der Herr der Welt!\u00ab", "tokens": ["Denn", "vor", "euch", "tritt", "der", "Herr", "der", "Welt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der K\u00fcnstler mei\u00dfelt emsig fort,", "tokens": ["Der", "K\u00fcnst\u00b7ler", "mei\u00b7\u00dfelt", "em\u00b7sig", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Jupiter steht bald beseelt,", "tokens": ["Und", "Ju\u00b7pi\u00b7ter", "steht", "bald", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ob dem Gotte nur das Wort", "tokens": ["Als", "ob", "dem", "Got\u00b7te", "nur", "das", "Wort"], "token_info": ["word", "word", "word", "word"