 "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDAT", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stand einst der Dichter wenig nach,", "tokens": ["Stand", "einst", "der", "Dich\u00b7ter", "we\u00b7nig", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Der vor dem Gott in Angst geriet,", "tokens": ["Der", "vor", "dem", "Gott", "in", "Angst", "ge\u00b7riet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den erst sein Griffel heilig sprach.", "tokens": ["Den", "erst", "sein", "Grif\u00b7fel", "hei\u00b7lig", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er war darin ganz wie ein Kind,", "tokens": ["Er", "war", "da\u00b7rin", "ganz", "wie", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das nur die eine Sorge hegt,", "tokens": ["Das", "nur", "die", "ei\u00b7ne", "Sor\u00b7ge", "hegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df stets die Puppe wohlgesinnt,", "tokens": ["Da\u00df", "stets", "die", "Pup\u00b7pe", "wohl\u00b7ge\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und niemand ihren Zorn erregt.", "tokens": ["Und", "nie\u00b7mand", "ih\u00b7ren", "Zorn", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Herz folgt dem Gedanken nach.", "tokens": ["Das", "Herz", "folgt", "dem", "Ge\u00b7dan\u00b7ken", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und solchem Quell entspringt wohl auch", "tokens": ["Und", "sol\u00b7chem", "Quell", "ent\u00b7springt", "wohl", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Furcht, die G\u00f6tzen heilig sprach,", "tokens": ["Die", "Furcht", ",", "die", "G\u00f6t\u00b7zen", "hei\u00b7lig", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die bei Heiden ernster Brauch.", "tokens": ["Und", "die", "bei", "Hei\u00b7den", "erns\u00b7ter", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dem, was dem eignen Hirn entstammt.", "tokens": ["Dem", ",", "was", "dem", "eig\u00b7nen", "Hirn", "ent\u00b7stammt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+