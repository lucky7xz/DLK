{"dta.poem.8111": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach zu dem Triumph schweben wir empor,", "tokens": ["Ach", "zu", "dem", "Tri\u00b7umph", "schwe\u00b7ben", "wir", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Engel, und ihr, Erben des Lichts, kommen zu des Sohns", "tokens": ["En\u00b7gel", ",", "und", "ihr", ",", "Er\u00b7ben", "des", "Lichts", ",", "kom\u00b7men", "zu", "des", "Sohns"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "PPER", "$,", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Himmelsgang! Du o Tod, du Flug zu dem Genu\u00df!", "tokens": ["Him\u00b7mels\u00b7gang", "!", "Du", "o", "Tod", ",", "du", "Flug", "zu", "dem", "Ge\u00b7nu\u00df", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "FM", "NN", "$,", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Gr\u00e4ber, und ihr Graun, Wonne seyd ihr, Himmel und", "tokens": ["Gr\u00e4\u00b7ber", ",", "und", "ihr", "Graun", ",", "Won\u00b7ne", "seyd", "ihr", ",", "Him\u00b7mel", "und"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "KON", "PPOSAT", "NN", "$,", "NN", "VAFIN", "PPER", "$,", "NN", "KON"], "meter": "+--+++-+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "sein Heil!", "tokens": ["sein", "Heil", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}