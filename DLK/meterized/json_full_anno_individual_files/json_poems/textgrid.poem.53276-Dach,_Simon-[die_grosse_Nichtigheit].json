{"textgrid.poem.53276": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[die grosse Nichtigheit]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die grosse Nichtigheit", "tokens": ["Die", "gros\u00b7se", "Nich\u00b7tig\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der kurtzen Lebens-zeit", "tokens": ["Der", "kurt\u00b7zen", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bek\u00fcmmert meinen Sinn,", "tokens": ["Be\u00b7k\u00fcm\u00b7mert", "mei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Frewden g\u00fcldner Schein", "tokens": ["Der", "Frew\u00b7den", "g\u00fcld\u00b7ner", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00df gantz verloschen seyn,", "tokens": ["Mu\u00df", "gantz", "ver\u00b7lo\u00b7schen", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich falle gar dahin:", "tokens": ["Ich", "fal\u00b7le", "gar", "da\u00b7hin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schaw, wie ein Rauch entsteht,", "tokens": ["Schaw", ",", "wie", "ein", "Rauch", "ent\u00b7steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Dahin f\u00e4hrt, vnd vergeht,", "tokens": ["Da\u00b7hin", "f\u00e4hrt", ",", "vnd", "ver\u00b7geht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wie Sch\u00e4m vnd Schatten weichen,", "tokens": ["Wie", "Sch\u00e4m", "vnd", "Schat\u00b7ten", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wie man die Handt vmbwendt,", "tokens": ["Wie", "man", "die", "Handt", "vm\u00b7bwendt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Ein Pfeil die L\u00fcffte trennt,", "tokens": ["Ein", "Pfeil", "die", "L\u00fcff\u00b7te", "trennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "So mu\u00df ein Mensch verbleichen.", "tokens": ["So", "mu\u00df", "ein", "Mensch", "ver\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vnd dennoch, dennoch sind", "tokens": ["Vnd", "den\u00b7noch", ",", "den\u00b7noch", "sind"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir tolles Volck so blind", "tokens": ["Wir", "tol\u00b7les", "Volck", "so", "blind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd geben nichts darauff:", "tokens": ["Vnd", "ge\u00b7ben", "nichts", "dar\u00b7auff", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wir bawen in die Welt,", "tokens": ["Wir", "ba\u00b7wen", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Al\u00df wer vns hie bestellt", "tokens": ["Al\u00df", "wer", "vns", "hie", "be\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PWS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Ewigheiten Lauff;", "tokens": ["Der", "E\u00b7wig\u00b7hei\u00b7ten", "Lauff", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der strebt durch Schwerd vnd Glut", "tokens": ["Der", "strebt", "durch", "Schwerd", "vnd", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Nach Ehre, der nach Gut,", "tokens": ["Nach", "Eh\u00b7re", ",", "der", "nach", "Gut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Kan nicht ers\u00e4ttigt werden,", "tokens": ["Kan", "nicht", "er\u00b7s\u00e4t\u00b7tigt", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Indessen k\u00f6mpt der Todt,", "tokens": ["In\u00b7des\u00b7sen", "k\u00f6mpt", "der", "Todt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Vnd f\u00fcllt vns mit dem Koht", "tokens": ["Vnd", "f\u00fcllt", "vns", "mit", "dem", "Koht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Der lang-erkratzten Erden.", "tokens": ["Der", "lang\u00b7er\u00b7kratz\u00b7ten", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Thorheit, welche man", "tokens": ["O", "Thor\u00b7heit", ",", "wel\u00b7che", "man"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PIS"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Fast nicht erm\u00e4ssen kan!", "tokens": ["Fast", "nicht", "er\u00b7m\u00e4s\u00b7sen", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel ist vns schlecht;", "tokens": ["Der", "Him\u00b7mel", "ist", "vns", "schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hie bawt man Hoff vnd Hau\u00df", "tokens": ["Hie", "bawt", "man", "Hoff", "vnd", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "VVFIN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vnd schl\u00e4get dort offt aus", "tokens": ["Vnd", "schl\u00e4\u00b7get", "dort", "offt", "aus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das wahre B\u00fcrger-Recht:", "tokens": ["Das", "wah\u00b7re", "B\u00fcr\u00b7ger\u00b7Recht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach endlich ist es gnug,", "tokens": ["Ach", "end\u00b7lich", "ist", "es", "gnug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Kompt, werdet einmal klug,", "tokens": ["Kompt", ",", "wer\u00b7det", "ein\u00b7mal", "klug", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Seht, wo ihr ewig bleibet,", "tokens": ["Seht", ",", "wo", "ihr", "e\u00b7wig", "blei\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wo weit von Pein vnd List", "tokens": ["Wo", "weit", "von", "Pein", "vnd", "List"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der Seelen Ruhstat ist", "tokens": ["Der", "See\u00b7len", "Ruh\u00b7stat", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Vnd kein Todt vns vertreibet.", "tokens": ["Vnd", "kein", "Todt", "vns", "ver\u00b7trei\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Steht eilends auff, vnd wacht!", "tokens": ["Steht", "ei\u00b7lends", "auff", ",", "vnd", "wacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es ist vmb Mitternacht,", "tokens": ["Es", "ist", "vmb", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man klopfft schon an die Th\u00fcr:", "tokens": ["Man", "klopfft", "schon", "an", "die", "Th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verseht die Lampen wol,", "tokens": ["Ver\u00b7seht", "die", "Lam\u00b7pen", "wol", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcllt sie mit Oele vol,", "tokens": ["F\u00fcllt", "sie", "mit", "O\u00b7e\u00b7le", "vol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Der Breutgam ist schon hier.", "tokens": ["Der", "Breut\u00b7gam", "ist", "schon", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ach komm, Herr Jesu, bald!", "tokens": ["Ach", "komm", ",", "Herr", "Je\u00b7su", ",", "bald", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "NN", "NE", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Komm, vnser Auffenthalt,", "tokens": ["Komm", ",", "vn\u00b7ser", "Auf\u00b7fent\u00b7halt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "La\u00df vns bereittet stehen,", "tokens": ["La\u00df", "vns", "be\u00b7reit\u00b7tet", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "VVFIN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.10": {"text": "Hie wohnt nur M\u00fch vnd Streit,", "tokens": ["Hie", "wohnt", "nur", "M\u00fch", "vnd", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Dort lauter Seeligheit,", "tokens": ["Dort", "lau\u00b7ter", "See\u00b7lig\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Wir wollen mit dir gehen!", "tokens": ["Wir", "wol\u00b7len", "mit", "dir", "ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die grosse Nichtigheit", "tokens": ["Die", "gros\u00b7se", "Nich\u00b7tig\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der kurtzen Lebens-zeit", "tokens": ["Der", "kurt\u00b7zen", "Le\u00b7bens\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bek\u00fcmmert meinen Sinn,", "tokens": ["Be\u00b7k\u00fcm\u00b7mert", "mei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Frewden g\u00fcldner Schein", "tokens": ["Der", "Frew\u00b7den", "g\u00fcld\u00b7ner", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mu\u00df gantz verloschen seyn,", "tokens": ["Mu\u00df", "gantz", "ver\u00b7lo\u00b7schen", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich falle gar dahin:", "tokens": ["Ich", "fal\u00b7le", "gar", "da\u00b7hin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Schaw, wie ein Rauch entsteht,", "tokens": ["Schaw", ",", "wie", "ein", "Rauch", "ent\u00b7steht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Dahin f\u00e4hrt, vnd vergeht,", "tokens": ["Da\u00b7hin", "f\u00e4hrt", ",", "vnd", "ver\u00b7geht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wie Sch\u00e4m vnd Schatten weichen,", "tokens": ["Wie", "Sch\u00e4m", "vnd", "Schat\u00b7ten", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wie man die Handt vmbwendt,", "tokens": ["Wie", "man", "die", "Handt", "vm\u00b7bwendt", ","], "token_info": ["word", "word", "wo