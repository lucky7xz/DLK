{"textgrid.poem.66800": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwirrende Schwalben, kreisende Dohlen ...", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwirrende Schwalben, kreisende Dohlen ...", "tokens": ["Schwir\u00b7ren\u00b7de", "Schwal\u00b7ben", ",", "krei\u00b7sen\u00b7de", "Doh\u00b7len", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$("], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Stehe auf schwindelndem Rande allein.", "tokens": ["Ste\u00b7he", "auf", "schwin\u00b7deln\u00b7dem", "Ran\u00b7de", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Fest den Boden fassen die Sohlen,", "tokens": ["Fest", "den", "Bo\u00b7den", "fas\u00b7sen", "die", "Soh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Blicke in wimmelnde Tiefe hinein.", "tokens": ["Bli\u00b7cke", "in", "wim\u00b7meln\u00b7de", "Tie\u00b7fe", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Aus dem verworrenen steinernen Meere", "tokens": ["Aus", "dem", "ver\u00b7wor\u00b7re\u00b7nen", "stei\u00b7ner\u00b7nen", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Rauscht es wie ferne Brandung empor,", "tokens": ["Rauscht", "es", "wie", "fer\u00b7ne", "Bran\u00b7dung", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und ich lausche dem Murmeln und h\u00f6re", "tokens": ["Und", "ich", "lau\u00b7sche", "dem", "Mur\u00b7meln", "und", "h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Einen geheimnisvoll raunenden Chor:", "tokens": ["Ei\u00b7nen", "ge\u00b7heim\u00b7nis\u00b7voll", "rau\u00b7nen\u00b7den", "Chor", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbmillionen Sterbliche wohnen", "tokens": ["\u00bb", "mil\u00b7li\u00b7o\u00b7nen", "Sterb\u00b7li\u00b7che", "woh\u00b7nen"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "VVINF"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Auf die winzige St\u00e4tte gebannt,", "tokens": ["Auf", "die", "win\u00b7zi\u00b7ge", "St\u00e4t\u00b7te", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "In den Pal\u00e4sten schlemmende Drohnen,", "tokens": ["In", "den", "Pa\u00b7l\u00e4s\u00b7ten", "schlem\u00b7men\u00b7de", "Droh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In den Kasernen die sch