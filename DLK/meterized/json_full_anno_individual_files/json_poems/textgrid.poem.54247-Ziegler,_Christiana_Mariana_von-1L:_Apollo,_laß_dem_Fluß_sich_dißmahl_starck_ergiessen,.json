{"textgrid.poem.54247": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Apollo, la\u00df dem Flu\u00df sich di\u00dfmahl starck ergiessen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Apollo, la\u00df dem Flu\u00df sich di\u00dfmahl starck ergiessen,", "tokens": ["A\u00b7pol\u00b7lo", ",", "la\u00df", "dem", "Flu\u00df", "sich", "di\u00df\u00b7mahl", "starck", "er\u00b7gies\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "NN", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fchr mit eigner Hand mich zum Parna\u00df hinan;", "tokens": ["Und", "f\u00fchr", "mit", "eig\u00b7ner", "Hand", "mich", "zum", "Par\u00b7na\u00df", "hi\u00b7nan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dein Zuspruch wird gewi\u00df mir Furcht und Schmertz vers\u00fcssen,", "tokens": ["Dein", "Zu\u00b7spruch", "wird", "ge\u00b7wi\u00df", "mir", "Furcht", "und", "Schmertz", "ver\u00b7s\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich mit bessern Muth auf Reime dencken kan.", "tokens": ["Da\u00df", "ich", "mit", "bes\u00b7sern", "Muth", "auf", "Rei\u00b7me", "den\u00b7cken", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer dir nicht lange Zeit zu F\u00fcssen hat gesessen,", "tokens": ["Wer", "dir", "nicht", "lan\u00b7ge", "Zeit", "zu", "F\u00fcs\u00b7sen", "hat", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der trifft doch nicht den Zweck, so hefftig er auch rennt,", "tokens": ["Der", "trifft", "doch", "nicht", "den", "Zweck", ",", "so", "heff\u00b7tig", "er", "auch", "rennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "$,", "ADV", "ADJD", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn auch das kleinste Kind kan, d\u00fcnckt mich, leicht ermessen,", "tokens": ["Denn", "auch", "das", "kleins\u00b7te", "Kind", "kan", ",", "d\u00fcnckt", "mich", ",", "leicht", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VMFIN", "$,", "VVFIN", "PPER", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df Ihn Calliope nur einen St\u00fcmper nennt.", "tokens": ["Da\u00df", "Ihn", "Cal\u00b7li\u00b7o\u00b7pe", "nur", "ei\u00b7nen", "St\u00fcm\u00b7per", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Ich pflege, wie du weist, in Tag hinein zu schmieren,", "tokens": ["Ich", "pfle\u00b7ge", ",", "wie", "du", "weist", ",", "in", "Tag", "hin\u00b7ein", "zu", "schmie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "NN", "APZR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie es die Phantasie mir ins Gehirne bringt;", "tokens": ["Wie", "es", "die", "Phan\u00b7ta\u00b7sie", "mir", "ins", "Ge\u00b7hir\u00b7ne", "bringt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "O lerne mir doch recht den Thon der Fl\u00f6the r\u00fchren,", "tokens": ["O", "ler\u00b7ne", "mir", "doch", "recht", "den", "Thon", "der", "Fl\u00f6\u00b7the", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der leider! noch gar lahm in deinen Ohren klingt,", "tokens": ["Der", "lei\u00b7der", "!", "noch", "gar", "lahm", "in", "dei\u00b7nen", "Oh\u00b7ren", "klingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$.", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wiewohl was will ich mich nach Phoebi Beystand reissen,", "tokens": ["Wie\u00b7wohl", "was", "will", "ich", "mich", "nach", "Phoe\u00b7bi", "Beys\u00b7tand", "reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "PPER", "PRF", "APPR", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da du, hochwerther Freund, wofern es mir verg\u00f6nnt,", "tokens": ["Da", "du", ",", "hoch\u00b7wert\u00b7her", "Freund", ",", "wo\u00b7fern", "es", "mir", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJA", "NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Am allerbesten k\u00f6ntst mich nach dem Pindus weisen,", "tokens": ["Am", "al\u00b7ler\u00b7bes\u00b7ten", "k\u00f6ntst", "mich", "nach", "dem", "Pin\u00b7dus", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der dich von Alters her noch seine Zierde nennt.", "tokens": ["Der", "dich", "von", "Al\u00b7ters", "her", "noch", "sei\u00b7ne", "Zier\u00b7de", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich w\u00fcrde, th\u00e4tst du di\u00df, den gr\u00f6sten Vortheil ziehen,", "tokens": ["Ich", "w\u00fcr\u00b7de", ",", "th\u00e4tst", "du", "di\u00df", ",", "den", "gr\u00f6s\u00b7ten", "Vor\u00b7theil", "zie\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "PDS", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Denn dein geschicktes Thun ist aller Welt bekannt,", "tokens": ["Denn", "dein", "ge\u00b7schick\u00b7tes", "Thun", "ist", "al\u00b7ler", "Welt", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Was w\u00fcrde mir vor Gl\u00fcck bey deinen Musen bl\u00fchen,", "tokens": ["Was", "w\u00fcr\u00b7de", "mir", "vor", "Gl\u00fcck", "bey", "dei\u00b7nen", "Mu\u00b7sen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Reichst du, geschickter Freund, mir die gelehrte Hand.", "tokens": ["Reichst", "du", ",", "ge\u00b7schick\u00b7ter", "Freund", ",", "mir", "die", "ge\u00b7lehr\u00b7te", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Allein dis wird wohl nicht nach meinen Wunsch geschehen,", "tokens": ["Al\u00b7lein", "dis", "wird", "wohl", "nicht", "nach", "mei\u00b7nen", "Wunsch", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Dieweil ich leider dich gar sparsam sehen kan.", "tokens": ["Die\u00b7weil", "ich", "lei\u00b7der", "dich", "gar", "spar\u00b7sam", "se\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dein K\u00f6nig, welcher dich zum Beystand ausersehen,", "tokens": ["Dein", "K\u00f6\u00b7nig", ",", "wel\u00b7cher", "dich", "zum", "Beys\u00b7tand", "au\u00b7ser\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wend deine Hand und Kiel zu etwas wichtgern an.", "tokens": ["Wend", "dei\u00b7ne", "Hand", "und", "Kiel", "zu", "et\u00b7was", "wicht\u00b7gern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NE", "APPR", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "O! schade, da\u00df ich nicht, du Ausbund kluger Seelen,", "tokens": ["O", "!", "scha\u00b7de", ",", "da\u00df", "ich", "nicht", ",", "du", "Aus\u00b7bund", "klu\u00b7ger", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "$,", "PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Ich w\u00fcnsch es tausendmahl, bey dir soll nahe seyn.", "tokens": ["Ich", "w\u00fcnsch", "es", "tau\u00b7send\u00b7mahl", ",", "bey", "dir", "soll", "na\u00b7he", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "APPR", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich w\u00fcrde dich gewi\u00df zum Leit-Gestirn erwehlen,", "tokens": ["Ich", "w\u00fcr\u00b7de", "dich", "ge\u00b7wi\u00df", "zum", "Leit\u00b7Ge\u00b7stirn", "er\u00b7weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Dein Umgang pr\u00e4gte mir viel kluge Lehren ein.", "tokens": ["Dein", "Um\u00b7gang", "pr\u00e4g\u00b7te", "mir", "viel", "klu\u00b7ge", "Leh\u00b7ren", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Wie herb und bitter mir dein Abschied eingegangen,", "tokens": ["Wie", "herb", "und", "bit\u00b7ter", "mir", "dein", "Ab\u00b7schied", "ein\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Verschweigt die Feder hier, denn wer dich hier erblickt,", "tokens": ["Ver\u00b7schweigt", "die", "Fe\u00b7der", "hier", ",", "denn", "wer", "dich", "hier", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,", "KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der tr\u00e4gt mit mir nach dir ein sehnliches Verlangen,", "tokens": ["Der", "tr\u00e4gt", "mit", "mir", "nach", "dir", "ein", "sehn\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Weil deine Trefflichkeit uns Seel und Geist bestrickt.", "tokens": ["Weil", "dei\u00b7ne", "Treff\u00b7lich\u00b7keit", "uns", "Seel", "und", "Geist", "be\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie viele w\u00fcnschten dir, und di\u00df bey vielen Klagen,", "tokens": ["Wie", "vie\u00b7le", "w\u00fcnschten", "dir", ",", "und", "di\u00df", "bey", "vie\u00b7len", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "$,", "KON", "PDS", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Worzu dein Scheiden sie mehr als zu billig trieb,", "tokens": ["Wor\u00b7zu", "dein", "Schei\u00b7den", "sie", "mehr", "als", "zu", "bil\u00b7lig", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "PIAT", "KOKOM", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Zephyr m\u00f6chten dich recht sanfft nach N-- tragen,", "tokens": ["Die", "Ze\u00b7phyr", "m\u00f6ch\u00b7ten", "dich", "recht", "sanfft", "nach", "N\u00b7", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADJD", "VVFIN", "APPR", "TRUNC", "VVINF", "$,"], "meter": "meter.parsing.error", "measure": "measure.parsing.error"}, "line.36": {"text": "Damit dein edler Fu\u00df befreyt von Unfall blieb.", "tokens": ["Da\u00b7mit", "dein", "ed\u00b7ler", "Fu\u00df", "be\u00b7freyt", "von", "Un\u00b7fall", "blieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "VVPP", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Jedoch wir m\u00fcssen uns bey solchen Schicksal fassen,", "tokens": ["Je\u00b7doch", "wir", "m\u00fcs\u00b7sen", "uns", "bey", "sol\u00b7chen", "Schick\u00b7sal", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Wer wei\u00df, ob man nicht einst auch Rosen wieder bricht.", "tokens": ["Wer", "wei\u00df", ",", "ob", "man", "nicht", "einst", "auch", "Ro\u00b7sen", "wie\u00b7der", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PIS", "PTKNEG", "ADV", "ADV", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich werde niemahls dich aus dem Ged\u00e4chtni\u00df lassen,", "tokens": ["Ich", "wer\u00b7de", "nie\u00b7mahls", "dich", "aus", "dem", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Denn 