{"textgrid.poem.43756": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einladung zur Kirme\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Nebel liegt auf Berg und Thal,", "tokens": ["Der", "Ne\u00b7bel", "liegt", "auf", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Feld ist leer, der Wald ist kahl,", "tokens": ["Das", "Feld", "ist", "leer", ",", "der", "Wald", "ist", "kahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur frischbes\u00e4tes Land wird gr\u00fcn,", "tokens": ["Nur", "frischbe\u00b7s\u00e4\u00b7tes", "Land", "wird", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur hie und da noch Blumen bl\u00fch'n.", "tokens": ["Nur", "hie", "und", "da", "noch", "Blu\u00b7men", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "ADV", "KON", "ADV", "ADV", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es schwiegen l\u00e4ngst des Waldes S\u00e4nger,", "tokens": ["Es", "schwie\u00b7gen", "l\u00e4ngst", "des", "Wal\u00b7des", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "