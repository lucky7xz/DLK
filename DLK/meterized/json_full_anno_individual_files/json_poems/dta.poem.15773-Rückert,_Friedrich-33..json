{"dta.poem.15773": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was deine Seele denkt, was dein Gem\u00fct empfindet,", "tokens": ["Was", "dei\u00b7ne", "See\u00b7le", "denkt", ",", "was", "dein", "Ge\u00b7m\u00fct", "emp\u00b7fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nun das rechte Wort dazu die Sprache findet;", "tokens": ["Wenn", "nun", "das", "rech\u00b7te", "Wort", "da\u00b7zu", "die", "Spra\u00b7che", "fin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie schwankend ist das Wort, wie schillerig vieldeutig,", "tokens": ["Wie", "schwan\u00b7kend", "ist", "das", "Wort", ",", "wie", "schil\u00b7le\u00b7rig", "viel\u00b7deu\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PWAV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eben dadurch auch wie reich und vielausbeutig!", "tokens": ["Und", "e\u00b7ben", "da\u00b7durch", "auch", "wie", "reich", "und", "viel\u00b7aus\u00b7beu\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "ADV", "KOKOM", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das allereinfachste, in welchem nur Ein Sinn", "tokens": ["Das", "al\u00b7ler\u00b7ein\u00b7fachs\u00b7te", ",", "in", "wel\u00b7chem", "nur", "Ein", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "APPR", "PRELS", "ADV", "ART", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Liegen zu k\u00f6nnen scheint, vielfachster liegt darinn.", "tokens": ["Lie\u00b7gen", "zu", "k\u00f6n\u00b7nen", "scheint", ",", "viel\u00b7fachs\u00b7ter", "liegt", "da\u00b7rinn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VMINF", "VVFIN", "$,", "ADV", "VVFIN", "PAV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Das merkest du zumeist, wenn du dir zum Ergetzen", "tokens": ["Das", "mer\u00b7kest", "du", "zu\u00b7meist", ",", "wenn", "du", "dir", "zum", "Er\u00b7get\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "APPRART", "NN"], "meter": "--+--+--+-+--", "measure": "anapaest.tri.plus"}, "line.2": {"text": "In deine Sprache willst aus fremder \u00fcbersetzen.", "tokens": ["In", "dei\u00b7ne", "Spra\u00b7che", "willst", "aus", "frem\u00b7der", "\u00fc\u00b7bers\u00b7et\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da sp\u00fcrst du erst des Meers Untief' und Klipp' und Riff,", "tokens": ["Da", "sp\u00fcrst", "du", "erst", "des", "Meers", "Un\u00b7tie\u00b7f'", "und", "Klipp'", "und", "Riff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wor\u00fcber leichthin sonst geht dein Gedankenschiff.", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "leich\u00b7thin", "sonst", "geht", "dein", "Ge\u00b7dan\u00b7ken\u00b7schiff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ja alles findest du, die Qual ist h\u00f6chst ergetzlich,", "tokens": ["Ja", "al\u00b7les", "fin\u00b7dest", "du", ",", "die", "Qual", "ist", "h\u00f6chst", "er\u00b7getz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "VVFIN", "PPER", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jemehr du es verstehst, jeminder \u00fcbersetzlich.", "tokens": ["Je\u00b7mehr", "du", "es", "ver\u00b7stehst", ",", "je\u00b7min\u00b7der", "\u00fc\u00b7bers\u00b7etz\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}