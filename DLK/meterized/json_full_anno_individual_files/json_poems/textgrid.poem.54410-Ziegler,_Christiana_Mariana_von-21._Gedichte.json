, "dies", "nicht", "durch", "den", "Ruf", "be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VAFIN", "ADV", "PIS", "PDS", "PTKNEG", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So lehrt uns solches doch ein Wink von hoher Hand.", "tokens": ["So", "lehrt", "uns", "sol\u00b7ches", "doch", "ein", "Wink", "von", "ho\u00b7her", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn wen Augustus pflegt aus vielen zu erwehlen,", "tokens": ["Denn", "wen", "Au\u00b7gus\u00b7tus", "pflegt", "aus", "vie\u00b7len", "zu", "er\u00b7weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NE", "VVFIN", "APPR", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der ist den w\u00fcrdigen mit Rechte beyzuzehlen.", "tokens": ["Der", "ist", "den", "w\u00fcr\u00b7di\u00b7gen", "mit", "Rech\u00b7te", "bey\u00b7zu\u00b7zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Musen nennen dich, gelehrter Freund, begl\u00fcckt,", "tokens": ["Die", "Mu\u00b7sen", "nen\u00b7nen", "dich", ",", "ge\u00b7lehr\u00b7ter", "Freund", ",", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,