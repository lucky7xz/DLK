{"textgrid.poem.54410": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "21. Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Musen nennen dich, gelehrter Freund, begl\u00fcckt,", "tokens": ["Die", "Mu\u00b7sen", "nen\u00b7nen", "dich", ",", "ge\u00b7lehr\u00b7ter", "Freund", ",", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dieweil dein Schicksal dich der Niedrigkeit entr\u00fcckt.", "tokens": ["Die\u00b7weil", "dein", "Schick\u00b7sal", "dich", "der", "Nied\u00b7rig\u00b7keit", "ent\u00b7r\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein sie irren sehr; denn alle Welt wird sagen:", "tokens": ["Al\u00b7lein", "sie", "ir\u00b7ren", "sehr", ";", "denn", "al\u00b7le", "Welt", "wird", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "$.", "KON", "PIAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Gl\u00fccke h\u00e4tte hier gewi\u00df nichts beygetragen.", "tokens": ["Das", "Gl\u00fc\u00b7cke", "h\u00e4t\u00b7te", "hier", "ge\u00b7wi\u00df", "nichts", "bey\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PIS", "VVINF", "$."], "