{"dta.poem.20480": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Lauretten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Laurette bleibstu ewig stein?", "tokens": ["Lau\u00b7ret\u00b7te", "bleibs\u00b7tu", "e\u00b7wig", "stein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Soll forthin unverkn\u00fcpffet seyn", "tokens": ["Soll", "for\u00b7thin", "un\u00b7ver\u00b7kn\u00fcpf\u00b7fet", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein englisch-seyn und dein erbarmen?", "tokens": ["Dein", "eng\u00b7lischseyn", "und", "dein", "er\u00b7bar\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KON", "PPOSAT", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm/ komm/ und \u00f6ffne deinen schoo\u00df", "tokens": ["Komm", "/", "komm", "/", "und", "\u00f6ff\u00b7ne", "dei\u00b7nen", "schoo\u00df"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "VVFIN", "$(", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und la\u00df uns beyde nackt und blo\u00df", "tokens": ["Und", "la\u00df", "uns", "bey\u00b7de", "nackt", "und", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PIS", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umgeben seyn mit geist und armen.", "tokens": ["Um\u00b7ge\u00b7ben", "seyn", "mit", "geist", "und", "ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "APPR", "NN", "KON", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df mich auff deiner schwanen-brust", "tokens": ["La\u00df", "mich", "auff", "dei\u00b7ner", "schwa\u00b7nen\u00b7brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die offt-versagte liebes-lust", "tokens": ["Die", "offt\u00b7ver\u00b7sag\u00b7te", "lie\u00b7bes\u00b7lust"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier zwischen furcht und scham geniessen.", "tokens": ["Hier", "zwi\u00b7schen", "furcht", "und", "scham", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und la\u00df mich tausend tausendmahl/", "tokens": ["Und", "la\u00df", "mich", "tau\u00b7send", "tau\u00b7send\u00b7mahl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "CARD", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach deiner g\u00fcldnen haare zahl/", "tokens": ["Nach", "dei\u00b7ner", "g\u00fcld\u00b7nen", "haa\u00b7re", "zahl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die geister-reichen lippen k\u00fcssen.", "tokens": ["Die", "geis\u00b7ter\u00b7rei\u00b7chen", "lip\u00b7pen", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df mich den ausbund deiner pracht/", "tokens": ["La\u00df", "mich", "den", "aus\u00b7bund", "dei\u00b7ner", "pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sammt und rosen nichtig macht/", "tokens": ["Der", "sammt", "und", "ro\u00b7sen", "nich\u00b7tig", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit meiner schlechten haut bedecken;", "tokens": ["Mit", "mei\u00b7ner", "schlech\u00b7ten", "haut", "be\u00b7de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn du deine lenden r\u00fchrst/", "tokens": ["Und", "wenn", "du", "dei\u00b7ne", "len\u00b7den", "r\u00fchrst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und deinen schoo\u00df gen himmel f\u00fchrst/", "tokens": ["Und", "dei\u00b7nen", "schoo\u00df", "gen", "him\u00b7mel", "f\u00fchrst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich zucker-s\u00fcsse lust erwecken.", "tokens": ["Sich", "zu\u00b7cke\u00b7rs\u00fcs\u00b7se", "lust", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und solte durch die heisse brunst/", "tokens": ["Und", "sol\u00b7te", "durch", "die", "heis\u00b7se", "brunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und deine hohe gegen-gunst", "tokens": ["Und", "dei\u00b7ne", "ho\u00b7he", "ge\u00b7gen\u00b7gunst"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir auch die seele gleich entfliessen.", "tokens": ["Mir", "auch", "die", "see\u00b7le", "gleich", "ent\u00b7flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ist dein zarter leib die bahr/", "tokens": ["So", "ist", "dein", "zar\u00b7ter", "leib", "die", "bahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die seele wird drey viertel jahr", "tokens": ["Die", "see\u00b7le", "wird", "drey", "vier\u00b7tel", "jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein himmels-rundter bauch umschliessen.", "tokens": ["Dein", "him\u00b7mels\u00b7rund\u00b7ter", "bauch", "um\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wer alsdann nach meiner zeit", "tokens": ["Und", "wer", "als\u00b7dann", "nach", "mei\u00b7ner", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu lieben dich wird seyn bereit/", "tokens": ["Zu", "lie\u00b7ben", "dich", "wird", "seyn", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und h\u00f6ren wird/ wie ich gestorben/", "tokens": ["Und", "h\u00f6\u00b7ren", "wird", "/", "wie", "ich", "ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VAFIN", "$(", "PWAV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird sagen: Wer also verdirbt/", "tokens": ["Wird", "sa\u00b7gen", ":", "Wer", "al\u00b7so", "ver\u00b7dirbt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$.", "PWS", "ADV", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und in dem zarten schoo\u00dfe stirbt/", "tokens": ["Und", "in", "dem", "zar\u00b7ten", "schoo\u00b7\u00dfe", "stirbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat einen fanfften tod erworben.", "tokens": ["Hat", "ei\u00b7nen", "fanff\u00b7ten", "tod", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}