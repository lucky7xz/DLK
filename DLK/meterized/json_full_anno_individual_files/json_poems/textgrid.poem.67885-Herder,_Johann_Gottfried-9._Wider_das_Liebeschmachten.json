{"textgrid.poem.67885": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "9. Wider das Liebeschmachten", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie gl\u00fccklich, wie selig, wer selbst sich besitzt,", "tokens": ["Wie", "gl\u00fcck\u00b7lich", ",", "wie", "se\u00b7lig", ",", "wer", "selbst", "sich", "be\u00b7sitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "$,", "PWS", "ADV", "PRF", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und borgt nicht von andern, was liebt ihm und n\u00fctzt,", "tokens": ["Und", "borgt", "nicht", "von", "an\u00b7dern", ",", "was", "liebt", "ihm", "und", "n\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "ADJA", "$,", "PWS", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und leiht nicht dem Zauber der Liebe sein Ohr", "tokens": ["Und", "leiht", "nicht", "dem", "Zau\u00b7ber", "der", "Lie\u00b7be", "sein", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und wird nicht durch Aechzen und Lechzen ein Thor.", "tokens": ["Und", "wird", "nicht", "durch", "A\u00b7ech\u00b7zen", "und", "Lech\u00b7zen", "ein", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Er hangt nicht an jedem verlangenden Blick,", "tokens": ["Er", "hangt", "nicht", "an", "je\u00b7dem", "ver\u00b7lan\u00b7gen\u00b7den", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und zieht sich dem Hangen und Bangen zur\u00fcck;", "tokens": ["Und", "zieht", "sich", "dem", "Han\u00b7gen", "und", "Ban\u00b7gen", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein Herzchen das immer nur wandert umher,", "tokens": ["Ein", "Herz\u00b7chen", "das", "im\u00b7mer", "nur", "wan\u00b7dert", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wird endlich gefangen, dann fliegt es nicht mehr.", "tokens": ["Wird", "end\u00b7lich", "ge\u00b7fan\u00b7gen", ",", "dann", "fliegt", "es", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Wer mit den Gefahren nur scherzet und spielt,", "tokens": ["Wer", "mit", "den", "Ge\u00b7fah\u00b7ren", "nur", "scher\u00b7zet", "und", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+