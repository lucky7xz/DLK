{"textgrid.poem.39649": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie r\u00fchmlich ist der Kampf der krieg'rischen Croaten", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie r\u00fchmlich ist der Kampf der krieg'rischen Croaten", "tokens": ["Wie", "r\u00fchm\u00b7lich", "ist", "der", "Kampf", "der", "krieg'\u00b7ri\u00b7schen", "Croa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Ungarns s\u00e4belklirrenden Magnaten!", "tokens": ["Mit", "Un\u00b7garns", "s\u00e4\u00b7bel\u00b7klir\u00b7ren\u00b7den", "Mag\u00b7na\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir sind, behaupten sie, Illyrier von Geschlecht;", "tokens": ["Wir", "sind", ",", "be\u00b7haup\u00b7ten", "sie", ",", "Il\u00b7ly\u00b7rier", "von", "Ge\u00b7schlecht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "VVFIN", "PPER", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihr aber, Hunnen und Magyaren,", "tokens": ["Ihr", "a\u00b7ber", ",", "Hun\u00b7nen", "und", "Ma\u00b7gy\u00b7a\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Seid asiatische Barbaren,", "tokens": ["Seid", "a\u00b7sia\u00b7ti\u00b7sche", "Bar\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wollt uns zwingen nun zu sprechen, wie ihr sprecht.", "tokens": ["Und", "wollt", "uns", "zwin\u00b7gen", "nun", "zu", "spre\u00b7chen", ",", "wie", "ihr", "sprecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir sind ein freies Volk in unsers Kaisers Staaten,", "tokens": ["Wir", "sind", "ein", "frei\u00b7es", "Volk", "in", "un\u00b7sers", "Kai\u00b7sers", "Staa\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Berufen, hier das Wohl des Landes zu berathen;", "tokens": ["Be\u00b7ru\u00b7fen", ",", "hier", "das", "Wohl", "des", "Lan\u00b7des", "zu", "be\u00b7ra\u00b7then", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Latein zu sprechen, war von jeher unser Recht.", "tokens": ["La\u00b7tein", "zu", "spre\u00b7chen", ",", "war", "von", "je\u00b7her", "un\u00b7ser", "Recht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "VAFIN", "APPR", "ADV", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Ach! w\u00e4ren unsre Professoren", "tokens": ["Ach", "!", "w\u00e4\u00b7ren", "uns\u00b7re", "Pro\u00b7fes\u00b7so\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch in Croatien geboren!", "tokens": ["Doch", "in", "Croa\u00b7ti\u00b7en", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So h\u00e4tten sie von Rom sich nicht so weit entfernt,", "tokens": ["So", "h\u00e4t\u00b7ten", "sie", "von", "Rom", "sich", "nicht", "so", "weit", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NE", "PRF", "PTKNEG", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und w\u00fc\u00dften was man in Secunda lernt.", "tokens": ["Und", "w\u00fc\u00df\u00b7ten", "was", "man", "in", "Se\u00b7cun\u00b7da", "lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PIS", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie r\u00fchmlich ist der Kampf der krieg'rischen Croaten", "tokens": ["Wie", "r\u00fchm\u00b7lich", "ist", "der", "Kampf", "der", "krieg'\u00b7ri\u00b7schen", "Croa\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Ungarns s\u00e4belklirrenden Magnaten!", "tokens": ["Mit", "Un\u00b7garns", "s\u00e4\u00b7bel\u00b7klir\u00b7ren\u00b7den", "Mag\u00b7na\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN