{"dta.poem.21314": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewillkommen, mein Junge, das ist mir lieb,", "tokens": ["\u201e", "will\u00b7kom\u00b7men", ",", "mein", "Jun\u00b7ge", ",", "das", "ist", "mir", "lieb", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da\u00df du mich nicht vergessen;", "tokens": ["Da\u00df", "du", "mich", "nicht", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seit dreizehn Jahren sah ich dich nicht,", "tokens": ["Seit", "drei\u00b7zehn", "Jah\u00b7ren", "sah", "ich", "dich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PPER", "PRF", "PTKNEG", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mir ging es schlecht unterdessen.", "tokens": ["Mir", "ging", "es", "schlecht", "un\u00b7ter\u00b7des\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+----+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "\u201ezu Biberich hab\u2019 ich Steine verschluckt,", "tokens": ["\u201e", "zu", "Bi\u00b7be\u00b7rich", "hab'", "ich", "Stei\u00b7ne", "ver\u00b7schluckt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKA", "ADJD", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wahrhaftig, sie schmeckten nicht lecker!", "tokens": ["Wahr\u00b7haf\u00b7tig", ",", "sie", "schmeck\u00b7ten", "nicht", "le\u00b7cker", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PPER", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Doch schwerer liegen im Magen mir", "tokens": ["Doch", "schwe\u00b7rer", "lie\u00b7gen", "im", "Ma\u00b7gen", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Verse von Niklas Becker.", "tokens": ["Die", "Ver\u00b7se", "von", "Nik\u00b7las", "Be\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201eer hat mich besungen, als ob ich noch", "tokens": ["\u201e", "er", "hat", "mich", "be\u00b7sun\u00b7gen", ",", "als", "ob", "ich", "noch"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PPER", "VVPP", "$,", "KOKOM", "KOUS", "PPER", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die reinste Jungfer w\u00e4re,", "tokens": ["Die", "reins\u00b7te", "Jung\u00b7fer", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sich von niemand rauben l\u00e4\u00dft", "tokens": ["Die", "sich", "von", "nie\u00b7mand", "rau\u00b7ben", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PIS", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Kr\u00e4nzlein ihrer Ehre.", "tokens": ["Das", "Kr\u00e4nz\u00b7lein", "ih\u00b7rer", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201ewenn ich es h\u00f6re, das dumme Lied,", "tokens": ["\u201e", "wenn", "ich", "es", "h\u00f6\u00b7re", ",", "das", "dum\u00b7me", "Lied", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann m\u00f6cht ich mir zerraufen", "tokens": ["Dann", "m\u00f6cht", "ich", "mir", "zer\u00b7rau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den wei\u00dfen Bart, ich m\u00f6chte f\u00fcrwahr", "tokens": ["Den", "wei\u00b7\u00dfen", "Bart", ",", "ich", "m\u00f6ch\u00b7te", "f\u00fcr\u00b7wahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mich in mir selbst ersaufen!", "tokens": ["Mich", "in", "mir", "selbst", "er\u00b7sau\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}