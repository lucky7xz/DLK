{"dta.poem.18233": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Steur' ich nach dem goldnen Vlie\u00df \u2014", "tokens": ["Steur'", "ich", "nach", "dem", "gold\u00b7nen", "Vlie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das verlorne Paradies,", "tokens": ["Das", "ver\u00b7lor\u00b7ne", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, wann werd ich's wiederfinden?", "tokens": ["O", ",", "wann", "werd", "ich's", "wie\u00b7der\u00b7fin\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tag und Nacht, in Schlaf und Wachen,", "tokens": ["Tag", "und", "Nacht", ",", "in", "Schlaf", "und", "Wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wogt um mich die dunkle Fluth,", "tokens": ["Wogt", "um", "mich", "die", "dunk\u00b7le", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Sehnsucht, die nicht ruht,", "tokens": ["Und", "die", "Sehn\u00b7sucht", ",", "die", "nicht", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ja, die Sehnsucht ist mein Nachen!", "tokens": ["Ja", ",", "die", "Sehn\u00b7sucht", "ist", "mein", "Na\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und so gehn denn Mond und Sterne", "tokens": ["Und", "so", "gehn", "denn", "Mond", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVINF", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer wieder meerempor;", "tokens": ["Im\u00b7mer", "wie\u00b7der", "mee\u00b7rem\u00b7por", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wie sie, winkt Edens Thor", "tokens": ["Doch", "wie", "sie", ",", "winkt", "E\u00b7dens", "Thor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "$,", "VVFIN", "NE", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mir ach, immer nur von Ferne.", "tokens": ["Mir", "ach", ",", "im\u00b7mer", "nur", "von", "Fer\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "XY", "$,", "ADV", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber la\u00df das Rad nur rollen,", "tokens": ["A\u00b7ber", "la\u00df", "das", "Rad", "nur", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie's das schon seit je gethan,", "tokens": ["Wie's", "das", "schon", "seit", "je", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADV", "APPR", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn auch ", "tokens": ["Denn", "auch"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "++", "measure": "spondeus"}, "line.4": {"text": "Wird sich ja vollenden wollen.", "tokens": ["Wird", "sich", "ja", "voll\u00b7en\u00b7den", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wind und Wellen werden schlafen", "tokens": ["Wind", "und", "Wel\u00b7len", "wer\u00b7den", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sein Ziel erreicht dein Boot,", "tokens": ["Und", "sein", "Ziel", "er\u00b7reicht", "dein", "Boot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sein Steuermann hei\u00dft Tod", "tokens": ["Denn", "sein", "Steu\u00b7er\u00b7mann", "hei\u00dft", "Tod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Himmel ist sein Hafen!", "tokens": ["Und", "der", "Him\u00b7mel", "ist", "sein", "Ha\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}}}}