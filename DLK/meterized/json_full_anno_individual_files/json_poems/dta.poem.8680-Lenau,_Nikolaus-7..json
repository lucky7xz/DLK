{"dta.poem.8680": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Des Berges Gipfel war erschwungen,", "tokens": ["Des", "Ber\u00b7ges", "Gip\u00b7fel", "war", "er\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der trotzig in die Tiefe schaut.", "tokens": ["Der", "trot\u00b7zig", "in", "die", "Tie\u00b7fe", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Natur, von deinem Reiz durchdrungen,", "tokens": ["Na\u00b7tur", ",", "von", "dei\u00b7nem", "Reiz", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie schlug mein Herz so frei, so laut!", "tokens": ["Wie", "schlug", "mein", "Herz", "so", "frei", ",", "so", "laut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Behaglich streckte dort das Land sich", "tokens": ["Be\u00b7hag\u00b7lich", "streck\u00b7te", "dort", "das", "Land", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Ebnen aus, weit, endlos weit,", "tokens": ["In", "Eb\u00b7nen", "aus", ",", "weit", ",", "end\u00b7los", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Th\u00fcrmen, Wald und Flur, und wand sich", "tokens": ["Mit", "Th\u00fcr\u00b7men", ",", "Wald", "und", "Flur", ",", "und", "wand", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Str\u00f6me Zier ums bunte Kleid;", "tokens": ["Der", "Str\u00f6\u00b7me", "Zier", "ums", "bun\u00b7te", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier stieg es pl\u00f6tzlich und entschlossen", "tokens": ["Hier", "stieg", "es", "pl\u00f6tz\u00b7lich", "und", "ent\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Empor, stets k\u00fchner himmelan,", "tokens": ["Em\u00b7por", ",", "stets", "k\u00fch\u00b7ner", "him\u00b7me\u00b7lan", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Eis und Schnee das Haupt umgossen,", "tokens": ["Mit", "Eis", "und", "Schnee", "das", "Haupt", "um\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vertrat den Wolken ihre Bahn.", "tokens": ["Ver\u00b7trat", "den", "Wol\u00b7ken", "ih\u00b7re", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bald hing mein Auge freudetrunken", "tokens": ["Bald", "hing", "mein", "Au\u00b7ge", "freu\u00b7de\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier an den Felsen, schroff und wild;", "tokens": ["Hier", "an", "den", "Fel\u00b7sen", ",", "schroff", "und", "wild", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald war die Seele still versunken", "tokens": ["Bald", "war", "die", "See\u00b7le", "still", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dort in der Ferne R\u00e4thselbild.", "tokens": ["Dort", "in", "der", "Fer\u00b7ne", "R\u00e4th\u00b7sel\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die dunkle Ferne sandte leise", "tokens": ["Die", "dunk\u00b7le", "Fer\u00b7ne", "sand\u00b7te", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Sehnsucht, ihre Schwester, mir,", "tokens": ["Die", "Sehn\u00b7sucht", ",", "ih\u00b7re", "Schwes\u00b7ter", ",", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rasch verfolgt' ich meine Reise", "tokens": ["Und", "rasch", "ver\u00b7folgt'", "ich", "mei\u00b7ne", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Berg hinab, zu ihr, zu ihr:", "tokens": ["Den", "Berg", "hin\u00b7ab", ",", "zu", "ihr", ",", "zu", "ihr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "PPER", "$,", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u201ewie manchen Zauber mag es geben,", "tokens": ["\u201e", "wie", "man\u00b7chen", "Zau\u00b7ber", "mag", "es", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eden die Natur auch dort ersann;", "tokens": ["\u201e", "den", "die", "Na\u00b7tur", "auch", "dort", "er\u00b7sann", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ewie mancher Biedre mag dort leben,", "tokens": ["\u201e", "wie", "man\u00b7cher", "Bied\u00b7re", "mag", "dort", "le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "PIAT", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u201edem ich die Hand noch dr\u00fccken kann!\u201c", "tokens": ["\u201e", "dem", "ich", "die", "Hand", "noch", "dr\u00fc\u00b7cken", "kann", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PRELS", "PPER", "ART", "NN", "ADV", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}