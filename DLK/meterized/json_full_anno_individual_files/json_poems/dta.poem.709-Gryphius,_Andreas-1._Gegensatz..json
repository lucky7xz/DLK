{"dta.poem.709": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1. Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Doch wenn ein nicht harter Regen ", "tokens": ["Doch", "wenn", "ein", "nicht", "har\u00b7ter", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "PTKNEG", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Diesen durst der Felder lescht. ", "tokens": ["Die\u00b7sen", "durst", "der", "Fel\u00b7der", "lescht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd die d\u00fcrren ", "tokens": ["Vnd", "die", "d\u00fcr\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wenn die winde sich bewegen:", "tokens": ["Wenn", "die", "win\u00b7de", "sich", "be\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd k\u00fclen lufft vnd ", "tokens": ["Vnd", "k\u00fc\u00b7len", "lufft", "vnd"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bald lebt was vorhin todt.", "tokens": ["Bald", "lebt", "was", "vor\u00b7hin", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So: ", "tokens": ["So", ":"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Dann lachen wir in noth.", "tokens": ["Dann", "la\u00b7chen", "wir", "in", "noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Wenn vn\u00df dein Allmachts Taw erquicket:", "tokens": ["Wenn", "vn\u00df", "dein", "All\u00b7machts", "Taw", "er\u00b7quic\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn vn\u00df dein Liebe-wind anblicket:", "tokens": ["Wenn", "vn\u00df", "dein", "Lie\u00b7be\u00b7wind", "an\u00b7bli\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn deines Segens Regen netzet:", "tokens": ["Wenn", "dei\u00b7nes", "Se\u00b7gens", "Re\u00b7gen", "net\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So fleu", "tokens": ["So", "fleu"], "token_info": ["word", "word"], "pos": ["ADV", "NN"], "meter": "-+", "measure": "iambic.single"}}}}}