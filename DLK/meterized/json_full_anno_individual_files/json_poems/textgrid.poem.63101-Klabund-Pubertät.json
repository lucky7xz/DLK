": ["NE", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00fcbsches Antlitz, pralle Br\u00fcste \u2013", "tokens": ["H\u00fcb\u00b7sches", "Ant\u00b7litz", ",", "pral\u00b7le", "Br\u00fcs\u00b7te", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aphrodite, ach versteckt sich!", "tokens": ["A\u00b7phro\u00b7di\u00b7te", ",", "ach", "ver\u00b7steckt", "sich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ITJ", "VVFIN", "PRF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jeden Abend, o wie gr\u00e4\u00dflich,", "tokens": ["Je\u00b7den", "A\u00b7bend", ",", "o", "wie", "gr\u00e4\u00df\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "FM", "KOKOM", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind sie immer \u00fcberschminkter,", "tokens": ["Sind", "sie", "im\u00b7mer", "\u00fc\u00b7bersc\u00b7hmink\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer h\u00e4\u00dflicher als h\u00e4\u00dflich.", "tokens": ["Im\u00b7mer", "h\u00e4\u00df\u00b7li\u00b7cher", "als", "h\u00e4\u00df\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Verstand, die H\u00e4nde ringt er.", "tokens": ["Mein", "Ver\u00b7stand", ",", "die", "H\u00e4n\u00b7de", "ringt", "er", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", 