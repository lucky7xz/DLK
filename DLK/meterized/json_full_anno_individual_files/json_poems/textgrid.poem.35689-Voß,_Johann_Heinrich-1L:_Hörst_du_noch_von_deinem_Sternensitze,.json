: {"text": "Und, vor Wonne, bebtest!", "tokens": ["Und", ",", "vor", "Won\u00b7ne", ",", "beb\u00b7test", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Da\u00df auch sie, bei ihrer Fackel Strahle,", "tokens": ["Da\u00df", "auch", "sie", ",", "bei", "ih\u00b7rer", "Fa\u00b7ckel", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Durch des Todes d\u00fcstre Schreckenthale,", "tokens": ["Durch", "des", "To\u00b7des", "d\u00fcst\u00b7re", "Schre\u00b7cken\u00b7tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hin, wo Engelhalleluja schallen,", "tokens": ["Hin", ",", "wo", "En\u00b7gel\u00b7hal\u00b7le\u00b7lu\u00b7ja", "schal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unerschrocken wallen;", "tokens": ["Un\u00b7er\u00b7schro\u00b7cken", "wal\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Und um dich, auf goldnen Blumenauen,", "tokens": ["Und", "um", "dich", ",", "auf", "gold\u00b7nen", "Blu\u00b7me\u00b7nau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine hellen Freund' und Kinder schauen,", "tokens": ["Dei\u00b7ne", "hel\u00b7len", "Freund'", "und", "Kin\u00b7der", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und den Lebenshain mit Ros' umr\u00f6tet,", "tokens": ["Und", "den", "Le\u00b7bens\u00b7hain", "mit", "Ros'", "um\u00b7r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Den f\u00fcr uns ihr s\u00e4tet!", "tokens": ["Den", "f\u00fcr", "uns", "ihr", "s\u00e4\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Unsrer Mutter thr\u00e4nenlose Trauer,", "tokens": ["Uns\u00b7rer", "Mut\u00b7ter", "thr\u00e4\u00b7nen\u00b7lo\u00b7se", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und des besten M\u00e4dchens bange Schauer,", "tokens": ["Und", "des", "bes\u00b7ten", "M\u00e4d\u00b7chens", "ban\u00b7ge", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! sie foltern, foltern noch am wehsten!", "tokens": ["Ach", "!", "sie", "fol\u00b7tern", ",", "fol\u00b7tern", "noch", "am", "wehs\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVINF", "$,", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ich kann nicht tr\u00f6sten!", "tokens": ["Und", "ich", "kann", "nicht", "tr\u00f6s\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}