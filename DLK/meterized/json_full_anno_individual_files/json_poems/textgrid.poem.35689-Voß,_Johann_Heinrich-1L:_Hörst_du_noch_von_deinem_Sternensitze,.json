{"textgrid.poem.35689": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: H\u00f6rst du noch von deinem Sternensitze,", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rst du noch von deinem Sternensitze,", "tokens": ["H\u00f6rst", "du", "noch", "von", "dei\u00b7nem", "Ster\u00b7nen\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sp\u00e4ht dein Blick, gew\u00f6hnt an Sonnenblitze,", "tokens": ["Sp\u00e4ht", "dein", "Blick", ",", "ge\u00b7w\u00f6hnt", "an", "Son\u00b7nen\u00b7blit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Noch in diesem Grabthal' unsre Thr\u00e4nen,", "tokens": ["Noch", "in", "die\u00b7sem", "Grabt\u00b7hal'", "uns\u00b7re", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unser starres Sehnen?", "tokens": ["Un\u00b7ser", "star\u00b7res", "Seh\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Oder fl\u00fcsterst du, noch jetzt der W\u00e4chter", "tokens": ["O\u00b7der", "fl\u00fcs\u00b7terst", "du", ",", "noch", "jetzt", "der", "W\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deiner Gattin, deiner S\u00f6hn' und T\u00f6chter,", "tokens": ["Dei\u00b7ner", "Gat\u00b7tin", ",", "dei\u00b7ner", "S\u00f6hn'", "und", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fl\u00fcsterst du, als Schutzgeist, unsrer Seele", "tokens": ["Fl\u00fcs\u00b7terst", "du", ",", "als", "Schutz\u00b7geist", ",", "uns\u00b7rer", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "G\u00f6ttliche Befehle?", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Be\u00b7feh\u00b7le", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Schwebst du hier auf diesen Balsaml\u00fcften", "tokens": ["Schwebst", "du", "hier", "auf", "die\u00b7sen", "Bal\u00b7sam\u00b7l\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die geheim der Nachtviol' entd\u00fcften,", "tokens": ["Die", "ge\u00b7heim", "der", "Nacht\u00b7vi\u00b7ol'", "ent\u00b7d\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie einst deinem Leben stille G\u00fcte,", "tokens": ["Wie", "einst", "dei\u00b7nem", "Le\u00b7ben", "stil\u00b7le", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Eh es, ach! verbl\u00fchte?", "tokens": ["Eh", "es", ",", "ach", "!", "ver\u00b7bl\u00fch\u00b7te", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ITJ", "$.", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "O! so wahr du schaust, an den ich glaube!", "tokens": ["O", "!", "so", "wahr", "du", "schaust", ",", "an", "den", "ich", "glau\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "ADJD", "PPER", "VVFIN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Z\u00fcrne nicht, o Vater, mit dem Staube,", "tokens": ["Z\u00fcr\u00b7ne", "nicht", ",", "o", "Va\u00b7ter", ",", "mit", "dem", "Stau\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "FM", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo ich dich \u2013 wo wir, nicht ohne Zagen,", "tokens": ["Wo", "ich", "dich", "\u2013", "wo", "wir", ",", "nicht", "oh\u00b7ne", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "$(", "PWAV", "PPER", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unsern Vater klagen!", "tokens": ["Un\u00b7sern", "Va\u00b7ter", "kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Leucht' in unsre finstern Sinnen Klarheit,", "tokens": ["Leucht'", "in", "uns\u00b7re", "fins\u00b7tern", "Sin\u00b7nen", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und geleite sie zur hohen Wahrheit,", "tokens": ["Und", "ge\u00b7lei\u00b7te", "sie", "zur", "ho\u00b7hen", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Welcher du durch Todesgram nachstrebtest,", "tokens": ["Wel\u00b7cher", "du", "durch", "To\u00b7des\u00b7gram", "nach\u00b7streb\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und, vor Wonne, bebtest!", "tokens": ["Und", ",", "vor", "Won\u00b7ne", ",", "beb\u00b7test", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Da\u00df auch sie, bei ihrer Fackel Strahle,", "tokens": ["Da\u00df", "auch", "sie", ",", "bei", "ih\u00b7rer", "Fa\u00b7ckel", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Durch des Todes d\u00fcstre Schreckenthale,", "tokens": ["Durch", "des", "To\u00b7des", "d\u00fcst\u00b7re", "Schre\u00b7cken\u00b7tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hin, wo Engelhalleluja schallen,", "tokens": ["Hin", ",", "wo", "En\u00b7gel\u00b7hal\u00b7le\u00b7lu\u00b7ja", "schal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unerschrocken wallen;", "tokens": ["Un\u00b7er\u00b7schro\u00b7cken", "wal\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Und um dich, auf goldnen Blumenauen,", "tokens": ["Und", "um", "dich", ",", "auf", "gold\u00b7nen", "Blu\u00b7me\u00b7nau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine hellen Freund' und Kinder schauen,", "tokens": ["Dei\u00b7ne", "hel\u00b7len", "Freund'", "und", "Kin\u00b7der", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und den Lebenshain mit Ros' umr\u00f6tet,", "tokens": ["Und", "den", "Le\u00b7bens\u00b7hain", "mit", "Ros'", "um\u00b7r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Den f\u00fcr uns ihr s\u00e4tet!", "tokens": ["Den", "f\u00fcr", "uns", "ihr", "s\u00e4\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Unsrer Mutter thr\u00e4nenlose Trauer,", "tokens": ["Uns\u00b7rer", "Mut\u00b7ter", "thr\u00e4\u00b7nen\u00b7lo\u00b7se", "Trau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und des besten M\u00e4dchens bange Schauer,", "tokens": ["Und", "des", "bes\u00b7ten", "M\u00e4d\u00b7chens", "ban\u00b7ge", "Schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! sie foltern, foltern noch am wehsten!", "tokens": ["Ach", "!", "sie", "fol\u00b7tern", ",", "fol\u00b7tern", "noch", "am", "wehs\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVINF", "$,", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ich kann nicht tr\u00f6sten!", "tokens": ["Und", "ich", "kann", "nicht", "tr\u00f6s\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "H\u00f6rst du noch von deinem Sternensitze,", "tokens": ["H\u00f6rst", "du", "noch", "von", "dei\u00b7nem", "Ster\u00b7nen\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sp\u00e4ht dein Blick, gew\u00f6hnt an Sonnenblitze,", "tokens": ["Sp\u00e4ht", "dein", "Blick", ",", "ge\u00b7w\u00f6hnt", "an", "Son\u00b7nen\u00b7blit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN