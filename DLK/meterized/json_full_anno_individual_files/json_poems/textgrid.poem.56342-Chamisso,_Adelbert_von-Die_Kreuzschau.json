{"textgrid.poem.56342": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Die Kreuzschau", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Pilger, der die H\u00f6hen \u00fcberstiegen,", "tokens": ["Der", "Pil\u00b7ger", ",", "der", "die", "H\u00f6\u00b7hen", "\u00fc\u00b7bers\u00b7tie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah jenseits schon das ausgespannte Tal", "tokens": ["Sah", "jen\u00b7seits", "schon", "das", "aus\u00b7ge\u00b7spann\u00b7te", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Abendglut vor seinen F\u00fc\u00dfen liegen.", "tokens": ["In", "A\u00b7bend\u00b7glut", "vor", "sei\u00b7nen", "F\u00fc\u00b7\u00dfen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf duft'ges Gras, im milden Sonnenstrahl", "tokens": ["Auf", "duft'\u00b7ges", "Gras", ",", "im", "mil\u00b7den", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Streckt' er ermattet sich zur Ruhe nieder,", "tokens": ["Streckt'", "er", "er\u00b7mat\u00b7tet", "sich", "zur", "Ru\u00b7he", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Indem er seinem Sch\u00f6pfer sich befahl.", "tokens": ["In\u00b7dem", "er", "sei\u00b7nem", "Sch\u00f6p\u00b7fer", "sich", "be\u00b7fahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ihm fielen zu die matten Augenlider,", "tokens": ["Ihm", "fie\u00b7len", "zu", "die", "mat\u00b7ten", "Au\u00b7gen\u00b7li\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch seinen wachen Geist enthob ein Traum", "tokens": ["Doch", "sei\u00b7nen", "wa\u00b7chen", "Geist", "ent\u00b7hob", "ein", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der ird'schen H\u00fclle seiner tr\u00e4gen Glieder.", "tokens": ["Der", "ird'\u00b7schen", "H\u00fcl\u00b7le", "sei\u00b7ner", "tr\u00e4\u00b7gen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der Schild der Sonne ward im Himmelsraum", "tokens": ["Der", "Schild", "der", "Son\u00b7ne", "ward", "im", "Him\u00b7mels\u00b7raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Zu Gottes Angesicht, das Firmament", "tokens": ["Zu", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", ",", "das", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Zu seinem Kleid, das Land zu dessen Saum.", "tokens": ["Zu", "sei\u00b7nem", "Kleid", ",", "das", "Land", "zu", "des\u00b7sen", "Saum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "PRELAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "\u00bbdu wirst dem, dessen Herz dich Vater nennt,", "tokens": ["\u00bb", "du", "wirst", "dem", ",", "des\u00b7sen", "Herz", "dich", "Va\u00b7ter", "nennt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "$,", "PRELAT", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Nicht, Herr, im Zorn entziehen deinen Frieden,", "tokens": ["Nicht", ",", "Herr", ",", "im", "Zorn", "ent\u00b7zie\u00b7hen", "dei\u00b7nen", "Frie\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "NN", "$,", "APPRART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Wenn seine Schw\u00e4chen er vor dir bekennt.", "tokens": ["Wenn", "sei\u00b7ne", "Schw\u00e4\u00b7chen", "er", "vor", "dir", "be\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Da\u00df, wen ein Weib gebar, sein Kreuz hienieden", "tokens": ["Da\u00df", ",", "wen", "ein", "Weib", "ge\u00b7bar", ",", "sein", "Kreuz", "hien\u00b7ie\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "PWS", "ART", "NN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Auch duldend tragen mu\u00df, ich wei\u00df es lange,", "tokens": ["Auch", "dul\u00b7dend", "tra\u00b7gen", "mu\u00df", ",", "ich", "wei\u00df", "es", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Doch sind der Menschen Last und Leid verschieden.", "tokens": ["Doch", "sind", "der", "Men\u00b7schen", "Last", "und", "Leid", "ver\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NE", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Mein Kreuz ist allzu schwer; sieh ich verlange", "tokens": ["Mein", "Kreuz", "ist", "all\u00b7zu", "schwer", ";", "sieh", "ich", "ver\u00b7lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$.", "VVIMP", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Die Last nur angemessen meiner Kraft;", "tokens": ["Die", "Last", "nur", "an\u00b7ge\u00b7mes\u00b7sen", "mei\u00b7ner", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ich unterliege, Herr, zu hartem Zwange.\u00ab", "tokens": ["Ich", "un\u00b7ter\u00b7lie\u00b7ge", ",", "Herr", ",", "zu", "har\u00b7tem", "Zwan\u00b7ge", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wie so er sprach zum H\u00f6chsten kinderhaft,", "tokens": ["Wie", "so", "er", "sprach", "zum", "H\u00f6chs\u00b7ten", "kin\u00b7der\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Kam brausend her der Sturm und es geschah,", "tokens": ["Kam", "brau\u00b7send", "her", "der", "Sturm", "und", "es", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "ART", "NN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Da\u00df aufw\u00e4rts er sich f\u00fchlte hingerafft.", "tokens": ["Da\u00df", "auf\u00b7w\u00e4rts", "er", "sich", "f\u00fchl\u00b7te", "hin\u00b7ge\u00b7rafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PRF", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Und wie er Boden fa\u00dfte, fand er da", "tokens": ["Und", "wie", "er", "Bo\u00b7den", "fa\u00df\u00b7te", ",", "fand", "er", "da"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Sich einsam in der Mitte r\u00e4um'ger Hallen,", "tokens": ["Sich", "ein\u00b7sam", "in", "der", "Mit\u00b7te", "r\u00e4um'\u00b7ger", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Wo ringsum sonder Zahl er Kreuze sah.", "tokens": ["Wo", "ring\u00b7sum", "son\u00b7der", "Zahl", "er", "Kreu\u00b7ze", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Uns eine Stimme h\u00f6rt' er dr\u00f6hnend hallen:", "tokens": ["Uns", "ei\u00b7ne", "Stim\u00b7me", "h\u00f6rt'", "er", "dr\u00f6h\u00b7nend", "hal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "\u00bbhier aufgespeichert ist das Leid; du hast", "tokens": ["\u00bb", "hier", "auf\u00b7ge\u00b7spei\u00b7chert", "ist", "das", "Leid", ";", "du", "hast"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "VVPP", "VAFIN", "ART", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Zu w\u00e4hlen unter diesen Kreuzen allen.\u00ab", "tokens": ["Zu", "w\u00e4h\u00b7len", "un\u00b7ter", "die\u00b7sen", "Kreu\u00b7zen", "al\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PDAT", "NN", "PIAT", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Versuchend ging er da, unschl\u00fcssig fast,", "tokens": ["Ver\u00b7su\u00b7chend", "ging", "er", "da", ",", "un\u00b7schl\u00fcs\u00b7sig", "fast", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Von einem Kreuz zum anderen umher,", "tokens": ["Von", "ei\u00b7nem", "Kreuz", "zum", "an\u00b7de\u00b7ren", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Sich auszupr\u00fcfen die bequemre Last.", "tokens": ["Sich", "aus\u00b7zu\u00b7pr\u00fc\u00b7fen", "die", "be\u00b7quem\u00b7re", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "Dies Kreuz war ihm zu gro\u00df und das zu schwer,", "tokens": ["Dies", "Kreuz", "war", "ihm", "zu", "gro\u00df", "und", "das", "zu", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "KON", "PDS", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "So schwer und gro\u00df war jenes andre nicht,", "tokens": ["So", "schwer", "und", "gro\u00df", "war", "je\u00b7nes", "and\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VAFIN", "PDS", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Doch scharf von Kanten dr\u00fcckt' es desto mehr.", "tokens": ["Doch", "scharf", "von", "Kan\u00b7ten", "dr\u00fcckt'", "es", "des\u00b7to", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Das dort, das warf wie Gold ein glei\u00dfend Licht,", "tokens": ["Das", "dort", ",", "das", "warf", "wie", "Gold", "ein", "glei\u00b7\u00dfend", "Licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "$,", "PDS", "VVFIN", "KOKOM", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Das lockt' ihn, unversucht es nicht zu lassen,", "tokens": ["Das", "lockt'", "ihn", ",", "un\u00b7ver\u00b7sucht", "es", "nicht", "zu", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ADJD", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Dem goldnen Glanz entsprach auch das Gewicht.", "tokens": ["Dem", "gold\u00b7nen", "Glanz", "ent\u00b7sprach", "auch", "das", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Er mochte dieses heben, jenes fassen,", "tokens": ["Er", "moch\u00b7te", "die\u00b7ses", "he\u00b7ben", ",", "je\u00b7nes", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "$,", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Zu keinem neigte noch sich seine Wahl,", "tokens": ["Zu", "kei\u00b7nem", "neig\u00b7te", "noch", "sich", "sei\u00b7ne", "Wahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ADV", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Es wollte keines, keines f\u00fcr ihn passen.", "tokens": ["Es", "woll\u00b7te", "kei\u00b7nes", ",", "kei\u00b7nes", "f\u00fcr", "ihn", "pas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "$,", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Durchmustert hatt er schon die ganze Zahl \u2013", "tokens": ["Durch\u00b7mus\u00b7tert", "hatt", "er", "schon", "die", "gan\u00b7ze", "Zahl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Verlorne M\u00fch! Vergebens war's geschehen!", "tokens": ["Ver\u00b7lor\u00b7ne", "M\u00fch", "!", "Ver\u00b7ge\u00b7bens", "wa\u00b7r's", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.45": {"text": "Durchmustern mu\u00dft er sie zum andern Mal.", "tokens": ["Durch\u00b7mus\u00b7tern", "mu\u00dft", "er", "sie", "zum", "an\u00b7dern", "Mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Und nun gewahrt' er, fr\u00fcher \u00fcbersehen,", "tokens": ["Und", "nun", "ge\u00b7wahrt'", "er", ",", "fr\u00fc\u00b7her", "\u00fc\u00b7ber\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Ein Kreuz, das leidlicher ihm schien zu sein,", "tokens": ["Ein", "Kreuz", ",", "das", "leid\u00b7li\u00b7cher", "ihm", "schien", "zu", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "