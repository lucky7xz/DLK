{"textgrid.poem.37270": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[selig sind die Auserw\u00e4hlten]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selig sind die Auserw\u00e4hlten,", "tokens": ["Se\u00b7lig", "sind", "die", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich liebten und verm\u00e4hlten;", "tokens": ["Die", "sich", "lieb\u00b7ten", "und", "ver\u00b7m\u00e4hl\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie tragen h\u00fcbsche Fr\u00fcchte.", "tokens": ["Denn", "sie", "tra\u00b7gen", "h\u00fcb\u00b7sche", "Fr\u00fcch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und so wuchert die Geschichte", "tokens": ["Und", "so", "wu\u00b7chert", "die", "Ge\u00b7schich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sichtbarlich von Ort zu Ort.", "tokens": ["Sicht\u00b7bar\u00b7lich", "von", "Ort", "zu", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Doch die braven Junggesellen,", "tokens": ["Doch", "die", "bra\u00b7ven", "Jung\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jungfern ohne Ehestellen,", "tokens": ["Jung\u00b7fern", "oh\u00b7ne", "E\u00b7hes\u00b7tel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welche ohne Leibeserben", "tokens": ["Wel\u00b7che", "oh\u00b7ne", "Lei\u00b7be\u00b7ser\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "So als Blattgew\u00e4chse sterben,", "tokens": ["So", "als", "Blatt\u00b7ge\u00b7w\u00e4ch\u00b7se", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Pflanzen sich durch Knollen fort.", "tokens": ["Pflan\u00b7zen", "sich", "durch", "Knol\u00b7len", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Selig sind die Auserw\u00e4hlten,", "tokens": ["Se\u00b7lig", "sind", "die", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich liebten und verm\u00e4hlten;", "tokens": ["Die", "sich", "lieb\u00b7ten", "und", "ver\u00b7m\u00e4hl\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie tragen h\u00fcbsche Fr\u00fcchte.", "tokens": ["Denn", "sie", "tra\u00b7gen", "h\u00fcb\u00b7sche", "Fr\u00fcch\u00b7te", "."], "token_info": 