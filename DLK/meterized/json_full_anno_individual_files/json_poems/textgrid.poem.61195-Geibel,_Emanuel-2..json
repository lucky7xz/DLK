{"textgrid.poem.61195": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Winde m\u00f6cht' ich meine Sorge geben,", "tokens": ["Dem", "Win\u00b7de", "m\u00f6cht'", "ich", "mei\u00b7ne", "Sor\u00b7ge", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df er hinaus ins weite Meer sie tr\u00fcge,", "tokens": ["Da\u00df", "er", "hin\u00b7aus", "ins", "wei\u00b7te", "Meer", "sie", "tr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich m\u00f6chte, meiner Jugend Traumesfl\u00fcge", "tokens": ["Ich", "m\u00f6ch\u00b7te", ",", "mei\u00b7ner", "Ju\u00b7gend", "Trau\u00b7mes\u00b7fl\u00fc\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erneuend, wieder k\u00fchn ins Blaue streben.", "tokens": ["Er\u00b7neu\u00b7end", ",", "wie\u00b7der", "k\u00fchn", "ins", "Blau\u00b7e", "stre\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ADJD", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch ernster ward und bittrer ward das Leben,", "tokens": ["Doch", "erns\u00b7ter", "ward", "und", "bit\u00b7trer", "ward", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es gibt uns Seufzer statt der Atemz\u00fcge,", "tokens": ["Es", "gibt", "uns", "Seuf\u00b7zer", "statt", "der", "A\u00b7tem\u00b7z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist jede Lust doch eine halbe L\u00fcge,", "tokens": ["Ist", "je\u00b7de", "Lust", "doch", "ei\u00b7ne", "hal\u00b7be", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Wetter so wie jetzt am Himmel schweben.", "tokens": ["Wenn", "Wet\u00b7ter", "so", "wie", "jetzt", "am", "Him\u00b7mel", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "KOKOM", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Lenz hat seinen Rosenduft verloren;", "tokens": ["Der", "Lenz", "hat", "sei\u00b7nen", "Ro\u00b7sen\u00b7duft", "ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Hoffnung selbst, die jugendliche, rasche,", "tokens": ["Die", "Hoff\u00b7nung", "selbst", ",", "die", "ju\u00b7gend\u00b7li\u00b7che", ",", "ra\u00b7sche", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Pocht wie ein Kind nur sch\u00fcchtern an den Toren.", "tokens": ["Pocht", "wie", "ein", "Kind", "nur", "sch\u00fcch\u00b7tern", "an", "den", "To\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Lust versieget mit dem Gold der Flasche,", "tokens": ["Die", "Lust", "ver\u00b7sie\u00b7get", "mit", "dem", "Gold", "der", "Fla\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nur der Schmerz steigt ewig neugeboren,", "tokens": ["Und", "nur", "der", "Schmerz", "steigt", "e\u00b7wig", "neu\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein dunkler Ph\u00f6nix, wieder aus der Asche.", "tokens": ["Ein", "dunk\u00b7ler", "Ph\u00f6\u00b7nix", ",", "wie\u00b7der", "aus", "der", "A\u00b7sche", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dem Winde m\u00f6cht' ich meine Sorge geben,", "tokens": ["Dem", "Win\u00b7de", "m\u00f6cht'", "ich", "mei\u00b7ne", "Sor\u00b7ge", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "