{"textgrid.poem.37105": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht nach Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfe Kehle des Hains, welche mir sonst, im May,", "tokens": ["S\u00fc\u00b7\u00dfe", "Keh\u00b7le", "des", "Hains", ",", "wel\u00b7che", "mir", "sonst", ",", "im", "May", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ganz den Himmel ins Herz fl\u00f6tete, Nachtigall,", "tokens": ["Ganz", "den", "Him\u00b7mel", "ins", "Herz", "fl\u00f6\u00b7te\u00b7te", ",", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Warum fl\u00f6tet dein Lied mir", "tokens": ["Wa\u00b7rum", "fl\u00f6\u00b7tet", "dein", "Lied", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "PPER"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Keine Wonne mehr in die Brust?", "tokens": ["Kei\u00b7ne", "Won\u00b7ne", "mehr", "in", "die", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Liebe l\u00e4chelt dir nicht! seufzet die Nachtigall,", "tokens": ["Lie\u00b7be", "l\u00e4\u00b7chelt", "dir", "nicht", "!", "seuf\u00b7zet", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die den Blumen des Mays hellere R\u00f6the giebt,", "tokens": ["Die", "den", "Blu\u00b7men", "des", "Mays", "hel\u00b7le\u00b7re", "R\u00f6\u00b7the", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und den Kehlen des Waldes", "tokens": ["Und", "den", "Keh\u00b7len", "des", "Wal\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Einen helleren Wonneklang.", "tokens": ["Ei\u00b7nen", "hel\u00b7le\u00b7ren", "Won\u00b7ne\u00b7klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Liebe l\u00e4chelt dir nicht! rauschet mir jedes Blatt \u2013", "tokens": ["Lie\u00b7be", "l\u00e4\u00b7chelt", "dir", "nicht", "!", "rau\u00b7schet", "mir", "je\u00b7des", "Blatt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Quillt die Thr\u00e4ne mir schon? Flattert mir das Phantom", "tokens": ["Quillt", "die", "Thr\u00e4\u00b7ne", "mir", "schon", "?", "Flat\u00b7tert", "mir", "das", "Phan\u00b7tom"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "$.", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.3": {"text": "Todter Freuden schon wieder", "tokens": ["Tod\u00b7ter", "Freu\u00b7den", "schon", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "ADV"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Vor den Augen der Phantasie?", "tokens": ["Vor", "den", "Au\u00b7gen", "der", "Phan\u00b7ta\u00b7sie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Rosicht schwebt es herauf. \u2013 \u2013 Laura, die Grazie,", "tokens": ["Ro\u00b7sicht", "schwebt", "es", "her\u00b7auf", ".", "\u2013", "\u2013", "Lau\u00b7ra", ",", "die", "Gra\u00b7zie", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "$(", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Laura h\u00fcpfet daher, die mir den ersten Rausch", "tokens": ["Lau\u00b7ra", "h\u00fcp\u00b7fet", "da\u00b7her", ",", "die", "mir", "den", "ers\u00b7ten", "Rausch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PAV", "$,", "PRELS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Ueberirrdischer Wonne", "tokens": ["Ue\u00b7be\u00b7rirr\u00b7di\u00b7scher", "Won\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Durch die bebende Seele go\u00df.", "tokens": ["Durch", "die", "be\u00b7ben\u00b7de", "See\u00b7le", "go\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Flieh hinweg, o Phantom! Laura, die Grazie,", "tokens": ["Flieh", "hin\u00b7weg", ",", "o", "Phan\u00b7tom", "!", "Lau\u00b7ra", ",", "die", "Gra\u00b7zie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "NN", "$.", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Liebt das D\u00f6rfchen nicht mehr, gaukelt von Ball zu Ball,", "tokens": ["Liebt", "das", "D\u00f6rf\u00b7chen", "nicht", "mehr", ",", "gau\u00b7kelt", "von", "Ball", "zu", "Ball", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Fleugt, im zirkelnden Reigen,", "tokens": ["Fleugt", ",", "im", "zir\u00b7keln\u00b7den", "Rei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Durch den schallenden Kerzensaal.", "tokens": ["Durch", "den", "schal\u00b7len\u00b7den", "Ker\u00b7zen\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Sie miskennet mein Herz, w\u00e4hnet mich kalt und dumm,", "tokens": ["Sie", "mis\u00b7ken\u00b7net", "mein", "Herz", ",", "w\u00e4h\u00b7net", "mich", "kalt", "und", "dumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil kein goldener Prunk mir vom Gewande blitzt,", "tokens": ["Weil", "kein", "gol\u00b7de\u00b7ner", "Prunk", "mir", "vom", "Ge\u00b7wan\u00b7de", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und mein Fu\u00df die Talente,", "tokens": ["Und", "mein", "Fu\u00df", "die", "Ta\u00b7len\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Die Lutetien lehrt, nicht hat.", "tokens": ["Die", "Lu\u00b7te\u00b7ti\u00b7en", "lehrt", ",", "nicht", "hat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Soll denn nie das Gef\u00fchl, welches ein Feuerku\u00df", "tokens": ["Soll", "denn", "nie", "das", "Ge\u00b7f\u00fchl", ",", "wel\u00b7ches", "ein", "Feu\u00b7er\u00b7ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.2": {"text": "An der klopfenden Brust einer Geliebten giebt,", "tokens": ["An", "der", "klop\u00b7fen\u00b7den", "Brust", "ei\u00b7ner", "Ge\u00b7lieb\u00b7ten", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Meine Seele durchstr\u00f6men,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "durch\u00b7str\u00f6\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Bis die Blume der Jugend welkt?", "tokens": ["Bis", "die", "Blu\u00b7me", "der", "Ju\u00b7gend", "welkt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Geu\u00df mir Lieb' in die Brust, wenn du des Sonnensaals", "tokens": ["Geu\u00df", "mir", "Lieb'", "in", "die", "Brust", ",", "wenn", "du", "des", "Son\u00b7nen\u00b7sa\u00b7als"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Zinnen wieder entsinkst, l\u00e4chelnder Mayenmond,", "tokens": ["Zin\u00b7nen", "wie\u00b7der", "ent\u00b7sinkst", ",", "l\u00e4\u00b7cheln\u00b7der", "Ma\u00b7yen\u00b7mond", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Oder wecke, mit lindem", "tokens": ["O\u00b7der", "we\u00b7cke", ",", "mit", "lin\u00b7dem"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Odem, Blumen auf meiner Gruft.", "tokens": ["O\u00b7dem", ",", "Blu\u00b7men", "auf", "mei\u00b7ner", "Gruft", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "S\u00fc\u00dfe Kehle des Hains, welche mir sonst, im May,", "tokens": ["S\u00fc\u00b7\u00dfe", "Keh\u00b7le", "des", "Hains", ",", "wel\u00b7che", "mir", "sonst", ",", "im", "May", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ganz den Himmel ins Herz fl\u00f6tete, Nachtigall,", "tokens": ["Ganz", "den", "Him\u00b7mel", "ins", "Herz", "fl\u00f6\u00b7te\u00b7te", ",", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Warum fl\u00f6tet dein Lied mir", "tokens": ["Wa\u00b7rum", "fl\u00f6\u00b7tet", "dein", "Lied", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "PPER"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Keine Wonne mehr in die Brust?", "tokens": ["Kei\u00b7ne", "Won\u00b7ne", "mehr", "in", "die", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Liebe l\u00e4chelt dir nicht! seufzet die Nachtigall,", "tokens": ["Lie\u00b7be", "l\u00e4\u00b7chelt", "dir", "nicht", "!", "seuf\u00b7zet", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die den Blumen des Mays hellere R\u00f6the giebt,", "tokens": ["Die", "den", "Blu\u00b7men", "des", "Mays", "hel\u00b7