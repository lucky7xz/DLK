{"textgrid.poem.37778": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Flammenruh nach Weisheit streben", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flammenruh nach Weisheit streben", "tokens": ["Flam\u00b7men\u00b7ruh", "nach", "Weis\u00b7heit", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Senkt den J\u00fcnger tief in Schlaf,", "tokens": ["Senkt", "den", "J\u00fcn\u00b7ger", "tief", "in", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es gl\u00fcht sein innres Leben,", "tokens": ["Und", "es", "gl\u00fcht", "sein", "inn\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wenn Blitz die Tanne traf.", "tokens": ["Als", "wenn", "Blitz", "die", "Tan\u00b7ne", "traf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Festlich statt der schwarzen Krone", "tokens": ["Fest\u00b7lich", "statt", "der", "schwar\u00b7zen", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4gt sie einen Flammenkranz,", "tokens": ["Tr\u00e4gt", "sie", "ei\u00b7nen", "Flam\u00b7men\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weihrauch tr\u00e4ufelt von dem Throne,", "tokens": ["Weih\u00b7rauch", "tr\u00e4u\u00b7felt", "von", "dem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Halme wirbeln rings im Tanz.", "tokens": ["Hal\u00b7me", "wir\u00b7beln", "rings", "im", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonst da dr\u00e4uten ihm die Bilder,", "tokens": ["Sonst", "da", "dr\u00e4u\u00b7ten", "ihm", "die", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schrecklich roth und blau gemalt,", "tokens": ["Schreck\u00b7lich", "roth", "und", "blau", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Zeichen noch viel wilder", "tokens": ["Und", "die", "Zei\u00b7chen", "noch", "viel", "wil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Thier in Flaschen schalt;", "tokens": ["Und", "das", "Thier", "in", "Fla\u00b7schen", "schalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An den tausend Messingscheiben,", "tokens": ["An", "den", "tau\u00b7send", "Mes\u00b7sing\u00b7schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo das Blei am Faden h\u00e4ngt,", "tokens": ["Wo", "das", "Blei", "am", "Fa\u00b7den", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00dft er sich erst m\u00fcde treiben,", "tokens": ["Mu\u00dft", "er", "sich", "erst", "m\u00fc\u00b7de", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eh der Schlaf ihn s\u00fc\u00df umf\u00e4ngt.", "tokens": ["Eh", "der", "Schlaf", "ihn", "s\u00fc\u00df", "um\u00b7f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Liebchen kommt nun ihn zu k\u00fcssen,", "tokens": ["Lieb\u00b7chen", "kommt", "nun", "ihn", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber er vernimmt sie nicht,", "tokens": ["A\u00b7ber", "er", "ver\u00b7nimmt", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmlisch mild die Sterne gr\u00fc\u00dfen", "tokens": ["Himm\u00b7lisch", "mild", "die", "Ster\u00b7ne", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er steht in vollem Licht,", "tokens": ["Und", "er", "steht", "in", "vol\u00b7lem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sie setzt sich ihm zu F\u00fc\u00dfen", "tokens": ["Und", "sie", "setzt", "sich", "ihm", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und umfasset seine Knie,", "tokens": ["Und", "um\u00b7fas\u00b7set", "sei\u00b7ne", "Knie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sollt sie ihn nicht wecken m\u00fcssen,", "tokens": ["Sollt", "sie", "ihn", "nicht", "we\u00b7cken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Er erwachet sonst wohl nie.", "tokens": ["Er", "er\u00b7wa\u00b7chet", "sonst", "wohl", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Leise kam sie erst geschlichen,", "tokens": ["Lei\u00b7se", "kam", "sie", "erst", "ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch nun schreit sie ihm in's Ohr,", "tokens": ["Doch", "nun", "schreit", "sie", "ihm", "in's", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Schlaf ist nicht gewichen,", "tokens": ["Und", "der", "Schlaf", "ist", "nicht", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es ist ein verschlo\u00dfnes Thor,", "tokens": ["Es", "ist", "ein", "ver\u00b7schlo\u00df\u00b7nes", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Und sie nimmt die B\u00fccher alle,", "tokens": ["Und", "sie", "nimmt", "die", "B\u00fc\u00b7cher", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ihn magisch tief versenkt,", "tokens": ["Die", "ihn", "ma\u00b7gisch", "tief", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat die m\u00e4chtgen Geister alle", "tokens": ["Hat", "die", "m\u00e4cht\u00b7gen", "Geis\u00b7ter", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In des Ofens Gluth gesenkt.", "tokens": ["In", "des", "O\u00b7fens", "Gluth", "ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und der Ofen wollt sich wundern,", "tokens": ["Und", "der", "O\u00b7fen", "wollt", "sich", "wun\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcttelt mit dem alten Kopf,", "tokens": ["Sch\u00fct\u00b7telt", "mit", "dem", "al\u00b7ten", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus allen alten Plundern", "tokens": ["Und", "aus", "al\u00b7len", "al\u00b7ten", "Plun\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stieg so mancher gr\u00fcne Knopf;", "tokens": ["Stieg", "so", "man\u00b7cher", "gr\u00fc\u00b7ne", "Knopf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcst im Kopfe, wild zum Schelten,", "tokens": ["W\u00fcst", "im", "Kop\u00b7fe", ",", "wild", "zum", "Schel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wacht er auf und schaut sie an,", "tokens": ["Wacht", "er", "auf", "und", "schaut", "sie", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die gern Alles will entgelten,", "tokens": ["Die", "gern", "Al\u00b7les", "will", "ent\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn sie ihn nur retten kann.", "tokens": ["Wenn", "sie", "ihn", "nur", "ret\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber er mit wilden Tritten", "tokens": ["A\u00b7ber", "er", "mit", "wil\u00b7den", "Trit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00f6\u00dfet Liebchen an die Erd,", "tokens": ["St\u00f6\u00b7\u00dfet", "Lieb\u00b7chen", "an", "die", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6ret nicht auf ihre Bitten,", "tokens": ["H\u00f6\u00b7ret", "nicht", "auf", "ih\u00b7re", "Bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht die Gluth nur auf dem Heerd:", "tokens": ["Sieht", "die", "Gluth", "nur", "auf", "dem", "Heerd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbo ihr Zeichen, ihr verbrennet,", "tokens": ["\u00bb", "o", "ihr", "Zei\u00b7chen", ",", "ihr", "ver\u00b7bren\u00b7net", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun ihr sie mir zugef\u00fchrt,", "tokens": ["Nun", "ihr", "sie", "mir", "zu\u00b7ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach woran wird nun erkennet", "tokens": ["Ach", "wo\u00b7ran", "wird", "nun", "er\u00b7ken\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PWAV", "VAFIN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ob die rechte ich ersp\u00fcrt!\u00ab", "tokens": ["Ob", "die", "rech\u00b7te", "ich", "er\u00b7sp\u00fcrt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ART", "ADJA", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "W\u00e4rst Du M\u00e4dchen mir ganz eigen,", "tokens": ["W\u00e4rst", "Du", "M\u00e4d\u00b7chen", "mir", "ganz", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein M\u00e4dchen lieben mu\u00df,", "tokens": ["Wie", "ein", "M\u00e4d\u00b7chen", "lie\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ganz geduldig Dich zu zeigen", "tokens": ["Ganz", "ge\u00b7dul\u00b7dig", "Dich", "zu", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r gewesen Dein Genu\u00df;", "tokens": ["W\u00e4r", "ge\u00b7we\u00b7sen", "Dein", "Ge\u00b7nu\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VAPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00e4r ich M\u00e4dchen Dir ganz eigen,", "tokens": ["W\u00e4r", "ich", "M\u00e4d\u00b7chen", "Dir", "ganz", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmer zweifelte ich mehr,", "tokens": ["Nim\u00b7mer", "zwei\u00b7fel\u00b7te", "ich", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sondern m\u00fc\u00dft die Kniee beugen,", "tokens": ["Son\u00b7dern", "m\u00fc\u00dft", "die", "Kni\u00b7ee", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und mein Herz w\u00e4r mir nicht schwer.", "tokens": ["Und", "mein", "Herz", "w\u00e4r", "mir", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Herrschen nicht und auch nicht dienen,", "tokens": ["Herr\u00b7schen", "nicht", "und", "auch", "nicht", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KON", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zweifel war mein Weltgeschick,", "tokens": ["Zwei\u00b7fel", "war", "mein", "Welt\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur beschw\u00f6ren, nicht verdienen", "tokens": ["Nur", "be\u00b7schw\u00f6\u00b7ren", ",", "nicht", "ver\u00b7die\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4\u00dft sich jedes G\u00f6ttergl\u00fcck:", "tokens": ["L\u00e4\u00dft", "sich", "je\u00b7des", "G\u00f6t\u00b7ter\u00b7gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weiber-Vorwitz, wer beschw\u00f6rt dich,", "tokens": ["Wei\u00b7ber\u00b7Vor\u00b7witz", ",", "wer", "be\u00b7schw\u00f6rt", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da es selbst nicht Lieben kann,", "tokens": ["Da", "es", "selbst", "nicht", "Lie\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADJA", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn die Liebste selbst, sie st\u00f6rt mich,", "tokens": ["Denn", "die", "Liebs\u00b7te", "selbst", ",", "sie", "st\u00f6rt", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Da ich war in ihrem Bann.", "tokens": ["Da", "ich", "war", "in", "ih\u00b7rem", "Bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Flammenruh nach Weisheit streben", "tokens": ["Flam\u00b7men\u00b7ruh", "nach", "Weis\u00b7heit", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Senkt den J\u00fcnger tief in Schlaf,", "tokens": ["Senkt", "den", "J\u00fcn\u00b7ger", "tief", "in", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es gl\u00fcht sein innres Leben,", "tokens": ["Und", "es", "gl\u00fcht", "sein", "inn\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wenn Blitz die Tanne traf.", "tokens": ["Als", "wenn", "Blitz", "die", "Tan\u00b7ne", "traf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Festlich statt der schwarzen Krone", "tokens": ["Fest\u00b7lich", "statt", "der", "schwar\u00b7zen", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4gt sie einen Flammenkranz,", "tokens": ["Tr\u00e4gt", "sie", "ei\u00b7nen", "Flam\u00b7men\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weihrauch tr\u00e4ufelt von dem Throne,", "tokens": ["Weih\u00b7rauch", "tr\u00e4u\u00b7felt", "von", "dem", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Halme wirbeln rings im Tanz.", "tokens": ["Hal\u00b7me", "wir\u00b7beln", "rings", "im", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sonst da dr\u00e4uten ihm die Bilder,", "tokens": ["Sonst", "da", "dr\u00e4u\u00b7ten", "ihm", "die", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schrecklich roth und blau gemalt,", "tokens": ["Schreck\u00b7lich", "roth", "und", "blau", "ge\u00b7malt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Zeichen noch viel wilder", "tokens": ["Und", "die", "Zei\u00b7chen", "noch", "viel", "wil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "PIAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Thier in Flaschen schalt;", "tokens": ["Und", "das", "Thier", "in", "Fla\u00b7schen", "schalt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An den tausend Messingscheiben,", "tokens": ["An", "den", "tau\u00b7send", "Mes\u00b7sing\u00b7schei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo das Blei am Faden h\u00e4ngt,", "tokens": ["Wo", "das", "Blei", "am", "Fa\u00b7den", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00dft er sich erst m\u00fcde treiben,", "tokens": ["Mu\u00dft", "er", "sich", "erst", "m\u00fc\u00b7de", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eh der Schlaf ihn s\u00fc\u00df umf\u00e4ngt.", "tokens": ["Eh", "der", "Schlaf", "ihn", "s\u00fc\u00df", "um\u00b7f\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Liebchen kommt nun ihn zu k\u00fcssen,", "tokens": ["Lieb\u00b7chen", "kommt", "nun", "ihn", "zu", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber er vernimmt sie nicht,", "tokens": ["A\u00b7ber", "er", "ver\u00b7nimmt", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Himmlisch mild die Sterne gr\u00fc\u00dfen", "tokens": ["Himm\u00b7lisch", "mild", "die", "Ster\u00b7ne", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er steht in vollem Licht,", "tokens": ["Und", "er", "steht", "in", "vol\u00b7lem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und sie setzt sich ihm zu F\u00fc\u00dfen", "tokens": ["Und", "sie", "setzt", "sich", "ihm", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und umfasset seine Knie,", "tokens": ["Und", "um\u00b7fas\u00b7set", "sei\u00b7ne", "Knie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sollt sie ihn nicht wecken m\u00fcssen,", "tokens": ["Sollt", "sie", "ihn", "nicht", "we\u00b7cken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Er erwachet sonst wohl nie.", "tokens": ["Er", "er\u00b7wa\u00b7chet", "sonst", "wohl", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Leise kam sie erst geschlichen,", "tokens": ["Lei\u00b7se", "kam", "sie", "erst", "ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch nun schreit sie ihm in's Ohr,", "tokens": ["Doch", "nun", "schreit", "sie", "ihm", "in's", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Schlaf ist nicht gewichen,", "tokens": ["Und", "der", "Schlaf", "ist", "nicht", "ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es ist ein verschlo\u00dfnes Thor,", "tokens": ["Es", "ist", "ein", "ver\u00b7schlo\u00df\u00b7nes", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Und sie nimmt die B\u00fccher alle,", "tokens": ["Und", "sie", "nimmt", "die", "B\u00fc\u00b7cher", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ihn magisch tief versenkt,", "tokens": ["Die", "ihn", "ma\u00b7gisch", "tief", "ver\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat die m\u00e4chtgen Geister alle", "tokens": ["Hat", "die", "m\u00e4cht\u00b7gen", "Geis\u00b7ter", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In des Ofens Gluth gesenkt.", "tokens": ["In", "des", "O\u00b7fens", "Gluth", "ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Und der Ofen wollt sich wundern,", "tokens": ["Und", "der", "O\u00b7fen", "wollt", "sich", "wun\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcttelt mit dem alten Kopf,", "tokens": ["Sch\u00fct\u00b7telt", "mit", "dem", "al\u00b7ten", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und aus allen alten Plundern", "tokens": ["Und", "aus", "al\u00b7len", "al\u00b7ten", "Plun\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stieg so mancher gr\u00fcne Knopf;", "tokens": ["Stieg", "so", "man\u00b7cher", "gr\u00fc\u00b7ne", "Knopf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "W\u00fcst im Kopfe, wild zum Schelten,", "tokens": ["W\u00fcst", "im", "Kop\u00b7fe", ",", "wild", "zum", "Schel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wacht er auf und schaut sie an,", "tokens": ["Wacht", "er", "auf", "und", "schaut", "sie", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die gern Alles will entgelten,", "tokens": ["Die", "gern", "Al\u00b7les", "will", "ent\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn sie ihn nur retten kann.", "tokens": ["Wenn", "sie", "ihn", "nur", "ret\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Aber er mit wilden Tritten", "tokens": ["A\u00b7ber", "er", "mit", "wil\u00b7den", "Trit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00f6\u00dfet Liebchen an die Erd,", "tokens": ["St\u00f6\u00b7\u00dfet", "Lieb\u00b7chen", "an", "die", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6ret nicht auf ihre Bitten,", "tokens": ["H\u00f6\u00b7ret", "nicht", "auf", "ih\u00b7re", "Bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht die Gluth nur auf dem Heerd:", "tokens": ["Sieht", "die", "Gluth", "nur", "auf", "dem", "Heerd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbo ihr 