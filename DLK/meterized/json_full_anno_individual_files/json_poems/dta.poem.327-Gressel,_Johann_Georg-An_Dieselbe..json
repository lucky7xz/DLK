{"dta.poem.327": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An Dieselbe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nster Engel schau doch die Seelen-Pein/", "tokens": ["Sch\u00f6ns\u00b7ter", "En\u00b7gel", "schau", "doch", "die", "See\u00b7len\u00b7Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und lasse den Schmertz geendiget seyn;", "tokens": ["Und", "las\u00b7se", "den", "Schmertz", "ge\u00b7en\u00b7di\u00b7get", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Linder die Plagen/", "tokens": ["Lin\u00b7der", "die", "Pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die ich ertragen/", "tokens": ["Die", "ich", "er\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Ohn sie zu klagen", "tokens": ["Ohn", "sie", "zu", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dir G\u00f6ttin der Brust;", "tokens": ["Dir", "G\u00f6t\u00b7tin", "der", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Die du suchst Lust", "tokens": ["Die", "du", "suchst", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Wenn nur bewust", "tokens": ["Wenn", "nur", "be\u00b7wust"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Schwere Plagen der krancken Brust.", "tokens": ["Schwe\u00b7re", "Pla\u00b7gen", "der", "kran\u00b7cken", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Mache Sch\u00f6nste mein Hertze bald gesund/", "tokens": ["Ma\u00b7che", "Sch\u00f6ns\u00b7te", "mein", "Hert\u00b7ze", "bald", "ge\u00b7sund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "VVFIN", "ADV", "ADJD", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und reich mir deinen bezuckerten Mund;", "tokens": ["Und", "reich", "mir", "dei\u00b7nen", "be\u00b7zu\u00b7cker\u00b7ten", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df mir geniessen", "tokens": ["La\u00df", "mir", "ge\u00b7nies\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In s\u00fcssen k\u00fcssen", "tokens": ["In", "s\u00fcs\u00b7sen", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der Liebe-Bissen", "tokens": ["Der", "Lie\u00b7be\u00b7Bis\u00b7sen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Da\u00df endlich mein Hertz", "tokens": ["Da\u00df", "end\u00b7lich", "mein", "Hertz"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Nach Angst und Schmertz", "tokens": ["Nach", "Angst", "und", "Schmertz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Schmeck s\u00fcssen Schertz/", "tokens": ["Schmeck", "s\u00fcs\u00b7sen", "Schertz", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Lieb\u2019 und k\u00fcsse mich doch mein Hertz.", "tokens": ["Lieb'", "und", "k\u00fcs\u00b7se", "mich", "doch", "mein", "Hertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6ne Wangen die ihr voll Rosen steht", "tokens": ["Sch\u00f6\u00b7ne", "Wan\u00b7gen", "die", "ihr", "voll", "Ro\u00b7sen", "steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "PPOSAT", "ADJD", "NN", "VVFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Um die ein Krantz von weissen Liljen geht/", "tokens": ["Um", "die", "ein", "Krantz", "von", "weis\u00b7sen", "Lil\u00b7jen", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jhr Wunder-Br\u00fcste/", "tokens": ["Ihr", "Wun\u00b7der\u00b7Br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Du Prunck-Ger\u00fcste", "tokens": ["Du", "Prun\u00b7ck\u00b7Ge\u00b7r\u00fcs\u00b7te"], "token_info": ["word", "word"], "pos": ["PPER", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Voll Himmels-L\u00fcste!", "tokens": ["Voll", "Him\u00b7mels\u00b7L\u00fcs\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ach lachet mich an/", "tokens": ["Ach", "la\u00b7chet", "mich", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da\u00df ich bald kan", "tokens": ["Da\u00df", "ich", "bald", "kan"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die Freuden-Bahn", "tokens": ["Die", "Freu\u00b7den\u00b7Bahn"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Bey euch treten mit Wollust an.", "tokens": ["Bey", "euch", "tre\u00b7ten", "mit", "Wol\u00b7lust", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "K\u00fcsse Sch\u00f6nste doch meinen krancken Mund", "tokens": ["K\u00fcs\u00b7se", "Sch\u00f6ns\u00b7te", "doch", "mei\u00b7nen", "kran\u00b7cken", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und heile im Kusse was du verwundt/", "tokens": ["Und", "hei\u00b7le", "im", "Kus\u00b7se", "was", "du", "ver\u00b7wundt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "APPRART", "NN", "PWS", "PPER", "VVPP", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Du kanst nur eben", "tokens": ["Du", "kanst", "nur", "e\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem matten Leben", "tokens": ["Dem", "mat\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Die Lindrung geben.", "tokens": ["Die", "Lind\u00b7rung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ich werde begl\u00fcckt/", "tokens": ["Ich", "wer\u00b7de", "be\u00b7gl\u00fcckt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und recht erquickt/", "tokens": ["Und", "recht", "er\u00b7quickt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ja gar entz\u00fcckt", "tokens": ["Ja", "gar", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "ADV", "VVPP"], "meter": "---+", "measure": "unknown.measure.single"}, "line.9": {"text": "Wenn mich Sch\u00f6nste dein Hertze begl\u00fcckt.", "tokens": ["Wenn", "mich", "Sch\u00f6ns\u00b7te", "dein", "Hert\u00b7ze", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "VVFIN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}