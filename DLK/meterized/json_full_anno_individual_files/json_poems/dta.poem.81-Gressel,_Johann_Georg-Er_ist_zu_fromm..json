{"dta.poem.81": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er ist zu fromm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin zu fromm/ zu fromm beym Frauen-Zimmer/", "tokens": ["Ich", "bin", "zu", "fromm", "/", "zu", "fromm", "beym", "Frau\u00b7en\u00b7Zim\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$(", "PTKA", "ADJD", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man klagt mich stets als allzufurchtsam an/", "tokens": ["Man", "klagt", "mich", "stets", "als", "all\u00b7zu\u00b7furcht\u00b7sam", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "KOUS", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Jungfern-Volck/ das saget je und immer/", "tokens": ["Das", "Jung\u00b7fern\u00b7Volck", "/", "das", "sa\u00b7get", "je", "und", "im\u00b7mer", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "VVFIN", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit meiner Gunst w\u00e4r ihnen nichts gethan.", "tokens": ["Mit", "mei\u00b7ner", "Gunst", "w\u00e4r", "ih\u00b7nen", "nichts", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich ehrte zwar das J\u00fcngferlich Geschlechte/", "tokens": ["Ich", "ehr\u00b7te", "zwar", "das", "J\u00fcng\u00b7fer\u00b7lich", "Ge\u00b7schlech\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und meine Pflicht/ die tr\u00fcge reichlich ein;", "tokens": ["Und", "mei\u00b7ne", "Pflicht", "/", "die", "tr\u00fc\u00b7ge", "reich\u00b7lich", "ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "ART", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch hielt ich mich nicht nach der ", "tokens": ["Doch", "hielt", "ich", "mich", "nicht", "nach", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und meine Hand wolt niemahls dreifte seyn.", "tokens": ["Und", "mei\u00b7ne", "Hand", "wolt", "nie\u00b7mahls", "dreif\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ADV", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die Finger sich nicht zu den Br\u00fcsten machten/", "tokens": ["Die", "Fin\u00b7ger", "sich", "nicht", "zu", "den", "Br\u00fcs\u00b7ten", "mach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Auch k\u00e4hm\u2019 ich nicht vor das Gelobte-Land.", "tokens": ["Auch", "k\u00e4hm'", "ich", "nicht", "vor", "das", "Ge\u00b7lob\u00b7te\u00b7Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Ich lie\u00df den Mund bey s\u00fcsser Kost verschmachten/", "tokens": ["Ich", "lie\u00df", "den", "Mund", "bey", "s\u00fcs\u00b7ser", "Kost", "ver\u00b7schmach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und w\u00e4re nur den Nahmen nach bekandt.", "tokens": ["Und", "w\u00e4\u00b7re", "nur", "den", "Nah\u00b7men", "nach", "be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mein Gut-seyn lie\u00df mich ihre Gunst verschertzen/", "tokens": ["Mein", "Gut\u00b7seyn", "lie\u00df", "mich", "ih\u00b7re", "Gunst", "ver\u00b7schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ich machte selbst mein Gl\u00fcck zur\u00fccke gehn/", "tokens": ["Ich", "mach\u00b7te", "selbst", "mein", "Gl\u00fcck", "zu\u00b7r\u00fc\u00b7cke", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Nicht das Geschenck/ das Tasten und das Hertzen/", "tokens": ["Nicht", "das", "Ge\u00b7schenck", "/", "das", "Tas\u00b7ten", "und", "das", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$(", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Das liesse nur vor einen Mann bestehn.", "tokens": ["Das", "lies\u00b7se", "nur", "vor", "ei\u00b7nen", "Mann", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die Klage mu\u00df ich aller Orten h\u00f6ren/", "tokens": ["Die", "Kla\u00b7ge", "mu\u00df", "ich", "al\u00b7ler", "Or\u00b7ten", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Sie hassen mich ob meiner G\u00fctigkeit;", "tokens": ["Sie", "has\u00b7sen", "mich", "ob", "mei\u00b7ner", "G\u00fc\u00b7tig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Doch kan ich mich an ihrem Wunsch nicht kehren/", "tokens": ["Doch", "kan", "ich", "mich", "an", "ih\u00b7rem", "Wunsch", "nicht", "keh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Macht gleich mein Fleisch mir unterweilen Streit.", "tokens": ["Macht", "gleich", "mein", "Fleisch", "mir", "un\u00b7ter\u00b7wei\u00b7len", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Ich will stets keusch in meinem Wesem bleiben/", "tokens": ["Ich", "will", "stets", "keusch", "in", "mei\u00b7nem", "We\u00b7sem", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Bannt man mich gleich zum Thor und Stadt hinaus;", "tokens": ["Bannt", "man", "mich", "gleich", "zum", "Thor", "und", "Stadt", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "APPRART", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ich wil mich nicht an ihre Dinger reiben/", "tokens": ["Ich", "wil", "mich", "nicht", "an", "ih\u00b7re", "Din\u00b7ger", "rei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "So sto\u00df\u2019 ich nicht die Fenster-Scheiben aus.", "tokens": ["So", "sto\u00df'", "ich", "nicht", "die", "Fens\u00b7ter\u00b7Schei\u00b7ben", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}