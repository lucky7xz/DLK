{"textgrid.poem.47037": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du ziehst, nicht sag' ich's, zum wievielten Male,", "tokens": ["Du", "ziehst", ",", "nicht", "sag'", "ich's", ",", "zum", "wie\u00b7viel\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKNEG", "VVFIN", "PIS", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Mond, am Himmel deine alten Kreise,", "tokens": ["O", "Mond", ",", "am", "Him\u00b7mel", "dei\u00b7ne", "al\u00b7ten", "Krei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Derweil mich selber hier im alten Gleise", "tokens": ["Der\u00b7weil", "mich", "sel\u00b7ber", "hier", "im", "al\u00b7ten", "Glei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du ziehen siehst durch diese s\u00fc\u00dfen Thale.", "tokens": ["Du", "zie\u00b7hen", "siehst", "durch", "die\u00b7se", "s\u00fc\u00b7\u00dfen", "Tha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Fenster aber dort, das blinkt, das schmale,", "tokens": ["Das", "Fens\u00b7ter", "a\u00b7ber", "dort", ",", "das", "blinkt", ",", "das", "schma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "PDS", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist noch vergittert nach der alten Weise;", "tokens": ["Ist", "noch", "ver\u00b7git\u00b7tert", "nach", "der", "al\u00b7ten", "Wei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kannst du, Freund, die Gitter mir nicht leise", "tokens": ["Und", "kannst", "du", ",", "Freund", ",", "die", "Git\u00b7ter", "mir", "nicht", "lei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "$,", "NN", "$,", "ART", "NN", "PPER", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zerbrechen, ach, mit einem deiner Strahle?", "tokens": ["Zer\u00b7bre\u00b7chen", ",", "ach", ",", "mit", "ei\u00b7nem", "dei\u00b7ner", "Strah\u00b7le", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "APPR", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kannst du, wie ohne Widerstand die Scheiben", "tokens": ["Kannst", "du", ",", "wie", "oh\u00b7ne", "Wi\u00b7der\u00b7stand", "die", "Schei\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du selbst durchdringst, nicht mich auch werden lassen,", "tokens": ["Du", "selbst", "durch\u00b7dringst", ",", "nicht", "mich", "auch", "wer\u00b7den", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PTKNEG", "PPER", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hineinzudringen, ganz in Licht zergangen?", "tokens": ["Hin\u00b7ein\u00b7zu\u00b7drin\u00b7gen", ",", "ganz", "in", "Licht", "zer\u00b7gan\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Umsonst! ich mu\u00df am dunklen Boden bleiben;", "tokens": ["Um\u00b7sonst", "!", "ich", "mu\u00df", "am", "dunk\u00b7len", "Bo\u00b7den", "blei\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VMFIN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du gehst allein, Freund, Feind, den ich mu\u00df hassen,", "tokens": ["Du", "gehst", "al\u00b7lein", ",", "Freund", ",", "Feind", ",", "den", "ich", "mu\u00df", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "NN", "$,", "NN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hin, wo du bleich willst ruhn auf roten Wangen.", "tokens": ["Hin", ",", "wo", "du", "bleich", "willst", "ruhn", "auf", "ro\u00b7ten", "Wan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "ADJD", "VMFIN", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {