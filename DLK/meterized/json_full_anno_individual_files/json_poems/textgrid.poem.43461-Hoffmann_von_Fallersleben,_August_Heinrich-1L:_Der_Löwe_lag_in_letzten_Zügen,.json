{"textgrid.poem.43461": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der L\u00f6we lag in letzten Z\u00fcgen,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der L\u00f6we lag in letzten Z\u00fcgen,", "tokens": ["Der", "L\u00f6\u00b7we", "lag", "in", "letz\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halb starr und ganz erbla\u00dft,", "tokens": ["Halb", "starr", "und", "ganz", "er\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ward ihm jedes Volksvergn\u00fcgen", "tokens": ["Da", "ward", "ihm", "je\u00b7des", "Volks\u00b7ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis in den Tod verha\u00dft.", "tokens": ["Bis", "in", "den", "Tod", "ver\u00b7ha\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Still soll es sein in meinem Reiche,", "tokens": ["Still", "soll", "es", "sein", "in", "mei\u00b7nem", "Rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "VAINF", "APPR", "PPOSAT", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Sang und kein Geschrei!", "tokens": ["Kein", "Sang", "und", "kein", "Ge\u00b7schrei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du, Storch, du \u00fcb' an jenem Teiche", "tokens": ["Du", ",", "Storch", ",", "du", "\u00fcb'", "an", "je\u00b7nem", "Tei\u00b7che"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir strenge Polizei! \u2013", "tokens": ["Mir", "stren\u00b7ge", "Po\u00b7li\u00b7zei", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da ist der Storch hinausgegangen", "tokens": ["Da", "ist", "der", "Storch", "hin\u00b7aus\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz still in Schilf und Ried,", "tokens": ["Ganz", "still", "in", "Schilf", "und", "Ried", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat gleich einen Frosch gefangen,", "tokens": ["Und", "hat", "gleich", "ei\u00b7nen", "Frosch", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der eben sang sein Lied.", "tokens": ["Der", "e\u00b7ben", "sang", "sein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbich \u00fcb' hier offen und in Stillen", "tokens": ["\u00bb", "ich", "\u00fcb'", "hier", "of\u00b7fen", "und", "in", "Stil\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ADV", "ADJD", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des K\u00f6nigs Polizei;", "tokens": ["Des", "K\u00f6\u00b7nigs", "Po\u00b7li\u00b7zei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Singen ist nach ", "tokens": ["Das", "Sin\u00b7gen", "ist", "nach"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Sonst aber niemals frei.\u00ab", "tokens": ["Sonst", "a\u00b7ber", "nie\u00b7mals", "frei", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbdu hast bei Sonn- und Mondenscheine,", "tokens": ["\u00bb", "du", "hast", "bei", "Sonn", "und", "Mon\u00b7den\u00b7schei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "TRUNC", "KON", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sogar in finstrer Nacht", "tokens": ["So\u00b7gar", "in", "finst\u00b7rer", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Volk der Fr\u00f6sche, Gro\u00df' und Kleine,", "tokens": ["Das", "Volk", "der", "Fr\u00f6\u00b7sche", ",", "Gro\u00df'", "und", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Aufruhr angefacht.\u00ab", "tokens": ["Zum", "Auf\u00b7ruhr", "an\u00b7ge\u00b7facht", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das wu\u00dft ich nicht, da\u00df auch mein Singen", "tokens": ["Das", "wu\u00dft", "ich", "nicht", ",", "da\u00df", "auch", "mein", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steht unter Polizei;", "tokens": ["Steht", "un\u00b7ter", "Po\u00b7li\u00b7zei", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich dachte mir, von allen Dingen", "tokens": ["Ich", "dach\u00b7te", "mir", ",", "von", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei noch das Singen frei.", "tokens": ["Sei", "noch", "das", "Sin\u00b7gen", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbihr Fr\u00f6sche seid in euren Teichen", "tokens": ["\u00bb", "ihr", "Fr\u00f6\u00b7sche", "seid", "in", "eu\u00b7ren", "Tei\u00b7chen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie jede Creatur,", "tokens": ["Wie", "je\u00b7de", "Crea\u00b7tur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ihr alle, du und deines Gleichen", "tokens": ["Ihr", "al\u00b7le", ",", "du", "und", "dei\u00b7nes", "Glei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "$,", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seid Unterthanen nur.\u00ab", "tokens": ["Seid", "Un\u00b7ter\u00b7tha\u00b7nen", "nur", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbund was die Unterthanen treiben,", "tokens": ["\u00bb", "und", "was", "die", "Un\u00b7ter\u00b7tha\u00b7nen", "trei\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PWS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steht unter der Censur:", "tokens": ["Steht", "un\u00b7ter", "der", "Cen\u00b7sur", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr d\u00fcrft nicht singen, schrei'n und schreiben", "tokens": ["Ihr", "d\u00fcrft", "nicht", "sin\u00b7gen", ",", "schrei'n", "und", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "VVFIN", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als mit Erlaubni\u00df nur.\u00ab", "tokens": ["Als", "mit", "Er\u00b7laub\u00b7ni\u00df", "nur", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "APPR", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Frosch der wollte widersprechen,", "tokens": ["Der", "Frosch", "der", "woll\u00b7te", "wi\u00b7der\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch half's ihm weiter nicht.", "tokens": ["Doch", "ha\u00b7lf's", "ihm", "wei\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Storch bestrafte sein Verbrechen,", "tokens": ["Der", "Storch", "be\u00b7straf\u00b7te", "sein", "Ver\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und fra\u00df den B\u00f6sewicht.", "tokens": ["Und", "fra\u00df", "den", "B\u00f6\u00b7se\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wo einer von den Froschgenossen", "tokens": ["Wo", "ei\u00b7ner", "von", "den", "Froschge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Noch je ein Liedlein sang,", "tokens": ["Noch", "je", "ein", "Lied\u00b7lein", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam der Storch daher geschossen,", "tokens": ["Da", "kam", "der", "Storch", "da\u00b7her", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der ihn sofort verschlang.", "tokens": ["Der", "ihn", "so\u00b7fort", "ver\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der K\u00f6nig lag in letzten Z\u00fcgen", "tokens": ["Der", "K\u00f6\u00b7nig", "lag", "in", "letz\u00b7ten", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und starb in schwerer Noth.", "tokens": ["Und", "starb", "in", "schwe\u00b7rer", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Storch verdarb das Volksvergn\u00fcgen,", "tokens": ["Der", "Storch", "ver\u00b7darb", "das", "Volks\u00b7ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch blieb der K\u00f6nig todt.", "tokens": ["Doch", "blieb", "der", "K\u00f6\u00b7nig", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der L\u00f6we lag in letzten Z\u00fcgen,", "tokens": ["Der", "L\u00f6\u00b7we", "lag", "in", "letz\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "w