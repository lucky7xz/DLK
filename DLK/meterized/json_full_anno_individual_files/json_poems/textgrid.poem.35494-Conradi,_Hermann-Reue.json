{"textgrid.poem.35494": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Reue", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habe die Nacht verzecht und verpra\u00dft,", "tokens": ["Ha\u00b7be", "die", "Nacht", "ver\u00b7zecht", "und", "ver\u00b7pra\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sa\u00df bei \u00fcppigen Dirnen zu Gast,", "tokens": ["Sa\u00df", "bei", "\u00fcp\u00b7pi\u00b7gen", "Dir\u00b7nen", "zu", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Trank mit Gesellen, mit losen ...", "tokens": ["Trank", "mit", "Ge\u00b7sel\u00b7len", ",", "mit", "lo\u00b7sen", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPR", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schreit' nun hinaus in den jungen Tag \u2013 \u2013", "tokens": ["Schreit'", "nun", "hin\u00b7aus", "in", "den", "jun\u00b7gen", "Tag", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "APZR", "APPR", "ART", "ADJA", "NN", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Kaum der Finsternis Thron zerbrach \u2013", "tokens": ["Kaum", "der", "Fins\u00b7ter\u00b7nis", "Thron", "zer\u00b7brach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Nach dem bacchantischen Lustgelag", "tokens": ["Nach", "dem", "bac\u00b7chan\u00b7ti\u00b7schen", "Lust\u00b7ge\u00b7lag"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Gr\u00fc\u00df' ich des Morgenrots Rosen!", "tokens": ["Gr\u00fc\u00df'", "ich", "des", "Mor\u00b7gen\u00b7rots", "Ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Gr\u00fc\u00dfe des Morgenrots Flammenschein,", "tokens": ["Gr\u00fc\u00b7\u00dfe", "des", "Mor\u00b7gen\u00b7rots", "Flam\u00b7men\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Sauge den k\u00fchlenden Fr\u00fchwind ein,", "tokens": ["Sau\u00b7ge", "den", "k\u00fch\u00b7len\u00b7den", "Fr\u00fch\u00b7wind", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und die Schatten zerrinnen,", "tokens": ["Und", "die", "Schat\u00b7ten", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die ich geschaut mit taumelndem Hirn, \u2013", "tokens": ["Die", "ich", "ge\u00b7schaut", "mit", "tau\u00b7meln\u00b7dem", "Hirn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die ich getr\u00e4umt mit brennen