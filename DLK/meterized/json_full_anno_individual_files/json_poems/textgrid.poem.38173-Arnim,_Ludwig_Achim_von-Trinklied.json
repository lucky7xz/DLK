{"textgrid.poem.38173": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der edle Wein", "tokens": ["Der", "ed\u00b7le", "Wein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ist doch der beste Schieferdecker,", "tokens": ["Ist", "doch", "der", "bes\u00b7te", "Schie\u00b7fer\u00b7de\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein schiefer Schein", "tokens": ["Sein", "schie\u00b7fer", "Schein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Macht alle Menschen etwas kecker,", "tokens": ["Macht", "al\u00b7le", "Men\u00b7schen", "et\u00b7was", "ke\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ich wundre mich,", "tokens": ["Ich", "wund\u00b7re", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Da\u00df er so klettern kann und steigen,", "tokens": ["Da\u00df", "er", "so", "klet\u00b7tern", "kann", "und", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und macht da\u00df sich", "tokens": ["Und", "macht", "da\u00df", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOUS", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Die gro\u00dfen H\u00e4upter vor ihm neigen.", "tokens": ["Die", "gro\u00b7\u00dfen", "H\u00e4up\u00b7ter", "vor", "ihm", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der muntre Trank", "tokens": ["Der", "mun\u00b7tre", "Trank"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Kann ohne Leiter weiter kommen,", "tokens": ["Kann", "oh\u00b7ne", "Lei\u00b7ter", "wei\u00b7ter", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er so blank", "tokens": ["Wenn", "er", "so", "blank"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Die Stirnenburg hat eingenommen,", "tokens": ["Die", "Stir\u00b7nen\u00b7burg", "hat", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als mancher, der", "tokens": ["Als", "man\u00b7cher", ",", "der"], "token_info": ["word", "word", "punct", "word"], "pos": ["KOUS", "PIS", "$,", "PRELS"], "meter": "-+--", "measure": "dactylic.init"}, "line.6": {"text": "Mit H\u00fclfe sich hinan will bringen,", "tokens": ["Mit", "H\u00fcl\u00b7fe", "sich", "hi\u00b7nan", "will", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "APZR", "VMFIN", "VVINF", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und ohn Gefahr", "tokens": ["Und", "ohn", "Ge\u00b7fahr"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "++-+", "measure": "iambic.di"}, "line.8": {"text": "Die H\u00e4lfte noch wei\u00df zu erzwingen.", "tokens": ["Die", "H\u00e4lf\u00b7te", "noch", "wei\u00df", "zu", "er\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Drum bleibts dabey,", "tokens": ["Drum", "bleibts", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Er hegt ein recht verg\u00f6ttert Leben,", "tokens": ["Er", "hegt", "ein", "recht", "ver\u00b7g\u00f6t\u00b7tert", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Weil er so frey", "tokens": ["Weil", "er", "so", "frey"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Kann in die L\u00fcfte schweben.", "tokens": ["Kann", "in", "die", "L\u00fcf\u00b7te", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und wenn wir ihn", "tokens": ["Und", "wenn", "wir", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "In unsre hohlen H\u00e4lse lassen", "tokens": ["In", "uns\u00b7re", "hoh\u00b7len", "H\u00e4l\u00b7se", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mit Pracht einziehn,", "tokens": ["Mit", "Pracht", "ein\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Empfinden wir ihn gleichermassen.", "tokens": ["Emp\u00b7fin\u00b7den", "wir", "ihn", "glei\u00b7cher\u00b7mas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann manches Haus,", "tokens": ["Dann", "man\u00b7ches", "Haus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So schwer es sonst auf S\u00e4ulen stehet,", "tokens": ["So", "schwer", "es", "sonst", "auf", "S\u00e4u\u00b7len", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00e4hrt mit hinaus,", "tokens": ["F\u00e4hrt", "mit", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "APZR", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Es merket, da\u00df es leichter gehet,", "tokens": ["Es", "mer\u00b7ket", ",", "da\u00df", "es", "leich\u00b7ter", "ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sobald der Wein", "tokens": ["So\u00b7bald", "der", "Wein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Durch seine Pfort ist eingezogen,", "tokens": ["Durch", "sei\u00b7ne", "Pfort", "ist", "ein\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So stimmt es ein,", "tokens": ["So", "stimmt", "es", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und meint es sey schon hochgeflogen.", "tokens": ["Und", "meint", "es", "sey", "schon", "hoch\u00b7ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn dies geschicht,", "tokens": ["Wenn", "dies", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "So k\u00f6nnte doch kein Haus bestehen,", "tokens": ["So", "k\u00f6nn\u00b7te", "doch", "kein", "Haus", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Morpheus nicht,", "tokens": ["Wenn", "Mor\u00b7pheus", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Der Baukunst an die Hand zu gehen,", "tokens": ["Der", "Bau\u00b7kunst", "an", "die", "Hand", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vor andren w\u00e4r", "tokens": ["Vor", "an\u00b7dren", "w\u00e4r"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIS", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Erfahren und so weit gekommen,", "tokens": ["Er\u00b7fah\u00b7ren", "und", "so", "weit", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df ihm die Ehr", "tokens": ["Da\u00df", "ihm", "die", "Ehr"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Von Sterblichen noch nie genommen.", "tokens": ["Von", "Sterb\u00b7li\u00b7chen", "noch", "nie", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann wenn der Wein", "tokens": ["Dann", "wenn", "der", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Aufleget gar zu schwere D\u00e4cher,", "tokens": ["Auf\u00b7le\u00b7get", "gar", "zu", "schwe\u00b7re", "D\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So mu\u00df es seyn,", "tokens": ["So", "mu\u00df", "es", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Da\u00df sie beschweren die Gem\u00e4cher,", "tokens": ["Da\u00df", "sie", "be\u00b7schwe\u00b7ren", "die", "Ge\u00b7m\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Macht er Verdru\u00df,", "tokens": ["Macht", "er", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "So mag er Schieferdecker bleiben,", "tokens": ["So", "mag", "er", "Schie\u00b7fer\u00b7de\u00b7cker", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dach Morpheus mu\u00df", "tokens": ["Dach", "Mor\u00b7pheus", "mu\u00df"], "token_info": ["word", "word", "word"], "pos": ["PAV", "NE", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Den Bau erhalten und forttreiben.", "tokens": ["Den", "Bau", "er\u00b7hal\u00b7ten", "und", "fort\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der edle Wein", "tokens": ["Der", "ed\u00b7le", "Wein"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ist doch der beste Schieferdecker,", "tokens": ["Ist", "doch", "der", "bes\u00b7te", "Schie\u00b7fer\u00b7de\u00b7cker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sein schiefer Schein", "tokens": ["Sein", "schie\u00b7fer", "Schein"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"],