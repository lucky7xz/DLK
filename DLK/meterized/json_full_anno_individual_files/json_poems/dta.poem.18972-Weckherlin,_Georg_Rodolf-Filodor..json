{"dta.poem.18972": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Filodor.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Damit wir nu die Zeit", "tokens": ["Da\u00b7mit", "wir", "nu", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nach langem layd vnd streit", "tokens": ["Nach", "lan\u00b7gem", "layd", "vnd", "streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit lieb vnd lust vertreiben;", "tokens": ["Mit", "lieb", "vnd", "lust", "ver\u00b7trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So will ich/ Myrta/ schreiben", "tokens": ["So", "will", "ich", "/", "Myr\u00b7ta", "/", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "$(", "NE", "$(", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Zeugnus vnsrer beut:", "tokens": ["Die", "Zeug\u00b7nus", "vns\u00b7rer", "beut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "All mein layd ist gestorben/", "tokens": ["All", "mein", "layd", "ist", "ge\u00b7stor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Weil der Lieb s\u00fcssen lohn/", "tokens": ["Weil", "der", "Lieb", "s\u00fcs\u00b7sen", "lohn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die s\u00fcsse Myrten Cron/", "tokens": ["Die", "s\u00fcs\u00b7se", "Myr\u00b7ten", "Cron", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich Filodor erworben.", "tokens": ["Ich", "Fi\u00b7lo\u00b7dor", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}