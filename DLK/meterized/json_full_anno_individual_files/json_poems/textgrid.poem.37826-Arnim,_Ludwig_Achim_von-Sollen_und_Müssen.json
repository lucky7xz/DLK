{"textgrid.poem.37826": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Sollen und M\u00fcssen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich soll und mu\u00df ein Buhlen haben,", "tokens": ["Ich", "soll", "und", "mu\u00df", "ein", "Buh\u00b7len", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trabe dich Thierlein, trabe,", "tokens": ["Tra\u00b7be", "dich", "Thier\u00b7lein", ",", "tra\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und sollt ich ihn aus der Erde graben,", "tokens": ["Und", "sollt", "ich", "ihn", "aus", "der", "Er\u00b7de", "gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Trabe dich Thierlein, trabe.", "tokens": ["Tra\u00b7be", "dich", "Thier\u00b7lein", ",", "tra\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Das Murmelthierlein hilft mir nicht,", "tokens": ["Das", "Mur\u00b7melt\u00b7hier\u00b7lein", "hilft", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es hat ein m\u00fcrrisch Angesicht,", "tokens": ["Es", "hat", "ein", "m\u00fcr\u00b7risch", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und will fast immer schlafen.", "tokens": ["Und", "will", "fast", "im\u00b7mer", "schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich soll und mu\u00df ein Buhlen erringen,", "tokens": ["Ich", "soll", "und", "mu\u00df", "ein", "Buh\u00b7len", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schwinge dich Falke, schwing dich,", "tokens": ["Schwin\u00b7ge", "dich", "Fal\u00b7ke", ",", "schwing", "dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du sollst mir ihn aus den L\u00fcften bringen,", "tokens": ["Du", "sollst", "mir", "ihn", "aus", "den", "L\u00fcf\u00b7ten", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schwinge dich Falke, schwing dich.", "tokens": ["Schwin\u00b7ge", "dich", "Fal\u00b7ke", ",", "schwing", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Das Turtelt\u00e4ublein hilft mir nicht,", "tokens": ["Das", "Tur\u00b7tel\u00b7t\u00e4u\u00b7blein", "hilft", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schnurren und girren kann ich nicht,", "tokens": ["Schnur\u00b7ren", "und", "gir\u00b7ren", "kann", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "PPER", "PTKNEG", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sein Leben mu\u00df es lassen.", "tokens": ["Sein", "Le\u00b7ben", "mu\u00df", "es", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich soll und mu\u00df ein Buhlen finden,", "tokens": ["Ich", "soll", "und", "mu\u00df", "ein", "Buh\u00b7len", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Laufe mein H\u00fcndlein, laufe,", "tokens": ["Lau\u00b7fe", "mein", "H\u00fcnd\u00b7lein", ",", "lau\u00b7fe", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und sollt ich ihn fangen mit meinen Winden,", "tokens": ["Und", "sollt", "ich", "ihn", "fan\u00b7gen", "mit", "mei\u00b7nen", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Laufe mein H\u00fcndlein, laufe.", "tokens": ["Lau\u00b7fe", "mein", "H\u00fcnd\u00b7lein", ",", "lau\u00b7fe", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Der edle Hirsch er hilft mir nicht,", "tokens": ["Der", "ed\u00b7le", "Hirsch", "er", "hilft", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Horn ist mir zu hoch gericht,", "tokens": ["Sein", "Horn", "ist", "mir", "zu", "hoch", "ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er m\u00f6chte mich erstechen.", "tokens": ["Er", "m\u00f6ch\u00b7te", "mich", "er\u00b7ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich soll und mu\u00df ein Buhlen haben,", "tokens": ["Ich", "soll", "und", "mu\u00df", "ein", "Buh\u00b7len", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schalle mein H\u00f6rnlein, schalle,", "tokens": ["Schal\u00b7le", "mein", "H\u00f6rn\u00b7lein", ",", "schal\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und wen du rufst, der mu\u00df mich laben,", "tokens": ["Und", "wen", "du", "rufst", ",", "der", "mu\u00df", "mich", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "ART", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schalle mein H\u00f6rnlein, schalle.", "tokens": ["Schal\u00b7le", "mein", "H\u00f6rn\u00b7lein", ",", "schal\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Drey sch\u00f6ne Thierlein stellen sich,", "tokens": ["Drey", "sch\u00f6\u00b7ne", "Thier\u00b7lein", "stel\u00b7len", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die holt kein Hund, kein Falke nicht,", "tokens": ["Die", "holt", "kein", "Hund", ",", "kein", "Fal\u00b7ke", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mu\u00df ich selber fangen.", "tokens": ["Die", "mu\u00df", "ich", "sel\u00b7ber", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ich soll und mu\u00df ein R\u00f6\u00dflein haben,", "tokens": ["Ich", "soll", "und", "mu\u00df", "ein", "R\u00f6\u00df\u00b7lein", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm mich J\u00e4gerlein, nimm mich,", "tokens": ["Nimm", "mich", "J\u00e4\u00b7ger\u00b7lein", ",", "nimm", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "VVIMP", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich m\u00f6cht gern durch die W\u00e4lder traben,", "tokens": ["Ich", "m\u00f6cht", "gern", "durch", "die", "W\u00e4l\u00b7der", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm mich, J\u00e4gerlein nimm mich.", "tokens": ["Nimm", "mich", ",", "J\u00e4\u00b7ger\u00b7lein", "nimm", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "VVIMP", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Trabst du gern, so nimm mein Ro\u00df,", "tokens": ["Trabst", "du", "gern", ",", "so", "nimm", "mein", "Ro\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "ADV", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So w\u00e4r ich dann das El\u00dflein los,", "tokens": ["So", "w\u00e4r", "ich", "dann", "das", "El\u00df\u00b7lein", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ade, Ade, mein R\u00f6\u00dflein!", "tokens": ["A\u00b7de", ",", "A\u00b7de", ",", "mein", "R\u00f6\u00df\u00b7lein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich soll und mu\u00df ein Falken kriegen,", "tokens": ["Ich", "soll", "und", "mu\u00df", "ein", "Fal\u00b7ken", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimm mich, J\u00e4gerlein nimm mich,", "tokens": ["Nimm", "mich", ",", "J\u00e4\u00b7ger\u00b7lein", "nimm", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "VVIMP", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der mu\u00df mit mir zum Himmel fliegen,", "tokens": ["Der", "mu\u00df", "mit", "mir", "zum", "Him\u00b7mel", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nimm mich, J\u00e4gerlein, nimm mich.", "tokens": ["Nimm", "mich", ",", "J\u00e4\u00b7ger\u00b7lein", ",", "nimm", "mich", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "VVIMP", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nimm hin, nimm hin mein Federspiel,", "tokens": ["Nimm", "hin", ",", "nimm", "hin", "mein", "Fe\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVIMP", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Lieb B\u00e4rbelein du warst zu viel,", "tokens": ["Lieb", "B\u00e4r\u00b7be\u00b7lein", "du", "warst", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "VAFIN", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ade, Ade, mein Falke.", "tokens": ["A\u00b7de", ",", "A\u00b7de", ",", "mein", "Fal\u00b7ke", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.13": {"line.1": {"text": "Ich soll und mu\u00df ein K\u00fc\u00dflein haben,", "tokens": ["Ich", "soll", "und", "mu\u00df", "ein", "K\u00fc\u00df\u00b7lein", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00fc\u00df mich, J\u00e4gerlein k\u00fc\u00df mich,", "tokens": ["K\u00fc\u00df", "mich", ",", "J\u00e4\u00b7ger\u00b7lein", "k\u00fc\u00df", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du sollst und mu\u00dft einen J\u00e4ger haben,", "tokens": ["Du", "sollst", "und", "mu\u00dft", "ei\u00b7nen", "J\u00e4\u00b7ger", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "K\u00fc\u00df mich, Jungfr\u00e4ulein k\u00fc\u00df mich.", "tokens": ["K\u00fc\u00df", "mich", ",", "Jung\u00b7fr\u00e4u\u00b7lein", "k\u00fc\u00df", "mich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}}, "stanza.14": {"line.1": {"text": "Die dritt, die dritt, die nenn ich nicht,", "tokens": ["Die", "dritt", ",", "die", "dritt", ",", "die", "nenn", "ich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PRELS", "ADJD", "$,", "PRELS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie hat ein klares Angesicht,", "tokens": ["Sie", "hat", "ein", "kla\u00b7res", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und soll mir nicht err\u00f6then.", "tokens": ["Und", "soll", "mir", "nicht", "er\u00b7r\u00f6\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Ich soll und mu\u00df ein Buhlen haben,", "tokens": ["Ich", "soll", "und", "mu\u00df", "ein", "Buh\u00b7len", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Trabe dich Thierlein, trabe,", "tokens": ["Tra\u00b7be", "dich", "Thier\u00b7lein", ",", "tra\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "VVFIN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und sollt ich ihn aus der Erde graben,", "tokens": ["Und", "sollt", "ich", "ihn", "aus", "der", "Er\u00b7de", "gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", 