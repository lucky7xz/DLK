{"dta.poem.2278": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Pflicht zu verliebten Gespr\u00e4chen,  \n an  \n Herrn Amtmann Fromm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905198996", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In den lauten Nachtigallen,", "tokens": ["In", "den", "lau\u00b7ten", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lokkt und schl\u00e4gt und iauchzt die Liebe;", "tokens": ["Lokkt", "und", "schl\u00e4gt", "und", "i\u00b7a\u00b7uchzt", "die", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In der Lerche unterm Himmel", "tokens": ["In", "der", "Ler\u00b7che", "un\u00b7term", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lobt und tirelirt die Liebe;", "tokens": ["Lobt", "und", "ti\u00b7re\u00b7lirt", "die", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "In dem Enter auf dem Wasser,", "tokens": ["In", "dem", "En\u00b7ter", "auf", "dem", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwimmt und schnattert nichts als Liebe;", "tokens": ["Schwimmt", "und", "schnat\u00b7tert", "nichts", "als", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PIS", "KOKOM", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In den Schwalben unterm Dache,", "tokens": ["In", "den", "Schwal\u00b7ben", "un\u00b7term", "Da\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zwitschert, baut und spricht die Liebe;", "tokens": ["Zwit\u00b7schert", ",", "baut", "und", "spricht", "die", "Lie\u00b7be", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In den Spatzen vor dem Fenster", "tokens": ["In", "den", "Spat\u00b7zen", "vor", "dem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lauscht und ruft und h\u00fcpft die Liebe;", "tokens": ["Lauscht", "und", "ruft", "und", "h\u00fcpft", "die", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "In dem T\u00e4uber, in der Taube", "tokens": ["In", "dem", "T\u00e4u\u00b7ber", ",", "in", "der", "Tau\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Girrt und lokkt und lacht die Liebe;", "tokens": ["Girrt", "und", "lokkt", "und", "lacht", "die", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "In den T\u00f6nen meiner Laute,", "tokens": ["In", "den", "T\u00f6\u00b7nen", "mei\u00b7ner", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Klingt und lobt und scherzt die Liebe;", "tokens": ["Klingt", "und", "lobt", "und", "scherzt", "die", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "In dem Kind auf meinem Schosse", "tokens": ["In", "dem", "Kind", "auf", "mei\u00b7nem", "Schos\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "H\u00fcpft und scherzt und singt die Liebe;", "tokens": ["H\u00fcpft", "und", "scherzt", "und", "singt", "die", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Alles Wild im freien Felde,", "tokens": ["Al\u00b7les", "Wild", "im", "frei\u00b7en", "Fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Alle V\u00f6gel unterm Himmel", "tokens": ["Al\u00b7le", "V\u00f6\u00b7gel", "un\u00b7term", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Haben Stimmen zu der Liebe;", "tokens": ["Ha\u00b7ben", "Stim\u00b7men", "zu", "der", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Alles scherzt und spricht vom Lieben;", "tokens": ["Al\u00b7les", "scherzt", "und", "spricht", "vom", "Lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "VVFIN", "APPRART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Soll ich denn davon nicht sprechen?", "tokens": ["Soll", "ich", "denn", "da\u00b7von", "nicht", "spre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PAV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}