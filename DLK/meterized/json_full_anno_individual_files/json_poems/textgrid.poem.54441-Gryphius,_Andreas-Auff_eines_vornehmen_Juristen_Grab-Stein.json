{"textgrid.poem.54441": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Auff eines vornehmen Juristen Grab-Stein", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ich durch all Gesetz vnnd alle Recht kont brechen;", "tokens": ["Der", "Ich", "durch", "all", "Ge\u00b7setz", "vnnd", "al\u00b7le", "Recht", "kont", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "KON", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem an Verstand vnd Kunst kaum jemand gleiche war;", "tokens": ["Dem", "an", "Ver\u00b7stand", "vnd", "Kunst", "kaum", "je\u00b7mand", "glei\u00b7che", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "ADV", "PIS", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Ich die Dunckelheit der sach\u1ebd machte klar;", "tokens": ["Der", "Ich", "die", "Dun\u00b7ckel\u00b7heit", "der", "sach\u1ebd", "mach\u00b7te", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hab vber mich den Todt must lassen Vrtheil sprechen/", "tokens": ["Hab", "vber", "mich", "den", "Todt", "must", "las\u00b7sen", "Vrtheil", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "VMFIN", "VVINF", "NN", "VVINF", "$("], "meter": "++--+-+--+-", "measu