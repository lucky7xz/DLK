{"dta.poem.7814": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "Die Fliege auf ihren Wangen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1665", "urn": "urn:nbn:de:kobv:b4-200905198447", "language": ["de:0.99"], "booktitle": "Birken, Sigmund von: Pegnesische Gespr\u00e4chspiel-Gesellschaft von Nymfen und Hirten. N\u00fcrnberg, 1665."}, "poem": {"stanza.1": {"line.1": {"text": "Darfst du/ du k\u00fcbnes Thier/ zu k\u00fcssen/ so", "tokens": ["Darfst", "du", "/", "du", "k\u00fcb\u00b7nes", "Thier", "/", "zu", "k\u00fcs\u00b7sen", "/", "so"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "$(", "PPER", "ADJA", "NN", "$(", "PTKZU", "VVINF", "$(", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die Wangen/ die so voll der Venus-r\u00f6selein?", "tokens": ["die", "Wan\u00b7gen", "/", "die", "so", "voll", "der", "Ve\u00b7nus\u00b7r\u00f6\u00b7se\u00b7lein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wilst du wor Honig hier/ wie Bienen/ holen", "tokens": ["wilst", "du", "wor", "Ho\u00b7nig", "hier", "/", "wie", "Bie\u00b7nen", "/", "ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "PPER", "NE", "NN", "ADV", "$(", "KOKOM", "NN", "$(", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich gl\u00e4ub\u2019/ es m\u00fcss in dir Adonis Seele ste-", "tokens": ["Ich", "gl\u00e4ub'", "/", "es", "m\u00fcss", "in", "dir", "A\u00b7do\u00b7nis", "See\u00b7le", "ste"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VMFIN", "APPR", "PPER", "NE", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "der Amarillis wolt mit einem Ku\u00df belecke\u0304/", "tokens": ["der", "A\u00b7ma\u00b7ril\u00b7lis", "wolt", "mit", "ei\u00b7nem", "Ku\u00df", "be\u00b7leck\u0113", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die er vor Venus hielt. Ach w\u00e4r dein Gl\u00fccke", "tokens": ["die", "er", "vor", "Ve\u00b7nus", "hielt", ".", "Ach", "w\u00e4r", "dein", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "APPR", "NN", "VVFIN", "$.", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "ach solt ach solt ich doch solch eine Fliege seyn!", "tokens": ["ach", "solt", "ach", "solt", "ich", "doch", "solch", "ei\u00b7ne", "Flie\u00b7ge", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "VMFIN", "PPER", "ADV", "PIAT", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ich wolte nicht/ wie du/ ihr setzen eine\u0304 Flecken.", "tokens": ["ich", "wol\u00b7te", "nicht", "/", "wie", "du", "/", "ihr", "set\u00b7zen", "ein\u0113", "Fle\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$(", "PWAV", "PPER", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Halt/ Fliege! lass einmal mich fliegen hin", "tokens": ["Halt", "/", "Flie\u00b7ge", "!", "lass", "ein\u00b7mal", "mich", "flie\u00b7gen", "hin"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$.", "VVFIN", "ADV", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "und trage meine\u0304 Mund in deinem hin zu ihr", "tokens": ["und", "tra\u00b7ge", "mein\u0113", "Mund", "in", "dei\u00b7nem", "hin", "zu", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADV", "APPR", "PPOSAT"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "daf\u00fcr solst du allzeit an meinem Tische essen.", "tokens": ["da\u00b7f\u00fcr", "solst", "du", "all\u00b7zeit", "an", "mei\u00b7nem", "Ti\u00b7sche", "es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Flieg hin/ mit diesem Gl\u00fcck! du darfst nun", "tokens": ["Flieg", "hin", "/", "mit", "die\u00b7sem", "Gl\u00fcck", "!", "du", "darfst", "nun"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$(", "APPR", "PDAT", "NN", "$.", "PPER", "VMFIN", "ADV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.13": {"text": "des Ambrosiner-Brods und Nectars/ weil", "tokens": ["des", "Am\u00b7bro\u00b7si\u00b7ner\u00b7Brods", "und", "Nec\u00b7tars", "/", "weil"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "KON", "NN", "$(", "KOUS"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "an diesem Tisch/ der nur vor G\u00f6tter ist/ ge-", "tokens": ["an", "die\u00b7sem", "Tisch", "/", "der", "nur", "vor", "G\u00f6t\u00b7ter", "ist", "/", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PDAT", "NN", "$(", "ART", "ADV", "APPR", "NN", "VAFIN", "$(", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}