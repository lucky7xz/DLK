{"dta.poem.2840": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df gelten, lieber Sohn, was irgend gelten mag,", "tokens": ["La\u00df", "gel\u00b7ten", ",", "lie\u00b7ber", "Sohn", ",", "was", "ir\u00b7gend", "gel\u00b7ten", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "ADV", "NN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr diesen j\u00fcngsten, wenn nicht bis zum j\u00fcngsten Tag!", "tokens": ["F\u00fcr", "die\u00b7sen", "j\u00fcng\u00b7sten", ",", "wenn", "nicht", "bis", "zum", "j\u00fcng\u00b7sten", "Tag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "KOUS", "PTKNEG", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "La\u00df andre gelten, und dich lassen andre gelten;", "tokens": ["La\u00df", "and\u00b7re", "gel\u00b7ten", ",", "und", "dich", "las\u00b7sen", "and\u00b7re", "gel\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "VVINF", "$,", "KON", "PPER", "VVFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist viel besser als einander niederschelten.", "tokens": ["Das", "ist", "viel", "bes\u00b7ser", "als", "ein\u00b7an\u00b7der", "nie\u00b7der\u00b7schel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "KOKOM", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Will dir nicht alles auch, was gilt, gleich g\u00fcltig scheinen;", "tokens": ["Will", "dir", "nicht", "al\u00b7les", "auch", ",", "was", "gilt", ",", "gleich", "g\u00fcl\u00b7tig", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PIS", "ADV", "$,", "PWS", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh's recht nur an! was gilts? dir wirds gleichg\u00fcltig scheinen.", "tokens": ["Sieh's", "recht", "nur", "an", "!", "was", "gilts", "?", "dir", "wirds", "gleich\u00b7g\u00fcl\u00b7tig", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKVZ", "$.", "PWS", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gleichg\u00fcltigkeit derart ist g\u00f6ttlicher Natur;", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig\u00b7keit", "der\u00b7art", "ist", "g\u00f6tt\u00b7li\u00b7cher", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleichg\u00fcltig nicht allein glaubt G\u00f6tter Epikur;", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "nicht", "al\u00b7lein", "glaubt", "G\u00f6t\u00b7ter", "E\u00b7pi\u00b7kur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "ADV", "VVFIN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gleichg\u00fcltig glaub' ich selbst auch Gott in diesem Sinn,", "tokens": ["Gleich\u00b7g\u00fcl\u00b7tig", "glaub'", "ich", "selbst", "auch", "Gott", "in", "die\u00b7sem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ADV", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ich ihm g\u00fcltig gleich wie alle Wesen bin.", "tokens": ["Da\u00df", "ich", "ihm", "g\u00fcl\u00b7tig", "gleich", "wie", "al\u00b7le", "We\u00b7sen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "ADV", "KOKOM", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}