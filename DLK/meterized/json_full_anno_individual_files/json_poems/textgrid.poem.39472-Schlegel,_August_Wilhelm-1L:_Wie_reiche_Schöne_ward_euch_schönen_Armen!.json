{"textgrid.poem.39472": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie reiche Sch\u00f6ne ward euch sch\u00f6nen Armen!", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie reiche Sch\u00f6ne ward euch sch\u00f6nen Armen!", "tokens": ["Wie", "rei\u00b7che", "Sch\u00f6\u00b7ne", "ward", "euch", "sch\u00f6\u00b7nen", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur m\u00fc\u00dft ihr euch mit anmuthsvollem Regen", "tokens": ["Nur", "m\u00fc\u00dft", "ihr", "euch", "mit", "an\u00b7muths\u00b7vol\u00b7lem", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht blo\u00df zur Rede, selbst beredt, bewegen:", "tokens": ["Nicht", "blo\u00df", "zur", "Re\u00b7de", ",", "selbst", "be\u00b7redt", ",", "be\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPRART", "NN", "$,", "ADV", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Arme sind gemacht, um zu umarmen.", "tokens": ["Die", "Ar\u00b7me", "sind", "ge\u00b7macht", ",", "um", "zu", "um\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Verbannt aus eurem Reich, mu\u00df ich verarmen;", "tokens": ["Ver\u00b7bannt", "aus", "eu\u00b7rem", "Reich", ",", "mu\u00df", "ich", "ver\u00b7ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wollt ihr mich in enge Bande legen,", "tokens": ["Doch", "wollt", "ihr", "mich", "in", "en\u00b7ge", "Ban\u00b7de", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So l\u00f6s't ihr mich: wer k\u00f6nnte Harm wohl hegen,", "tokens": ["So", "l\u00f6s't", "ihr", "mich", ":", "wer", "k\u00f6nn\u00b7te", "Harm", "wohl", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$.", "PWS", "VMFIN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gehegt in Armen, die von Lieb' erwarmen?", "tokens": ["Ge\u00b7hegt", "in", "Ar\u00b7men", ",", "die", "von", "Lieb'", "er\u00b7war\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So zart ger\u00fcndet von den Schultern nieder", "tokens": ["So", "zart", "ge\u00b7r\u00fcn\u00b7det", "von", "den", "Schul\u00b7tern", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Gr\u00fcbchen spielet an den Ellenbogen,", "tokens": ["Ihr", "G