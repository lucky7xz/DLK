 {"text": "Vor dem Zaren", "tokens": ["Vor", "dem", "Za\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Der Tataren", "tokens": ["Der", "Ta\u00b7ta\u00b7ren"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Er Dich m\u00f6ge treu bewahren,", "tokens": ["Er", "Dich", "m\u00f6\u00b7ge", "treu", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Denn Sibirien ist gar nah',", "tokens": ["Denn", "Si\u00b7bi\u00b7ri\u00b7en", "ist", "gar", "nah'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sieh Dich um, Germania!", "tokens": ["Sieh", "Dich", "um", ",", "Ger\u00b7ma\u00b7nia", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Da\u00df sich F\u00fcrst und Volk vertraue,", "tokens": ["Da\u00df", "sich", "F\u00fcrst", "und", "Volk", "ver\u00b7trau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir kein Pfaff' das Licht verbaue,", "tokens": ["Dir", "kein", "Pfaff'", "das", "Licht", "ver\u00b7bau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df kein Marat", "tokens": ["Da\u00df", "kein", "Ma\u00b7rat"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dich verf\u00fchre", "tokens": ["Dich", "ver\u00b7f\u00fch\u00b7re"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und Dich dann septembrisiere,", "tokens": ["Und", "Dich", "dann", "sep\u00b7temb\u00b7ri\u00b7sie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Denn die Marats sind schon da,", "tokens": ["Denn", "die", "Ma\u00b7rats", "sind", "schon", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADV", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Wahre Dich, Germania!", "tokens": ["Wah\u00b7re", "Dich", ",", "Ger\u00b7ma\u00b7nia", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "Da\u00df Dich Gott in Gnaden h\u00fcte,", "tokens": ["Da\u00df", "Dich", "Gott", "in", "Gna\u00b7den", "h\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herzblatt Du der Weltenbl\u00fcte.", "tokens": ["Herz\u00b7blatt", "Du", "der", "Wel\u00b7ten\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "V\u00f6lkerwehre,", "tokens": ["V\u00f6l\u00b7ker\u00b7weh\u00b7re", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Stern der Ehre,", "tokens": ["Stern", "der", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Da\u00df Du strahlst von Meer zu Meere,", "tokens": ["Da\u00df", "Du", "strahlst", "von", "Meer", "zu", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und Dein Wort sei fern und nah'", "tokens": ["Und", "Dein", "Wort", "sei", "fern", "und", "nah'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "NN"], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Und Dein Schwert, Germania!", "tokens": ["Und", "Dein", "Schwert", ",", "Ger\u00b7ma\u00b7nia", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}