{"textgrid.poem.34702": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Meer hat seine Perlen,", "tokens": ["Das", "Meer", "hat", "sei\u00b7ne", "Per\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Himmel hat seine Sterne,", "tokens": ["Der", "Him\u00b7mel", "hat", "sei\u00b7ne", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aber mein Herz, mein Herz,", "tokens": ["A\u00b7ber", "mein", "Herz", ",", "mein", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Mein Herz hat seine Liebe.", "tokens": ["Mein", "Herz", "hat", "sei\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gro\u00df ist das Meer und der Himmel,", "tokens": ["Gro\u00df", "ist", "das", "Meer", "und", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Doch gr\u00f6\u00dfer ist mein Herz,", "tokens": ["Doch", "gr\u00f6\u00b7\u00dfer", "ist", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00f6ner als Perlen und Sterne", "tokens": ["Und", "sch\u00f6\u00b7ner", "als", "Per\u00b7len", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Leuchtet und strahlt meine Liebe.", "tokens": ["Leuch\u00b7tet", "und", "strahlt", "mei\u00b7ne", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Du kleines, junges M\u00e4dchen,", "tokens": ["Du", "klei\u00b7nes", ",", "jun\u00b7ges", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Komm an mein gro\u00dfes Herz;", "tokens": ["Komm", "an", "mein", "gro\u00b7\u00dfes", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Herz und das Meer und der Himmel", "tokens": ["Mein", "Herz", "und", "das", "Meer", "und", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vergehn vor lauter Liebe.", "tokens": ["Ver\u00b7gehn", "vor", "lau\u00b7ter", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "An die blaue Himmelsdecke,", "tokens": ["An", "die", "blau\u00b7e", "Him\u00b7mels\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die sch\u00f6nen Sterne blinken,", "tokens": ["Wo", "die", "sch\u00f6\u00b7nen", "Ster\u00b7ne", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00f6cht ich pressen meine Lippen,", "tokens": ["M\u00f6cht", "ich", "pres\u00b7sen", "mei\u00b7ne", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pressen wild und st\u00fcrmisch weinen.", "tokens": ["Pres\u00b7sen", "wild", "und", "st\u00fcr\u00b7misch", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jene Sterne sind die Augen", "tokens": ["Je\u00b7ne", "Ster\u00b7ne", "sind", "die", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Liebsten, tausendf\u00e4ltig", "tokens": ["Mei\u00b7ner", "Liebs\u00b7ten", ",", "tau\u00b7send\u00b7f\u00e4l\u00b7tig"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schimmern sie und gr\u00fc\u00dfen freundlich", "tokens": ["Schim\u00b7mern", "sie", "und", "gr\u00fc\u00b7\u00dfen", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "KON", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der blauen Himmelsdecke.", "tokens": ["Aus", "der", "blau\u00b7en", "Him\u00b7mels\u00b7de\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nach der blauen Himmelsdecke,", "tokens": ["Nach", "der", "blau\u00b7en", "Him\u00b7mels\u00b7de\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach den Augen der Geliebten,", "tokens": ["Nach", "den", "Au\u00b7gen", "der", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Heb ich andachtsvoll die Arme,", "tokens": ["Heb", "ich", "an\u00b7dachts\u00b7voll", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und ich bitte und ich flehe:", "tokens": ["Und", "ich", "bit\u00b7te", "und", "ich", "fle\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "KON", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbholde Augen, Gnadenlichter,", "tokens": ["\u00bb", "hol\u00b7de", "Au\u00b7gen", ",", "Gna\u00b7den\u00b7lich\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oh, beseligt meine Seele,", "tokens": ["Oh", ",", "be\u00b7se\u00b7ligt", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft mich sterben und erwerben", "tokens": ["La\u00dft", "mich", "ster\u00b7ben", "und", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch und euren ganzen Himmel!\u00ab", "tokens": ["Euch", "und", "eu\u00b7ren", "gan\u00b7zen", "Him\u00b7mel", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "KON", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aus den Himmelsaugen droben", "tokens": ["Aus", "den", "Him\u00b7mel\u00b7sau\u00b7gen", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fallen zitternd goldne Funken", "tokens": ["Fal\u00b7len", "zit\u00b7ternd", "gold\u00b7ne", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die Nacht, und meine Seele", "tokens": ["Durch", "die", "Nacht", ",", "und", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dehnt sich liebeweit und weiter.", "tokens": ["Dehnt", "sich", "lie\u00b7be\u00b7weit", "und", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "KON", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Oh, ihr Himmelsaugen droben!", "tokens": ["Oh", ",", "ihr", "Him\u00b7mel\u00b7sau\u00b7gen", "dro\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weint euch aus in meine Seele,", "tokens": ["Weint", "euch", "aus", "in", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df von lichten Sternentr\u00e4nen", "tokens": ["Da\u00df", "von", "lich\u00b7ten", "Ster\u00b7nen\u00b7tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberflie\u00dfet meine Seele.", "tokens": ["\u00dc\u00b7berf\u00b7lie\u00b7\u00dfet", "mei\u00b7ne", "See\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Eingewiegt von Meereswellen", "tokens": ["Ein\u00b7ge\u00b7wiegt", "von", "Mee\u00b7res\u00b7wel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und von tr\u00e4umenden Gedanken,", "tokens": ["Und", "von", "tr\u00e4u\u00b7men\u00b7den", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieg ich still in der Kaj\u00fcte,", "tokens": ["Lieg", "ich", "still", "in", "der", "Ka\u00b7j\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem dunkeln Winkelbette.", "tokens": ["In", "dem", "dun\u00b7keln", "Win\u00b7kel\u00b7bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Durch die offne Luke schau ich", "tokens": ["Durch", "die", "off\u00b7ne", "Lu\u00b7ke", "schau", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Droben hoch die hellen Sterne,", "tokens": ["Dro\u00b7ben", "hoch", "die", "hel\u00b7len", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die geliebten, s\u00fc\u00dfen Augen", "tokens": ["Die", "ge\u00b7lieb\u00b7ten", ",", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner s\u00fc\u00dfen Vielgeliebten.", "tokens": ["Mei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Viel\u00b7ge\u00b7lieb\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Die geliebten, s\u00fc\u00dfen Augen", "tokens": ["Die", "ge\u00b7lieb\u00b7ten", ",", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachen \u00fcber meinem Haupte,", "tokens": ["Wa\u00b7chen", "\u00fc\u00b7ber", "mei\u00b7nem", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie blinken und sie winken", "tokens": ["Und", "sie", "blin\u00b7ken", "und", "sie", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der blauen Himmelsdecke.", "tokens": ["Aus", "der", "blau\u00b7en", "Him\u00b7mels\u00b7de\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Nach der blauen Himmelsdecke", "tokens": ["Nach", "der", "blau\u00b7en", "Him\u00b7mels\u00b7de\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schau ich selig lange Stunden,", "tokens": ["Schau", "ich", "se\u00b7lig", "lan\u00b7ge", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ein wei\u00dfer Nebelschleier", "tokens": ["Bis", "ein", "wei\u00b7\u00dfer", "Ne\u00b7bel\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir verh\u00fcllt die lieben Augen.", "tokens": ["Mir", "ver\u00b7h\u00fcllt", "die", "lie\u00b7ben", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "An die bretterne Schiffswand,", "tokens": ["An", "die", "bret\u00b7ter\u00b7ne", "Schiffs\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--++", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wo mein tr\u00e4umendes Haupt liegt,", "tokens": ["Wo", "mein", "tr\u00e4u\u00b7men\u00b7des", "Haupt", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Branden die Wellen, die wilden Wellen;", "tokens": ["Bran\u00b7den", "die", "Wel\u00b7len", ",", "die", "wil\u00b7den", "Wel\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Sie rauschen und murmeln", "tokens": ["Sie", "rau\u00b7schen", "und", "mur\u00b7meln"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Mir heimlich ins Ohr:", "tokens": ["Mir", "heim\u00b7lich", "ins", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "\u00bbbet\u00f6rter Geselle!", "tokens": ["\u00bb", "be\u00b7t\u00f6r\u00b7ter", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Dein Arm ist kurz, und der Himmel ist weit,", "tokens": ["Dein", "Arm", "ist", "kurz", ",", "und", "der", "Him\u00b7mel", "ist", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und die Sterne droben sind festgenagelt", "tokens": ["Und", "die", "Ster\u00b7ne", "dro\u00b7ben", "sind", "fest\u00b7ge\u00b7na\u00b7gelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VAFIN", "VVPP"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Mit goldnen N\u00e4geln \u2013", "tokens": ["Mit", "gold\u00b7nen", "N\u00e4\u00b7geln", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Vergebliches Sehnen, vergebliches Seufzen,", "tokens": ["Ver\u00b7geb\u00b7li\u00b7ches", "Seh\u00b7nen", ",", "ver\u00b7geb\u00b7li\u00b7ches", "Seuf\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "Das beste w\u00e4re, du schliefest ein.\u00ab", "tokens": ["Das", "bes\u00b7te", "w\u00e4\u00b7re", ",", "du", "schlie\u00b7fest", "ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Es tr\u00e4umte mir von einer weiten Heide,", "tokens": ["Es", "tr\u00e4um\u00b7te", "mir", "von", "ei\u00b7ner", "wei\u00b7ten", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weit \u00fcberdeckt von stillem, wei\u00dfem Schnee,", "tokens": ["Weit", "\u00fc\u00b7berd\u00b7eckt", "von", "stil\u00b7lem", ",", "wei\u00b7\u00dfem", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und unterm wei\u00dfen Schnee lag ich begraben", "tokens": ["Und", "un\u00b7term", "wei\u00b7\u00dfen", "Schnee", "lag", "ich", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schlief den einsam kalten Todesschlaf.", "tokens": ["Und", "schlief", "den", "ein\u00b7sam", "kal\u00b7ten", "To\u00b7des\u00b7schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Doch droben aus dem dunkeln Himmel schauten", "tokens": ["Doch", "dro\u00b7ben", "aus", "dem", "dun\u00b7keln", "Him\u00b7mel", "schau\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Herunter auf mein Grab die Sternenaugen,", "tokens": ["Her\u00b7un\u00b7ter", "auf", "mein", "Grab", "die", "Ster\u00b7nen\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die s\u00fc\u00dfen Augen! und sie gl\u00e4nzten sieghaft", "tokens": ["Die", "s\u00fc\u00b7\u00dfen", "Au\u00b7gen", "!", "und", "sie", "gl\u00e4nz\u00b7ten", "sieg\u00b7haft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PPER", "ADJA", "NN"], "meter": "-+-+-+-