{"textgrid.poem.37510": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Sechstens", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Padua war gro\u00df Gedr\u00e4nge", "tokens": ["Zu", "Pa\u00b7dua", "war", "gro\u00df", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der andachtsvollen Christenmenge.", "tokens": ["Der", "an\u00b7dachts\u00b7vol\u00b7len", "Chris\u00b7ten\u00b7men\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man eilt zu Kanzeln und Alt\u00e4ren,", "tokens": ["Man", "eilt", "zu", "Kan\u00b7zeln", "und", "Al\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den frommen Antonio zu h\u00f6ren,", "tokens": ["Den", "from\u00b7men", "An\u00b7to\u00b7nio", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der sich alldorten seiner Predigt", "tokens": ["Der", "sich", "all\u00b7dor\u00b7ten", "sei\u00b7ner", "Pre\u00b7digt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit wunderbarer Kraft entledigt.", "tokens": ["Mit", "wun\u00b7der\u00b7ba\u00b7rer", "Kraft", "ent\u00b7le\u00b7digt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Auch t\u00e4t er oft, vom Geist getrieben,", "tokens": ["Auch", "t\u00e4t", "er", "oft", ",", "vom", "Geist", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Herrliche Zeichen und Wunder ver\u00fcben.", "tokens": ["Herr\u00b7li\u00b7che", "Zei\u00b7chen", "und", "Wun\u00b7der", "ver\u00b7\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.9": {"text": "Jedoch die Kinder dieser Welt,", "tokens": ["Je\u00b7doch", "die", "Kin\u00b7der", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Denen so etwas selten gef\u00e4llt,", "tokens": ["De\u00b7nen", "so", "et\u00b7was", "sel\u00b7ten", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Murren und munkeln so allerlei", "tokens": ["Mur\u00b7ren", "und", "mun\u00b7keln", "so", "al\u00b7ler\u00b7lei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ADV", "PIAT"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Von Teufelsk\u00fcnsten und Zauberei", "tokens": ["Von", "Teu\u00b7fels\u00b7k\u00fcns\u00b7ten", "und", "Zau\u00b7be\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Und verklagen den frommen Antonius", "tokens": ["Und", "ver\u00b7kla\u00b7gen", "den", "from\u00b7men", "An\u00b7to\u00b7ni\u00b7us"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NE"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Beim guten Bischof Rusticus.", "tokens": ["Beim", "gu\u00b7ten", "Bi\u00b7schof", "Rus\u00b7ti\u00b7cus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Bischof l\u00e4\u00dft den Bruder kommen:", "tokens": ["Der", "Bi\u00b7schof", "l\u00e4\u00dft", "den", "Bru\u00b7der", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbich hab' von deiner Kunst vernommen!", "tokens": ["\u00bb", "ich", "hab'", "von", "dei\u00b7ner", "Kunst", "ver\u00b7nom\u00b7men", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein, mein Freund, wie ist der Glaube?\u00ab", "tokens": ["Al\u00b7lein", ",", "mein", "Freund", ",", "wie", "ist", "der", "Glau\u00b7be", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Flugs nimmt Antonio seine Haube", "tokens": ["Flugs", "nimmt", "An\u00b7to\u00b7nio", "sei\u00b7ne", "Hau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4ngt sie, wie an einen Pfahl,", "tokens": ["Und", "h\u00e4ngt", "sie", ",", "wie", "an", "ei\u00b7nen", "Pfahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An einen warmen Sonnenstrahl.", "tokens": ["An", "ei\u00b7nen", "war\u00b7men", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bischof sprach: \u00bbBravo! \u2013 Allein!", "tokens": ["Der", "Bi\u00b7schof", "sprach", ":", "\u00bb", "Bra\u00b7vo", "!", "\u2013", "Al\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ITJ", "$.", "$(", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies kann auch Teufelsblendwerk sein!\u00ab", "tokens": ["Dies", "kann", "auch", "Teu\u00b7fels\u00b7blend\u00b7werk", "sein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "ADV", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nun spielte da im Sand herum", "tokens": ["Nun", "spiel\u00b7te", "da", "im", "Sand", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Findelknabe, taub und stumm,", "tokens": ["Ein", "Fin\u00b7del\u00b7kna\u00b7be", ",", "taub", "und", "stumm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und keiner hatte je erfahren,", "tokens": ["Und", "kei\u00b7ner", "hat\u00b7te", "je", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wer Vater oder Mutter waren. \u2013", "tokens": ["Wer", "Va\u00b7ter", "o\u00b7der", "Mut\u00b7ter", "wa\u00b7ren", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Antonius sprach: \u00bbSag an, mein Kind,", "tokens": ["An\u00b7to\u00b7ni\u00b7us", "sprach", ":", "\u00bb", "Sag", "an", ",", "mein", "Kind", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "$(", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Wer deine lieben Eltern sind!!\u00ab", "tokens": ["Wer", "dei\u00b7ne", "lie\u00b7ben", "El\u00b7tern", "sind", "!!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "O Wunder! Der bis diese Stund'", "tokens": ["O", "Wun\u00b7der", "!", "Der", "bis", "die\u00b7se", "Stund'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ART", "APPR", "PDAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Nicht sprechen konnte, sprach jetzund:", "tokens": ["Nicht", "spre\u00b7chen", "konn\u00b7te", ",", "sprach", "je\u00b7tzund", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbder Bischof Rusticus, der ist...\u00ab", "tokens": ["\u00bb", "der", "Bi\u00b7schof", "Rus\u00b7ti\u00b7cus", ",", "der", "ist", "...", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "NE", "$,", "PRELS", "VAFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbpis\u2013s\u2013s\u2013s\u2013s\u2013s\u2013t!!!\u00ab", "tokens": ["\u00bb", "pis", "\u2013", "s", "\u2013", "s", "\u2013", "s", "\u2013", "s", "\u2013", "s", "\u2013", "t", "!!!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "FM.la", "$(", "XY", "$(", "XY", "$(", "XY", "$(", "XY", "$(", "XY", "$(", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sprach der Bischof \u2013 \u00bbes ist schon recht!!", "tokens": ["Sprach", "der", "Bi\u00b7schof", "\u2013", "\u00bb", "es", "ist", "schon", "recht", "!!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$(", "$(", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Antonius, du bist ein Gottesknecht!!\u00ab", "tokens": ["An\u00b7to\u00b7ni\u00b7us", ",", "du", "bist", "ein", "Got\u00b7tes\u00b7knecht", "!!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Seit dieser Zeit sah gro\u00df und klein", "tokens": ["Seit", "die\u00b7ser", "Zeit", "sah", "gro\u00df", "und", "klein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Antonius mit dem Heilgenschein.", "tokens": ["An\u00b7to\u00b7ni\u00b7us", "mit", "dem", "Heil\u00b7gen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Zu Padua war gro\u00df Gedr\u00e4nge", "tokens": ["Zu", "Pa\u00b7dua", "war", "gro\u00df", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der andachtsvollen Christenmenge.", "tokens": ["Der", "an\u00b7dachts\u00b7vol\u00b7len", "Chris\u00b7ten\u00b7men\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man eilt zu Kanzeln und Alt\u00e4ren,", "tokens": ["Man", "eilt", "zu", "Kan\u00b7zeln", "und", "Al\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den frommen Antonio zu h\u00f6ren,", "tokens": ["Den", "from\u00b7men", "An\u00b7to\u00b7nio", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der sich alldorten seiner Predigt", "tokens": ["Der", "sich", "all\u00b7dor\u00b7ten", "sei\u00b7ner", "Pre\u00b7digt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "mea