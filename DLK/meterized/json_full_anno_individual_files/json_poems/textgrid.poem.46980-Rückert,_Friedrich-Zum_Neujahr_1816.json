, "dei\u00b7ner", "Un\u00b7schuld", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei mir gegr\u00fc\u00dft, du junges Jahr!", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "du", "jun\u00b7ges", "Jahr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon manches hab' ich aus der Wiege", "tokens": ["Schon", "man\u00b7ches", "hab'", "ich", "aus", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Genommen und zu Grab gelegt,", "tokens": ["Ge\u00b7nom\u00b7men", "und", "zu", "Grab", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Damit ans Licht ein andres stiege,", "tokens": ["Da\u00b7mit", "ans", "Licht", "ein", "and\u00b7res", "stie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und s\u00fc\u00dfe Hoffnung stets gehegt:", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "Hoff\u00b7nung", "stets", "ge\u00b7hegt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Die Hoffnung aller Welt und meine,", "tokens": ["Die", "Hoff\u00b7nung", "al\u00b7ler", "Welt", "und", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die jedem Jahr entgegent\u00f6nt,", "tokens": ["Die", "je\u00b7dem", "Jahr", "ent\u00b7ge\u00b7gen\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob endlich einmal das erscheine,", "tokens": ["Ob", "end\u00b7lich", "ein\u00b7mal", "das", "er\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von welchem sei das Werk gekr\u00f6nt,", "tokens": ["Von", "wel\u00b7chem", "sei", "das", "Werk", "ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob endlich das sei abgebrochen,", "tokens": ["Ob", "end\u00b7lich", "das", "sei", "ab\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von welchem uns erf\u00fcllet sei,", "tokens": ["Von", "wel\u00b7chem", "uns", "er\u00b7f\u00fcl\u00b7let", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was von den vor'gen ward versprochen?", "tokens": ["Was", "von", "den", "vor'\u00b7gen", "ward", "ver\u00b7spro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn du das bist, so sag' mir's frei.", "tokens": ["Wenn", "du", "das", "bist", ",", "so", "sag'", "mir's", "frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VAFIN", "$,", "ADV", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Ich kann durch meiner R\u00fchrung Z\u00e4hren", "tokens": ["Ich", "kann", "durch", "mei\u00b7ner", "R\u00fch\u00b7rung", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht deine Z\u00fcge deutlich sehn;", "tokens": ["Nicht", "dei\u00b7ne", "Z\u00fc\u00b7ge", "deut\u00b7lich", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein L\u00e4cheln scheint sie zu verkl\u00e4ren:", "tokens": ["Ein", "L\u00e4\u00b7cheln", "scheint", "sie", "zu", "ver\u00b7kl\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sprich, soll durch dich uns Heil geschehn?", "tokens": ["Sprich", ",", "soll", "durch", "dich", "uns", "Heil", "ge\u00b7schehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VMFIN", "APPR", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Willst du nicht wieder t\u00e4uschend schwinden,", "tokens": ["Willst", "du", "nicht", "wie\u00b7der", "t\u00e4u\u00b7schend", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie vor dir deiner Br\u00fcder g'nug,", "tokens": ["Wie", "vor", "dir", "dei\u00b7ner", "Br\u00fc\u00b7der", "g'\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Da\u00df wir den Glauben wieder finden,", "tokens": ["Da\u00df", "wir", "den", "Glau\u00b7ben", "wie\u00b7der", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den uns geraubt der Zeiten Lug?", "tokens": ["Den", "uns", "ge\u00b7raubt", "der", "Zei\u00b7ten", "Lug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Willst du den bangen Kn\u00e4ul entwirren,", "tokens": ["Willst", "du", "den", "ban\u00b7gen", "Kn\u00e4ul", "ent\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der um der Menschheit Brust sich schlang,", "tokens": ["Der", "um", "der", "Menschheit", "Brust", "sich", "schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und l\u00f6sen ird'scher Zwietracht Klirren", "tokens": ["Und", "l\u00f6\u00b7sen", "ird'\u00b7scher", "Zwiet\u00b7racht", "Klir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf in harmon'schen Sph\u00e4renklang?", "tokens": ["Auf", "in", "har\u00b7mon'\u00b7schen", "Sph\u00e4\u00b7ren\u00b7klang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Auff\u00fchren aus bewegten Stoffen", "tokens": ["Auf\u00b7f\u00fch\u00b7ren", "aus", "be\u00b7weg\u00b7ten", "Stof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Bau, der auf sich selbst kann ruhn?", "tokens": ["Den", "Bau", ",", "der", "auf", "sich", "selbst", "kann", "ruhn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PRF", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kurz, was wir w\u00fcnschen, was wir hoffen,", "tokens": ["Kurz", ",", "was", "wir", "w\u00fcn\u00b7schen", ",", "was", "wir", "hof\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ja, was wir fordern, willst du's thun?", "tokens": ["Ja", ",", "was", "wir", "for\u00b7dern", ",", "willst", "du's", "thun", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "PPER", "VVFIN", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "O seligstes der Zeitenkinder,", "tokens": ["O", "se\u00b7ligs\u00b7tes", "der", "Zei\u00b7ten\u00b7kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wenn das Geschick das Amt dir beut,", "tokens": ["Wenn", "das", "Ge\u00b7schick", "das", "Amt", "dir", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu sein der Ernte Garbenbinder,", "tokens": ["Zu", "sein", "der", "Ern\u00b7te", "Gar\u00b7ben\u00b7bin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die jene vor dir ausgestreut!", "tokens": ["Die", "je\u00b7ne", "vor", "dir", "aus\u00b7ge\u00b7streut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So w\u00fcnsch' ich dir vom Himmel heuer", "tokens": ["So", "w\u00fcn\u00b7sch'", "ich", "dir", "vom", "Him\u00b7mel", "heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "ADV"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Den besten Sonnenschein, der frommt,", "tokens": ["Den", "bes\u00b7ten", "Son\u00b7nen\u00b7schein", ",", "der", "frommt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df in die gro\u00dfe V\u00f6lkerscheuer", "tokens": ["Da\u00df", "in", "die", "gro\u00b7\u00dfe", "V\u00f6l\u00b7ker\u00b7scheu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Weizen unberegnet kommt.", "tokens": ["Der", "Wei\u00b7zen", "un\u00b7be\u00b7reg\u00b7net", "kommt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "So w\u00fcnsch' ich, da\u00df ein neues Leben", "tokens": ["So", "w\u00fcn\u00b7sch'", "ich", ",", "da\u00df", "ein", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Der alten Erde Mark durchdringt,", "tokens": ["Der", "al\u00b7ten", "Er\u00b7de", "Mark", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df aus des n\u00e4chsten Herbstes Reben", "tokens": ["Da\u00df", "aus", "des", "n\u00e4chs\u00b7ten", "Herbs\u00b7tes", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns goldnes Heil entgegen springt;", "tokens": ["Uns", "gold\u00b7nes", "Heil", "ent\u00b7ge\u00b7gen", "springt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df bei des Jahres Brot und Weine", "tokens": ["Da\u00df", "bei", "des", "Jah\u00b7res", "Brot", "und", "Wei\u00b7ne"], "token_info": ["