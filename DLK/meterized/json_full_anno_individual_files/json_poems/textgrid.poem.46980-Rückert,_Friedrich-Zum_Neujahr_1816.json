{"textgrid.poem.46980": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Zum Neujahr 1816", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Scho\u00df der Mitternacht geboren,", "tokens": ["Im", "Scho\u00df", "der", "Mit\u00b7ter\u00b7nacht", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Worin das Kind bewu\u00dftlos lag,", "tokens": ["Wo\u00b7rin", "das", "Kind", "be\u00b7wu\u00dft\u00b7los", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erwacht, zum Leben jetzt erkoren,", "tokens": ["Er\u00b7wacht", ",", "zum", "Le\u00b7ben", "jetzt", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Jahr am ernsten Glockenschlag.", "tokens": ["Das", "Jahr", "am", "erns\u00b7ten", "Glo\u00b7cken\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An seiner Wieg' ein Engel sitzet,", "tokens": ["An", "sei\u00b7ner", "Wieg'", "ein", "En\u00b7gel", "sit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dem vom zwiefachen Angesicht", "tokens": ["Dem", "vom", "zwie\u00b7fa\u00b7chen", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Zwiefacher Glanz des Lebens blitzet,", "tokens": ["Zwie\u00b7fac\u00b7her", "Glanz", "des", "Le\u00b7bens", "blit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Hier Abendrot, dort Morgenlicht.", "tokens": ["Hier", "A\u00b7ben\u00b7drot", ",", "dort", "Mor\u00b7gen\u00b7licht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier mit dem abendroten Blicke", "tokens": ["Hier", "mit", "dem", "a\u00b7ben\u00b7dro\u00b7ten", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schaut er nach Westen hin und sinnt,", "tokens": ["Schaut", "er", "nach", "Wes\u00b7ten", "hin", "und", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zusammenfassend die Geschicke", "tokens": ["Zu\u00b7sam\u00b7men\u00b7fas\u00b7send", "die", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Jahre, die vor\u00fcber sind:", "tokens": ["Der", "Jah\u00b7re", ",", "die", "vor\u00b7\u00fc\u00b7ber", "sind", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort mit dem Morgenantlitz wendet", "tokens": ["Dort", "mit", "dem", "Mor\u00b7gen\u00b7ant\u00b7litz", "wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Er sich erwartungsvoll zum Ost,", "tokens": ["Er", "sich", "er\u00b7war\u00b7tungs\u00b7voll", "zum", "Ost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem, was von dort die Zukunft sendet,", "tokens": ["Dem", ",", "was", "von", "dort", "die", "Zu\u00b7kunft", "sen\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Entgegenblickend still getrost.", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7bli\u00b7ckend", "still", "ge\u00b7trost", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann, w\u00e4hrend in des Engels Mienen", "tokens": ["Dann", ",", "w\u00e4h\u00b7rend", "in", "des", "En\u00b7gels", "Mie\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Abendrot stets matter gl\u00fcht,", "tokens": ["Das", "A\u00b7ben\u00b7drot", "stets", "mat\u00b7ter", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer heller ist erschienen", "tokens": ["Und", "im\u00b7mer", "hel\u00b7ler", "ist", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ihnen, was wie Morgen spr\u00fcht;", "tokens": ["Auf", "ih\u00b7nen", ",", "was", "wie", "Mor\u00b7gen", "spr\u00fcht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nimmt er das Kind aus seiner Wiegen,", "tokens": ["Nimmt", "er", "das", "Kind", "aus", "sei\u00b7ner", "Wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und aus des Engels Auge bricht", "tokens": ["Und", "aus", "des", "En\u00b7gels", "Au\u00b7ge", "bricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Thr\u00e4ne, die darein gestiegen,", "tokens": ["Die", "Thr\u00e4\u00b7ne", ",", "die", "da\u00b7rein", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Indes sein Mund zum Kindlein spricht:", "tokens": ["In\u00b7des", "sein", "Mund", "zum", "Kin\u00b7dlein", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O du, der j\u00fcngste jetzt der S\u00f6hne,", "tokens": ["O", "du", ",", "der", "j\u00fcngs\u00b7te", "jetzt", "der", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die unsre Mutter Zeit gebar,", "tokens": ["Die", "uns\u00b7re", "Mut\u00b7ter", "Zeit", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei mir in deiner Unschuld Sch\u00f6ne,", "tokens": ["Sei", "mir", "in", "dei\u00b7ner", "Un\u00b7schuld", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei mir gegr\u00fc\u00dft, du junges Jahr!", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "du", "jun\u00b7ges", "Jahr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schon manches hab' ich aus der Wiege", "tokens": ["Schon", "man\u00b7ches", "hab'", "ich", "aus", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Genommen und zu Grab gelegt,", "tokens": ["Ge\u00b7nom\u00b7men", "und", "zu", "Grab", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Damit ans Licht ein andres stiege,", "tokens": ["Da\u00b7mit", "ans", "Licht", "ein", "and\u00b7res", "stie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und s\u00fc\u00dfe Hoffnung stets gehegt:", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "Hoff\u00b7nung", "stets", "ge\u00b7hegt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Hoffnung aller Welt und meine,", "tokens": ["Die", "Hoff\u00b7nung", "al\u00b7ler", "Welt", "und", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die jedem Jahr entgegent\u00f6nt,", "tokens": ["Die", "je\u00b7dem", "Jahr", "ent\u00b7ge\u00b7gen\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob endlich einmal das erscheine,", "tokens": ["Ob", "end\u00b7lich", "ein\u00b7mal", "das", "er\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von welchem sei das Werk gekr\u00f6nt,", "tokens": ["Von", "wel\u00b7chem", "sei", "das", "Werk", "ge\u00b7kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob endlich das sei abgebrochen,", "tokens": ["Ob", "end\u00b7lich", "das", "sei", "ab\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von welchem uns erf\u00fcllet sei,", "tokens": ["Von", "wel\u00b7chem", "uns", "er\u00b7f\u00fcl\u00b7let", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was von den vor'gen ward versprochen?", "tokens": ["Was", "von", "den", "vor'\u00b7gen", "ward", "ver\u00b7spro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn du das bist, so sag' mir's frei.", "tokens": ["Wenn", "du", "das", "bist", ",", "so", "sag'", "mir's", "frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VAFIN", "$,", "ADV", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich kann durch meiner R\u00fchrung Z\u00e4hren", "tokens": ["Ich", "kann", "durch", "mei\u00b7ner", "R\u00fch\u00b7rung", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht deine Z\u00fcge deutlich sehn;", "tokens": ["Nicht", "dei\u00b7ne", "Z\u00fc\u00b7ge", "deut\u00b7lich", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein L\u00e4cheln scheint sie zu verkl\u00e4ren:", "tokens": ["Ein", "L\u00e4\u00b7cheln", "scheint", "sie", "zu", "ver\u00b7kl\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sprich, soll durch dich uns Heil geschehn?", "tokens": ["Sprich", ",", "soll", "durch", "dich", "uns", "Heil", "ge\u00b7schehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VMFIN", "APPR", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Willst du nicht wieder t\u00e4uschend schwinden,", "tokens": ["Willst", "du", "nicht", "wie\u00b7der", "t\u00e4u\u00b7schend", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie vor dir deiner Br\u00fcder g'nug,", "tokens": ["Wie", "vor", "dir", "dei\u00b7ner", "Br\u00fc\u00b7der", "g'\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Da\u00df wir den Glauben wieder finden,", "tokens": ["Da\u00df", "wir", "den", "Glau\u00b7ben", "wie\u00b7der", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den uns geraubt der Zeiten Lug?", "tokens": ["Den", "uns", "ge\u00b7raubt", "der", "Zei\u00b7ten", "Lug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Willst du den bangen Kn\u00e4ul entwirren,", "tokens": ["Willst", "du", "den", "ban\u00b7gen", "Kn\u00e4ul", "ent\u00b7wir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der um der Menschheit Brust sich schlang,", "tokens": ["Der", "um", "der", "Menschheit", "Brust", "sich", "schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und l\u00f6sen ird'scher Zwietracht Klirren", "tokens": ["Und", "l\u00f6\u00b7sen", "ird'\u00b7scher", "Zwiet\u00b7racht", "Klir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf in harmon'schen Sph\u00e4renklang?", "tokens": ["Auf", "in", "har\u00b7mon'\u00b7schen", "Sph\u00e4\u00b7ren\u00b7klang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Auff\u00fchren aus bewegten Stoffen", "tokens": ["Auf\u00b7f\u00fch\u00b7ren", "aus", "be\u00b7weg\u00b7ten", "Stof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Bau, der auf sich selbst kann ruhn?", "tokens": ["Den", "Bau", ",", "der", "auf", "sich", "selbst", "kann", "ruhn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PRF", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kurz, was wir w\u00fcnschen, was wir hoffen,", "tokens": ["Kurz", ",", "was", "wir", "w\u00fcn\u00b7schen", ",", "was", "wir", "hof\u00b7fen", ","]