{"textgrid.poem.32900": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Zephyr, der das stille Sehnen", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Zephyr, der das stille Sehnen", "tokens": ["Du", "Ze\u00b7phyr", ",", "der", "das", "stil\u00b7le", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Sch\u00e4fers, M\u00e4dchen oft gesagt,", "tokens": ["Des", "Sch\u00e4\u00b7fers", ",", "M\u00e4d\u00b7chen", "oft", "ge\u00b7sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er, zu bl\u00f6de bey der Sch\u00f6nen,", "tokens": ["Wenn", "er", ",", "zu", "bl\u00f6\u00b7de", "bey", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur B\u00fcschen seinen Kummer klagt.", "tokens": ["Nur", "B\u00fc\u00b7schen", "sei\u00b7nen", "Kum\u00b7mer", "klagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Findst du dem Schlummer \u00fcberlassen", "tokens": ["Findst", "du", "dem", "Schlum\u00b7mer", "\u00fc\u00b7ber\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Selinden, die die Lieb emp\u00f6rt,", "tokens": ["Se\u00b7lin\u00b7den", ",", "die", "die", "Lieb", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lispl' ihr doch zu: ich w\u00fcrd erblassen,", "tokens": ["Lispl'", "ihr", "doch", "zu", ":", "ich", "w\u00fcrd", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie mein W\u00fcnschen nicht erh\u00f6rt.", "tokens": ["Wenn", "sie", "mein", "W\u00fcn\u00b7schen", "nicht", "er\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch fragt sie nichts nach meinen Klagen,", "tokens": ["Doch", "fragt", "sie", "nichts", "nach", "mei\u00b7nen", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lacht sie wohl noch zu der Gefahr;", "tokens": ["Lacht", "sie", "wohl", "noch", "zu", "der", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "So kannst du im Vertraun ihr sagen:", "tokens": ["So", "kannst", "du", "im", "Ver\u00b7traun", "ihr", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was du erst sagtest, sey nicht wahr.", "tokens": ["Was", "du", "erst", "sag\u00b7test", ",", "sey", "nicht", "wahr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du Zephyr, der das stille Sehnen", "tokens": ["Du", "Ze\u00b7phyr", ",", "der", "das", "stil\u00b7le", "Seh\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Sch\u00e4fers, M\u00e4dchen oft gesagt,", "tokens": ["Des", "Sch\u00e4\u00b7fers", ",", "M\u00e4d\u00b7chen", "oft", "ge\u00b7sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn er, zu bl\u00f6de bey der Sch\u00f6nen,", "tokens": ["Wenn", "er", ",", "zu", "bl\u00f6\u00b7de", "bey", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur B\u00fcschen seinen Kummer klagt.", "tokens": ["Nur", "B\u00fc\u00b7schen", "sei\u00b7nen", "Kum\u00b7mer", "klagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Findst du dem Schlummer \u00fcberlassen", "tokens": ["Findst", "du", "dem", "Schlum\u00b7mer", "\u00fc\u00b7ber\u00b7las\u