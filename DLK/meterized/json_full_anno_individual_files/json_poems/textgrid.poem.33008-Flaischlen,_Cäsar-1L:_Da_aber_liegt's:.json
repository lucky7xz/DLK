{"textgrid.poem.33008": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da aber liegt's:", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da aber liegt's:", "tokens": ["Da", "a\u00b7ber", "liegt's", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "der eine biegt's,", "tokens": ["der", "ei\u00b7ne", "biegt's", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ART", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "der andre bricht's!", "tokens": ["der", "and\u00b7re", "bricht's", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "la\u00df nur das Schwert nicht in die Scheiderosten,", "tokens": ["la\u00df", "nur", "das", "Schwert", "nicht", "in", "die", "Schei\u00b7de\u00b7ros\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "den freien Mut des freien Manns!", "tokens": ["den", "frei\u00b7en", "Mut", "des", "frei\u00b7en", "Manns", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer etwas will, der kann's ...", "tokens": ["Wer", "et\u00b7was", "will", ",", "der", "kann's", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "$,", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "der kann's!", "tokens": ["der", "kann's", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "und w\u00fcrd es eine Welt ihn kosten!", "tokens": ["und", "w\u00fcrd", "es", "ei\u00b7ne", "Welt", "ihn", "kos\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was du vor dir bist, nur entscheidet!", "tokens": ["Was", "du", "vor", "dir", "bist", ",", "nur", "ent\u00b7schei\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VAFIN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der Spruch der Welt, du lieber Gott,", "tokens": ["der", "Spruch", "der", "Welt", ",", "du", "lie\u00b7ber", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "zerrt heute hist und morgen hott,", "tokens": ["zerrt", "heu\u00b7te", "hist", "und", "mor\u00b7gen", "hott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und wenn sie dich mit Purpur kleidet ...", "tokens": ["und", "wenn", "sie", "dich", "mit", "Pur\u00b7pur", "klei\u00b7det", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "f\u00fcr das, was einer litt und leidet,", "tokens": ["f\u00fcr", "das", ",", "was", "ei\u00b7ner", "litt", "und", "lei\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "ist all ihr Purpur Fastnachts-Spott!", "tokens": ["ist", "all", "ihr", "Pur\u00b7pur", "Fast\u00b7nachts\u00b7S\u00b7pott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was du vor dir bist, nur entscheidet", "tokens": ["Was", "du", "vor", "dir", "bist", ",", "nur", "ent\u00b7schei\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPER", "VAFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und wird des Ganzen innerer Kern ...", "tokens": ["und", "wird", "des", "Gan\u00b7zen", "in\u00b7ne\u00b7rer", "Kern", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "nicht Gl\u00fcck, nicht Zufall oder Stern!", "tokens": ["nicht", "Gl\u00fcck", ",", "nicht", "Zu\u00b7fall", "o\u00b7der", "Stern", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und was dann auch dagegen streitet,", "tokens": ["und", "was", "dann", "auch", "da\u00b7ge\u00b7gen", "strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "der Frei