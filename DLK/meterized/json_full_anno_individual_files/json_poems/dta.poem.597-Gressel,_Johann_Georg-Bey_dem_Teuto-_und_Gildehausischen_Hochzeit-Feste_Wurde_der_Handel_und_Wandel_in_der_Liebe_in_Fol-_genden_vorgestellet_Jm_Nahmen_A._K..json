{"dta.poem.597": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Bey dem  \n Teuto- und Gildehausischen  \n Hochzeit-Feste  \n Wurde der Handel und Wandel in der Liebe in Fol-  \n genden vorgestellet/  \n  Jm Nahmen   A. K.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ist die Liebe nicht ein Handel/", "tokens": ["Ist", "die", "Lie\u00b7be", "nicht", "ein", "Han\u00b7del", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleicht sie nicht der Kauff mannschafft?", "tokens": ["Gleicht", "sie", "nicht", "der", "Kauff", "mann\u00b7schafft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja! und diesen sch\u00f6nen Wandel", "tokens": ["Ja", "!", "und", "die\u00b7sen", "sch\u00f6\u00b7nen", "Wan\u00b7del"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "KON", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treibet man mit solcher Krafft;", "tokens": ["Trei\u00b7bet", "man", "mit", "sol\u00b7cher", "Krafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df man Hertz um Hertz verkauffet/", "tokens": ["Da\u00df", "man", "Hertz", "um", "Hertz", "ver\u00b7kauf\u00b7fet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn ein jedes Menschen-Kind", "tokens": ["Denn", "ein", "je\u00b7des", "Men\u00b7schen\u00b7Kind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hin nach ihrer B\u00f6rse lauffet/", "tokens": ["Hin", "nach", "ih\u00b7rer", "B\u00f6r\u00b7se", "lauf\u00b7fet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo man tausend K\u00e4uffer findt.", "tokens": ["Wo", "man", "tau\u00b7send", "K\u00e4uf\u00b7fer", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Die Advis-Brief sind die Blicke/", "tokens": ["Die", "Ad\u00b7vis\u00b7Brief", "sind", "die", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und der Wechsel-Zettul geht", "tokens": ["Und", "der", "Wech\u00b7sel\u00b7Zet\u00b7tul", "geht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.11": {"text": "Selten mit ", "tokens": ["Sel\u00b7ten", "mit"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Was man redet/ das besteht.", "tokens": ["Was", "man", "re\u00b7det", "/", "das", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$(", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Wann ", "tokens": ["Wann"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Erst dahin es bringen kan/", "tokens": ["Erst", "da\u00b7hin", "es", "brin\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df er sich zu ihr darf finden", "tokens": ["Da\u00df", "er", "sich", "zu", "ihr", "darf", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Alsdenn geht der Handel an.", "tokens": ["Als\u00b7denn", "geht", "der", "Han\u00b7del", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Da\u00df er ihr getreu sich nennet/", "tokens": ["Da\u00df", "er", "ihr", "ge\u00b7treu", "sich", "nen\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "PRF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ist das erste Handlungs-Geld/", "tokens": ["Ist", "das", "ers\u00b7te", "Hand\u00b7lungs\u00b7Geld", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Wenn das keinen Mangel kennet/", "tokens": ["Wenn", "das", "kei\u00b7nen", "Man\u00b7gel", "ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Alsdenn ist es wohl bestellt;", "tokens": ["Als\u00b7denn", "ist", "es", "wohl", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Wenn es richtig wird befunden/", "tokens": ["Wenn", "es", "rich\u00b7tig", "wird", "be\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wenn es nicht den Werth verliehrt/", "tokens": ["Wenn", "es", "nicht", "den", "Werth", "ver\u00b7liehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Denn wird Hertz und Geist verbunden/", "tokens": ["Denn", "wird", "Hertz", "und", "Geist", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "KON", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Und der Handel fortgef\u00fchrt.", "tokens": ["Und", "der", "Han\u00b7del", "fort\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Giebt der Himmel dann Gedeyen", "tokens": ["Giebt", "der", "Him\u00b7mel", "dann", "Ge\u00b7de\u00b7yen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.26": {"text": "Da\u00df er heim sie f\u00fchren kan/", "tokens": ["Da\u00df", "er", "heim", "sie", "f\u00fch\u00b7ren", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "PPER", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Alsdenn geht es an ein Freuen/", "tokens": ["Als\u00b7denn", "geht", "es", "an", "ein", "Freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und die Zahl-Woch hebt sich an", "tokens": ["Und", "die", "Zahl\u00b7Woch", "hebt", "sich", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Alle N\u00e4chte in dem Bette", "tokens": ["Al\u00b7le", "N\u00e4ch\u00b7te", "in", "dem", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Ist der t\u00e4gliche ", "tokens": ["Ist", "der", "t\u00e4g\u00b7li\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.31": {"text": "Allda liebt man in die Wette/", "tokens": ["All\u00b7da", "liebt", "man", "in", "die", "Wet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Und vermehret den Gewinn.", "tokens": ["Und", "ver\u00b7meh\u00b7ret", "den", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "An der Zins wird nichts erlassen/", "tokens": ["An", "der", "Zins", "wird", "nichts", "er\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Noch viel minder ", "tokens": ["Noch", "viel", "min\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "Weil einjeder bester Maassen", "tokens": ["Weil", "ein\u00b7je\u00b7der", "bes\u00b7ter", "Maas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Seine Liebes-Schuld abf\u00fchrt.", "tokens": ["Sei\u00b7ne", "Lie\u00b7bes\u00b7Schuld", "ab\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.37": {"text": "So hat man viel zu ", "tokens": ["So", "hat", "man", "viel", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.38": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.39": {"text": "Will in keinem Ding ", "tokens": ["Will", "in", "kei\u00b7nem", "Ding"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.40": {"text": "Alles bringet er zum Stand.", "tokens": ["Al\u00b7les", "brin\u00b7get", "er", "zum", "Stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Nach so richtigem Belegen", "tokens": ["Nach", "so", "rich\u00b7ti\u00b7gem", "Be\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Kommt lebendiger Gewinn/", "tokens": ["Kommt", "le\u00b7ben\u00b7di\u00b7ger", "Ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Und der hochgew\u00fcnschte Seegen", "tokens": ["Und", "der", "hoch\u00b7ge\u00b7w\u00fcnschte", "See\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.44": {"text": "Zieht mit Wucher sich dahin/", "tokens": ["Zieht", "mit", "Wu\u00b7cher", "sich", "da\u00b7hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PRF", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.45": {"text": "Mag man denn nicht weiter handeln", "tokens": ["Mag", "man", "denn", "nicht", "wei\u00b7ter", "han\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "PTKNEG", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "So greifft man die Renten an/", "tokens": ["So", "greifft", "man", "die", "Ren\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "PTKVZ", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.47": {"text": "Und das angenehme Wandeln/", "tokens": ["Und", "das", "an\u00b7ge\u00b7neh\u00b7me", "Wan\u00b7deln", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Wird den Kindern eingethan.", "tokens": ["Wird", "den", "Kin\u00b7dern", "ein\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Jhr! ihr treu-verliebte Hertzen/", "tokens": ["Ihr", "!", "ihr", "treu\u00b7ver\u00b7lieb\u00b7te", "Hert\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Die ihr heute euch verbindt/", "tokens": ["Die", "ihr", "heu\u00b7te", "euch", "ver\u00b7bindt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Und bey ", "tokens": ["Und", "bey"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.52": {"text": "Euch zu solchen Handel findt/", "tokens": ["Euch", "zu", "sol\u00b7chen", "Han\u00b7del", "findt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Jhr/ ihr werdet schon erfahren/", "tokens": ["Ihr", "/", "ihr", "wer\u00b7det", "schon", "er\u00b7fah\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPER", "VAFIN", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Was mein Feder-Kiel hier setzt.", "tokens": ["Was", "mein", "Fe\u00b7der\u00b7Kiel", "hier", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Handelt wol mit denen Waaren/", "tokens": ["Han\u00b7delt", "wol", "mit", "de\u00b7nen", "Waa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PRELS", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Da mit ", "tokens": ["Da", "mit"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "++", "measure": "spondeus"}, "line.57": {"text": "Lasset den ", "tokens": ["Las\u00b7set", "den"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.58": {"text": "Doch erst nach drey Viertel Jahr", "tokens": ["Doch", "erst", "nach", "drey", "Vier\u00b7tel", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "CARD", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.59": {"text": "Alsdenn wird in allen St\u00fccken", "tokens": ["Als\u00b7denn", "wird", "in", "al\u00b7len", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "Eure Handlung offenbahr/", "tokens": ["Eu\u00b7re", "Hand\u00b7lung", "of\u00b7fen\u00b7bahr", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.61": {"text": "Himmel! segne Bud und Laden/", "tokens": ["Him\u00b7mel", "!", "seg\u00b7ne", "Bud", "und", "La\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Sch\u00fctze sie durch deine Gunst/", "tokens": ["Sch\u00fct\u00b7ze", "sie", "durch", "dei\u00b7ne", "Gunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.63": {"text": "Und verleihe viel ", "tokens": ["Und", "ver\u00b7lei\u00b7he", "viel"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.64": {"text": "Dieser keuschen Liebes-Brunst.", "tokens": ["Die\u00b7ser", "keu\u00b7schen", "Lie\u00b7bes\u00b7Brunst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}