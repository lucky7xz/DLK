{"dta.poem.4305": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Herbst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich fuhr im Herbst, um in demselben der Welt ver\u00e4nderte", "tokens": ["Ich", "fuhr", "im", "Herbst", ",", "um", "in", "dem\u00b7sel\u00b7ben", "der", "Welt", "ver\u00b7\u00e4n\u00b7der\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "KOUI", "APPR", "PDAT", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Gestalt,", "tokens": ["Ge\u00b7stalt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Mit Lust und Andacht, zu beschauen, vergn\u00fcgt nach meinem", "tokens": ["Mit", "Lust", "und", "An\u00b7dacht", ",", "zu", "be\u00b7schau\u00b7en", ",", "ver\u00b7gn\u00fcgt", "nach", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PTKZU", "VVINF", "$,", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "kleinen Wald.", "tokens": ["klei\u00b7nen", "Wald", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Ich sahe, wie der kalte Nord bereits mit Laub und Kraut", "tokens": ["Ich", "sa\u00b7he", ",", "wie", "der", "kal\u00b7te", "Nord", "be\u00b7reits", "mit", "Laub", "und", "Kraut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "gehandelt,", "tokens": ["ge\u00b7han\u00b7delt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Es schien der vormahls gr\u00fcne Wald nicht mehr derselbe,", "tokens": ["Es", "schien", "der", "vor\u00b7mahls", "gr\u00fc\u00b7ne", "Wald", "nicht", "mehr", "der\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADV", "ADJA", "NN", "PTKNEG", "ADV", "PDAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "ganz verwandelt,", "tokens": ["ganz", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Der gr\u00f6\u00dfte Theil der hohen Wipfel schien \u00fcberg\u00fcldet, ob-", "tokens": ["Der", "gr\u00f6\u00df\u00b7te", "Theil", "der", "ho\u00b7hen", "Wip\u00b7fel", "schien", "\u00fc\u00b7berg\u00b7\u00fcl\u00b7det", ",", "ob"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "VVPP", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "gleich gr\u00fcn", "tokens": ["gleich", "gr\u00fcn"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Indem noch nicht verbleichtes Laub noch hie und da dazwi-", "tokens": ["In\u00b7dem", "noch", "nicht", "ver\u00b7bleich\u00b7tes", "Laub", "noch", "hie", "und", "da", "da\u00b7zwi"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PTKNEG", "ADJA", "NN", "ADV", "ADV", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "schen schien.", "tokens": ["schen", "schien", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Hier sah man gelb auf gr\u00fcnen Stellen, und gr\u00fcne dort auf", "tokens": ["Hier", "sah", "man", "gelb", "auf", "gr\u00fc\u00b7nen", "Stel\u00b7len", ",", "und", "gr\u00fc\u00b7ne", "dort", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "gelben sch\u00f6n,", "tokens": ["gel\u00b7ben", "sch\u00f6n", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Dort in der bunten Nachbarschaft auch roht auf gelb- und", "tokens": ["Dort", "in", "der", "bun\u00b7ten", "Nach\u00b7bar\u00b7schaft", "auch", "roht", "auf", "gelb", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "APPR", "TRUNC", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "gr\u00fcnen steh'n.", "tokens": ["gr\u00fc\u00b7nen", "steh'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["ADJA", "VVFIN", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Viel\u2019 deckt\u2019 ein Glanz, als ein Rubien, viel\u2019 Bl\u00e4tter waren", "tokens": ["Viel'", "deckt'", "ein", "Glanz", ",", "als", "ein", "Ru\u00b7bi\u00b7en", ",", "viel'", "Bl\u00e4t\u00b7ter", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "$,", "PIAT", "NN", "VAFIN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "gelblich-bleich,", "tokens": ["gelb\u00b7lich\u00b7bleich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Viel\u2019 waren roht und gr\u00fcn gesprengt, viel\u2019 roht und gr\u00fcn", "tokens": ["Viel'", "wa\u00b7ren", "roht", "und", "gr\u00fcn", "ge\u00b7sprengt", ",", "viel'", "roht", "und", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJD", "KON", "ADJD", "VVPP", "$,", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "und gelb zugleich.", "tokens": ["und", "gelb", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Die meisten waren nicht so sch\u00f6n, entf\u00e4rbet, dunkel, falb", "tokens": ["Die", "meis\u00b7ten", "wa\u00b7ren", "nicht", "so", "sch\u00f6n", ",", "ent\u00b7f\u00e4r\u00b7bet", ",", "dun\u00b7kel", ",", "falb"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ART", "PIS", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "VVFIN", "$,", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "und braun,", "tokens": ["und", "braun", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Doch war der andern bunter Glanz um desto lieblicher zu", "tokens": ["Doch", "war", "der", "an\u00b7dern", "bun\u00b7ter", "Glanz", "um", "des\u00b7to", "lieb\u00b7li\u00b7cher", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "ADJA", "NN", "APPR", "ADV", "ADJD", "PTKZU"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.24": {"text": "schau'n,", "tokens": ["schau'n", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Jm Gegensatz der Nachbarschaft, indem er, als auf dunklem", "tokens": ["Jm", "Ge\u00b7gen\u00b7satz", "der", "Nach\u00b7bar\u00b7schaft", ",", "in\u00b7dem", "er", ",", "als", "auf", "dunk\u00b7lem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "$,", "KOUS", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.26": {"text": "Grunde,", "tokens": ["Grun\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Als w\u00e4r\u2019 es recht mit Flei\u00df geordnet, um desto mehr erhoben", "tokens": ["Als", "w\u00e4r'", "es", "recht", "mit", "Flei\u00df", "ge\u00b7ord\u00b7net", ",", "um", "des\u00b7to", "mehr", "er\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,", "KOUI", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "stunde.", "tokens": ["stun\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Auf dunkler Felder weiten Fl\u00e4chen,", "tokens": ["Auf", "dunk\u00b7ler", "Fel\u00b7der", "wei\u00b7ten", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Worauf, doch sparsam, hier und dar schon wieder gr\u00fcne", "tokens": ["Wo\u00b7rauf", ",", "doch", "spar\u00b7sam", ",", "hier", "und", "dar", "schon", "wie\u00b7der", "gr\u00fc\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "$,", "ADV", "ADJD", "$,", "ADV", "KON", "PAV", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Spitzen brechen,", "tokens": ["Spit\u00b7zen", "bre\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Erblickt\u2019 man gelbes Schilf in Gr\u00e4ben, auch mancher", "tokens": ["Er\u00b7blickt'", "man", "gel\u00b7bes", "Schilf", "in", "Gr\u00e4\u00b7ben", ",", "auch", "man\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "ADJA", "NN", "APPR", "NN", "$,", "ADV", "PIAT"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Dorn-Strauch l\u00e4\u00dft itzt sch\u00f6n,", "tokens": ["Dorn\u00b7Strauch", "l\u00e4\u00dft", "itzt", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Nebst seiner rohten Beeren Funkeln, die Menge bunter", "tokens": ["Nebst", "sei\u00b7ner", "roh\u00b7ten", "Bee\u00b7ren", "Fun\u00b7keln", ",", "die", "Men\u00b7ge", "bun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Bl\u00e4tter seh'n.", "tokens": ["Bl\u00e4t\u00b7ter", "seh'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["NN", "VVFIN", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Doch zeigt er auch die Stacheln schon, da er schon mehren-", "tokens": ["Doch", "zeigt", "er", "auch", "die", "Sta\u00b7cheln", "schon", ",", "da", "er", "schon", "meh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "theils entlaubt,", "tokens": ["theils", "ent\u00b7laubt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.10": {"text": "Und seines Schmucks, der sie verh\u00fcllte, durch Nebel, Wind", "tokens": ["Und", "sei\u00b7nes", "Schmucks", ",", "der", "sie", "ver\u00b7h\u00fcll\u00b7te", ",", "durch", "Ne\u00b7bel", ",", "Wind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "und Frost beraubt.", "tokens": ["und", "Frost", "be\u00b7raubt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Da, wo mit der bedeckten Luft sich der Gesichts-Kreis", "tokens": ["Da", ",", "wo", "mit", "der", "be\u00b7deck\u00b7ten", "Luft", "sich", "der", "Ge\u00b7sichts\u00b7Kreis"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "ART", "ADJA", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-++++", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "sanft verbindet,", "tokens": ["sanft", "ver\u00b7bin\u00b7det", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Verliert er sich im klaren Duft, so da\u00df man kaum die Grenzen", "tokens": ["Ver\u00b7liert", "er", "sich", "im", "kla\u00b7ren", "Duft", ",", "so", "da\u00df", "man", "kaum", "die", "Gren\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$,", "ADV", "KOUS", "PIS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "findet.", "tokens": ["fin\u00b7det", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Es scheint, ob ruhe die Natur, und als wenn eine sanfte", "tokens": ["Es", "scheint", ",", "ob", "ru\u00b7he", "die", "Na\u00b7tur", ",", "und", "als", "wenn", "ei\u00b7ne", "sanf\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "ART", "NN", "$,", "KON", "KOKOM", "KOUS", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Stille,", "tokens": ["Stil\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "So da\u00df sie uns fast sichtbar ist, das Feld, so weit man sieht,", "tokens": ["So", "da\u00df", "sie", "uns", "fast", "sicht\u00b7bar", "ist", ",", "das", "Feld", ",", "so", "weit", "man", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "ADV", "ADJD", "VAFIN", "$,", "ART", "NN", "$,", "ADV", "ADJD", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "erf\u00fclle.", "tokens": ["er\u00b7f\u00fcl\u00b7le", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Die Fluht, nachdem die Wasser-Linsen, und alles auf den", "tokens": ["Die", "Fluht", ",", "nach\u00b7dem", "die", "Was\u00b7ser\u00b7Lin\u00b7sen", ",", "und", "al\u00b7les", "auf", "den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "$,", "KON", "PIS", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Grund gefallen,", "tokens": ["Grund", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was erst auf ihrer Fl\u00e4che schwamm, ist itzt im Herbst so rein,", "tokens": ["Was", "erst", "auf", "ih\u00b7rer", "Fl\u00e4\u00b7che", "schwamm", ",", "ist", "itzt", "im", "Herbst", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,", "VAFIN", "ADV", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "so klar,", "tokens": ["so", "klar", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Als es in keiner Zeit vom Jahr.", "tokens": ["Als", "es", "in", "kei\u00b7ner", "Zeit", "vom", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es scheint itzt mehr als sonst poliert, und gleicht itzt reinen", "tokens": ["Es", "scheint", "itzt", "mehr", "als", "sonst", "po\u00b7liert", ",", "und"