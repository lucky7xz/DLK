{"textgrid.poem.59844": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe, die mich lieben,", "tokens": ["Ich", "lie\u00b7be", ",", "die", "mich", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hasse, die mich hassen \u2013", "tokens": ["Und", "has\u00b7se", ",", "die", "mich", "has\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So hab' ich's stets getrieben", "tokens": ["So", "hab'", "ich's", "stets", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und will davon nicht lassen.", "tokens": ["Und", "will", "da\u00b7von", "nicht", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dem Mann von Kraft und Mute", "tokens": ["Dem", "Mann", "von", "Kraft", "und", "Mu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gilt dieses als das Rechte:", "tokens": ["Gilt", "die\u00b7ses", "als", "das", "Rech\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Gute f\u00fcr das Gute,", "tokens": ["Das", "Gu\u00b7te", "f\u00fcr", "das", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Schlechte f\u00fcr das Schlechte!", "tokens": ["Das", "Schlech\u00b7te", "f\u00fcr", "das", "Schlech\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man liebt, was gut und wacker,", "tokens": ["Man", "liebt", ",", "was", "gut", "und", "wa\u00b7cker", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Man kos't der Sch\u00f6nheit Wange,", "tokens": ["Man", "kos't", "der", "Sch\u00f6n\u00b7heit", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man pflegt die Saat im Acker \u2013", "tokens": ["Man", "pflegt", "die", "Saat", "im", "A\u00b7cker", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch man zertritt die Schlange.", "tokens": ["Doch", "man", "zer\u00b7tritt", "die", "Schlan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Unbill an Ehr' und Leibe", "tokens": ["Un\u00b7bill", "an", "Ehr'", "und", "Lei\u00b7be"], "to