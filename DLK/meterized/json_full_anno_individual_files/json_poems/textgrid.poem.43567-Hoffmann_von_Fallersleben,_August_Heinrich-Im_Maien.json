{"textgrid.poem.43567": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Im Maien", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Maien, im Maien, da ist es so sch\u00f6n,", "tokens": ["Im", "Mai\u00b7en", ",", "im", "Mai\u00b7en", ",", "da", "ist", "es", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da bl\u00fcht es im Thal und da gr\u00fcnt's auf den H\u00f6h'n;", "tokens": ["Da", "bl\u00fcht", "es", "im", "Thal", "und", "da", "gr\u00fcnt's", "auf", "den", "H\u00f6h'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "KON", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Es singen die V\u00f6gel in jubelnder Lust,", "tokens": ["Es", "sin\u00b7gen", "die", "V\u00f6\u00b7gel", "in", "ju\u00b7beln\u00b7der", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Was lebet, das ist sich des Lebens bewu\u00dft.", "tokens": ["Was", "le\u00b7bet", ",", "das", "ist", "sich", "des", "Le\u00b7bens", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PDS", "VAFIN", "PRF", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amp