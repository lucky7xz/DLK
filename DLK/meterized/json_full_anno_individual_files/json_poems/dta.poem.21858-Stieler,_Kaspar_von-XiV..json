{"dta.poem.21858": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XiV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Durch Schwachheit ist mir meine St\u00e4rte", "tokens": ["Durch", "Schwach\u00b7heit", "ist", "mir", "mei\u00b7ne", "St\u00e4r\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "durch Schwachheit ward ich meiner Krafft", "tokens": ["durch", "Schwach\u00b7heit", "ward", "ich", "mei\u00b7ner", "Krafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nu bin ich durch die Schw\u00e4che worden", "tokens": ["Nu", "bin", "ich", "durch", "die", "Schw\u00e4\u00b7che", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "VAPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "doch l\u00e4\u00dft au\u00df Schwachheit meine schw\u00e4-", "tokens": ["doch", "l\u00e4\u00dft", "au\u00df", "Schwach\u00b7heit", "mei\u00b7ne", "schw\u00e4"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}