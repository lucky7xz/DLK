{"dta.poem.1231": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du w\u00fcster Ort/ an welchen mich verleiten", "tokens": ["Du", "w\u00fcs\u00b7ter", "Ort", "/", "an", "wel\u00b7chen", "mich", "ver\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$(", "APPR", "PWAT", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Climenens Grausamkeiten/", "tokens": ["Cli\u00b7me\u00b7nens", "Grau\u00b7sam\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hier ingeheim zu suchen Grab und Tod/", "tokens": ["Hier", "in\u00b7ge\u00b7heim", "zu", "su\u00b7chen", "Grab", "und", "Tod", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dir klag ich meine Noth:", "tokens": ["Dir", "klag", "ich", "mei\u00b7ne", "Noth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Leiden ist zu gro\u00df es hier nicht auszubreiten/", "tokens": ["Mein", "Lei\u00b7den", "ist", "zu", "gro\u00df", "es", "hier", "nicht", "aus\u00b7zu\u00b7brei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein stummes Holtz wird mich darum nicht machen roth.", "tokens": ["Dein", "stum\u00b7mes", "Holtz", "wird", "mich", "da\u00b7rum", "nicht", "ma\u00b7chen", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PRF", "PAV", "PTKNEG", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Hertze lebt in Hoffen und in Sorgen/", "tokens": ["Mein", "Hert\u00b7ze", "lebt", "in", "Hof\u00b7fen", "und", "in", "Sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dem zu jenem Morgen/", "tokens": ["Von", "dem", "zu", "je\u00b7nem", "Mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich suche Ruh/ und wei\u00df nicht wo/ noch wie/", "tokens": ["Ich", "su\u00b7che", "Ruh", "/", "und", "wei\u00df", "nicht", "wo", "/", "noch", "wie", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "KON", "VVFIN", "PTKNEG", "PWAV", "$(", "ADV", "KOKOM", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sey linder weder sie/", "tokens": ["Sey", "lin\u00b7der", "we\u00b7der", "sie", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Halt mich f\u00fcr ihrem Ha\u00df auff eine Zeit verborgen/", "tokens": ["Halt", "mich", "f\u00fcr", "ih\u00b7rem", "Ha\u00df", "auff", "ei\u00b7ne", "Zeit", "ver\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sey Zeuge wo ich bin/ und doch verrath mich nie.", "tokens": ["Sey", "Zeu\u00b7ge", "wo", "ich", "bin", "/", "und", "doch", "ver\u00b7rath", "mich", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PWAV", "PPER", "VAFIN", "$(", "KON", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}