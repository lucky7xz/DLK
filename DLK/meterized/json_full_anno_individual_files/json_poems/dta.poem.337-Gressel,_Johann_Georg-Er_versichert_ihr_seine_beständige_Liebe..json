{"dta.poem.337": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er versichert ihr seine best\u00e4ndige Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Fesselt mich ihr sch\u00f6nen Augen", "tokens": ["Fes\u00b7selt", "mich", "ihr", "sch\u00f6\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich bin schon damit vergn\u00fcgt.", "tokens": ["Ich", "bin", "schon", "da\u00b7mit", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich begehr mich nicht zu retten", "tokens": ["Ich", "be\u00b7gehr", "mich", "nicht", "zu", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den angenehmen Ketten", "tokens": ["Von", "den", "an\u00b7ge\u00b7neh\u00b7men", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jhre Last gar leichte wiegt.", "tokens": ["Ih\u00b7re", "Last", "gar", "leich\u00b7te", "wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fesselt mich ihr sch\u00f6nen Augen", "tokens": ["Fes\u00b7selt", "mich", "ihr", "sch\u00f6\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich bin schon damit vergn\u00fcgt/", "tokens": ["Ich", "bin", "schon", "da\u00b7mit", "ver\u00b7gn\u00fcgt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "VVPP", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Sch\u00f6nheit hat mich \u00fcberwunden", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "hat", "mich", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will auch gern ihr Sclave seyn/", "tokens": ["Ich", "will", "auch", "gern", "ihr", "Scla\u00b7ve", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo man mit angenehmen Blicken", "tokens": ["Wo", "man", "mit", "an\u00b7ge\u00b7neh\u00b7men", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Angeschlossenen pflegt zu erquicken/", "tokens": ["Die", "An\u00b7ge\u00b7schlos\u00b7se\u00b7nen", "pflegt", "zu", "er\u00b7qui\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da klagt man \u00fcber keine Pein/", "tokens": ["Da", "klagt", "man", "\u00fc\u00b7ber", "kei\u00b7ne", "Pein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die rechte Freyheit wird gefunden", "tokens": ["Die", "rech\u00b7te", "Frey\u00b7heit", "wird", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn man ein solch Gef\u00e4ngni\u00df kriegt", "tokens": ["Wenn", "man", "ein", "solch", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", "kriegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "PIAT", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Da man an statt den Stein", "tokens": ["Da", "man", "an", "statt", "den", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "An einer sch\u00f6nen Brust gefesselt liegt.", "tokens": ["An", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Brust", "ge\u00b7fes\u00b7selt", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Fesseln will ich ewig tragen", "tokens": ["Die", "Fes\u00b7seln", "will", "ich", "e\u00b7wig", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie fallen mir niemahls zu schwehr/", "tokens": ["Sie", "fal\u00b7len", "mir", "nie\u00b7mahls", "zu", "schwehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich freu mich ob den s\u00fcssen Banden/", "tokens": ["Ich", "freu", "mich", "ob", "den", "s\u00fcs\u00b7sen", "Ban\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOUS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die in der treuen Brust verhanden.", "tokens": ["Die", "in", "der", "treu\u00b7en", "Brust", "ver\u00b7han\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn diese stammen von dir her.", "tokens": ["Denn", "die\u00b7se", "stam\u00b7men", "von", "dir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}