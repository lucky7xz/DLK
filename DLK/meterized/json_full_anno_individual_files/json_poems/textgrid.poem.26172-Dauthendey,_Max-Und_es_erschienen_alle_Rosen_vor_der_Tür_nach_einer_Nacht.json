{"textgrid.poem.26172": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Und es erschienen alle Rosen vor der T\u00fcr nach einer Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und es erschienen alle Rosen vor der T\u00fcr nach einer Nacht,", "tokens": ["Und", "es", "er\u00b7schie\u00b7nen", "al\u00b7le", "Ro\u00b7sen", "vor", "der", "T\u00fcr", "nach", "ei\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Es hat sie ein Gedanke, ein einziger von dir, zur Welt gebracht,", "tokens": ["Es", "hat", "sie", "ein", "Ge\u00b7dan\u00b7ke", ",", "ein", "ein\u00b7zi\u00b7ger", "von", "dir", ",", "zur", "Welt", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "$,", "ART", "ADJA", "APPR", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Du fragtest nicht, hast l\u00e4ssig nur ihn vor dir hingedacht.", "tokens": ["Du", "frag\u00b7test", "nicht", ",", "hast", "l\u00e4s\u00b7sig", "nur", "ihn", "vor", "dir", "hin\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VAFIN", "ADJD", "ADV", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Du hattest \u00fcberm\u00fctig Sehnsucht nach der Rose Lust und G\u00f6tterpracht,", "tokens": ["Du", "hat\u00b7test", "\u00fc\u00b7berm\u00b7\u00fc\u00b7tig", "Sehn\u00b7sucht", "nach", "der", "Ro\u00b7se", "Lust", "und", "G\u00f6t\u00b7ter\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Schwerbl\u00fctig sind dir alle purpurnen und k\u00f6niglichen Knospen unbewu\u00dft erwacht.", "tokens": ["Schwer\u00b7bl\u00fc\u00b7tig", "sind", "dir", "al\u00b7le", "pur\u00b7pur\u00b7nen", "und", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Knos\u00b7pen", "un\u00b7be\u00b7wu\u00dft", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PIAT", "ADJA", "KON", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "+--+-+-++--+-+-+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.6": {"text": "Sie f\u00fcllen Reihen kleiner B\u00e4ume vor der T\u00fcr und sind rund aufgequollen,", "tokens": ["Sie", "f\u00fcl\u00b7len", "Rei\u00b7hen", "klei\u00b7ner", "B\u00e4u\u00b7me", "vor", "der", "T\u00fcr", "und", "sind", "rund", "auf\u00b7ge\u00b7quol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJA", "NN", "APPR", "ART", "NN", "KON", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+++-+-", "measure": "unknown.measure.octa.plus"}, "line.7": {"text": "Als ob sie wie begl\u00fcckte Lippen heimliche Kosenamen nennen wollen,", "tokens": ["Als", "ob", "sie", "wie", "be\u00b7gl\u00fcck\u00b7te", "Lip\u00b7pen", "heim\u00b7li\u00b7che", "Ko\u00b7se\u00b7na\u00b7men", "nen\u00b7nen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "KOKOM", "ADJA", "NN", "ADJA", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+--+--+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Anbetend sitzen sie vor deinem Zimmer, so wie ein still verliebter Schwarm.", "tokens": ["An\u00b7be\u00b7tend", "sit\u00b7zen", "sie", "vor", "dei\u00b7nem", "Zim\u00b7mer", ",", "so", "wie", "ein", "still", "ver\u00b7lieb\u00b7ter", "Schwarm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "KOKOM", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.invert"}, "line.9": {"text": "O, \u00f6ffne, immer wie f\u00fcr deine Rosen, f\u00fcr meine Inbrunst deinen Arm.", "tokens": ["O", ",", "\u00f6ff\u00b7ne", ",", "im\u00b7mer", "wie", "f\u00fcr", "dei\u00b7ne", "Ro\u00b7sen", ",", "f\u00fcr", "mei\u00b7ne", "In\u00b7brunst", "dei\u00b7nen", "Arm", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "ADV", "KOKOM", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Und es erschienen alle Rosen vor der T\u00fcr nach einer Nacht,", "tokens": ["Und", "es", "er\u00b7schie\u00b7nen", "al\u00b7le", "Ro\u00b7sen", "vor", "der", "T\u00fcr", "nach", "ei\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Es hat sie ein Gedanke, ein einziger von dir, zur Welt gebrac