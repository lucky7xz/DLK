en", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Irthum ficht den selten an", "tokens": ["Ir\u00b7thum", "ficht", "den", "sel\u00b7ten", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Der die Zunge wei\u00df zu zwingen/", "tokens": ["Der", "die", "Zun\u00b7ge", "wei\u00df", "zu", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Allzeit war", "tokens": ["All\u00b7zeit", "war"], "token_info": ["word", "word"], "pos": ["ADV", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Tugendhafft das Schweigen.", "tokens": ["Tu\u00b7gend\u00b7hafft", "das", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}