{"dta.poem.21397": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gie\u00dft nicht zu viel Cajenne-Piment", "tokens": ["Gie\u00dft", "nicht", "zu", "viel", "Ca\u00b7jen\u00b7ne\u00b7Pi\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PTKA", "PIAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In Eure Mokturtelsuppen,", "tokens": ["In", "Eu\u00b7re", "Mok\u00b7tur\u00b7tel\u00b7sup\u00b7pen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auch Eure Karpfen sind Euch nicht gesund,", "tokens": ["Auch", "Eu\u00b7re", "Karp\u00b7fen", "sind", "Euch", "nicht", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr kocht sie so fett mit den Schuppen.", "tokens": ["Ihr", "kocht", "sie", "so", "fett", "mit", "den", "Schup\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Kalkuten schaden Euch nicht viel,", "tokens": ["Kal\u00b7ku\u00b7ten", "scha\u00b7den", "Euch", "nicht", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch h\u00fctet Euch vor der T\u00fccke", "tokens": ["Doch", "h\u00fc\u00b7tet", "Euch", "vor", "der", "T\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Des Vogels, der sein Ey gelegt", "tokens": ["Des", "Vo\u00b7gels", ",", "der", "sein", "Ey", "ge\u00b7legt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In des B\u00fcrgermeisters Per\u00fccke. \u2013 \u2013", "tokens": ["In", "des", "B\u00fcr\u00b7ger\u00b7meis\u00b7ters", "Pe\u00b7r\u00fc\u00b7cke", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "$(", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wer dieser fatale Vogel ist,", "tokens": ["Wer", "die\u00b7ser", "fa\u00b7ta\u00b7le", "Vo\u00b7gel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich brauch es Euch nicht zu sagen \u2013", "tokens": ["Ich", "brauch", "es", "Euch", "nicht", "zu", "sa\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKZU", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denk\u2019 ich an ihn, so dreht sich herum", "tokens": ["Denk'", "ich", "an", "ihn", ",", "so", "dreht", "sich", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "$,", "ADV", "VVFIN", "PRF", "APZR"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Das Essen in meinem Magen.", "tokens": ["Das", "Es\u00b7sen", "in", "mei\u00b7nem", "Ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}