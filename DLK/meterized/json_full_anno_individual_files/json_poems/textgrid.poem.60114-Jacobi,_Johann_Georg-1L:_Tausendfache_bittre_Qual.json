{"textgrid.poem.60114": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tausendfache bittre Qual", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tausendfache bittre Qual", "tokens": ["Tau\u00b7send\u00b7fa\u00b7che", "bitt\u00b7re", "Qual"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gabst du mir, o Liebe! Tausend Mahl", "tokens": ["Gabst", "du", "mir", ",", "o", "Lie\u00b7be", "!", "Tau\u00b7send", "Mahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "FM", "NN", "$.", "CARD", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Lohntest du mit Dornenkr\u00e4nzen", "tokens": ["Lohn\u00b7test", "du", "mit", "Dor\u00b7nen\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Treu; und jenes milde Gl\u00e4nzen", "tokens": ["Mei\u00b7ner", "Treu", ";", "und", "je\u00b7nes", "mil\u00b7de", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KON", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Deiner Fackel ward ein Donnerstrahl.", "tokens": ["Dei\u00b7ner", "Fa\u00b7ckel", "ward", "ein", "Don\u00b7ner\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Zarte Lauben sah ich dich entbl\u00e4ttern,", "tokens": ["Zar\u00b7te", "Lau\u00b7ben", "sah", "ich", "dich", "ent\u00b7bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Junge Spr\u00f6\u00dflinge zerschmettern;", "tokens": ["Jun\u00b7ge", "Spr\u00f6\u00df\u00b7lin\u00b7ge", "zer\u00b7schmet\u00b7tern", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Und in Abgrund sank das bl\u00fcthenreiche Thal.", "tokens": ["Und", "in", "Ab\u00b7grund", "sank", "das", "bl\u00fc\u00b7then\u00b7rei\u00b7che", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Dennoch zeuch, o Liebe! zeuch hernieder;", "tokens": ["Den\u00b7noch", "zeuch", ",", "o", "Lie\u00b7be", "!", "zeuch", "her\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "FM", "NN", "$.", "VVIMP", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Rufe mich ins Leben wieder", "tokens": ["Ru\u00b7fe", "mich", "ins", "Le\u00b7ben", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Aus der \u00f6den, kalten Todesnacht.", "tokens": ["Aus", "der", "\u00f6\u00b7den", ",", "kal\u00b7ten", "To\u00b7des\u00b7nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Liebe, die allein", "tokens": ["Lie\u00b7be", ",", "die", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Sonne, Mond und Sternenschein", "tokens": ["Son\u00b7ne", ",", "Mond", "und", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Uns zu Licht in unsrer W\u00fcste macht!", "tokens": ["Uns", "zu", "Licht", "in", "uns\u00b7rer", "W\u00fcs\u00b7te", "macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "Liebe, die allein", "tokens": ["Lie\u00b7be", ",", "die", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Aus den Wolken in den Hain", "tokens": ["Aus", "den", "Wol\u00b7ken", "in", "den", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Fr\u00fchlingswonn' herunter lacht!", "tokens": ["Fr\u00fch\u00b7lings\u00b7wonn'", "her\u00b7un\u00b7ter", "lacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APZR", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "O besuche mich in dieser Todesnacht.", "tokens": ["O", "be\u00b7su\u00b7che", "mich", "in", "die\u00b7ser", "To\u00b7des\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "Bring den K\u00f6cher mit, voll s\u00fc\u00dfer Pfeile;", "tokens": ["Bring", "den", "K\u00f6\u00b7cher", "mit", ",", "voll", "s\u00fc\u00b7\u00dfer", "Pfei\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Deine Dornen auch, und Donnerkeile;", "tokens": ["Dei\u00b7ne", "Dor\u00b7nen", "auch", ",", "und", "Don\u00b7ner\u00b7kei\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Nur, o Liebe! a\u00df ein neuer Tag", "tokens": ["Nur", ",", "o", "Lie\u00b7be", "!", "a\u00df", "ein", "neu\u00b7er", "Tag"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "FM", "NN", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "In den Finsternissen mir beginne;", "tokens": ["In", "den", "Fins\u00b7ter\u00b7nis\u00b7sen", "mir", "be\u00b7gin\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Wieder vollen, warmen Schlag", "tokens": ["Wie\u00b7der", "vol\u00b7len", ",", "war\u00b7men", "Schlag"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Mein erstorbnes Herz gewinne:", "tokens": ["Mein", "ers\u00b7torb\u00b7nes", "Herz", "ge\u00b7win\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Da\u00df ein holdes Angesicht", "tokens": ["Da\u00df", "ein", "hol\u00b7des", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Mir zum Engel sich verkl\u00e4re;", "tokens": ["Mir", "zum", "En\u00b7gel", "sich", "ver\u00b7kl\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Seine Stimme, wenn es singt und spricht,", "tokens": ["Sei\u00b7ne", "Stim\u00b7me", ",", "wenn", "es", "singt", "und", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "Mir ein Laut aus einer h\u00f6hern Sph\u00e4re \u2013", "tokens": ["Mir", "ein", "Laut", "aus", "ei\u00b7ner", "h\u00f6\u00b7hern", "Sph\u00e4\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "APPR", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "Wenn das M\u00e4dchen gr\u00fc\u00dft, sein Gru\u00df ein voller May,", "tokens": ["Wenn", "das", "M\u00e4d\u00b7chen", "gr\u00fc\u00dft", ",", "sein", "Gru\u00df", "ein", "vol\u00b7ler", "May", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.30": {"text": "Und der H\u00e4ndedruck ein Himmel sey!", "tokens": ["Und", "der", "H\u00e4n\u00b7de\u00b7druck", "ein", "Him\u00b7mel", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Kann, o traute Liebe! nie,", "tokens": ["Kann", ",", "o", "trau\u00b7te", "Lie\u00b7be", "!", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "$,", "FM", "VVFIN", "NN", "$.", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "So mit innigem Verlangen,", "tokens": ["So", "mit", "in\u00b7ni\u00b7gem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.33": {"text": "All so fest, wie meine Seele sie,", "tokens": ["All", "so", "fest", ",", "wie", "mei\u00b7ne", "See\u00b7le", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$,", "PWAV", "PPOSAT", "NN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.34": {"text": "Mich die Engelseel' umfangen \u2013", "tokens": ["Mich", "die", "En\u00b7gel\u00b7seel'", "um\u00b7fan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "So erleucht' ein Blick von ihr", "tokens": ["So", "er\u00b7leucht'", "ein", "Blick", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Diese dunkeln Pfade mir;", "tokens": ["Die\u00b7se", "dun\u00b7keln", "Pfa\u00b7de", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "La\u00df mich nur um ihre Sch\u00f6nheit schweben,", "tokens": ["La\u00df", "mich", "nur", "um", "ih\u00b7re", "Sch\u00f6n\u00b7heit", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.38": {"text": "Und mein Herz in ihrem Glanze leben!", "tokens": ["Und", "mein", "Herz", "in", "ih\u00b7rem", "Glan\u00b7ze", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Tausendfache bittre Qual", "tokens": ["Tau\u00b7send\u00b7fa\u00b7che", "bitt\u00b7re", "Qual"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gabst du mir, o Liebe! Tausend Mahl", "tokens": ["Gabst", "du", "mir", ",", "o", "Lie\u00b7be", "!", "Tau\u00b7send", "Mahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "FM", "NN", "$.", "CARD", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Lohntest du mit Dornenkr\u00e4nzen", "tokens": ["Lohn\u00b7test", "du", "mit", "Dor\u00b7nen\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner Treu; und jenes milde Gl\u00e4nzen", "tokens": ["Mei\u00b7ner", "Treu", ";", "und", "je\u00b7nes", "mil\u00b7de", "Gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KON", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Deiner Fackel ward ein Donnerstrahl.", "tokens": ["Dei\u00b7ner", "Fa\u00b7ckel", "ward", "ein", "Don\u00b7ner\u00b7strahl", "."], "t