{"dta.poem.11560": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Idylle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Unter die Eiche gestreckt, im jung belaubten Geh\u00f6lze", "tokens": ["Un\u00b7ter", "die", "Ei\u00b7che", "ge\u00b7streckt", ",", "im", "jung", "be\u00b7laub\u00b7ten", "Ge\u00b7h\u00f6l\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "APPRART", "ADJD", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Lag ich, ein B\u00fcchlein vor mir, das mir das lieblichste", "tokens": ["Lag", "ich", ",", "ein", "B\u00fcch\u00b7lein", "vor", "mir", ",", "das", "mir", "das", "lieb\u00b7lichs\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "APPR", "PPER", "$,", "PRELS", "PPER", "ART", "ADJA"], "meter": "+--+----+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Jene M\u00e4hrchen erz\u00e4hlt's von der G\u00e4nsemagd und von", "tokens": ["Je\u00b7ne", "M\u00e4hr\u00b7chen", "er\u00b7z\u00e4hlt's", "von", "der", "G\u00e4n\u00b7se\u00b7magd", "und", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "APPR"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Von dem Machandelboom; wahrlich, man wird sie", "tokens": ["Von", "dem", "Ma\u00b7chan\u00b7del\u00b7boom", ";", "wahr\u00b7lich", ",", "man", "wird", "sie"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "$,", "PIS", "VAFIN", "PPER"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Gr\u00fcnlicher Maienschein warf mir geringelte Lichter", "tokens": ["Gr\u00fcn\u00b7li\u00b7cher", "Mai\u00b7en\u00b7schein", "warf", "mir", "ge\u00b7rin\u00b7gel\u00b7te", "Lich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+--+-+---+--+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Auf's beschattete Buch, neckische Bilder zum Text.", "tokens": ["Auf's", "be\u00b7schat\u00b7te\u00b7te", "Buch", ",", "ne\u00b7cki\u00b7sche", "Bil\u00b7der", "zum", "Text", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Ferne h\u00f6r' ich die Holzart fallen, ich h\u00f6re den Gukuk", "tokens": ["Fer\u00b7ne", "h\u00f6r'", "ich", "die", "Holz\u00b7art", "fal\u00b7len", ",", "ich", "h\u00f6\u00b7re", "den", "Gu\u00b7kuk"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Und es lispelt ein Bach wenige Schritte vor mir.", "tokens": ["Und", "es", "lis\u00b7pelt", "ein", "Bach", "we\u00b7ni\u00b7ge", "Schrit\u00b7te", "vor", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "M\u00e4hrchenhaft f\u00fchl' ich mich selbst, mit aufgeschlossenen", "tokens": ["M\u00e4hr\u00b7chen\u00b7haft", "f\u00fchl'", "ich", "mich", "selbst", ",", "mit", "auf\u00b7ge\u00b7schlos\u00b7se\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "ADV", "$,", "APPR", "VVINF"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Seh' ich, wie helle! den Wald, ruft mir der Gukuk,", "tokens": ["Seh'", "ich", ",", "wie", "hel\u00b7le", "!", "den", "Wald", ",", "ruft", "mir", "der", "Gu\u00b7kuk", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJA", "$.", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Pl\u00f6tzlich rauscht es im Laub, \u2014 wird doch Sneewittchen", "tokens": ["Pl\u00f6tz\u00b7lich", "rauscht", "es", "im", "Laub", ",", "wird", "doch", "Snee\u00b7witt\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,", "$(", "VAFIN", "ADV", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Oder, bezaubert, ein Reh? Nicht doch, kein Wunder", "tokens": ["O\u00b7der", ",", "be\u00b7zau\u00b7bert", ",", "ein", "Reh", "?", "Nicht", "doch", ",", "kein", "Wun\u00b7der"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "VVPP", "$,", "ART", "NN", "$.", "PTKNEG", "ADV", "$,", "PIAT", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.13": {"text": "Siehe, mein Nachbarskind aus dem Dorf, mein artiges", "tokens": ["Sie\u00b7he", ",", "mein", "Nach\u00b7bars\u00b7kind", "aus", "dem", "Dorf", ",", "mein", "ar\u00b7ti\u00b7ges"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$,", "PPOSAT", "ADJA"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.14": {"text": "M\u00fc\u00dfig lief es in Wald, weil es den Vater dort wei\u00df,", "tokens": ["M\u00fc\u00b7\u00dfig", "lief", "es", "in", "Wald", ",", "weil", "es", "den", "Va\u00b7ter", "dort", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "NN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Ehrbar setzet es sich an meine Seite, vertraulich", "tokens": ["Ehr\u00b7bar", "set\u00b7zet", "es", "sich", "an", "mei\u00b7ne", "Sei\u00b7te", ",", "ver\u00b7trau\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,", "ADJD"], "meter": "+-+---+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Plaudern wir Dieses und Das, und ich erz\u00e4hle sofort", "tokens": ["Plau\u00b7dern", "wir", "Die\u00b7ses", "und", "Das", ",", "und", "ich", "er\u00b7z\u00e4h\u00b7le", "so\u00b7fort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PDAT", "KON", "PDS", "$,", "KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.17": {"text": "Gar ausf\u00fchrlich die Leiden des unvergleichlichen M\u00e4dchens,", "tokens": ["Gar", "aus\u00b7f\u00fchr\u00b7lich", "die", "Lei\u00b7den", "des", "un\u00b7ver\u00b7gleich\u00b7li\u00b7chen", "M\u00e4d\u00b7chens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Dem von der Mutter Hand dreimal der Tod schon gedroht.", "tokens": ["Dem", "von", "der", "Mut\u00b7ter", "Hand", "drei\u00b7mal", "der", "Tod", "schon", "ge\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.19": {"text": "Denn die Eitle, die K\u00f6nigin, ha\u00dfte sie, weil sie so sch\u00f6n war,", "tokens": ["Denn", "die", "Eit\u00b7le", ",", "die", "K\u00f6\u00b7ni\u00b7gin", ",", "ha\u00df\u00b7te", "sie", ",", "weil", "sie", "so", "sch\u00f6n", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "--+--+-++--+-+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Grimmig, da mu\u00dfte sie fliehn, wohnte bei Zwergen", "tokens": ["Grim\u00b7mig", ",", "da", "mu\u00df\u00b7te", "sie", "fliehn", ",", "wohn\u00b7te", "bei", "Zwer\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,", "VVFIN", "APPR", "NN"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.21": {"text": "Aber die K\u00f6nigin findet sie bald; sie klopfet am Hause,", "tokens": ["A\u00b7ber", "die", "K\u00f6\u00b7ni\u00b7gin", "fin\u00b7det", "sie", "bald", ";", "sie", "klop\u00b7fet", "am", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Bietet, als Kr\u00e4merin, schlau, lockende Waare zu Kauf.", "tokens": ["Bie\u00b7tet", ",", "als", "Kr\u00e4\u00b7me\u00b7rin", ",", "schlau", ",", "lo\u00b7cken\u00b7de", "Waa\u00b7re", "zu", "Kauf", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "NN", "$,", "ADJD", "$,", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.23": {"text": "Arglos \u00f6ffnet das Kind, den Rath der Zwerge vergessend,", "tokens": ["Arg\u00b7los", "\u00f6ff\u00b7net", "das", "Kind", ",", "den", "Rath", "der", "Zwer\u00b7ge", "ver\u00b7ges\u00b7send", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Und das Liebchen empf\u00e4ngt, ach! den vergifteten Kamm.", "tokens": ["Und", "das", "Lieb\u00b7chen", "emp\u00b7f\u00e4ngt", ",", "ach", "!", "den", "ver\u00b7gif\u00b7te\u00b7ten", "Kamm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "ITJ", "$.", "ART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Welch ein Jammer, da nun die Kleinen zu Hause ge-", "tokens": ["Welch", "ein", "Jam\u00b7mer", ",", "da", "nun", "die", "Klei\u00b7nen", "zu", "Hau\u00b7se", "ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "KOUS", "ADV", "ART", "NN", "APPR", "NN", "TRUNC"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "Welcher K\u00fcnste bedarf's, bis die Erstarrte erwacht!", "tokens": ["Wel\u00b7cher", "K\u00fcns\u00b7te", "be\u00b7da\u00b7rf's", ",", "bis", "die", "Er\u00b7starr\u00b7te", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "NE", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.27": {"text": "Doch zum zweiten Mal kommt, zum dritten Male, ver-", "tokens": ["Doch", "zum", "zwei\u00b7ten", "Mal", "kommt", ",", "zum", "drit\u00b7ten", "Ma\u00b7le", ",", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,", "TRUNC"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.28": {"text": "Die Verderberin, leicht hat sie das M\u00e4dchen beschwazt,", "tokens": ["Die", "Ver\u00b7der\u00b7be\u00b7rin", ",", "leicht", "hat", "sie", "das", "M\u00e4d\u00b7chen", "be\u00b7schwazt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Schn\u00fcrt in das zierliche Leibchen sie ein, den Athem er-", "tokens": ["Schn\u00fcrt", "in", "das", "zier\u00b7li\u00b7che", "Leib\u00b7chen", "sie", "ein", ",", "den", "A\u00b7them", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "In dem Busen; zuletzt bringt sie die t\u00f6dtliche Frucht.", "tokens": ["In", "dem", "Bu\u00b7sen", ";", "zu\u00b7letzt", "bringt", "sie", "die", "t\u00f6dt\u00b7li\u00b7che", "Frucht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Nun ist alle H\u00fclfe umsonst; wie weinen die Zwerge!", "tokens": ["Nun", "ist", "al\u00b7le", "H\u00fcl\u00b7fe", "um\u00b7sonst", ";", "wie", "wei\u00b7nen", "die", "Zwer\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVFIN", "$.", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Ein krystallener Sarg schlie\u00dfet die Aermste nun ein,", "tokens": ["Ein", "krys\u00b7tal\u00b7le\u00b7ner", "Sarg", "schlie\u00b7\u00dfet", "die", "A\u00b7erms\u00b7te", "nun", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "--+--++---+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Frei gestellt auf den Berg, ein Anblick allen Gestirnen,", "tokens": ["Frei", "ge\u00b7stellt", "auf", "den", "Berg", ",", "ein", "An\u00b7blick", "al\u00b7len", "Ge\u00b7stir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Unverwelklich ruht innen die s\u00fc\u00dfe Gestalt.", "tokens": ["Un\u00b7ver\u00b7wel\u00b7klich", "ruht", "in\u00b7nen", "die", "s\u00fc\u00b7\u00dfe", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.35": {"text": "\u2014 So weit war ich gekommen, da drang aus dem n\u00e4ch-", "tokens": ["So", "weit", "war", "ich", "ge\u00b7kom\u00b7men", ",", "da", "drang", "aus", "dem", "n\u00e4ch"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "VAFIN", "PPER", "VVPP", "$,", "ADV", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Hinter mir Nachtigallschlag herrlich auf Einmal hervor,", "tokens": ["Hin\u00b7ter", "mir", "Nach\u00b7ti\u00b7gall\u00b7schlag", "herr\u00b7lich", "auf", "Ein\u00b7mal", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "ADJD", "APPR", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.37": {"text": "Troff wie Honig durch das Gezweig und spr\u00fchte wie Feuer", "tokens": ["Troff", "wie", "Ho\u00b7nig", "durch", "das", "Ge\u00b7zweig", "und", "spr\u00fch\u00b7te", "wie", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "NN", "APPR", "ART", "NN", "KON", "VVFIN", "KOKOM", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.38": {"text": "Zackige T\u00f6ne, mir traf freudig ein Schauer das Herz,", "tokens": ["Za\u00b7cki\u00b7ge", "T\u00f6\u00b7ne", ",", "mir", "traf", "freu\u00b7dig", "ein", "Schau\u00b7er", "das", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.39": {"text": "Wie wenn der G\u00f6ttinnen Eine, vor\u00fcberfliehend, dem", "tokens": ["Wie", "wenn", "der", "G\u00f6t\u00b7tin\u00b7nen", "Ei\u00b7ne", ",", "vor\u00b7\u00fc\u00b7berf\u00b7lie\u00b7hend", ",", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOKOM", "KOUS", "ART", "NN", 