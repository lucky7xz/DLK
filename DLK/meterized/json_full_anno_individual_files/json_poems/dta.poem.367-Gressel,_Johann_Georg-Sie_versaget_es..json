{"dta.poem.367": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie versaget es.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Schweigt Verwegner! nun die Wangen", "tokens": ["Schweigt", "Ver\u00b7weg\u00b7ner", "!", "nun", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch nicht gut und gnugsahm seyn;", "tokens": ["Euch", "nicht", "gut", "und", "gnug\u00b7sahm", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "KON", "VVFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wolt ihr nicht die Brust umfangen/", "tokens": ["Wolt", "ihr", "nicht", "die", "Brust", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Graut euch vor der Lippen Pracht?", "tokens": ["Graut", "euch", "vor", "der", "Lip\u00b7pen", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wohlan! wird auch die Schoo\u00df veracht?", "tokens": ["Wo\u00b7hlan", "!", "wird", "auch", "die", "Schoo\u00df", "ver\u00b7acht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schlu\u00df ist schon gemacht.", "tokens": ["Der", "Schlu\u00df", "ist", "schon", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weil ihr es selber so gesprochen", "tokens": ["Weil", "ihr", "es", "sel\u00b7ber", "so", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df sie vor euch zu schlecht;", "tokens": ["Da\u00df", "sie", "vor", "euch", "zu", "schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "So sprech ich dieses recht;", "tokens": ["So", "sprech", "ich", "die\u00b7ses", "recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Der nimmt kein Unrecht ein/", "tokens": ["Der", "nimmt", "kein", "Un\u00b7recht", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "ART", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Der \u00fcber sich den Stecken selbst gebrochen.", "tokens": ["Der", "\u00fc\u00b7ber", "sich", "den", "Ste\u00b7cken", "selbst", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PRF", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Jhr solt hinfort vergebens seuffzen m\u00fcssen", "tokens": ["Ihr", "solt", "hin\u00b7fort", "ver\u00b7ge\u00b7bens", "seuff\u00b7zen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "VMINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und nimmermehr", "tokens": ["Und", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Die Hand/ vielweniger die F\u00fcsse k\u00fcssen.", "tokens": ["Die", "Hand", "/", "viel\u00b7we\u00b7ni\u00b7ger", "die", "F\u00fcs\u00b7se", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}