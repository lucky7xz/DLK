{"dta.poem.23665": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und w\u00e4r' er auch f\u00fcr euch nichts weiter als ein Ketzer,", "tokens": ["Und", "w\u00e4r'", "er", "auch", "f\u00fcr", "euch", "nichts", "wei\u00b7ter", "als", "ein", "Ket\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPER", "PIS", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So war er doch ein biedrer edler deutscher Mann,", "tokens": ["So", "war", "er", "doch", "ein", "bie\u00b7drer", "ed\u00b7ler", "deut\u00b7scher", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den man im besten Weine wie im schlechtsten Kr\u00e4tzer", "tokens": ["Den", "man", "im", "bes\u00b7ten", "Wei\u00b7ne", "wie", "im", "schlechts\u00b7ten", "Kr\u00e4t\u00b7zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPRART", "ADJA", "NN", "KOKOM", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Genug nie loben noch auch je beschimpfen kann.", "tokens": ["Ge\u00b7nug", "nie", "lo\u00b7ben", "noch", "auch", "je", "be\u00b7schimp\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und h\u00e4tt' er nur gesprochen das Eine Wort,", "tokens": ["Und", "h\u00e4tt'", "er", "nur", "ge\u00b7spro\u00b7chen", "das", "Ei\u00b7ne", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "ART", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "So m\u00fcsst' er leben unter uns hinfort:", "tokens": ["So", "m\u00fcsst'", "er", "le\u00b7ben", "un\u00b7ter", "uns", "hin\u00b7fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u201ewer nicht liebt Wein, Weib und Gesang,", "tokens": ["\u201e", "wer", "nicht", "liebt", "Wein", ",", "Weib", "und", "Ge\u00b7sang", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PTKNEG", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Der bleibt ein Narr sein Lebelang!\u201c", "tokens": ["Der", "bleibt", "ein", "Narr", "sein", "Le\u00b7be\u00b7lang", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Hoch lebe du ehrlicher ", "tokens": ["Hoch", "le\u00b7be", "du", "ehr\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADJD"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}}}}