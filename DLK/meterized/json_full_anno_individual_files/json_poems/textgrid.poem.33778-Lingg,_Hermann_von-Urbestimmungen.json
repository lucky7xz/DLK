{"textgrid.poem.33778": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Urbestimmungen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tritt ein Volk ein in die Geschichte,", "tokens": ["Tritt", "ein", "Volk", "ein", "in", "die", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Weise mit Kronen schreiten voran,", "tokens": ["Wei\u00b7se", "mit", "Kro\u00b7nen", "schrei\u00b7ten", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ihrer F\u00fchrung erstem Lichte", "tokens": ["Ih\u00b7rer", "F\u00fch\u00b7rung", "ers\u00b7tem", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reihen Zug an Zug sich an,", "tokens": ["Rei\u00b7hen", "Zug", "an", "Zug", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Immer gedr\u00e4ngter wogt es, je l\u00e4nger", "tokens": ["Im\u00b7mer", "ge\u00b7dr\u00e4ng\u00b7ter", "wogt", "es", ",", "je", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "$,", "ADV", "ADJD"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Helden erscheinen, und Seher und S\u00e4nger", "tokens": ["Hel\u00b7den", "er\u00b7schei\u00b7nen", ",", "und", "Se\u00b7her", "und", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "KON", "NN", "KON", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.7": {"text": "F\u00fcllen erst\u00fcrmend die weitere Bahn.", "tokens": ["F\u00fcl\u00b7len", "er\u00b7st\u00fcr\u00b7mend", "die", "wei\u00b7te\u00b7re", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Hilfreich auch kommt vieles entgegen,", "tokens": ["Hilf\u00b7reich", "auch", "kommt", "vie\u00b7les", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Sterne, mitwandelnd, bestimmen das Jahr,", "tokens": ["Ster\u00b7ne", ",", "mit\u00b7wan\u00b7delnd", ",", "be\u00b7stim\u00b7men", "das", "Jahr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und der \u00c4hre wildsprossender Segen", "tokens": ["Und", "der", "\u00c4h\u00b7re", "wild\u00b7spros\u00b7sen\u00b7der", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bietet sich selbst den Erm\u00fcdeten dar.", "tokens": ["Bie\u00b7tet", "sich", "selbst", "den", "Er\u00b7m\u00fc\u00b7de\u00b7ten", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Da nun den g\u00fctigen Gottesgeschenken", "tokens": ["Da", "nun", "den", "g\u00fc\u00b7ti\u00b7gen", "Got\u00b7tes\u00b7ge\u00b7schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Weihn sie zum dauernden Angedenken", "tokens": ["Weihn", "sie", "zum", "dau\u00b7ern\u00b7den", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Festlichen Tag und bekr\u00e4nzten Altar.", "tokens": ["Fest\u00b7li\u00b7chen", "Tag", "und", "be\u00b7kr\u00e4nz\u00b7ten", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Alles Urspr\u00fcnglich' und Eigne verk\u00fcndet", "tokens": ["Al\u00b7les", "Ur\u00b7spr\u00fcng\u00b7lich'", "und", "Eig\u00b7ne", "ver\u00b7k\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sein gewaltig Erwachen jetzt,", "tokens": ["Sein", "ge\u00b7wal\u00b7tig", "Er\u00b7wa\u00b7chen", "jetzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Alles Dauernde wird gegr\u00fcndet,", "tokens": ["Al\u00b7les", "Dau\u00b7ern\u00b7de", "wird", "ge\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Alles Heilige festgesetzt.", "tokens": ["Al\u00b7les", "Hei\u00b7li\u00b7ge", "fest\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Unersch\u00f6pflicher Sch\u00e4tze Gaben", "tokens": ["Un\u00b7er\u00b7sch\u00f6pf\u00b7li\u00b7cher", "Sch\u00e4t\u00b7ze", "Ga\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Werden den Seelen eingegraben,", "tokens": ["Wer\u00b7den", "den", "See\u00b7len", "ein\u00b7ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Um sie zu wahren unverletzt.", "tokens": ["Um", "sie", "zu", "wah\u00b7ren", "un\u00b7ver\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Also zog Mose mit Israels St\u00e4mmen", "tokens": ["Al\u00b7so", "zog", "Mo\u00b7se", "mit", "Is\u00b7raels", "St\u00e4m\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "APPR", "NE", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u00dcber die W\u00fcste nach Kanaan,", "tokens": ["\u00dc\u00b7ber", "die", "W\u00fcs\u00b7te", "nach", "Ka\u00b7naan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So von des Kaukasus schneeigen K\u00e4mmen", "tokens": ["So", "von", "des", "Kau\u00b7ka\u00b7sus", "schne\u00b7e\u00b7i\u00b7gen", "K\u00e4m\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+---+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "St\u00fcrmte der rauhe Pelasger heran,", "tokens": ["St\u00fcrm\u00b7te", "der", "rau\u00b7he", "Pe\u00b7las\u00b7ger", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Und aus dem Dunkel urn\u00e4chtiger Eichen", "tokens": ["Und", "aus", "dem", "Dun\u00b7kel", "ur\u00b7n\u00e4ch\u00b7ti\u00b7ger", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "\u00dcber der Weltbesieger Leichen,", "tokens": ["\u00dc\u00b7ber", "der", "Welt\u00b7be\u00b7sie\u00b7ger", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Brach der germanische V\u00f6lkerorkan.", "tokens": ["Brach", "der", "ger\u00b7ma\u00b7ni\u00b7sche", "V\u00f6l\u00b7ker\u00b7or\u00b7kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Dauernd bleibt uns, was Hellenen", "tokens": ["Dau\u00b7ernd", "bleibt", "uns", ",", "was", "Hel\u00b7le\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PWS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nstes in Kunst und Leben erdacht,", "tokens": ["Sch\u00f6ns\u00b7tes", "in", "Kunst", "und", "Le\u00b7ben", "er\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Was trotz Fesseln und Arenen", "tokens": ["Was", "trotz", "Fes\u00b7seln", "und", "A\u00b7re\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "KON", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Gro\u00dfes der m\u00e4chtige R\u00f6mer vollbracht.", "tokens": ["Gro\u00b7\u00dfes", "der", "m\u00e4ch\u00b7ti\u00b7ge", "R\u00f6\u00b7mer", "voll\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Wenn die Fluten das Land zerstreuten,", "tokens": ["Wenn", "die", "Flu\u00b7ten", "das", "Land", "zer\u00b7streu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "W\u00fcrde noch Shakespeare England bedeuten,", "tokens": ["W\u00fcr\u00b7de", "noch", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "En\u00b7gland", "be\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "NE", "VVINF", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Luther und Kant die deutsche Macht.", "tokens": ["Lu\u00b7ther", "und", "Kant", "die", "deut\u00b7sche", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Das sind die Male der Nationen,", "tokens": ["Das", "sind", "die", "Ma\u00b7le", "der", "Na\u00b7ti\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], 