{"textgrid.poem.65184": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Wort hab' ich erkoren,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wort hab' ich erkoren,", "tokens": ["Ein", "Wort", "hab'", "ich", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das in der Lebensschlacht", "tokens": ["Das", "in", "der", "Le\u00b7bens\u00b7schlacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Schutz, stets unverloren,", "tokens": ["Ein", "Schutz", ",", "stets", "un\u00b7ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich hieb- und schu\u00dffest macht.", "tokens": ["Mich", "hie\u00b7b", "und", "schu\u00df\u00b7fest", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "TRUNC", "KON", "ADJD", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Man lernt es nur mit Schmerzen,", "tokens": ["Man", "lernt", "es", "nur", "mit", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch wer's erlernen kann,", "tokens": ["Doch", "wer's", "er\u00b7ler\u00b7nen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"},