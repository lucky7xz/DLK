{"dta.poem.6206": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Jahrs-Zeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein solches ordentlichs Bewegen,", "tokens": ["Ein", "sol\u00b7ches", "or\u00b7dent\u00b7lichs", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und eines solchen Drucks und Dranges fruchtbars regen,", "tokens": ["Und", "ei\u00b7nes", "sol\u00b7chen", "Drucks", "und", "Dran\u00b7ges", "frucht\u00b7bars", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "KON", "NN", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herrscht nach und nach in allen Erden-Theilen.", "tokens": ["Herrscht", "nach", "und", "nach", "in", "al\u00b7len", "Er\u00b7den\u00b7T\u00b7hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}