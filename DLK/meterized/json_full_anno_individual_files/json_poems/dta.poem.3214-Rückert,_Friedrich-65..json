{"dta.poem.3214": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "65.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und nur durch Eines hast du dich als Kind verrathen,", "tokens": ["Und", "nur", "durch", "Ei\u00b7nes", "hast", "du", "dich", "als", "Kind", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIS", "VAFIN", "PPER", "PRF", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du dem M\u00fctterlein nicht konntest lang' entrathen.", "tokens": ["Da\u00df", "du", "dem", "M\u00fct\u00b7ter\u00b7lein", "nicht", "konn\u00b7test", "lang'", "ent\u00b7ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein halbes Jahr ist's nur, da\u00df du bist hingegangen,", "tokens": ["Ein", "hal\u00b7bes", "Jahr", "ist's", "nur", ",", "da\u00df", "du", "bist", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schon hast du sie nachgezogen mit Verlangen.", "tokens": ["Und", "schon", "hast", "du", "sie", "nach\u00b7ge\u00b7zo\u00b7gen", "mit", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie oder hat sie ihr Verlangen nachgezogen?", "tokens": ["Wie", "o\u00b7der", "hat", "sie", "ihr", "Ver\u00b7lan\u00b7gen", "nach\u00b7ge\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entgegen sind sich zwei Verlangen nur geflogen.", "tokens": ["Ent\u00b7ge\u00b7gen", "sind", "sich", "zwei", "Ver\u00b7lan\u00b7gen", "nur", "ge\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PRF", "CARD", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die deine Mutter war, war sie doch meine auch;", "tokens": ["Die", "dei\u00b7ne", "Mut\u00b7ter", "war", ",", "war", "sie", "doch", "mei\u00b7ne", "auch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "$,", "VAFIN", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie haben wir getheilt mit so ungleichem Brauch?", "tokens": ["Wie", "ha\u00b7ben", "wir", "ge\u00b7theilt", "mit", "so", "un\u00b7glei\u00b7chem", "Brauch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dein Theil ist dort mit ihr zu lachen im Vereine,", "tokens": ["Dein", "Theil", "ist", "dort", "mit", "ihr", "zu", "la\u00b7chen", "im", "Ver\u00b7ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mein's hier, da\u00df getrennt ich von euch beiden weine.", "tokens": ["Und", "mein's", "hier", ",", "da\u00df", "ge\u00b7trennt", "ich", "von", "euch", "bei\u00b7den", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "$,", "KOUS", "VVPP", "PPER", "APPR", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich bin wol alt genug, der Mutter zu entw\u00f6hnen,", "tokens": ["Ich", "bin", "wol", "alt", "ge\u00b7nug", ",", "der", "Mut\u00b7ter", "zu", "ent\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du jung und sch\u00f6n, um dort mit Palmen sie zu kr\u00f6nen.", "tokens": ["Du", "jung", "und", "sch\u00f6n", ",", "um", "dort", "mit", "Pal\u00b7men", "sie", "zu", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$,", "KOUI", "ADV", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Doch bitt' ich, da\u00df du mir den Schaden dadurch b\u00fc\u00dfest,", "tokens": ["Doch", "bitt'", "ich", ",", "da\u00df", "du", "mir", "den", "Scha\u00b7den", "da\u00b7durch", "b\u00fc\u00b7\u00dfest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du den Vater auch und Bruder sch\u00f6n mir gr\u00fc\u00dfest.", "tokens": ["Da\u00df", "du", "den", "Va\u00b7ter", "auch", "und", "Bru\u00b7der", "sch\u00f6n", "mir", "gr\u00fc\u00b7\u00dfest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "KON", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Denn Vater, Bruder auch, sie gingen dir voraus,", "tokens": ["Denn", "Va\u00b7ter", ",", "Bru\u00b7der", "auch", ",", "sie", "gin\u00b7gen", "dir", "vo\u00b7raus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "NN", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenig fehlt, so hast du dort dein ganzes Haus.", "tokens": ["Und", "we\u00b7nig", "fehlt", ",", "so", "hast", "du", "dort", "dein", "gan\u00b7zes", "Haus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}