{"dta.poem.16490": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "151.  Der Mensch ist GOttes kindbett.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Da GOtt das erstemahl hat seinen Sohn gebohrn/", "tokens": ["Da", "Gott", "das", "ers\u00b7te\u00b7mahl", "hat", "sei\u00b7nen", "Sohn", "ge\u00b7bohrn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da hat er mich und dich zum Kindbett au\u00dferkohrn.", "tokens": ["Da", "hat", "er", "mich", "und", "dich", "zum", "Kind\u00b7bett", "au\u00b7\u00dfer\u00b7kohrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "KON", "PPER", "APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}