{"textgrid.poem.45838": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[sie las den Brief]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie las den Brief, ich soll Dich freundlich lieben.", "tokens": ["Sie", "las", "den", "Brief", ",", "ich", "soll", "Dich", "freund\u00b7lich", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Brief hat sie im z\u00e4rtern Sinn ber\u00fchrt", "tokens": ["Der", "Brief", "hat", "sie", "im", "z\u00e4r\u00b7tern", "Sinn", "be\u00b7r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie hat ans End, ein kleines ja geschrieben", "tokens": ["Sie", "hat", "ans", "End", ",", "ein", "klei\u00b7nes", "ja", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "$,", "ART", "ADJA", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So nimm mich an, so sch\u00f6n zu Dir verf\u00fchrt", "tokens": ["So", "nimm", "mich", "an", ",", "so", "sch\u00f6n", "zu", "Dir", "ver\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ist Dir und mir der sch\u00f6ne Trost geblieben", "tokens": ["Ist", "Dir