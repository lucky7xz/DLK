{"textgrid.poem.38190": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Ygels Art", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ygels Art ist manchem bekannt,", "tokens": ["Y\u00b7gels", "Art", "ist", "man\u00b7chem", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PIAT", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Thut weit hin und her wandern,", "tokens": ["Thut", "weit", "hin", "und", "her", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Singers Lied durch Stadt und Land", "tokens": ["Sin\u00b7gers", "Lied", "durch", "Stadt", "und", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich singt einer zum andern,", "tokens": ["Sich", "singt", "ei\u00b7ner", "zum", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "APPRART", "ADJA", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ygel auch hat diesen Gebrauch,", "tokens": ["Y\u00b7gel", "auch", "hat", "die\u00b7sen", "Ge\u00b7brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PDAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Dabey Stacheln zu spitzen.", "tokens": ["Da\u00b7bey", "Sta\u00b7cheln", "zu", "spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Manchem Hund zersticht er das Maul", "tokens": ["Man\u00b7chem", "Hund", "zer\u00b7sticht", "er", "das", "Maul"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "So auch Singer mit Witzen.", "tokens": ["So", "auch", "Sin\u00b7ger", "mit", "Wit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "APPR", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Yglein er setzt j\u00e4hrlich so fromm,", "tokens": ["Yg\u00b7lein", "er", "setzt", "j\u00e4hr\u00b7lich", "so", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Sie m\u00fcssen selbst sich ern\u00e4hren,", "tokens": ["Sie", "m\u00fcs\u00b7sen", "selbst", "sich", "er\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Bald du holder Sommergast komm,", "tokens": ["Bald", "du", "hol\u00b7der", "Som\u00b7mer\u00b7gast", "komm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Schwalbe in Zucht und Ehren!", "tokens": ["Schwal\u00b7be", "in", "Zucht", "und", "Eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ygels Art ist manchem bekannt,", "tokens"