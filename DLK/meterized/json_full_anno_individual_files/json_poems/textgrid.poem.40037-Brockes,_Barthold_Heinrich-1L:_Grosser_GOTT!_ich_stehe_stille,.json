{"textgrid.poem.40037": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Grosser GOTT! ich stehe stille,", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Grosser GOTT! ich stehe stille,", "tokens": ["Gros\u00b7ser", "GoTT", "!", "ich", "ste\u00b7he", "stil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erstaun' ab aller F\u00fclle", "tokens": ["Und", "er\u00b7staun'", "ab", "al\u00b7ler", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Aller Vollenkommenheit,", "tokens": ["Al\u00b7ler", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Pracht und Lieblichkeit,", "tokens": ["Al\u00b7ler", "Pracht", "und", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die ich, wo ich geh' und stehe,", "tokens": ["Die", "ich", ",", "wo", "ich", "geh'", "und", "ste\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit Ergetzen h\u00f6r' und sehe,", "tokens": ["Mit", "Er\u00b7get\u00b7zen", "h\u00f6r'", "und", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.7": {"text": "Sonderlich zu dieser Zeit.", "tokens": ["Son\u00b7der\u00b7lich", "zu", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df mich sehen, la\u00df mich h\u00f6ren,", "tokens": ["La\u00df", "mich", "se\u00b7hen", ",", "la\u00df", "mich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Grosses ", "tokens": ["Gros\u00b7ses"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Alles, was ich h\u00f6r' und sehe!", "tokens": ["Al\u00b7les", ",", "was", "ich", "h\u00f6r'", "und", "se\u00b7he", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6re die V\u00f6gel mit klingenden Kehlen,", "tokens": ["Ich", "h\u00f6\u00b7re", "die", "V\u00f6\u00b7gel", "mit", "klin\u00b7gen\u00b7den", "Keh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Vom lieblichen Gr\u00fcnen der W\u00e4lder entz\u00fcckt,", "tokens": ["Vom", "lieb\u00b7li\u00b7chen", "Gr\u00fc\u00b7nen", "der", "W\u00e4l\u00b7der", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Mit Freuden erz\u00e4hlen:", "tokens": ["Mit", "Freu\u00b7den", "er\u00b7z\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Man h\u00f6ret im lieblich-beweglichen Wallen,", "tokens": ["Man", "h\u00f6\u00b7ret", "im", "lieb\u00b7lich\u00b7be\u00b7weg\u00b7li\u00b7chen", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wann Zephir sanft \u00fcber die Aeren hinf\u00e4hrt,", "tokens": ["Wann", "Ze\u00b7phir", "sanft", "\u00fc\u00b7ber", "die", "A\u00b7e\u00b7ren", "hin\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+---++--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Mit Zischen erschallen:", "tokens": ["Mit", "Zi\u00b7schen", "er\u00b7schal\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Man h\u00f6ret die Wellen in rauschenden B\u00e4chen,", "tokens": ["Man", "h\u00f6\u00b7ret", "die", "Wel\u00b7len", "in", "rau\u00b7schen\u00b7den", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wann jede sich fr\u00f6hlich bald hebet, bald senckt,", "tokens": ["Wann", "je\u00b7de", "sich", "fr\u00f6h\u00b7lich", "bald", "he\u00b7bet", ",", "bald", "senckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "PRF", "ADJD", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sanft murmeln und sprechen:", "tokens": ["Sanft", "mur\u00b7meln", "und", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Man h\u00f6ret die Sprache der lispelnden Winde,", "tokens": ["Man", "h\u00f6\u00b7ret", "die", "Spra\u00b7che", "der", "lis\u00b7peln\u00b7den", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es mercket der Seelen aufmerckende Kraft,", "tokens": ["Es", "mer\u00b7cket", "der", "See\u00b7len", "auf\u00b7mer\u00b7cken\u00b7de", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie s\u00e4useln gelinde:", "tokens": ["Sie", "s\u00e4u\u00b7seln", "ge\u00b7lin\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Wir k\u00f6nnen in Th\u00e4lern, auf Bergen und H\u00f6hen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "in", "Th\u00e4\u00b7lern", ",", "auf", "Ber\u00b7gen", "und", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "In lieblicher B\u00fcsche beschatteten Pracht", "tokens": ["In", "lieb\u00b7li\u00b7cher", "B\u00fc\u00b7sche", "be\u00b7schat\u00b7te\u00b7ten", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Nichts deutlicher sehen:", "tokens": ["Nichts", "deut\u00b7li\u00b7cher", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Als: ", "tokens": ["Als", ":"], "token_info": ["word", "punct"], "pos": ["KOUS", "$."], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Wir sehn, wenn wir sehen best\u00e4ndig getrieben", "tokens": ["Wir", "sehn", ",", "wenn", "wir", "se\u00b7hen", "be\u00b7st\u00e4n\u00b7dig", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "ADJD", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "So viele Planeten, den Himmel, die Welt,", "tokens": ["So", "vie\u00b7le", "Pla\u00b7ne\u00b7ten", ",", "den", "Him\u00b7mel", ",", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "In ihren geschrieben;", "tokens": ["In", "ih\u00b7ren", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Hier zeigt sich ", "tokens": ["Hier", "zeigt", "sich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.8": {"line.1": {"text": "Es giebt der Gesch\u00f6pfe vortreffliches Wesen", "tokens": ["Es", "giebt", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "vor\u00b7treff\u00b7li\u00b7ches", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "In seiner Ver\u00e4nderung, Ordnung und Zier", "tokens": ["In", "sei\u00b7ner", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", ",", "Ord\u00b7nung", "und", "Zier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Uns deutlich zu lesen:", "tokens": ["Uns", "deut\u00b7lich", "zu", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Sch\u00f6pfer aller Ding' ist hier!", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Ding'", "ist", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Wir k\u00f6nnen, wenn liebliche Bluhmen uns r\u00fchren,", "tokens": ["Wir", "k\u00f6n\u00b7nen", ",", "wenn", "lieb\u00b7li\u00b7che", "Bluh\u00b7men", "uns", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die G\u00f6ttlicher Finger so herrlich geschm\u00fcckt,", "tokens": ["Die", "G\u00f6tt\u00b7li\u00b7cher", "Fin\u00b7ger", "so", "herr\u00b7lich", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "In jeglicher sp\u00fcren:", "tokens": ["In", "jeg\u00b7li\u00b7cher", "sp\u00fc\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auch hier ist ", "tokens": ["Auch", "hier", "ist"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Wenn neidliche Bissen uns Anmuth erwecken,", "tokens": ["Wenn", "neid\u00b7li\u00b7che", "Bis\u00b7sen", "uns", "An\u00b7muth", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "NN", "VVINF", "$,"], "meter": "-+--+-+++-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und k\u00fchles Getr\u00e4ncke die Lippen uns netzt;", "tokens": ["Und", "k\u00fch\u00b7les", "Ge\u00b7tr\u00e4n\u00b7cke", "die", "Lip\u00b7pen", "uns", "netzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Kann jeder recht schmecken:", "tokens": ["Kann", "je\u00b7der", "recht", "schme\u00b7cken", ":"], "token_info":