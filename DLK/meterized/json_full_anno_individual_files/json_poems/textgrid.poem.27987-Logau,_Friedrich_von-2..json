{"textgrid.poem.27987": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist mir meine Lust bei Todten stets zu leben,", "tokens": ["Es", "ist", "mir", "mei\u00b7ne", "Lust", "bei", "Tod\u00b7ten", "stets", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit denen um und um, die nicht seyn, seyn gegeben,", "tokens": ["Mit", "de\u00b7nen", "um", "und", "um", ",", "die", "nicht", "seyn", ",", "seyn", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "KON", "PTKVZ", "$,", "PRELS", "PTKNEG", "VAINF", "$,", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zu fragen, die sind taub, zu h\u00f6ren, die nichts sagen,", "tokens": ["Zu", "fra\u00b7gen", ",", "die", "sind", "taub", ",", "zu", "h\u00f6\u00b7ren", ",", "die", "nichts", "sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "VAFIN", "ADJD", "$,", "PTKZU", "VVINF", "$,", "PRELS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und die, die haben nichts, sehr viel hingegen tragen,", "tokens": ["Und", "die", ",", "die", "ha\u00b7ben", "nichts", ",", "sehr", "viel", "hin\u00b7ge\u00b7gen", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "VAFIN", "PIS", "$,", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu halten lieb und werth. Ich bin auff die beflissen,", "tokens": ["Zu", "hal\u00b7ten", "lieb", "und", "werth", ".", "Ich", "bin", "auff", "die", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "KON", "ADJD", "$.", "PPER", "VAFIN", "APPR", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die mir viel gutes thun und doch von mir nichts wissen;", "tokens": ["Die", "mir", "viel", "gu\u00b7tes", "thun", "und", "doch", "von", "mir", "nichts", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "ADJA", "VVINF", "KON", "ADV", "APPR", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich halte diese hoch, die mich nur an nicht sehen;", "tokens": ["Ich", "hal\u00b7te", "die\u00b7se", "hoch", ",", "die", "mich", "nur", "an", "nicht", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "ADJD", "$,", "PRELS", "PPER", "ADV", "APPR", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die manchmal mich mit Ernst verh\u00f6hnen, schelten, schm\u00e4hen,", "tokens": ["Die", "manch\u00b7mal", "mich", "mit", "Ernst", "ver\u00b7h\u00f6h\u00b7nen", ",", "schel\u00b7ten", ",", "schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "PPER", "APPR", "NE", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sind meine beste Freund. Und solt ich die begeben,", "tokens": ["Sind", "mei\u00b7ne", "bes\u00b7te", "Freund", ".", "Und", "solt", "ich", "die", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$.", "KON", "VMFIN", "PPER", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Eh geb ich alle Welt, eh geb ich auch das Leben.", "tokens": ["Eh", "geb", "ich", "al\u00b7le", "Welt", ",", "eh", "geb", "ich", "auch", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$,", "KOUS", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist mir meine Lust bei Todten stets zu leben,", "tokens": ["Es", "ist", "mir", "mei\u00b7ne", "Lust", "bei", "Tod\u00b7ten", "stets", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit denen um und um, die nicht seyn, seyn gegeben,", "tokens": ["Mit", "de\u00b7nen", "um", "und", "um", ",", "die", "nicht", "seyn", ",", "seyn", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "KON", "PTKVZ", "$,", "PRELS", "PTKNEG", "VAINF", "$,", "PPOSAT", "VVPP", "$,"],