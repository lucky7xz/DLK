{"dta.poem.12909": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uber die entfernung seiner liebsten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bald die sonne kommt, und ihren glantz erh\u00f6ht,", "tokens": ["So", "bald", "die", "son\u00b7ne", "kommt", ",", "und", "ih\u00b7ren", "glantz", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00df der kalte schnee in linder flut verschie\u00dfen:", "tokens": ["So", "mu\u00df", "der", "kal\u00b7te", "schnee", "in", "lin\u00b7der", "flut", "ver\u00b7schie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ich hingegen mu\u00df in thr\u00e4nen-saltz zerflie\u00dfen,", "tokens": ["Und", "ich", "hin\u00b7ge\u00b7gen", "mu\u00df", "in", "thr\u00e4\u00b7nen\u00b7saltz", "zer\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn meine sonne weicht, wenn Daphne von mir geht.", "tokens": ["Wenn", "mei\u00b7ne", "son\u00b7ne", "weicht", ",", "wenn", "Daph\u00b7ne", "von", "mir", "geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS"