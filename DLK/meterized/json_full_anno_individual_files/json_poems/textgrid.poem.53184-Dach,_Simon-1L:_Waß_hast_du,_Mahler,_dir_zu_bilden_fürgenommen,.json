{"textgrid.poem.53184": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wa\u00df hast du, Mahler, dir zu bilden f\u00fcrgenommen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wa\u00df hast du, Mahler, dir zu bilden f\u00fcrgenommen,", "tokens": ["Wa\u00df", "hast", "du", ",", "Mah\u00b7ler", ",", "dir", "zu", "bil\u00b7den", "f\u00fcr\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "NN", "$,", "PPER", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich G\u00f6ttin, die ich nie euch zu Gesichte kommen,", "tokens": ["Mich", "G\u00f6t\u00b7tin", ",", "die", "ich", "nie", "euch", "zu", "Ge\u00b7sich\u00b7te", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "ADV", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Lufft vnd Zungen Kind, der Deutung Mutter bin,", "tokens": ["Der", "Lufft", "vnd", "Zun\u00b7gen", "Kind", ",", "der", "Deu\u00b7tung", "Mut\u00b7ter", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "NN", "$,", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00fchr eine Stimme zwar, nicht aber Geist noch Sinn?", "tokens": ["F\u00fchr", "ei\u00b7ne", "Stim\u00b7me", "zwar", ",", "nicht", "a\u00b7ber", "Geist", "noch", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "PTKNEG", "ADV", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die ich den letzten Klang am End erst wiederbringe", "tokens": ["Die", "ich", "den", "letz\u00b7ten", "Klang", "am", "End", "erst", "wie\u00b7der\u00b7brin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vnd nichts, ohn wa\u00df man mir erst vorsagt, schertzhafft singe?", "tokens": ["Vnd", "nichts", ",", "ohn", "wa\u00df", "man", "mir", "erst", "vor\u00b7sagt", ",", "schertz\u00b7hafft", "sin\u00b7ge", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "KOUI", "KOUS", "PIS", "PPER", "ADV", "VVPP", "$,", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich wohn in ewerm Ohr, vnd nirgends \u00fcberall,", "tokens": ["Ich", "wohn", "in", "e\u00b7werm", "Ohr", ",", "vnd", "nir\u00b7gends", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vnd wilst du recht mein Bild dir mahlen, mahl den Schall!", "tokens": ["Vnd", "wilst", "du", "recht", "mein", "Bild", "dir", "mah\u00b7len", ",", "mahl", "den", "Schall", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "PPOSAT", "NN", "PPER", "VVINF", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure