{"dta.poem.3819": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckwunsch an die Pathe,  \n oder Pathen-Briefgen.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebte Path, so bist du nun auch in dem aus-", "tokens": ["Ge\u00b7lieb\u00b7te", "Path", ",", "so", "bist", "du", "nun", "auch", "in", "dem", "aus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "erwehlten Orden,", "tokens": ["er\u00b7wehl\u00b7ten", "Or\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Durch diese reiche Seegens-Quell ein Glied an", "tokens": ["Durch", "die\u00b7se", "rei\u00b7che", "See\u00b7gens\u00b7Quell", "ein", "Glied", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Christi Leib geworden,", "tokens": ["Chris\u00b7ti", "Leib", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VAPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Dein Heiland thut dir seine Liebe, durch seinen Geist", "tokens": ["Dein", "Hei\u00b7land", "thut", "dir", "sei\u00b7ne", "Lie\u00b7be", ",", "durch", "sei\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "der Gnade kund,", "tokens": ["der", "Gna\u00b7de", "kund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ach halte doch, was du versprochen, brich nicht den", "tokens": ["Ach", "hal\u00b7te", "doch", ",", "was", "du", "ver\u00b7spro\u00b7chen", ",", "brich", "nicht", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "$,", "PWS", "PPER", "VVINF", "$,", "VVIMP", "PTKNEG", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "treugemeinten Bund,", "tokens": ["treu\u00b7ge\u00b7mein\u00b7ten", "Bund", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Denn wirst du ewig gl\u00fccklich seyn; du bist ein aus-", "tokens": ["Denn", "wirst", "du", "e\u00b7wig", "gl\u00fcck\u00b7lich", "seyn", ";", "du", "bist", "ein", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "ADJD", "VAINF", "$.", "PPER", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "erwehlter Christe,", "tokens": ["er\u00b7wehl\u00b7ter", "Chris\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Du hast des Satans Werck entsagt, so fliehe auch", "tokens": ["Du", "hast", "des", "Sa\u00b7tans", "Werck", "ent\u00b7sagt", ",", "so", "flie\u00b7he", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "die schn\u00f6den L\u00fcste.", "tokens": ["die", "schn\u00f6\u00b7den", "L\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Dein Wachsthum wird durch deinen Heiland Ja,", "tokens": ["Dein", "Wach\u00b7sthum", "wird", "durch", "dei\u00b7nen", "Hei\u00b7land", "Ja", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "PTKANT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Amen und gesegnet seyn,", "tokens": ["A\u00b7men", "und", "ge\u00b7seg\u00b7net", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Denn trifft der frommen Eltern W\u00fcnschen mit", "tokens": ["Denn", "trifft", "der", "from\u00b7men", "El\u00b7tern", "W\u00fcn\u00b7schen", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Freude und Vergn\u00fcgen ein.", "tokens": ["Freu\u00b7de", "und", "Ver\u00b7gn\u00fc\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "So wachse denn an Wissenschafft, an Alter, Jah-", "tokens": ["So", "wach\u00b7se", "denn", "an", "Wis\u00b7sen\u00b7schafft", ",", "an", "Al\u00b7ter", ",", "Jah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "$,", "APPR", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "ren und an Gl\u00fccke,", "tokens": ["ren", "und", "an", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Der treuste Himmel gebe dir die Anmuths-voll und", "tokens": ["Der", "treus\u00b7te", "Him\u00b7mel", "ge\u00b7be", "dir", "die", "An\u00b7muths\u00b7voll", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "holden Blicke,", "tokens": ["hol\u00b7den", "Bli\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Die\u00df flehet zu des H\u00f6chsten Throne, der dich bey dei-", "tokens": ["Die\u00df", "fle\u00b7het", "zu", "des", "H\u00f6chs\u00b7ten", "Thro\u00b7ne", ",", "der", "dich", "bey", "dei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "nem Bund vertrat,", "tokens": ["nem", "Bund", "ver\u00b7trat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.23": {"text": "Ein Hertze, das dich redlich liebet, dein treu und", "tokens": ["Ein", "Hert\u00b7ze", ",", "das", "dich", "red\u00b7lich", "lie\u00b7bet", ",", "dein", "treu", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "VVFIN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,", "PPOSAT", "ADJD", "KON"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "wohlgesinnter Path.", "tokens": ["wohl\u00b7ge\u00b7sinn\u00b7ter", "Path", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}