{"textgrid.poem.45984": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Einer Jungfrau bei dem Geschenk der Sakontala", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kluges mir geliebtes Wesen", "tokens": ["Ein", "klu\u00b7ges", "mir", "ge\u00b7lieb\u00b7tes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach gestern:", "tokens": ["Sprach", "ge\u00b7stern", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dieses Buchs Gestalt", "tokens": ["Die\u00b7ses", "Buchs", "Ge\u00b7stalt"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Schwebt mir im Sinn, seit ich's gelesen", "tokens": ["Schwebt", "mir", "im", "Sinn", ",", "seit", "ich's", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "PIS", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit einer r\u00fchrenden Gewalt.", "tokens": ["Mit", "ei\u00b7ner", "r\u00fch\u00b7ren\u00b7den", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kann mir es nicht anders denken,", "tokens": ["Ich", "kann", "mir", "es", "nicht", "an\u00b7ders", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als jener mag'schen Linie Spur,", "tokens": ["Als", "je\u00b7ner", "mag'\u00b7schen", "Li\u00b7nie", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In die sich Huld und Unschuld senken", "tokens": ["In", "die", "sich", "Huld", "und", "Un\u00b7schuld", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Zu rein jungfr\u00e4ulicher Figur.", "tokens": ["Zu", "rein", "jung\u00b7fr\u00e4u\u00b7li\u00b7cher", "Fi\u00b7gur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Unschuld'ger ist's als eine Blume,", "tokens": ["Un\u00b7schuld'\u00b7ger", "ist's", "als", "ei\u00b7ne", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Es denkt unschuldig \u2013 ist ein Geist,", "tokens": ["Es", "denkt", "un\u00b7schul\u00b7dig", "\u2013", "ist", "ein", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den, wie ein Kelch die Heiligtume,", "tokens": ["Den", ",", "wie", "ein", "Kelch", "die", "Hei\u00b7lig\u00b7tu\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ein klar durchsicht'ger Leib verschleu\u00dft.", "tokens": ["Ein", "klar", "durch\u00b7sicht'\u00b7ger", "Leib", "ver\u00b7schleu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Hier ist nicht Nacktheit, ist nicht H\u00fclle,", "tokens": ["Hier", "ist", "nicht", "Nackt\u00b7heit", ",", "ist", "nicht", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "$,", "VAFIN", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Hier ist nicht Schuld, nicht Kampf \u2013 hier ist,", "tokens": ["Hier", "ist", "nicht", "Schuld", ",", "nicht", "Kampf", "\u2013", "hier", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "$,", "PTKNEG", "NN", "$(", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df ich die Form mit Geist erf\u00fclle,", "tokens": ["Da\u00df", "ich", "die", "Form", "mit", "Geist", "er\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ein Wesen, wie du Freundin bist.", "tokens": ["Ein", "We\u00b7sen", ",", "wie", "du", "Freun\u00b7din", "bist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Als im verlorenen Paradiese", "tokens": ["Als", "im", "ver\u00b7lo\u00b7re\u00b7nen", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Du aus des Sch\u00f6pfers H\u00e4nden giengst,", "tokens": ["Du", "aus", "des", "Sch\u00f6p\u00b7fers", "H\u00e4n\u00b7den", "giengst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Auch du so klar und rein wie diese", "tokens": ["Auch", "du", "so", "klar", "und", "rein", "wie", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "ADJD", "KON", "ADJD", "KOKOM", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Sakontala den Geist empfliengst.", "tokens": ["Sa\u00b7kon\u00b7ta\u00b7la", "den", "Geist", "empf\u00b7liengst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und diesen Schein willst du nicht lassen,", "tokens": ["Und", "die\u00b7sen", "Schein", "willst", "du", "nicht", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Er ist ein Strahl aus Gottes Geist,", "tokens": ["Er", "ist", "ein", "Strahl", "aus", "Got\u00b7tes", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Will alle Farbe auch erblassen,", "tokens": ["Will", "al\u00b7le", "Far\u00b7be", "auch", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Dies Licht kein Tod dir je entrei\u00dft.", "tokens": ["Dies", "Licht", "kein", "Tod", "dir", "je", "ent\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "PIAT", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ich aber bringe dir den Spiegel,", "tokens": ["Ich", "a\u00b7ber", "brin\u00b7ge", "dir", "den", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Du schaust hinein, und kennst dich nicht,", "tokens": ["Du", "schaust", "hin\u00b7ein", ",", "und", "kennst", "dich", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Dein Sehen deckt der Demut Siegel,", "tokens": ["Dein", "Se\u00b7hen", "deckt", "der", "De\u00b7mut", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Das nur dein Richter einst zerbricht,", "tokens": ["Das", "nur", "dein", "Rich\u00b7ter", "einst", "zer\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "So w\u00e4re auch nach ihrem Wesen,", "tokens": ["So", "w\u00e4\u00b7re", "auch", "nach", "ih\u00b7rem", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Sakontala, die dir wohl gleicht,", "tokens": ["Sa\u00b7kon\u00b7ta\u00b7la", ",", "die", "dir", "wohl", "gleicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "F\u00fcr solchen Spiegel blind gewesen,", "tokens": ["F\u00fcr", "sol\u00b7chen", "Spie\u00b7gel", "blind", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "H\u00e4tt' man dein Bild ihr dargereicht.", "tokens": ["H\u00e4tt'", "man", "dein", "Bild", "ihr", "dar\u00b7ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Doch klingt ein Griff verwandter T\u00f6ne,", "tokens": ["Doch", "klingt", "ein", "Griff", "ver\u00b7wand\u00b7ter", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Den Gott in unsre Harfen tut,", "tokens": ["Den", "Gott", "in", "uns\u00b7re", "Har\u00b7fen", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Von je und jetzt in gleicher Sch\u00f6ne,", "tokens": ["Von", "je", "und", "jetzt", "in", "glei\u00b7cher", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Denn alles ist in ihm ja gut!", "tokens": ["Denn", "al\u00b7les", "ist", "in", "ihm", "ja", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein kluges mir geliebtes Wesen", "tokens": ["Ein", "klu\u00b7ges", "mir", "ge\u00b7lieb\u00b7tes", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach gestern:", "tokens": ["Sprach", "ge\u00b7stern", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dieses Buchs Gestalt", "tokens": ["Die\u00b7ses", "Buchs", "Ge\u00b7stalt"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Schwebt mir im Sinn, seit ich's gelesen", "tokens": ["Schwebt", "mir", "im", "Sinn", ",", "seit", "ich's", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "PIS", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit einer r\u00fchrenden Gewalt.", "tokens": ["Mit", "ei\u00b7ner", "r\u00fch\u00b7ren\u00b7den", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kann mir es nicht anders denken,", "tokens": ["Ich", "kann", "mir", "es", "nicht", "an\u00b7ders", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER