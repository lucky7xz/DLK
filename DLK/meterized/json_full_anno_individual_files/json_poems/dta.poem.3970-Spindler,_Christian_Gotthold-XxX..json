{"dta.poem.3970": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XxX.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Brief, gef\u00fcllt mit heissen Thr\u00e4nen,", "tokens": ["Ein", "Brief", ",", "ge\u00b7f\u00fcllt", "mit", "heis\u00b7sen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt dir ein jammernd \u00e4chtzend Sehnen", "tokens": ["Zeigt", "dir", "ein", "jam\u00b7mernd", "\u00e4cht\u00b7zend", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das tausend Seuffzer in sich fa\u00dft,", "tokens": ["Das", "tau\u00b7send", "Seuff\u00b7zer", "in", "sich", "fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Kind, Charlotte, ist erblast.", "tokens": ["Mein", "Kind", ",", "Char\u00b7lot\u00b7te", ",", "ist", "er\u00b7blast", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}