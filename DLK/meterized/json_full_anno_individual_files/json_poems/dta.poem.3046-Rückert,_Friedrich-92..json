{"dta.poem.3046": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Wahrheit treu zu seyn, die du in dir empfindest,", "tokens": ["Der", "Wahr\u00b7heit", "treu", "zu", "seyn", ",", "die", "du", "in", "dir", "emp\u00b7fin\u00b7dest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VAINF", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist der Schwur, von dem du nie dich selbst entbindest.", "tokens": ["Das", "ist", "der", "Schwur", ",", "von", "dem", "du", "nie", "dich", "selbst", "ent\u00b7bin\u00b7dest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "VVFIN", "$,", "APPR", "PRELS", "PPER", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Irrthum feind zu seyn, das geht unmittelbar", "tokens": ["Dem", "Irr\u00b7thum", "feind", "zu", "seyn", ",", "das", "geht", "un\u00b7mit\u00b7tel\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VAINF", "$,", "PDS", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daraus hervor, und bringt sogleich dich in Gefahr.", "tokens": ["Da\u00b7raus", "her\u00b7vor", ",", "und", "bringt", "sog\u00b7leich", "dich", "in", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn von dem Irrthum l\u00e4\u00dft sich diese Welt nicht scheiden;", "tokens": ["Denn", "von", "dem", "Irr\u00b7thum", "l\u00e4\u00dft", "sich", "die\u00b7se", "Welt", "nicht", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "PDAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer ihn nicht leiden will, dem mu\u00df sie selbst verleiden.", "tokens": ["Wer", "ihn", "nicht", "lei\u00b7den", "will", ",", "dem", "mu\u00df", "sie", "selbst", "ver\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Wahrheit ist der Welt durchaus nicht aufzudringen,", "tokens": ["Die", "Wahr\u00b7heit", "ist", "der", "Welt", "durc\u00b7haus", "nicht", "auf\u00b7zu\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Irrthum ist nur durch den andern zu bezwingen.", "tokens": ["Ein", "Irr\u00b7thum", "ist", "nur", "durch", "den", "an\u00b7dern", "zu", "be\u00b7zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Aeu\u00dferstes wird stets ein Aeu\u00dferstes verdr\u00e4ngen,", "tokens": ["Ein", "A\u00b7e\u00b7u\u00b7\u00dfers\u00b7tes", "wird", "stets", "ein", "A\u00b7e\u00b7u\u00b7\u00dfers\u00b7tes", "ver\u00b7dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und immer wird das Volk an andern G\u00f6tzen h\u00e4ngen.", "tokens": ["Und", "im\u00b7mer", "wird", "das", "Volk", "an", "an\u00b7dern", "G\u00f6t\u00b7zen", "h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch \u00e4rgern soll sich nicht an diesem Dienst der G\u00f6tzen,", "tokens": ["Doch", "\u00e4r\u00b7gern", "soll", "sich", "nicht", "an", "die\u00b7sem", "Dienst", "der", "G\u00f6t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PRF", "PTKNEG", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer sich im stillen kann an seinem Gott erg\u00f6tzen.", "tokens": ["Wer", "sich", "im", "stil\u00b7len", "kann", "an", "sei\u00b7nem", "Gott", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}