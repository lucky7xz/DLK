{"dta.poem.9827": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Wider die sich schminckende Frauens.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komt endlich nun die zeit/ da\u00df in der nacht-cornette", "tokens": ["Komt", "end\u00b7lich", "nun", "die", "zeit", "/", "da\u00df", "in", "der", "nacht\u00b7cor\u00b7net\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "ART", "NN", "$(", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sich zum schlaffe schickt/ so eile nicht zu bette;", "tokens": ["Sie", "sich", "zum", "schlaf\u00b7fe", "schickt", "/", "so", "ei\u00b7le", "nicht", "zu", "bet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPRART", "ADJA", "VVFIN", "$(", "ADV", "VVFIN", "PTKNEG", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wart\u2019 erst mein lieber mann/ bi\u00df deine sch\u00f6ne frau", "tokens": ["Wart'", "erst", "mein", "lie\u00b7ber", "mann", "/", "bi\u00df", "dei\u00b7ne", "sch\u00f6\u00b7ne", "frau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die farben ihrer haut dem nacht-tisch anvertrau;", "tokens": ["Die", "far\u00b7ben", "ih\u00b7rer", "haut", "dem", "nacht\u00b7tisch", "an\u00b7ver\u00b7trau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "VVFIN", "ART", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bi\u00df sie die lilien und rosen ihrer wangen", "tokens": ["Bi\u00df", "sie", "die", "li\u00b7li\u00b7en", "und", "ro\u00b7sen", "ih\u00b7rer", "wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Der w\u00e4scherin geschickt/ in t\u00fcchern aufgefangen;", "tokens": ["Der", "w\u00e4\u00b7sche\u00b7rin", "ge\u00b7schickt", "/", "in", "t\u00fc\u00b7chern", "auf\u00b7ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die zwar den gantzen tag ihr angesicht geputzt/", "tokens": ["Die", "zwar", "den", "gant\u00b7zen", "tag", "ihr", "an\u00b7ge\u00b7sicht", "ge\u00b7putzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nun aber auf einmahl vier t\u00fccher eingeschmutzt.", "tokens": ["Nun", "a\u00b7ber", "auf", "ein\u00b7mahl", "vier", "t\u00fc\u00b7cher", "ein\u00b7ge\u00b7schmutzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADV", "CARD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}