{"textgrid.poem.42714": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[ein m\u00e4nnlicher Briefmark erlebte]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein m\u00e4nnlicher Briefmark erlebte", "tokens": ["Ein", "m\u00e4nn\u00b7li\u00b7cher", "Brief\u00b7mark", "er\u00b7leb\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Was Sch\u00f6nes, bevor er klebte.", "tokens": ["Was", "Sch\u00f6\u00b7nes", ",", "be\u00b7vor", "er", "kleb\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er war von einer Prinzessin beleckt.", "tokens": ["Er", "war", "von", "ei\u00b7ner", "Prin\u00b7zes\u00b7sin", "be\u00b7leckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da war die Liebe in ihm erweckt.", "tokens": ["Da", "war", "die", "Lie\u00b7be", "in", "ihm", "er\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er wollte sie wiederk\u00fcssen,", "tokens": ["Er", "woll\u00b7te", "sie", "wie\u00b7der\u00b7k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da hat er verreisen m\u00fcssen.", "tokens": ["Da", "hat", "er", "ver\u00b7rei\u00b7sen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "So liebte er sie vergebens.", "tokens": ["So", "lieb\u00b7te", "er", "sie", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das ist die Tragik des Lebens!", "tokens": ["Das", "ist", "die", "Tra\u00b7gik", "des", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ein m\u00e4nnlicher Briefmark erlebte", "tokens": ["Ein", "m\u00e4nn\u00b7li\u00b7cher", "Brief\u00b7mark", "er\u00b7leb\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Was Sch\u00f6nes, bevor er klebte.", "tokens": ["Was", "Sch\u00f6\u00b7nes", ",", "be\u00b7vor", "er", "kleb\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er war von einer Prinzessin beleckt.", "tokens": ["Er", "war", "von", "ei\u00b7ner", "Prin\u00b7zes\u00b7sin", "be\u00b7leckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da war die Liebe in ihm erweckt.", "tokens": ["Da", "war", "die", "Lie\u00b7be", "in", "ihm", "er\u00b7weckt", "."], "token_info"