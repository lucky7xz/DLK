{"textgrid.poem.61446": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Acker, ewig umgew\u00fchlt vom Pfluge,", "tokens": ["Der", "A\u00b7cker", ",", "e\u00b7wig", "um\u00b7ge\u00b7w\u00fchlt", "vom", "Pflu\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ersch\u00f6pft sich endlich, gute Frucht zu tragen:", "tokens": ["Er\u00b7sch\u00f6pft", "sich", "end\u00b7lich", ",", "gu\u00b7te", "Frucht", "zu", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wird zuletzt nach h\u00f6chster Bl\u00fcte Tagen", "tokens": ["So", "wird", "zu\u00b7letzt", "nach", "h\u00f6chs\u00b7ter", "Bl\u00fc\u00b7te", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Geist der V\u00f6lker siech und lahm im Fluge.", "tokens": ["Der", "Geist", "der", "V\u00f6l\u00b7ker", "siech", "und", "lahm", "im", "Flu\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Wissen \u00fcbersch\u00e4rft sich selbst zum Luge,", "tokens": ["Das", "Wis\u00b7sen", "\u00fc\u00b7bersc\u00b7h\u00e4rft", "sich", "selbst", "zum", "Lu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Kunst wird Machwerk, alles Glauben Fragen,", "tokens": ["Die", "Kunst", "wird", "Mach\u00b7werk", ",", "al\u00b7les", "Glau\u00b7ben", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Zweifel, w\u00e4gend stets anstatt zu wagen,", "tokens": ["Und", "Zwei\u00b7fel", ",", "w\u00e4\u00b7gend", "stets", "an\u00b7statt", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KOUS", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "W\u00fcrgt jede Tat beim ersten Atemzuge.", "tokens": ["W\u00fcrgt", "je\u00b7de", "Tat", "beim", "ers\u00b7ten", "A\u00b7tem\u00b7zu\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ausging die Zeugung, w\u00e4hrend tausend Zungen", "tokens": ["Aus\u00b7ging", "die", "Zeu\u00b7gung", ",", "w\u00e4h\u00b7rend", "tau\u00b7send", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Freiheit, Kraft und Gr\u00f6\u00dfe prahlend dichten,", "tokens": ["Von", "Frei\u00b7heit", ",", "Kraft", "und", "Gr\u00f6\u00b7\u00dfe", "prah\u00b7lend", "dich\u00b7ten", ","], "token_info":