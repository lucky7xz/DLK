{"textgrid.poem.44423": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Notturno", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Z\u00f6gernd, stille,", "tokens": ["Z\u00f6\u00b7gernd", ",", "stil\u00b7le", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "In des Dunkels n\u00e4chtger H\u00fclle,", "tokens": ["In", "des", "Dun\u00b7kels", "n\u00e4cht\u00b7ger", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind wir hier.", "tokens": ["Sind", "wir", "hier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und, den Finger leicht gekr\u00fcmmt,", "tokens": ["Und", ",", "den", "Fin\u00b7ger", "leicht", "ge\u00b7kr\u00fcmmt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leise, leise,", "tokens": ["Lei\u00b7se", ",", "lei\u00b7se", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Pochen wir", "tokens": ["Po\u00b7chen", "wir"], "token_info": ["word", "word"], "pos": ["NN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "An des Liebchens Kammert\u00fcr.", "tokens": ["An", "des", "Lieb\u00b7chens", "Kam\u00b7mer\u00b7t\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nun steigend,", "tokens": ["Doch", "nun", "stei\u00b7gend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Hebend, schwellend,", "tokens": ["He\u00b7bend", ",", "schwel\u00b7lend", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Stark und st\u00e4rker, lauter, laut", "tokens": ["Stark", "und", "st\u00e4r\u00b7ker", ",", "lau\u00b7ter", ",", "laut"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "KON", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rufen aus wir hochvertraut:", "tokens": ["Ru\u00b7fen", "aus", "wir", "hoch\u00b7ver\u00b7traut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schlaf du nicht,", "tokens": ["Schlaf", "du", "nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wenn der Neigung Stimme spricht!", "tokens": ["Wenn", "der", "Nei\u00b7gung", "Stim\u00b7me", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sucht' ein Weiser nah und ferne", "tokens": ["Sucht'", "ein", "Wei\u00b7ser", "nah", "und", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschen einst mit der Laterne;", "tokens": ["Men\u