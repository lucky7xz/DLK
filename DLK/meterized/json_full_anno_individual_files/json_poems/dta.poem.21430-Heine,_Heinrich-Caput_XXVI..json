{"dta.poem.21430": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ein gl\u00e4nzender Uniform erscheint", "tokens": ["\u201e", "in", "gl\u00e4n\u00b7zen\u00b7der", "U\u00b7ni\u00b7form", "er\u00b7scheint"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Corps der Diplomaten;", "tokens": ["Das", "Corps", "der", "Dip\u00b7lo\u00b7ma\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie gratuliren mit Vorbehalt", "tokens": ["Sie", "gra\u00b7tu\u00b7li\u00b7ren", "mit", "Vor\u00b7be\u00b7halt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Im Namen der Nachbarstaaten.", "tokens": ["Im", "Na\u00b7men", "der", "Nach\u00b7bar\u00b7staa\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201ees kommt die geistliche Deputazion,", "tokens": ["\u201e", "es", "kommt", "die", "geist\u00b7li\u00b7che", "De\u00b7pu\u00b7ta\u00b7zi\u00b7on", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rabiner und Past\u00f6re \u2013", "tokens": ["Ra\u00b7bi\u00b7ner", "und", "Pas\u00b7t\u00f6\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ach! da kommt der Hoffmann auch", "tokens": ["Doch", "ach", "!", "da", "kommt", "der", "Hoff\u00b7mann", "auch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$.", "ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seiner Censorscheere!", "tokens": ["Mit", "sei\u00b7ner", "Cen\u00b7sor\u00b7schee\u00b7re", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201edie Scheere klirrt in seiner Hand,", "tokens": ["\u201e", "die", "Schee\u00b7re", "klirrt", "in", "sei\u00b7ner", "Hand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es r\u00fcckt der wilde Geselle", "tokens": ["Es", "r\u00fcckt", "der", "wil\u00b7de", "Ge\u00b7sel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dir auf den Leib \u2013 Er schneidet in\u2019s Fleisch \u2013", "tokens": ["Dir", "auf", "den", "Leib", "\u2013", "Er", "schnei\u00b7det", "in's", "Fleisch", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "APPRART", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Es war die beste Stelle.\u201c", "tokens": ["Es", "war", "die", "bes\u00b7te", "Stel\u00b7le", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}