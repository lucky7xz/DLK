{"textgrid.poem.59708": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Waldgrutz", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr, s\u00fc\u00dfe Blumen, gr\u00fcne Haine,", "tokens": ["Ihr", ",", "s\u00fc\u00b7\u00dfe", "Blu\u00b7men", ",", "gr\u00fc\u00b7ne", "Hai\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O seid ihr endlich wieder mein?", "tokens": ["O", "seid", "ihr", "end\u00b7lich", "wie\u00b7der", "mein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In euch geborgen gar alleine,", "tokens": ["In", "euch", "ge\u00b7bor\u00b7gen", "gar", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nie bin ich bei euch allein:", "tokens": ["Doch", "nie", "bin", "ich", "bei", "euch", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr sprecht mit wundersamer Stimme", "tokens": ["Ihr", "sprecht", "mit", "wun\u00b7der\u00b7sa\u00b7mer", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die einz'ge Sprache ohne Trug,", "tokens": ["Die", "einz'\u00b7ge", "Spra\u00b7che", "oh\u00b7ne", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Vogel predigt hier, die Imme,", "tokens": ["Der", "Vo\u00b7gel", "pre\u00b7digt", "hier", ",", "die", "Im\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Bl\u00fctenzweig wie Gottes Buch.", "tokens": ["Der", "Bl\u00fc\u00b7ten\u00b7zweig", "wie", "Got\u00b7tes", "Buch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Gottes Buch! O welche Kl\u00e4nge", "tokens": ["O", "Got\u00b7tes", "Buch", "!", "O", "wel\u00b7che", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "NN", "$.", "NE", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus allerstillster Einsamkeit!", "tokens": ["Aus", "al\u00b7ler\u00b7stills\u00b7ter", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entflohn dem wilden Weltgedr\u00e4nge", "tokens": ["Ent\u00b7flohn", "dem", "wil\u00b7den", "Welt\u00b7ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu h\u00f6hrer Welt Gemeinsamkeit:", "tokens": ["Zu", "h\u00f6h\u00b7rer", "Welt", "Ge\u00b7mein\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn wie aus l\u00e4ngst vergangnen Tagen,", "tokens": ["Denn", "wie", "aus", "l\u00e4ngst", "ver\u00b7gang\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie aus der Geister Ewigkeit", "tokens": ["Wie", "aus", "der", "Geis\u00b7ter", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Haucht's hier von Fabeln und von Sagen", "tokens": ["Haucht's", "hier", "von", "Fa\u00b7beln", "und", "von", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So dicht, als Lenzwind Bl\u00fcten schneit.", "tokens": ["So", "dicht", ",", "als", "Lenz\u00b7wind", "Bl\u00fc\u00b7ten", "schneit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Gottes Buch! O heil'ge M\u00e4chte!", "tokens": ["O", "Got\u00b7tes", "Buch", "!", "O", "heil'\u00b7ge", "M\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hier brecht ihr alle Siegel auf:", "tokens": ["Hier", "brecht", "ihr", "al\u00b7le", "Sie\u00b7gel", "auf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geheimnis stummer Mittern\u00e4chte", "tokens": ["Ge\u00b7heim\u00b7nis", "stum\u00b7mer", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Sonnenlauf und Mondenlauf,", "tokens": ["Und", "Son\u00b7nen\u00b7lauf", "und", "Mon\u00b7den\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was von irren Wandelsternen", "tokens": ["Und", "was", "von", "ir\u00b7ren", "Wan\u00b7dels\u00b7ter\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die tiefe Menschenbrust durchkreist,", "tokens": ["Die", "tie\u00b7fe", "Men\u00b7schen\u00b7brust", "durch\u00b7kreist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kann hier der stille Lauscher lernen,", "tokens": ["Kann", "hier", "der", "stil\u00b7le", "Lau\u00b7scher", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo alles hoch nach oben weist.", "tokens": ["Wo", "al\u00b7les", "hoch", "nach", "o\u00b7ben", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Gottes Buch! O s\u00fc\u00dfes Wehen,", "tokens": ["O", "Got\u00b7tes", "Buch", "!", "O", "s\u00fc\u00b7\u00dfes", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$.", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das s\u00e4uselnd durch die Zweige geht!", "tokens": ["Das", "s\u00e4u\u00b7selnd", "durch", "die", "Zwei\u00b7ge", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O leises Fl\u00fcstern aus den H\u00f6hen,", "tokens": ["O", "lei\u00b7ses", "Fl\u00fcs\u00b7tern", "aus", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo aller Herzen Sonne steht!", "tokens": ["Wo", "al\u00b7ler", "Her\u00b7zen", "Son\u00b7ne", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O s\u00fc\u00dfes Ahnen, s\u00fc\u00dfes Sehnen!", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Ah\u00b7nen", ",", "s\u00fc\u00b7\u00dfes", "Seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier ist dein trauter Liebesort,", "tokens": ["Hier", "ist", "dein", "trau\u00b7ter", "Lie\u00b7be\u00b7sort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hier findet Gram die ersten Tr\u00e4nen", "tokens": ["Hier", "fin\u00b7det", "Gram", "die", "ers\u00b7ten", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Zorn sein mild Vers\u00f6hnungswort.", "tokens": ["Und", "Zorn", "sein", "mild", "Ver\u00b7s\u00f6h\u00b7nungs\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum kommt, ihr Blumen, kommt, ihr Haine,", "tokens": ["Drum", "kommt", ",", "ihr", "Blu\u00b7men", ",", "kommt", ",", "ihr", "Hai\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Komm, stille, fromme Waldesnacht,", "tokens": ["Komm", ",", "stil\u00b7le", ",", "from\u00b7me", "Wal\u00b7des\u00b7nacht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und werdet, bleibet ewig meine", "tokens": ["Und", "wer\u00b7det", ",", "blei\u00b7bet", "e\u00b7wig", "mei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "$,", "VVFIN", "ADJD", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit aller s\u00fc\u00dfen Gottesmacht,", "tokens": ["Mit", "al\u00b7ler", "s\u00fc\u00b7\u00dfen", "Got\u00b7tes\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit allen V\u00f6geln, allen Immen,", "tokens": ["Mit", "al\u00b7len", "V\u00f6\u00b7geln", ",", "al\u00b7len", "Im\u00b7men", ","], "token_info": ["word", "word",