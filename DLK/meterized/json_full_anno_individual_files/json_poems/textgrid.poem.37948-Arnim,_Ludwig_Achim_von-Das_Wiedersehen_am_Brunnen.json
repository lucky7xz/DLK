_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich hab schon lang gestanden hier,", "tokens": ["Ich", "hab", "schon", "lang", "ge\u00b7stan\u00b7den", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich stand allhier wohl sieben Jahr.\u00ab", "tokens": ["Ich", "stand", "all\u00b7hier", "wohl", "sie\u00b7ben", "Jahr", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "CARD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbhast lang gestanden, das ist nicht wahr,", "tokens": ["\u00bb", "hast", "lang", "ge\u00b7stan\u00b7den", ",", "das", "ist", "nicht", "wahr", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADJD", "VVPP", "$,", "PDS", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich hab noch nicht geschlafen.", "tokens": ["Ich", "hab", "noch", "nicht", "ge\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich hab gelegn und hab gedacht,", "tokens": ["Ich", "hab", "ge\u00b7legn", "und", "hab", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo nur mein Schatz noch bleiben mag,", "tokens": ["Wo", "nur", "mein", "Schatz", "noch", "blei\u00b7ben", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er macht mir allzulang, zu lang,", "tokens": ["Er", "macht", "mir", "all\u00b7zu\u00b7lang", ",", "zu", "lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir wird ganz angst und bange.\u00ab", "tokens": ["Mir", "wird", "ganz", "angst", "und", "ban\u00b7ge", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbwo ich so lang geblieben bin,", "tokens": ["\u00bb", "wo", "ich", "so", "lang", "ge\u00b7blie\u00b7ben", "bin", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ADV", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das darf dir wohl gesaget seyn,", "tokens": ["Das", "darf", "dir", "wohl", "ge\u00b7sa\u00b7get", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey Bier und Wein, wo Jungfern seyn,", "tokens": ["Bey", "Bier", "und", "Wein", ",", "wo", "Jung\u00b7fern", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PWAV", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da bin ich allzeit gerne.\u00ab", "tokens": ["Da", "bin", "ich", "all\u00b7zeit", "ger\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es war wohl um die Mitternacht,", "tokens": ["Es", "war", "wohl", "um", "die", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der W\u00e4chter fing zu l\u00e4uten an:", "tokens": ["Der", "W\u00e4ch\u00b7ter", "fing", "zu", "l\u00e4u\u00b7ten", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKZU", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbsteh auf, wer bey Feinsliebchen liegt,", "tokens": ["\u00bb", "steh", "auf", ",", "wer", "bey", "Feins\u00b7lieb\u00b7chen", "liegt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tag kommt angeschlichen.\u00ab", "tokens": ["Der", "Tag", "kommt", "an\u00b7ge\u00b7schli\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das B\u00fcrschlein auf die Leiter sprang,", "tokens": ["Das", "B\u00fcr\u00b7schlein", "auf", "die", "Lei\u00b7ter", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und 