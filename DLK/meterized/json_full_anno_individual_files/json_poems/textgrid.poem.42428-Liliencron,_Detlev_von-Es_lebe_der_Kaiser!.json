{"textgrid.poem.42428": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Es lebe der Kaiser!", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war die Zeit um Sonnenuntergang,", "tokens": ["Es", "war", "die", "Zeit", "um", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich kam vom linken Fl\u00fcgel hergejagt.", "tokens": ["Ich", "kam", "vom", "lin\u00b7ken", "Fl\u00fc\u00b7gel", "her\u00b7ge\u00b7jagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Granaten heulten, hei\u00df im M\u00f6rderdrang,", "tokens": ["Gra\u00b7na\u00b7ten", "heul\u00b7ten", ",", "hei\u00df", "im", "M\u00f6r\u00b7der\u00b7drang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Hol' euch die Pest, wohin ihr immer schlagt.", "tokens": ["Hol'", "euch", "die", "Pest", ",", "wo\u00b7hin", "ihr", "im\u00b7mer", "schlagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich flog indessen, das war nichts gewagt,", "tokens": ["Ich", "flog", "in\u00b7des\u00b7sen", ",", "das", "war", "nichts", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PDS", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Unter sich kreuzendem Gescho\u00df in Mitten.", "tokens": ["Un\u00b7ter", "sich", "kreu\u00b7zen\u00b7dem", "Ge\u00b7scho\u00df", "in", "Mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Rechts reden unsre Rohre, ungefragt,", "tokens": ["Rechts", "re\u00b7den", "uns\u00b7re", "Roh\u00b7re", ",", "un\u00b7ge\u00b7fragt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Links wollen feindliche sich das verbitten.", "tokens": ["Links", "wol\u00b7len", "feind\u00b7li\u00b7che", "sich", "das", "ver\u00b7bit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADJA", "PRF", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Gez\u00e4nk und Anspuken, ich bin hindurchgeritten.", "tokens": ["Ge\u00b7z\u00e4nk", "und", "A\u00b7nspu\u00b7ken", ",", "ich", "bin", "hin\u00b7durch\u00b7ge\u00b7rit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Pl\u00f6tzlich erkenn' ich einen Johanniter", "tokens": ["Pl\u00f6tz\u00b7lich", "er\u00b7kenn'", "ich", "ei\u00b7nen", "Jo\u00b7han\u00b7ni\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Am roten Kreuz auf seiner wei\u00dfen Binde.", "tokens": ["Am", "ro\u00b7ten", "Kreuz", "auf", "sei\u00b7ner", "wei\u00b7\u00dfen", "Bin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo kommst du her, du schneidiger Samariter,", "tokens": ["Wo", "kommst", "du", "her", ",", "du", "schnei\u00b7di\u00b7ger", "Sa\u00b7ma\u00b7ri\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was trieb dich, da\u00df ich hier im Kampf dich finde?", "tokens": ["Was", "trieb", "dich", ",", "da\u00df", "ich", "hier", "im", "Kampf", "dich", "fin\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Er aber ri\u00df vom Haupt den Hut geschwinde,", "tokens": ["Er", "a\u00b7ber", "ri\u00df", "vom", "Haupt", "den", "Hut", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und schwang ihn viel, den seltnen L\u00fcftekreiser,", "tokens": ["Und", "schwang", "ihn", "viel", ",", "den", "selt\u00b7nen", "L\u00fcf\u00b7te\u00b7krei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und schwang ihn hoch im schwachen Abendwinde,", "tokens": ["Und", "schwang", "ihn", "hoch", "im", "schwa\u00b7chen", "Ab\u00b7end\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und rief, vom Reiten angestrengt und heiser,", "tokens": ["Und", "rief", ",", "vom", "Rei\u00b7ten", "an\u00b7ge\u00b7strengt", "und", "hei\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPRART", "NN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Gestern ward unser greiser, gro\u00dfer K\u00f6nig Kaiser.", "tokens": ["Ge\u00b7stern", "ward", "un\u00b7ser", "grei\u00b7ser", ",", "gro\u00b7\u00dfer", "K\u00f6\u00b7nig", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und zum Salute donnern die Batterieen", "tokens": ["Und", "zum", "Sa\u00b7lu\u00b7te", "don\u00b7nern", "die", "Bat\u00b7te\u00b7ri\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Kaisergru\u00df, wie niemals er gebracht.", "tokens": ["Den", "Kai\u00b7ser\u00b7gru\u00df", ",", "wie", "nie\u00b7mals", "er", "ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zweihundertf\u00fcnfzig hei\u00dfe Munde schrieen", "tokens": ["Zwei\u00b7hun\u00b7dert\u00b7f\u00fcnf\u00b7zig", "hei\u00b7\u00dfe", "Mun\u00b7de", "schri\u00b7een"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Gru\u00df hinaus mit aller Atemmacht.", "tokens": ["Den", "Gru\u00df", "hin\u00b7aus", "mit", "al\u00b7ler", "A\u00b7tem\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APZR", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Scheu schielt aus gelbges\u00e4umter Wolkennacht", "tokens": ["Scheu", "schielt", "aus", "gelb\u00b7ge\u00b7s\u00e4um\u00b7ter", "Wol\u00b7ken\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zum ersten Mal die wei\u00dfe Wintersonne,", "tokens": ["Zum", "ers\u00b7ten", "Mal", "die", "wei\u00b7\u00dfe", "Win\u00b7ter\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und schwefelfarben leuchtete die Schlacht", "tokens": ["Und", "schwe\u00b7fel\u00b7far\u00b7ben", "leuch\u00b7te\u00b7te", "die", "Schlacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bis auf die fernst marschierende Kolonne \u2013", "tokens": ["Bis", "auf", "die", "fernst", "mar\u00b7schie\u00b7ren\u00b7de", "Ko\u00b7lon\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df hoch mein jung Soldatenherze schlug in Wonne.", "tokens": ["Da\u00df", "hoch", "mein", "jung", "Sol\u00b7da\u00b7ten\u00b7her\u00b7ze", "schlug", "in", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Tot lag vor mir ein Garde mobile du Nord,", "tokens": ["Tot", "lag", "vor", "mir", "ein", "Gar\u00b7de", "mo\u00b7bi\u00b7le", "du", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ART", "NN", "VMFIN", "PPER", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es scharrt mein Fuchs und blies ihm in die Haare.", "tokens": ["Es", "scharrt", "mein", "Fuchs", "und", "blies", "ihm", "in", "die", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NE", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da klang ein Ton her\u00fcber an mein Ohr,", "tokens": ["Da", "klang", "ein", "Ton", "her\u00b7\u00fc\u00b7ber", "an", "mein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den H\u00f6llenl\u00e4rm durchstie\u00df der Ton, der klare.", "tokens": ["Den", "H\u00f6l\u00b7len\u00b7l\u00e4rm", "durch\u00b7stie\u00df", "der", "Ton", ",", "der", "kla\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "N\u00fcchtern, nicht wie die schmetternde Fanfare,", "tokens": ["N\u00fcch\u00b7tern", ",", "nicht", "wie", "die", "schmet\u00b7tern\u00b7de", "Fan\u00b7fa\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Klang her das Horn von jenen Musketieren.", "tokens": ["Klang", "her", "das", "Horn", "von", "je\u00b7nen", "Mus\u00b7ke\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Da\u00df dir, mein Vaterland, es Gott bewahre,", "tokens": ["Da\u00df", "dir", ",", "mein", "Va\u00b7ter\u00b7land", ",", "es", "Gott", "be\u00b7wah\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das Infanterie Signal zum Avancieren.", "tokens": ["Das", "In\u00b7fan\u00b7te\u00b7rie", "Sig\u00b7nal", "zum", "A\u00b7van\u00b7cie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Dann bist du sicher vor Franzosen und Baschkiren.", "tokens": ["Dann", "bist", "du", "si\u00b7cher", "vor", "Fran\u00b7zo\u00b7sen", "und", "Baschki\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Zum Sturm, zum Sturm! Die H\u00f6rner schreien! Drauf!", "tokens": ["Zum", "Sturm", ",", "zum", "Sturm", "!", "Die", "H\u00f6r\u00b7ner", "schrei\u00b7en", "!", "Drauf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$.", "ART", "NN", "VVFIN", "$.", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es sprang mein Degen zischend aus dem Gatter.", "tokens": ["Es", "sprang", "mein", "De\u00b7gen", "zi\u00b7schend", "aus", "dem", "Gat\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und rechts und links, wo nur ein Flintenlauf,", "tokens": ["Und", "rechts", "und", "links", ",", "wo", "nur", "ein", "Flin\u00b7ten\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "$,", "PWAV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich ri\u00df ihn mit ins feindliche Geknatter.", "tokens": ["Ich", "ri\u00df", "ihn", "mit", "ins", "feind\u00b7li\u00b7che", "Ge\u00b7knat\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Lerman, Lerman! Durch Blut, Gewehrgeschnatter,", "tokens": ["Ler\u00b7man", ",", "Ler\u00b7man", "!", "Durch", "Blut", ",", "Ge\u00b7wehr\u00b7ge\u00b7schnat\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "APPR", "NN", "$,", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Durch Schutt und Qualm! Schon fliehn die Kugelspritzen.", "tokens": ["Durch", "Schutt", "und", "Qualm", "!", "Schon", "fliehn", "die", "Ku\u00b7gel\u00b7sprit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Wolf brach ein, und matter wird und matter", "tokens": ["Der", "Wolf", "brach", "ein", ",", "und", "mat\u00b7ter", "wird", "und", "mat\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "VAFIN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Widerstand, wo seine Z\u00e4hne blitzen.", "tokens": ["Der", "Wi\u00b7der\u00b7stand", ",", "wo", "sei\u00b7ne", "Z\u00e4h\u00b7ne", "blit\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und Siegesband umflattert unsre Fahnenspitze.", "tokens": ["Und", "Sie\u00b7ges\u00b7band", "um\u00b7flat\u00b7tert", "uns\u00b7re", "Fah\u00b7nens\u00b7pit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es war die Zeit um Sonnenuntergang,", "tokens": ["Es", "war", "die", "Zeit", "um", "Son\u00b7nen\u00b7un\u00b7ter\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$