{"textgrid.poem.44291": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Genius der Zukunft", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn bei mancher Kluft im Leben", "tokens": ["Wenn", "bei", "man\u00b7cher", "Kluft", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PIAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hocherschreckt die Menschen beben,", "tokens": ["Ho\u00b7cher\u00b7schreckt", "die", "Men\u00b7schen", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gram das wunde Herz durchdringt,", "tokens": ["Gram", "das", "wun\u00b7de", "Herz", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Brust Verzweiflung ringt,", "tokens": ["In", "der", "Brust", "Ver\u00b7zwei\u00b7flung", "ringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Siehe, da f\u00fchrt sie mit warnendem Blicke,", "tokens": ["Sie\u00b7he", ",", "da", "f\u00fchrt", "sie", "mit", "war\u00b7nen\u00b7dem", "Bli\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "L\u00e4chelnd der Genius vom Abgrund zur\u00fccke,", "tokens": ["L\