s": ["KOUS", "ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei des einzgen Sohnes Grabe,", "tokens": ["Bei", "des", "einz\u00b7gen", "Soh\u00b7nes", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm so manche Tr\u00e4ne zollt,", "tokens": ["Ihm", "so", "man\u00b7che", "Tr\u00e4\u00b7ne", "zollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die dem matten Aug entrollt.", "tokens": ["Die", "dem", "mat\u00b7ten", "Aug", "ent\u00b7rollt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Holder, da zeigst mit belebender Milde", "tokens": ["Hol\u00b7der", ",", "da", "zeigst", "mit", "be\u00b7le\u00b7ben\u00b7der", "Mil\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Du ihm den Sohn in Elysiens Gefilde,", "tokens": ["Du", "ihm", "den", "Sohn", "in", "E\u00b7ly\u00b7si\u00b7ens", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sehet, an Lethens begl\u00fcckenden Strand,", "tokens": ["Se\u00b7het", ",", "an", "Le\u00b7thens", "be\u00b7gl\u00fc\u00b7cken\u00b7den", "Strand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR