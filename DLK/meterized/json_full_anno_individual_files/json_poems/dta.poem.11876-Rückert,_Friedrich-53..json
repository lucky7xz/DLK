{"dta.poem.11876": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solange du nur denkst, ohn' es in dir zu f\u00fchlen,", "tokens": ["So\u00b7lan\u00b7ge", "du", "nur", "denkst", ",", "ohn'", "es", "in", "dir", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "$,", "KOUI", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird ein Gedanke nur den andern weiter sp\u00fclen.", "tokens": ["Wird", "ein", "Ge\u00b7dan\u00b7ke", "nur", "den", "an\u00b7dern", "wei\u00b7ter", "sp\u00fc\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ART", "ADJA", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht wahr ist was du denkst, nur was du f\u00fchlst ist wahr;", "tokens": ["Nicht", "wahr", "ist", "was", "du", "denkst", ",", "nur", "was", "du", "f\u00fchlst", "ist", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PWS", "PPER", "VVFIN", "$,", "ADV", "PWS", "PPER", "VVFIN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchs Denken machst du dir nur das Gef\u00fchlte klar.", "tokens": ["Durchs", "Den\u00b7ken", "machst", "du", "dir", "nur", "das", "Ge\u00b7f\u00fchl\u00b7te", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was du Gef\u00fchltes denkst, das wirst du auch behalten,", "tokens": ["Was", "du", "Ge\u00b7f\u00fchl\u00b7tes", "denkst", ",", "das", "wirst", "du", "auch", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und im Ged\u00e4chtnis wird dirs ewig nicht veralten.", "tokens": ["Und", "im", "Ge\u00b7d\u00e4cht\u00b7nis", "wird", "dirs", "e\u00b7wig", "nicht", "ver\u00b7al\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PIS", "ADJD", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das seinen Namen zwar vom Denken hat empfangen,", "tokens": ["Das", "sei\u00b7nen", "Na\u00b7men", "zwar", "vom", "Den\u00b7ken", "hat", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nur Gef\u00fchltes bleibt im Angedenken hangen.", "tokens": ["Doch", "nur", "Ge\u00b7f\u00fchl\u00b7tes", "bleibt", "im", "An\u00b7ge\u00b7den\u00b7ken", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}