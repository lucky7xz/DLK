{"textgrid.poem.41617": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer hat dir, fragtest du, dies fremde Weh gegeben,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer hat dir, fragtest du, dies fremde Weh gegeben,", "tokens": ["Wer", "hat", "dir", ",", "frag\u00b7test", "du", ",", "dies", "frem\u00b7de", "Weh", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "PDS", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Meere gleich, das sich an schwarzen Klippen bricht?", "tokens": ["Dem", "Mee\u00b7re", "gleich", ",", "das", "sich", "an", "schwar\u00b7zen", "Klip\u00b7pen", "bricht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u2013 Hat unser Herz einmal geerntet, ist das Leben", "tokens": ["\u2013", "Hat", "un\u00b7ser", "Herz", "ein\u00b7mal", "ge\u00b7ern\u00b7tet", ",", "ist", "das", "Le\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur noch ein Leiden! Fremd ist dies Geheimnis nicht,", "tokens": ["Nur", "noch", "ein", "Lei\u00b7den", "!", "Fremd", "ist", "dies", "Ge\u00b7heim\u00b7nis", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$.", "NN", "VAFIN", "PDS", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es ist ein schlichter Schmerz, der nicht in Nacht verh\u00fcllt ist", "tokens": ["Es", "ist", "ein", "schlich\u00b7ter", "Schmerz", ",", "der", "nicht", "in", "Nacht", "ver\u00b7h\u00fcllt", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "APPR", "NN", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deiner Freude gleich sich ruhig zeigen will.", "tokens": ["Und", "dei\u00b7ner", "Freu\u00b7de", "gleich", "sich", "ru\u00b7hig", "zei\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "PRF", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum frag nicht, Sch\u00f6ne, die von Neugier ganz erf\u00fcllt ist!", "tokens": ["Drum", "frag", "nicht", ",", "Sch\u00f6\u00b7ne", ",", "die", "von", "Neu\u00b7gier", "ganz", "er\u00b7f\u00fcllt", "ist", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "NN", "$,", "PRELS", "APPR", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sei deiner Stimme Klang auch lieblich, schweige still!", "tokens": ["Sei", "dei\u00b7ner", "Stim\u00b7me", "Klang", "auch", "lieb\u00b7lich", ",", "schwei\u00b7ge", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "ADV", "ADJD", "$,", "VVFIN", "PTKVZ", "$."],