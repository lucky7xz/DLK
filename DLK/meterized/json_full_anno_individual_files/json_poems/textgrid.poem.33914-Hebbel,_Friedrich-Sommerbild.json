{"textgrid.poem.33914": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sommerbild", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah des Sommers letzte Rose steh'n,", "tokens": ["Ich", "sah", "des", "Som\u00b7mers", "letz\u00b7te", "Ro\u00b7se", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie war, als ob sie bluten k\u00f6nne, roth;", "tokens": ["Sie", "war", ",", "als", "ob", "sie", "blu\u00b7ten", "k\u00f6n\u00b7ne", ",", "roth", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da sprach ich schauernd im Vor\u00fcbergeh'n:", "tokens": ["Da", "sprach", "ich", "schau\u00b7ernd", "im", "Vor\u00b7\u00fcber\u00b7geh'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So weit im Leben, ist zu nah' am Tod!", "tokens": ["So", "weit", "im", "Le\u00b7ben", ",", "ist", "zu", "nah'", "am", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,", "VAFIN", "PTKA", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es regte sich kein Hauch am hei\u00dfen Tag,", "tokens": ["Es", "reg\u00b7te", "sich", "kein", "Hauch", "am", "hei\u00b7\u00dfen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nur leise strich ein wei\u00dfer Schmetterling;", "tokens": ["Nur", "lei\u00b7se", "strich", "ein", "wei\u00b7\u00dfer", "Schmet\u00b7ter\u00b7ling", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch, ob auch kaum die Luft sein Fl\u00fcgelschlag", "tokens": ["Doch", ",", "ob", "auch", "kaum", "die", "Luft", "sein", "Fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bewegte, sie empfand es und verging.", "tokens": ["Be\u00b7weg\u00b7te", ",", "sie", "emp\u00b7fand", "es", "und", "ver\u00b7ging", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sah des Sommers letzte Rose steh'n,", "tokens": ["Ich", "sah", "des", "Som\u00b7mers", "letz\u00b7te", "Ro\u00b7se", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie war, als ob sie bluten k\u00f6nne, roth;", "tokens": ["Sie", "war", ",", "als", "ob", "sie", "blu\u00b7ten", "k\u00f6n\u00b7ne", ",", "roth", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["P