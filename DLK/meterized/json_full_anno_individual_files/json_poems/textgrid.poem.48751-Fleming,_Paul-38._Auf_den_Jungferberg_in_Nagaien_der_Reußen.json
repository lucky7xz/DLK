{"textgrid.poem.48751": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "38. Auf den Jungferberg in Nagaien der Reu\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ists, wie ihr Keu\u00dfen es nicht lasset unerwiesen,", "tokens": ["Ists", ",", "wie", "ihr", "Keu\u00b7\u00dfen", "es", "nicht", "las\u00b7set", "un\u00b7er\u00b7wie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VVFIN", "PPER", "PTKNEG", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df Niemand von uns wei\u00df, ist di\u00df derselbe Berg,", "tokens": ["da\u00df", "Nie\u00b7mand", "von", "uns", "wei\u00df", ",", "ist", "di\u00df", "der\u00b7sel\u00b7be", "Berg", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "VVFIN", "$,", "VAFIN", "PDS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "auf dem auf eine Zeit gewohnt ein kluger Zwerg", "tokens": ["auf", "dem", "auf", "ei\u00b7ne", "Zeit", "ge\u00b7wohnt", "ein", "klu\u00b7ger", "Zwerg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ART", "NN", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und eine Jungfrau auch, der Ankunft von den Riesen,", "tokens": ["und", "ei\u00b7ne", "Jung\u00b7frau", "auch", ",", "der", "An\u00b7kunft", "von", "den", "Rie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "darvon er itzt noch hei\u00dft, und langet bis nach", "tokens": ["dar\u00b7von", "er", "itzt", "noch", "hei\u00dft", ",", "und", "lan\u00b7get", "bis", "nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "ADV", "VVFIN", "$,", "KON", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "der weitgelegnen Stadt? Was f\u00fcr ein Wunderwerk", "tokens": ["der", "weit\u00b7ge\u00b7leg\u00b7nen", "Stadt", "?", "Was", "f\u00fcr", "ein", "Wun\u00b7der\u00b7werk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PWS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bekr\u00e4ftigt euer Wort? Zeugt nichts nicht ihre St\u00e4rk',", "tokens": ["be\u00b7kr\u00e4f\u00b7tigt", "eu\u00b7er", "Wort", "?", "Zeugt", "nichts", "nicht", "ih\u00b7re", "St\u00e4rk'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PIS", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ihr Leben und ihr Tun, der Tochter der Odrysen?", "tokens": ["ihr", "Le\u00b7ben", "und", "ihr", "Tun", ",", "der", "Toch\u00b7ter", "der", "Od\u00b7ry\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nein. Die verge\u00dfne Zeit hat Alles mit sich fort.", "tokens": ["Nein", ".", "Die", "ver\u00b7ge\u00df\u00b7ne", "Zeit", "hat", "Al\u00b7les", "mit", "sich", "fort", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "ADJA", "NN", "VAFIN", "PIS", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wunder mich des Tuns und ehre diesen Ort,", "tokens": ["Ich", "wun\u00b7der", "mich", "des", "Tuns", "und", "eh\u00b7re", "die\u00b7sen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PRF", "ART", "NN", "KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der mir fast halbe Furcht mit seiner Wildn\u00fc\u00df machet.", "tokens": ["der", "mir", "fast", "hal\u00b7be", "Furcht", "mit", "sei\u00b7ner", "Wild\u00b7n\u00fc\u00df", "ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der wolkengleiche Fels wirft frech sein Haupt empor.", "tokens": ["Der", "wol\u00b7ken\u00b7glei\u00b7che", "Fels", "wirft", "frech", "sein", "Haupt", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So tut ein wilder Mensch und trutzt der G\u00f6tter Chor,", "tokens": ["So", "tut", "ein", "wil\u00b7der", "Mensch", "und", "trutzt", "der", "G\u00f6t\u00b7ter", "Chor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "A