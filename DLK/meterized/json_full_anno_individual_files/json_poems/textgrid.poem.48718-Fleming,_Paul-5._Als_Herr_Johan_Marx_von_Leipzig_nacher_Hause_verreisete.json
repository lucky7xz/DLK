{"textgrid.poem.48718": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. Als Herr Johan Marx von Leipzig nacher Hause verreisete", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und du wilst auch nun fort zu denen ", "tokens": ["Und", "du", "wilst", "auch", "nun", "fort", "zu", "de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "PTKVZ", "APPR", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "du Priester der Natur und Kind der Meditrin,", "tokens": ["du", "Pries\u00b7ter", "der", "Na\u00b7tur", "und", "Kind", "der", "Me\u00b7di\u00b7trin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "des Vaterlandes Gunst, die ruft dich von uns hin", "tokens": ["des", "Va\u00b7ter\u00b7lan\u00b7des", "Gunst", ",", "die", "ruft", "dich", "von", "uns", "hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "dort auf dein ", "tokens": ["dort", "auf", "dein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT"], "meter": "--+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "und erste Wiege sein. Kein Bitten deiner Br\u00fcder", "tokens": ["und", "ers\u00b7te", "Wie\u00b7ge", "sein", ".", "Kein", "Bit\u00b7ten", "dei\u00b7ner", "Br\u00fc\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VAINF", "$.", "PIAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "verleget dir den Pa\u00df, kein hies'ge Menschg\u00f6ttin", "tokens": ["ver\u00b7le\u00b7get", "dir", "den", "Pa\u00df", ",", "kein", "hies'\u00b7ge", "Menschg\u00f6t\u00b7tin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "(was kan di\u00df Volk sonst nicht?) kan halten deinen Sinn,", "tokens": ["(", "was", "kan", "di\u00df", "Volk", "sonst", "nicht", "?", ")", "kan", "hal\u00b7ten", "dei\u00b7nen", "Sinn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VMFIN", "PDS", "NN", "ADV", "PTKNEG", "$.", "$(", "VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "du mu\u00dft und solt von uns. So setze dich doch nieder", "tokens": ["du", "mu\u00dft", "und", "solt", "von", "uns", ".", "So", "set\u00b7ze", "dich", "doch", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "APPR", "PPER", "$.", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "und eile nicht so sehr. Lies diesen Brief noch hier,", "tokens": ["und", "ei\u00b7le", "nicht", "so", "sehr", ".", "Lies", "die\u00b7sen", "Brief", "noch", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADV", "$.", "NN", "PDAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hex