{"dta.poem.8417": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die k\u00fcnftige Geliebte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Du s\u00fcsses M\u00e4dchen? wehet das Fl\u00fcgelkleid", "tokens": ["Du", "s\u00fcs\u00b7ses", "M\u00e4d\u00b7chen", "?", "we\u00b7het", "das", "Fl\u00fc\u00b7gel\u00b7kleid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir an der Schulter? bebt der Strauss dir", "tokens": ["Dir", "an", "der", "Schul\u00b7ter", "?", "bebt", "der", "Strauss", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "$.", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Schon an der wallenden sch\u00f6nen Brust auf?", "tokens": ["Schon", "an", "der", "wal\u00b7len\u00b7den", "sch\u00f6\u00b7nen", "Brust", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein s\u00fcsses Zittern zittert durch mein Gebein,", "tokens": ["Ein", "s\u00fcs\u00b7ses", "Zit\u00b7tern", "zit\u00b7tert", "durch", "mein", "Ge\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wann mir dein Bildniss l\u00e4chelnd entgegen tanzt,", "tokens": ["Wann", "mir", "dein", "Bild\u00b7niss", "l\u00e4\u00b7chelnd", "ent\u00b7ge\u00b7gen", "tanzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ADJD", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wann ichs auf meinem Schoosse wiege,", "tokens": ["Wann", "ichs", "auf", "mei\u00b7nem", "Schoos\u00b7se", "wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und an den klopfenden Busen dr\u00fccke.", "tokens": ["Und", "an", "den", "klop\u00b7fen\u00b7den", "Bu\u00b7sen", "dr\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Garten taumelt; r\u00f6theres Abendroth", "tokens": ["Der", "Gar\u00b7ten", "tau\u00b7melt", ";", "r\u00f6\u00b7the\u00b7res", "A\u00b7ben\u00b7droth"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durchstr\u00f6mt die Bl\u00e4tter, purpert die Maienluft;", "tokens": ["Durch\u00b7str\u00f6mt", "die", "Bl\u00e4t\u00b7ter", ",", "pur\u00b7pert", "die", "Mai\u00b7en\u00b7luft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wie Engelfl\u00fcgel nieders\u00e4useln,", "tokens": ["Wie", "En\u00b7gel\u00b7fl\u00fc\u00b7gel", "nie\u00b7der\u00b7s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rauschet die Laube vom Kussgelispel.", "tokens": ["Rau\u00b7schet", "die", "Lau\u00b7be", "vom", "Kuss\u00b7ge\u00b7lis\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "An deiner Leinwand flattert vielleicht mein Bild", "tokens": ["An", "dei\u00b7ner", "Lein\u00b7wand", "flat\u00b7tert", "viel\u00b7leicht", "mein", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir auch entgegen, schmiegt sich an deine Brust,", "tokens": ["Dir", "auch", "ent\u00b7ge\u00b7gen", ",", "schmiegt", "sich", "an", "dei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "---+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und eine Sehnsuchtsthr\u00e4ne tr\u00e4ufelt", "tokens": ["Und", "ei\u00b7ne", "Sehn\u00b7suchtst\u00b7hr\u00e4\u00b7ne", "tr\u00e4u\u00b7felt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ueber die seidenen Purpurblumen.", "tokens": ["Ue\u00b7ber", "die", "sei\u00b7de\u00b7nen", "Pur\u00b7pur\u00b7blu\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Seid mir gesegnet, Thr\u00e4nen! Ihr flosset mir!", "tokens": ["Seid", "mir", "ge\u00b7seg\u00b7net", ",", "Thr\u00e4\u00b7nen", "!", "Ihr", "flos\u00b7set", "mir", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "VVPP", "$,", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bald schl\u00e4gt die Stunde! Ach dann entk\u00fcss' ich euch", "tokens": ["Bald", "schl\u00e4gt", "die", "Stun\u00b7de", "!", "Ach", "dann", "ent\u00b7k\u00fcss'", "ich", "euch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ITJ", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dem blauen Aug, der weissen Wange;", "tokens": ["Dem", "blau\u00b7en", "Aug", ",", "der", "weis\u00b7sen", "Wan\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trinke den Taumel der Erdenwonne!", "tokens": ["Trin\u00b7ke", "den", "Tau\u00b7mel", "der", "Er\u00b7den\u00b7won\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "An voller Quelle weil' ich, und sch\u00f6pfe mir", "tokens": ["An", "vol\u00b7ler", "Quel\u00b7le", "weil'", "ich", ",", "und", "sch\u00f6p\u00b7fe", "mir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Freuden jede, Himmel auf Himmel mir,", "tokens": ["Der", "Freu\u00b7den", "je\u00b7de", ",", "Him\u00b7mel", "auf", "Him\u00b7mel", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "$,", "NN", "APPR", "NN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie, deren Seelen mich umschwebten,", "tokens": ["Sie", ",", "de\u00b7ren", "See\u00b7len", "mich", "um\u00b7schweb\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann ich im Haine der Zukunft tr\u00e4umte!", "tokens": ["Wann", "ich", "im", "Hai\u00b7ne", "der", "Zu\u00b7kunft", "tr\u00e4um\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Bl\u00fch' unterdessen sch\u00f6ner und sch\u00f6ner auf,", "tokens": ["Bl\u00fch'", "un\u00b7ter\u00b7des\u00b7sen", "sch\u00f6\u00b7ner", "und", "sch\u00f6\u00b7ner", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du s\u00fcsses M\u00e4dchen! Leitet, ihr Tugenden,", "tokens": ["Du", "s\u00fcs\u00b7ses", "M\u00e4d\u00b7chen", "!", "Lei\u00b7tet", ",", "ihr", "Tu\u00b7gen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie eine Schaar von Schwesterengeln,", "tokens": ["Wie", "ei\u00b7ne", "Schaar", "von", "Schwes\u00b7te\u00b7ren\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie durch die Pfade des Erdenlebens!", "tokens": ["Sie", "durch", "die", "Pfa\u00b7de", "des", "Er\u00b7den\u00b7le\u00b7bens", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Ein reinrer Aether lache herab auf dich!", "tokens": ["Ein", "rein\u00b7rer", "A\u00b7e\u00b7ther", "la\u00b7che", "her\u00b7ab", "auf", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "T\u00f6nt, Nachtigallen, wann sich der Abend neigt,", "tokens": ["T\u00f6nt", ",", "Nach\u00b7ti\u00b7gal\u00b7len", ",", "wann", "sich", "der", "A\u00b7bend", "neigt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Apfelbaum vor ihrem Fenster,", "tokens": ["Im", "Ap\u00b7fel\u00b7baum", "vor", "ih\u00b7rem", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Goldene Tr\u00e4um' um ihr M\u00e4dchenbette!", "tokens": ["Gol\u00b7de\u00b7ne", "Tr\u00e4um'", "um", "ihr", "M\u00e4d\u00b7chen\u00b7bet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Doch s\u00fcssre Tr\u00e4ume thaue das Morgenroth", "tokens": ["Doch", "s\u00fcss\u00b7re", "Tr\u00e4u\u00b7me", "thau\u00b7e", "das", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um deine Schl\u00e4fen, Tr\u00e4ume der Serafim,", "tokens": ["Um", "dei\u00b7ne", "Schl\u00e4\u00b7fen", ",", "Tr\u00e4u\u00b7me", "der", "Se\u00b7ra\u00b7fim", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wann jener Tag dem Meer' entschimmert,", "tokens": ["Wann", "je\u00b7ner", "Tag", "dem", "Meer'", "ent\u00b7schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich dich unter den Blumen finde!", "tokens": ["Da", "ich", "dich", "un\u00b7ter", "den", "Blu\u00b7men", "fin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}