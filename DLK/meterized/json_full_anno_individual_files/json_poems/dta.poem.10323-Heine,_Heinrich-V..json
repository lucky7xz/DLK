{"dta.poem.10323": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.71", "en:0.28"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Was treibt und tobt mein tolles Blut?", "tokens": ["Was", "treibt", "und", "tobt", "mein", "tol\u00b7les", "Blut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was flammt mein Herz in wilder Gluth?", "tokens": ["Was", "flammt", "mein", "Herz", "in", "wil\u00b7der", "Gluth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kocht mein Blut und zischt und g\u00e4hrt,", "tokens": ["Es", "kocht", "mein", "Blut", "und", "zischt", "und", "g\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und grimme Gluth mein Herz verzehrt.", "tokens": ["Und", "grim\u00b7me", "Gluth", "mein", "Herz", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das Blut ist toll, die Flamme wild,", "tokens": ["Das", "Blut", "ist", "toll", ",", "die", "Flam\u00b7me", "wild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil zu mir kam ein Traumgebild;", "tokens": ["Weil", "zu", "mir", "kam", "ein", "Traum\u00b7ge\u00b7bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kam der finstre Sohn der Nacht,", "tokens": ["Es", "kam", "der", "finst\u00b7re", "Sohn", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat mich keuchend fortgebracht.", "tokens": ["Und", "hat", "mich", "keu\u00b7chend", "fort\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er bracht' mich in ein helles Haus,", "tokens": ["Er", "bracht'", "mich", "in", "ein", "hel\u00b7les", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Harfenklang und Saus und Braus,", "tokens": ["Wo", "Har\u00b7fen\u00b7klang", "und", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Fackelglanz und Kerzenschein;", "tokens": ["Und", "Fa\u00b7ckel\u00b7glanz", "und", "Ker\u00b7zen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kam zum Saal, ich trat hinein.", "tokens": ["Ich", "kam", "zum", "Saal", ",", "ich", "trat", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das war ein lustig Hochzeitfest;", "tokens": ["Das", "war", "ein", "lus\u00b7tig", "Hoch\u00b7zeit\u00b7fest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Tafel sa\u00dfen froh die G\u00e4st'.", "tokens": ["Zu", "Ta\u00b7fel", "sa\u00b7\u00dfen", "froh", "die", "G\u00e4st'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ich nach dem Brautpaar schaut', \u2014", "tokens": ["Und", "wie", "ich", "nach", "dem", "Braut\u00b7paar", "schaut'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O weh! mein Liebchen war die Braut.", "tokens": ["O", "weh", "!", "mein", "Lieb\u00b7chen", "war", "die", "Braut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das war mein Liebchen wunnesam,", "tokens": ["Das", "war", "mein", "Lieb\u00b7chen", "wun\u00b7ne\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein fremder Mann war Br\u00e4utigam;", "tokens": ["Ein", "frem\u00b7der", "Mann", "war", "Br\u00e4u\u00b7ti\u00b7gam", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dicht hinter'm Ehrenstuhl der Braut,", "tokens": ["Dicht", "hin\u00b7ter'm", "Eh\u00b7ren\u00b7stuhl", "der", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da blieb ich stehn, gab keinen Laut.", "tokens": ["Da", "blieb", "ich", "stehn", ",", "gab", "kei\u00b7nen", "Laut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es rauscht Musik, \u2014 gar still stand ich;", "tokens": ["Es", "rauscht", "Mu\u00b7sik", ",", "gar", "still", "stand", "ich", ";"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "$(", "ADV", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freudenl\u00e4rm betr\u00fcbte mich.", "tokens": ["Der", "Freu\u00b7den\u00b7l\u00e4rm", "be\u00b7tr\u00fcb\u00b7te", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Br\u00e4utgam oft gar z\u00e4rtlich blickt,", "tokens": ["Der", "Br\u00e4ut\u00b7gam", "oft", "gar", "z\u00e4rt\u00b7lich", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Braut erwiedert's hold und nickt.", "tokens": ["Die", "Braut", "er\u00b7wie\u00b7dert's", "hold", "und", "nickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Br\u00e4utgam f\u00fcllt den Becher sein,", "tokens": ["Der", "Br\u00e4ut\u00b7gam", "f\u00fcllt", "den", "Be\u00b7cher", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trinkt daraus, und reicht gar fein", "tokens": ["Und", "trinkt", "da\u00b7raus", ",", "und", "reicht", "gar", "fein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "$,", "KON", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Braut ihn hin; sie l\u00e4chelt Dank, \u2014", "tokens": ["Der", "Braut", "ihn", "hin", ";", "sie", "l\u00e4\u00b7chelt", "Dank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "NN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Weh! mein rothes Blut sie trank.", "tokens": ["O", "Weh", "!", "mein", "ro\u00b7thes", "Blut", "sie", "trank", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Braut ein h\u00fcbsches Aepflein nahm,", "tokens": ["Die", "Braut", "ein", "h\u00fcb\u00b7sches", "A\u00b7e\u00b7pflein", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und reicht es hin dem Br\u00e4utigam.", "tokens": ["Und", "reicht", "es", "hin", "dem", "Br\u00e4u\u00b7ti\u00b7gam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der nahm sein Messer, schnitt hinein, \u2014", "tokens": ["Der", "nahm", "sein", "Mes\u00b7ser", ",", "schnitt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Weh! das war das Herze mein.", "tokens": ["O", "Weh", "!", "das", "war", "das", "Her\u00b7ze", "mein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PDS", "VAFIN", "PDS", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie \u00e4ugeln s\u00fc\u00df, sie \u00e4ugeln lang,", "tokens": ["Sie", "\u00e4u\u00b7geln", "s\u00fc\u00df", ",", "sie", "\u00e4u\u00b7geln", "lang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Br\u00e4ut'gam k\u00fchn die Braut umschlang,", "tokens": ["Der", "Br\u00e4ut'\u00b7gam", "k\u00fchn", "die", "Braut", "um\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und k\u00fc\u00dft sie auf die Wangen roth, \u2014", "tokens": ["Und", "k\u00fc\u00dft", "sie", "auf", "die", "Wan\u00b7gen", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Weh! mich k\u00fc\u00dft der kalte Tod.", "tokens": ["O", "Weh", "!", "mich", "k\u00fc\u00dft", "der", "kal\u00b7te", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie Blei lag meine Zung' im Mund',", "tokens": ["Wie", "Blei", "lag", "mei\u00b7ne", "Zung'", "im", "Mund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich kein W\u00f6rtlein sprechen kunt.", "tokens": ["Da\u00df", "ich", "kein", "W\u00f6rt\u00b7lein", "spre\u00b7chen", "kunt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da rauscht es auf, der Tanz begann;", "tokens": ["Da", "rauscht", "es", "auf", ",", "der", "Tanz", "be\u00b7gann", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das schmucke Brautpaar tanzt voran.", "tokens": ["Das", "schmu\u00b7cke", "Braut\u00b7paar", "tanzt", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und wie ich stand so leichenstumm,", "tokens": ["Und", "wie", "ich", "stand", "so", "lei\u00b7chen\u00b7stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die T\u00e4nzer schweben flink herum; \u2014", "tokens": ["Die", "T\u00e4n\u00b7zer", "schwe\u00b7ben", "flink", "he\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein leises Wort der Br\u00e4ut'gam spricht,", "tokens": ["Ein", "lei\u00b7ses", "Wort", "der", "Br\u00e4ut'\u00b7gam", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Braut wird roth, doch z\u00fcrnt sie nicht. \u2014 \u2014", "tokens": ["Die", "Braut", "wird", "roth", ",", "doch", "z\u00fcrnt", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}