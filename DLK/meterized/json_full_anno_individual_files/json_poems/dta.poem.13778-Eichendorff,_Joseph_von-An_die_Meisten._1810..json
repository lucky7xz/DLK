{"dta.poem.13778": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Meisten.  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Einen Wald doch kenn' ich droben,", "tokens": ["Ei\u00b7nen", "Wald", "doch", "kenn'", "ich", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauschend mit den gr\u00fcnen Kronen,", "tokens": ["Rau\u00b7schend", "mit", "den", "gr\u00fc\u00b7nen", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00e4mme br\u00fcderlich verwoben,", "tokens": ["St\u00e4m\u00b7me", "br\u00fc\u00b7der\u00b7lich", "ver\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo das alte Recht mag wohnen.", "tokens": ["Wo", "das", "al\u00b7te", "Recht", "mag", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Manche auf sein Rauschen merken,", "tokens": ["Man\u00b7che", "auf", "sein", "Rau\u00b7schen", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ein neu Geschlecht wird st\u00e4rken", "tokens": ["Und", "ein", "neu", "Ge\u00b7schlecht", "wird", "st\u00e4r\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN", "VAFIN", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Dieser Wald zu deutschen Werken.", "tokens": ["Die\u00b7ser", "Wald", "zu", "deut\u00b7schen", "Wer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}