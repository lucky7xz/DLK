{"textgrid.poem.56135": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir aber, Herr, o was weih ich dir, sag,", "tokens": ["Dir", "a\u00b7ber", ",", "Herr", ",", "o", "was", "weih", "ich", "dir", ",", "sag", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "FM", "PWS", "VVFIN", "PPER", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der das Ohr den Gesch\u00f6pfen gelehrt? \u2013", "tokens": ["der", "das", "Ohr", "den", "Ge\u00b7sch\u00f6p\u00b7fen", "ge\u00b7lehrt", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Mein Erinnern an einen Fr\u00fchlingstag,", "tokens": ["Mein", "E\u00b7rin\u00b7nern", "an", "ei\u00b7nen", "Fr\u00fch\u00b7lings\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "seinen Abend, in Ru\u00dfland \u2013, ein Pferd...", "tokens": ["sei\u00b7nen", "A\u00b7bend", ",", "in", "Ru\u00df\u00b7land", "\u2013", ",", "ein", "Pferd", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NE", "$(", "$,", "ART", "NN", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Her\u00fcber vom Dorf kam der Schimmel allein,", "tokens": ["Her\u00b7\u00fc\u00b7ber", "vom",