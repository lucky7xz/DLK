{"textgrid.poem.59570": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Rache", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf zur Rache! auf zur Rache!", "tokens": ["Auf", "zur", "Ra\u00b7che", "!", "auf", "zur", "Ra\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "$.", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erwache, edles Volk, erwache!", "tokens": ["Er\u00b7wa\u00b7che", ",", "ed\u00b7les", "Volk", ",", "er\u00b7wa\u00b7che", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erhebe lautes Kriegsgeschrei!", "tokens": ["Er\u00b7he\u00b7be", "lau\u00b7tes", "Kriegs\u00b7ge\u00b7schrei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df in T\u00e4lern, la\u00df auf H\u00f6hen", "tokens": ["La\u00df", "in", "T\u00e4\u00b7lern", ",", "la\u00df", "auf", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "APPR", "NN", "$,", "VVIMP", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der Freiheit stolze Fahnen wehen!", "tokens": ["Der", "Frei\u00b7heit", "stol\u00b7ze", "Fah\u00b7nen", "we\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Schandeketten brich inzwei!", "tokens": ["Die", "Schan\u00b7de\u00b7ket\u00b7ten", "brich", "in\u00b7zwei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn der Satan ist gekommen,", "tokens": ["Denn", "der", "Sa\u00b7tan", "ist", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er hat sich Fleisch und Bein genommen", "tokens": ["Er", "hat", "sich", "Fleisch", "und", "Bein", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und will der Herr der Erde sein,", "tokens": ["Und", "will", "der", "Herr", "der", "Er\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die Weisheit tappt geblendet,", "tokens": ["Und", "die", "Weis\u00b7heit", "tappt", "ge\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Mut und Ehre kriecht gesch\u00e4ndet", "tokens": ["Und", "Mut", "und", "Eh\u00b7re", "kriecht", "ge\u00b7sch\u00e4n\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und will nicht in den Tod hinein.", "tokens": ["Und", "will", "nicht", "in", "den", "Tod", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die Wahrheit traurt verstummet,", "tokens": ["Und", "die", "Wahr\u00b7heit", "traurt", "ver\u00b7stum\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die brandgemalte L\u00fcge summet", "tokens": ["Die", "brand\u00b7ge\u00b7mal\u00b7te", "L\u00fc\u00b7ge", "sum\u00b7met"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Frech jede gro\u00dfe Tugend an,", "tokens": ["Frech", "je\u00b7de", "gro\u00b7\u00dfe", "Tu\u00b7gend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00fchn durch Schwert und Henkerbeile", "tokens": ["K\u00fchn", "durch", "Schwert", "und", "Hen\u00b7ker\u00b7bei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meint sie, da\u00df seine Donnerkeile", "tokens": ["Meint", "sie", ",", "da\u00df", "sei\u00b7ne", "Don\u00b7ner\u00b7kei\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Himmel nicht mehr schwingen kann.", "tokens": ["Der", "Him\u00b7mel", "nicht", "mehr", "schwin\u00b7gen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum zur Rache auf! zur Rache!", "tokens": ["Drum", "zur", "Ra\u00b7che", "auf", "!", "zur", "Ra\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "PTKVZ", "$.", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erwache, edles Volk, erwache!", "tokens": ["Er\u00b7wa\u00b7che", ",", "ed\u00b7les", "Volk", ",", "er\u00b7wa\u00b7che", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und tilge weg des Teufels Spott!", "tokens": ["Und", "til\u00b7ge", "weg", "des", "Teu\u00b7fels", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist er stark durch L\u00fcgenk\u00fcnste,", "tokens": ["Ist", "er", "stark", "durch", "L\u00fc\u00b7gen\u00b7k\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du rei\u00dfe h\u00f6llische Gespinste", "tokens": ["Du", "rei\u00b7\u00dfe", "h\u00f6l\u00b7li\u00b7sche", "Ge\u00b7spins\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Inzwei durch deinen st\u00e4rkern Gott:", "tokens": ["In\u00b7zwei", "durch", "dei\u00b7nen", "st\u00e4r\u00b7kern", "Gott", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Durch Gott, vor dem die Teufel zittern,", "tokens": ["Durch", "Gott", ",", "vor", "dem", "die", "Teu\u00b7fel", "zit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wann wild in Schlachtenungewittern", "tokens": ["Wann", "wild", "in", "Schlach\u00b7ten\u00b7un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Donner durch die Reihen f\u00e4hrt,", "tokens": ["Der", "Don\u00b7ner", "durch", "die", "Rei\u00b7hen", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wann die Freien fr\u00f6hlich sterben,", "tokens": ["Wann", "die", "Frei\u00b7en", "fr\u00f6h\u00b7lich", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tyrannensch\u00e4del gleich den Scherben", "tokens": ["Ty\u00b7ran\u00b7nen\u00b7sch\u00e4\u00b7del", "gleich", "den", "Scher\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zersplittern durch der Tapfern Schwert.", "tokens": ["Zer\u00b7split\u00b7tern", "durch", "der", "Tap\u00b7fern", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf! es gilt die h\u00f6chsten Fehden,", "tokens": ["Auf", "!", "es", "gilt", "die", "h\u00f6chs\u00b7ten", "Feh\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die stummen St\u00f6cke m\u00f6chten reden,", "tokens": ["Die", "stum\u00b7men", "St\u00f6\u00b7cke", "m\u00f6ch\u00b7ten", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der stumme Stein Posaune sein,", "tokens": ["Der", "stum\u00b7me", "Stein", "Po\u00b7sau\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Faule Berge sich bewegen,", "tokens": ["Fau\u00b7le", "Ber\u00b7ge", "sich", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihr nur griffet nicht zum Degen?", "tokens": ["Und", "ihr", "nur", "grif\u00b7fet", "nicht", "zum", "De\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr wolltet faul zum Kampfe sein?", "tokens": ["Ihr", "woll\u00b7tet", "faul", "zum", "Kamp\u00b7fe", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf! die Stunde hat geschlagen \u2013", "tokens": ["Auf", "!", "die", "Stun\u00b7de", "hat", "ge\u00b7schla\u00b7gen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit Gott dem Herrn wir wollen's wagen:", "tokens": ["Mit", "Gott", "dem", "Herrn", "wir", "wol\u00b7len's", "wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Frisch in den heil'gen Kampf hinein!", "tokens": ["Frisch", "in", "den", "heil'\u00b7gen", "Kampf", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00dft in T\u00e4lern, la\u00dft auf H\u00f6hen", "tokens": ["La\u00dft", "in", "T\u00e4\u00b7lern", ",", "la\u00dft", "auf", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "APPR", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Fahnen hoch gen Himmel wehen!", "tokens": ["Die", "Fah\u00b7nen", "hoch", "gen", "Him\u00b7mel", "we\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Freiheit soll die Losung sein!", "tokens": ["Die", "Frei\u00b7heit", "soll", "die", "Lo\u00b7sung", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf zur Rache! auf zur Rache!", "tokens": ["Auf", "zur", "Ra\u00b7che", "!", "auf", "zur", "Ra\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"]