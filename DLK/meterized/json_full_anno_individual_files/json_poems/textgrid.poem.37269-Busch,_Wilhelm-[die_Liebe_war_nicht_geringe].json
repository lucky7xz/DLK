{"textgrid.poem.37269": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[die Liebe war nicht geringe]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe war nicht geringe.", "tokens": ["Die", "Lie\u00b7be", "war", "nicht", "ge\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sie wurden ordentlich bla\u00df;", "tokens": ["Sie", "wur\u00b7den", "or\u00b7dent\u00b7lich", "bla\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie sagten sich tausend Dinge", "tokens": ["Sie", "sag\u00b7ten", "sich", "tau\u00b7send", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wu\u00dften noch immer was.", "tokens": ["Und", "wu\u00df\u00b7ten", "noch", "im\u00b7mer", "was", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PIS", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie mu\u00dften sich lange qu\u00e4len,", "tokens": ["Sie", "mu\u00df\u00b7ten", "sich", "lan\u00b7ge", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch schlie\u00dflich kam's dazu,", "tokens": ["Doch", "schlie\u00df\u00b7lich", "kam's", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sie sich konnten verm\u00e4hlen.", "tokens": ["Da\u00df", "sie", "sich", "konn\u00b7ten", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jetzt haben die Seelen Ruh.", "tokens": ["Jetzt", "ha\u00b7ben", "die", "See\u00b7len", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Bei eines Strumpfes Bereitung", "tokens": ["Bei", "ei\u00b7nes", "Strump\u00b7fes", "Be\u00b7rei\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sitzt sie im Morgenhabit;", "tokens": ["Sitzt", "sie", "im", "Mor\u00b7gen\u00b7ha\u00b7bit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Er liest in der K\u00f6lnischen Zeitung", "tokens": ["Er", "liest", "in", "der", "K\u00f6l\u00b7ni\u00b7schen", "Zei\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und teilt ihr das N\u00f6tige mit.", "tokens": ["Und", "teilt", "ihr", "das", "N\u00f6\u00b7ti\u00b7ge", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Die Liebe war nicht geringe.", "tokens":