{"dta.poem.20004": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Spindelmanns Recension der Gegend .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "N\u00e4her mu\u00df ich jezt betrachten", "tokens": ["N\u00e4\u00b7her", "mu\u00df", "ich", "jezt", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese Gegend durch das Glas,", "tokens": ["Die\u00b7se", "Ge\u00b7gend", "durch", "das", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie ist nicht ganz zu verachten,", "tokens": ["Sie", "ist", "nicht", "ganz", "zu", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PTKZU", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nur die Fern ist allzubla\u00df.", "tokens": ["Nur", "die", "Fern", "ist", "all\u00b7zu\u00b7bla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jene Burg auf steiler H\u00f6he", "tokens": ["Je\u00b7ne", "Burg", "auf", "stei\u00b7ler", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nenn' ich abgeschmackt und dumm,", "tokens": ["Nenn'", "ich", "ab\u00b7ge\u00b7schmackt", "und", "dumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meinem Auge thut sie wehe,", "tokens": ["Mei\u00b7nem", "Au\u00b7ge", "thut", "sie", "we\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Flu\u00df, der g\u00e4nzlich krumm.", "tokens": ["Wie", "der", "Flu\u00df", ",", "der", "g\u00e4nz\u00b7lich", "krumm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jene M\u00fchl' in w\u00fcsten Kl\u00fcften", "tokens": ["Je\u00b7ne", "M\u00fchl'", "in", "w\u00fcs\u00b7ten", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Giebt mir gar zu rohen Schall,", "tokens": ["Giebt", "mir", "gar", "zu", "ro\u00b7hen", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber ein gesundes D\u00fcften", "tokens": ["A\u00b7ber", "ein", "ge\u00b7sun\u00b7des", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weht aus ihrem Eselsstall.", "tokens": ["Weht", "aus", "ih\u00b7rem", "E\u00b7sels\u00b7stall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df hier Schl\u00fcsselblumen stehen,", "tokens": ["Da\u00df", "hier", "Schl\u00fcs\u00b7sel\u00b7blu\u00b7men", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4tt' ich das nur eh' gewu\u00dft!", "tokens": ["H\u00e4tt'", "ich", "das", "nur", "eh'", "ge\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mu\u00df sie schnell zu pfl\u00fccken gehen:", "tokens": ["Mu\u00df", "sie", "schnell", "zu", "pfl\u00fc\u00b7cken", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn sie dienen meiner Brust.", "tokens": ["Denn", "sie", "die\u00b7nen", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kr\u00e4uter, die zwar farbig bl\u00fchen,", "tokens": ["Kr\u00e4u\u00b7ter", ",", "die", "zwar", "far\u00b7big", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch zu Thee nicht dienlich sind,", "tokens": ["Doch", "zu", "Thee", "nicht", "dien\u00b7lich", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch nicht brauchbar sind zu Br\u00fchen,", "tokens": ["Doch", "nicht", "brauch\u00b7bar", "sind", "zu", "Br\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberlass' ich gern dem Wind.", "tokens": ["Ue\u00b7ber\u00b7lass'", "ich", "gern", "dem", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}