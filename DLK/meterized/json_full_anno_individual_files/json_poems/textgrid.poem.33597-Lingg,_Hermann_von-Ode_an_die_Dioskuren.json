{"textgrid.poem.33597": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Ode an die Dioskuren", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein in Nacht eindunkelndes Land, o Rom, und", "tokens": ["Dein", "in", "Nacht", "ein\u00b7dun\u00b7keln\u00b7des", "Land", ",", "o", "Rom", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "APPR", "NN", "ART", "NN", "$,", "FM", "NE", "$,", "KON"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Alles ruht schon, aber am Himmel zuckt es", "tokens": ["Al\u00b7les", "ruht", "schon", ",", "a\u00b7ber", "am", "Him\u00b7mel", "zuckt", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "$,", "ADV", "APPRART", "NN", "VVFIN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Wetterschw\u00fcl herauf und erhellt die beiden", "tokens": ["Wet\u00b7ter\u00b7schw\u00fcl", "her\u00b7auf", "und", "er\u00b7hellt", "die", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "KON", "VVFIN", "ART", "PIAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Erznen Kolosse.", "tokens": ["Erz\u00b7nen", "Ko\u00b7los\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Euch begr\u00fc\u00df' ich, m\u00e4chtige Meerbeherrscher!", "tokens": ["Euch", "be\u00b7gr\u00fc\u00df'", "ich", ",", "m\u00e4ch\u00b7ti\u00b7ge", "Meer\u00b7be\u00b7herr\u00b7scher", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Euch begr\u00fc\u00dft mein Lied, Polydeukes, dich und", "tokens": ["Euch", "be\u00b7gr\u00fc\u00dft", "mein", "Lied", ",", "Po\u00b7ly\u00b7deu\u00b7kes", ",", "dich", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$,", "PPER", "KON"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Kastor! Mutvoll euch zum Olymp auf schwingt ihr,", "tokens": ["Kas\u00b7tor", "!", "Mut\u00b7voll", "euch", "zum", "O\u00b7lymp", "auf", "schwingt", "ihr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "APPRART", "NN", "APPR", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "S\u00f6hne der Leda!", "tokens": ["S\u00f6h\u00b7ne", "der", "Le\u00b7da", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "W\u00e4hrend Blitz auf Blitz mit dem Dunkel streitet,", "tokens": ["W\u00e4h\u00b7rend", "Blitz", "auf", "Blitz", "mit", "dem", "Dun\u00b7kel", "strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Eilt ihr her auf schimmernden Rossen, flatternd", "tokens": ["Eilt", "ihr", "her", "auf", "schim\u00b7mern\u00b7den", "Ros\u00b7sen", ",", "flat\u00b7ternd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,", "VVPP"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Nachtgew\u00f6lk hindurch und dem Schiffer hilfreich", "tokens": ["Nacht\u00b7ge\u00b7w\u00f6lk", "hin\u00b7durch", "und", "dem", "Schif\u00b7fer", "hilf\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PAV", "KON", "ART", "NN", "ADJD"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Mitten im Seesturm!", "tokens": ["Mit\u00b7ten", "im", "Sees\u00b7turm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Tief ins Vorzeitgrau zu den G\u00f6ttern f\u00fchrt ihr", "tokens": ["Tief", "ins", "Vor\u00b7zeit\u00b7grau", "zu", "den", "G\u00f6t\u00b7tern", "f\u00fchrt", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Meinen Blick zur\u00fcck zum Heroenalter", "tokens": ["Mei\u00b7nen", "Blick", "zu\u00b7r\u00fcck", "zum", "He\u00b7ro\u00b7en\u00b7al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Und zur\u00fcck zu Helena's unverg\u00e4nglich", "tokens": ["Und", "zu\u00b7r\u00fcck", "zu", "He\u00b7le\u00b7na's", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NE", "ADJD"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lockendem Liebreiz.", "tokens": ["Lo\u00b7cken\u00b7dem", "Lieb\u00b7reiz", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Welch ein herrlich Menschengeschlecht umbl\u00fcht' euch!", "tokens": ["Welch", "ein", "herr\u00b7lich", "Men\u00b7schen\u00b7ge\u00b7schlecht", "um\u00b7bl\u00fcht'", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Jagdenfroh, k\u00fchn, wild, in der vollen Sch\u00f6nheit", "tokens": ["Jag\u00b7den\u00b7froh", ",", "k\u00fchn", ",", "wild", ",", "in", "der", "vol\u00b7len", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Erster Jugendkraft, in best\u00e4nd'gem Kampf mit", "tokens": ["Ers\u00b7ter", "Ju\u00b7gend\u00b7kraft", ",", "in", "be\u00b7st\u00e4n\u00b7d'\u00b7gem", "Kampf", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "APPR"], "meter": "+-+----+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Himmel und Erde.", "tokens": ["Him\u00b7mel", "und", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch als lang hernach in der Zeiten Umlauf", "tokens": ["Doch", "als", "lang", "her\u00b7nach", "in", "der", "Zei\u00b7ten", "Um\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "ADV", "APPR", "ART", "NN", "NN"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hellas' Volk aufbl\u00fcht in erhabner Freiheit,", "tokens": ["Hel\u00b7las'", "Volk", "auf\u00b7bl\u00fcht", "in", "er\u00b7hab\u00b7ner", "Frei\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Horch, da schallt Siegsruf, am Olymp, am Isthmus", "tokens": ["Horch", ",", "da", "schallt", "Siegs\u00b7ruf", ",", "am", "O\u00b7lymp", ",", "am", "Isth\u00b7mus"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Donnern die Wagen!", "tokens": ["Don\u00b7nern", "die", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Auf zum Wettkampf eilt, was Athen, was Argos", "tokens": ["Auf", "zum", "Wett\u00b7kampf", "eilt", ",", "was", "A\u00b7then", ",", "was", "Ar\u00b7gos"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "APPRART", "NN", "VVFIN", "$,", "PRELS", "NE", "$,", "PWS", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Oder Sparta's Fluren bewohnt, es dr\u00e4ngt sich", "tokens": ["O\u00b7der", "Spar\u00b7ta's", "Flu\u00b7ren", "be\u00b7wohnt", ",", "es", "dr\u00e4ngt", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NE", "NN", "VVPP", "$,", "PPER", "VVFIN", "PRF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Schar an Schar kampft\u00fcchtiger M\u00e4nner, hoher", "tokens": ["Schar", "an", "Schar", "kampf\u00b7t\u00fcch\u00b7ti\u00b7ger", "M\u00e4n\u00b7ner", ",", "ho\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "NN", "ADJA", "NN", "$,", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "G\u00f6ttergestalten.", "tokens": ["G\u00f6t\u00b7ter\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "In des Tempels schattigem Hain, wo hochher", "tokens": ["In", "des", "Tem\u00b7pels", "schat\u00b7ti\u00b7gem", "Hain", ",", "wo", "hoch\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "PWAV", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "\u00dcber Lorbeer Pinien schau'n, da schimmern", "tokens": ["\u00dc\u00b7ber", "Lor\u00b7beer", "Pi\u00b7ni\u00b7en", "schau'n", ",", "da", "schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "NE", "VVINF", "$,", "ADV", "VVFIN"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Weihgeschenke rings und in Purpur goldreich", "tokens": ["Weih\u00b7ge\u00b7schen\u00b7ke", "rings", "und", "in", "Pur\u00b7pur", "gold\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "APPR", "NN", "ADJD"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Strahlende G\u00fcrtel.", "tokens": ["Strah\u00b7len\u00b7de", "G\u00fcr\u00b7tel", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Auf! ans Ziel jetzt! Z\u00fcgelt die Hengste, Knaben! \u2013", "tokens": ["Auf", "!", "ans", "Ziel", "jetzt", "!", "Z\u00fc\u00b7gelt", "die", "Hengs\u00b7te", ",", "Kna\u00b7ben", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "$.", "APPRART", "NN", "ADV", "$.", "NN", "ART", "NN", "$,", "NN", "$.", "$("], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "So zwingt Mut, rein menschlicher Mut die Wildheit,", "tokens": ["So", "zwingt", "Mut", ",", "rein", "menschli\u00b7cher", "Mut", "die", "Wild\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "ADJD", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "So h\u00e4lt Freiheit ruhig die Z\u00fcgel aufrecht,", "tokens": ["So", "h\u00e4lt", "Frei\u00b7heit", "ru\u00b7hig", "die", "Z\u00fc\u00b7gel", "auf\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ruhig und siegreich!", "tokens": ["Ru\u00b7hig", "und", "sieg\u00b7reich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Welch ein Tag, ihr Himmlischen! Wie das Volk jauchzt,", "tokens": ["Welch", "ein", "Tag", ",", "ihr", "Himm\u00b7li\u00b7schen", "!", "Wie", "das", "Volk", "jauchzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "$,", "PPOSAT", "NN", "$.", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Um den Sieger jauchzt, den der Fichtenzweig kr\u00f6nt!", "tokens": ["Um", "den", "Sie\u00b7ger", "jauchzt", ",", "den", "der", "Fich\u00b7ten\u00b7zweig", "kr\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von des S\u00e4ngers Lippen erbl\u00fcht ihm ewig-", "tokens": ["Von", "des", "S\u00e4n\u00b7gers", "Lip\u00b7pen", "er\u00b7bl\u00fcht", "ihm", "e\u00b7wig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPER", "TRUNC"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dauernder Nachruhm.", "tokens": ["Dau\u00b7ern\u00b7der", "Nach\u00b7ruhm", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Doch nur ihr seid Allen das sch\u00f6nste Vorbild", "tokens": ["Doch", "nur", "ihr", "seid", "Al\u00b7len", "das", "sch\u00f6ns\u00b7te", "Vor\u00b7bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "VAFIN", "PIS", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Edlen Sinns und mutiger Jugend! Liebend", "tokens": ["Ed\u00b7len", "Sinns", "und", "mu\u00b7ti\u00b7ger", "Ju\u00b7gend", "!", "Lie\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$.", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Teilt ihr euch in alle Gefahr und alle", "tokens": ["Teilt", "ihr", "euch", "in", "al\u00b7le", "Ge\u00b7fahr", "und", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "KON", "PIAT"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Freude des Sieges!", "tokens": ["Freu\u00b7de", "des", "Sie\u00b7ges", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Auch am Himmel bleibt ihr vereinigt; liebend", "tokens": ["Auch", "am", "Him\u00b7mel", "bleibt", "ihr", "ver\u00b7ei\u00b7nigt", ";", "lie\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "VVPP", "$.", "ADJD"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Steigt ihr selbst zum Orkus hinab und teilet", "tokens": ["Steigt", "ihr", "selbst", "zum", "Or\u00b7kus", "hin\u00b7ab", "und", "tei\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "KON", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Dort Unsterblichkeit und zugleich die dunkeln", "tokens": ["Dort", "U\u00b7nsterb\u00b7lich\u00b7keit", "und", "zu\u00b7gleich", "die", "dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "ADV", "ART", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Lose des Todes.", "tokens": ["Lo\u00b7se", "des", "To\u00b7des", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "L\u00e4ngst in Erzgu\u00df ragend am Meer sah staunend", "tokens": ["L\u00e4ngst", "in", "Erz\u00b7gu\u00df", "ra\u00b7gend", "am", "Meer", "sah", "stau\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VVPP", "APPRART", "NN", "VVFIN", "ADJD"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Euch die Nachwelt; aber es kamen einstmals", "tokens": ["Euch", "die", "Nach\u00b7welt", ";", "a\u00b7ber", "es", "ka\u00b7men", "einst\u00b7mals"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$.", "KON", "PPER", "VVFIN", "ADV"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"},