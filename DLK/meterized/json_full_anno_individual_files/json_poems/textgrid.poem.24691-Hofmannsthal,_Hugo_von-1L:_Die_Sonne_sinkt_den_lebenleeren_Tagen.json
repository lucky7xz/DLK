{"textgrid.poem.24691": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Sonne sinkt den lebenleeren Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne sinkt den lebenleeren Tagen", "tokens": ["Die", "Son\u00b7ne", "sinkt", "den", "le\u00b7ben\u00b7lee\u00b7ren", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sinkt der Stadt vergoldend und gewaltig,", "tokens": ["Und", "sinkt", "der", "Stadt", "ver\u00b7gol\u00b7dend", "und", "ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wie sie sank der Zeit, die viel zu sagen", "tokens": ["So", "wie", "sie", "sank", "der", "Zeit", ",", "die", "viel", "zu", "sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und viel zu schenken hatte, vielgehaltig.", "tokens": ["Und", "viel", "zu", "schen\u00b7ken", "hat\u00b7te", ",", "viel\u00b7ge\u00b7hal\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "PTKZU", "VVINF", "VAFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Schatten scheint die goldne Luft zu tragen", "tokens": ["Und", "Schat\u00b7ten", "scheint", "die", "gold\u00b7ne", "Luft", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Versunkener Tage, bla\u00df und zartgestaltig,", "tokens": ["Ver\u00b7sun\u00b7ke\u00b7ner", "Ta\u00b7ge", ",", "bla\u00df", "und", "zart\u00b7ge\u00b7stal\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und alle Stunden, die vor\u00fcbergleiten,", "tokens": ["Und", "al\u00b7le", "Stun\u00b7den", ",", "die", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verh\u00fcllt ein Hauch verkl\u00e4rter M\u00f6glichkeiten.", "tokens": ["Ver\u00b7h\u00fcllt", "ein", "Hauch", "ver\u00b7kl\u00e4r\u00b7ter", "M\u00f6g\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Morgen war in blassen weiten G\u00e4rten,", "tokens": ["Ein", "Mor\u00b7gen", "war", "in", "blas\u00b7sen", "wei\u00b7ten", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von k\u00fchlem Duft und Einsamkeit durchzogen,", "tokens": ["Von", "k\u00fch\u00b7lem", "Duft", "und", "Ein\u00b7sam\u00b7keit", "durch\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne steigt, es finden sich Gef\u00e4hrten,", "tokens": ["Die", "Son\u00b7ne", "steigt", ",", "es", "fin\u00b7den", "sich", "Ge\u00b7f\u00e4hr\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PRF", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Lauben tretend, aus lebendigen Bogen,", "tokens": ["Aus", "Lau\u00b7ben", "tre\u00b7tend", ",", "aus", "le\u00b7ben\u00b7di\u00b7gen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und die Gedanken, die sich funkelnd mehrten", "tokens": ["Und", "die", "Ge\u00b7dan\u00b7ken", ",", "die", "sich", "fun\u00b7kelnd", "mehr\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und aus der Einsamkeit die Sch\u00f6nheit sogen,", "tokens": ["Und", "aus", "der", "Ein\u00b7sam\u00b7keit", "die", "Sch\u00f6n\u00b7heit", "so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ergie\u00dfen sich in losgebundenen Scharen", "tokens": ["Er\u00b7gie\u00b7\u00dfen", "sich", "in", "los\u00b7ge\u00b7bun\u00b7de\u00b7nen", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Mit offenen Lippen, Efeu in den Haaren.", "tokens": ["Mit", "of\u00b7fe\u00b7nen", "Lip\u00b7pen", ",", "E\u00b7feu", "in", "den", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Und alle Dinge werden uns lebendig:", "tokens": ["Und", "al\u00b7le", "Din\u00b7ge", "wer\u00b7den", "uns", "le\u00b7ben\u00b7dig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Winde weht der Atem der M\u00e4naden,", "tokens": ["Im", "Win\u00b7de", "weht", "der", "A\u00b7tem", "der", "M\u00e4\u00b7na\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus dunklen Teichen winkt es silberh\u00e4ndig,", "tokens": ["Aus", "dunk\u00b7len", "Tei\u00b7chen", "winkt", "es", "sil\u00b7ber\u00b7h\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die vertr\u00e4umten fl\u00fcstern, die Dryaden,", "tokens": ["Und", "die", "ver\u00b7tr\u00e4um\u00b7ten", "fl\u00fcs\u00b7tern", ",", "die", "Dry\u00b7a\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVINF", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In leisen Schauern sehnend und best\u00e4ndig", "tokens": ["In", "lei\u00b7sen", "Schau\u00b7ern", "seh\u00b7nend", "und", "be\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von n\u00e4chtigen geheimnisvollen Gnaden", "tokens": ["Von", "n\u00e4ch\u00b7ti\u00b7gen", "ge\u00b7heim\u00b7nis\u00b7vol\u00b7len", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mit gelbem warmem Mond und stillem Prangen", "tokens": ["Mit", "gel\u00b7bem", "war\u00b7mem", "Mond", "und", "stil\u00b7lem", "Pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und vieler Sch\u00f6nheit, die vorbeigegangen.", "tokens": ["Und", "vie\u00b7ler", "Sch\u00f6n\u00b7heit", ",", "die", "vor\u00b7bei\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch aus dem Garten sind wir schon getreten:", "tokens": ["Doch", "aus", "dem", "Gar\u00b7ten", "sind", "wir", "schon", "ge\u00b7tre\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf goldenen Fluten harren die Galeeren", "tokens": ["Auf", "gol\u00b7de\u00b7nen", "Flu\u00b7ten", "har\u00b7ren", "die", "Ga\u00b7lee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Fl\u00f6tenklang und Segeln, wei\u00dfgebl\u00e4hten ...", "tokens": ["Mit", "Fl\u00f6\u00b7ten\u00b7klang", "und", "Se\u00b7geln", ",", "wei\u00df\u00b7ge\u00b7bl\u00e4h\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und weiter Treppen k\u00f6nigliche Ehren", "tokens": ["Und", "wei\u00b7ter", "Trep\u00b7pen", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit Purpurprunk und silbernen Trompeten ...", "tokens": ["Mit", "Pur\u00b7pur\u00b7prunk", "und", "sil\u00b7ber\u00b7nen", "Trom\u00b7pe\u00b7ten", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Und von ber\u00fchmten griechischen Het\u00e4ren,", "tokens": ["Und", "von", "be\u00b7r\u00fchm\u00b7ten", "grie\u00b7chi\u00b7schen", "He\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In goldenes Braun und Pfirsichrot geh\u00fcllt,", "tokens": ["In", "gol\u00b7de\u00b7nes", "Braun", "und", "Pfir\u00b7si\u00b7ch\u00b7rot", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ist der Balkone Gitterwerk erf\u00fcllt.", "tokens": ["Ist", "der", "Bal\u00b7ko\u00b7ne", "Git\u00b7ter\u00b7werk", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Es gleitet flink durch dunkelblaue Wogen", "tokens": ["Es", "glei\u00b7tet", "flink", "durch", "dun\u00b7kel\u00b7blau\u00b7e", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das goldene Schiff der Insel nun entgegen,", "tokens": ["Das", "gol\u00b7de\u00b7ne", "Schiff", "der", "In\u00b7sel", "nun", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Fl\u00f6tenschall ist singend vorgeflogen,", "tokens": ["Der", "Fl\u00f6\u00b7ten\u00b7schall", "ist", "sin\u00b7gend", "vor\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auf den blumen-\u00fcberquollnen Wegen", "tokens": ["Und", "auf", "den", "blu\u00b7men\u00b7\u00fcber\u00b7quoll\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aus des Theaters schwarzem Marmorbogen", "tokens": ["Aus", "des", "The\u00b7a\u00b7ters", "schwar\u00b7zem", "Mar\u00b7mor\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sieht man den Chor sich feierlich bewegen,", "tokens": ["Sieht", "man", "den", "Chor", "sich", "fei\u00b7er\u00b7lich", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Um Bacchos und die Musen anzurufen,", "tokens": ["Um", "Bac\u00b7chos", "und", "die", "Mu\u00b7sen", "an\u00b7zu\u00b7ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "KON", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die aus dem Rausche die Trag\u00f6die schufen.", "tokens": ["Die", "aus", "dem", "Rau\u00b7sche", "die", "Tra\u00b7g\u00f6\u00b7die", "schu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Im Fackelschein, wo alle Schatten schwanken,", "tokens": ["Im", "Fa\u00b7ckel\u00b7schein", ",", "wo", "al\u00b7le", "Schat\u00b7ten", "schwan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist die Trag\u00f6die k\u00f6niglich beendet,", "tokens": ["Ist", "die", "Tra\u00b7g\u00f6\u00b7die", "k\u00f6\u00b7nig\u00b7lich", "be\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit schweren reifen purpurnen Gedanken", "tokens": ["Mit", "schwe\u00b7ren", "rei\u00b7fen", "pur\u00b7pur\u00b7nen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "ADJA", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sind wir zur Heimfahrt durch die Nacht gewendet.", "tokens": ["Sind", "wir", "zur", "Heim\u00b7fahrt", "durch", "die", "Nacht", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie die Formen all in Dunkel sanken,", "tokens": ["Und", "wie", "die", "For\u00b7men", "all", "in", "Dun\u00b7kel", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PIAT", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So hat auch alles Irdische geendet,", "tokens": ["So", "hat", "auch", "al\u00b7les", "Ir\u00b7di\u00b7sche", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wie der Schlaf im leisen Takt der Wogen \u2013", "tokens": ["Und", "wie", "der", "Schlaf", "im", "lei\u00b7sen", "Takt", "der", "Wo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Willkommen k\u00e4me jetzt der Tod gezogen.", "tokens": ["Will\u00b7kom\u00b7men", "k\u00e4\u00b7me", "jetzt", "der", "Tod", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Sonne sinkt den lebenleeren Tagen", "tokens": ["Die", "Son\u00b7ne", "sinkt", "den", "le\u00b7ben\u00b7lee