{"dta.poem.16301": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Schlange f\u00fchlte lang ein innerliches Qu\u00e4len,", "tokens": ["Die", "Schlan\u00b7ge", "f\u00fchl\u00b7te", "lang", "ein", "in\u00b7ner\u00b7li\u00b7ches", "Qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihre alte Haut nicht ab sich wollte sch\u00e4len.", "tokens": ["Da\u00df", "ih\u00b7re", "al\u00b7te", "Haut", "nicht", "ab", "sich", "woll\u00b7te", "sch\u00e4\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "APPR", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie wu\u00dfte keinen Rath noch Mittel zu ergreifen,", "tokens": ["Sie", "wu\u00df\u00b7te", "kei\u00b7nen", "Rath", "noch", "Mit\u00b7tel", "zu", "er\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die unbequeme H\u00fcll' auf einmal abzustreifen.", "tokens": ["Die", "un\u00b7be\u00b7que\u00b7me", "H\u00fcll'", "auf", "ein\u00b7mal", "ab\u00b7zu\u00b7strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So rathlos wie sie gieng, unachtsam fiel die Schlange", "tokens": ["So", "rath\u00b7los", "wie", "sie", "gieng", ",", "u\u00b7nacht\u00b7sam", "fiel", "die", "Schlan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In eine Schling' am Weg, gestellt zu ihrem Fange.", "tokens": ["In", "ei\u00b7ne", "Schling'", "am", "Weg", ",", "ge\u00b7stellt", "zu", "ih\u00b7rem", "Fan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Geblieben w\u00e4re sie sonst in der Schlinge hangen,", "tokens": ["Ge\u00b7blie\u00b7ben", "w\u00e4\u00b7re", "sie", "sonst", "in", "der", "Schlin\u00b7ge", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nur durch den alten Balg ist sie der Schling' entgangen.", "tokens": ["Nur", "durch", "den", "al\u00b7ten", "Balg", "ist", "sie", "der", "Schling'", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie lie\u00df den Schlauf darin, und ist hindurch geschl\u00fcpft,", "tokens": ["Sie", "lie\u00df", "den", "Schlauf", "da\u00b7rin", ",", "und", "ist", "hin\u00b7durch", "ge\u00b7schl\u00fcpft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PAV", "$,", "KON", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hat die l\u00e4st'ge Haut zugleich nun abgestr\u00fcpft.", "tokens": ["Und", "hat", "die", "l\u00e4st'\u00b7ge", "Haut", "zu\u00b7gleich", "nun", "ab\u00b7ge\u00b7str\u00fcpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So ist der innre Mensch durch den Verlust entronnen", "tokens": ["So", "ist", "der", "inn\u00b7re", "Mensch", "durch", "den", "Ver\u00b7lust", "ent\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des \u00e4u\u00dferen, und hat dadurch sich selbst gewonnen.", "tokens": ["Des", "\u00e4u\u00b7\u00dfe\u00b7ren", ",", "und", "hat", "da\u00b7durch", "sich", "selbst", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KON", "VAFIN", "PAV", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}