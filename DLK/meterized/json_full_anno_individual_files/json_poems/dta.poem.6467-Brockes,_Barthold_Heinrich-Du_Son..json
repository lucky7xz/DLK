{"dta.poem.6467": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Du Son.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df das, so wir gesagt von C\u00f6rpern, sicherlich", "tokens": ["Da\u00df", "das", ",", "so", "wir", "ge\u00b7sagt", "von", "C\u00f6r\u00b7pern", ",", "si\u00b7cher\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PDS", "$,", "ADV", "PPER", "VVPP", "APPR", "NE", "$,", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in der That gegr\u00fcndet sey.", "tokens": ["Und", "in", "der", "That", "ge\u00b7gr\u00fcn\u00b7det", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch diesen stetigen Geruch kan man ja fassen,", "tokens": ["Durch", "die\u00b7sen", "ste\u00b7ti\u00b7gen", "Ge\u00b7ruch", "kan", "man", "ja", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Da\u00df der geringste Theil vom C\u00f6rper sich,", "tokens": ["Da\u00df", "der", "ge\u00b7rings\u00b7te", "Theil", "vom", "C\u00f6r\u00b7per", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bis ins Unendliche noch k\u00f6nne theilen lassen.", "tokens": ["Bis", "ins", "Un\u00b7end\u00b7li\u00b7che", "noch", 