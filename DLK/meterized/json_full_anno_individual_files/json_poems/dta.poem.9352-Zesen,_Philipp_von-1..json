{"dta.poem.9352": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "We\u00df ist der rothe Mund/ das g\u00fcldne Licht", "tokens": ["We\u00df", "ist", "der", "ro\u00b7the", "Mund", "/", "das", "g\u00fcld\u00b7ne", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das durch die sp\u00e4ten n\u00e4chte bricht?", "tokens": ["das", "durch", "die", "sp\u00e4\u00b7ten", "n\u00e4ch\u00b7te", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "We\u00df seyn die rosen-wangen?", "tokens": ["We\u00df", "seyn", "die", "ro\u00b7sen\u00b7wan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "We\u00df ist das lachen doch/", "tokens": ["We\u00df", "ist", "das", "la\u00b7chen", "doch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PDS", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "das mich nun f\u00fchrt gefangen", "tokens": ["das", "mich", "nun", "f\u00fchrt", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "aus s\u00fc\u00dfe Liebes-joch?", "tokens": ["aus", "s\u00fc\u00b7\u00dfe", "Lie\u00b7bes\u00b7joch", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hastu gezeiget dann/ du sch\u00f6ner berg/", "tokens": ["Has\u00b7tu", "ge\u00b7zei\u00b7get", "dann", "/", "du", "sch\u00f6\u00b7ner", "berg", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "ADV", "$(", "PPER", "ADJA", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "ein solches Licht und su\u00dfes Werck\u2019", "tokens": ["ein", "sol\u00b7ches", "Licht", "und", "su\u00b7\u00dfes", "Werck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So kann ich warlich sprechen/", "tokens": ["So", "kann", "ich", "war\u00b7lich", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "die freundligkeit wohnt hier/", "tokens": ["die", "freund\u00b7lig\u00b7keit", "wohnt", "hier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "den spiegel mustu brechen/", "tokens": ["den", "spie\u00b7gel", "mus\u00b7tu", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "o Venus/ selbsten dier.", "tokens": ["o", "Ve\u00b7nus", "/", "selbs\u00b7ten", "dier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier hatt sich Tugend selbst gepflantzet ein/", "tokens": ["Hier", "hatt", "sich", "Tu\u00b7gend", "selbst", "ge\u00b7pflant\u00b7zet", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "NN", "ADV", "VVPP", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hier ist das milde freundlich-seyn/", "tokens": ["hier", "ist", "das", "mil\u00b7de", "freund\u00b7lich\u00b7seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "was sol ich danu nun schlie\u00dfen", "tokens": ["was", "sol", "ich", "da\u00b7nu", "nun", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "aus jhrer Liebligkeit/", "tokens": ["aus", "jhrer", "Lieb\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "die Gratien selbst flie\u00dfen", "tokens": ["die", "Gra\u00b7ti\u00b7en", "selbst", "flie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "und brechen durch den neid.", "tokens": ["und", "bre\u00b7chen", "durch", "den", "neid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das urtheil sprech\u2019 ich nun/ ", "tokens": ["Das", "ur\u00b7theil", "sprech'", "ich", "nun", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "den g\u00fcldnen Apfel geb ich dier/", "tokens": ["den", "g\u00fcld\u00b7nen", "Ap\u00b7fel", "geb", "ich", "dier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die Tugend die ich kaum gesehen/", "tokens": ["die", "Tu\u00b7gend", "die", "ich", "kaum", "ge\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "hatt mich schon so entz\u00fcckt/", "tokens": ["hatt", "mich", "schon", "so", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "was wird dann wohl geschehen/", "tokens": ["was", "wird", "dann", "wohl", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "wenn ich dich recht erblickt.", "tokens": ["wenn", "ich", "dich", "recht", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}