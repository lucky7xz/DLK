{"textgrid.poem.35591": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ruh und Schweigen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hirten begruben die Sonne im kahlen Wald.", "tokens": ["Hir\u00b7ten", "be\u00b7gru\u00b7ben", "die", "Son\u00b7ne", "im", "kah\u00b7len", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Ein Fischer zog", "tokens": ["Ein", "Fi\u00b7scher", "zog"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "In h\u00e4renem Netz den Mond aus frierendem Weiher.", "tokens": ["In", "h\u00e4\u00b7re\u00b7nem", "Netz", "den", "Mond", "aus", "frie\u00b7ren\u00b7dem", "Wei\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "In blauem Kristall", "tokens": ["In", "blau\u00b7em", "Kris\u00b7tall"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wohnt der bleiche Mensch, die Wang' an seine Sterne gelehnt;", "tokens": ["Wohnt", "der", "blei\u00b7che", "Mensch", ",", "die", "Wang'", "an", "sei\u00b7ne", "Ster\u00b7ne", "ge\u00b7lehnt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.