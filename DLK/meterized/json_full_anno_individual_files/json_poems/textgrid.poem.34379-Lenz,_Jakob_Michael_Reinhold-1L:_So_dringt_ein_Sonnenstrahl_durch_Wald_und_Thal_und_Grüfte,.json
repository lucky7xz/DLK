{"textgrid.poem.34379": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: So dringt ein Sonnenstrahl durch Wald und Thal und Gr\u00fcfte,", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So dringt ein Sonnenstrahl durch Wald und Thal und Gr\u00fcfte,", "tokens": ["So", "dringt", "ein", "Son\u00b7nen\u00b7strahl", "durch", "Wald", "und", "Thal", "und", "Gr\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verkl\u00e4rt das Meer im Sturm und dort den sichern Quell,", "tokens": ["Ver\u00b7kl\u00e4rt", "das", "Meer", "im", "Sturm", "und", "dort", "den", "si\u00b7chern", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vergoldet hier ein Blatt, dort starre Felsenkl\u00fcfte,", "tokens": ["Ver\u00b7gol\u00b7det", "hier", "ein", "Blatt", ",", "dort", "star\u00b7re", "Fel\u00b7sen\u00b7kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Macht hier des L\u00f6wen Zorn und dort ein W\u00fcrmchen hell.", "tokens": ["Macht", "hier", "des", "L\u00f6\u00b7wen", "Zorn", "und", "dort", "ein", "W\u00fcrm\u00b7chen", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "KON", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "So sehn wir an dem Tag', als Catharinens Leben", "tokens": ["So", "sehn", "wir", "an", "dem", "Tag'", ",", "als", "Cat\u00b7ha\u00b7ri\u00b7nens", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Wohl der Welt erh\u00f6ht, das Vorurtheil der Zeit", "tokens": ["Das", "Wohl", "der", "Welt", "er\u00b7h\u00f6ht", ",", "das", "Vor\u00b7urt\u00b7heil", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie einen Vorhang fliehn, den Tempel sich erheben", "tokens": ["Wie", "ei\u00b7nen", "Vor\u00b7hang", "fliehn", ",", "den", "Tem\u00b7pel", "sich", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,", "ART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der mit den Sternen w\u00e4hrt von Ru\u00dflands Herrlichkeit.", "tokens": ["Der", "mit", "den", "Ster\u00b7nen", "w\u00e4hrt", "von", "Ru\u00df\u00b7lands", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht f\u00fcr die Ehre nur besorgt, entfernt zu gl\u00e4nzen,", "tokens": ["Nicht", "f\u00fcr", "die", "Eh\u00b7re", "nur", "be\u00b7sorgt", ",", "ent\u00b7fernt", "zu", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADV", "ADJD", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zieht wesentliches Gl\u00fcck auf dampfend Eis am B\u00e4r,", "tokens": ["Zieht", "we\u00b7sent\u00b7li\u00b7ches", "Gl\u00fcck", "auf", "damp\u00b7fend", "Eis", "am", "B\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Zum S\u00fcd, wo Steppen gl\u00fchn, winkt Sie mit Himmels-Kr\u00e4nzen", "tokens": ["Zum", "S\u00fcd", ",", "wo", "Step\u00b7pen", "gl\u00fchn", ",", "winkt", "Sie", "mit", "Him\u00b7mels\u00b7Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "NN", "VVINF", "$,", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verdienst Talent Geschmack aus Rosenhaynen her.", "tokens": ["Ver\u00b7dienst", "Ta\u00b7lent", "Ge\u00b7schmack", "aus", "Ro\u00b7sen\u00b7hay\u00b7nen", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So tritt kein Sterblicher, kein Strauchler auf die B\u00fchne,", "tokens": ["So", "tritt", "kein", "Sterb\u00b7li\u00b7cher", ",", "kein", "Strauch\u00b7ler", "auf", "die", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein gro\u00dfes Herz allein sch\u00e4tzt was ihm \u00e4hnlich ist.", "tokens": ["Ein", "gro\u00b7\u00dfes", "Herz", "al\u00b7lein", "sch\u00e4tzt", "was", "ihm", "\u00e4hn\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "PWS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Durch Peters Monument verhei\u00dfet Catharine", "tokens": ["Durch", "Pe\u00b7ters", "Mo\u00b7nu\u00b7ment", "ver\u00b7hei\u00b7\u00dfet", "Cat\u00b7ha\u00b7ri\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und theilt die Loosung aus nach welchem Maa\u00df sie mi\u00dft.", "tokens": ["Und", "theilt", "die", "Loo\u00b7sung", "aus", "nach", "wel\u00b7chem", "Maa\u00df", "sie", "mi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "APPR", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Bald wird, wo der Colo\u00df nach seinen Flotten schaute", "tokens": ["Bald", "wird", ",", "wo", "der", "Co\u00b7lo\u00df", "nach", "sei\u00b7nen", "Flot\u00b7ten", "schau\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "PWAV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Er, wie eine Welt der Sch\u00f6pfer, werden sah,", "tokens": ["Die", "Er", ",", "wie", "ei\u00b7ne", "Welt", "der", "Sch\u00f6p\u00b7fer", ",", "wer\u00b7den", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Europens Retter ziehn, bald steht ihr Argonaute", "tokens": ["Eu\u00b7ro\u00b7pens", "Ret\u00b7ter", "ziehn", ",", "bald", "steht", "ihr", "Ar\u00b7go\u00b7nau\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVINF", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf viel Jahrhunderte zum Schutz der Handlung da.", "tokens": ["Auf", "viel", "Jahr\u00b7hun\u00b7der\u00b7te", "zum", "Schutz", "der", "Hand\u00b7lung", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Bald wird sie Gallizins, Rumanzoff, Panins finden,", "tokens": ["Bald", "wird", "sie", "Gal\u00b7li\u00b7zins", ",", "Ru\u00b7man\u00b7zoff", ",", "Pa\u00b7nins", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "$,", "VVFIN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Des Sultans ganze Macht Verg\u00fcnstigung nur seyn.", "tokens": ["Des", "Sul\u00b7tans", "gan\u00b7ze", "Macht", "Ver\u00b7g\u00fcns\u00b7ti\u00b7gung", "nur", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn wei\u00df die G\u00f6ttliche wie Rom zu \u00fcberwinden,", "tokens": ["Denn", "wei\u00df", "die", "G\u00f6tt\u00b7li\u00b7che", "wie", "Rom", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KOKOM", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wu\u00dfte Rom wie sie nicht Nachbarn zu verzeyhn.", "tokens": ["So", "wu\u00df\u00b7te", "Rom", "wie", "sie", "nicht", "Nach\u00b7barn", "zu", "ver\u00b7zeyhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "PWAV", "PPER", "PTKNEG", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Hier ist mehr als Trajan. Von Hermen rings umgeben,", "tokens": ["Hier", "ist", "mehr", "als", "Tra\u00b7jan", ".", "Von", "Her\u00b7men", "rings", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "KOUS", "NN", "$.", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Ingiald zaubrisch einst durch Runen V\u00f6lker lenkt',", "tokens": ["Wie", "In\u00b7gi\u00b7ald", "zaub\u00b7risch", "einst", "durch", "Ru\u00b7nen", "V\u00f6l\u00b7ker", "lenkt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sehn wir auch sie Gesetz den wildsten Schw\u00e4rmen geben", "tokens": ["Sehn", "wir", "auch", "sie", "Ge\u00b7setz", "den", "wilds\u00b7ten", "Schw\u00e4r\u00b7men", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die, so besch\u00fctzt, aus Dank sich selber eingeschr\u00e4nkt.", "tokens": ["Die", ",", "so", "be\u00b7sch\u00fctzt", ",", "aus", "Dank", "sich", "sel\u00b7ber", "ein\u00b7ge\u00b7schr\u00e4nkt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "VVPP", "$,", "APPR", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "O r\u00fchrendes Gesicht! Vom End der Erde str\u00f6men", "tokens": ["O", "r\u00fch\u00b7ren\u00b7des", "Ge\u00b7sicht", "!", "Vom", "End", "der", "Er\u00b7de", "str\u00f6\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die V\u00f6lker um den Trohn, den sie sich selbst erw\u00e4hlt,", "tokens": ["Die", "V\u00f6l\u00b7ker", "um", "den", "Trohn", ",", "den", "sie", "sich", "selbst", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sind, da sie ihr Band aus Mutterh\u00e4nden nehmen,", "tokens": ["Und", "sind", ",", "da", "sie", "ihr", "Band", "aus", "Mut\u00b7ter\u00b7h\u00e4n\u00b7den", "neh\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word"