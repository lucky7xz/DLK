{"dta.poem.13192": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewer deine Majest\u00e4t auch einsam gehen sieht,", "tokens": ["\u201e", "wer", "dei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "auch", "ein\u00b7sam", "ge\u00b7hen", "sieht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201emerckt, da\u00df ihn solcher Blick auf die Gedancken zieht:", "tokens": ["\u201e", "merckt", ",", "da\u00df", "ihn", "sol\u00b7cher", "Blick", "auf", "die", "Ge\u00b7dan\u00b7cken", "zieht", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eda\u00df viele Feinde seynd, die deinen Thron best\u00fcrmen,", "tokens": ["\u201e", "da\u00df", "vie\u00b7le", "Fein\u00b7de", "seynd", ",", "die", "dei\u00b7nen", "Thron", "be\u00b7st\u00fcr\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIAT", "NN", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "120\u201dUnd da\u00df die Tugenden dich wieder ihn beschirmen.", "tokens": ["\"", "Und", "da\u00df", "die", "Tu\u00b7gen\u00b7den", "dich", "wie\u00b7der", "ihn", "be\u00b7schir\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "ART", "NN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eer stellt sich vor, wie du den Ruhm nur jenem gibst", "tokens": ["\u201e", "er", "stellt", "sich", "vor", ",", "wie", "du", "den", "Ruhm", "nur", "je\u00b7nem", "gibst"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PRF", "PTKVZ", "$,", "PWAV", "PPER", "ART", "NN", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edem er geb\u00fchrt, und der dich liebt, weil du ihn liebst.", "tokens": ["\u201e", "dem", "er", "ge\u00b7b\u00fchrt", ",", "und", "der", "dich", "liebt", ",", "weil", "du", "ihn", "liebst", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "VVPP", "$,", "KON", "ART", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}