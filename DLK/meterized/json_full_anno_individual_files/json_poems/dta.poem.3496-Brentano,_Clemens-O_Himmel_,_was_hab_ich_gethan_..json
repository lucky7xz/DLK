{"dta.poem.3496": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "O Himmel ,  was hab ich gethan .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Klosterleben ist eine harte Pein,               ", "tokens": ["Das", "Klos\u00b7ter\u00b7le\u00b7ben", "ist", "ei\u00b7ne", "har\u00b7te", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil ich ohn mein Liebchen mu\u00df seyn;", "tokens": ["Weil", "ich", "ohn", "mein", "Lieb\u00b7chen", "mu\u00df", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich habe mich drein ergeben zur Zeit,", "tokens": ["Ich", "ha\u00b7be", "mich", "drein", "er\u00b7ge\u00b7ben", "zur", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Orden ertrag ich mit Schmerz und mit Leid.", "tokens": ["Den", "Or\u00b7den", "er\u00b7trag", "ich", "mit", "Schmerz", "und", "mit", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "O Himmel, was hab ich gethan?", "tokens": ["O", "Him\u00b7mel", ",", "was", "hab", "ich", "ge\u00b7than", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Die Liebe war schuldig daran.", "tokens": ["Die", "Lie\u00b7be", "war", "schul\u00b7dig", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Und komm ich am Morgen zur Kirche hinein,", "tokens": ["Und", "komm", "ich", "am", "Mor\u00b7gen", "zur", "Kir\u00b7che", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So sing ich die Metten allein;", "tokens": ["So", "sing", "ich", "die", "Met\u00b7ten", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und wenn ich das ", "tokens": ["Und", "wenn", "ich", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "So liegt mir mein Herzallerliebster im Sinn.", "tokens": ["So", "liegt", "mir", "mein", "Her\u00b7zal\u00b7ler\u00b7liebs\u00b7ter", "im", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Ach Himmel, was hab ich gethan?", "tokens": ["Ach", "Him\u00b7mel", ",", "was", "hab", "ich", "ge\u00b7than", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Die Liebe ist schuldig daran.", "tokens": ["Die", "Lie\u00b7be", "ist", "schul\u00b7dig", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Des Mittags wenn ich zum Essen hin geh,", "tokens": ["Des", "Mit\u00b7tags", "wenn", "ich", "zum", "Es\u00b7sen", "hin", "geh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "PPER", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So find ich mein Tischlein allein;", "tokens": ["So", "find", "ich", "mein", "Tisc\u00b7hlein", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da e\u00df ich mein Brod und trinke mein Wein,", "tokens": ["Da", "e\u00df", "ich", "mein", "Brod", "und", "trin\u00b7ke", "mein", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach k\u00f6nnt ich bei meinem lieb Sch\u00e4tzelein seyn.", "tokens": ["Ach", "k\u00f6nnt", "ich", "bei", "mei\u00b7nem", "lieb", "Sch\u00e4t\u00b7zel\u00b7ein", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "VAINF", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "O Himmel, was hab ich gethan?", "tokens": ["O", "Him\u00b7mel", ",", "was", "hab", "ich", "ge\u00b7than", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Die Liebe ist schuldig daran.", "tokens": ["Die", "Lie\u00b7be", "ist", "schul\u00b7dig", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Des Abends, wenn ich nun schlafen da geh,", "tokens": ["Des", "A\u00b7bends", ",", "wenn", "ich", "nun", "schla\u00b7fen", "da", "geh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "So find ich mein Bettlein ja leer;", "tokens": ["So", "find", "ich", "mein", "Bet\u00b7tlein", "ja", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Da greif ich bald hin, da greif ich bald her,", "tokens": ["Da", "greif", "ich", "bald", "hin", ",", "da", "greif", "ich", "bald", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ach wenn ich bei meinem Herzliebsten doch w\u00e4r!", "tokens": ["Ach", "wenn", "ich", "bei", "mei\u00b7nem", "Herz\u00b7liebs\u00b7ten", "doch", "w\u00e4r", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.5": {"text": "Ach Himmel, was hab ich gethan?", "tokens": ["Ach", "Him\u00b7mel", ",", "was", "hab", "ich", "ge\u00b7than", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Die Liebe ist schuldig daran.", "tokens": ["Die", "Lie\u00b7be", "ist", "schul\u00b7dig", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Da k\u00f6mmt ja mein Vater und Mutter auch her,", "tokens": ["Da", "k\u00f6mmt", "ja", "mein", "Va\u00b7ter", "und", "Mut\u00b7ter", "auch", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie beten wohl f\u00fcr sich allein;", "tokens": ["Sie", "be\u00b7ten", "wohl", "f\u00fcr", "sich", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie haben bundf\u00e4rbige R\u00f6cklein auch an,", "tokens": ["Sie", "ha\u00b7ben", "bund\u00b7f\u00e4r\u00b7bi\u00b7ge", "R\u00f6ck\u00b7lein", "auch", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und ich, ich mu\u00df in dem Kuttenrock stahn.", "tokens": ["Und", "ich", ",", "ich", "mu\u00df", "in", "dem", "Kut\u00b7ten\u00b7rock", "stahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ach Himmel, was hab ich gethan?", "tokens": ["Ach", "Him\u00b7mel", ",", "was", "hab", "ich", "ge\u00b7than", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Die Liebe ist schuldig daran.", "tokens": ["Die", "Lie\u00b7be", "ist", "schul\u00b7dig", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}