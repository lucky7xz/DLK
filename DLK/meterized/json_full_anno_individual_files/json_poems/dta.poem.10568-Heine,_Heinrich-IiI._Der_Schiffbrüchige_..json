{"dta.poem.10568": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Der Schiffbr\u00fcchige .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Hoffnung und Liebe! Alles zertr\u00fcmmert!", "tokens": ["Hoff\u00b7nung", "und", "Lie\u00b7be", "!", "Al\u00b7les", "zer\u00b7tr\u00fcm\u00b7mert", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "PIS", "VVPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und ich selber, gleich einer Leiche,", "tokens": ["Und", "ich", "sel\u00b7ber", ",", "gleich", "ei\u00b7ner", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die grollend ausgeworfen das Meer,", "tokens": ["Die", "grol\u00b7lend", "aus\u00b7ge\u00b7wor\u00b7fen", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Lieg' ich am Strande,", "tokens": ["Lieg'", "ich", "am", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Am \u00f6den, kahlen Strande.", "tokens": ["Am", "\u00f6\u00b7den", ",", "kah\u00b7len", "Stran\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vor mir woget die Wasserw\u00fcste,", "tokens": ["Vor", "mir", "wo\u00b7get", "die", "Was\u00b7ser\u00b7w\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Hinter mir liegt nur Kummer und Elend,", "tokens": ["Hin\u00b7ter", "mir", "liegt", "nur", "Kum\u00b7mer", "und", "E\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und \u00fcber mich hin ziehen die Wolken,", "tokens": ["Und", "\u00fc\u00b7ber", "mich", "hin", "zie\u00b7hen", "die", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Die formlos grauen T\u00f6chter der Luft,", "tokens": ["Die", "form\u00b7los", "grau\u00b7en", "T\u00f6ch\u00b7ter", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Die aus dem Meer', in Nebeleimern,", "tokens": ["Die", "aus", "dem", "Meer'", ",", "in", "Ne\u00b7be\u00b7lei\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Das Wasser sch\u00f6pfen,", "tokens": ["Das", "Was\u00b7ser", "sch\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Und es m\u00fchsam schleppen und schleppen,", "tokens": ["Und", "es", "m\u00fch\u00b7sam", "schlep\u00b7pen", "und", "schlep\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVINF", "KON", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Und es wieder versch\u00fctten in's Meer,", "tokens": ["Und", "es", "wie\u00b7der", "ver\u00b7sch\u00fct\u00b7ten", "in's", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Ein tr\u00fcbes, langweil'ges Gesch\u00e4ft,", "tokens": ["Ein", "tr\u00fc\u00b7bes", ",", "lang\u00b7weil'\u00b7ges", "Ge\u00b7sch\u00e4ft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Und nutzlos, wie mein eignes Leben.", "tokens": ["Und", "nutz\u00b7los", ",", "wie", "mein", "eig\u00b7nes", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "KOKOM", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Wogen murmeln, die M\u00f6ven schrillen,", "tokens": ["Die", "Wo\u00b7gen", "mur\u00b7meln", ",", "die", "M\u00f6\u00b7ven", "schril\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Alte Erinn'rungen wehen mich an,", "tokens": ["Al\u00b7te", "Er\u00b7inn'\u00b7run\u00b7gen", "we\u00b7hen", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Vergessene Tr\u00e4ume, erloschene Bilder,", "tokens": ["Ver\u00b7ges\u00b7se\u00b7ne", "Tr\u00e4u\u00b7me", ",", "er\u00b7lo\u00b7sche\u00b7ne", "Bil\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Qualvoll s\u00fc\u00dfe, tauchen hervor!", "tokens": ["Qual\u00b7voll", "s\u00fc\u00b7\u00dfe", ",", "tau\u00b7chen", "her\u00b7vor", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Es lebt ein Weib im Norden,", "tokens": ["Es", "lebt", "ein", "Weib", "im", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein sch\u00f6nes Weib, k\u00f6niglich sch\u00f6n.", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Weib", ",", "k\u00f6\u00b7nig\u00b7lich", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die schlanke Zypressengestalt", "tokens": ["Die", "schlan\u00b7ke", "Zyp\u00b7res\u00b7sen\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umschlie\u00dft ein l\u00fcstern wei\u00dfes Gewand;", "tokens": ["Um\u00b7schlie\u00dft", "ein", "l\u00fcs\u00b7tern", "wei\u00b7\u00dfes", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Die dunkle Lockenf\u00fclle,", "tokens": ["Die", "dunk\u00b7le", "Lo\u00b7cken\u00b7f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie eine selige Nacht, ergie\u00dft sich", "tokens": ["Wie", "ei\u00b7ne", "se\u00b7li\u00b7ge", "Nacht", ",", "er\u00b7gie\u00dft", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "VVFIN", "PRF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Von dem hohen, flechtengekr\u00f6nten Haupte,", "tokens": ["Von", "dem", "ho\u00b7hen", ",", "flech\u00b7ten\u00b7ge\u00b7kr\u00f6n\u00b7ten", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sie ringelt sich tr\u00e4umerisch s\u00fc\u00df", "tokens": ["Sie", "rin\u00b7gelt", "sich", "tr\u00e4u\u00b7me\u00b7risch", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "Um das s\u00fc\u00dfe, blasse Antlitz;", "tokens": ["Um", "das", "s\u00fc\u00b7\u00dfe", ",", "blas\u00b7se", "Ant\u00b7litz", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und aus dem s\u00fc\u00dfen, blassen Antlitz,", "tokens": ["Und", "aus", "dem", "s\u00fc\u00b7\u00dfen", ",", "blas\u00b7sen", "Ant\u00b7litz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Gro\u00df und gewaltig, strahlt ein Auge,", "tokens": ["Gro\u00df", "und", "ge\u00b7wal\u00b7tig", ",", "strahlt", "ein", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wie eine schwarze Sonne.", "tokens": ["Wie", "ei\u00b7ne", "schwar\u00b7ze", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O, du schwarze Sonne, wie oft,", "tokens": ["O", ",", "du", "schwar\u00b7ze", "Son\u00b7ne", ",", "wie", "oft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,", "PWAV", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Entz\u00fcckend oft, trank ich aus dir", "tokens": ["Ent\u00b7z\u00fc\u00b7ckend", "oft", ",", "trank", "ich", "aus", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wilden Begeist'rungsflammen,", "tokens": ["Die", "wil\u00b7den", "Be\u00b7geist'\u00b7rungs\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und stand und taumelte, feuerberauscht \u2014", "tokens": ["Und", "stand", "und", "tau\u00b7mel\u00b7te", ",", "feu\u00b7er\u00b7be\u00b7rauscht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "ADJD", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Dann schwebte ein taubenmildes L\u00e4cheln", "tokens": ["Dann", "schweb\u00b7te", "ein", "tau\u00b7ben\u00b7mil\u00b7des", "L\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Um die hochgesch\u00fcrzten, stolzen Lippen,", "tokens": ["Um", "die", "hoch\u00b7ge\u00b7sch\u00fcrz\u00b7ten", ",", "stol\u00b7zen", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Und die hochgesch\u00fcrzten, stolzen Lippen", "tokens": ["Und", "die", "hoch\u00b7ge\u00b7sch\u00fcrz\u00b7ten", ",", "stol\u00b7zen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Hauchten Worte, s\u00fc\u00df wie Mondlicht,", "tokens": ["Hauch\u00b7ten", "Wor\u00b7te", ",", "s\u00fc\u00df", "wie", "Mond\u00b7licht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zart wie der Duft der Rose \u2014", "tokens": ["Und", "zart", "wie", "der", "Duft", "der", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und meine Seele erhob sich", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "er\u00b7hob", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und flog, wie ein Aar, hinauf in den Himmel!", "tokens": ["Und", "flog", ",", "wie", "ein", "Aar", ",", "hin\u00b7auf", "in", "den", "Him\u00b7mel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Schweigt, ihr Wogen und M\u00f6ven!", "tokens": ["Schweigt", ",", "ihr", "Wo\u00b7gen", "und", "M\u00f6\u00b7ven", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Vor\u00fcber ist Alles, Gl\u00fcck und Hoffnung,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "Al\u00b7les", ",", "Gl\u00fcck", "und", "Hoff\u00b7nung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hoffnung und Liebe! Ich liege am Boden,", "tokens": ["Hoff\u00b7nung", "und", "Lie\u00b7be", "!", "Ich", "lie\u00b7ge", "am", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Ein \u00f6der, schiffbr\u00fcchiger Mann,", "tokens": ["Ein", "\u00f6\u00b7der", ",", "schiff\u00b7br\u00fc\u00b7chi\u00b7ger", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und dr\u00fccke mein gl\u00fchendes Antlitz", "tokens": ["Und", "dr\u00fc\u00b7cke", "mein", "gl\u00fc\u00b7hen\u00b7des", "Ant\u00b7litz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "In den feuchten Sand.", "tokens": ["In", "den", "feuch\u00b7ten", "Sand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}}}}