], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Von uns getahn? Denn, wahrlich h\u00e4ttest du,", "tokens": ["Von", "uns", "ge\u00b7tahn", "?", "Denn", ",", "wahr\u00b7lich", "h\u00e4t\u00b7test", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$.", "KON", "$,", "ADV", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wie ich, gelebt, so lebtest du noch immer.", "tokens": ["Wie", "ich", ",", "ge\u00b7lebt", ",", "so", "leb\u00b7test", "du", "noch", "im\u00b7mer", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}}}}