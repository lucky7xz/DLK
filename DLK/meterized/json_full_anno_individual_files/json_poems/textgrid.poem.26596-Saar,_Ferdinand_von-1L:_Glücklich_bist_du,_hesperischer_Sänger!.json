{"textgrid.poem.26596": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00fccklich bist du, hesperischer S\u00e4nger!", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich bist du, hesperischer S\u00e4nger!", "tokens": ["Gl\u00fcck\u00b7lich", "bist", "du", ",", "hes\u00b7pe\u00b7ri\u00b7scher", "S\u00e4n\u00b7ger", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ob in Venedigs Gondel du tr\u00e4umst,", "tokens": ["Ob", "in", "Ve\u00b7ne\u00b7digs", "Gon\u00b7del", "du", "tr\u00e4umst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "NE", "PPER", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ob in Florenz du weilst oder im ernsten Rom \u2013", "tokens": ["Ob", "in", "Flo\u00b7renz", "du", "weilst", "o\u00b7der", "im", "erns\u00b7ten", "Rom", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "PPER", "VVFIN", "KON", "APPRART", "ADJA", "NE", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ob du wandelst am Gestad des blauen Tyrrhenermeers:", "tokens": ["Ob", "du", "wan\u00b7delst", "am", "Ge\u00b7stad", "des", "blau\u00b7en", "Tyr\u00b7rhe\u00b7ner\u00b7meers", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "\u00dcberall lebt dir ein Volk,", "tokens": ["\u00dc\u00b7be\u00b7rall", "lebt", "dir", "ein", "Volk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Das unbefangen noch,", "tokens": ["Das", "un\u00b7be\u00b7fan\u00b7gen", "noch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Empf\u00e4nglich an Herz und Sinn,", "tokens": ["Emp\u00b7f\u00e4ng\u00b7lich", "an", "Herz", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gern deinem Liede lauscht", "tokens": ["Gern", "dei\u00b7nem", "Lie\u00b7de", "lauscht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und nimmer dir vorwirft,", "tokens": ["Und", "nim\u00b7mer", "dir", "vor\u00b7wirft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df du die eig'nen Gedanken gro\u00dfgezogen", "tokens": ["Da\u00df", "du", "die", "eig'\u00b7nen", "Ge\u00b7dan\u00b7ken", "gro\u00df\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "An Dante's Geist,", "tokens": ["An", "Dan\u00b7te's", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Oder getr\u00e4nkt sie", "tokens": ["O\u00b7der", "ge\u00b7tr\u00e4nkt", "sie"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVPP", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Mit Petrarca's schmelzendem Wehmuthslaut.", "tokens": ["Mit", "Pe\u00b7tra\u00b7rca's", "schmel\u00b7zen\u00b7dem", "Weh\u00b7muths\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Nicht verstellt es dem Nachgebor'nen", "tokens": ["Nicht", "ver\u00b7stellt", "es", "dem", "Nach\u00b7ge\u00b7bor'\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Des Ruhmes Pfad mit Standbildern der Vergangenheit,", "tokens": ["Des", "Ruh\u00b7mes", "Pfad", "mit", "Stand\u00b7bil\u00b7dern", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.16": {"text": "Und wie stolz es auch ist", "tokens": ["Und", "wie", "stolz", "es", "auch", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "PPER", "ADV", "VAFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Auf der Vorzeit Gr\u00f6\u00dfen:", "tokens": ["Auf", "der", "Vor\u00b7zeit", "Gr\u00f6\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Nicht minder stolz und neidlos", "tokens": ["Nicht", "min\u00b7der", "stolz", "und", "neid\u00b7los"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Blickt es auf den Sohn der Gegenwart. \u2013", "tokens": ["Blickt", "es", "auf", "den", "Sohn", "der", "Ge\u00b7gen\u00b7wart", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Ach, wie so anders beschieden es die G\u00f6tter", "tokens": ["Ach", ",", "wie", "so", "an\u00b7ders", "be\u00b7schie\u00b7den", "es", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Dem nordischen Sangesgenossen!", "tokens": ["Dem", "nor\u00b7di\u00b7schen", "San\u00b7ges\u00b7ge\u00b7nos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.22": {"text": "Taub bleibt ihm ein Volk von \u00bbDenkern\u00ab,", "tokens": ["Taub", "bleibt", "ihm", "ein", "Volk", "von", "\u00bb", "Den\u00b7kern", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "APPR", "$(", "NN", "$(", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "Das Todte feiert,", "tokens": ["Das", "Tod\u00b7te", "fei\u00b7ert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Um Lebendige einzusargen;", "tokens": ["Um", "Le\u00b7ben\u00b7di\u00b7ge", "ein\u00b7zu\u00b7sar\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Ein Volk,", "tokens": ["Ein", "Volk", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "Das seit jeher", "tokens": ["Das", "seit", "je\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+--", "measure": "dactylic.init"}, "line.27": {"text": "Am liebsten fremden Kl\u00e4ngen gelauscht,", "tokens": ["Am", "liebs\u00b7ten", "frem\u00b7den", "Kl\u00e4n\u00b7gen", "ge\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.28": {"text": "An heimischen tadelnd, was es an jenen preis't,", "tokens": ["An", "hei\u00b7mi\u00b7schen", "ta\u00b7delnd", ",", "was", "es", "an", "je\u00b7nen", "preis't", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVPP", "$,", "PWS", "PPER", "APPR", "PDS", "VVFIN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Und, schulmeisternd, best\u00e4ndig fordert,", "tokens": ["Und", ",", "schul\u00b7meis\u00b7ternd", ",", "be\u00b7st\u00e4n\u00b7dig", "for\u00b7dert", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.30": {"text": "Was es, stumpfsinnig,", "tokens": ["Was", "es", ",", "stumpf\u00b7sin\u00b7nig", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "$,", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.31": {"text": "Am Gebotenen nicht erkennt.", "tokens": ["Am", "Ge\u00b7bo\u00b7te\u00b7nen", "nicht", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.32": {"text": "So, mehr und mehr in sich selbs