{"dta.poem.21485": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "In der Osternacht.  \n 1881.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df duftet und leise athmet", "tokens": ["S\u00fc\u00df", "duf\u00b7tet", "und", "lei\u00b7se", "ath\u00b7met"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "ADJD", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Drau\u00dfen die Osternacht,", "tokens": ["Drau\u00b7\u00dfen", "die", "Os\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ruhig tr\u00e4umen die Gassen,", "tokens": ["Ru\u00b7hig", "tr\u00e4u\u00b7men", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Vom blauen Monde bewacht.", "tokens": ["Vom", "blau\u00b7en", "Mon\u00b7de", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Die d\u00fcrren Zweige der Linde", "tokens": ["Die", "d\u00fcr\u00b7ren", "Zwei\u00b7ge", "der", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wiegen und schwanken im Wind,", "tokens": ["Wie\u00b7gen", "und", "schwan\u00b7ken", "im", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und durch die schauernden L\u00fcfte", "tokens": ["Und", "durch", "die", "schau\u00b7ern\u00b7den", "L\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Blut des Fr\u00fchlings rinnt.", "tokens": ["Das", "Blut", "des", "Fr\u00fch\u00b7lings", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Glocken t\u00f6nen und l\u00e4uten", "tokens": ["Die", "Glo\u00b7cken", "t\u00f6\u00b7nen", "und", "l\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Leise ins stille Gemach,", "tokens": ["Lei\u00b7se", "ins", "stil\u00b7le", "Ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Sie l\u00e4uten und rufen den Fr\u00fchling", "tokens": ["Sie", "l\u00e4u\u00b7ten", "und", "ru\u00b7fen", "den", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Im klopfenden Busen wach.", "tokens": ["Im", "klop\u00b7fen\u00b7den", "Bu\u00b7sen", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Und von den Bl\u00e4ttern der Bibel", "tokens": ["Und", "von", "den", "Bl\u00e4t\u00b7tern", "der", "Bi\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Hebe ich tr\u00e4umend mein Haupt, \u2014", "tokens": ["He\u00b7be", "ich", "tr\u00e4u\u00b7mend", "mein", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PPOSAT", "NN", "$,", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Und schaue des Heilands Augen,", "tokens": ["Und", "schau\u00b7e", "des", "Hei\u00b7lands", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den l\u00e4ngst ich gestorben geglaubt.", "tokens": ["Den", "l\u00e4ngst", "ich", "ge\u00b7stor\u00b7ben", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VVPP", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Ich sehe die rothen Wunden", "tokens": ["Ich", "se\u00b7he", "die", "ro\u00b7then", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und den bleichen, friedlichen Mund,", "tokens": ["Und", "den", "blei\u00b7chen", ",", "fried\u00b7li\u00b7chen", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und um die Schl\u00e4fe geflochten", "tokens": ["Und", "um", "die", "Schl\u00e4\u00b7fe", "ge\u00b7floch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Dornen blutigen Bund.", "tokens": ["Der", "Dor\u00b7nen", "blu\u00b7ti\u00b7gen", "Bund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Ich trinke von seinen Augen", "tokens": ["Ich", "trin\u00b7ke", "von", "sei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Thr\u00e4nen schmerzliche Glut, \u2026", "tokens": ["Der", "Thr\u00e4\u00b7nen", "schmerz\u00b7li\u00b7che", "Glut", ",", "\u2026"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und f\u00fchle, wie sanft seine Rechte", "tokens": ["Und", "f\u00fch\u00b7le", ",", "wie", "sanft", "sei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Auf meinem Haupte ruht . . . . .", "tokens": ["Auf", "mei\u00b7nem", "Haup\u00b7te", "ruht", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "$.", "$.", "$.", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Unnahbar unendliche Gottheit,", "tokens": ["Un\u00b7nah\u00b7bar", "un\u00b7end\u00b7li\u00b7che", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sind\u2019s wilde Schmerzen allein,", "tokens": ["Sin\u00b7d's", "wil\u00b7de", "Schmer\u00b7zen", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die von dir reden und zeugen", "tokens": ["Die", "von", "dir", "re\u00b7den", "und", "zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVINF", "KON", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Und deinem g\u00f6ttlichen Sein?", "tokens": ["Und", "dei\u00b7nem", "g\u00f6tt\u00b7li\u00b7chen", "Sein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Sind\u2019s nur die Schauer des Todes,", "tokens": ["Sin\u00b7d's", "nur", "die", "Schau\u00b7er", "des", "To\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Aus denen dein Mund uns spricht,", "tokens": ["Aus", "de\u00b7nen", "dein", "Mund", "uns", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und strahlt nicht auch leuchtend im Fr\u00fchling", "tokens": ["Und", "strahlt", "nicht", "auch", "leuch\u00b7tend", "im", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "VVPP", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Dein himmlisches Angesicht?", "tokens": ["Dein", "himm\u00b7li\u00b7sches", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Die Glocken t\u00f6nen und l\u00e4uten,", "tokens": ["Die", "Glo\u00b7cken", "t\u00f6\u00b7nen", "und", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es webt und quillt in der Luft,", "tokens": ["Es", "webt", "und", "quillt", "in", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Rings fl\u00fcstert ein s\u00fc\u00dfer Zauber,", "tokens": ["Rings", "fl\u00fcs\u00b7tert", "ein", "s\u00fc\u00b7\u00dfer", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und str\u00f6mt ein Rosenduft.", "tokens": ["Und", "str\u00f6mt", "ein", "Ro\u00b7sen\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}