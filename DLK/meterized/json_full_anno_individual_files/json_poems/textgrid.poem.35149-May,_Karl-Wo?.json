{"textgrid.poem.35149": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wo?", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo liegt dein Heil? Liegt es in deinem Leibe,", "tokens": ["Wo", "liegt", "dein", "Heil", "?", "Liegt", "es", "in", "dei\u00b7nem", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "F\u00fcr den du dich und tausend Andre plagst?", "tokens": ["F\u00fcr", "den", "du", "dich", "und", "tau\u00b7send", "And\u00b7re", "plagst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PRF", "KON", "CARD", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denkst du, da\u00df er dein Mittelpunkt verbleibe,", "tokens": ["Denkst", "du", ",", "da\u00df", "er", "dein", "Mit\u00b7tel\u00b7punkt", "ver\u00b7blei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um den du dich im Kreise treibst und jagst?", "tokens": ["Um", "den", "du", "dich", "im", "Krei\u00b7se", "treibst", "und", "jagst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "PRF", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du widmest ihm fast jeden der Gedanken,", "tokens": ["Du", "wid\u00b7mest", "ihm", "fast", "je\u00b7den", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und deine Pl\u00e4ne, deine W\u00fcnsche ranken", "tokens": ["Und", "dei\u00b7ne", "Pl\u00e4\u00b7ne", ",", "dei\u00b7ne", "W\u00fcn\u00b7sche", "ran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sich nur um dieses theure G\u00f6tzenbild,", "tokens": ["Sich", "nur", "um", "die\u00b7ses", "theu\u00b7re", "G\u00f6t\u00b7zen\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das dir als Krone aller Sch\u00f6pfung gilt.", "tokens": ["Das", "dir", "als", "Kro\u00b7ne", "al\u00b7ler", "Sch\u00f6p\u00b7fung", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "KOUS", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schau dir ihn an! Sieh krank und siech ihn liegen", "tokens": ["Schau", "dir", "ihn", "an", "!", "Sieh", "krank", "und", "siech", "ihn", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "PTKVZ", "$.", "NE", "ADJD", "KON", "VVIMP", "PPER", "VVFIN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Zu seiner eignen und des N\u00e4chsten Pein!", "tokens": ["Zu", "sei\u00b7ner", "eig\u00b7nen", "und", "des", "N\u00e4chs\u00b7ten", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der winzigste Bacill kann ihn besiegen,", "tokens": ["Der", "win\u00b7zigs\u00b7te", "Ba\u00b7cill", "kann", "ihn", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der kleinste Fehltritt ihm verderblich sein.", "tokens": ["Der", "kleins\u00b7te", "Fehl\u00b7tritt", "ihm", "ver\u00b7derb\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Betrachtest du sein Kommen und sein Gehen,", "tokens": ["Be\u00b7trach\u00b7test", "du", "sein", "Kom\u00b7men", "und", "sein", "Ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So wirst du's nicht begreifen, nicht verstehen,", "tokens": ["So", "wirst", "du's", "nicht", "be\u00b7grei\u00b7fen", ",", "nicht", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df dieser Schw\u00e4chling dir als fester Halt", "tokens": ["Da\u00df", "die\u00b7ser", "Schw\u00e4ch\u00b7ling", "dir", "als", "fes\u00b7ter", "Halt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr deinen Geist, f\u00fcr deine Seele galt.", "tokens": ["F\u00fcr", "dei\u00b7nen", "Geist", ",", "f\u00fcr", "dei\u00b7ne", "See\u00b7le", "galt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun denke nach! Er selbst wird ja gehalten", "tokens": ["Nun", "den\u00b7ke", "nach", "!", "Er", "selbst", "wird", "ja", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$.", "PPER", "ADV", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Anstatt da\u00df er zu st\u00fctzen je verstand:", "tokens": ["An\u00b7statt", "da\u00df", "er", "zu", "st\u00fct\u00b7zen", "je", "ver\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PTKZU", "VVINF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe will das Kind zum Mann gestalten;", "tokens": ["Die", "Lie\u00b7be", "will", "das", "Kind", "zum", "Mann", "ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie thut es freundlich durch die Elternhand.", "tokens": ["Sie", "thut", "es", "freund\u00b7lich", "durch", "die", "El\u00b7tern\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Gattenliebe f\u00fchrt ihn dann durchs Leben,", "tokens": ["Die", "Gat\u00b7ten\u00b7lie\u00b7be", "f\u00fchrt", "ihn", "dann", "durchs", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um Festigung und Reife ihm zu geben,", "tokens": ["Um", "Fes\u00b7ti\u00b7gung", "und", "Rei\u00b7fe", "ihm", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wenn er scheiden geht, sagt ihm das Weh", "tokens": ["Und", "wenn", "er", "schei\u00b7den", "geht", ",", "sagt", "ihm", "das", "Weh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Liebe seiner Kinder noch Ade.", "tokens": ["Der", "Lie\u00b7be", "sei\u00b7ner", "Kin\u00b7der", "noch", "A\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wei\u00dft du es nun? Es geht und strahlt die Liebe", "tokens": ["Wei\u00dft", "du", "es", "nun", "?", "Es", "geht", "und", "strahlt", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$.", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr alle Welt von Gottes Himmel aus,", "tokens": ["F\u00fcr", "al\u00b7le", "Welt", "von", "Got\u00b7tes", "Him\u00b7mel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ob sie lange, ob sie kurz nur bliebe,", "tokens": ["Und", "ob", "sie", "lan\u00b7ge", ",", "ob", "sie", "kurz", "nur", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie kommt und weilt und wirkt in jedem Haus.", "tokens": ["Sie", "kommt", "und", "weilt", "und", "wirkt", "in", "je\u00b7dem", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie ist die einzge St\u00fctze jedes Lebens;", "tokens": ["Sie", "ist", "die", "einz\u00b7ge", "St\u00fct\u00b7ze", "je\u00b7des", "Le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Leben ohne Liebe ist vergebens,", "tokens": ["Ein", "Le\u00b7ben", "oh\u00b7ne", "Lie\u00b7be", "ist", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Denn wo sie fehlt, da flieht das innre Gl\u00fcck,", "tokens": ["Denn", "wo", "sie", "fehlt", ",", "da", "flieht", "das", "inn\u00b7re", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und dann bleibt freilich nur der Leib zur\u00fcck.", "tokens": ["Und", "dann", "bleibt", "frei\u00b7lich", "nur", "der", "Leib", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wo liegt dein Heil? O, la\u00df dich doch belehren;", "tokens": ["Wo", "liegt", "dein", "Heil", "?", "O", ",", "la\u00df", "dich", "doch", "be\u00b7leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "NE", "$,", "VVIMP", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Leibessorge bietet dir es nicht,", "tokens": ["Die", "Lei\u00b7bes\u00b7sor\u00b7ge", "bie\u00b7tet", "dir", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du hast fortan nach innen dich zu kehren,", "tokens": ["Du", "hast", "for\u00b7tan", "nach", "in\u00b7nen", "dich", "zu", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo Gott durch deine Seele zu dir spricht.", "tokens": ["Wo", "Gott", "durch", "dei\u00b7ne", "See\u00b7le", "zu", "dir", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und diese Stimme wird auf deine Fragen", "tokens": ["Und", "die\u00b7se", "Stim\u00b7me", "wird", "auf", "dei\u00b7ne", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dir jederzeit die rechte Antwort sagen.", "tokens": ["Dir", "je\u00b7der\u00b7zeit", "die", "rech\u00b7te", "Ant\u00b7wort", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nur sie, nur sie verk\u00fcndet dir dein Heil,", "tokens": ["Nur", "sie", ",", "nur", "sie", "ver\u00b7k\u00fcn\u00b7det", "dir", "dein", "Heil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und folgst du ihr, so wird es dir zu theil.", "tokens": ["Und", "folgst", "du", "ihr", ",", "so", "wird", "es", "dir", "zu", "theil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wo liegt dein Heil? Liegt es in deinem Leibe,", "tokens": ["Wo", "liegt", "dein", "Heil", "?", "Liegt", "es", "in", "dei\u00b7nem", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "F\u00fcr den du dich und tausend Andre plagst?", "tokens": ["F\u00fcr", "den", "du", "dich", "und", "tau\u00b7send", "And\u00b7re", "plagst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPER", "PRF", "KON", "CARD", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denkst du, da\u00df er dein Mittelpunkt verbleibe,", "tokens": ["Denkst", "du", ",", "da\u00df", "er", "dein", "Mit\u00b7tel\u00b7punkt", "ver\u00b7blei\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um den du dich im Kreise treibst und jagst?", "tokens": ["Um", "den", "du", "dich", "im", "Krei\u00b7se", "treibst", "und", "jagst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PPER", "PRF", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du widmest ihm fast jeden der Gedanken,", "tokens": ["Du", "wid\u00b7mest", "ihm", "fast", "je\u00b7den", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und deine Pl\u00e4ne, deine W\u00fcnsche ranken", "tokens": ["Und", "dei\u00b7ne", "Pl\u00e4\u00b7ne", ",", "dei\u00b7ne", "W\u00fcn\u00b7sche", "ran\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sich nur um dieses theure G\u00f6tzenbild,", "tokens": ["Sich", "nur", "um", "die\u00b7ses", "theu\u00b7re", "G\u00f6t\u00b7zen\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Das dir als Krone aller Sch\u00f6pfung gilt.", "tokens": ["Das", "dir", "als", "Kro\u00b7ne", "al\u00b7ler", "Sch\u00f6p\u00b7fung", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "KOUS", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Schau dir ihn an! Sieh krank und siech ihn liegen", "tokens": ["Schau", "dir", "ihn", "an", "!", "Sieh", "krank", "und", "siech", "ihn", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "PTKVZ", "$.", "NE", "ADJD", "KON", "VVIMP", "PPER", "VVFIN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Zu seiner eignen und des N\u00e4chsten Pein!", "tokens": ["Zu", "sei\u00b7ner", "eig\u00b7nen", "und", "des", "N\u00e4chs\u00b7ten", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der winzigste Bacill kann ihn besiegen,", "tokens": ["Der", "win\u00b7zigs\u00b7te", "Ba\u00b7cill", "kann", "ihn", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der kleinste Fehltritt ihm verderblich sein.", "tokens": ["Der", "kleins\u00b