d", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$.", "PPER", "ADV", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Anstatt da\u00df er zu st\u00fctzen je verstand:", "tokens": ["An\u00b7statt", "da\u00df", "er", "zu", "st\u00fct\u00b7zen", "je", "ver\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PTKZU", "VVINF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Liebe will das Kind zum Mann gestalten;", "tokens": ["Die", "Lie\u00b7be", "will", "das", "Kind", "zum", "Mann", "ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie thut es freundlich durch die Elternhand.", "tokens": ["Sie", "thut", "es", "freund\u00b7lich", "durch", "die", "El\u00b7tern\u00b7hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Gattenliebe f\u00fchrt ihn dann durchs Leben,", "tokens": ["Die", "Gat\u00b7ten\u00b7lie\u00b7be", "f\u00fchrt", "ihn", "dann", "durchs", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um Festigung und Reife ihm zu geben,", "tokens": ["Um", "Fes\u00b7ti\u00b7gung", "und", "Rei\u00b7fe", "ihm", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wenn er scheiden geht, sagt ihm das Weh", "tokens": ["Und", "wenn", "er", "schei\u00b7den", "geht", ",", "sagt", "ihm", "das", "Weh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Liebe seiner Kinder noch Ade.", "tokens": ["Der", "Lie\u00b7be", "sei\u00b7ner", "Kin\u00b7der", "noch", "A\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Wei\u00dft du es nun? Es geht und strahlt die Liebe", "tokens": ["Wei\u00dft", "du", "es", "nun", "?", "Es", "geht", "und", "strahlt", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$.", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr alle Welt von Gottes Himmel aus,", "tokens": ["F\u00fcr", "al\u00b7le", "Welt", "von", "Got\u00b7tes", "Him\u00b7mel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und ob sie lange, ob sie kurz nur bliebe,", "tokens": ["Und", "ob", "sie", "lan\u00b7ge", ",", "ob", "sie", "kurz", "nur", "blie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measur