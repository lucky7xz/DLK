{"textgrid.poem.61751": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Deutsches Leben", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was steht ihr d\u00fcster und betroffen,", "tokens": ["Was", "steht", "ihr", "d\u00fcs\u00b7ter", "und", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr ein deutsch Panier doch tragt,", "tokens": ["Die", "ihr", "ein", "deutsch", "Pa\u00b7nier", "doch", "tragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJD", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun endlich, endlich unsrem Hoffen", "tokens": ["Nun", "end\u00b7lich", ",", "end\u00b7lich", "uns\u00b7rem", "Hof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Morgen der Erf\u00fcllung tagt?", "tokens": ["Ein", "Mor\u00b7gen", "der", "Er\u00b7f\u00fcl\u00b7lung", "tagt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O bannt von eurer Stirn die Wolke!", "tokens": ["O", "bannt", "von", "eu\u00b7rer", "Stirn", "die", "Wol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verscheucht den w\u00fcsten Traum der Nacht,", "tokens": ["Ver\u00b7scheucht", "den", "w\u00fcs\u00b7ten", "Traum", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als w\u00e4r' es aus mit unsrem Volke,", "tokens": ["Als", "w\u00e4r'", "es", "aus", "mit", "uns\u00b7rem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Weil's anders kam, als ihr gedacht.", "tokens": ["Weil's", "an\u00b7ders", "kam", ",", "als", "ihr", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn als der Sturm der sieben Wochen", "tokens": ["Denn", "als", "der", "Sturm", "der", "sie\u00b7ben", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ersch\u00fcttert nah und fern,", "tokens": ["Die", "Welt", "er\u00b7sch\u00fct\u00b7tert", "nah", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl hat er morsche Zier gebrochen,", "tokens": ["Wohl", "hat", "er", "mor\u00b7sche", "Zier", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nimmer unsres Wesens Kern.", "tokens": ["Doch", "nim\u00b7mer", "uns\u00b7res", "We\u00b7sens", "Kern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus tausend Quellen um die Wette", "tokens": ["Aus", "tau\u00b7send", "Quel\u00b7len", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Braust unversiegt von Ort zu Ort,", "tokens": ["Braust", "un\u00b7ver\u00b7siegt", "von", "Ort", "zu", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Braust stolzer nur im neuen Bette", "tokens": ["Braust", "stol\u00b7zer", "nur", "im", "neu\u00b7en", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Strom des deutschen Lebens fort.", "tokens": ["Der", "Strom", "des", "deut\u00b7schen", "Le\u00b7bens", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch wettert durch der Schlacht Gedr\u00f6hne", "tokens": ["Noch", "wet\u00b7tert", "durch", "der", "Schlacht", "Ge\u00b7dr\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Schwert, ein Blitz in deutscher Hand,", "tokens": ["Das", "Schwert", ",", "ein", "Blitz", "in", "deut\u00b7scher", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch wissen l\u00e4chelnd unsre S\u00f6hne", "tokens": ["Noch", "wis\u00b7sen", "l\u00e4\u00b7chelnd", "uns\u00b7re", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu sterben f\u00fcr das Vaterland.", "tokens": ["Zu", "ster\u00b7ben", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und die in schwindelnden Gedanken", "tokens": ["Und", "die", "in", "schwin\u00b7deln\u00b7den", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Herrn der Welt sich schon geglaubt,", "tokens": ["Die", "Herrn", "der", "Welt", "sich", "schon", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit bangem Neide sehn die Franken", "tokens": ["Mit", "ban\u00b7gem", "Nei\u00b7de", "sehn", "die", "Fran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Kranz des Siegs auf ", "tokens": ["Den", "Kranz", "des", "Siegs", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Noch waltet am ererbten Herde", "tokens": ["Noch", "wal\u00b7tet", "am", "er\u00b7erb\u00b7ten", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der deutsche Bauer schlicht und stark,", "tokens": ["Der", "deut\u00b7sche", "Bau\u00b7er", "schlicht", "und", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beharrlich, wie die Kraft der Erde,", "tokens": ["Be\u00b7harr\u00b7lich", ",", "wie", "die", "Kraft", "der", "Er\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die treu ihn n\u00e4hrt mit ihrem Mark.", "tokens": ["Die", "treu", "ihn", "n\u00e4hrt", "mit", "ih\u00b7rem", "Mark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch w\u00e4chst auf hohem Schlo\u00df, dem Ruhme", "tokens": ["Noch", "w\u00e4chst", "auf", "ho\u00b7hem", "Schlo\u00df", ",", "dem", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nacheifernd, den der Ahn gewann,", "tokens": ["Na\u00b7ch\u00b7ei\u00b7fernd", ",", "den", "der", "Ahn", "ge\u00b7wann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Manch k\u00fchner Spro\u00df zum Rittertume", "tokens": ["Manch", "k\u00fch\u00b7ner", "Spro\u00df", "zum", "Rit\u00b7ter\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Geistes und des Schwerts heran.", "tokens": ["Des", "Geis\u00b7tes", "und", "des", "Schwerts", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Noch bl\u00fcht gesegnet in der Runde", "tokens": ["Noch", "bl\u00fcht", "ge\u00b7seg\u00b7net", "in", "der", "Run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der St\u00e4dte Wandel, Kunst und Flei\u00df;", "tokens": ["Der", "St\u00e4d\u00b7te", "Wan\u00b7del", ",", "Kunst", "und", "Flei\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch wurzelt dort im festen Grunde", "tokens": ["Noch", "wur\u00b7zelt", "dort", "im", "fes\u00b7ten", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des B\u00fcrgersinns der Freiheit Reis.", "tokens": ["Des", "B\u00fcr\u00b7ger\u00b7sinns", "der", "Frei\u00b7heit", "Reis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Wettkampf jeder Kraft erschaffen,", "tokens": ["Im", "Wett\u00b7kampf", "je\u00b7der", "Kraft", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gedeiht das Neue Tag f\u00fcr Tag,", "tokens": ["Ge\u00b7deiht", "das", "Neu\u00b7e", "Tag", "f\u00fcr", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch b\u00fcrgt die ernste Pflicht der Waffen,", "tokens": ["Doch", "b\u00fcrgt", "die", "erns\u00b7te", "Pflicht", "der", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df alte Zucht nicht rosten mag.", "tokens": ["Da\u00df", "al\u00b7te", "Zucht", "nicht", "ros\u00b7ten", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "ADJA", "NN", "PTKNEG", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Noch l\u00e4\u00dft zu nimmerm\u00fcdem Streben", "tokens": ["Noch", "l\u00e4\u00dft", "zu", "nim\u00b7mer\u00b7m\u00fc\u00b7dem", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Forschung ihre Fackel wehn,", "tokens": ["Die", "For\u00b7schung", "ih\u00b7re", "Fa\u00b7ckel", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Vorzeit reichen Schatz zu heben,", "tokens": ["Der", "Vor\u00b