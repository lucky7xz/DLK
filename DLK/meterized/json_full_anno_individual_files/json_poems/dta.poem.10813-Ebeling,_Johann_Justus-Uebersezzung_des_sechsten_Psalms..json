{"dta.poem.10813": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Uebersezzung des sechsten  \n Psalms.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr! der du ein GOtt der G\u00f6tter,", "tokens": ["Herr", "!", "der", "du", "ein", "Gott", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Straf mich nicht in deinen Zorn,", "tokens": ["Straf", "mich", "nicht", "in", "dei\u00b7nen", "Zorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sondern sey doch mein Erretter,", "tokens": ["Son\u00b7dern", "sey", "doch", "mein", "Er\u00b7ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und la\u00df deiner G\u00fcte Born,", "tokens": ["Und", "la\u00df", "dei\u00b7ner", "G\u00fc\u00b7te", "Born", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf mich armen S\u00fcnder fliessen;", "tokens": ["Auf", "mich", "ar\u00b7men", "S\u00fcn\u00b7der", "flies\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df mich deiner Huld geniessen,", "tokens": ["La\u00df", "mich", "dei\u00b7ner", "Huld", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df mich stat des Donners Grimm,", "tokens": ["La\u00df", "mich", "stat", "des", "Don\u00b7ners", "Grimm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6ren deine Gnaden Stimm.", "tokens": ["H\u00f6\u00b7ren", "dei\u00b7ne", "Gna\u00b7den", "Stimm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin schwach von vielen S\u00fcnden,", "tokens": ["Ich", "bin", "schwach", "von", "vie\u00b7len", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Seele ist sehr matt,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ist", "sehr", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei dir mu\u00df ich H\u00fclfe finden,", "tokens": ["Bei", "dir", "mu\u00df", "ich", "H\u00fcl\u00b7fe", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Darum fleh ich deine Gnad.", "tokens": ["Da\u00b7rum", "fleh", "ich", "dei\u00b7ne", "Gnad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein Gebeine ist erschrokken:", "tokens": ["Mein", "Ge\u00b7bei\u00b7ne", "ist", "er\u00b7schrok\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und von innren Weh ganz trokken:", "tokens": ["Und", "von", "inn\u00b7ren", "Weh", "ganz", "trok\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Darum HErr! komm ich zu dir,", "tokens": ["Da\u00b7rum", "Herr", "!", "komm", "ich", "zu", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$.", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Heil mich, weil ich Noth versp\u00fcr.", "tokens": ["Heil", "mich", ",", "weil", "ich", "Noth", "ver\u00b7sp\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Meine Seele ist verzaget,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ist", "ver\u00b7za\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil mich des Gewissens Bis,", "tokens": ["Weil", "mich", "des", "Ge\u00b7wis\u00b7sens", "Bis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Tag und Nacht ganz trostlos naget,", "tokens": ["Tag", "und", "Nacht", "ganz", "trost\u00b7los", "na\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine H\u00fclf ist mir gewis:", "tokens": ["Dei\u00b7ne", "H\u00fclf", "ist", "mir", "ge\u00b7wis", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber HErr! mir ist sehr bange,", "tokens": ["A\u00b7ber", "Herr", "!", "mir", "ist", "sehr", "ban\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn es w\u00e4hrt die Zeit sehr lange,", "tokens": ["Denn", "es", "w\u00e4hrt", "die", "Zeit", "sehr", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da ich bei dem innren Weh,", "tokens": ["Da", "ich", "bei", "dem", "inn\u00b7ren", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Um dein Gnaden Antliz fleh.", "tokens": ["Um", "dein", "Gna\u00b7den", "Ant\u00b7liz", "fleh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wende dich, sprich meiner Seelen,", "tokens": ["Wen\u00b7de", "dich", ",", "sprich", "mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein vergn\u00fcgend Trostwort zu,", "tokens": ["Ein", "ver\u00b7gn\u00fc\u00b7gend", "Trost\u00b7wort", "zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Damit ich nach langen Qu\u00e4len,", "tokens": ["Da\u00b7mit", "ich", "nach", "lan\u00b7gen", "Qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich finde H\u00fclf und Ruh.", "tokens": ["End\u00b7lich", "fin\u00b7de", "H\u00fclf", "und", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hilf mir um der G\u00fcte Willen,", "tokens": ["Hilf", "mir", "um", "der", "G\u00fc\u00b7te", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn du kanst den Kummer stillen", "tokens": ["Denn", "du", "kanst", "den", "Kum\u00b7mer", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der mich bisher Tag und Nacht,", "tokens": ["Der", "mich", "bis\u00b7her", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Viele tr\u00fcbe Stunden macht.", "tokens": ["Vie\u00b7le", "tr\u00fc\u00b7be", "Stun\u00b7den", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn mich wird das Grab verschr\u00e4nken,", "tokens": ["Wenn", "mich", "wird", "das", "Grab", "ver\u00b7schr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Todt mich weggeraft,", "tokens": ["Und", "der", "Todt", "mich", "weg\u00b7ge\u00b7raft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kan ich nicht an dich gedenken;", "tokens": ["Kan", "ich", "nicht", "an", "dich", "ge\u00b7den\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn mir meine Lebenskrafft", "tokens": ["Wenn", "mir", "mei\u00b7ne", "Le\u00b7bens\u00b7krafft"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist, durch lange Angst genommen,", "tokens": ["Ist", ",", "durch", "lan\u00b7ge", "Angst", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich zu dem Grabe kommen;", "tokens": ["Und", "ich", "zu", "dem", "Gra\u00b7be", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "So verstummt mein blasser Mund,", "tokens": ["So", "ver\u00b7stummt", "mein", "blas\u00b7ser", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und macht deine G\u00fct nicht kund.", "tokens": ["Und", "macht", "dei\u00b7ne", "G\u00fct", "nicht", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich bin schon von Seufzen m\u00fcde,", "tokens": ["Ich", "bin", "schon", "von", "Seuf\u00b7zen", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Weil mein Auge immer thr\u00e4nt,", "tokens": ["Weil", "mein", "Au\u00b7ge", "im\u00b7mer", "thr\u00e4nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Herze nur nach Friede,", "tokens": ["Und", "mein", "Her\u00b7ze", "nur", "nach", "Frie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit gebrochnen Seufzern sehnt:", "tokens": ["Mit", "ge\u00b7broch\u00b7nen", "Seuf\u00b7zern", "sehnt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein Blut schwimmt im Thr\u00e4nen-Fl\u00fcssen,", "tokens": ["Mein", "Blut", "schwimmt", "im", "Thr\u00e4\u00b7nen\u00b7F\u00b7l\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die aus meinen Augen schiessen,", "tokens": ["Die", "aus", "mei\u00b7nen", "Au\u00b7gen", "schies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die nasse Wehmuth zeigt,", "tokens": ["Und", "die", "nas\u00b7se", "Weh\u00b7muth", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was vor Angst das Herz verschweigt.", "tokens": ["Was", "vor", "Angst", "das", "Herz", "ver\u00b7schweigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Die Gestalt ist ganz verfallen,", "tokens": ["Die", "Ge\u00b7stalt", "ist", "ganz", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich bin vor Trauren alt,", "tokens": ["Und", "ich", "bin", "vor", "Trau\u00b7ren", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich mu\u00df so lange wallen,", "tokens": ["Weil", "ich", "mu\u00df", "so", "lan\u00b7ge", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eh ich sehe die Gestalt", "tokens": ["Eh", "ich", "se\u00b7he", "die", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die mich wiederum erquikket;", "tokens": ["Die", "mich", "wie\u00b7de\u00b7rum", "er\u00b7quik\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meine Seele wird gedr\u00fckket:", "tokens": ["Mei\u00b7ne", "See\u00b7le", "wird", "ge\u00b7dr\u00fck\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Allenthalben kommen mir,", "tokens": ["Al\u00b7len\u00b7thal\u00b7ben", "kom\u00b7men", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nichts als Angst Geb\u00fcrge f\u00fcr.", "tokens": ["Nichts", "als", "Angst", "Ge\u00b7b\u00fcr\u00b7ge", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "APPR", "NN", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Weichet von mir Uebelth\u00e4ter,", "tokens": ["Wei\u00b7chet", "von", "mir", "Ue\u00b7belt\u00b7h\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D