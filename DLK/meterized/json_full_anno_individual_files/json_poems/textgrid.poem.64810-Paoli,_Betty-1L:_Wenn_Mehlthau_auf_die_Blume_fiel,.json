{"textgrid.poem.64810": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn Mehlthau auf die Blume fiel,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Mehlthau auf die Blume fiel,", "tokens": ["Wenn", "Mehlt\u00b7hau", "auf", "die", "Blu\u00b7me", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sinkt sie ohne Hoffen,", "tokens": ["Da", "sinkt", "sie", "oh\u00b7ne", "Hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ward ein Baum des Blitzes Ziel,", "tokens": ["Und", "ward", "ein", "Baum", "des", "Blit\u00b7zes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00fcrzt er, zu Tod getroffen.", "tokens": ["St\u00fcrzt", "er", ",", "zu", "Tod", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn in einer Menschenbrust", "tokens": ["Und", "wenn", "in", "ei\u00b7ner", "Men\u00b7schen\u00b7brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erregt die Qualen werden,", "tokens": ["Er\u00b7regt", "die", "Qua\u00b7len", "wer\u00b7den", ","], "token_info": ["word", "word",