{"textgrid.poem.41872": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wandre \u00fcber Berg und Thal", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wandre \u00fcber Berg und Thal", "tokens": ["Ich", "wand\u00b7re", "\u00fc\u00b7ber", "Berg", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcber gr\u00fcne Haiden,", "tokens": ["Und", "\u00fc\u00b7ber", "gr\u00fc\u00b7ne", "Hai\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mit mir wandert meine Qual,", "tokens": ["Und", "mit", "mir", "wan\u00b7dert", "mei\u00b7ne", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will nimmer von mir scheiden;", "tokens": ["Will", "nim\u00b7mer", "von", "mir", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und schifft' ich auch durch's weite Meer,", "tokens": ["Und", "schifft'", "ich", "auch", "durch's", "wei\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie k\u00e4m' auch dort wohl hinterher.", "tokens": ["Sie", "k\u00e4m'", "auch", "dort", "wohl", "hin\u00b7ter\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl bl\u00fchn viel Blumen auf der Flur,", "tokens": ["Wohl", "bl\u00fchn", "viel", "Blu\u00b7men", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hab' ich nicht gesehen,", "tokens": ["Die", "hab'", "ich", "nicht", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn Eine Blume seh' ich nur", "tokens": ["Denn", "Ei\u00b7ne", "Blu\u00b7me", "seh'", "ich", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf allen Wegen stehen.", "tokens": ["Auf", "al\u00b7len", "We\u00b7gen", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nach ihr hab' ich mich oft geb\u00fcckt", "tokens": ["Nach", "ihr", "hab'", "ich", "mich", "oft", "ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und doch sie nimmer abgepfl\u00fcckt.", "tokens": ["Und", "doch", "sie", "nim\u00b7mer", "ab\u00b7ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Bienen sumsen durch das Gras", "tokens": ["Die", "Bie\u00b7nen", "sum\u00b7sen", "durch", "das", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4ngen an den Bl\u00fcthen;", "tokens": ["Und", "h\u00e4n\u00b7gen", "an", "den", "Bl\u00fc\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das macht mein Auge tr\u00fcb' und na\u00df,", "tokens": ["Das", "macht", "mein", "Au\u00b7ge", "tr\u00fcb'", "und", "na\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kann mir's nicht verbieten.", "tokens": ["Ich", "kann", "mir's", "nicht", "ver\u00b7bie\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr s\u00fc\u00dfen Lippen, roth und weich,", "tokens": ["Ihr", "s\u00fc\u00b7\u00dfen", "Lip\u00b7pen", ",", "roth", "und", "weich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl hing ich nimmer so an euch!", "tokens": ["Wohl", "hing", "ich", "nim\u00b7mer", "so", "an", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gar lieblich singen nah' und fern", "tokens": ["Gar", "lieb\u00b7lich", "sin\u00b7gen", "nah'", "und", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die V\u00f6gel auf den Zweigen;", "tokens": ["Die", "V\u00f6\u00b7gel", "auf", "den", "Zwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohl s\u00e4ng' ich mit den V\u00f6geln gern,", "tokens": ["Wohl", "s\u00e4ng'", "ich", "mit", "den", "V\u00f6\u00b7geln", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch mu\u00df ich traurig schweigen,", "tokens": ["Doch", "mu\u00df", "ich", "trau\u00b7rig", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn Liebeslust und Liebespein", "tokens": ["Denn", "Lie\u00b7bes\u00b7lust", "und", "Lie\u00b7be\u00b7spein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die bleiben jedes gern allein.", "tokens": ["Die", "blei\u00b7ben", "je\u00b7des", "gern", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Am Himmel seh' ich fl\u00fcgelschnell", "tokens": ["Am", "Him\u00b7mel", "seh'", "ich", "fl\u00fc\u00b7gel\u00b7schnell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wolken weiter ziehen,", "tokens": ["Die", "Wol\u00b7ken", "wei\u00b7ter", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Welle rieselt leicht und hell,", "tokens": ["Die", "Wel\u00b7le", "rie\u00b7selt", "leicht", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df immer nahn und fliehen;", "tokens": ["Mu\u00df", "im\u00b7mer", "nahn", "und", "flie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJA", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch haschen, wenn's vom Winde ruht,", "tokens": ["Doch", "ha\u00b7schen", ",", "wenn's", "vom", "Win\u00b7de", "ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich Wolk' und Wolke, Fluth und Fluth.", "tokens": ["Sich", "Wol\u00b7k'", "und", "Wol\u00b7ke", ",", "Fluth", "und", "Fluth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ich wandre hin, ich wandre her", "tokens": ["Ich", "wand\u00b7re", "hin", ",", "ich", "wand\u00b7re", "her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey Sturm und heitern Tagen,", "tokens": ["Bey", "Sturm", "und", "hei\u00b7tern", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und doch erschau' ich's nimmermehr", "tokens": ["Und", "doch", "er\u00b7schau'", "ich's", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kann es nicht erjagen.", "tokens": ["Und", "kann", "es", "nicht", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Liebessehnen, Liebesqual,", "tokens": ["O", "Lie\u00b7bes\u00b7seh\u00b7nen", ",", "Lie\u00b7bes\u00b7qual", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wann ruht der Wanderer einmal?", "tokens": ["Wann", "ruht", "der", "Wan\u00b7de\u00b7rer", "ein\u00b7mal", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Ich wandre \u00fcber Berg und Thal", "tokens": ["Ich", "wand\u00b7re", "\u00fc\u00b7ber", "Berg", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcber gr\u00fcne Haiden,", "tokens": ["Und", "\u00fc\u00b7ber", "gr\u00fc\u00b7ne", "Hai\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punc