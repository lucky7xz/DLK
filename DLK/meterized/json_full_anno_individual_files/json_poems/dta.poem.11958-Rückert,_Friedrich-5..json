{"dta.poem.11958": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Entweder \u00fcberstreng an andern magst du schelten", "tokens": ["Ent\u00b7we\u00b7der", "\u00fc\u00b7bers\u00b7treng", "an", "an\u00b7dern", "magst", "du", "schel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIS", "VMFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Flecken, um dadurch nur selber rein zu gelten;", "tokens": ["Den", "Fle\u00b7cken", ",", "um", "da\u00b7durch", "nur", "sel\u00b7ber", "rein", "zu", "gel\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "PAV", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Oder entschuldigen zu nachsichtvoll die Schw\u00e4chen", "tokens": ["O\u00b7der", "ent\u00b7schul\u00b7di\u00b7gen", "zu", "nach\u00b7sicht\u00b7voll", "die", "Schw\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKA", "ADJD", "ART", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der andern, da\u00df sie nur dir selbst den Stab nicht brechen:", "tokens": ["Der", "an\u00b7dern", ",", "da\u00df", "sie", "nur", "dir", "selbst", "den", "Stab", "nicht", "bre\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "PPER", "ADV", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du hast in jedem Fall zum Fehler dich bekannt,", "tokens": ["Du", "hast", "in", "je\u00b7dem", "Fall", "zum", "Feh\u00b7ler", "dich", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "APPRART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort weil du ihn zu gro\u00df, hier weil zu klein genannt.", "tokens": ["Dort", "weil", "du", "ihn", "zu", "gro\u00df", ",", "hier", "weil", "zu", "klein", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "PTKA", "ADJD", "$,", "ADV", "KOUS", "PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Magst du ihn schweigend dort ableugnen, hier einr\u00e4umen,", "tokens": ["Magst", "du", "ihn", "schwei\u00b7gend", "dort", "ab\u00b7leug\u00b7nen", ",", "hier", "ein\u00b7r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADJD", "ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In jedem Fall wirst du zu bessern ihn vers\u00e4umen.", "tokens": ["In", "je\u00b7dem", "Fall", "wirst", "du", "zu", "bes\u00b7sern", "ihn", "ver\u00b7s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPER", "PTKZU", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}