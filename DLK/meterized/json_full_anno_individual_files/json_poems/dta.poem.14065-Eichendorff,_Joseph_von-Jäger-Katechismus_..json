{"dta.poem.14065": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4ger-Katechismus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch, kommt ein J\u00e4ger, wohlgeboren,", "tokens": ["Doch", ",", "kommt", "ein", "J\u00e4\u00b7ger", ",", "wohl\u00b7ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Horn irrt, er blitzt rosenroth,", "tokens": ["Das", "Horn", "irrt", ",", "er", "blitzt", "ro\u00b7sen\u00b7roth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da ist das Hirschlein wohl verloren,", "tokens": ["Da", "ist", "das", "Hirsc\u00b7hlein", "wohl", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stellt selber sich zum lust'gen Tod.", "tokens": ["Stellt", "sel\u00b7ber", "sich", "zum", "lust'\u00b7gen", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}