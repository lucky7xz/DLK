{"dta.poem.114": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er  gratuli ret seinen Freund zu der  \n Vers\u00f6hnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So mu\u00df dich/ werther Freund/ nach dunckeln Finsternissen", "tokens": ["So", "mu\u00df", "dich", "/", "wert\u00b7her", "Freund", "/", "nach", "dun\u00b7ckeln", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$(", "ADJA", "NN", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein helles Gnaden-Licht mit seinen Strahlen k\u00fcssen/", "tokens": ["Ein", "hel\u00b7les", "Gna\u00b7den\u00b7Licht", "mit", "sei\u00b7nen", "Strah\u00b7len", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es \u00e4ndert sich die Zeit/ und l\u00e4st nach Angst und Pein", "tokens": ["Es", "\u00e4n\u00b7dert", "sich", "die", "Zeit", "/", "und", "l\u00e4st", "nach", "Angst", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$(", "KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dennoch ein holdes Gl\u00fcck das beste Labsahl seyn.", "tokens": ["Den\u00b7noch", "ein", "hol\u00b7des", "Gl\u00fcck", "das", "bes\u00b7te", "Lab\u00b7sahl", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hast du schon eine Zeit ", "tokens": ["Hast", "du", "schon", "ei\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Jetzt wird die Sonne dich so zu bestrahlen wissen/", "tokens": ["Jetzt", "wird", "die", "Son\u00b7ne", "dich", "so", "zu", "be\u00b7strah\u00b7len", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df der ", "tokens": ["Da\u00df", "der"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Dir k\u00fcnfftig heissen wird. Der Sturm h\u00f6rt auf zu dreun.", "tokens": ["Dir", "k\u00fcnff\u00b7tig", "heis\u00b7sen", "wird", ".", "Der", "Sturm", "h\u00f6rt", "auf", "zu", "dreun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VAFIN", "$.", "ART", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Lust und Vergn\u00fcgen sucht mit Anmuth dich zu kr\u00f6nen", "tokens": ["Lust", "und", "Ver\u00b7gn\u00fc\u00b7gen", "sucht", "mit", "An\u00b7muth", "dich", "zu", "kr\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "NN", "PPER", "PTKZU", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Und den gehabten Schmertz durch Freude auszus\u00f6hnen/", "tokens": ["Und", "den", "ge\u00b7hab\u00b7ten", "Schmertz", "durch", "Freu\u00b7de", "aus\u00b7zu\u00b7s\u00f6h\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Liebe ist bem\u00fcht mit ihrem zarten Kleinen/", "tokens": ["Die", "Lie\u00b7be", "ist", "be\u00b7m\u00fcht", "mit", "ih\u00b7rem", "zar\u00b7ten", "Klei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nach ausgestandner Prob\u2019 nach Schmertzen und nach Weinen", "tokens": ["Nach", "aus\u00b7ge\u00b7stand\u00b7ner", "Prob'", "nach", "Schmert\u00b7zen", "und", "nach", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit aller Lieblichkeit die Unlust zu vers\u00fcssen/", "tokens": ["Mit", "al\u00b7ler", "Lieb\u00b7lich\u00b7keit", "die", "Un\u00b7lust", "zu", "ver\u00b7s\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die dein getreues Hertz von ihnen leiden m\u00fcssen.", "tokens": ["Die", "dein", "ge\u00b7treu\u00b7es", "Hertz", "von", "ih\u00b7nen", "lei\u00b7den", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}