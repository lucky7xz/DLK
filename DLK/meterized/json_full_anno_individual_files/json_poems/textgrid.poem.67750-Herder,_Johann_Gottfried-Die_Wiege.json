{"textgrid.poem.67750": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Wiege", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist der kleine Sklave, der in Banden", "tokens": ["Wer", "ist", "der", "klei\u00b7ne", "Skla\u00b7ve", ",", "der", "in", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus diesem fr\u00fchen Sarge Klagen weint? \u2013", "tokens": ["Aus", "die\u00b7sem", "fr\u00fc\u00b7hen", "Sar\u00b7ge", "Kla\u00b7gen", "weint", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Bruder? Br\u00fcder, o, so l\u00f6set seine Banden,", "tokens": ["Mein", "Bru\u00b7der", "?", "Br\u00fc\u00b7der", ",", "o", ",", "so", "l\u00f6\u00b7set", "sei\u00b7ne", "Ban\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "NN", "$,", "FM", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Macht seinen Seufzern Platz! die hemmt kein Feind.", "tokens": ["Macht", "sei\u00b7nen", "Seuf\u00b7zern", "Platz", "!", "die", "hemmt", "kein", "Feind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$.", "ART", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Wurm kann sich im Tode kr\u00fcmmen, winden,", "tokens": ["Der", "Wurm", "kann", "sich", "im", "To\u00b7de", "kr\u00fcm\u00b7men", ",", "win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPRART", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Lamm fleht seinen M\u00f6rder an;", "tokens": ["Das", "Lamm", "fleht", "sei\u00b7nen", "M\u00f6r\u00b7der", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und einen, Euren S\u00e4ugling la\u00dft Ihr binden,", "tokens": ["Und", "ei\u00b7nen", ",", "Eu\u00b7ren", "S\u00e4ug\u00b7ling", "la\u00dft", "Ihr", "bin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Kaum da\u00df er seufzen kann!", "tokens": ["Kaum", "da\u00df", "er", "seuf\u00b7zen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O Weltank\u00f6mmling, Deinen zarten H\u00e4nden", "tokens": ["O", "Wel\u00b7tan\u00b7k\u00f6mm\u00b7ling", ",", "Dei\u00b7nen", "zar\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Pr\u00e4gt dieses Band elende Knechtschaft ein;", "tokens": ["Pr\u00e4gt", "die\u00b7ses", "Band", "e\u00b7len\u00b7de", "Knecht\u00b7schaft", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um Deinen Gang von Sarg zu Sarg zu enden,", "tokens": ["Um", "Dei\u00b7nen", "Gang", "von", "Sarg", "zu", "Sarg", "zu", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00dft Du der Sklaven ew'ger Sklave sein.", "tokens": ["Mu\u00dft", "Du", "der", "Skla\u00b7ven", "ew'\u00b7ger", "Skla\u00b7ve", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dies Trauerlied war's, das im Weben", "tokens": ["Dies", "Trau\u00b7er\u00b7lied", "wa\u00b7r's", ",", "das", "im", "We\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Parze Deinem Schicksal sang,", "tokens": ["Die", "Par\u00b7ze", "Dei\u00b7nem", "Schick\u00b7sal", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da sie Dein Band zum Leben", "tokens": ["Da", "sie", "Dein", "Band", "zum", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als Kette um Dich schlang.", "tokens": ["Als", "Ket\u00b7te", "um", "Dich", "schlang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O, wenn Du einst im Chaos von Ideen", "tokens": ["O", ",", "wenn", "Du", "einst", "im", "Chaos", "von", "I\u00b7deen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Arbeitest, Fesseln f\u00fchlst und aufw\u00e4rts ringst,", "tokens": ["Ar\u00b7bei\u00b7test", ",", "Fes\u00b7seln", "f\u00fchlst", "und", "auf\u00b7w\u00e4rts", "ringst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Wenn Du schiffbr\u00fcchig einst, um Tag zu sehen,", "tokens": ["Wenn", "Du", "schiff\u00b7br\u00fc\u00b7chig", "einst", ",", "um", "Tag", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "$,", "KOUI", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Abgrund, wie im Eisenpanzer, dringst,", "tokens": ["Vom", "Ab\u00b7grund", ",", "wie", "im", "Ei\u00b7sen\u00b7pan\u00b7zer", ",", "dringst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPRART", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Einst schaust nach neuen Aethersbahnen,", "tokens": ["Einst", "schaust", "nach", "neu\u00b7en", "A\u00b7e\u00b7thers\u00b7bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wie Sonnenpferde rasch, Du einst", "tokens": ["Wie", "Son\u00b7nen\u00b7pfer\u00b7de", "rasch", ",", "Du", "einst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "ADJD", "$,", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nach neuen Unterthanen", "tokens": ["Nach", "neu\u00b7en", "Un\u00b7ter\u00b7tha\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Blut mit Heldenthr\u00e4nen weinst \u2013", "tokens": ["Blut", "mit", "Hel\u00b7dent\u00b7hr\u00e4\u00b7nen", "weinst", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nicht weibisch Wasser; wenn aus Deiner Seele", "tokens": ["Nicht", "wei\u00b7bisch", "Was\u00b7ser", ";", "wenn", "aus", "Dei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "NN", "$.", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir in die Fl\u00fcgel st\u00fcrmet Adlerswuth,", "tokens": ["Dir", "in", "die", "Fl\u00fc\u00b7gel", "st\u00fcr\u00b7met", "Ad\u00b7lers\u00b7wuth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du w\u00e4gst den schweren Leib im Staub der H\u00f6hle,", "tokens": ["Du", "w\u00e4gst", "den", "schwe\u00b7ren", "Leib", "im", "Staub", "der", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und immer mehr lockt Dich der Sonne Gluth;", "tokens": ["Und", "im\u00b7mer", "mehr", "lockt", "Dich", "der", "Son\u00b7ne", "Gluth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch schon vom ersten Morgensterne", "tokens": ["Doch", "schon", "vom", "ers\u00b7ten", "Mor\u00b7gens\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ermattet, blickst zur Tiefe Du herab", "tokens": ["Er\u00b7mat\u00b7tet", ",", "blickst", "zur", "Tie\u00b7fe", "Du", "her\u00b7ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "APPRART", "NN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und schaust in grauer Ferne", "tokens": ["Und", "schaust", "in", "grau\u00b7er", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Erdball, der Dich gab:", "tokens": ["Den", "Erd\u00b7ball", ",", "der", "Dich", "gab", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dann pocht Dein Herz, da\u00df, die auf Erden wohnen,", "tokens": ["Dann", "pocht", "Dein", "Herz", ",", "da\u00df", ",", "die", "auf", "Er\u00b7den", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu Staub geboren sind, zu Finsterni\u00df.", "tokens": ["Zu", "Staub", "ge\u00b7bo\u00b7ren", "sind", ",", "zu", "Fins\u00b7ter\u00b7ni\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vielleicht erdr\u00fcckte Dir Gedankenmillionen", "tokens": ["Viel\u00b7leicht", "er\u00b7dr\u00fcck\u00b7te", "Dir", "Ge\u00b7dan\u00b7ken\u00b7mil\u00b7li\u00b7o\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der erste Griff, der Dich zum Lichte ri\u00df;", "tokens": ["Der", "ers\u00b7te", "Griff", ",", "der", "Dich", "zum", "Lich\u00b7te", "ri\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der erste Zug aus Mutterbr\u00fcsten", "tokens": ["Der", "ers\u00b7te", "Zug", "aus", "Mut\u00b7ter\u00b7br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gab Dir vielleicht ein Maa\u00df von Pein,", "tokens": ["Gab", "Dir", "viel\u00b7leicht", "ein", "Maa\u00df", "von", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von tausend schwarzen L\u00fcsten", "tokens": ["Von", "tau\u00b7send", "schwar\u00b7zen", "L\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Gift und Lastern ein;", "tokens": ["Und", "Gift", "und", "Las\u00b7tern", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nein, S\u00e4ugling, Tr\u00e4nk in Deine S\u00e4fte,", "tokens": ["Nein", ",", "S\u00e4ug\u00b7ling", ",", "Tr\u00e4nk", "in", "Dei\u00b7ne", "S\u00e4f\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ruh in Dein Herz und Seele ins Gehirn.", "tokens": ["Ruh", "in", "Dein", "Herz", "und", "See\u00b7le", "ins", "Ge\u00b7hirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Stets laben Dich mit Milch der Tugend Kr\u00e4fte;", "tokens": ["Stets", "la\u00b7ben", "Dich", "mit", "Milch", "der", "Tu\u00b7gend", "Kr\u00e4f\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stets lache so, wie jetzo, Deine Stirn;", "tokens": ["Stets", "la\u00b7che", "so", ",", "wie", "jet\u00b7zo", ",", "Dei\u00b7ne", "Stirn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PWAV", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nie sprech' Dein Vater aus Erb