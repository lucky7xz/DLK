{"dta.poem.13835": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die ernsthafte Fastnacht  1814.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00e4ger! la\u00dft die H\u00f6rner klingen", "tokens": ["J\u00e4\u00b7ger", "!", "la\u00dft", "die", "H\u00f6r\u00b7ner", "klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Morgen kalt und blank!", "tokens": ["Durch", "den", "Mor\u00b7gen", "kalt", "und", "blank", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl, sie l\u00e4\u00dft sich noch bezwingen,", "tokens": ["Wohl", ",", "sie", "l\u00e4\u00dft", "sich", "noch", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt sie alten deutschen Klang.", "tokens": ["H\u00f6rt", "sie", "al\u00b7ten", "deut\u00b7schen", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}