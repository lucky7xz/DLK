{"textgrid.poem.24671": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jede Seele, sie durchwandelt der Gesch\u00f6pfe Stufenleiter:", "tokens": ["Je\u00b7de", "See\u00b7le", ",", "sie", "durch\u00b7wan\u00b7delt", "der", "Ge\u00b7sch\u00f6p\u00b7fe", "Stu\u00b7fen\u00b7lei\u00b7ter", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Formentauschend, rein und reiner, immer h\u00f6her, hell und heiter,", "tokens": ["For\u00b7men\u00b7tau\u00b7schend", ",", "rein", "und", "rei\u00b7ner", ",", "im\u00b7mer", "h\u00f6\u00b7her", ",", "hell", "und", "hei\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KON", "ADJA", "$,", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Lebt sie fort im Wurm, im Frosche, im Vampir, im niedern Sklaven,", "tokens": ["Lebt", "sie", "fort", "im", "Wurm", ",", "im", "Fro\u00b7sche", ",", "im", "Vam\u00b7pir", ",", "im", "nie\u00b7dern", "Skla\u00b7ven", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Dann im T\u00e4nzer, im Poet, im Trunkenbold, im edlen Streiter ...", "tokens": ["Dann", "im", "T\u00e4n\u00b7zer", ",", "im", "Po\u00b7et", ",", "im", "Trun\u00b7ken\u00b7bold", ",", "im", "ed\u00b7len", "Strei\u00b7ter", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "ADJA", "NN", "$("], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Sehet: eine gleiche Reihe Seelenh\u00fcllen, Truggestalten", "tokens": ["Se\u00b7het", ":", "ei\u00b7ne", "glei\u00b7che", "Rei\u00b7he", "See\u00b7len\u00b7h\u00fcl\u00b7len", ",", "Trug\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$.", "ART", "ADJA", "NN", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Mu\u00df der Dichtergeist durchwandeln, stets verkl\u00e4rter, stets befreiter:", "tokens": ["Mu\u00df", "der", "Dich\u00b7ter\u00b7geist", "durch\u00b7wan\u00b7deln", ",", "stets", "ver\u00b7kl\u00e4r\u00b7ter", ",", "stets", "be\u00b7frei\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Und er war im Werden Gaukler, war Vampir und war Brahmane,", "tokens": ["Und", "er", "war", "im", "Wer\u00b7den", "Gauk\u00b7ler", ",", "war", "Vam\u00b7pir", "und", "war", "Brah\u00b7ma\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,", "VAFIN", "NE", "KON", "VAFIN", "NE", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Leere Formen l\u00e4\u00dft er leblos 