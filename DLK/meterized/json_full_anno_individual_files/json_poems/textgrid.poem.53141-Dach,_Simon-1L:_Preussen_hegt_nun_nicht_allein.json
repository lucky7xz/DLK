{"textgrid.poem.53141": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Preussen hegt nun nicht allein", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Preussen hegt nun nicht allein", "tokens": ["Preus\u00b7sen", "hegt", "nun", "nicht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PTKNEG", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Demant-Stein',", "tokens": ["De\u00b7mant\u00b7Stein'", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Vnl\u00e4ngst hat zu gutter Stunden", "tokens": ["Vn\u00b7l\u00e4ngst", "hat", "zu", "gut\u00b7ter", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein erhitzter Tugend-", "tokens": ["Ein", "er\u00b7hitz\u00b7ter", "Tu\u00b7gen\u00b7d"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "TRUNC"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie bekandt", "tokens": ["Wie", "be\u00b7kandt"], "token_info": ["word", "word"], "pos": ["PWAV", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Eine ", "tokens": ["Ei\u00b7ne"], "token_info": ["word"], "pos": ["ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Ihre Pflantz ist jmmerdar", "tokens": ["Ih\u00b7re", "Pflantz", "ist", "jm\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kalckstein zwar,", "tokens": ["Kalck\u00b7stein", "zwar", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADV", "$,"], "meter": "++-", "measure": "unknown.measure.di"}, "line.3": {"text": "Ihm nur ist Sie Alabaster:", "tokens": ["Ihm", "nur", "ist", "Sie", "A\u00b7la\u00b7bas\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gebt Ihm aber rund vnd schlecht'", "tokens": ["Gebt", "Ihm", "a\u00b7ber", "rund", "vnd", "schlecht'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hierinn recht,", "tokens": ["Hie\u00b7rinn", "recht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Also jrren ist kein Laster.", "tokens": ["Al\u00b7so", "jr\u00b7ren", "ist", "kein", "Las\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hieraus, spricht Er, h\u00e4tte ba\u00df", "tokens": ["Hier\u00b7aus", ",", "spricht", "Er", ",", "h\u00e4t\u00b7te", "ba\u00df"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PAV", "$,", "VVFIN", "PPER", "$,", "VAFIN", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Phidias", "tokens": ["Phi\u00b7di\u00b7as"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Selbs Minerven Bild gehawen,", "tokens": ["Selbs", "Mi\u00b7ner\u00b7ven", "Bild", "ge\u00b7ha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Venus ist nach langem Flei\u00df", "tokens": ["Ve\u00b7nus", "ist", "nach", "lan\u00b7gem", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also wei\u00df", "tokens": ["Al\u00b7so", "wei\u00df"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Vnd so Purpur-roht zu schawen.", "tokens": ["Vnd", "so", "Pur\u00b7pur\u00b7roht", "zu", "scha\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+---", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Aber nach der Sternen Schlu\u00df", "tokens": ["A\u00b7ber", "nach", "der", "Ster\u00b7nen", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sol vnd mu\u00df", "tokens": ["Sol", "vnd", "mu\u00df"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "KON", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Diese Kalck-Pflantz in das Fewer.", "tokens": ["Die\u00b7se", "Kal\u00b7ck\u00b7Pflantz", "in", "das", "Fe\u00b7wer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Was thut Brandt? er widerstrebt", "tokens": ["Was", "thut", "Brandt", "?", "er", "wi\u00b7der\u00b7strebt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd erbebt", "tokens": ["Vnd", "er\u00b7bebt"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Vber diesem Vngehewer.", "tokens": ["Vber", "die\u00b7sem", "Vn\u00b7ge\u00b7he\u00b7wer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Zwar ich geb', o Himmel, stat", "tokens": ["Zwar", "ich", "geb'", ",", "o", "Him\u00b7mel", ",", "stat"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "FM", "NN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinem Raht,", "tokens": ["Dei\u00b7nem", "Raht", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Spricht Er, doch mit dem Bescheide,", "tokens": ["Spricht", "Er", ",", "doch", "mit", "dem", "Be\u00b7schei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df Ihr mein Brandt m\u00f6g' allein", "tokens": ["Da\u00df", "Ihr", "mein", "Brandt", "m\u00f6g'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VMFIN", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Dienlich seyn", "tokens": ["Dien\u00b7lich", "seyn"], "token_info": ["word", "word"], "pos": ["ADJD", "VAINF"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Vnd Sie sonst kein Fewer leide.", "tokens": ["Vnd", "Sie", "sonst", "kein", "Fe\u00b7wer", "lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aetna w\u00fctet selbs nicht so,", "tokens": ["A\u00b7et\u00b7na", "w\u00fc\u00b7tet", "selbs", "nicht", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Als die Loh,", "tokens": ["Als", "die", "Loh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Welche schl\u00e4gt aus meinem Hertzen,", "tokens": ["Wel\u00b7che", "schl\u00e4gt", "aus", "mei\u00b7nem", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Glut hatte nicht das Schlo\u00df", "tokens": ["Die", "Glut", "hat\u00b7te", "nicht", "das", "Schlo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Pergamos,", "tokens": ["Per\u00b7ga\u00b7mos", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Brandt es gleich von tausent Kertzen.", "tokens": ["Brandt", "es", "gleich", "von", "tau\u00b7sent", "Kert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kalckstein-\u00f6fen sind, ich weis,", "tokens": ["Kalck\u00b7stein\u00b7\u00f6\u00b7fen", "sind", ",", "ich", "weis", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so heis,", "tokens": ["Nicht", "so", "heis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Lasst Sie meine Hitz' erst f\u00fchlen,", "tokens": ["Lasst", "Sie", "mei\u00b7ne", "Hitz'", "erst", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie wird Ihr sehn alle Krafft", "tokens": ["Sie", "wird", "Ihr", "sehn", "al\u00b7le", "Krafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Weggerafft", "tokens": ["Weg\u00b7ge\u00b7rafft"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Vnd sich w\u00fcnschen offt zu k\u00fchlen.", "tokens": ["Vnd", "sich", "w\u00fcn\u00b7schen", "offt", "zu", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Seine Bitte wird begn\u00fcgt,", "tokens": ["Sei\u00b7ne", "Bit\u00b7te", "wird", "be\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Darumb liegt", "tokens": ["Da\u00b7rumb", "liegt"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Sie in seinen Hertzens-Flammen,", "tokens": ["Sie", "in", "sei\u00b7nen", "Hert\u00b7zens\u00b7Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er legt eifrig vnd ohn Rhue", "tokens": ["Er", "legt", "eif\u00b7rig", "vnd", "ohn", "Rhue"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "APPR", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Fewer zu,", "tokens": ["Fe\u00b7wer", "zu", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Das schl\u00e4gt \u00fcber Sie zusammen.", "tokens": ["Das", "schl\u00e4gt", "\u00fc\u00b7ber", "Sie", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Sonst, nimmt wo der wilde Brandt", "tokens": ["Sonst", ",", "nimmt", "wo", "der", "wil\u00b7de", "Brandt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PWAV", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vberhand,", "tokens": ["Vber\u00b7hand", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da ist grosse Noht zugegen:", "tokens": ["Da", "ist", "gros\u00b7se", "Noht", "zu\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Brandt wird nur allein", "tokens": ["Die\u00b7ser", "Brandt", "wird", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dienlich seyn", "tokens": ["Dien\u00b7lich", "seyn"], "token_info": ["word", "word"], "pos": ["ADJD", "VAINF"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Vnd viel tausent Lust erregen.", "tokens": ["Vnd", "viel", "tau\u00b7sent", "Lust", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Kalck ist alsobald nicht gut", "tokens": ["Kalck", "ist", "al\u00b7so\u00b7bald", "nicht", "gut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "PTKNEG", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der Glut,", "tokens": ["Nach", "der", "Glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Er mu\u00df erst geleschet werden,", "tokens": ["Er", "mu\u00df", "erst", "ge\u00b7le\u00b7schet", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Brennt vnd lescht, bawt mit der Zeit", "tokens": ["Brennt", "vnd", "lescht", ",", "bawt", "mit", "der", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Beyderseit", "tokens": ["Bey\u00b7der\u00b7seit"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Tugend-Tempel nur auff Erden!", "tokens": ["Tu\u00b7gen\u00b7dTem\u00b7pel", "nur", "auff", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Preussen hegt nun nicht allein", "tokens": ["Preus\u00b7sen", "hegt", "nun", "nicht", "al\