{"dta.poem.6124": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Zeug, der fl\u00fc\u00dfig ist, worinn die Erde geht,", "tokens": ["Der", "Zeug", ",", "der", "fl\u00fc\u00b7\u00dfig", "ist", ",", "wo\u00b7rinn", "die", "Er\u00b7de", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird in ovalem Creis in h\u00f6chster Eil gedreht.", "tokens": ["Wird", "in", "o\u00b7va\u00b7lem", "Creis", "in", "h\u00f6chs\u00b7ter", "Eil", "ge\u00b7dreht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Mond, der auch darinn sich reget,", "tokens": ["Der", "Mond", ",", "der", "auch", "da\u00b7rinn", "sich", "re\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PAV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird dennoch nicht so schnell beweget,", "tokens": ["Wird", "den\u00b7noch", "nicht", "so", "schnell", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Beym kleinsten Durchschnitt nun, wo sich der Weg verengt,", "tokens": ["Beym", "kleins\u00b7ten", "Durch\u00b7schnitt", "nun", ",", "wo", "sich", "der", "Weg", "ver\u00b7engt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist, durch die Festigkeit des Mondes, dieser Flu\u00df", "tokens": ["Ist", ",", "durch", "die", "Fes\u00b7tig\u00b7keit", "des", "Mon\u00b7des", ",", "die\u00b7ser", "Flu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gehemmet und gedr\u00e4ngt:", "tokens": ["Ge\u00b7hem\u00b7met", "und", "ge\u00b7dr\u00e4ngt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Durch welchen Sto\u00df die Erde dann, ger\u00fccket,", "tokens": ["Durch", "wel\u00b7chen", "Sto\u00df", "die", "Er\u00b7de", "dann", ",", "ge\u00b7r\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Aus ihrer Lag\u2019 in etwas weichen mu\u00df.", "tokens": ["Aus", "ih\u00b7rer", "Lag'", "in", "et\u00b7was", "wei\u00b7chen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Dahin, wo dieser Weg dadurch zugleich gedr\u00fccket,", "tokens": ["Da\u00b7hin", ",", "wo", "die\u00b7ser", "Weg", "da\u00b7durch", "zu\u00b7gleich", "ge\u00b7dr\u00fc\u00b7cket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "PDAT", "NN", "PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Macht, da\u00df der Lufft-Flu\u00df auch daselbst gepresset wird.", "tokens": ["Macht", ",", "da\u00df", "der", "Lufft\u00b7Flu\u00df", "auch", "da\u00b7selbst", "ge\u00b7pres\u00b7set", "wird", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ADV", "PAV", "VVPP", "VAFIN", "$."], "meter": "+--++--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Die Wasser, so allda, beym doppeln Punct der Erden,", "tokens": ["Die", "Was\u00b7ser", ",", "so", "all\u00b7da", ",", "beym", "dop\u00b7peln", "Punct", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Gedr\u00fccket und gedr\u00e4nget werden;", "tokens": ["Ge\u00b7dr\u00fc\u00b7cket", "und", "ge\u00b7dr\u00e4n\u00b7get", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Sind an dem Ort, woselbst der Circkel ist gezogen,", "tokens": ["Sind", "an", "dem", "Ort", ",", "wo\u00b7selbst", "der", "Cir\u00b7ckel", "ist", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,", "PWAV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Tag und Nacht gleich macht, gebogen,", "tokens": ["Der", "Tag", "und", "Nacht", "gleich", "macht", ",", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und in der Mitten gleichsam hol:", "tokens": ["Und", "in", "der", "Mit\u00b7ten", "gleich\u00b7sam", "hol", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wodurch im Gegentheil bey einem jeden Pol", "tokens": ["Wo\u00b7durch", "im", "Ge\u00b7gen\u00b7theil", "bey", "ei\u00b7nem", "je\u00b7den", "Pol"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "APPR", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Fluhten sich erh\u00f6hn,", "tokens": ["Die", "Fluh\u00b7ten", "sich", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Und \u00fcber ihre Ufer gehn.", "tokens": ["Und", "\u00fc\u00b7ber", "ih\u00b7re", "U\u00b7fer", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}