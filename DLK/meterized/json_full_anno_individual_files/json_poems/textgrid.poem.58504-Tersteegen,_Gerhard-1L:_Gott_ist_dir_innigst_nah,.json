{"textgrid.poem.58504": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gott ist dir innigst nah,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott ist dir innigst nah,", "tokens": ["Gott", "ist", "dir", "in\u00b7nigst", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Such doch nicht in der Ferne!", "tokens": ["Such", "doch", "nicht", "in", "der", "Fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Gott ist ein Herzensgott,", "tokens": ["Gott", "ist", "ein", "Her\u00b7zens\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Gibt sich dem Herzen gerne;", "tokens": ["Gibt", "sich", "dem", "Her\u00b7zen", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kehr um und kehre ein,", "tokens": ["Kehr", "um", "und", "keh\u00b7re", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Er ist dir g'nug allein!", "tokens": ["Er", "ist", "dir", "g'\u00b7nug", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Mit dir, mit deinen Sachen", "tokens": ["Mit", "dir", ",", "mit", "dei\u00b7nen", "Sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "La\u00df ihn freim\u00e4chtig machen,", "tokens": ["La\u00df", "ihn", "frei\u00b7m\u00e4ch\u00b7tig", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Was du machst, ihm gescheh,", "tokens": ["Was", "du", "machst", ",", "ihm", "ge\u00b7scheh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Nicht dich noch Werk beseh!", "tokens": ["Nicht", "dich", "noch", "Werk", "be\u00b7seh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gott ist dir innigst nah,", "tokens": ["Gott", "ist", "dir", "in\u00b7nigst", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Such doch nicht in der Ferne!", "tokens": ["Such", "doch", "nicht", "in", "der", "Fer\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Gott ist ein Herzensgott,", "tokens": ["Gott", "ist", "ein", "Her\u00b7zens\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measur