{"textgrid.poem.64107": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der letzte der Kimbern", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hei\u00df hat die Julisonne gebrannt", "tokens": ["Wie", "hei\u00df", "hat", "die", "Ju\u00b7li\u00b7son\u00b7ne", "ge\u00b7brannt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf der raudischen Felder st\u00e4ubenden Sand!", "tokens": ["Auf", "der", "rau\u00b7di\u00b7schen", "Fel\u00b7der", "st\u00e4u\u00b7ben\u00b7den", "Sand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da sind sie erlegen, die Nordlandh\u00fcnen:", "tokens": ["Da", "sind", "sie", "er\u00b7le\u00b7gen", ",", "die", "Nord\u00b7land\u00b7h\u00fc\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht frommte die riesige Kraft den K\u00fchnen:", "tokens": ["Nicht", "fromm\u00b7te", "die", "rie\u00b7si\u00b7ge", "Kraft", "den", "K\u00fch\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Zu hei\u00df die Hitze, zu dunstig der Dunst,", "tokens": ["Zu", "hei\u00df", "die", "Hit\u00b7ze", ",", "zu", "duns\u00b7tig", "der", "Dunst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NN", "$,", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zu lauernd des Marius Feldherrnkunst!", "tokens": ["Zu", "lau\u00b7ernd", "des", "Ma\u00b7rius", "Feld\u00b7herrn\u00b7kunst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "NE", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Von allen Seiten umgarnt der Keil: \u2013", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten", "um\u00b7garnt", "der", "Keil", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "ART", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da verfehlt des gedr\u00e4ngten Gew\u00fchls kein Pfeil:", "tokens": ["Da", "ver\u00b7fehlt", "des", "ge\u00b7dr\u00e4ng\u00b7ten", "Ge\u00b7w\u00fchls", "kein", "Pfeil", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Von Kohorten umfa\u00dft wie von ehernen Zangen,", "tokens": ["Von", "Ko\u00b7hor\u00b7ten", "um\u00b7fa\u00dft", "wie", "von", "e\u00b7her\u00b7nen", "Zan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Wie so grimmig die sieglosen Recken rangen!", "tokens": ["Wie", "so", "grim\u00b7mig", "die", "sieg\u00b7lo\u00b7sen", "Re\u00b7cken", "ran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Erst fielen die Vordersten, wie sie gestanden,", "tokens": ["Erst", "fie\u00b7len", "die", "Vor\u00b7ders\u00b7ten", ",", "wie", "sie", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+--+---+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die mit Ketten die G\u00fcrtel zusammen banden:", "tokens": ["Die", "mit", "Ket\u00b7ten", "die", "G\u00fcr\u00b7tel", "zu\u00b7sam\u00b7men", "ban\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und \u00fcber sie hin die numidischen Rosse!", "tokens": ["Und", "\u00fc\u00b7ber", "sie", "hin", "die", "nu\u00b7mi\u00b7di\u00b7schen", "Ros\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In die nackten Leiber der Braus der Geschosse!", "tokens": ["In", "die", "nack\u00b7ten", "Lei\u00b7ber", "der", "Braus", "der", "Ge\u00b7schos\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Da ist vor der Glut der Mittagssonnen", "tokens": ["Da", "ist", "vor", "der", "Glut", "der", "Mit\u00b7tags\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "In Schwei\u00df und in Blut ihre Kraft zerronnen,", "tokens": ["In", "Schwei\u00df", "und", "in", "Blut", "ih\u00b7re", "Kraft", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und Tausende mehr sind erstickt und verschmachtet,", "tokens": ["Und", "Tau\u00b7sen\u00b7de", "mehr", "sind", "er\u00b7stickt", "und", "ver\u00b7schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Als das breite Schwert der Legionen geschlachtet.", "tokens": ["Als", "das", "brei\u00b7te", "Schwert", "der", "Le\u00b7gi\u00b7o\u00b7nen", "ge\u00b7schlach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.4": {"line.1": {"text": "Nun ragt aus dem rings umbrandenden Sturm", "tokens": ["Nun", "ragt", "aus", "dem", "rings", "um\u00b7bran\u00b7den\u00b7den", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch Einer: ein letzter einsamer Turm.", "tokens": ["Noch", "Ei\u00b7ner", ":", "ein", "letz\u00b7ter", "ein\u00b7sa\u00b7mer", "Turm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$.", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Zur\u00fcck an die Burg der Wagen gedr\u00e4ngt,", "tokens": ["Zu\u00b7r\u00fcck", "an", "die", "Burg", "der", "Wa\u00b7gen", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Geschossen und Rossen und Speeren umengt,", "tokens": ["Von", "Ge\u00b7schos\u00b7sen", "und", "Ros\u00b7sen", "und", "Spee\u00b7ren", "u\u00b7mengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Das helmlose Haupt von den roten Locken", "tokens": ["Das", "helm\u00b7lo\u00b7se", "Haupt", "von", "den", "ro\u00b7ten", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Umwogt wie von lohenden Feuerflocken:", "tokens": ["Um\u00b7wogt", "wie", "von", "lo\u00b7hen\u00b7den", "Feu\u00b7er\u00b7flo\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Held Boiorich ist's, der Kimbernk\u00f6nig,", "tokens": ["Held", "Boi\u00b7o\u00b7rich", "ist's", ",", "der", "Kim\u00b7bern\u00b7k\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der zum Zweikampf Marius gefordert hat.", "tokens": ["Der", "zum", "Zwei\u00b7kampf", "Ma\u00b7rius", "ge\u00b7for\u00b7dert", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "NE", "VVPP", "VAFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Doch eisig erwiderte der und h\u00f6hnig:", "tokens": ["Doch", "ei\u00b7sig", "er\u00b7wi\u00b7der\u00b7te", "der", "und", "h\u00f6h\u00b7nig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "KON", "ADJD", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "\u00bbei, wenn der Barbar des Lebens satt,", "tokens": ["\u00bb", "ei", ",", "wenn", "der", "Bar\u00b7bar", "des", "Le\u00b7bens", "satt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "KOUS", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.11": {"text": "So komm' er morgen aufs raudische Feld:", "tokens": ["So", "komm'", "er", "mor\u00b7gen", "aufs", "rau\u00b7di\u00b7sche", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Dort wird er vor Abend den Schatten gesellt.\u00ab", "tokens": ["Dort", "wird", "er", "vor", "A\u00b7bend", "den", "Schat\u00b7ten", "ge\u00b7sellt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Noch trotzt er, wie der umstellte B\u00e4r:", "tokens": ["Noch", "trotzt", "er", ",", "wie", "der", "um\u00b7stell\u00b7te", "B\u00e4r", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rings um ihn die r\u00f6mische Meute her.", "tokens": ["Rings", "um", "ihn", "die", "r\u00f6\u00b7mi\u00b7sche", "Meu\u00b7te", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Marius ruft aus der Ferne vom Ro\u00df:", "tokens": ["Und", "Ma\u00b7rius", "ruft", "aus", "der", "Fer\u00b7ne", "vom", "Ro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00bbhier, Legionare! Hieher! Auf diesen!", "tokens": ["\u00bb", "hier", ",", "Le\u00b7gi\u00b7o\u00b7na\u00b7re", "!", "Hie\u00b7her", "!", "Auf", "die\u00b7sen", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$.", "ADV", "$.", "APPR", "PDAT", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Doch verletzt ihn nicht mit Speer und Gescho\u00df:", "tokens": ["Doch", "ver\u00b7letzt", "ihn", "nicht", "mit", "Speer", "und", "Ge\u00b7scho\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "Lebendig, gebunden, bringt mir den Riesen,", "tokens": ["Le\u00b7ben\u00b7dig", ",", "ge\u00b7bun\u00b7den", ",", "bringt", "mir", "den", "Rie\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+---+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Der schm\u00fcckt wie kein andrer mir den Triumph!\u00ab", "tokens": ["Der", "schm\u00fcckt", "wie", "kein", "an\u00b7drer", "mir", "den", "Tri\u00b7umph", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "PIAT", "ADJA", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch mit des zerbrochenen Langschwerts Stumpf", "tokens": ["Doch", "mit", "des", "zer\u00b7bro\u00b7che\u00b7nen", "Lang\u00b7schwerts", "Stumpf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "----+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Gewaltige w\u00fctet in solchen Streichen, \u2013", "tokens": ["Der", "Ge\u00b7wal\u00b7ti\u00b7ge", "w\u00fc\u00b7tet", "in", "sol\u00b7chen", "Strei\u00b7chen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,", "$("], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ihn vermag kein R\u00f6mergriff zu erreichen,", "tokens": ["Ihn", "ver\u00b7mag", "kein", "R\u00f6\u00b7mer\u00b7griff", "zu", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und sie schauen mit Grausen der Ihrigen Leichen", "tokens": ["Und", "sie", "schau\u00b7en", "mit", "Grau\u00b7sen", "der", "Ih\u00b7ri\u00b7gen", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "ART", "PPOSAT", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Hochum geh\u00e4uft. Wie, entbl\u00f6\u00dft des Schildes,", "tokens": ["Hoch\u00b7um", "ge\u00b7h\u00e4uft", ".", "Wie", ",", "ent\u00b7bl\u00f6\u00dft", "des", "Schil\u00b7des", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "PWAV", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die breite Brust nach dem Tode begehrt! \u2013", "tokens": ["Die", "brei\u00b7te", "Brust", "nach", "dem", "To\u00b7de", "be\u00b7gehrt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da zuckt von unten ein t\u00fcckisches Schwert:", "tokens": ["Da", "zuckt", "von", "un\u00b7ten", "ein", "t\u00fc\u00b7cki\u00b7sches", "Schwert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbwillkommen, ihr Wonnen des Walhallgefildes!\u00ab", "tokens": ["\u00bb", "will\u00b7kom\u00b7men", ",", "ihr", "Won\u00b7nen", "des", "Wal\u00b7hall\u00b7ge\u00b7fil\u00b7des", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "PPOSAT", "NN", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Er ruft's und stirbt im Stehen: der Wall", "tokens": ["Er", "ruft's", "und", "stirbt", "im", "Ste\u00b7hen", ":", "der", "Wall"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "li