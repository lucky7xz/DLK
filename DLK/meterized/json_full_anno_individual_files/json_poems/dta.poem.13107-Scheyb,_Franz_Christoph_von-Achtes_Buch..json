{"dta.poem.13107": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ehieraus versteht ihr wohl, da\u00df es der P\u00f6bel ist,", "tokens": ["\u201e", "hier\u00b7aus", "ver\u00b7steht", "ihr", "wohl", ",", "da\u00df", "es", "der", "P\u00f6\u00b7bel", "ist", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201eder nichts, als nach dem Wahn der Tadelsucht, ermi\u00dft.", "tokens": ["\u201e", "der", "nichts", ",", "als", "nach", "dem", "Wahn", "der", "Ta\u00b7del\u00b7sucht", ",", "er\u00b7mi\u00dft", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "PIS", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eer kennt nicht, oder hart, was eine Wahrheit seye;", "tokens": ["\u201e", "er", "kennt", "nicht", ",", "o\u00b7der", "hart", ",", "was", "ei\u00b7ne", "Wahr\u00b7heit", "se\u00b7ye", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "KON", "ADJD", "$,", "PRELS", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "\u201ebesonders wann ich sie mit meinem Schmuck bestreue,", "tokens": ["\u201e", "be\u00b7son\u00b7ders", "wann", "ich", "sie", "mit", "mei\u00b7nem", "Schmuck", "be\u00b7streu\u00b7e", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PWAV", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eum durch das scharffe Licht, so dessen Augen fliehn,", "tokens": ["\u201e", "um", "durch", "das", "scharf\u00b7fe", "Licht", ",", "so", "des\u00b7sen", "Au\u00b7gen", "fliehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUI", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "410\u201dDer Mi\u00dfgunst nicht zu schnell die Larve wegzuziehn;", "tokens": ["\"", "Der", "Mi\u00df\u00b7gunst", "nicht", "zu", "schnell", "die", "Lar\u00b7ve", "weg\u00b7zu\u00b7ziehn", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eweil ich Verdru\u00df und Zorn in solchem Aug erweckte,", "tokens": ["\u201e", "weil", "ich", "Ver\u00b7dru\u00df", "und", "Zorn", "in", "sol\u00b7chem", "Aug", "er\u00b7weck\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewann ich der Wahrheit Glanz in vollem Schein entdeckte.", "tokens": ["\u201e", "wann", "ich", "der", "Wahr\u00b7heit", "Glanz", "in", "vol\u00b7lem", "Schein", "ent\u00b7deck\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eer wei\u00df nicht, wie sie sich in alle Sachen dringt;", "tokens": ["\u201e", "er", "wei\u00df", "nicht", ",", "wie", "sie", "sich", "in", "al\u00b7le", "Sa\u00b7chen", "dringt", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewas Wohlgefallen sie dem Wissens-Eifer bringt.", "tokens": ["\u201e", "was", "Wohl\u00b7ge\u00b7fal\u00b7len", "sie", "dem", "Wis\u00b7sens\u00b7Ei\u00b7fer", "bringt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "415\u201dWie k\u00f6nnte dann sein Geist, sein tr\u00fcbes Auge sehen", "tokens": ["\"", "Wie", "k\u00f6nn\u00b7te", "dann", "sein", "Geist", ",", "sein", "tr\u00fc\u00b7bes", "Au\u00b7ge", "se\u00b7hen"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "VMFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201edie Wahrheit im Gedicht und in der Fabel stehen?", "tokens": ["\u201e", "die", "Wahr\u00b7heit", "im", "Ge\u00b7dicht", "und", "in", "der", "Fa\u00b7bel", "ste\u00b7hen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eer, welcher kaum vermerckt, was ihm vor Augen schwebt,", "tokens": ["\u201e", "er", ",", "wel\u00b7cher", "kaum", "ver\u00b7merckt", ",", "was", "ihm", "vor", "Au\u00b7gen", "schwebt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PRELS", "ADV", "VVPP", "$,", "PWS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201enur Abentheuern nach, und nach Gespenstern strebt;", "tokens": ["\u201e", "nur", "A\u00b7bent\u00b7heu\u00b7ern", "nach", ",", "und", "nach", "Ge\u00b7spens\u00b7tern", "strebt", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "PTKVZ", "$,", "KON", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201edurch die Verwunderung Unwissenheit erkl\u00e4ret,", "tokens": ["\u201e", "durch", "die", "Ver\u00b7wun\u00b7de\u00b7rung", "Un\u00b7wis\u00b7sen\u00b7heit", "er\u00b7kl\u00e4\u00b7ret", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "420\u201dDurch die Leichtgl\u00e4ubigkeit dieselbige vermehret:", "tokens": ["\"", "Durch", "die", "Leicht\u00b7gl\u00e4u\u00b7big\u00b7keit", "die\u00b7sel\u00b7bi\u00b7ge", "ver\u00b7meh\u00b7ret", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "PDS", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "\u201eer, welcher mich veracht und ungl\u00fcckseelig nennt,", "tokens": ["\u201e", "er", ",", "wel\u00b7cher", "mich", "ver\u00b7acht", "und", "un\u00b7gl\u00fcck\u00b7see\u00b7lig", "nennt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PRELS", "PPER", "VVFIN", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201eweil er der K\u00fcnste Lust und Treflichkeit nicht kennt,", "tokens": ["\u201e", "weil", "er", "der", "K\u00fcns\u00b7te", "Lust", "und", "Tref\u00b7lich\u00b7keit", "nicht", "kennt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "ADJA", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201everblender und verstockt den hellen Tag verschm\u00e4het,", "tokens": ["\u201e", "ver\u00b7blen\u00b7der", "und", "ver\u00b7stockt", "den", "hel\u00b7len", "Tag", "ver\u00b7schm\u00e4\u00b7het", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "KON", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201ewell seine Bl\u00f6digkeit dadurch verrathen stehet.", "tokens": ["\u201e", "well", "sei\u00b7ne", "Bl\u00f6\u00b7dig\u00b7keit", "da\u00b7durch", "ver\u00b7ra\u00b7then", "ste\u00b7het", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAT", "PPOSAT", "NN", "PAV", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}