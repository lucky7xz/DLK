{"textgrid.poem.62748": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal in ein Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie der Rauch", "tokens": ["Gleich\u00b7wie", "der", "Rauch"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Das Auge blendet,", "tokens": ["Das", "Au\u00b7ge", "blen\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Da\u00df man nicht sehen kan,", "tokens": ["Da\u00df", "man", "nicht", "se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wohin man sich hinwendet.", "tokens": ["Wo\u00b7hin", "man", "sich", "hin\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gleich also ist es auch", "tokens": ["Gleich", "al\u00b7so", "ist", "es", "auch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit einem Mann,", "tokens": ["Mit", "ei\u00b7nem", "Mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der sich vom Zorn besiegen l\u00e4\u00dft.", "tokens": ["Der", "sich", "vom", "Zorn", "be\u00b7sie\u00b7gen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "po