, "Gins\u00b7ter", ",", "an", "dem", "Quell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Horch, wie's da so flink und hell", "tokens": ["Horch", ",", "wie's", "da", "so", "flink", "und", "hell"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf und nieder raschelt!", "tokens": ["Auf", "und", "nie\u00b7der", "ra\u00b7schelt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "PTKVZ", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Halt, Lazertchen, la\u00df mich sehn,", "tokens": ["Halt", ",", "La\u00b7zert\u00b7chen", ",", "la\u00df", "mich", "sehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der Sonnenstrahl so sch\u00f6n", "tokens": ["Wie", "der", "Son\u00b7nen\u00b7strahl", "so", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Spielt auf deinem R\u00fccken!", "tokens": ["Spielt", "auf", "dei\u00b7nem", "R\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Nachtigall ist auch dabei,", "tokens": ["Nach\u00b7ti\u00b7gall", "ist", "auch", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PAV", "$,"], "meter": "+-+-+-+",