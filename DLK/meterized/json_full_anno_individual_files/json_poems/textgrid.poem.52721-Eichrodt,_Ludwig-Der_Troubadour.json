: "unknown.measure.tetra"}, "line.7": {"text": "Mich in Zauberbanden festgehalten.", "tokens": ["Mich", "in", "Zau\u00b7berb\u00b7an\u00b7den", "fest\u00b7ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Liebensw\u00fcrdig waren diese,", "tokens": ["Lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "wa\u00b7ren", "die\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geist- und witzvoll jene sehr,", "tokens": ["Geist", "und", "witz\u00b7voll", "je\u00b7ne", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "VVFIN", "PDS", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich jede Tugend priese,", "tokens": ["Wenn", "ich", "je\u00b7de", "Tu\u00b7gend", "prie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4nd ich keinen Athem mehr;", "tokens": ["F\u00e4nd", "ich", "kei\u00b7nen", "A\u00b7them", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcr die Herrlichen, die g\u00f6ttergleichen,", "tokens": ["F\u00fcr", "die", "Herr\u00b7li\u00b7chen", ",", "die", "g\u00f6t\u00b7ter\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Denen selbst die Stern am Himmel weichen,", "tokens": ["De\u00b7nen", "selbst", "die"