{"textgrid.poem.52226": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Von acht Rossen", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00e4hrt im Land 'ne Staatskarosse;", "tokens": ["F\u00e4hrt", "im", "Land", "'ne", "Staats\u00b7ka\u00b7ros\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehn sie acht famose Rosse,", "tokens": ["Ziehn", "sie", "acht", "fa\u00b7mo\u00b7se", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feurig, ein beherzt Gespann!", "tokens": ["Feu\u00b7rig", ",", "ein", "be\u00b7herzt", "Ge\u00b7spann", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines ward am Rhein geboren,", "tokens": ["Ei\u00b7nes", "ward", "am", "Rhein", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hebt das Haupt und spitzt die Ohren,", "tokens": ["Hebt", "das", "Haupt", "und", "spitzt", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zieht vor allen mutig an.", "tokens": ["Zieht", "vor", "al\u00b7len", "mu\u00b7tig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei\u00dft ein andres in die Stange,", "tokens": ["Bei\u00dft", "ein", "and\u00b7res", "in", "die", "Stan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Fischer mit Gesange", "tokens": ["Wo", "der", "Fi\u00b7scher", "mit", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Froh den goldnen Bernstein fischt;", "tokens": ["Froh", "den", "gold\u00b7nen", "Bern\u00b7stein", "fischt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kr\u00e4ftig schnaubt es mit den N\u00fcstern.", "tokens": ["Kr\u00e4f\u00b7tig", "schnaubt", "es", "mit", "den", "N\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die es lechzend in den d\u00fcstern", "tokens": ["Die", "es", "lech\u00b7zend", "in", "den", "d\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ostseewellen sich erfrischt.", "tokens": ["Ost\u00b7see\u00b7wel\u00b7len", "sich", "er\u00b7frischt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ist das dritte aufgewachsen", "tokens": ["Ist", "das", "drit\u00b7te", "auf\u00b7ge\u00b7wach\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem guten Lande Sachsen,", "tokens": ["In", "dem", "gu\u00b7ten", "Lan\u00b7de", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tritt den Boden fest und stark.", "tokens": ["Tritt", "den", "Bo\u00b7den", "fest", "und", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dies hier stammt aus Schlesiens Talen,", "tokens": ["Dies", "hier", "stammt", "aus", "Schle\u00b7si\u00b7ens", "Ta\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Jene zwei sind aus Westfalen", "tokens": ["Je\u00b7ne", "zwei", "sind", "aus", "West\u00b7fa\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "CARD", "VAFIN", "APPR", "NN"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und der Brandenburger Mark.", "tokens": ["Und", "der", "Bran\u00b7den\u00b7bur\u00b7ger", "Mark", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seht alsdann mit breitem Nacken", "tokens": ["Seht", "als\u00b7dann", "mit", "brei\u00b7tem", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch den Pommern und Polacken \u2013", "tokens": ["Noch", "den", "Pom\u00b7mern", "und", "Po\u00b7la\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch ein derb und stattlich Paar! \u2013", "tokens": ["Auch", "ein", "derb", "und", "statt\u00b7lich", "Paar", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJD", "KON", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Also ziehn die acht trotz einem;", "tokens": ["Al\u00b7so", "ziehn", "die", "acht", "trotz", "ei\u00b7nem", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "CARD", "APPR", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Frisch und mutig \u2013 doch an keinem", "tokens": ["Frisch", "und", "mu\u00b7tig", "\u2013", "doch", "an", "kei\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$(", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist auch nur ein falsches Haar!", "tokens": ["Ist", "auch", "nur", "ein", "fal\u00b7sches", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wollt' es glauben nur der Lenker!", "tokens": ["Wollt'", "es", "glau\u00b7ben", "nur", "der", "Len\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch der denkt: \u00bbHol' euch der Henker!", "tokens": ["Doch", "der", "denkt", ":", "\u00bb", "Hol'", "euch", "der", "Hen\u00b7ker", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "$.", "$(", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Immer mehr schwillt euch der Kamm!", "tokens": ["Im\u00b7mer", "mehr", "schwillt", "euch", "der", "Kamm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Wahr ist's, ihr seid brav und wacker!", "tokens": ["Wahr", "ist's", ",", "ihr", "seid", "brav", "und", "wa\u00b7cker", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch ein paar von euch sind Racker!\u00ab", "tokens": ["Doch", "ein", "paar", "von", "euch", "sind", "Ra\u00b7cker", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "PIAT", "APPR", "PPER", "VAFIN", "NN", "$.", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "H\u00e4lt somit die Z\u00fcgel stramm.", "tokens": ["H\u00e4lt", "so\u00b7mit", "die", "Z\u00fc\u00b7gel", "stramm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "T\u00f6nt herauf zu ihm ein Schnauben,", "tokens": ["T\u00f6nt", "her\u00b7auf", "zu", "ihm", "ein", "Schnau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht er: \u00bbWas sich die erlauben!\u00ab", "tokens": ["Spricht", "er", ":", "\u00bb", "Was", "sich", "die", "er\u00b7lau\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "PWS", "PRF", "ART", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruckt mit Z\u00fcrnen am Gebi\u00df.", "tokens": ["Ruckt", "mit", "Z\u00fcr\u00b7nen", "am", "Ge\u00b7bi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schallt ein Huf recht dreist metallen,", "tokens": ["Schallt", "ein", "Huf", "recht", "dreist", "me\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich erregt es sein Mi\u00dffallen \u2013", "tokens": ["Gleich", "er\u00b7regt", "es", "sein", "Mi\u00df\u00b7fal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja doch, es gef\u00e4llt ihm mi\u00df!", "tokens": ["Ja", "doch", ",", "es", "ge\u00b7f\u00e4llt", "ihm", "mi\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Wollen sie sich eines neuen", "tokens": ["Wol\u00b7len", "sie", "sich", "ei\u00b7nes", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Peitschenreglements nicht freuen \u2013", "tokens": ["Peit\u00b7schen\u00b7reg\u00b7le\u00b7ments", "nicht", "freu\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei, wie straft sie da sein Pfiff!", "tokens": ["Ei", ",", "wie", "straft", "sie", "da", "sein", "Pfiff", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, wie f\u00e4llt ihm da vom Munde", "tokens": ["Ei", ",", "wie", "f\u00e4llt", "ihm", "da", "vom", "Mun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ander Wort, als zu der Stunde,", "tokens": ["An\u00b7der", "Wort", ",", "als", "zu", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drin die Z\u00fcgel er ergriff!", "tokens": ["Drin", "die", "Z\u00fc\u00b7gel", "er", "er\u00b7griff", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wolln mit ehrerbiet'gem Wiehren", "tokens": ["Wolln", "mit", "ehr\u00b7er\u00b7biet'\u00b7gem", "Wieh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flehn sie oder Klage f\u00fchren,", "tokens": ["Flehn", "sie", "o\u00b7der", "Kla\u00b7ge", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solches gilt als Schabernack!", "tokens": ["Sol\u00b7ches", "gilt", "als", "Scha\u00b7ber\u00b7nack", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vollends wird der Stab gebrochen", "tokens": ["Vol\u00b7lends", "wird", "der", "Stab", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcber gar ein zweites Pochen", "tokens": ["\u00dc\u00b7ber", "gar", "ein", "zwei\u00b7tes", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um denselben Habersack!", "tokens": ["Um", "den\u00b7sel\u00b7ben", "Ha\u00b7ber\u00b7sack", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ziehn darum, die gerne fl\u00f6gen,", "tokens": ["Ziehn", "da\u00b7rum", ",", "die", "ger\u00b7ne", "fl\u00f6\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stolz und brausend gern ihn z\u00f6gen,", "tokens": ["Stolz", "und", "brau\u00b7send", "gern", "ihn", "z\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Langsam jetzo sein Gef\u00e4hr!", "tokens": ["Lang\u00b7sam", "jet\u00b7zo", "sein", "Ge\u00b7f\u00e4hr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets des rechten Vorw\u00e4rts harrend,", "tokens": ["Stets", "des", "rech\u00b7ten", "Vor\u00b7w\u00e4rts", "har\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stampfend nicht, doch daf\u00fcr scharrend", "tokens": ["Stamp\u00b7fend", "nicht", ",", "doch", "da\u00b7f\u00fcr", "schar\u00b7rend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKNEG", "$,", "KON", "PAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Stille desto mehr!", "tokens": ["In", "der", "Stil\u00b7le", "des\u00b7to", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Immer ruhig, immer sachte,", "tokens": ["Im\u00b7mer", "ru\u00b7hig", ",", "im\u00b7mer", "sach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr getreuen, lieben Achte!", "tokens": ["Ihr", "ge\u00b7treu\u00b7en", ",", "lie\u00b7ben", "Ach\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines glaubt und bleibt dabei:", "tokens": ["Ei\u00b7nes", "glaubt", "und", "bleibt", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Steckt der Karrn einmal im Drecke,", "tokens": ["Steckt", "der", "Karrn", "ein\u00b7mal", "im", "Dre\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hui, dann geht es rasch vom Flecke,", "tokens": ["Hui", ",", "dann", "geht", "es", "rasch", "vom", "Fle\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Z\u00e4ume fliegen frei!", "tokens": ["Und", "die", "Z\u00e4u\u00b7me", "flie\u00b7gen", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "F\u00e4hrt im Land 'ne Staatskarosse;", "tokens": ["F\u00e4hrt", "im", "Land", "'ne", "Staats\u00b7ka\u00b7ros\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehn sie acht famose Rosse,", "tokens": ["Ziehn", "sie", "acht", "fa\u00b7mo\u00b7se", "Ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Feurig, ein beherzt Gespann!", "tokens": ["Feu\u00b7rig", ",", "ein", "be\u00b7herzt", "Ge\u00b7spann", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eines ward am Rhein geboren,", "tokens": ["Ei\u00b7nes", "ward", "am", "Rhein", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPRART", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hebt das Haupt und spitzt die Ohren,", "tokens": ["Hebt", "das", "Haupt", "und", "spitzt", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zieht vor allen mutig an.", "tokens": ["Zieht", "vor", "al\u00b7len", "mu\u00b7tig", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Bei\u00dft ein andres in die Stange,", "tokens": ["Bei\u00dft", "ein", "and\u00b7res", "in", "die", "Stan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Fischer mit Gesange", "tokens": ["Wo", "der", "Fi\u00b7scher", "mit", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Froh den goldnen Bernstein fischt;", "tokens": ["Froh", "den", "gold\u00b7nen", "Bern\u00b7stein", "fischt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kr\u00e4ftig schnaubt es mit den N\u00fcstern.", "tokens": ["Kr\u00e4f\u00b7tig", "schnaubt", "es", "mit", "den", "N\u00fcs\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die es lechzend in den d\u00fcstern", "tokens": ["Die", "es", "lech\u00b7zend", "in", "den", "d\u00fcs\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ostseewellen sich erfrischt.", "tokens": ["Ost\u00b7see\u00b7wel\u00b7len", "sich", "er\u00b7frischt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ist das dritte aufgewachsen", "tokens": ["Ist", "das", "drit\u00b7te", "auf\u00b7ge\u00b7wach\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem guten Lande Sachsen,", "tokens": ["In", "dem", "gu\u00b7ten", "Lan\u00b7de", "Sach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tritt den Boden fest und stark.", "tokens": ["Tritt", "den", "Bo\u00b7den", "fest", "und", "stark", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dies hier stammt aus Schlesiens Talen,", "tokens": ["Dies", "hier", "stammt", "aus", "Schle\u00b7si\u00b7ens", "Ta\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Jene zwei sind aus Westfalen", "tokens": ["Je\u00b7ne", "zwei", "sind", "aus", "West\u00b7fa\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "CARD", "VAFIN", "APPR", "NN"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und der Brandenburger Mark.", "tokens": ["Und", "der", "Bran\u00b7den\u00b7bur\u00b7ger", "Mark", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Seht alsdann mit breitem Nacken", "tokens": ["Seht", "als\u00b7dann", "mit", "brei\u00b7tem", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch den Pommern und Polacken \u2013", "tokens": ["Noch", "den", "Pom\u00b7mern", "und", "Po\u00b7la\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch ein derb und stattlich Paar! \u2013", "tokens": ["Auch", "ein", "derb", "und", "statt\u00b7lich", "Paar", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "ADJD", "KON", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Also ziehn die acht trotz einem;", "tokens": ["Al\u00b7so", "ziehn", "die", "acht", "trotz", "ei\u00b7nem", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "CARD", "APPR", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Frisch und mutig \u2013 doch an keinem", "tokens": ["Frisch", "und", "mu\u00b7tig", "\u2013", "doch", "an", "kei\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "$(", "ADV", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist auch nur ein falsches Haar!", "tokens": ["Ist", "auch", "nur", "ein", "fal\u00b7sches", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wollt' es glauben nur der Lenker!", "tokens": ["Wollt'", "es", "glau\u00b7ben", "nur", "der", "Len\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch der denkt: \u00bbHol' euch der Henker!", "tokens": ["Doch", "der", "denkt", ":", "\u00bb", "Hol'", "euch", "der", "Hen\u00b7ker", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "$.", "$(", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Immer mehr schwillt euch der Kamm!", "tokens": ["Im\u00b7mer", "mehr", "schwillt", "euch", "der", "Kamm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Wahr ist's, ihr seid brav und wacker!", "tokens": ["Wahr", "ist's", ",", "ihr", "seid", "brav", "und", "wa\u00b7cker", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch ein paar von euch sind Racker!\u00ab", "tokens": ["Doch", "ein", "paar", "von", "euch", "sind", "Ra\u00b7cker", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "PIAT", "APPR", "PPER", "VAFIN", "NN", "$.", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "H\u00e4lt somit die Z\u00fcgel stramm.", "tokens": ["H\u00e4lt", "so\u00b7mit", "die", "Z\u00fc\u00b7gel", "stramm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "T\u00f6nt herauf zu ihm ein Schnauben,", "tokens": ["T\u00f6nt", "her\u00b7auf", "zu", "ihm", "ein", "Schnau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spricht er: \u00bbWas sich die erlauben!\u00ab", "tokens": ["Spricht", "er", ":", "\u00bb", "Was", "sich", "die", "er\u00b7lau\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "PWS", "PRF", "ART", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruckt mit Z\u00fcrnen am Gebi\u00df.", "tokens": ["Ruckt", "mit", "Z\u00fcr\u00b7nen", "am", "Ge\u00b7bi\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schallt ein Huf recht dreist metallen,", "tokens": ["Schallt", "ein", "Huf", "recht", "dreist", "me\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich erregt es sein Mi\u00dffallen \u2013", "tokens": ["Gleich", "er\u00b7regt", "es", "sein", "Mi\u00df\u00b7fal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja doch, es gef\u00e4llt ihm mi\u00df!", "tokens": ["Ja", "doch", ",", "es", "ge\u00b7f\u00e4llt", "ihm", "mi\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.17": {"line.1": {"text": "Wollen sie sich eines neuen", "tokens": ["Wol\u00b7len", "sie", "sich", "ei\u00b7nes", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Peitschenreglements nicht freuen \u2013", "tokens": ["Peit\u00b7schen\u00b7reg\u00b7le\u00b7ments", "nicht", "freu\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei, wie straft sie da sein Pfiff!", "tokens": ["Ei", ",", "wie", "straft", "sie", "da", "sein", "Pfiff", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, wie f\u00e4llt ihm da vom Munde", "tokens": ["Ei", ",", "wie", "f\u00e4llt", "ihm", "da", "vom", "Mun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ander Wort, als zu der Stunde,", "tokens": ["An\u00b7der", "Wort", ",", "als", "zu", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drin die Z\u00fcgel er ergriff!", "tokens": ["Drin", "die", "Z\u00fc\u00b7gel", "er", "er\u00b7griff", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Wolln mit ehrerbiet'gem Wiehren", "tokens": ["Wolln", "mit", "ehr\u00b7er\u00b7biet'\u00b7gem", "Wieh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flehn sie oder Klage f\u00fchren,", "tokens": ["Flehn", "sie", "o\u00b7der", "Kla\u00b7ge", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Solches gilt als Schabernack!", "tokens": ["Sol\u00b7ches", "gilt", "als", "Scha\u00b7ber\u00b7nack", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vollends wird der Stab gebrochen", "tokens": ["Vol\u00b7lends", "wird", "der", "Stab", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcber gar ein zweites Pochen", "tokens": ["\u00dc\u00b7ber", "gar", "ein", "zwei\u00b7tes", "Po\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um denselben Habersack!", "tokens": ["Um", "den\u00b7sel\u00b7ben", "Ha\u00b7ber\u00b7sack", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Ziehn darum, die gerne fl\u00f6gen,", "tokens": ["Ziehn", "da\u00b7rum", ",", "die", "ger\u00b7ne", "fl\u00f6\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stolz und brausend gern ihn z\u00f6gen,", "tokens": ["Stolz", "und", "brau\u00b7send", "gern", "ihn", "z\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Langsam jetzo sein Gef\u00e4hr!", "tokens": ["Lang\u00b7sam", "jet\u00b7zo", "sein", "Ge\u00b7f\u00e4hr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets des rechten Vorw\u00e4rts harrend,", "tokens": ["Stets", "des", "rech\u00b7ten", "Vor\u00b7w\u00e4rts", "har\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stampfend nicht, doch daf\u00fcr scharrend", "tokens": ["Stamp\u00b7fend", "nicht", ",", "doch", "da\u00b7f\u00fcr", "schar\u00b7rend"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKNEG", "$,", "KON", "PAV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Stille desto mehr!", "tokens": ["In", "der", "Stil\u00b7le", "des\u00b7to", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Immer ruhig, immer sachte,", "tokens": ["Im\u00b7mer", "ru\u00b7hig", ",", "im\u00b7mer", "sach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihr getreuen, lieben Achte!", "tokens": ["Ihr", "ge\u00b7treu\u00b7en", ",", "lie\u00b7ben", "Ach\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines glaubt und bleibt dabei:", "tokens": ["Ei\u00b7nes", "glaubt", "und", "bleibt", "da\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KON", "VVFIN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Steckt der Karrn einmal im Drecke,", "tokens": ["Steckt", "der", "Karrn", "ein\u00b7mal", "im", "Dre\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hui, dann geht es rasch vom Flecke,", "tokens": ["Hui", ",", "dann", "geht", "es", "rasch", "vom", "Fle\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "$,", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Z\u00e4ume fliegen frei!", "tokens": ["Und", "die", "Z\u00e4u\u00b7me", "flie\u00b7gen", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": [