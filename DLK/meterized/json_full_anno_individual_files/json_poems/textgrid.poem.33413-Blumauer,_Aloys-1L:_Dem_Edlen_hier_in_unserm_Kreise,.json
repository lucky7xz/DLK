{"textgrid.poem.33413": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dem Edlen hier in unserm Kreise,", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem Edlen hier in unserm Kreise,", "tokens": ["Dem", "Ed\u00b7len", "hier", "in", "un\u00b7serm", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der, w\u00e4hrend wir so manche Maurerreise", "tokens": ["Der", ",", "w\u00e4h\u00b7rend", "wir", "so", "man\u00b7che", "Mau\u00b7rer\u00b7rei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf eb'nem Boden ganz bequem vollbracht,", "tokens": ["Auf", "eb'\u00b7nem", "Bo\u00b7den", "ganz", "be\u00b7quem", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Runde um die Welt gemacht,", "tokens": ["Die", "Run\u00b7de", "um", "die", "Welt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der da, gef\u00fchrt durch alle Elemente,", "tokens": ["Der", "da", ",", "ge\u00b7f\u00fchrt", "durch", "al\u00b7le", "E\u00b7le\u00b7men\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Vom Thau des Himmels und vom Blitze mehr", "tokens": ["Vom", "Thau", "des", "Him\u00b7mels", "und", "vom", "Blit\u00b7ze", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "KON", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als mancher Lehrling, der auch noch so sehr", "tokens": ["Als", "man\u00b7cher", "Lehr\u00b7ling", ",", "der", "auch", "noch", "so", "sehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "$,", "PRELS", "ADV", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von Feu'r und Wasser litt, uns sagen k\u00f6nnte,", "tokens": ["Von", "Feu'r", "und", "Was\u00b7ser", "litt", ",", "uns", "sa\u00b7gen", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und dessen ganze Fahrt, wo immerdar", "tokens": ["Und", "des\u00b7sen", "gan\u00b7ze", "Fahrt", ",", "wo", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Tod ihn an aus hundert Schl\u00fcnden g\u00e4hnte,", "tokens": ["Der", "Tod", "ihn", "an", "aus", "hun\u00b7dert", "Schl\u00fcn\u00b7den", "g\u00e4hn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "APPR", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ein ewiges ", "tokens": ["Ein", "e\u00b7wi\u00b7ges"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Der aber auch daf\u00fcr auf seinem schweren Pfade", "tokens": ["Der", "a\u00b7ber", "auch", "da\u00b7f\u00fcr", "auf", "sei\u00b7nem", "schwe\u00b7ren", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auf zwei und siebzig volle Grade", "tokens": ["Auf", "zwei", "und", "sieb\u00b7zig", "vol\u00b7le", "Gra\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "KON", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Dem Pole nahe r\u00fcckte, und daher", "tokens": ["Dem", "Po\u00b7le", "na\u00b7he", "r\u00fcck\u00b7te", ",", "und", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "KON", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die neidenswerthe Freude hatte,", "tokens": ["Die", "nei\u00b7dens\u00b7wert\u00b7he", "Freu\u00b7de", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df er sich seinem Ziele mehr \u2013", "tokens": ["Da\u00df", "er", "sich", "sei\u00b7nem", "Zie\u00b7le", "mehr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Als mancher Maurer sich dem seinen \u2013 nahte;", "tokens": ["Als", "man\u00b7cher", "Mau\u00b7rer", "sich", "dem", "sei\u00b7nen", "\u2013", "nah\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PRF", "ART", "PPOSAT", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Der mit Begierde da nach jeder Spur", "tokens": ["Der", "mit", "Be\u00b7gier\u00b7de", "da", "nach", "je\u00b7der", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Von Weisheit und von Menschenkenntni\u00df haschte,", "tokens": ["Von", "Weis\u00b7heit", "und", "von", "Men\u00b7schen\u00b7kennt\u00b7ni\u00df", "haschte", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und die so mannichfalt'ge, menschliche Natur", "tokens": ["Und", "die", "so", "man\u00b7nich\u00b7falt'\u00b7ge", ",", "menschli\u00b7che", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADV", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.21": {"text": "Bald in dem h\u00f6chsten Putze, und bald nur", "tokens": ["Bald", "in", "dem", "h\u00f6chs\u00b7ten", "Put\u00b7ze", ",", "und", "bald", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "KON", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Im Negligee, wie bei'm Erwachen, \u00fcberraschte;", "tokens": ["Im", "Neg\u00b7li\u00b7gee", ",", "wie", "bei'm", "Er\u00b7wa\u00b7chen", ",", "\u00fc\u00b7berr\u00b7aschte", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der in dem Bilde, das uns seine Hand", "tokens": ["Der", "in", "dem", "Bil\u00b7de", ",", "das", "uns", "sei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Davon entwarf, auch nicht den kleinsten Zug verfehlte,", "tokens": ["Da\u00b7von", "ent\u00b7warf", ",", "auch", "nicht", "den", "kleins\u00b7ten", "Zug", "ver\u00b7fehl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und uns den Menschen, so wie er in jedem Land", "tokens": ["Und", "uns", "den", "Men\u00b7schen", ",", "so", "wie", "er", "in", "je\u00b7dem", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "NN", "$,", "ADV", "KOKOM", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ihn von Natur und Kunst gebildet fand,", "tokens": ["Ihn", "von", "Na\u00b7tur", "und", "Kunst", "ge\u00b7bil\u00b7det", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Rein, wie die Wahrheit selbst, vor Augen stellte;", "tokens": ["Rein", ",", "wie", "die", "Wahr\u00b7heit", "selbst", ",", "vor", "Au\u00b7gen", "stell\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "ADV", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Kurzum dem Mann, der \u2013 wie sein Werk beweist \u2013", "tokens": ["Kur\u00b7zum", "dem", "Mann", ",", "der", "\u2013", "wie", "sein", "Werk", "be\u00b7weist", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "$(", "KOKOM", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Als Meister um die Welt gereist,", "tokens": ["Als", "Meis\u00b7ter", "um", "die", "Welt", "ge\u00b7reist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Dem, Br\u00fcder, wollen wir zu fernerem Gedeihen", "tokens": ["Dem", ",", "Br\u00fc\u00b7der", ",", "wol\u00b7len", "wir", "zu", "fer\u00b7ne\u00b7rem", "Ge\u00b7dei\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "$,", "NN", "$,", "VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der schweren Reisekunst dies Freudenfeuer weihen,", "tokens": ["Der", "schwe\u00b7ren", "Rei\u00b7se\u00b7kunst", "dies", "Freu\u00b7den\u00b7feu\u00b7er", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und uns dabei der Hoffnung freuen:", "tokens": ["Und", "uns", "da\u00b7bei", "der", "Hoff\u00b7nung", "freu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Es schmeck' ihm hier ein kleines Brudermahl", "tokens": ["Es", "schmeck'", "ihm", "hier", "ein", "klei\u00b7nes", "Bru\u00b7der\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "In unsern milden Zonen besser,", "tokens": ["In", "un\u00b7sern", "mil\u00b7den", "Zo\u00b7nen", "bes\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Als dort ein \u2013 w\u00e4r's auch maurerisches \u2013 Mahl", "tokens": ["Als", "dort", "ein", "\u2013", "w\u00e4r's", "auch", "mau\u00b7re\u00b7ri\u00b7sches", "\u2013", "Mahl"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADV", "ART", "$(", "VAFIN", "ADV", "ADJA", "$(", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Bei einem Bruder Menschenfresser.", "tokens": ["Bei", "ei\u00b7nem", "Bru\u00b7der", "Men\u00b7schen\u00b7fres\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dem Edlen hier in unserm Kreise,", "tokens": ["Dem", "Ed\u00b7len", "hier", "in", "un\u00b7serm", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der, w\u00e4hrend wir so manche Maurerreise", "tokens": ["Der", ",", "w\u00e4h\u00b7rend", "wir", "so", "man\u00b7che", "Mau\u00b7rer\u00b7rei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "PIAT", "