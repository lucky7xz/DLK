{"dta.poem.9211": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "3.  Nachsatz.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Die ascher-mittwoch sol auff einen freytag fallen/", "tokens": ["Die", "a\u00b7scher\u00b7mitt\u00b7woch", "sol", "auff", "ei\u00b7nen", "frey\u00b7tag", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man sol die Martins-gans zum oster-h\u00e4\u00dfgen", "tokens": ["Man", "sol", "die", "Mar\u00b7tins\u00b7gans", "zum", "os\u00b7ter\u00b7h\u00e4\u00df\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "stallen/", "tokens": ["stal\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Die m\u00e4dgen sollen lux/ die b\u00fcfgen lutze heissen/", "tokens": ["Die", "m\u00e4d\u00b7gen", "sol\u00b7len", "lux", "/", "die", "b\u00fcf\u00b7gen", "lut\u00b7ze", "heis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "NE", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der besen-binder sol die kupfferst\u00fccke reissen/", "tokens": ["Der", "be\u00b7sen\u00b7bin\u00b7der", "sol", "die", "kupf\u00b7fer\u00b7st\u00fc\u00b7cke", "reis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der stunden-ruffer sol Concerten musiciren/", "tokens": ["Der", "stun\u00b7den\u00b7ruf\u00b7fer", "sol", "Con\u00b7cer\u00b7ten", "mu\u00b7si\u00b7ci\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und der gemeine lauff sol sich durchaus verlieren/", "tokens": ["Und", "der", "ge\u00b7mei\u00b7ne", "lauff", "sol", "sich", "durc\u00b7haus", "ver\u00b7lie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo ich der stoltzen seel in dem beliebten leibe", "tokens": ["Wo", "ich", "der", "stolt\u00b7zen", "seel", "in", "dem", "be\u00b7lieb\u00b7ten", "lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht auch bi\u00df an den tod getreu und g\u00fcnstig bleibe.", "tokens": ["Nicht", "auch", "bi\u00df", "an", "den", "tod", "ge\u00b7treu", "und", "g\u00fcns\u00b7tig", "blei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "APPR", "ART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}