{"dta.poem.7248": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Die f\u00fcnffte Abtheilung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Adelheit bistu mein ", "tokens": ["A\u00b7del\u00b7heit", "bis\u00b7tu", "mein"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Bistu nicht ", "tokens": ["Bis\u00b7tu", "nicht"], "token_info": ["word", "word"], "pos": ["ADV", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ach soll ich nun einsam ", "tokens": ["Ach", "soll", "ich", "nun", "ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ech. weben/ vnd im elend seyn?", "tokens": ["Ech", ".", "we\u00b7ben", "/", "vnd", "im", "e\u00b7lend", "seyn", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVINF", "$(", "KON", "APPRART", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach wo bi", "tokens": ["Ach", "wo", "bi"], "token_info": ["word", "word", "word"], "pos": ["ITJ", "PWAV", "NE"], "meter": "+--", "measure": "dactylic.init"}, "line.6": {"text": "nur nicht et", "tokens": ["nur", "nicht", "et"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PTKNEG", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "schadts.", "tokens": ["schadts", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Fort f\u00fcr fort ich ", "tokens": ["Fort", "f\u00fcr", "fort", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR"