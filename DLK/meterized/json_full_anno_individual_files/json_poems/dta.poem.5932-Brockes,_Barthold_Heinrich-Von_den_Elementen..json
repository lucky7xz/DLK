{"dta.poem.5932": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Elementen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie, wenn einer schreibt, die Lettern, welche sich", "tokens": ["Gleich", "wie", ",", "wenn", "ei\u00b7ner", "schreibt", ",", "die", "Let\u00b7tern", ",", "wel\u00b7che", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOKOM", "$,", "KOUS", "PIS", "VVFIN", "$,", "ART", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gar verschiedentlich", "tokens": ["So", "gar", "ver\u00b7schie\u00b7dent\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und so ver\u00e4nderlich geordnet, finden,", "tokens": ["Und", "so", "ver\u00b7\u00e4n\u00b7der\u00b7lich", "ge\u00b7ord\u00b7net", ",", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVPP", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Indem sie sich bald trennen, bald verbinden,", "tokens": ["In\u00b7dem", "sie", "sich", "bald", "tren\u00b7nen", ",", "bald", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf ungez\u00e4hlte Art gestellet und gesetzt;", "tokens": ["Auf", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Art", "ge\u00b7stel\u00b7let", "und", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem menschlichen Gesichte", "tokens": ["Dem", "menschli\u00b7chen", "Ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Formiren die f\u00fcrtrefflichen Gedichte,", "tokens": ["For\u00b7mi\u00b7ren", "die", "f\u00fcr\u00b7treff\u00b7li\u00b7chen", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wodurch ", "tokens": ["Wo\u00b7durch"], "token_info": ["word"], "pos": ["PWAV"], "meter": "-+", "measure": "iambic.single"}}}}}