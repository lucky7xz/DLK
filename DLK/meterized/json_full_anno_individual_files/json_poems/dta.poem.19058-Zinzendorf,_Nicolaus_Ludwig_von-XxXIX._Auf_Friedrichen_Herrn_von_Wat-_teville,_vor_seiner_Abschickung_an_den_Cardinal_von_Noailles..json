{"dta.poem.19058": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIX.  Auf Friedrichen Herrn von Wat-  \n teville, vor seiner Abschickung an den  \n Cardinal von Noailles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "So wagt sich, theurer Freund! zu dir derselbe Kiel,", "tokens": ["So", "wagt", "sich", ",", "theu\u00b7rer", "Freund", "!", "zu", "dir", "der\u00b7sel\u00b7be", "Kiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "ADJD", "NN", "$.", "APPR", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der zwar nicht gantz und gar des Schreibens unerfahren,", "tokens": ["Der", "zwar", "nicht", "gantz", "und", "gar", "des", "Schrei\u00b7bens", "un\u00b7er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "ADV", "KON", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der aber auch ein Feind vom blossen W\u00f6rter-Spiel,", "tokens": ["Der", "a\u00b7ber", "auch", "ein", "Feind", "vom", "blos\u00b7sen", "W\u00f6r\u00b7ter\u00b7Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sonderlich den Ruhm der Freunde pflegt zu sparen.", "tokens": ["Und", "son\u00b7der\u00b7lich", "den", "Ruhm", "der", "Freun\u00b7de", "pflegt", "zu", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man siehet, da\u00df er sich nun offenbarlich regt:", "tokens": ["Man", "sie\u00b7het", ",", "da\u00df", "er", "sich", "nun", "of\u00b7fen\u00b7bar\u00b7lich", "regt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht dein verdientes Lob der Erden kund zu machen,", "tokens": ["Nicht", "dein", "ver\u00b7dien\u00b7tes", "Lob", "der", "Er\u00b7den", "kund", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "ADJA", "NN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als welches Amt der HErr an jenem Tage tr\u00e4gt;", "tokens": ["Als", "wel\u00b7ches", "Amt", "der", "Herr", "an", "je\u00b7nem", "Ta\u00b7ge", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAT", "NN", "ART", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Er will nur itzt der Welt und ihrer Thorheit lachen.", "tokens": ["Er", "will", "nur", "itzt", "der", "Welt", "und", "ih\u00b7rer", "Thor\u00b7heit", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So bald man sich bekehrt, so ist es ausgemacht,", "tokens": ["So", "bald", "man", "sich", "be\u00b7kehrt", ",", "so", "ist", "es", "aus\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PRF", "VVPP", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und stammte man vorher aus Wittekindes Lenden,", "tokens": ["Und", "stamm\u00b7te", "man", "vor\u00b7her", "aus", "Wit\u00b7te\u00b7kin\u00b7des", "Len\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den man der Hoheit selbst zur Wurtzel ausgedacht;", "tokens": ["Den", "man", "der", "Ho\u00b7heit", "selbst", "zur", "Wurt\u00b7zel", "aus\u00b7ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wird sich unversehns das Blatt der Ehren wenden.", "tokens": ["So", "wird", "sich", "un\u00b7ver\u00b7sehns", "das", "Blatt", "der", "Eh\u00b7ren", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die gantze Welt erstaunt, wie herrlich jener Mann,", "tokens": ["Die", "gant\u00b7ze", "Welt", "er\u00b7staunt", ",", "wie", "herr\u00b7lich", "je\u00b7ner", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "PWAV", "ADJD", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bey aller Redlichkeit, an Geld und Gut geworden:", "tokens": ["Bey", "al\u00b7ler", "Red\u00b7lich\u00b7keit", ",", "an", "Geld", "und", "Gut", "ge\u00b7wor\u00b7den", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "NN", "KON", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kaum aber, da\u00df er sich zu GOtt bekehren kan;", "tokens": ["Kaum", "a\u00b7ber", ",", "da\u00df", "er", "sich", "zu", "Gott", "be\u00b7keh\u00b7ren", "kan", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So eilet sie mit ihm zum nechsten Bettel-Orden.", "tokens": ["So", "ei\u00b7let", "sie", "mit", "ihm", "zum", "nechs\u00b7ten", "Bet\u00b7tel\u00b7Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Warum? Was ist der Welt? Sie l\u00e4st sich einerseits", "tokens": ["Wa\u00b7rum", "?", "Was", "ist", "der", "Welt", "?", "Sie", "l\u00e4st", "sich", "ei\u00b7ner\u00b7seits"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "$.", "PWS", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Von einem Christenthum, bey eitler Welt-Lust, tr\u00e4umen:", "tokens": ["Von", "ei\u00b7nem", "Chris\u00b7ten\u00b7thum", ",", "bey", "eit\u00b7ler", "Welt\u00b7Lust", ",", "tr\u00e4u\u00b7men", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie kan die Th\u00f6rin denn mit Christi hohem Creutz", "tokens": ["Wie", "kan", "die", "Th\u00f6\u00b7rin", "denn", "mit", "Chris\u00b7ti", "ho\u00b7hem", "Creutz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "NN", "KON", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Hoheit ihres Stuhls auf keine Weise reimen?", "tokens": ["Die", "Ho\u00b7heit", "ih\u00b7res", "Stuhls", "auf", "kei\u00b7ne", "Wei\u00b7se", "rei\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wenn sich ein fremd Gesicht in ihren Gr\u00e4ntzen zeigt,", "tokens": ["Wenn", "sich", "ein", "fremd", "Ge\u00b7sicht", "in", "ih\u00b7ren", "Gr\u00e4nt\u00b7zen", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.22": {"text": "So pflegt sies iedermann an W\u00fcrde vorzuziehn:", "tokens": ["So", "pflegt", "sies", "ie\u00b7der\u00b7mann", "an", "W\u00fcr\u00b7de", "vor\u00b7zu\u00b7ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der Fremdling aber sey nur Christo zugeneigt;", "tokens": ["Der", "Fremd\u00b7ling", "a\u00b7ber", "sey", "nur", "Chris\u00b7to", "zu\u00b7gen\u00b7eigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADV", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So mag er nur fein bald aus ihrer Gegend fliehn.", "tokens": ["So", "mag", "er", "nur", "fein", "bald", "aus", "ih\u00b7rer", "Ge\u00b7gend", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der wird geschwind ein Graf, aufs wenigste Baron,", "tokens": ["Der", "wird", "ge\u00b7schwind", "ein", "Graf", ",", "aufs", "we\u00b7nigs\u00b7te", "Ba\u00b7ron", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ART", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der in der Fremde nur das Seinige verbrauset;", "tokens": ["Der", "in", "der", "Frem\u00b7de", "nur", "das", "Sei\u00b7ni\u00b7ge", "ver\u00b7brau\u00b7set", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "ART", "PPOSS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der aber scheint der Welt, wo nicht ein Huren-Sohn,", "tokens": ["Der", "a\u00b7ber", "scheint", "der", "Welt", ",", "wo", "nicht", "ein", "Hu\u00b7ren\u00b7Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Doch nicht viel ehrlicher, dem vor der Welt-Lust grauset.", "tokens": ["Doch", "nicht", "viel", "ehr\u00b7li\u00b7cher", ",", "dem", "vor", "der", "Welt\u00b7Lust", "grau\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer sich der Eitelkeit der Welt gebrauchen kan,", "tokens": ["Wer", "sich", "der", "Ei\u00b7tel\u00b7keit", "der", "Welt", "ge\u00b7brau\u00b7chen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wei\u00df mit falschem Glantz sein Nichts seyn auszumahlen,", "tokens": ["Und", "wei\u00df", "mit", "fal\u00b7schem", "Glantz", "sein", "Nichts", "seyn", "aus\u00b7zu\u00b7mah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "VAINF", "PIS", "VAINF", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der trete nur getrost vor alle Welt heran,", "tokens": ["Der", "tre\u00b7te", "nur", "ge\u00b7trost", "vor", "al\u00b7le", "Welt", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Von Reichthum, Klugheit, Stand, von was er will zu pralen.", "tokens": ["Von", "Reicht\u00b7hum", ",", "Klug\u00b7heit", ",", "Stand", ",", "von", "was", "er", "will", "zu", "pra\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "APPR", "PRELS", "PPER", "VMFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Erde ruht auf Wind, (verg\u00f6nnt mir dieses Spiel,)", "tokens": ["Die", "Er\u00b7de", "ruht", "auf", "Wind", ",", "(", "ver\u00b7g\u00f6nnt", "mir", "die\u00b7ses", "Spiel", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "$(", "VVFIN", "PPER", "PDAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Drum klingt ihr nichts so gro\u00df, als solcherley Get\u00f6se,", "tokens": ["Drum", "klingt", "ihr", "nichts", "so", "gro\u00df", ",", "als", "sol\u00b7cher\u00b7ley", "Ge\u00b7t\u00f6\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PIS", "ADV", "ADJD", "$,", "KOUS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da wisse man nur nichts, nur rede man fein viel;", "tokens": ["Da", "wis\u00b7se", "man", "nur", "nichts", ",", "nur", "re\u00b7de", "man", "fein", "viel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PIS", "$,", "ADV", "VVFIN", "PIS", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Man habe gleich kein Hertz, man thue nur sehr b\u00f6se.", "tokens": ["Man", "ha\u00b7be", "gleich", "kein", "Hertz", ",", "man", "thue", "nur", "sehr", "b\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "PIAT", "NN", "$,", "PIS", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Verzeih! dem Lauf der Welt, ", "tokens": ["Ver\u00b7zeih", "!", "dem", "Lauf", "der", "Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Du Kleinod Arelats, du Burg von ", "tokens": ["Du", "Klei\u00b7nod", "Ar\u00b7e\u00b7lats", ",", "du", "Burg", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "NE", "$,", "PPER", "NN", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Gehn gleich von deinem Stamm die h\u00f6chsten Sprossen ", "tokens": ["Gehn", "gleich", "von", "dei\u00b7nem", "Stamm", "die", "h\u00f6chs\u00b7ten", "Spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Doch schweigt der F\u00fcrwitz selbst von deiner W\u00fcrde stille.", "tokens": ["Doch", "schweigt", "der", "F\u00fcr\u00b7witz", "selbst", "von", "dei\u00b7ner", "W\u00fcr\u00b7de", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Warum, der edle Zweig, den Teutschland j\u00fcngst gesehn,", "tokens": ["Wa\u00b7rum", ",", "der", "ed\u00b7le", "Zweig", "