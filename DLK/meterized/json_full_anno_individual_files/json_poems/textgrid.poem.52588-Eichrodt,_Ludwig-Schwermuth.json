{"textgrid.poem.52588": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Schwermuth", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Garten wars; die lenzigen Winde f\u00e4cheln \u2013", "tokens": ["Im", "Gar\u00b7ten", "wars", ";", "die", "len\u00b7zi\u00b7gen", "Win\u00b7de", "f\u00e4\u00b7cheln", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$.", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Abschied, Umarmung, Ku\u00df und schmerzvoll L\u00e4cheln.", "tokens": ["Ab\u00b7schied", ",", "Um\u00b7ar\u00b7mung", ",", "Ku\u00df", "und", "schmerz\u00b7voll", "L\u00e4\u00b7cheln", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "ADJD", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Die Thr\u00e4ne quillt, ich k\u00fc\u00df sie von den Wangen.", "tokens": ["Die", "Thr\u00e4\u00b7ne", "quillt", ",", "ich", "k\u00fc\u00df", "sie", "von", "den", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ade sch\u00f6ne Lieb! so bin ich fortgegangen.", "tokens": ["A\u00b7de", "sch\u00f6\u00b7ne", "Lieb", "!", "so", "bin", "ich", "fort\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun bin ich fern, fern einsam herzbeklommen.", "tokens": ["Nun", "bin", "ich", "fern", ",", "fern", "ein\u00b7sam", "herz\u00b7be\u00b7klom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Abschied war vom sch\u00f6nsten Kind genommen.", "tokens": ["Der", "Ab\u00b7schied", "war", "vom", "sch\u00f6ns\u00b7ten", "Kind", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun rieselt kalter Regen, und mich schauert.", "tokens": ["Nun", "rie\u00b7selt", "kal\u00b7ter", "Re\u00b7gen", ",", "und", "mich", "schau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Bl\u00fcthenbaum im Garten friert und trauert.", "tokens": ["Der", "Bl\u00fc\u00b7then\u00b7baum", "im", "Gar\u00b7ten", "friert", "und", "trau\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er tr\u00e4umt von s\u00fc\u00dfen warmen Sonnenstrahlen.", "tokens": ["Er", "tr\u00e4umt", "von", "s\u00fc\u00b7\u00dfen", "war\u00b7men", "Son\u00b7nen\u00b7strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iam