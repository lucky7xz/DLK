{"dta.poem.1302": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Nun hab ich wiederum ein Jahr gelegt zur\u00fccke/", "tokens": ["Nun", "hab", "ich", "wie\u00b7de\u00b7rum", "ein", "Jahr", "ge\u00b7legt", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Creutz/ Kummer/ Sorg und Angst ist/ leyder/ mein Ge-", "tokens": ["Creutz", "/", "Kum\u00b7mer", "/", "Sorg", "und", "Angst", "ist", "/", "ley\u00b7der", "/", "mein", "Ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "VAFIN", "$(", "ADV", "$(", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "winn/", "tokens": ["winn", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Doch wohl mir/ da\u00df ich noch hindurch gedrungen bin/", "tokens": ["Doch", "wohl", "mir", "/", "da\u00df", "ich", "noch", "hin\u00b7durch", "ge\u00b7drun\u00b7gen", "bin", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$(", "KOUS", "PPER", "ADV", "PAV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch Nebel/ Wolck und Sturm des H\u00f6chsten H\u00fclff er-", "tokens": ["Durch", "Ne\u00b7bel", "/", "Wolck", "und", "Sturm", "des", "H\u00f6chs\u00b7ten", "H\u00fclff", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "NN", "KON", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "blicke.", "tokens": ["bli\u00b7cke", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Der Himmel ist vor mich auff allen Seiten dicke/", "tokens": ["Der", "Him\u00b7mel", "ist", "vor", "mich", "auff", "al\u00b7len", "Sei\u00b7ten", "di\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch meine Wohlfart mu\u00df bey tr\u00fcbeu Tagen bl\u00fchn.", "tokens": ["Doch", "mei\u00b7ne", "Wohl\u00b7fart", "mu\u00df", "bey", "tr\u00fc\u00b7beu", "Ta\u00b7gen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der H\u00f6chste l\u00e4st mich vor am Wehmutts-Karne ziehn/", "tokens": ["Der", "H\u00f6chs\u00b7te", "l\u00e4st", "mich", "vor", "am", "Weh\u00b7mutts\u00b7Kar\u00b7ne", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Damit ich mich dadurch zu bessern Zeiten schicke.", "tokens": ["Da\u00b7mit", "ich", "mich", "da\u00b7durch", "zu", "bes\u00b7sern", "Zei\u00b7ten", "schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "GoTT/ dessen G\u00fcte mich von Jugend auff ern\u00e4hrt/", "tokens": ["GoTT", "/", "des\u00b7sen", "G\u00fc\u00b7te", "mich", "von", "Ju\u00b7gend", "auff", "er\u00b7n\u00e4hrt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PRELAT", "NN", "PRF", "APPR", "NN", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und unverhofftes Gl\u00fcck aus milder Hand beschert/", "tokens": ["Und", "un\u00b7ver\u00b7hoff\u00b7tes", "Gl\u00fcck", "aus", "mil\u00b7der", "Hand", "be\u00b7schert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wird ferner auch f\u00fcr mich vergn\u00fcgte Nothdurfft schaffen.", "tokens": ["Wird", "fer\u00b7ner", "auch", "f\u00fcr", "mich", "ver\u00b7gn\u00fcg\u00b7te", "Noth\u00b7durfft", "schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "APPR", "PPER", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Undanck ist zwar gro\u00df/ doch ist mir auch bekandt/", "tokens": ["Der", "Un\u00b7danck", "ist", "zwar", "gro\u00df", "/", "doch", "ist", "mir", "auch", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$(", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das sein geneigter Schlu\u00df vor Weyrauch-schwacher Hand", "tokens": ["Das", "sein", "ge\u00b7neig\u00b7ter", "Schlu\u00df", "vor", "Wey\u00b7rauch\u00b7schwa\u00b7cher", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Uns seinen Segen l\u00e4st mit vollen Armen raffen.", "tokens": ["Uns", "sei\u00b7nen", "Se\u00b7gen", "l\u00e4st", "mit", "vol\u00b7len", "Ar\u00b7men", "raf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}