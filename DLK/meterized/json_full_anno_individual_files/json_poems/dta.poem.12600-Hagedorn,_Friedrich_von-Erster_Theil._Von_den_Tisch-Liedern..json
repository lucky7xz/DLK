{"dta.poem.12600": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt einmal, ihr muntern Br\u00fcder!", "tokens": ["H\u00f6rt", "ein\u00b7mal", ",", "ihr", "mun\u00b7tern", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Jtzo k\u00f6mmt der Fr\u00fchling wieder,", "tokens": ["Jt\u00b7zo", "k\u00f6mmt", "der", "Fr\u00fch\u00b7ling", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht, er k\u00f6mmt, von Blumen schwer.", "tokens": ["Seht", ",", "er", "k\u00f6mmt", ",", "von", "Blu\u00b7men", "schwer", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "$,", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gebt mir gleich aus diesem Fasse", "tokens": ["Gebt", "mir", "gleich", "aus", "die\u00b7sem", "Fas\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem honigs\u00fcssen Nasse.", "tokens": ["Von", "dem", "ho\u00b7ni\u00b7gs\u00fcs\u00b7sen", "Nas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eilet! Einen Becher her!", "tokens": ["Ei\u00b7let", "!", "Ei\u00b7nen", "Be\u00b7cher", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}