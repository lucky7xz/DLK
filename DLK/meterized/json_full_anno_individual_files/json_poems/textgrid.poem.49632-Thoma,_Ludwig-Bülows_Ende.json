{"textgrid.poem.49632": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "B\u00fclows Ende", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die G'schichte ist traurig zum Lesen;", "tokens": ["Die", "G'\u00b7schich\u00b7te", "ist", "trau\u00b7rig", "zum", "Le\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Sie hat sich geschehen in Kiel.", "tokens": ["Sie", "hat", "sich", "ge\u00b7sche\u00b7hen", "in", "Kiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "VVPP", "APPR", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da ist das Begr\u00e4bnis gewesen", "tokens": ["Da", "ist", "das", "Be\u00b7gr\u00e4b\u00b7nis", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VAPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vom Kanzler, der hinterrucks fiel.", "tokens": ["Vom", "Kanz\u00b7ler", ",", "der", "hin\u00b7ter\u00b7rucks", "fiel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Drei Lumpen sind \u00fcber ihn kommen:", "tokens": ["Drei", "Lum\u00b7pen", "sind", "\u00fc\u00b7ber", "ihn", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein Junker, ein Pfaff, ein Polack,", "tokens": ["E