{"dta.poem.3191": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Welt ist \u00f6d' und leer, und grenzenlos der Raum,", "tokens": ["Die", "Welt", "ist", "\u00f6d'", "und", "leer", ",", "und", "gren\u00b7zen\u00b7los", "der", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nicht die Liebe wohnt mit einem Himmelstraum, \u2014", "tokens": ["Wo", "nicht", "die", "Lie\u00b7be", "wohnt", "mit", "ei\u00b7nem", "Him\u00b7mel\u00b7straum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo nicht die Liebe wohnt, von der, zu der du gehst,", "tokens": ["Wo", "nicht", "die", "Lie\u00b7be", "wohnt", ",", "von", "der", ",", "zu", "der", "du", "gehst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um deren Mittelpunkt du dich im Geiste drehst.", "tokens": ["Um", "de\u00b7ren", "Mit\u00b7tel\u00b7punkt", "du", "dich", "im", "Geis\u00b7te", "drehst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "VVFIN", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum denke, wo du gehst, damit nicht \u00f6d' erscheine", "tokens": ["Drum", "den\u00b7ke", ",", "wo", "du", "gehst", ",", "da\u00b7mit", "nicht", "\u00f6d'", "er\u00b7schei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "KOUS", "PTKNEG", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Welt, da\u00df eine Lieb' auch dort wohnt, irgend eine, \u2014", "tokens": ["Die", "Welt", ",", "da\u00df", "ei\u00b7ne", "Lieb'", "auch", "dort", "wohnt", ",", "ir\u00b7gend", "ei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "ADV", "ADV", "VVFIN", "$,", "ADV", "ART", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da\u00df irgend einer dort tr\u00e4umt seinen Liebestraum;", "tokens": ["Da\u00df", "ir\u00b7gend", "ei\u00b7ner", "dort", "tr\u00e4umt", "sei\u00b7nen", "Lie\u00b7best\u00b7raum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den g\u00f6nn' ihm, tr\u00e4ume mit, und voll sei dir der Raum.", "tokens": ["Den", "g\u00f6nn'", "ihm", ",", "tr\u00e4u\u00b7me", "mit", ",", "und", "voll", "sei", "dir", "der", "Raum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "VAFIN", 