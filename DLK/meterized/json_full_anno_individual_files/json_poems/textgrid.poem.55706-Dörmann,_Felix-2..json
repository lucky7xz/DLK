, "measure": "iambic.tri"}, "line.34": {"text": "T\u00f6dlich das Gift", "tokens": ["T\u00f6d\u00b7lich", "das", "Gift"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "\u00dcberflie\u00dfen zu mir,", "tokens": ["\u00dc\u00b7berf\u00b7lie\u00b7\u00dfen", "zu", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.36": {"text": "Was liegt daran! \u2013", "tokens": ["Was", "liegt", "da\u00b7ran", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PAV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Sterbend noch wollen wir", "tokens": ["Ster\u00b7bend", "noch", "wol\u00b7len", "wir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VMFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.38": {"text": "H\u00f6chster Wonnen t\u00f6dliche Freuden", "tokens": ["H\u00f6chs\u00b7ter", "Won\u00b7nen", "t\u00f6d\u00b7li\u00b7che", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.39": {"text": "Ausgenie\u00dfen.", "tokens": ["Aus\u00b7ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}