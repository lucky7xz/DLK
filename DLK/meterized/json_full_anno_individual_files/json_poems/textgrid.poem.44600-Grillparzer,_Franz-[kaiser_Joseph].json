{"textgrid.poem.44600": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[kaiser Joseph]", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So braucht ihr f\u00fcrder mich denn nun nicht weiter,", "tokens": ["So", "braucht", "ihr", "f\u00fcr\u00b7der", "mich", "denn", "nun", "nicht", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "ADV", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gehoben ist der langentbehrte Schatz,", "tokens": ["Ge\u00b7ho\u00b7ben", "ist", "der", "lan\u00b7gent\u00b7behr\u00b7te", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin euch nur noch der metallne Reiter,", "tokens": ["Ich", "bin", "euch", "nur", "noch", "der", "me\u00b7tall\u00b7ne", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aufs h\u00f6chste gut, zu schm\u00fccken euren Platz.", "tokens": ["Aufs", "h\u00f6chs\u00b7te", "gut", ",", "zu", "schm\u00fc\u00b7cken", "eu\u00b7ren", "Platz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJD", "$,", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bis etwa wieder euch das Volk vonn\u00f6ten", "tokens": ["Bis", "et\u00b7wa", "wie\u00b7der", "euch", "das", "Volk", "von\u00b7n\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht mehr ausreicht eure schwarze Kunst.", "tokens": ["Und", "nicht", "mehr", "aus\u00b7reicht", "eu\u00b7re", "schwar\u00b7ze", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann kehrt zur\u00fcck ihr ohne Schamerr\u00f6ten,", "tokens": ["Dann", "kehrt", "zu\u00b7r\u00fcck", "ihr", "oh\u00b7ne", "Scha\u00b7mer\u00b7r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann komm ich wieder neu bei euch in Gunst.", "tokens": ["Dann", "komm", "ich", "wie\u00b7der", "neu", "bei", "euch", "in", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So, schon bei meines Standbilds erstem Gr\u00fcnden,", "tokens": ["So", ",", "schon", "bei", "mei\u00b7nes", "Stand\u00b7bilds", "ers\u00b7tem", "Gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als rings mein Neffe von Gefahr bedroht,", "tokens": ["Als", "rings", "mein", "Nef\u00b7fe", "von", "Ge\u00b7fahr", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Helfer in der F\u00fcrsten Kreis zu finden,", "tokens": ["Kein", "Hel\u00b7fer", "in", "der", "F\u00fcrs\u00b7ten", "Kreis", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "All\u00fcberall nur Untergang und Tod,", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "nur", "Un\u00b7ter\u00b7gang", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da war mein Bild ein feierlich Gel\u00fcbde,", "tokens": ["Da", "war", "mein", "Bild", "ein", "fei\u00b7er\u00b7lich", "Ge\u00b7l\u00fcb\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu folgen k\u00fcnftig meines Waltens Spur;", "tokens": ["Zu", "fol\u00b7gen", "k\u00fcnf\u00b7tig", "mei\u00b7nes", "Wal\u00b7tens", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Solang das Wetter seinen Himmel tr\u00fcbte,", "tokens": ["So\u00b7lang", "das", "Wet\u00b7ter", "sei\u00b7nen", "Him\u00b7mel", "tr\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hielt er auch halb den notgedrungnen Schwur;", "tokens": ["Hielt", "er", "auch", "halb", "den", "not\u00b7ge\u00b7drung\u00b7nen", "Schwur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch als der Korse dem Geschick erlegen,", "tokens": ["Doch", "als", "der", "Kor\u00b7se", "dem", "Ge\u00b7schick", "er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erlitt der Vorsatz einen tiefen Fall,", "tokens": ["Er\u00b7litt", "der", "Vor\u00b7satz", "ei\u00b7nen", "tie\u00b7fen", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Alte kam zur\u00fcck auf allen Wegen,", "tokens": ["Das", "Al\u00b7te", "kam", "zu\u00b7r\u00fcck", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er schuf Papier, ich war nur noch Metall.", "tokens": ["Er", "schuf", "Pa\u00b7pier", ",", "ich", "war", "nur", "noch", "Me\u00b7tall", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Von neuem kam Gefahr von neuer Seite,", "tokens": ["Von", "neu\u00b7em", "kam", "Ge\u00b7fahr", "von", "neu\u00b7er", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es war das Volk, das diesmal sich erhob,", "tokens": ["Es", "war", "das", "Volk", ",", "das", "dies\u00b7mal", "sich", "er\u00b7hob", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begreiflich allererst, doch toll im Streite,", "tokens": ["Be\u00b7greif\u00b7lich", "al\u00b7le\u00b7rerst", ",", "doch", "toll", "im", "Strei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von mir erwarte Wahnsinn nimmer Lob.", "tokens": ["Von", "mir", "er\u00b7war\u00b7te", "Wahn\u00b7sinn", "nim\u00b7mer", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da kam der Enkel, wie zuvor sein Ahne", "tokens": ["Da", "kam", "der", "En\u00b7kel", ",", "wie", "zu\u00b7vor", "sein", "Ah\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zugleich mit seinem Volk zu mir heran,", "tokens": ["Zu\u00b7gleich", "mit", "sei\u00b7nem", "Volk", "zu", "mir", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Standbild schm\u00fcckte ihrer Hoffnung Fahne,", "tokens": ["Mein", "Stand\u00b7bild", "schm\u00fcck\u00b7te", "ih\u00b7rer", "Hoff\u00b7nung", "Fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Schwur erscholl dort oben vom Altan.", "tokens": ["Ihr", "Schwur", "er\u00b7scholl", "dort", "o\u00b7ben", "vom", "Al\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und weil das Wort, als luftig, nicht gen\u00fcgte,", "tokens": ["Und", "weil", "das", "Wort", ",", "als", "luf\u00b7tig", ",", "nicht", "ge\u00b7n\u00fcg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "KOUS", "ADJD", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ward selbst mein Name, \u00fcberh\u00f6rt seit lang,", "tokens": ["Ward", "selbst", "mein", "Na\u00b7me", ",", "\u00fc\u00b7ber\u00b7h\u00f6rt", "seit", "lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum B\u00fcrgen ihres Bundes, und man f\u00fcgte", "tokens": ["Zum", "B\u00fcr\u00b7gen", "ih\u00b7res", "Bun\u00b7des", ",", "und", "man", "f\u00fcg\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "KON", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihn einem andern bei von minderm Klang.", "tokens": ["Ihn", "ei\u00b7nem", "an\u00b7dern", "bei", "von", "min\u00b7derm", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Was nun seitdem geschah, will ich nicht schelten,", "tokens": ["Was", "nun", "seit\u00b7dem", "ge\u00b7schah", ",", "will", "ich", "nicht", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PAV", "VVFIN", "$,", "VMFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es sucht ein jeder anderswo das Gl\u00fcck,", "tokens": ["Es", "sucht", "ein", "je\u00b7der", "an\u00b7ders\u00b7wo", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nur soll es als das meine nimmer gelten,", "tokens": ["Nur", "soll", "es", "als", "das", "mei\u00b7ne", "nim\u00b7mer", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "KOUS", "PDS", "VVFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gebt meinen Namen mir vorerst zur\u00fcck.", "tokens": ["Gebt", "mei\u00b7nen", "Na\u00b7men", "mir", "vor\u00b7erst", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Tauscht Furcht und \u00dcbermut nach dem Bedarfe,", "tokens": ["Tauscht", "Furcht", "und", "\u00dc\u00b7ber\u00b7mut", "nach", "dem", "Be\u00b7dar\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seid geistig Knecht, damit die Willk\u00fcr frei,", "tokens": ["Seid", "geis\u00b7tig", "Knecht", ",", "da\u00b7mit", "die", "Will\u00b7k\u00fcr", "frei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "NN", "$,", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor allem aber gebt zur\u00fcck die Larve,", "tokens": ["Vor", "al\u00b7lem", "a\u00b7ber", "gebt", "zu\u00b7r\u00fcck", "die", "Lar\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die V\u00f6lkerfastnacht ist ja doch vorbei.", "tokens": ["Die", "V\u00f6l\u00b7ker\u00b7fast\u00b7nacht", "ist", "ja", "doch", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "So braucht ihr f\u00fcrder mich denn nun nicht weiter,", "tokens": ["So", "braucht", "ihr", "f\u00fcr\u00b7der", "mich", "denn", "nun", "nicht", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPER", "ADV", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gehoben ist der langentbehrte Schatz,", "tokens": ["Ge\u00b7ho\u00b7ben", "ist", "der", "lan\u00b7gent\u00b7behr\u00b7te", "Sc