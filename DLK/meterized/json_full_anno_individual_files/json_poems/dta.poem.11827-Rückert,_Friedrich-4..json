{"dta.poem.11827": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zufrieden mit mir selbst, mit Gott und mit der Welt,", "tokens": ["Zu\u00b7frie\u00b7den", "mit", "mir", "selbst", ",", "mit", "Gott", "und", "mit", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$,", "APPR", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab' ich das Gute nur zu thun, das mir gef\u00e4llt.", "tokens": ["Hab'", "ich", "das", "Gu\u00b7te", "nur", "zu", "thun", ",", "das", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht als sei Gutes mir durchaus zu thun beschieden;", "tokens": ["Nicht", "als", "sei", "Gu\u00b7tes", "mir", "durc\u00b7haus", "zu", "thun", "be\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "VAFIN", "NN", "PPER", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wollt' ichs gern nicht thun, wie w\u00e4r' ich denn zufrieden?", "tokens": ["Doch", "wollt'", "ichs", "gern", "nicht", "thun", ",", "wie", "w\u00e4r'", "ich", "denn", "zu\u00b7frie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word