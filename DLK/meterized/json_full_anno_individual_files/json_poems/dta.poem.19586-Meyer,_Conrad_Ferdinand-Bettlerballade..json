{"dta.poem.19586": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Bettlerballade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Prinz Bertarit bewirthet Verona's Bettlerschaft", "tokens": ["Prinz", "Ber\u00b7ta\u00b7rit", "be\u00b7wirt\u00b7het", "Ve\u00b7ro\u00b7na's", "Bett\u00b7ler\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NE", "VVFIN", "NE", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Mit Weizenbrot und Kuchen und edlem Traubensaft.", "tokens": ["Mit", "Wei\u00b7zen\u00b7brot", "und", "Ku\u00b7chen", "und", "ed\u00b7lem", "Trau\u00b7ben\u00b7saft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gebeten ist ein Jeder, der sich mit Lumpen deckt,", "tokens": ["Ge\u00b7be\u00b7ten", "ist", "ein", "Je\u00b7der", ",", "der", "sich", "mit", "Lum\u00b7pen", "deckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "PIS", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der, heischend auf den Br\u00fccken der Etsch, die Rechte reckt.", "tokens": ["Der", ",", "hei\u00b7schend", "auf", "den", "Br\u00fc\u00b7cken", "der", "Etsch", ",", "die", "Rech\u00b7te", "reckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Auf edlen Marmorsesseln im Saale thronen sie,", "tokens": ["Auf", "ed\u00b7len", "Mar\u00b7mor\u00b7ses\u00b7seln", "im", "Saa\u00b7le", "thro\u00b7nen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Durch Riss' und L\u00f6cher gucken Ellbogen, Zeh' und Knie.", "tokens": ["Durch", "Riss'", "und", "L\u00f6\u00b7cher", "gu\u00b7cken", "Ell\u00b7bo\u00b7gen", ",", "Zeh'", "und", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "ADJA", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nicht nach Geburt und W\u00fcrden, sie sitzen grell gemischt,", "tokens": ["Nicht", "nach", "Ge\u00b7burt", "und", "W\u00fcr\u00b7den", ",", "sie", "sit\u00b7zen", "grell", "ge\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jetzt werden noch die Hasen und H\u00fchner aufgetischt.", "tokens": ["Jetzt", "wer\u00b7den", "noch", "die", "Ha\u00b7sen", "und", "H\u00fch\u00b7ner", "auf\u00b7ge\u00b7tischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der tastet nach dem Becher. Er durstet und ist blind.", "tokens": ["Der", "tas\u00b7tet", "nach", "dem", "Be\u00b7cher", ".", "Er", "durs\u00b7tet", "und", "ist", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den Kr\u00fcppel ohne Arme bedient ein frommes Kind.", "tokens": ["Den", "Kr\u00fcp\u00b7pel", "oh\u00b7ne", "Ar\u00b7me", "be\u00b7dient", "ein", "from\u00b7mes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ein reizend stumpfes N\u00e4schen geckt unter strupp'gem Schopf,", "tokens": ["Ein", "rei\u00b7zend", "stump\u00b7fes", "N\u00e4\u00b7schen", "geckt", "un\u00b7ter", "strupp'\u00b7gem", "Schopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mit wildem Mosesbarte prahlt ein Charakterkopf.", "tokens": ["Mit", "wil\u00b7dem", "Mo\u00b7ses\u00b7bar\u00b7te", "prahlt", "ein", "Cha\u00b7rak\u00b7ter\u00b7kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Herzen sind ges\u00e4ttigt. Beginne, Musica!", "tokens": ["Die", "Her\u00b7zen", "sind", "ge\u00b7s\u00e4t\u00b7tigt", ".", "Be\u00b7gin\u00b7ne", ",", "Mu\u00b7si\u00b7ca", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "NN", "$,", "NE", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein Dudelsack, ein Hackbrett und Geig' und Harf' ist da \u2014", "tokens": ["Ein", "Du\u00b7del\u00b7sack", ",", "ein", "Hack\u00b7brett", "und", "Geig'", "und", "Ha\u00b7rf'", "ist", "da"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "KON", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der Prinz, noch schier ein Knabe, wie Gottes Engel sch\u00f6n,", "tokens": ["Der", "Prinz", ",", "noch", "schier", "ein", "Kna\u00b7be", ",", "wie", "Got\u00b7tes", "En\u00b7gel", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "ART", "NN", "$,", "PWAV", "NN", "NE", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Erhebt den vollen Becher und singt durch das Get\u00f6n:", "tokens": ["Er\u00b7hebt", "den", "vol\u00b7len", "Be\u00b7cher", "und", "singt", "durch", "das", "Ge\u00b7t\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u201emit frisch gepfl\u00fcckten Rosen bekr\u00f6n' ich mir das Haupt,", "tokens": ["\u201e", "mit", "frisch", "ge\u00b7pfl\u00fcck\u00b7ten", "Ro\u00b7sen", "be\u00b7kr\u00f6n'", "ich", "mir", "das", "Haupt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJD", "ADJA", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des Reiches eh'rne Krone hat mir der Ohm geraubt.", "tokens": ["Des", "Rei\u00b7ches", "eh'r\u00b7ne", "Kro\u00b7ne", "hat", "mir", "der", "Ohm", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er lie\u00df mir Tag und Sonne! Mein \u00fcbrig Gut ist klein!", "tokens": ["Er", "lie\u00df", "mir", "Tag", "und", "Son\u00b7ne", "!", "Mein", "\u00fcb\u00b7rig", "Gut", "ist", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$.", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So will ich mit den Armen als Armer fr\u00f6hlich sein!\u201c", "tokens": ["So", "will", "ich", "mit", "den", "Ar\u00b7men", "als", "Ar\u00b7mer", "fr\u00f6h\u00b7lich", "sein", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "KOUS", "NN", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Ein Bettler st\u00fcrzt ins Zimmer. \u201eGrumell, wo kommst du her?\u201c", "tokens": ["Ein", "Bett\u00b7ler", "st\u00fcrzt", "ins", "Zim\u00b7mer", ".", "\u201e", "Gru\u00b7mell", ",", "wo", "kommst", "du", "her", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "$(", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Schreckensbleiche stammelt: \u201eIch lauscht' von ungef\u00e4hr,", "tokens": ["Der", "Schre\u00b7ckens\u00b7blei\u00b7che", "stam\u00b7melt", ":", "\u201e", "Ich", "lauscht'", "von", "un\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN", "APPR", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Gebettet an der Hofburg \u2014 Dein Ohm schickt M\u00f6rder aus,", "tokens": ["Ge\u00b7bet\u00b7tet", "an", "der", "Hof\u00b7burg", "Dein", "Ohm", "schickt", "M\u00f6r\u00b7der", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$(", "PPOSAT", "NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Nimm meinen braunen Mantel!\u201c Erzschritt umdr\u00f6hnt das Haus.", "tokens": ["Nimm", "mei\u00b7nen", "brau\u00b7nen", "Man\u00b7tel", "!", "\u201c", "Erz\u00b7schritt", "um\u00b7dr\u00f6hnt", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$.", "$(", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u201edr\u00fcck in die Stirn den Hut dir! Er schattet tief! Geschwind!", "tokens": ["\u201e", "dr\u00fcck", "in", "die", "Stirn", "den", "Hut", "dir", "!", "Er", "schat\u00b7tet", "tief", "!", "Ge\u00b7schwind", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "ART", "NN", "PPER", "$.", "PPER", "VVFIN", "ADJD", "$.", "NN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Da hast du meinen Stecken! Entspring, geliebtes Kind!\u201c", "tokens": ["Da", "hast", "du", "mei\u00b7nen", "Ste\u00b7cken", "!", "Ent\u00b7spring", ",", "ge\u00b7lieb\u00b7tes", "Kind", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$.", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die M\u00f6rder nahen klirrend. Ein Bettler schleicht davon.", "tokens": ["Die", "M\u00f6r\u00b7der", "na\u00b7hen", "klir\u00b7rend", ".", "Ein", "Bett\u00b7ler", "schleicht", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVPP", "$.", "ART", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "\u2014 \u201eWer bist du? Zeig das Antlitz!\u201c Gehobne Dolche drohn.", "tokens": ["\u201e", "Wer", "bist", "du", "?", "Zeig", "das", "Ant\u00b7litz", "!", "\u201c", "Ge\u00b7hob\u00b7ne", "Dol\u00b7che", "drohn", "."], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PWS", "VAFIN", "PPER", "$.", "NN", "ART", "NN", "$.", "$(", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "\u2014 \u201eLa\u00df ihn! Es ist Grumello! Ich kenn' das Loch im Hut!", "tokens": ["\u201e", "La\u00df", "ihn", "!", "Es", "ist", "Gru\u00b7mel\u00b7lo", "!", "Ich", "kenn'", "das", "Loch", "im", "Hut", "!"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "VVIMP", "PPER", "$.", "PPER", "VAFIN", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich kenn' den Ri\u00df im Aermel! Wir opfern edler Blut!\u201c", "tokens": ["Ich", "kenn'", "den", "Ri\u00df", "im", "A\u00b7er\u00b7mel", "!", "Wir", "op\u00b7fern", "ed\u00b7ler", "Blut", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$.", "PPER", "VVFIN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Sie sp\u00e4hen durch die Hallen und suchen Bertarit,", "tokens": ["Sie", "sp\u00e4\u00b7hen", "durch", "die", "Hal\u00b7len", "und", "su\u00b7chen", "Ber\u00b7ta\u00b7rit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Der unter dunkelm Mantel dem dunkeln Tod entflieht.", "tokens": ["Der", "un\u00b7ter", "dun\u00b7kelm", "Man\u00b7tel", "dem", "dun\u00b7keln", "Tod", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Er fuhr in fremde L\u00e4nder und ward darob zum Mann.", "tokens": ["Er", "fuhr", "in", "frem\u00b7de", "L\u00e4n\u00b7der", "und", "ward", "da\u00b7rob", "zum", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "KON", "VAFIN", "PAV", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er kehrte heim gepanzert. Den Ohm erschlug er dann.", "tokens": ["Er", "kehr\u00b7te", "heim", "ge\u00b7pan\u00b7zert", ".", "Den", "Ohm", "er\u00b7schlug", "er", "dann", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "VVPP", "$.", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Verona nahm er st\u00fcrmend in rothem Feuerschein.", "tokens": ["Ve\u00b7ro\u00b7na", "nahm", "er", "st\u00fcr\u00b7mend", "in", "ro\u00b7them", "Feu\u00b7er\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-----+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Am Abend lud der K\u00f6nig Verona's Bettler ein.", "tokens": ["Am", "A\u00b7bend", "lud", "der", "K\u00f6\u00b7nig", "Ve\u00b7ro\u00b7na's", "Bett\u00b7ler", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}