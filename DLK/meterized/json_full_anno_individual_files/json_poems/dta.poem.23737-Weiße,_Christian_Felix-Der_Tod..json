{"dta.poem.23737": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Es sterben Greise,", "tokens": ["Es", "ster\u00b7ben", "Grei\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und sind nicht weise,", "tokens": ["Und", "sind", "nicht", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und wenn man sie nunmehr begr\u00e4bt,", "tokens": ["Und", "wenn", "man", "sie", "nun\u00b7mehr", "be\u00b7gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird sie kein Edler klagen;", "tokens": ["Wird", "sie", "kein", "Ed\u00b7ler", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn man wei\u00df nichts zu sagen,", "tokens": ["Denn", "man", "wei\u00df", "nichts", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Als da\u00df sie lang genug gelebt.", "tokens": ["Als", "da\u00df", "sie", "lang", "ge\u00b7nug", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sollt ich nicht streben", "tokens": ["Sollt", "ich", "nicht", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Also zu leben,", "tokens": ["Al\u00b7so", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df, wenn man mich auch jung begr\u00e4bt,", "tokens": ["Da\u00df", ",", "wenn", "man", "mich", "auch", "jung", "be\u00b7gr\u00e4bt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PIS", "PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Frommen bey mir klagen,", "tokens": ["Die", "From\u00b7men", "bey", "mir", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und zu einander sagen:", "tokens": ["Und", "zu", "ein\u00b7an\u00b7der", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O h\u00e4tt er l\u00e4nger doch gelebt!", "tokens": ["O", "h\u00e4tt", "er", "l\u00e4n\u00b7ger", "doch", "ge\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}