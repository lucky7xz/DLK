{"textgrid.poem.39223": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "Prolog", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ha Nordlandsluft und Nordlandswind!", "tokens": ["Ha", "Nord\u00b7lands\u00b7luft", "und", "Nord\u00b7lands\u00b7wind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Lust verwehter Tage!", "tokens": ["O", "Lust", "ver\u00b7weh\u00b7ter", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie hab' ich dich einst so hei\u00df geminnt,", "tokens": ["Wie", "hab'", "ich", "dich", "einst", "so", "hei\u00df", "ge\u00b7minnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vollbusige Nordlandsage.", "tokens": ["Voll\u00b7bu\u00b7si\u00b7ge", "Nord\u00b7land\u00b7sa\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du rittest mit mir auf Odins Tier,", "tokens": ["Du", "rit\u00b7test", "mit", "mir", "auf", "O\u00b7dins", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "NE", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Acht Hufe hatte der Renner,", "tokens": ["Acht", "Hu\u00b7fe", "hat\u00b7te", "der", "Ren\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Du sa\u00dfest in Walhall' neben mir", "tokens": ["Du", "sa\u00b7\u00dfest", "in", "Wal\u00b7hall'", "ne\u00b7ben", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPER"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Und schenktest den Meth der M\u00e4nner.", "tokens": ["Und", "schenk\u00b7test", "den", "Me\u00b7th", "der", "M\u00e4n\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6rt' als Fey mit dem Wasserfall", "tokens": ["Ich", "h\u00f6rt'", "als", "Fey", "mit", "dem", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dich tote Helden bejammern,", "tokens": ["Dich", "to\u00b7te", "Hel\u00b7den", "be\u00b7jam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und sah dich als Alf in des Berges Hall'", "tokens": ["Und", "sah", "dich", "als", "Alf", "in", "des", "Ber\u00b7ges", "Hall'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Am gl\u00fchenden Golde hammern.", "tokens": ["Am", "gl\u00fc\u00b7hen\u00b7den", "Gol\u00b7de", "ham\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Du fuhrest mit mir \u00fcber'n M\u00e2\u0101lstrom,", "tokens": ["Du", "fuh\u00b7rest", "mit", "mir", "\u00fc\u00b7ber'n", "M\u00e2\u0101l\u00b7strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+--++--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es dampften des Strudels M\u00e4uler,", "tokens": ["Es", "dampf\u00b7ten", "des", "Stru\u00b7dels", "M\u00e4u\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Du webtest in Trondhiems schwarzem Dom", "tokens": ["Du", "web\u00b7test", "in", "Trond\u00b7hiems", "schwar\u00b7zem", "Dom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als D\u00e4mmerung um die Pfeiler.", "tokens": ["Als", "D\u00e4m\u00b7me\u00b7rung", "um", "die", "Pfei\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sah dich \u00fcber die schlafende See", "tokens": ["Ich", "sah", "dich", "\u00fc\u00b7ber", "die", "schla\u00b7fen\u00b7de", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als Schwanenjungfrau schwimmen,", "tokens": ["Als", "Schwa\u00b7nen\u00b7jung\u00b7frau", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sah dich \u00fcber den Gletscherschnee", "tokens": ["Und", "sah", "dich", "\u00fc\u00b7ber", "den", "Glet\u00b7scher\u00b7schnee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als Nordlicht zackig glimmen.", "tokens": ["Als", "Nord\u00b7licht", "za\u00b7ckig", "glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich sah dich \u00fcber die Heide der Schlacht", "tokens": ["Ich", "sah", "dich", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "der", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Als Adler schweigend schweben,", "tokens": ["Als", "Ad\u00b7ler", "schwei\u00b7gend", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und sah dich in dem Auge der Nacht", "tokens": ["Und", "sah", "dich", "in", "dem", "Au\u00b7ge", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Als Witwentr\u00e4ne beben.", "tokens": ["Als", "Wit\u00b7went\u00b7r\u00e4\u00b7ne", "be\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du botest mir deine Wange rot,", "tokens": ["Du", "bo\u00b7test", "mir", "dei\u00b7ne", "Wan\u00b7ge", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du Schlanke, Hohe, Frische,", "tokens": ["Du", "Schlan\u00b7ke", ",", "Ho\u00b7he", ",", "Fri\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJA", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und brachest mir dann das Haferbrot", "tokens": ["Und", "bra\u00b7chest", "mir", "dann", "das", "Ha\u00b7fer\u00b7brot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "An des Normanns rauhem Tische.", "tokens": ["An", "des", "Nor\u00b7manns", "rau\u00b7hem", "Ti\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich sah dich den Busen der Nordlandsdirn'", "tokens": ["Ich", "sah", "dich", "den", "Bu\u00b7sen", "der", "Nord\u00b7lands\u00b7dirn'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Als Freias Schmuck umkreisen,", "tokens": ["Als", "Frei\u00b7as", "Schmuck", "um\u00b7krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du klangest um jede Normannsstirn", "tokens": ["Du", "klan\u00b7gest", "um", "je\u00b7de", "Nor\u00b7mannss\u00b7tirn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Als Helm aus Wielands Eisen.", "tokens": ["Als", "Helm", "aus", "Wie\u00b7lands", "Ei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich sende dir diesen Ku\u00df nach Nord,", "tokens": ["Ich", "sen\u00b7de", "dir", "die\u00b7sen", "Ku\u00df", "nach", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "NN", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er brennt wie Islands Feuer,", "tokens": ["Er", "brennt", "wie", "Is\u00b7lands", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aufjauchzend springt dies Lied an Bord", "tokens": ["Auf\u00b7jauch\u00b7zend", "springt", "dies", "Lied", "an", "Bord"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PDS", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wendet zu dir sein Steuer.", "tokens": ["Und", "wen\u00b7det", "zu", "dir", "sein", "Steu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Mag sich's mit dir auf Nordlands Riff", "tokens": ["Mag", "sich's", "mit", "dir", "auf", "Nord\u00b7lands", "Riff"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als klagende Tanne wiegen,", "tokens": ["Als", "kla\u00b7gen\u00b7de", "Tan\u00b7ne", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und mag's mit dir als Geisterschiff", "tokens": ["Und", "mag's", "mit", "dir", "als", "Geis\u00b7ter\u00b7schiff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "PPER", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch Nordlands Meere fliegen!", "tokens": ["Durch", "Nord\u00b7lands", "Mee\u00b7re", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ha Nordlandsluft und Nordlandswind!", "tokens": ["Ha", "Nord\u00b7lands\u00b7luft", "und", "Nord\u00b7lands\u00b7wind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Lust verwehter Tage!", "tokens": ["O", "Lust", "ver\u00b7weh\u00b7ter", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie hab' ich dich einst so hei\u00df geminnt,", "tokens": ["Wie", "hab'", "ich", "dich", "einst", "so", "hei\u00df", "ge\u00b7minnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vollbusige Nordlandsage.", "tokens": ["Voll\u00b7bu\u00b7si\u00b7ge", "Nord\u00b7land\u00b7sa\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "met