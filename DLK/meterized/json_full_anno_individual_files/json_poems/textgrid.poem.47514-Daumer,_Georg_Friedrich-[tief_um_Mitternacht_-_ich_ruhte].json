{"textgrid.poem.47514": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[tief um Mitternacht - ich ruhte]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief um Mitternacht \u2013 ich ruhte", "tokens": ["Tief", "um", "Mit\u00b7ter\u00b7nacht", "\u2013", "ich", "ruh\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "NN", "$(", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lange schon auf meinem Lager \u2013", "tokens": ["Lan\u00b7ge", "schon", "auf", "mei\u00b7nem", "La\u00b7ger", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da begann es sch\u00f6n zu spuken,", "tokens": ["Da", "be\u00b7gann", "es", "sch\u00f6n", "zu", "spu\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da \u2013 ich hatt' es nicht erwartet \u2013", "tokens": ["Da", "\u2013", "ich", "hatt'", "es", "nicht", "er\u00b7war\u00b7tet", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stellte sich das holdeste", "tokens": ["Stell\u00b7te", "sich", "das", "hol\u00b7des\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Gespenster bei mir ein.", "tokens": ["Der", "Ge\u00b7spens\u00b7ter", "bei", "mir", "ein