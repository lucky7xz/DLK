ens": ["Sei\u00b7ne", "Wel\u00b7len", "dort", "ein", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hemmt sein rauschen, sieht mich weinen,", "tokens": ["Hemmt", "sein", "rau\u00b7schen", ",", "sieht", "mich", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aechzet mir aus Mitleid nach.", "tokens": ["A\u00b7ech\u00b7zet", "mir", "aus", "Mit\u00b7leid", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Doch sie wird einst wiederkehren,", "tokens": ["Doch", "sie", "wird", "einst", "wie\u00b7der\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird den Wankelmuth bereun,", "tokens": ["Wird", "den", "Wan\u00b7kel\u00b7muth", "be\u00b7re\u00b7un", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Aber dann vergebens Thr\u00e4nen", "tokens": ["A\u00b7ber", "dann", "ver\u00b7ge\u00b7bens", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner kalten Asche weihn.", "tokens": ["Mei\u00b7ner", "kal\u00b7ten", "A\u00b7sche", "weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}