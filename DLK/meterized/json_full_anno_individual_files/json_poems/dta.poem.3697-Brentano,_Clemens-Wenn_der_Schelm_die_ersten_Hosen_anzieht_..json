{"dta.poem.3697": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wenn der Schelm die ersten Hosen anzieht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zimmerm\u00e4ntle, Zimmerm\u00e4ntle,               ", "tokens": ["Zim\u00b7mer\u00b7m\u00e4nt\u00b7le", ",", "Zim\u00b7mer\u00b7m\u00e4nt\u00b7le", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leih mir deine Hosen. \u2014", "tokens": ["Leih", "mir", "dei\u00b7ne", "Ho\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Nein, nein, leih dir sie nit,", "tokens": ["Nein", ",", "nein", ",", "leih", "dir", "sie", "nit", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "VVIMP", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie hangen hinterm Ofen!", "tokens": ["Sie", "han\u00b7gen", "hin\u00b7term", "O\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}