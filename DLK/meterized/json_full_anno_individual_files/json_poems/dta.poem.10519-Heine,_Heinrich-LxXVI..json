{"dta.poem.10519": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxXVI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Bist du wirklich mir so feindlich,", "tokens": ["Bist", "du", "wirk\u00b7lich", "mir", "so", "feind\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist du wirklich ganz verwandelt?", "tokens": ["Bist", "du", "wirk\u00b7lich", "ganz", "ver\u00b7wan\u00b7delt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Welt will ich es klagen,", "tokens": ["Al\u00b7ler", "Welt", "will", "ich", "es", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du mich so schlecht behandelt.", "tokens": ["Da\u00df", "du", "mich", "so", "schlecht", "be\u00b7han\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O Ihr undankbaren Lippen,", "tokens": ["O", "Ihr", "un\u00b7dank\u00b7ba\u00b7ren", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sagt, wie k\u00f6nnt Ihr Schlimmes sagen", "tokens": ["Sagt", ",", "wie", "k\u00f6nnt", "Ihr", "Schlim\u00b7mes", "sa\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "VVFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von dem Manne, der so liebend", "tokens": ["Von", "dem", "Man\u00b7ne", ",", "der", "so", "lie\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch gek\u00fc\u00dft, in sch\u00f6nen Tagen?", "tokens": ["Euch", "ge\u00b7k\u00fc\u00dft", ",", "in", "sch\u00f6\u00b7nen", "Ta\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}