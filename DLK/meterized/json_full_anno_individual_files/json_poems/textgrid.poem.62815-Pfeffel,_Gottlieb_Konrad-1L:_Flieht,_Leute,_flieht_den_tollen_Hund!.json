{"textgrid.poem.62815": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Flieht, Leute, flieht den tollen Hund!", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Flieht, Leute, flieht den tollen Hund!", "tokens": ["Flieht", ",", "Leu\u00b7te", ",", "flieht", "den", "tol\u00b7len", "Hund", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So rief zu Neros Zeit ein Rudel blasser Jungen,", "tokens": ["So", "rief", "zu", "Ne\u00b7ros", "Zeit", "ein", "Ru\u00b7del", "blas\u00b7ser", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die mit genauer Noth des Unthiers Wuth entsprungen.", "tokens": ["Die", "mit", "ge\u00b7nau\u00b7er", "Noth", "des", "Un\u00b7thiers", "Wuth", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Warnung floh von Mund zu Mund", "tokens": ["Die", "War\u00b7nung", "floh", "von", "Mund", "zu", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durchs halbe Rom. Es lief was laufen konnte.", "tokens": ["Durchs", "hal\u00b7be", "Rom", ".", "Es", "lief", "was", "lau\u00b7fen",