{"textgrid.poem.65766": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "40", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm meine Lieben alle, mein Gespiele:", "tokens": ["Nimm", "mei\u00b7ne", "Lie\u00b7ben", "al\u00b7le", ",", "mein", "Ge\u00b7spie\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "PIS", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welch neues Eigentum erwirbst du dran?", "tokens": ["Welch", "neu\u00b7es", "Ei\u00b7gen\u00b7tum", "er\u00b7wirbst", "du", "dran", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nicht Liebe, die als Liebchen dir gefiele;", "tokens": ["Nicht", "Lie\u00b7be", ",", "die", "als", "Lieb\u00b7chen", "dir", "ge\u00b7fie\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn meine ganze war vorl\u00e4ngst dir untertan.", "tokens": ["Denn", "mei\u00b7ne", "gan\u00b7ze", "war", "vor\u00b7l\u00e4ngst", "dir", "un\u00b7ter\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VAFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nimmst du f\u00fcr meine Liebe nun mein Lieb,", "tokens": ["Nimmst", "du", "f\u00fcr", "mei\u00b7ne", "Lie\u00b7be", "nun", "mein", "Lieb", ","], "token_info": ["word", "word", "word"