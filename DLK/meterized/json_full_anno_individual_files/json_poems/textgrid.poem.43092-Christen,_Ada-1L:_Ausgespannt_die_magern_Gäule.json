{"textgrid.poem.43092": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ausgespannt die magern G\u00e4ule", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ausgespannt die magern G\u00e4ule", "tokens": ["Aus\u00b7ge\u00b7spannt", "die", "ma\u00b7gern", "G\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem morschen Thespiskarren;", "tokens": ["Von", "dem", "mor\u00b7schen", "Thes\u00b7pis\u00b7kar\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Engagirt bin ich f\u00fcr's Leben,", "tokens": ["En\u00b7ga\u00b7girt", "bin", "ich", "f\u00fcr's", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer weiter wird gefahren.", "tokens": ["Nim\u00b7mer", "wei\u00b7ter", "wird", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf dem kleinen St\u00fcckchen Erde", "tokens": ["Auf", "dem", "klei\u00b7nen", "St\u00fcck\u00b7chen", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Bude festgestellt \u2013", "tokens": ["Ist", "die", "Bu\u00b7de", "fest\u00b7ge\u00b7stellt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Kreis, der oft copirte,", "tokens": ["Und", "der", "Kreis", ",", "der", "oft", "co\u00b7pir\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist nun wirklich meine Welt.", "tokens": ["Ist", "nun", "wirk\u00b7lich", "mei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ausgespannt die magern G\u00e4ule", "tokens": ["Aus\u00b7ge\u00b7spannt", "die", "ma\u00b7gern", "G\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem morschen Thespiskarren;", "tokens": ["Von", "dem", "mor\u00b7schen", "Thes\u00b7pis\u00b7kar\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Engagirt bin ich f\u00fcr's Leben,", "tokens": ["En\u00b7ga\u00b7girt", "bin", "ich", "f\u00fcr's", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimmer weiter wird gefahren.", "tokens": ["Nim\u00b7mer", "wei\u00b7ter", "wird", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf dem kleinen St\u00fcckchen Erde", "tokens": ["Auf", "dem", "klei\u00b7nen", "St\u00fcck\