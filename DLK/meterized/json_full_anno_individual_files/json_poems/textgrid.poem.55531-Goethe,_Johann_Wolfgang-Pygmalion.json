{"textgrid.poem.55531": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Pygmalion", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal ein Hagenstolz,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "Ha\u00b7gen\u00b7stolz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hie\u00df Pygmalion;", "tokens": ["Der", "hie\u00df", "Pyg\u00b7ma\u00b7li\u00b7on", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er machte manches Bild von Holz.", "tokens": ["Er", "mach\u00b7te", "man\u00b7ches", "Bild", "von", "Holz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Marmor und von Ton.", "tokens": ["Von", "Mar\u00b7mor", "und", "von", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und dieses war sein Zeitvertreib", "tokens": ["Und", "die\u00b7ses", "war", "sein", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle seine Lust.", "tokens": ["Und", "al\u00b7le", "sei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein junges, sch\u00f6nes, sanftes Weib", "tokens": ["Kein", "jun\u00b7ges", ",", "sch\u00f6\u00b7nes", ",", "sanf\u00b7tes", "Weib"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erw\u00e4rmte seine Brust.", "tokens": ["Er\u00b7w\u00e4rm\u00b7te", "sei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denn er war klug und furchte sehr", "tokens": ["Denn", "er", "war", "klug", "und", "furch\u00b7te", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der H\u00f6rner schwer Gewicht;", "tokens": ["Der", "H\u00f6r\u00b7ner", "schwer", "Ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn schon seit vielen Jahren her", "tokens": ["Denn", "schon", "seit", "vie\u00b7len", "Jah\u00b7ren", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Traut man den Weibern nicht.", "tokens": ["Traut", "man", "den", "Wei\u00b7bern", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Doch es sei einer noch so wild,", "tokens": ["Doch", "es", "sei", "ei\u00b7ner", "noch", "so", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gern wird er M\u00e4dchen sehn.", "tokens": ["Gern", "wird", "er", "M\u00e4d\u00b7chen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum macht' er sich gar manches Bild", "tokens": ["Drum", "macht'", "er", "sich", "gar", "man\u00b7ches", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von M\u00e4dchen jung und sch\u00f6n.", "tokens": ["Von", "M\u00e4d\u00b7chen", "jung", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Einst hatt er sich ein Bild gemacht,", "tokens": ["Einst", "hatt", "er", "sich", "ein", "Bild", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es staunte, wer es sah;", "tokens": ["Es", "staun\u00b7te", ",", "wer", "es", "sah", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es stand in aller Sch\u00f6nheit Pracht", "tokens": ["Es", "stand", "in", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein junges M\u00e4dchen da.", "tokens": ["Ein", "jun\u00b7ges", "M\u00e4d\u00b7chen", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie schien belebt und weich und warm,", "tokens": ["Sie", "schien", "be\u00b7lebt", "und", "weich", "und", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War nur von kaltem Stein;", "tokens": ["War", "nur", "von", "kal\u00b7tem", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die hohe Brust, der wei\u00dfe Arm", "tokens": ["Die", "ho\u00b7he", "Brust", ",", "der", "wei\u00b7\u00dfe", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lud zur Umarmung ein.", "tokens": ["Lud", "zur", "Um\u00b7ar\u00b7mung", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Auge war empor gewandt,", "tokens": ["Das", "Au\u00b7ge", "war", "em\u00b7por", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Halb auf zum Ku\u00df der Mund.", "tokens": ["Halb", "auf", "zum", "Ku\u00df", "der", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sah das Werk von seiner Hand,", "tokens": ["Er", "sah", "das", "Werk", "von", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Amor scho\u00df ihn wund.", "tokens": ["Und", "A\u00b7mor", "scho\u00df", "ihn", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er war von Liebe ganz erf\u00fcllt,", "tokens": ["Er", "war", "von", "Lie\u00b7be", "ganz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was die Liebe tut!", "tokens": ["Und", "was", "die", "Lie\u00b7be", "tut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er geht, umarmt das kalte Bild,", "tokens": ["Er", "geht", ",", "um\u00b7armt", "das", "kal\u00b7te", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umarmet es mit Glut.", "tokens": ["Um\u00b7ar\u00b7met", "es", "mit", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da trat ein guter Freund herein", "tokens": ["Da", "trat", "ein", "gu\u00b7ter", "Freund", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sah dem Narren zu,", "tokens": ["Und", "sah", "dem", "Nar\u00b7ren", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sprach: \u00bbDu umarmest harten Stein,", "tokens": ["Sprach", ":", "\u00bb", "Du", "um\u00b7ar\u00b7mest", "har\u00b7ten", "Stein", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O welch ein Tor bist du!", "tokens": ["O", "welch", "ein", "Tor", "bist", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ich kauft ein sch\u00f6nes M\u00e4dchen mir,", "tokens": ["Ich", "kauft", "ein", "sch\u00f6\u00b7nes", "M\u00e4d\u00b7chen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Willst du, ich geb dir sie?", "tokens": ["Willst", "du", ",", "ich", "geb", "dir", "sie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sie gef\u00e4llt gewi\u00dflich dir", "tokens": ["Und", "sie", "ge\u00b7f\u00e4llt", "ge\u00b7wi\u00df\u00b7lich", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weit besser als wie die.", "tokens": ["Weit", "bes\u00b7ser", "als", "wie", "die", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "KOKOM", "ART", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.11": {"line.1": {"text": "Sag, ob du es zufrieden bist \u2013\u00ab", "tokens": ["Sag", ",", "ob", "du", "es", "zu\u00b7frie\u00b7den", "bist", "\u2013", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sah es nun wohl ein,", "tokens": ["Er", "sah", "es", "nun", "wohl", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein M\u00e4dchen, das lebendig ist,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "das", "le\u00b7ben\u00b7dig", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sei besser als von Stein.", "tokens": ["Sei", "bes\u00b7ser", "als", "von", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er spricht zu seinem Freunde: \u00bbJa.\u00ab", "tokens": ["Er", "spricht", "zu", "sei\u00b7nem", "Freun\u00b7de", ":", "\u00bb", "Ja", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$(", "PTKANT", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der geht und holt sie her.", "tokens": ["Der", "geht", "und", "holt", "sie", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er gl\u00fchte schon, eh er sie sah,", "tokens": ["Er", "gl\u00fch\u00b7te", "schon", ",", "eh", "er", "sie", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Jetzt gl\u00fcht er zweimal mehr.", "tokens": ["Jetzt", "gl\u00fcht", "er", "zwei\u00b7mal", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er atmet tief, sein Herze schlug,", "tokens": ["Er", "at\u00b7met", "tief", ",", "sein", "Her\u00b7ze", "schlug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er eilt, und ohne Trau", "tokens": ["Er", "eilt", ",", "und", "oh\u00b7ne", "Trau"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimmt er \u2013 man ist nicht immer klug \u2013,", "tokens": ["Nimmt", "er", "\u2013", "man", "ist", "nicht", "im\u00b7mer", "klug", "\u2013", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$(", "PIS", "VAFIN", "PTKNEG", "ADV", "ADJD", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nimmt er sie sich, zur Frau.", "tokens": ["Nimmt", "er", "sie", "sich", ",", "zur", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PRF", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Flieht, Freunde, ja die Liebe nicht,", "tokens": ["Flieht", ",", "Freun\u00b7de", ",", "ja", "die", "Lie\u00b7be", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn niemand flieht ihr Reich:", "tokens": ["Denn", "nie\u00b7mand", "flieht", "ihr", "Reich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn euch Amor einmal kriegt,", "tokens": ["Und", "wenn", "euch", "A\u00b7mor", "ein\u00b7mal", "kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann ist es aus mit euch.", "tokens": ["Dann", "ist", "es", "aus", "mit", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Wer wild ist, alle M\u00e4dchen flieht,", "tokens": ["Wer", "wild", "ist", ",", "al\u00b7le", "M\u00e4d\u00b7chen", "flieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich unempfindlich glaubt,", "tokens": ["Sich", "un\u00b7emp\u00b7find\u00b7lich", "glaubt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem ist, wenn er ein M\u00e4dchen sieht,", "tokens": ["Dem", "ist", ",", "wenn", "er", "ein", "M\u00e4d\u00b7chen", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herze gleich geraubt.", "tokens": ["Das", "Her\u00b7ze", "gleich", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Drum seht oft M\u00e4dchen, k\u00fcsset sie,", "tokens": ["Drum", "seht", "oft", "M\u00e4d\u00b7chen", ",", "k\u00fcs\u00b7set", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und liebt sie auch wohl gar,", "tokens": ["Und", "liebt", "sie", "auch", "wohl", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gew\u00f6hnt euch dran, und werdet nie", "tokens": ["Ge\u00b7w\u00f6hnt", "euch", "dran", ",", "und", "wer\u00b7det", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "PPER", "PAV", "$,", "KON", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Tor, wie jener war.", "tokens": ["Ein", "Tor", ",", "wie", "je\u00b7ner", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PDS", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Nun, lieben Freunde, merkt euch dies", "tokens": ["Nun", ",", "lie\u00b7ben", "Freun\u00b7de", ",", "merkt", "euch", "dies"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "PDS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und folget mir genau;", "tokens": ["Und", "fol\u00b7get", "mir", "ge\u00b7nau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonst straft euch Amor ganz gewi\u00df", "tokens": ["Sonst", "straft", "euch", "A\u00b7mor", "ganz", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gibt euch eine Frau.", "tokens": ["Und", "gibt", "euch", "ei\u00b7ne", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Es war einmal ein Hagenstolz,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "Ha\u00b7gen\u00b7stolz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der hie\u00df Pygmalion;", "tokens": ["Der", "hie\u00df", "Pyg\u00b7ma\u00b7li\u00b7on", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er machte manches Bild von Holz.", "tokens": ["Er", "mach\u00b7te", "man\u00b7ches", "Bild", "von", "Holz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Marmor und von Ton.", "tokens": ["Von", "Mar\u00b7mor", "und", "von", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Und dieses war sein Zeitvertreib", "tokens": ["Und", "die\u00b7ses", "war", "sein", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und alle seine Lust.", "tokens": ["Und", "al\u00b7le", "sei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein junges, sch\u00f6nes, sanftes Weib", "tokens": ["Kein", "jun\u00b7ges", ",", "sch\u00f6\u00b7nes", ",", "sanf\u00b7tes", "Weib"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erw\u00e4rmte seine Brust.", "tokens": ["Er\u00b7w\u00e4rm\u00b7te", "sei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Denn er war klug und furchte sehr", "tokens": ["Denn", "er", "war", "klug", "und", "furch\u00b7te", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der H\u00f6rner schwer Gewicht;", "tokens": ["Der", "H\u00f6r\u00b7ner", "schwer", "Ge\u00b7wicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn schon seit vielen Jahren her", "tokens": ["Denn", "schon", "seit", "vie\u00b7len", "Jah\u00b7ren", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Traut man den Weibern nicht.", "tokens": ["Traut", "man", "den", "Wei\u00b7bern", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKNEG", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.21": {"line.1": {"text": "Doch es sei einer noch so wild,", "tokens": ["Doch", "es", "sei", "ei\u00b7ner", "noch", "so", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gern wird er M\u00e4dchen sehn.", "tokens": ["Gern", "wird", "er", "M\u00e4d\u00b7chen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum macht' er sich gar manches Bild", "tokens": ["Drum", "macht'", "er", "sich", "gar", "man\u00b7ches", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von M\u00e4dchen jung und sch\u00f6n.", "tokens": ["Von", "M\u00e4d\u00b7chen", "jung", "und", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Einst hatt er sich ein Bild gemacht,", "tokens": ["Einst", "hatt", "er", "sich", "ein", "Bild", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es staunte, wer es sah;", "tokens": ["Es", "staun\u00b7te", ",", "wer", "es", "sah", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es stand in aller Sch\u00f6nheit Pracht", "tokens": ["Es", "stand", "in", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein junges M\u00e4dchen da.", "tokens": ["Ein", "jun\u00b7ges", "M\u00e4d\u00b7chen", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Sie schien belebt und weich und warm,", "tokens": ["Sie", "schien", "be\u00b7lebt", "und", "weich", "und", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War nur von kaltem Stein;", "tokens": ["War", "nur", "von", "kal\u00b7tem", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die hohe Brust, der wei\u00dfe Arm", "tokens": ["Die", "ho\u00b7he", "Brust", ",", "der", "wei\u00b7\u00dfe", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lud zur Umarmung ein.", "tokens": ["Lud", "zur", "Um\u00b7ar\u00b7mung", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Das Auge war empor gewandt,", "tokens": ["Das", "Au\u00b7ge", "war", "em\u00b7por", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Halb auf zum Ku\u00df der Mund.", "tokens": ["Halb", "auf", "zum", "Ku\u00df", "der", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sah das Werk von seiner Hand,", "tokens": ["Er", "sah", "das", "Werk", "von", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Amor scho\u00df ihn wund.", "tokens": ["Und", "A\u00b7mor", "scho\u00df", "ihn", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Er war von Liebe ganz erf\u00fcllt,", "tokens": ["Er", "war", "von", "Lie\u00b7be", "ganz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und was die Liebe tut!", "tokens": ["Und", "was", "die", "Lie\u00b7be", "tut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er geht, umarmt das kalte Bild,", "tokens": ["Er", "geht", ",", "um\u00b7armt", "das", "kal\u00b7te", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umarmet es mit Glut.", "tokens": ["Um\u00b7ar\u00b7met", "es", "mit", "Glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Da trat ein guter Freund herein", "tokens": ["Da", "trat", "ein", "gu\u00b7ter", "Freund", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sah dem Narren zu,", "tokens": ["Und", "sah", "dem", "Nar\u00b7ren", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sprach: \u00bbDu umarmest harten Stein,", "tokens": ["Sprach", ":", "\u00bb", "Du", "um\u00b7ar\u00b7mest", "har\u00b7ten", "Stein", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O welch ein Tor bist du!", "tokens": ["O", "welch", "ein", "Tor", "bist", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure