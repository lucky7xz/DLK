{"textgrid.poem.24141": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ritten einst die tapfern Troubadours", "tokens": ["Hier", "rit\u00b7ten", "einst", "die", "tap\u00b7fern", "Trou\u00b7ba\u00b7dours"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Schwert und Laute ihrer Liebe nach;", "tokens": ["Mit", "Schwert", "und", "Lau\u00b7te", "ih\u00b7rer", "Lie\u00b7be", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier gl\u00fchte einst das Gl\u00fcck der gro\u00dfen Kunst,", "tokens": ["Hier", "gl\u00fch\u00b7te", "einst", "das", "Gl\u00fcck", "der", "gro\u00b7\u00dfen", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die wie die Sonne der Provence schien:", "tokens": ["Die", "wie", "die", "Son\u00b7ne", "der", "Pro\u00b7ven\u00b7ce", "schien", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein goldnes Siegeszeichen, ein Juwel,", "tokens": ["Ein", "gold\u00b7nes", "Sie\u00b7ges\u00b7zei\u00b7chen", ",", "ein", "Ju\u00b7wel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der sch\u00f6nsten Tage sch\u00f6nster Schmuck. Es sprang", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Ta\u00b7ge", "sch\u00f6ns\u00b7ter", "Schmuck", ".", "Es", "sprang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das Lied gleich einem sch\u00f6nen Pagen froh", "tokens": ["Das", "Lied", "gleich", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Pa\u00b7gen", "froh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Den Frauen in den Schoo\u00df. Doch manchmal wars", "tokens": ["Den", "Frau\u00b7en", "in", "den", "Schoo\u00df", ".", "Doch", "manch\u00b7mal", "wars"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "KON", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie Mistralwind und fegte durch das Land", "tokens": ["Wie", "Mis\u00b7tral\u00b7wind", "und", "feg\u00b7te", "durch", "das", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und trieb die Wolken und zertr\u00fcmmerte,", "tokens": ["Und", "trieb", "die", "Wol\u00b7ken", "und", "zer\u00b7tr\u00fcm\u00b7mer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Was alt und morsch war. Sieg und Segen trug", "tokens": ["Was", "alt", "und", "morsch", "war", ".", "Sieg", "und", "Se\u00b7gen", "trug"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$.", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Des Verses Fl\u00fcgel, der sch\u00f6n gl\u00e4nzende,", "tokens": ["Des", "Ver\u00b7ses", "Fl\u00fc\u00b7gel", ",", "der", "sch\u00f6n", "gl\u00e4n\u00b7zen\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.13": {"text": "Durch diese L\u00fcfte voller Blumenduft,", "tokens": ["Durch", "die\u00b7se", "L\u00fcf\u00b7te", "vol\u00b7ler", "Blu\u00b7men\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Und Liebe l\u00e4chelte dem Liede zu.", "tokens": ["Und", "Lie\u00b7be", "l\u00e4\u00b7chel\u00b7te", "dem", "Lie\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In diesen Liedern war kein m\u00fcder Ton,", "tokens": ["In", "die\u00b7sen", "Lie\u00b7dern", "war", "kein", "m\u00fc\u00b7der", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und auch die Traurigkeit war stolz und stark,", "tokens": ["Und", "auch", "die", "Trau\u00b7rig\u00b7keit", "war", "stolz", "und", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn adelig war noch die Kunst des Lieds,", "tokens": ["Denn", "a\u00b7de\u00b7lig", "war", "noch", "die", "Kunst", "des", "Lieds", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und wer zu sch\u00f6nen Frauen sich verma\u00df", "tokens": ["Und", "wer", "zu", "sch\u00f6\u00b7nen", "Frau\u00b7en", "sich", "ver\u00b7ma\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Stimme zu erheben und das Herz,", "tokens": ["Die", "Stim\u00b7me", "zu", "er\u00b7he\u00b7ben", "und", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der wu\u00dfte, was sich ziemt. So wu\u00dft er auch,", "tokens": ["Der", "wu\u00df\u00b7te", ",", "was", "sich", "ziemt", ".", "So", "wu\u00dft", "er", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PRELS", "PRF", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df nicht f\u00fcr Alles Worte ziemlich sind", "tokens": ["Da\u00df", "nicht", "f\u00fcr", "Al\u00b7les", "Wor\u00b7te", "ziem\u00b7lich", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "PIAT", "NN", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und Schweigen eine edle Kunst der Herzen ist,", "tokens": ["Und", "Schwei\u00b7gen", "ei\u00b7ne", "ed\u00b7le", "Kunst", "der", "Her\u00b7zen", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die eher brechen, als schamlos den Gram", "tokens": ["Die", "e\u00b7her", "bre\u00b7chen", ",", "als", "scham\u00b7los", "den", "Gram"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "$,", "KOUS", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Schw\u00e4che zeigen. \u2013 Ach, wir reden viel", "tokens": ["Der", "Schw\u00e4\u00b7che", "zei\u00b7gen", ".", "\u2013", "Ach", ",", "wir", "re\u00b7den", "viel"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "$(", "ITJ", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Von neuen T\u00f6nen und von neuer Kunst,", "tokens": ["Von", "neu\u00b7en", "T\u00f6\u00b7nen", "und", "von", "neu\u00b7er", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und unsre Herzen sind so j\u00e4mmerlich,", "tokens": ["Und", "uns\u00b7re", "Her\u00b7zen", "sind", "so", "j\u00e4m\u00b7mer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Da\u00df uns die Knechte jener Troubadours", "tokens": ["Da\u00df", "uns", "die", "Knech\u00b7te", "je\u00b7ner", "Trou\u00b7ba\u00b7dours"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Verachten w\u00fcrden, s\u00e4hen sie, wie wir", "tokens": ["Ver\u00b7ach\u00b7ten", "w\u00fcr\u00b7den", ",", "s\u00e4\u00b7hen", "sie", ",", "wie", "wir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Schamlos entbl\u00f6\u00dfen, was so ekel ist:", "tokens": ["Scham\u00b7los", "ent\u00b7bl\u00f6\u00b7\u00dfen", ",", "was", "so", "e\u00b7kel", "ist", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Das Tr\u00fcbe, Dumpfe, Schwache, all die Qual", "tokens": ["Das", "Tr\u00fc\u00b7be", ",", "Dump\u00b7fe", ",", "Schwa\u00b7che", ",", "all", "die", "Qual"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "ADJA", "$,", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Des machtlos ungeb\u00e4ndigten, den Satz", "tokens": ["Des", "macht\u00b7los", "un\u00b7ge\u00b7b\u00e4n\u00b7dig\u00b7ten", ",", "den", "Satz"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der Seele voller Krampf und Mi\u00dfbegier.", "tokens": ["Der", "See\u00b7le", "vol\u00b7ler", "Krampf", "und", "Mi\u00df\u00b7be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir wollen f\u00fcrder nicht so \u00fcppig sein", "tokens": ["Wir", "wol\u00b7len", "f\u00fcr\u00b7der", "nicht", "so", "\u00fcp\u00b7pig", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In gro\u00dfen Worten und Versprechungen", "tokens": ["In", "gro\u00b7\u00dfen", "Wor\u00b7ten", "und", "Ver\u00b7spre\u00b7chun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Von neuen Weisen einer neuen Kunst.", "tokens": ["Von", "neu\u00b7en", "Wei\u00b7sen", "ei\u00b7ner", "neu\u00b7en", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir wollen wieder schweigen lernen, und die Zucht,", "tokens": ["Wir", "wol\u00b7len", "wie\u00b7der", "schwei\u00b7gen", "ler\u00b7nen", ",", "und", "die", "Zucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "VVINF", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Adelsmeisterin, angehn, da\u00df sie", "tokens": ["Die", "A\u00b7dels\u00b7meis\u00b7te\u00b7rin", ",", "an\u00b7gehn", ",", "da\u00df", "sie"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wachsam und strenge bei uns sei, wenn wir", "tokens": ["Wach\u00b7sam", "und", "s