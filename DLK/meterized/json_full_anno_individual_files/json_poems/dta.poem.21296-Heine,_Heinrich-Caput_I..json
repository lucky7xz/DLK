{"dta.poem.21296": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kenne die Weise, ich kenne den Text,", "tokens": ["Ich", "ken\u00b7ne", "die", "Wei\u00b7se", ",", "ich", "ken\u00b7ne", "den", "Text", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich kenn\u2019 auch die Herren Verfasser;", "tokens": ["Ich", "kenn'", "auch", "die", "Her\u00b7ren", "Ver\u00b7fas\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ich wei\u00df, sie tranken heimlich Wein", "tokens": ["Ich", "wei\u00df", ",", "sie", "tran\u00b7ken", "heim\u00b7lich", "Wein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und predigten \u00f6ffentlich Wasser.", "tokens": ["Und", "pre\u00b7dig\u00b7ten", "\u00f6f\u00b7fent\u00b7lich", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Ein neues Lied, ein besseres Lied,", "tokens": ["Ein", "neu\u00b7es", "Lied", ",", "ein", "bes\u00b7se\u00b7res", "Lied", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "O Freunde, will ich Euch dichten!", "tokens": ["O", "Freun\u00b7de", ",", "will", "ich", "Euch", "dich\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VMFIN", "PPER", "PPER", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wir wollen hier auf Erden schon", "tokens": ["Wir", "wol\u00b7len", "hier", "auf", "Er\u00b7den", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Himmelreich errichten.", "tokens": ["Das", "Him\u00b7mel\u00b7reich", "er\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir wollen auf Erden gl\u00fccklich seyn,", "tokens": ["Wir", "wol\u00b7len", "auf", "Er\u00b7den", "gl\u00fcck\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wollen nicht mehr darben;", "tokens": ["Und", "wol\u00b7len", "nicht", "mehr", "dar\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verschlemmen soll nicht der faule Bauch,", "tokens": ["Ver\u00b7schlem\u00b7men", "soll", "nicht", "der", "fau\u00b7le", "Bauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Was flei\u00dfige H\u00e4nde erwarben.", "tokens": ["Was", "flei\u00b7\u00dfi\u00b7ge", "H\u00e4n\u00b7de", "er\u00b7war\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Es w\u00e4chst hienieden Brod genug", "tokens": ["Es", "w\u00e4chst", "hien\u00b7ie\u00b7den", "Brod", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr alle Menschenkinder,", "tokens": ["F\u00fcr", "al\u00b7le", "Men\u00b7schen\u00b7kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch Rosen und Myrthen, Sch\u00f6nheit und Lust,", "tokens": ["Auch", "Ro\u00b7sen", "und", "Myr\u00b7then", ",", "Sch\u00f6n\u00b7heit", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Zuckererbsen nicht minder.", "tokens": ["Und", "Zu\u00b7cker\u00b7erb\u00b7sen", "nicht", "min\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}