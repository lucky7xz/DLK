ens": ["und", "in", "Ja\u00b7pan", "ist", "al\u00b7les", "so", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Da sitzt die Geisha. Ihr Haar gl\u00e4nzt wie Lack.", "tokens": ["Da", "sitzt", "die", "Geis\u00b7ha", ".", "Ihr", "Haar", "gl\u00e4nzt", "wie", "Lack", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPOSAT", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Leise duftet die Rose.", "tokens": ["Lei\u00b7se", "duf\u00b7tet", "die", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Vor ihr steht plaudernd im strahlenden Tag", "tokens": ["Vor", "ihr", "steht", "plau\u00b7dernd", "im", "strah\u00b7len\u00b7den", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "kr\u00e4ftig der junge Matrose.", "tokens": ["kr\u00e4f\u00b7tig", "der", "jun\u00b7ge", "Mat\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Und er erz\u00e4hlt diesem seidenen Kind", "tokens": ["Und", "er", "er\u00b7z\u00e4hlt", "die\u00b7sem", "sei\u00b7de\u00b7nen", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PDAT", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.6": {"text": "davon, wie gro\u00df seine Landsleute sind.", "tokens": ["da\u00b7von", ",", "wie", "gro\u00df", "sei\u00b7ne", "Lands\u00b7leu\u00b7te", "sind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Stra\u00dfe und Saal", "tokens": ["Stra\u00b7\u00dfe", "und", "Saal"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "pyramidal.", "tokens": ["py\u00b7ra\u00b7mi\u00b7dal", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Sieh, und die Kleine wundert sich blo\u00df \u2013", "tokens": ["Sieh", ",", "und", "die", "Klei\u00b7ne", "wun\u00b7dert", "sich", "blo\u00df", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "ART", "ADJA", "VVFIN", "PRF", "ADV", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "denkt sich: Wie mag das wohl sein?", "tokens": ["denkt", "sich", ":", "Wie", "mag", "das", "wohl", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$.", "PWAV", "VMFIN", "PDS", "ADV", "VAINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "In Europa ist alles so gro\u00df, so gro\u00df \u2013", "tokens": ["In", "Eu\u00b7ro\u00b7pa", "ist", "al\u00b7les", "so", "gro\u00df", ",", "so", "gro\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PIS", "ADV", "ADJD", "$,", "ADV", "ADJD", "$("], "meter": "+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.12": {"text": "und in Japan ist alles so klein!", "tokens": ["und", "in", "Ja\u00b7pan", "ist", "al\u00b7les", "so", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Da ist ein Wald \u2013 ein ganz kleiner Wald \u2013", "tokens": ["Da", "ist", "ein", "Wald", "\u2013", "ein", "ganz", "klei\u00b7ner", "Wald", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "ART", "ADV", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "abendlich d\u00e4mmern die Stunden.", "tokens": ["a\u00b7bend\u00b7lich", "d\u00e4m\u00b7mern", "die", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Horch! wie das Vogelgezwitscher verhallt . . .", "tokens": ["Horch", "!", "wie", "das", "Vo\u00b7gel\u00b7ge\u00b7zwit\u00b7scher", "ver\u00b7hallt", ".", ".", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "$.", "PWAV", "ART", "NN", "VVPP", "$.", "$.", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Geisha und er sind verschwunden.", "tokens": ["Geis\u00b7ha", "und", "er", "sind", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Abendland \u2013 Morgenland \u2013 Mund an Mund \u2013", "tokens": ["A\u00b7bend\u00b7land", "\u2013", "Mor\u00b7gen\u00b7land", "\u2013", "Mund", "an", "Mund", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "NN", "$(", "NN", "APPR", "NN", "$("],