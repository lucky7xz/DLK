f Erden,", "tokens": ["So", "gehst", "du", ",", "wo", "du", "gehst", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Von himmlischem Geleit umgeben;", "tokens": ["Von", "himm\u00b7li\u00b7schem", "Ge\u00b7leit", "um\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Und die dir n\u00e4chst im Leben standen,", "tokens": ["Und", "die", "dir", "n\u00e4chst", "im", "Le\u00b7ben", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Die siehst du dich zun\u00e4chst umschweben.", "tokens": ["Die", "siehst", "du", "dich", "zu\u00b7n\u00e4chst", "um\u00b7schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}