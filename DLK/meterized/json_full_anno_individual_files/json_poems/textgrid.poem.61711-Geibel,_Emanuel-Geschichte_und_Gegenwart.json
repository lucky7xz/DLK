{"textgrid.poem.61711": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Geschichte und Gegenwart", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die im Wirrsal dieser Tage", "tokens": ["Du", ",", "die", "im", "Wirr\u00b7sal", "die\u00b7ser", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich zur Prophetin Gott ersah,", "tokens": ["Sich", "zur", "Pro\u00b7phe\u00b7tin", "Gott", "er\u00b7sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hoch und ernst mit deiner Wage,", "tokens": ["Wie", "hoch", "und", "ernst", "mit", "dei\u00b7ner", "Wa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geschichte, stehst du vor mir da!", "tokens": ["Ge\u00b7schich\u00b7te", ",", "stehst", "du", "vor", "mir", "da", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sibylle, der vom keuschen Munde", "tokens": ["Si\u00b7byl\u00b7le", ",", "der", "vom", "keu\u00b7schen", "Mun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Zeugenwort der Dinge t\u00f6nt,", "tokens": ["Das", "Zeu\u00b7gen\u00b7wort", "der", "Din\u00b7ge", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die mit jahrtausendalter Kunde", "tokens": ["Die", "mit", "jahr\u00b7tau\u00b7sen\u00b7dal\u00b7ter", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des j\u00fcngsten Morgens Leid vers\u00f6hnt.", "tokens": ["Des", "j\u00fcng\u00b7sten", "Mor\u00b7gens", "Leid", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohl hast du ewig unbestochen,", "tokens": ["Wohl", "hast", "du", "e\u00b7wig", "un\u00b7be\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Zorn und Liebe nie entflammt,", "tokens": ["Von", "Zorn", "und", "Lie\u00b7be", "nie", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Sterblichen ihr Recht gesprochen,", "tokens": ["Den", "Sterb\u00b7li\u00b7chen", "ihr", "Recht", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch schm\u00fcckt dich heut ein h\u00f6her Amt.", "tokens": ["Doch", "schm\u00fcckt", "dich", "heut", "ein", "h\u00f6\u00b7her", "Amt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit k\u00fchner Hand im Zeitenbuche", "tokens": ["Mit", "k\u00fch\u00b7ner", "Hand", "im", "Zei\u00b7ten\u00b7bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aufbl\u00e4tternd, was von Anfang war,", "tokens": ["Auf\u00b7bl\u00e4t\u00b7ternd", ",", "was", "von", "An\u00b7fang", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Machst du mit priesterlichem Spruche", "tokens": ["Machst", "du", "mit", "pries\u00b7ter\u00b7li\u00b7chem", "Spru\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Weltgeheimnis offenbar.", "tokens": ["Das", "Welt\u00b7ge\u00b7heim\u00b7nis", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn tief im Schutt bis an die Br\u00fcste,", "tokens": ["Denn", "tief", "im", "Schutt", "bis", "an", "die", "Br\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Haupt von Flugsand \u00fcberschneit,", "tokens": ["Das", "Haupt", "von", "Flug\u00b7sand", "\u00fc\u00b7bersc\u00b7hneit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lag schweigend, wie die Sphinx der W\u00fcste,", "tokens": ["Lag", "schwei\u00b7gend", ",", "wie", "die", "Sphinx", "der", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein R\u00e4tselbild, Vergangenheit.", "tokens": ["Dein", "R\u00e4t\u00b7sel\u00b7bild", ",", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Auge, das an Stirn und Falten", "tokens": ["Das", "Au\u00b7ge", ",", "das", "an", "Stirn", "und", "Fal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur hier und dort ein Zeichen las,", "tokens": ["Nur", "hier", "und", "dort", "ein", "Zei\u00b7chen", "las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verlor, vom N\u00e4chsten festgehalten,", "tokens": ["Ver\u00b7lor", ",", "vom", "N\u00e4chs\u00b7ten", "fest\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des Ganzen ungeheures Ma\u00df.", "tokens": ["Des", "Gan\u00b7zen", "un\u00b7ge\u00b7heu\u00b7res", "Ma\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch nun allm\u00e4hlich aus den Tiefen,", "tokens": ["Doch", "nun", "all\u00b7m\u00e4h\u00b7lich", "aus", "den", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die nimmerm\u00fcder Flei\u00df durchgr\u00e4bt,", "tokens": ["Die", "nim\u00b7mer\u00b7m\u00fc\u00b7der", "Flei\u00df", "durch\u00b7gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich \u00fcberdeckt mit Hieroglyphen", "tokens": ["Sich", "\u00fc\u00b7berd\u00b7eckt", "mit", "Hie\u00b7ro\u00b7gly\u00b7phen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Des Riesenleibes Umri\u00df hebt;", "tokens": ["Des", "Rie\u00b7sen\u00b7lei\u00b7bes", "Um\u00b7ri\u00df", "hebt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun in untr\u00fcglicher Gestaltung", "tokens": ["Nun", "in", "un\u00b7tr\u00fcg\u00b7li\u00b7cher", "Ge\u00b7stal\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sprache Fu\u00dfspur vielverzweigt", "tokens": ["Der", "Spra\u00b7che", "Fu\u00df\u00b7spur", "viel\u00b7ver\u00b7zweigt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns der Geschlechter fr\u00fche Spaltung", "tokens": ["Uns", "der", "Ge\u00b7schlech\u00b7ter", "fr\u00fc\u00b7he", "Spal\u00b7tung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Und ihren fr\u00fchsten Bund uns zeigt:", "tokens": ["Und", "ih\u00b7ren", "fr\u00fchs\u00b7ten", "Bund", "uns", "zeigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun rollt vor dem betroffnen Blicke", "tokens": ["Nun", "rollt", "vor", "dem", "be\u00b7troff\u00b7nen", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In festgegliedertem Verlauf", "tokens": ["In", "fest\u00b7ge\u00b7glie\u00b7der\u00b7tem", "Ver\u00b7lauf"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kette sich der Weltgeschicke", "tokens": ["Die", "Ket\u00b7te", "sich", "der", "Welt\u00b7ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ein vollendet Kunstwerk auf;", "tokens": ["Wie", "ein", "voll\u00b7en\u00b7det", "Kunst\u00b7werk", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "VVPP", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun sehn wir reifend durch die Zeiten,", "tokens": ["Nun", "sehn", "wir", "rei\u00b7fend", "durch", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Antlitz wandelnd Zug um Zug,", "tokens": ["Das", "Ant\u00b7litz", "wan\u00b7delnd", "Zug", "um", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Gottes Offenbarung schreiten,", "tokens": ["Des", "Got\u00b7tes", "Of\u00b7fen\u00b7ba\u00b7rung", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die jeder gab, was sie ertrug.", "tokens": ["Die", "je\u00b7der", "gab", ",", "was", "sie", "er\u00b7trug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wohl lastet \u00fcber weiten R\u00e4umen", "tokens": ["Wohl", "las\u00b7tet", "\u00fc\u00b7ber", "wei\u00b7ten", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unsichrer D\u00e4mmrung tr\u00fcber Flor,", "tokens": ["Un\u00b7sich\u00b7rer", "D\u00e4mm\u00b7rung", "tr\u00fc\u00b7ber", "Flor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Doch w\u00e4chst in Bildern dort und Tr\u00e4umen", "tokens": ["Doch", "w\u00e4chst", "in", "Bil\u00b7dern", "dort", "und", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Sehnsucht nach dem Licht empor;", "tokens": ["Die", "Sehn\u00b7sucht", "nach", "dem", "Licht", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl st\u00fcrzt, was Macht und Kunst erschufen,", "tokens": ["Wohl", "st\u00fcrzt", ",", "was", "Macht", "und", "Kunst", "er\u00b7schu\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie f\u00fcr die Ewigkeit bestimmt;", "tokens": ["Wie", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", "be\u00b7stimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch alle Tr\u00fcmmer werden Stufen,", "tokens": ["Doch", "al\u00b7le", "Tr\u00fcm\u00b7mer", "wer\u00b7den", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Darauf die Menschheit weiter klimmt.", "tokens": ["Da\u00b7rauf", "die", "Menschheit", "wei\u00b7ter", "klimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und wie wir so aus Nacht zum Glanze", "tokens": ["Und", "wie", "wir", "so", "aus", "Nacht", "zum", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wandel der Geschlechter sehn,", "tokens": ["Den", "Wan\u00b7del", "der", "Ge\u00b7schlech\u00b7ter", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erkennen wir \u2013 den Blick aufs Ganze \u2013", "tokens": ["Er\u00b7ken\u00b7nen", "wir", "\u2013", "den", "Blick", "aufs", "Gan\u00b7ze", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die St\u00e4tte da wir selber stehn;", "tokens": ["Die", "St\u00e4t\u00b7te", "da", "wir", "sel\u00b7ber", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sp\u00fcren, froh des hohen Waltens,", "tokens": ["Wir", "sp\u00fc\u00b7ren", ",", "froh", "des", "ho\u00b7hen", "Wal\u00b7tens", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Den heil'gen Fortgang des Entfaltens", "tokens": ["Den", "heil'\u00b7gen", "Fort\u00b7gang", "des", "Ent\u00b7fal\u00b7tens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Tag auch, der uns ", "tokens": ["Im", "Tag", "auch", ",", "der", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "PRELS", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Und o