{"dta.poem.8533": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Getreu .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So du mir th&#228;test auch Schmach und Hohn,", "tokens": ["So", "du", "mir", "th", "&#228;", "test", "auch", "Schmach", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VVFIN", "$(", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht wollt\u2019 ich es klagen den Kindern,", "tokens": ["Nicht", "wollt'", "ich", "es", "kla\u00b7gen", "den", "Kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PPER", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Und schl\u00e4gst du mir ab die rechte Hand,", "tokens": ["Und", "schl\u00e4gst", "du", "mir", "ab", "die", "rech\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Noch wollt\u2019 ich die Linke dir bieten;", "tokens": ["Noch", "wollt'", "ich", "die", "Lin\u00b7ke", "dir", "bie\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "So aber du n\u00e4hmst das unselige Haupt,", "tokens": ["So", "a\u00b7ber", "du", "n\u00e4hmst", "das", "un\u00b7se\u00b7li\u00b7ge", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Noch wollt\u2019 ich warnend dir rufen:", "tokens": ["Noch", "wollt'", "ich", "war\u00b7nend", "dir", "ru\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u201efernab, fernab stell\u2019 o Pascha dich,", "tokens": ["\u201e", "fern\u00b7ab", ",", "fern\u00b7ab", "stell'", "o", "Pa\u00b7scha", "dich", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "FM", "FM", "FM", "NN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Da\u00df nicht mein Blut dich besprenge;", "tokens": ["Da\u00df", "nicht", "mein", "Blut", "dich", "be\u00b7spren\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Denn unschuldiges Blut, wen es trifft,", "tokens": ["Denn", "un\u00b7schul\u00b7di\u00b7ges", "Blut", ",", "wen", "es", "trifft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Der f\u00e4llt in schnelles Verderben.\u201c", "tokens": ["Der", "f\u00e4llt", "in", "schnel\u00b7les", "Ver\u00b7der\u00b7ben", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}