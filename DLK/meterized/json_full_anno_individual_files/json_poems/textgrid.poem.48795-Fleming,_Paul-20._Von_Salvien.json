{"textgrid.poem.48795": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "20. Von Salvien", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So anders sicher ist, was in die leise Ohren", "tokens": ["So", "an\u00b7ders", "si\u00b7cher", "ist", ",", "was", "in", "die", "lei\u00b7se", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VAFIN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der L\u00fcfte wird gesagt, wenn bei geheimer Nacht", "tokens": ["der", "L\u00fcf\u00b7te", "wird", "ge\u00b7sagt", ",", "wenn", "bei", "ge\u00b7hei\u00b7mer", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "um sein geliebtes Haus ein muntrer Buler wacht,", "tokens": ["um", "sein", "ge\u00b7lieb\u00b7tes", "Haus", "ein", "mun\u00b7trer", "Bu\u00b7ler", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der viel redt, doch mehr denkt, ich hab' es oft, verschworen,", "tokens": ["der", "viel", "redt", ",", "doch", "mehr", "denkt", ",", "ich", "hab'", "es", "oft", ",", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADV", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "zu lieben keine mehr, weil Eine war verloren,", "tokens": ["zu", "lie\u00b7ben", "kei\u00b7ne", "mehr", ",", "weil", "Ei\u00b7ne", "war", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PIAT", "PIS", "$,", "KOUS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die ich mir, wie sie mich ihr hatte zugedacht,", "tokens": ["die", "ich", "mir", ",", "wie", "sie", "mich", "ihr", "hat\u00b7te", "zu\u00b7ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PPER", "$,", "PWAV", "PPER", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "sie aber, ", "tokens": ["sie", "a\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "da\u00df ich den harten Sinn zu \u00e4ndern hab' erkoren.", "tokens": ["da\u00df", "ich", "den", "har\u00b7ten", "Sinn", "zu", "\u00e4n\u00b7dern", "hab'", "er\u00b7ko\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verzeihe dieses mir, du selige ", "tokens": ["Ver\u00b7zei\u00b7he", "die\u00b7ses", "mir", ",", "du", "se\u00b7li\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PDAT", "PPER", "$,", "PPER", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df ich mir "