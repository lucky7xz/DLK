{"dta.poem.1138": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "24. Die Sch\u00f6ne Ein\u00e4ugigte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du sch\u00f6nes Green Kind/ wer giebet mir Bericht ", "tokens": ["Du", "sch\u00f6\u00b7nes", "Green", "Kind", "/", "wer", "gie\u00b7bet", "mir", "Be\u00b7richt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "NN", "$(", "PWS", "VVFIN", "PPER", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Warum dein zartes Haubt ein einig Auge tr\u00e4get?", "tokens": ["Wa\u00b7rum", "dein", "zar\u00b7tes", "Haubt", "ein", "ei\u00b7nig", "Au\u00b7ge", "tr\u00e4\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Machts wohl Cupido hier/ als wie ein Sch\u00fctze pfleget/", "tokens": ["Machts", "wohl", "Cu\u00b7pi\u00b7do", "hier", "/", "als", "wie", "ein", "Sch\u00fct\u00b7ze", "pfle\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NE", "ADV", "$(", "KOUS", "KOKOM", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der am gewi\u00dften zielt durch unzertheiltes Licht?", "tokens": ["Der", "am", "ge\u00b7wi\u00df\u00b7ten", "zielt", "durch", "un\u00b7zer\u00b7theil\u00b7tes", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Thu nun das ander\u2019 auff/ die Jagt ist schon verricht;", "tokens": ["Thu", "nun", "das", "an\u00b7der'", "auff", "/", "die", "Jagt", "ist", "schon", "ver\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "PIS", "APPR", "$(", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was wiltu mehr/ wenn mich der erste Schu\u00df erleget?", "tokens": ["Was", "wil\u00b7tu", "mehr", "/", "wenn", "mich", "der", "ers\u00b7te", "Schu\u00df", "er\u00b7le\u00b7get", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "$(", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gib nicht zu/ da\u00df ein Theil vom Antlitz Schatten heget/", "tokens": ["Gib", "nicht", "zu", "/", "da\u00df", "ein", "Theil", "vom", "Ant\u00b7litz", "Schat\u00b7ten", "he\u00b7get", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "PTKZU", "$(", "KOUS", "ART", "NN", "APPRART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn jenes immersort die heisse Sonne sticht.", "tokens": ["Wenn", "je\u00b7nes", "im\u00b7mer\u00b7sort", "die", "heis\u00b7se", "Son\u00b7ne", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich irre mich/ mein Schatz: Was \u00e4userlich gebricht/", "tokens": ["Ich", "ir\u00b7re", "mich", "/", "mein", "Schatz", ":", "Was", "\u00e4u\u00b7ser\u00b7lich", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "$.", "PWS", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wird innerlich ersetzt: Du kanst dem Himmel gleichen/", "tokens": ["Wird", "in\u00b7ner\u00b7lich", "er\u00b7setzt", ":", "Du", "kanst", "dem", "Him\u00b7mel", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Den nur ein grosses Licht auff einmahl mu\u00df bestreichen.", "tokens": ["Den", "nur", "ein", "gros\u00b7ses", "Licht", "auff", "ein\u00b7mahl", "mu\u00df", "be\u00b7strei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "APPR", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man siehet Ph\u00f6bus Glantz dem nahen Monden weichen/", "tokens": ["Man", "sie\u00b7het", "Ph\u00f6\u00b7bus", "Glantz", "dem", "na\u00b7hen", "Mon\u00b7den", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und diesen wiederum vor jenes Pracht verbleichen.", "tokens": ["Und", "die\u00b7sen", "wie\u00b7de\u00b7rum", "vor", "je\u00b7nes", "Pracht", "ver\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer eine Sonne sieht/ acht tausend Sternen nicht.", "tokens": ["Wer", "ei\u00b7ne", "Son\u00b7ne", "sieht", "/", "acht", "tau\u00b7send", "Ster\u00b7nen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$(", "CARD", "CARD", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}