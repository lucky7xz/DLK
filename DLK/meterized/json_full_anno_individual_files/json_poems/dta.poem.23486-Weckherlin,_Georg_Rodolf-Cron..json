{"dta.poem.23486": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Cron.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1619", "urn": "urn:nbn:de:kobv:b4-20627-4", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Avf das sein lob w\u00fcrdig vnd wehrhaft", "tokens": ["Avf", "das", "sein", "lob", "w\u00fcr\u00b7dig", "vnd", "wehr\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "PPOSAT", "NN", "ADJD", "KON", "NN"], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "sey", "tokens": ["sey"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Bringt seine Gnad vnd G\u00fct zuwege\u0303;", "tokens": ["Bringt", "sei\u00b7ne", "Gnad", "vnd", "G\u00fct", "zu\u00b7weg\u1ebd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr Sie ist Jhm sein volck hold vn\u0303 getrew/", "tokens": ["F\u00fcr", "Sie", "ist", "Jhm", "sein", "volck", "hold", "v\u00f1", "ge\u00b7trew", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PPOSAT", "NN", "ADJD", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als f\u00fcr seinen reichesten seegen:", "tokens": ["Als", "f\u00fcr", "sei\u00b7nen", "rei\u00b7ches\u00b7ten", "see\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Durch Sie wirt Er geehret in dem Land/", "tokens": ["Durch", "Sie", "wirt", "Er", "ge\u00b7eh\u00b7ret", "in", "dem", "Land", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Sie ist allein der rechte Amors-brand/", "tokens": ["Sie", "ist", "al\u00b7lein", "der", "rech\u00b7te", "A\u00b7mor\u00b7sbrand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Damit Er die sehlen anzindet/", "tokens": ["Da\u00b7mit", "Er", "die", "seh\u00b7len", "an\u00b7zin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "VVINF", "VVFIN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Vnd Sie so gehorsam erfindet/", "tokens": ["Vnd", "Sie", "so", "ge\u00b7hor\u00b7sam", "er\u00b7fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Das Sie Jhn/ nein/ vilmehr f\u00fcr Jhn", "tokens": ["Das", "Sie", "Jhn", "/", "nein", "/", "vil\u00b7mehr", "f\u00fcr", "Jhn"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PPER", "PPER", "$(", "PTKANT", "$(", "ADV", "APPR", "PPER"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.11": {"text": "F\u00f6rchten/ vnd Jhn mehr dan Sich selbs", "tokens": ["F\u00f6rch\u00b7ten", "/", "vnd", "Jhn", "mehr", "dan", "Sich", "selbs"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "KON", "PPER", "ADV", "ADV", "PRF", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "bewahren;", "tokens": ["be\u00b7wah\u00b7ren", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "F\u00fcr Jhn (Jhren besten gewin)", "tokens": ["F\u00fcr", "Jhn", "(", "Ih\u00b7ren", "bes\u00b7ten", "ge\u00b7win", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Weder arbeit noch leib vnd leben spahren:", "tokens": ["We\u00b7der", "ar\u00b7beit", "noch", "leib", "vnd", "le\u00b7ben", "spah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "NN", "KON", "VVINF", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "Er ist Jhr Vatter/ Jhr Patron/", "tokens": ["Er", "ist", "Ihr", "Vat\u00b7ter", "/", "Ihr", "Pat\u00b7ron", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ja Jhr Got selbs von de\u0303 sie gnad erlange\u0303/", "tokens": ["Ja", "Ihr", "Got", "selbs", "von", "d\u1ebd", "sie", "gnad", "er\u00b7lang\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPOSAT", "NN", "ADV", "APPR", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.17": {"text": "Vnd durch Gnad soll auch Er empfange\u0303", "tokens": ["Vnd", "durch", "Gnad", "soll", "auch", "Er", "emp\u00b7fang\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VMFIN", "ADV", "PPER", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.18": {"text": "Von Gottes hand des Himels Gnade\u0303-cron.", "tokens": ["Von", "Got\u00b7tes", "hand", "des", "Hi\u00b7mels", "Gna\u00b7d\u1ebd\u00b7cron", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}