{"dta.poem.11824": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der heilige Kebir sah eine M\u00fchle drehn,", "tokens": ["Der", "hei\u00b7li\u00b7ge", "Ke\u00b7bir", "sah", "ei\u00b7ne", "M\u00fch\u00b7le", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und weinte, da\u00df kein Korn da ganz hindurch kann gehn.", "tokens": ["Und", "wein\u00b7te", ",", "da\u00df", "kein", "Korn", "da", "ganz", "hin\u00b7durch", "kann", "gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PIAT", "NN", "ADV", "ADV", "PAV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er weint' ums K\u00f6rnlein nicht, er weint' ums Weltgeschick,", "tokens": ["Er", "weint'", "ums", "K\u00f6rn\u00b7lein", "nicht", ",", "er", "weint'", "ums", "Welt\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKNEG", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das tausend Leben so malmt jeden Augenblick.", "tokens": ["Das", "tau\u00b7send", "Le\u00b7ben", "so", "malmt", "je\u00b7den", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-