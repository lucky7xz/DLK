{"textgrid.poem.24841": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Im Grase", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um mich schw\u00e4rmender Bienen Gesumm;", "tokens": ["Um", "mich", "schw\u00e4r\u00b7men\u00b7der", "Bie\u00b7nen", "Ge\u00b7summ", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJA", "NN", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Fernher Singen von Schnittern;", "tokens": ["Fern\u00b7her", "Sin\u00b7gen", "von", "Schnit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sommerl\u00fcfte, die hei\u00df ringsum", "tokens": ["Som\u00b7mer\u00b7l\u00fcf\u00b7te", ",", "die", "hei\u00df", "ring\u00b7sum"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ueber der Wiese zittern!", "tokens": ["Ue\u00b7ber", "der", "Wie\u00b7se", "zit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Hoch aus dunkelndem Himmelsblau,", "tokens": ["Hoch", "aus", "dun\u00b7keln\u00b7dem", "Him\u00b7mels\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Drin die Wolken verschwimmen,", "tokens": ["Drin", "die", "Wol\u00b7ken", "ver\u00b7schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Quillt es und rinnt hernieder wie Tau,", "tokens": ["Quillt", "es", "und", "rinnt", "her\u00b7nie\u00b7der", "wie", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "S\u00e4uselt wie liebe Stimmen,", "tokens": ["S\u00e4u\u00b7selt", "wie", "lie\u00b7be", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Gaukelt und lacht mir hinweg das Leid,", "tokens": ["Gau\u00b7kelt", "und", "lacht", "mir", "hin\u00b7weg", "das", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Hebt die Erdengewichte,", "tokens": ["Hebt", "die", "Er\u00b7den\u00b7ge\u00b7wich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Bis die Seele, gel\u00f6st, befreit,", "tokens": ["Bis", "die", "See\u00b7le", ",", "ge\u00b7l\u00f6st", ",", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Schw\u00e4rmt in dem himmlischen Lichte.", "tokens": ["Schw\u00e4rmt", "in", "dem", "himm\u00b7li\u00b7schen", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stan