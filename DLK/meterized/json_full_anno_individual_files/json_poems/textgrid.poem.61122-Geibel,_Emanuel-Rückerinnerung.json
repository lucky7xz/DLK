{"textgrid.poem.61122": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fcckerinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft wenn die Sommernacht auf lauen Fl\u00fcgeln", "tokens": ["Oft", "wenn", "die", "Som\u00b7mer\u00b7nacht", "auf", "lau\u00b7en", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von G\u00e4rten, Bl\u00fctenw\u00e4ldern, Rebenh\u00fcgeln", "tokens": ["Von", "G\u00e4r\u00b7ten", ",", "Bl\u00fc\u00b7ten\u00b7w\u00e4l\u00b7dern", ",", "Re\u00b7ben\u00b7h\u00fc\u00b7geln"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des S\u00fcdens D\u00fcfte zu mir tr\u00e4gt,", "tokens": ["Des", "S\u00fc\u00b7dens", "D\u00fcf\u00b7te", "zu", "mir", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn durch das Bogenwerk am S\u00e4ulengange", "tokens": ["Wenn", "durch", "das", "Bo\u00b7gen\u00b7werk", "am", "S\u00e4u\u00b7len\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Mondstrahl spielt, und fern mit s\u00fc\u00dfem Klange", "tokens": ["Der", "Mond\u00b7strahl", "spielt", ",", "und", "fern", "mit", "s\u00fc\u00b7\u00dfem", "Klan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Nachtigall am Brunnen schl\u00e4gt;", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "am", "Brun\u00b7nen", "schl\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn mit Geplauder dann, mit Scherz und Singen", "tokens": ["Wenn", "mit", "Ge\u00b7plau\u00b7der", "dann", ",", "mit", "Scherz", "und", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADV", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die muntern Freunde lachend mich umringen,", "tokens": ["Die", "mun\u00b7tern", "Freun\u00b7de", "la\u00b7chend", "mich", "um\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Laut' im Arm, das Glas zur Hand:", "tokens": ["Die", "Laut'", "im", "Arm", ",", "das", "Glas", "zur", "Hand", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da werd' ich pl\u00f6tzlich stumm, und die Gedanken", "tokens": ["Da", "werd'", "ich", "pl\u00f6tz\u00b7lich", "stumm", ",", "und", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schweifen, Zugv\u00f6geln gleich, mit irrem Schwanken", "tokens": ["Schwei\u00b7fen", ",", "Zug\u00b7v\u00f6\u00b7geln", "gleich", ",", "mit", "ir\u00b7rem", "Schwan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "ADV", "$,", "APPR", "ADJA", "NN"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Sehns\u00fcchtig heim ins Vaterland.", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "heim", "ins", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir ist es dann, als sei ich doch im Grunde", "tokens": ["Mir", "ist", "es", "dann", ",", "als", "sei", "ich", "doch", "im", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Schiffer nur, gef\u00fchrt von b\u00f6ser Stunde", "tokens": ["Ein", "Schif\u00b7fer", "nur", ",", "ge\u00b7f\u00fchrt", "von", "b\u00f6\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu eines Zaubereilands Pracht,", "tokens": ["Zu", "ei\u00b7nes", "Zau\u00b7be\u00b7rei\u00b7lands", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als m\u00fc\u00dft' ich dieses Mondlichts s\u00fc\u00dfes Weben", "tokens": ["Als", "m\u00fc\u00dft'", "ich", "die\u00b7ses", "Mond\u00b7lichts", "s\u00fc\u00b7\u00dfes", "We\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und diese Bl\u00fctend\u00fcfte freudig geben", "tokens": ["Und", "die\u00b7se", "Bl\u00fc\u00b7ten\u00b7d\u00fcf\u00b7te", "freu\u00b7dig", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr eine deutsche Nebelnacht.", "tokens": ["F\u00fcr", "ei\u00b7ne", "deut\u00b7sche", "Ne\u00b7bel\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da denk' ich, wie ich bei des Herbstes St\u00fcrmen", "tokens": ["Da", "denk'", "ich", ",", "wie", "ich", "bei", "des", "Herbs\u00b7tes", "St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Oftmals entlang den Kirchhof an den T\u00fcrmen", "tokens": ["Oft\u00b7mals", "ent\u00b7lang", "den", "Kirch\u00b7hof", "an", "den", "T\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des gotischen Doms vor\u00fcberschritt;", "tokens": ["Des", "go\u00b7ti\u00b7schen", "Doms", "vor\u00b7\u00fc\u00b7bersc\u00b7hritt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Glocken schlugen an, gleich roten Sternen", "tokens": ["Die", "Glo\u00b7cken", "schlu\u00b7gen", "an", ",", "gleich", "ro\u00b7ten", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schwankten im Zug der Gassen die Laternen,", "tokens": ["Schwank\u00b7ten", "im", "Zug", "der", "Gas\u00b7sen", "die", "La\u00b7ter\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Und \u00fcber Gr\u00e4bern scholl mein Tritt.", "tokens": ["Und", "\u00fc\u00b7ber", "Gr\u00e4\u00b7bern", "scholl", "mein", "Tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Laut auf die D\u00e4cher prasselte der Regen;", "tokens": ["Laut", "auf", "die", "D\u00e4\u00b7cher", "pras\u00b7sel\u00b7te", "der", "Re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Am Bogentor schlug mir der Wind entgegen", "tokens": ["Am", "Bo\u00b7gen\u00b7tor", "schlug", "mir", "der", "Wind", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sch\u00fcttelt' heftig mit Gebraus", "tokens": ["Und", "sch\u00fct\u00b7telt'", "hef\u00b7tig", "mit", "Ge\u00b7braus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alten Ulmen, die dort finster ragen;", "tokens": ["Die", "al\u00b7ten", "Ul\u00b7men", ",", "die", "dort", "fins\u00b7ter", "ra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch ich, den Mantel fester umgeschlagen,", "tokens": ["Doch", "ich", ",", "den", "Man\u00b7tel", "fes\u00b7ter", "um\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Eilte zum hohen Giebelhaus.", "tokens": ["Eil\u00b7te", "zum", "ho\u00b7hen", "Gie\u00b7bel\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "O Freude, wenn ich dann, vom Regen tropfend,", "tokens": ["O", "Freu\u00b7de", ",", "wenn", "ich", "dann", ",", "vom", "Re\u00b7gen", "trop\u00b7fend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "ADV", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Herz in ungest\u00fcmer Sehnsucht klopfend,", "tokens": ["Das", "Herz", "in", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Sehn\u00b7sucht", "klop\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Empor die breiten Treppen flog,", "tokens": ["Em\u00b7por", "die", "brei\u00b7ten", "Trep\u00b7pen", "flog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und von den dunklen Galerien droben", "tokens": ["Und", "von", "den", "dunk\u00b7len", "Ga\u00b7le\u00b7ri\u00b7en", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sich mir, vom Schein der Lampe mild umwoben,", "tokens": ["Sich", "mir", ",", "vom", "Schein", "der", "Lam\u00b7pe", "mild", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "$,", "APPRART", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Lockenhaupt entgegen bog! -", "tokens": ["Ein", "Lo\u00b7cken\u00b7haupt", "ent\u00b7ge\u00b7gen", "bog", "!"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PTKVZ", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Oft wenn die Sommernacht auf lauen Fl\u00fcgeln", "tokens": ["Oft", "wenn", "die", "Som\u00b7mer\u00b7nacht", "auf", "lau\u00b7en", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von G\u00e4rten, Bl\u00fctenw\u00e4ldern, Rebenh\u00fcgeln", "tokens": ["Von", "G\u00e4r\u00b7ten", ",", "Bl\u00fc\u00b7ten\u00b7w\u00e4l\u00b7dern", ",", "Re\u00b7ben\u00b7h\u00fc\u00b7geln"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des S\u