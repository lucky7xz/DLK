{"dta.poem.9939": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "48.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich entsteh aus kleinen bl\u00e4ttern/ und mu\u00df leiden doch so viel.", "tokens": ["Ich", "ent\u00b7steh", "aus", "klei\u00b7nen", "bl\u00e4t\u00b7tern", "/", "und", "mu\u00df", "lei\u00b7den", "doch", "so", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "VVINF", "$(", "KON", "VMFIN", "VVINF", "KON", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Ich mu\u00df \u00fcber land und meer/ es sey krieg/ sey friede/ reisen.", "tokens": ["Ich", "mu\u00df", "\u00fc\u00b7ber", "land", "und", "meer", "/", "es", "sey", "krieg", "/", "sey", "frie\u00b7de", "/", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "$(", "PPER", "VAFIN", "NN", "$(", "VAFIN", "NN", "$(", "VVINF", "$."], "meter": "-+--+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Meistens geh ich auf im rauch/ bald verderb\u2019 ich auch durchs ei-", "tokens": ["Meis\u00b7tens", "geh", "ich", "auf", "im", "rauch", "/", "bald", "ver\u00b7derb'", "ich", "auch", "durchs", "ei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$(", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "TRUNC"], "meter": "+-----+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Der und jener/ so mich liebet/ mich zu pulver machen will. (sen.", "tokens": ["Der", "und", "je\u00b7ner", "/", "so", "mich", "lie\u00b7bet", "/", "mich", "zu", "pul\u00b7ver", "ma\u00b7chen", "will", ".", "(", "sen."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "abbreviation"], "pos": ["ART", "KON", "PDAT", "$(", "ADV", "PPER", "VVFIN", "$(", "PPER", "PTKA", "ADJD", "VVINF", "VMFIN", "$.", "$(", "XY"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}