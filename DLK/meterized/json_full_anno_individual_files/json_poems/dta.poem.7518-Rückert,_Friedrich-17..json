{"dta.poem.7518": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verstand zu seinem Bau braucht manche St\u00fctz' und Kr\u00fccke,", "tokens": ["Ver\u00b7stand", "zu", "sei\u00b7nem", "Bau", "braucht", "man\u00b7che", "St\u00fctz'", "und", "Kr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Natur und Fantasie baut ganz aus Einem St\u00fccke.", "tokens": ["Na\u00b7tur", "und", "Fan\u00b7ta\u00b7sie", "baut", "ganz", "aus", "Ei\u00b7nem", "St\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die St\u00fctzen fehlen nicht, sie sind nur nicht zu sehn;", "tokens": ["Die", "St\u00fct\u00b7zen", "feh\u00b7len", "nicht", ",", "sie", "sind", "nur", "nicht", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf sich selber steht, was scheint auf Nichts zu stehn.", "tokens": ["Und", "auf", "sich", "sel\u00b7ber", "steht", ",", "was", "scheint", "auf", "Nichts", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "VVFIN", "$,", "PWS", "VVFIN", "APPR", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was du begreifen kannst, siehst du in seiner Bl\u00f6\u00dfe;", "tokens": ["Was", "du", "be\u00b7grei\u00b7fen", "kannst", ",", "siehst", "du", "in", "sei\u00b7ner", "Bl\u00f6\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Stets unbegreiflich ist die Sch\u00f6nheit und die Gr\u00f6\u00dfe.", "tokens": ["Stets", "un\u00b7be\u00b7greif\u00b7lich", "ist", "die", "Sch\u00f6n\u00b7heit", "und", "die", "Gr\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}