{"textgrid.poem.32431": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "110. Der spiels\u00fcchtige Deutsche", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So \u00e4u\u00dferst war, nach Tacitus Bericht,", "tokens": ["So", "\u00e4u\u00b7\u00dferst", "war", ",", "nach", "Ta\u00b7ci\u00b7tus", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der alte Deutsch' aufs Spiel erpicht,", "tokens": ["Der", "al\u00b7te", "Deut\u00b7sch'", "aufs", "Spiel", "er\u00b7picht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da\u00df, wenn er ins Verlieren kam,", "tokens": ["Da\u00df", ",", "wenn", "er", "ins", "Ver\u00b7lie\u00b7ren", "kam", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er endlich keinen Anstand nahm,", "tokens": ["Er", "end\u00b7lich", "kei\u00b7nen", "An\u00b7stand", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den letzten Schatz von allen Sch\u00e4tzen,", "tokens": ["Den", "letz\u00b7ten", "Schatz", "von", "al\u00b7len", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich selber, auf das Spiel zu setzen.", "tokens": ["Sich", "sel\u00b7ber", ",", "auf", "das", "Spiel", "zu", "set\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie unbegreiflich rasch! wie wild!", "tokens": ["Wie", "un\u00b7be\u00b7greif\u00b7lich", "rasch", "!", "wie", "wild", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJD", "$.", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob dieses noch vom Deutschen gilt?", "tokens": ["Ob", "die\u00b7ses", "noch", "vom", "Deut\u00b7schen", "gilt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vom deutschen Manne schwerlich. \u2013 Doch,", "tokens": ["Vom", "deut\u00b7schen", "Man\u00b7ne", "schwer\u00b7lich", ".", "\u2013", "Doch", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "$.", "$(", "KON", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom deutschen Weibe gilt es noch.", "tokens": ["Vom", "deut\u00b7schen", "Wei\u00b7be", "gilt", "es", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So \u00e4u\u00dferst war, nach Tacitus Bericht,", "tokens": ["So", "\u00e4u\u00b7\u00dferst", "war", ",", "nach", "Ta\u00b7ci\u00b7tus", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "punct", "wo