{"textgrid.poem.56796": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer laut von diesem l\u00e4ngst verla\u00dfnen Turm", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer laut von diesem l\u00e4ngst verla\u00dfnen Turm", "tokens": ["Wer", "laut", "von", "die\u00b7sem", "l\u00e4ngst", "ver\u00b7la\u00df\u00b7nen", "Turm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "APPR", "PDAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Tannen Ringwald \u00fcberrufen wollte,", "tokens": ["der", "Tan\u00b7nen", "Ring\u00b7wald", "\u00fc\u00b7ber\u00b7ru\u00b7fen", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und tr\u00fcge, was er riefe, st\u00e4rkster Sturm,", "tokens": ["und", "tr\u00fc\u00b7ge", ",", "was", "er", "rie\u00b7fe", ",", "st\u00e4rks\u00b7ter", "Sturm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ihm ahnte, da\u00df es nie ein Ziel errollte.", "tokens": ["ihm", "ahn\u00b7te", ",", "da\u00df", "es", "nie", "ein", "Ziel", "er\u00b7roll\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So einsam steigt der alte Bau empor;", "tokens": ["So", "ein\u00b7sam", "steigt", "der", "al\u00b7te", "Bau", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "er f\u00fchlte F\u00fcrsten einst auf seinen Stufen,", "tokens": ["er", "f\u00fchl\u00b7te", "F\u00fcrs\u00b7ten", "einst", "auf", "sei\u00b7nen", "Stu\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "bis, dunkler Taten schauerlich verrufen,", "tokens": ["bis", ",", "dunk\u00b7ler", "Ta\u00b7ten", "schau\u00b7er\u00b7lich", "ver\u00b7ru\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "sein stiller Reiz der Menschen Gunst verlor.", "tokens": ["sein", "stil\u00b7ler", "Reiz", "der", "Men\u00b7schen", "Gunst", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur da\u00df von J\u00e4gern sich zuweilen wer", "tokens": ["Nur", "da\u00df", "von", "J\u00e4\u00b7gern", "sich", "zu\u00b7wei\u00b7len", "wer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "NN", "PRF", "ADV", "PWS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vorbei verirrt, von wanderfrohen Seelen,", "tokens": ["vor\u00b7bei", "ver\u00b7irrt", ",", "von", "wan\u00b7der\u00b7fro\u00b7hen", "See\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "von Bettelpack, und wer die Kreuz und Quer", "tokens": ["von", "Bet\u00b7tel\u00b7pack", ",", "und", "wer", "die", "Kreuz", "und", "Quer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "PWS", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "den Forst durchschleicht, sich Holz und Wild zu stehlen;", "tokens": ["den", "Forst", "durch\u00b7schleicht", ",", "sich", "Holz", "und", "Wild", "zu", "steh\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRF", "NN", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "nur da\u00df an seinem Fu\u00df zuweilen sich,", "tokens": ["nur", "da\u00df", "an", "sei\u00b7nem", "Fu\u00df", "zu\u00b7wei\u00b7len", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "PPOSAT", "NN", "ADV", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "wie heut, Zigeunervolk sein Reisig schichtet", "tokens": ["wie", "heut", ",", "Zi\u00b7geu\u00b7ner\u00b7volk", "sein", "Rei\u00b7sig", "schich\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und mit der Bogen wehmutwildem Strich", "tokens": ["und", "mit", "der", "Bo\u00b7gen", "weh\u00b7mut\u00b7wil\u00b7dem", "Strich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "sein Weltweh in den fremden Frieden dichtet.", "tokens": ["sein", "Welt\u00b7weh", "in", "den", "frem\u00b7den", "Frie\u00b7den", "dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In allen Kronen h\u00e4ngt noch goldner Glanz ...", "tokens": ["In", "al\u00b7len", "Kro\u00b7nen", "h\u00e4ngt", "noch", "gold\u00b7ner", "Glanz", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne s\u00e4umt noch, ihren Tag zu enden ...", "tokens": ["Die", "Son\u00b7ne", "s\u00e4umt", "noch", ",", "ih\u00b7ren", "Tag", "zu", "en\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der S\u00f6llerbl\u00f6cke halb zerfallnen Kranz", "tokens": ["Der", "S\u00f6l\u00b7ler\u00b7bl\u00f6\u00b7cke", "halb", "zer\u00b7fall\u00b7nen", "Kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "umlodert noch ihr scheidendes Verschwenden ...", "tokens": ["um\u00b7lo\u00b7dert", "noch", "ihr", "schei\u00b7den\u00b7des", "Ver\u00b7schwen\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und aus dem Purpur schwillt es wie ein Born,", "tokens": ["Und", "aus", "dem", "Pur\u00b7pur", "schwillt", "es", "wie", "ein", "Born", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "KOKOM", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "ein Strom von T\u00f6nen \u2013: Abends erst Erschauern", "tokens": ["ein", "Strom", "von", "T\u00f6\u00b7nen", "\u2013", ":", "A\u00b7bends", "erst", "Er\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$(", "$.", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "erregt des Turms uraltes \u00c4olshorn,", "tokens": ["er\u00b7regt", "des", "Turms", "ur\u00b7al\u00b7tes", "\u00c4\u00b7ols\u00b7horn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "der Sonne nachzujauchzen, nachzutrauern.", "tokens": ["der", "Son\u00b7ne", "nach\u00b7zu\u00b7jauch\u00b7zen", ",", "nach\u00b7zu\u00b7trau\u00b7ern", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Heimatlosen drunten horchen auf \u2013 \u2013.", "tokens": ["Die", "Hei\u00b7mat\u00b7lo\u00b7sen", "drun\u00b7ten", "hor\u00b7chen", "auf", "\u2013", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "$(", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und einer nimmt die Geige von den Knien", "tokens": ["Und", "ei\u00b7ner", "nimmt", "die", "Gei\u00b7ge", "von", "den", "Kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und strebt mit manchem j\u00e4hen Sprung und Lauf", "tokens": ["und", "strebt", "mit", "man\u00b7chem", "j\u00e4\u00b7hen", "Sprung", "und", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "des Winds Gesang phantastisch zu durchziehen ...", "tokens": ["des", "Winds", "Ge\u00b7sang", "phan\u00b7tas\u00b7tisch", "zu", "durch\u00b7zie\u00b7hen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wie so Wind und Seele sich verweben,", "tokens": ["Und", "wie", "so", "Wind", "und", "See\u00b7le", "sich", "ver\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "NN", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "erwachen mehr und mehr der treuen Geigen ...", "tokens": ["er\u00b7wa\u00b7chen", "mehr", "und", "mehr", "der", "treu\u00b7en", "Gei\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein aller Leidenschaften schluchzend Leben", "tokens": ["Ein", "al\u00b7ler", "Lei\u00b7den\u00b7schaf\u00b7ten", "schluch\u00b7zend", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "erst\u00fcrmt des Himmels immer tiefres Schweigen.", "tokens": ["er\u00b7st\u00fcrmt", "des", "Him\u00b7mels", "im\u00b7mer", "tief\u00b7res", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gefangen folgt zuletzt die ganze Schar", "tokens": ["Ge\u00b7fan\u00b7gen", "folgt", "zu\u00b7letzt", "die", "gan\u00b7ze", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Windposaune wunderlichen Launen ...", "tokens": ["der", "Wind\u00b7po\u00b7sau\u00b7ne", "wun\u00b7der\u00b7li\u00b7chen", "Lau\u00b7nen", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun rast es tollk\u00fchn, unberechenbar ...", "tokens": ["Nun", "rast", "es", "toll\u00b7k\u00fchn", ",", "un\u00b7be\u00b7re\u00b7chen\u00b7bar", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun stockt es wie in fragendem Erstaunen ...", "tokens": ["Nun", "stockt", "es", "wie", "in", "fra\u00b7gen\u00b7dem", "Er\u00b7stau\u00b7nen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KOKOM", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Oh Sonne! Sonne! Mutter! Mutter! flehen,", "tokens": ["Oh", "Son\u00b7ne", "!", "Son\u00b7ne", "!", "Mut\u00b7ter", "!", "Mut\u00b7ter", "!", "fle\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["XY", "NN", "$.", "NN", "$.", "NN", "$.", "NN", "$.", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "verzweifeln, weinen, drohen all die Stimmen", "tokens": ["ver\u00b7zwei\u00b7feln", ",", "wei\u00b7nen", ",", "dro\u00b7hen", "all", "die", "Stim\u00b7men"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVINF", "$,", "VVINF", "$,", "VVFIN", "PIAT", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "und drohn und flehn in immer bangren Wehen,", "tokens": ["und", "drohn", "und", "flehn", "in", "im\u00b7mer", "bang\u00b7ren", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "je mehr des Tages Br\u00e4nde rings verglimmen.", "tokens": ["je", "mehr", "des", "Ta\u00b7ges", "Br\u00e4n\u00b7de", "rings", "ver\u00b7glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch droben \u2013 seht ihr? die Zigeunerin!", "tokens": ["Doch", "dro\u00b7ben", "\u2013", "seht", "ihr", "?", "die", "Zi\u00b7geu\u00b7ne\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "VVFIN", "PPER", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entstahl sie sich dem Kreis der braunen S\u00f6hne?", "tokens": ["Ent\u00b7stahl", "sie", "sich", "dem", "Kreis", "der", "brau\u00b7nen", "S\u00f6h\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo kam sie her, das Weib? Wie kam sie hin?", "tokens": ["Wo", "kam", "sie", "her", ",", "das", "Weib", "?", "Wie", "kam", "sie", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "$.", "PWAV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie w\u00e4chst sie hoch in schattenhafter Sch\u00f6ne!", "tokens": ["Wie", "w\u00e4chst", "sie", "hoch", "in", "schat\u00b7ten\u00b7haf\u00b7ter", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und h\u00f6rt ihr \u2013 h\u00f6rt! wie ihre Lippen singen \u2013", "tokens": ["Und", "h\u00f6rt", "ihr", "\u2013", "h\u00f6rt", "!", "wie", "ih\u00b7re", "Lip\u00b7pen", "sin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "VVFIN", "$.", "KOKOM", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "ein Lied, das endlich alles \u00fcberwindet,", "tokens": ["ein", "Lied", ",", "das", "end\u00b7lich", "al\u00b7les", "\u00fc\u00b7berw\u00b7in\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "in sich die andern Stimmen alle bindet,", "tokens": ["in", "sich", "die", "an\u00b7dern", "Stim\u00b7men", "al\u00b7le", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iam