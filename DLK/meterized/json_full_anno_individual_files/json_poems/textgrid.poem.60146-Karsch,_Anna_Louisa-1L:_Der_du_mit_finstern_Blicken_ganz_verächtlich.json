{"textgrid.poem.60146": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der du mit finstern Blicken ganz ver\u00e4chtlich", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du mit finstern Blicken ganz ver\u00e4chtlich", "tokens": ["Der", "du", "mit", "fins\u00b7tern", "Bli\u00b7cken", "ganz", "ver\u00b7\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gl\u00fcck, Ruhm und Freuden \u00fcbersiehst,", "tokens": ["Gl\u00fcck", ",", "Ruhm", "und", "Freu\u00b7den", "\u00fc\u00b7ber\u00b7siehst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht mehr Lorenzo bist, und einsam mittern\u00e4chtlich", "tokens": ["Nicht", "mehr", "Lo\u00b7ren\u00b7zo", "bist", ",", "und", "ein\u00b7sam", "mit\u00b7ter\u00b7n\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "NE", "VAFIN", "$,", "KON", "ADJD", "ADV"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ein andrer Young, den Schlummer fliehst,", "tokens": ["Ein", "an\u00b7drer", "Y\u00b7o\u00b7ung", ",", "den", "Schlum\u00b7mer", "fliehst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und jammernd sitzest, hier, wo die Gebeine", "tokens": ["Und", "jam\u00b7mernd", "sit\u00b7zest", ",", "hier", ",", "wo", "die", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Freundin ruhn, mit Sand bestreut;", "tokens": ["Der", "Freun\u00b7din", "ruhn", ",", "mit", "Sand", "be\u00b7streut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freundschaft f\u00fchrt mich nach, auf da\u00df ich mit dir weine,", "tokens": ["Die", "Freund\u00b7schaft", "f\u00fchrt", "mich", "nach", ",", "auf", "da\u00df", "ich", "mit", "dir", "wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ger\u00fchrt durch deine Traurigkeit!", "tokens": ["Ge\u00b7r\u00fchrt", "durch", "dei\u00b7ne", "Trau\u00b7rig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf meine Leyer will ich ernste Saiten", "tokens": ["Auf", "mei\u00b7ne", "Le\u00b7yer", "will", "ich", "erns\u00b7te", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit fromm gewordnen H\u00e4nden ziehn,", "tokens": ["Mit", "fromm", "ge\u00b7word\u00b7nen", "H\u00e4n\u00b7den", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will singen, wie der Geist sich feyerlich bereiten", "tokens": ["Will", "sin\u00b7gen", ",", "wie", "der", "Geist", "sich", "fey\u00b7er\u00b7lich", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "ART", "NN", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Soll, in die obre Welt zu ziehn!", "tokens": ["Soll", ",", "in", "die", "ob\u00b7re", "Welt", "zu", "ziehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Den Tod und seinen vollgef\u00fcllten K\u00f6cher,", "tokens": ["Den", "Tod", "und", "sei\u00b7nen", "voll\u00b7ge\u00b7f\u00fcll\u00b7ten", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus dem er Pfeil an Pfeile nimt;", "tokens": ["Aus", "dem", "er", "Pfeil", "an", "Pfei\u00b7le", "nimt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Regenbogen-Thron, besessen von dem R\u00e4cher", "tokens": ["Den", "Re\u00b7gen\u00b7bo\u00b7gen\u00b7Thron", ",", "be\u00b7ses\u00b7sen", "von", "dem", "R\u00e4\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Frevler f\u00fcr die Glut bestimmt;", "tokens": ["Der", "Frev\u00b7ler", "f\u00fcr", "die", "Glut", "be\u00b7stimmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Donner des Gerichtes, wie sie rollen", "tokens": ["Die", "Don\u00b7ner", "des", "Ge\u00b7rich\u00b7tes", ",", "wie", "sie", "rol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Pol zu Pol, und, wie alsdann", "tokens": ["Von", "Pol", "zu", "Pol", ",", "und", ",", "wie", "als\u00b7dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "KON", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geb\u00fcrge vor dem Zorn den S\u00fcnder decken sollen,", "tokens": ["Ge\u00b7b\u00fcr\u00b7ge", "vor", "dem", "Zorn", "den", "S\u00fcn\u00b7der", "de\u00b7cken", "sol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der nicht den Blick ertragen kan,", "tokens": ["Der", "nicht", "den", "Blick", "er\u00b7tra\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit dem der Richter von des Himmels H\u00f6he", "tokens": ["Mit", "dem", "der", "Rich\u00b7ter", "von", "des", "Him\u00b7mels", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihn z\u00fcrnet in den Pfuhl hinab!", "tokens": ["Ihn", "z\u00fcr\u00b7net", "in", "den", "Pfuhl", "hin\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er st\u00fcrzt; o, wenn doch nur den H\u00f6llensturz nicht s\u00e4he,", "tokens": ["Er", "st\u00fcrzt", ";", "o", ",", "wenn", "doch", "nur", "den", "H\u00f6l\u00b7len\u00b7sturz", "nicht", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "FM", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Engel, den der Herr ihm gab!", "tokens": ["Der", "En\u00b7gel", ",", "den", "der", "Herr", "ihm", "gab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Freund, also will ich singen, da\u00df dich Schauer", "tokens": ["Freund", ",", "al\u00b7so", "will", "ich", "sin\u00b7gen", ",", "da\u00df", "dich", "Schau\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ergreifen sollen, wenn du h\u00f6rst,", "tokens": ["Er\u00b7grei\u00b7fen", "sol\u00b7len", ",", "wenn", "du", "h\u00f6rst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis da\u00df du deine hochgeliebte s\u00fcsse Trauer", "tokens": ["Bis", "da\u00df", "du", "dei\u00b7ne", "hoch\u00b7ge\u00b7lieb\u00b7te", "s\u00fcs\u00b7se", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "PPER", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Wollust deines Herzens mehrst.", "tokens": ["Die", "Wol\u00b7lust", "dei\u00b7nes", "Her\u00b7zens", "mehrst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das Rauschen meines Liedes soll dich fassen", "tokens": ["Das", "Rau\u00b7schen", "mei\u00b7nes", "Lie\u00b7des", "soll", "dich", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mit dir \u00fcber Wolkengang", "tokens": ["Und", "mit", "dir", "\u00fc\u00b7ber", "Wol\u00b7ken\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu deiner Freundin fliehn, und pl\u00f6zlich dich verlassen,", "tokens": ["Zu", "dei\u00b7ner", "Freun\u00b7din", "fliehn", ",", "und", "pl\u00f6z\u00b7lich", "dich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,", "KON", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bey ihrem himmlischen Gesang!", "tokens": ["Bey", "ih\u00b7rem", "himm\u00b7li\u00b7schen", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der du mit finstern Blicken ganz ver\u00e4chtlich", "tokens": ["Der", "du", "mit", "fins\u00b7tern", "Bli\u00b7cken", "ganz", "ver\u00b7\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gl\u00fcck, Ruhm und Freuden \u00fcbersiehst,", "tokens": ["Gl\u00fcck", ",", "Ruhm", "und", "Freu\u00b7den", "\u00fc\u00b7ber\u00b7siehst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht mehr Lorenzo bist, und einsam mittern\u00e4chtlich", "tokens": ["Nicht", "mehr", "Lo\u00b7ren\u00b7zo", "bist", ",", "und", "ein\u00b7sam", "mit\u00b7ter\u00b7n\u00e4cht\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "NE", "VAFIN", "$,", "KON", "ADJD", "ADV"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Ein andrer Young, den Schlummer fliehst,", "tokens": ["Ein", "an\u00b7drer", "Y\u00b7o\u00b7ung", ",", "den", "Schlum\u00b7mer", "fliehst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und jammernd sitzest, hier, wo die Gebeine", "tokens": ["Und", "jam\u00b7mernd", "sit\u00b7zest", ",", "hier", ",", "wo", "die", "Ge\u00b7bei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "wo