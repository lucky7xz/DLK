 "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "APPR", "APPR", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Durch Kleider gepeiniget ganz und gar,", "tokens": ["Durch", "Klei\u00b7der", "ge\u00b7pei\u00b7ni\u00b7get", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "ADV", "KON", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Die die Menschenfigur ihm verhunzten!", "tokens": ["Die", "die", "Men\u00b7schen\u00b7fi\u00b7gur", "ihm", "ver\u00b7hunz\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Und das ... Einer dem Andern zu Gunsten!", "tokens": ["Und", "das", "...", "Ei\u00b7ner", "dem", "An\u00b7dern", "zu", "Guns\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "PIS", "ART", "ADJA", "APPR", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6rte den dumm-philistr\u00f6sesten Tropf", "tokens": ["Ich", "h\u00f6r\u00b7te", "den", "dum\u00b7mphi\u00b7lis\u00b7tr\u00f6\u00b7ses\u00b7ten", "Tropf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um alte Gesetzlasten klagen,", "tokens": ["Um", "al\u00b7te", "Ge\u00b7setz\u00b7las\u00b7ten", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sah' ihn voll Wuth mit dem eigenen Zopf", "tokens": ["Und", "sah'", "ihn", "voll", "Wut