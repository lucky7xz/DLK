{"dta.poem.22686": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "237. Die jnnerliche Geburt GOttes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ach freude! GOtt wird Mensch/ und ist auch schon", "tokens": ["Ach", "freu\u00b7de", "!", "Gott", "wird", "Mensch", "/", "und", "ist", "auch", "schon"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "NN", "VAFIN", "NN", "$(", "KON", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "gebohren!", "tokens": ["ge\u00b7boh\u00b7ren", "!"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Wo da? In mir: Er hat zur Mutter mich erkohren.", "tokens": ["Wo", "da", "?", "In", "mir", ":", "Er", "hat", "zur", "Mut\u00b7ter", "mich", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "APPR", "PPER", "$.", "PPER", "VAFIN", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie gehet es dann zu? ", "tokens": ["Wie", "ge\u00b7het", "es", "dann", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df Krippelein mein Hertz/ der Leib der ist die H\u00f6l:", "tokens": ["Da\u00df", "Krip\u00b7pe\u00b7lein", "mein", "Hertz", "/", "der", "Leib", "der", "ist", "die", "H\u00f6l", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$(", "ART", "NN", "ART", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die neu Gerechtigkeit sind Windeln und sind Binde\u0303:", "tokens": ["Die", "neu", "Ge\u00b7rech\u00b7tig\u00b7keit", "sind", "Win\u00b7deln", "und", "sind", "Bin\u00b7d\u1ebd", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "NN", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "Der Joseph GOttesfurcht: Die Kr\u00e4ffte de\u00df Gem\u00fctts", "tokens": ["Der", "Jo\u00b7se\u00b7ph", "Got\u00b7tes\u00b7furcht", ":", "Die", "Kr\u00e4ff\u00b7te", "de\u00df", "Ge\u00b7m\u00fctts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "NN", "$.", "ART", "NN", "ART", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Sind Engel die sich freun: Die Klarheit ist jhr Blitz:", "tokens": ["Sind", "En\u00b7gel", "die", "sich", "freun", ":", "Die", "Klar\u00b7heit", "ist", "jhr", "Blitz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PRELS", "PRF", "VVINF", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die keusche Sinnen sind die Hirten die jhn finden.", "tokens": ["Die", "keu\u00b7sche", "Sin\u00b7nen", "sind", "die", "Hir\u00b7ten", "die", "jhn", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ART", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}