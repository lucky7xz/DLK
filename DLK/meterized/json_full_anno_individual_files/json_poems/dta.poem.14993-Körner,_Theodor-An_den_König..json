{"dta.poem.14993": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An den K\u00f6nig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Bricht auch das Herz vom h\u00f6chsten Schmerz bezwungen,", "tokens": ["Bricht", "auch", "das", "Herz", "vom", "h\u00f6chs\u00b7ten", "Schmerz", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit letzter Kraft ", "tokens": ["Mit", "letz\u00b7ter", "Kraft"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Jammer stirbt im h\u00f6chsten Siegestone.", "tokens": ["Der", "Jam\u00b7mer", "stirbt", "im", "h\u00f6chs\u00b7ten", "Sie\u00b7ge\u00b7sto\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ja, bis das letzte deutsche Wort verklungen,", "tokens": ["Ja", ",", "bis", "das", "letz\u00b7te", "deut\u00b7sche", "Wort", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "ART", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Jauchzt noch das Vaterland von seinem Sohne,", "tokens": ["Jauchzt", "noch", "das", "Va\u00b7ter\u00b7land", "von", "sei\u00b7nem", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der k\u00e4mpfend f\u00fcr sein Volk und seine Krone,", "tokens": ["Der", "k\u00e4mp\u00b7fend", "f\u00fcr", "sein", "Volk", "und", "sei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Sich k\u00f6niglich den K\u00f6nigstod errungen!", "tokens": ["Sich", "k\u00f6\u00b7nig\u00b7lich", "den", "K\u00f6\u00b7nig\u00b7stod", "er\u00b7run\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Sieg fleugt auf, aus deines Blutes B\u00e4chen", "tokens": ["Der", "Sieg", "fleugt", "auf", ",", "aus", "dei\u00b7nes", "Blu\u00b7tes", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dein Name soll des W\u00fcthrichs Mauern brechen,", "tokens": ["Dein", "Na\u00b7me", "soll", "des", "W\u00fcth\u00b7richs", "Mau\u00b7ern", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Das treue Volk mu\u00df seinen K\u00f6nig r\u00e4chen! \u2014", "tokens": ["Das", "treu\u00b7e", "Volk", "mu\u00df", "sei\u00b7nen", "K\u00f6\u00b7nig", "r\u00e4\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Du aber, sanft entschlummert unter Leichen,", "tokens": ["Du", "a\u00b7ber", ",", "sanft", "ent\u00b7schlum\u00b7mert", "un\u00b7ter", "Lei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Erwache sanft in ", "tokens": ["Er\u00b7wa\u00b7che", "sanft", "in"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJD", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Die Palmen bl\u00fchn ", "tokens": ["Die", "Pal\u00b7men", "bl\u00fchn"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}}}}}