{"dta.poem.18641": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Cantata  \n  A uf den  G eburths- T ag  I hro  \n K\u00f6nigl. Majest. in D\u00e4nnen. Frieder.  \n des  IV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Begl\u00fccktes Licht!", "tokens": ["Be\u00b7gl\u00fcck\u00b7tes", "Licht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Das je die Welt gesehen/", "tokens": ["Das", "je", "die", "Welt", "ge\u00b7se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vor deren Glantz ein sterbliches Gesicht", "tokens": ["Vor", "de\u00b7ren", "Glantz", "ein", "sterb\u00b7li\u00b7ches", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Nur mu\u00df geb\u00fcckt/ und mit Erstaunen gehen.", "tokens": ["Nur", "mu\u00df", "ge\u00b7b\u00fcckt", "/", "und", "mit", "Er\u00b7stau\u00b7nen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVPP", "$(", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O Sonne! Deren Pracht", "tokens": ["O", "Son\u00b7ne", "!", "De\u00b7ren", "Pracht"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vollkommener als Phoebus Klarheit lacht!", "tokens": ["Voll\u00b7kom\u00b7me\u00b7ner", "als", "Phoe\u00b7bus", "Klar\u00b7heit", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NE", "NN", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Weil deren Purpur-Strahl so rein/", "tokens": ["Weil", "de\u00b7ren", "Pur\u00b7pur\u00b7Strahl", "so", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Tugenden die Flecken seyn.", "tokens": ["Das", "Tu\u00b7gen\u00b7den", "die", "Fle\u00b7cken", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Himmel ach! Durch deine G\u00fcte", "tokens": ["Him\u00b7mel", "ach", "!", "Durch", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ITJ", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmt man dich hier selber an.", "tokens": ["Nimmt", "man", "dich", "hier", "sel\u00b7ber", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gottes-Furcht und Gnade zeigen/", "tokens": ["Got\u00b7tes\u00b7Furcht", "und", "Gna\u00b7de", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie man hier zum Himmel steigen/", "tokens": ["Wie", "man", "hier", "zum", "Him\u00b7mel", "stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und bey G\u00f6ttern Leben kan.", "tokens": ["Und", "bey", "G\u00f6t\u00b7tern", "Le\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Himmel ach! Durch deine G\u00fcte", "tokens": ["Him\u00b7mel", "ach", "!", "Durch", "dei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ITJ", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimmt man dich hier selber an.", "tokens": ["Nimmt", "man", "dich", "hier", "sel\u00b7ber", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So blitzt die Landes-Sonne:", "tokens": ["So", "blitzt", "die", "Lan\u00b7des\u00b7Son\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O wunder sch\u00f6nes Licht!", "tokens": ["O", "wun\u00b7der", "sch\u00f6\u00b7nes", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An dem der Menschen Lust und Wonne/", "tokens": ["An", "dem", "der", "Men\u00b7schen", "Lust", "und", "Won\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vespasian durch Nordens-Himmel bricht.", "tokens": ["Ves\u00b7pa\u00b7si\u00b7an", "durch", "Nor\u00b7dens\u00b7Him\u00b7mel", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch k\u00f6nnen auch geringe D\u00fcnste-Strahlen/", "tokens": ["Doch", "k\u00f6n\u00b7nen", "auch", "ge\u00b7rin\u00b7ge", "D\u00fcns\u00b7te\u00b7Strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn sie der Sonnen Lieblichkeit/", "tokens": ["Wenn", "sie", "der", "Son\u00b7nen", "Lieb\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So offt sie will den Regen-Bogen mahlen/", "tokens": ["So", "offt", "sie", "will", "den", "Re\u00b7gen\u00b7Bo\u00b7gen", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit ihren Gold bestreut:", "tokens": ["Mit", "ih\u00b7ren", "Gold", "be\u00b7streut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Entz\u00fcnden sich in einer Brust", "tokens": ["Ent\u00b7z\u00fcn\u00b7den", "sich", "in", "ei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zur Danckbarkeit viel tausend Freuden-Kertzen/", "tokens": ["Zur", "Dan\u00b7ck\u00b7bar\u00b7keit", "viel", "tau\u00b7send", "Freu\u00b7den\u00b7Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "CARD", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "So brennt der Nord bey dieser G\u00f6tter-Lust", "tokens": ["So", "brennt", "der", "Nord", "bey", "die\u00b7ser", "G\u00f6t\u00b7ter\u00b7Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Von Millionen Hertzen.", "tokens": ["Von", "Mil\u00b7lion\u00b7en", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "So hoch das Gl\u00fcck den frohen Belt gesetzet/", "tokens": ["So", "hoch", "das", "Gl\u00fcck", "den", "fro\u00b7hen", "Belt", "ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "So gro\u00df hier das Vergn\u00fcgen ist/", "tokens": ["So", "gro\u00df", "hier", "das", "Ver\u00b7gn\u00fc\u00b7gen", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So sehr hat sich auch Lieb und Treu gek\u00fc\u00dft!", "tokens": ["So", "sehr", "hat", "sich", "auch", "Lieb", "und", "Treu", "ge\u00b7k\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PRF", "ADV", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und diesen Wunsch den Sternen einge\u00e4tzet.", "tokens": ["Und", "die\u00b7sen", "Wunsch", "den", "Ster\u00b7nen", "ein\u00b7ge\u00b7\u00e4t\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Himmel K\u00fcsse mit Ergetzen/", "tokens": ["Him\u00b7mel", "K\u00fcs\u00b7se", "mit", "Er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Was dir selber gleich zu sch\u00e4tzen/", "tokens": ["Was", "dir", "sel\u00b7ber", "gleich", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und la\u00df uns gl\u00fcckselig sehn.", "tokens": ["Und", "la\u00df", "uns", "gl\u00fcck\u00b7se\u00b7lig", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Friederich/ des Landes Sonne/", "tokens": ["Frie\u00b7de\u00b7rich", "/", "des", "Lan\u00b7des", "Son\u00b7ne", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Unterthanen Wonne", "tokens": ["Und", "der", "Un\u00b7ter\u00b7tha\u00b7nen", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wisse nichts von untergehn.", "tokens": ["Wis\u00b7se", "nichts", "von", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPR", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}