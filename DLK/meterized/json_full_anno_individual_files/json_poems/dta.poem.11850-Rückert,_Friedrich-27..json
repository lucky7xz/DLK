{"dta.poem.11850": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht hemme du im Gang die sinnlichen Gen\u00fcsse,", "tokens": ["Nicht", "hem\u00b7me", "du", "im", "Gang", "die", "sinn\u00b7li\u00b7chen", "Ge\u00b7n\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Leibeshaushalt braucht als Zufl\u00fcss' und Abfl\u00fcsse.", "tokens": ["Die", "Lei\u00b7bes\u00b7haus\u00b7halt", "braucht", "als", "Zu\u00b7fl\u00fcss'", "und", "Ab\u00b7fl\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Schaffner schaffe nur im Kreis, der ihm geh\u00f6rt,", "tokens": ["Der", "Schaff\u00b7ner", "schaf\u00b7fe", "nur", "im", "Kreis", ",", "der", "ihm", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit die Herrin herrsch' im Innern ungest\u00f6rt.", "tokens": ["Da\u00b7mit", "die", "Her\u00b7rin", "herr\u00b7sch'", "im", "In\u00b7nern", "un\u00b7ge\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-++