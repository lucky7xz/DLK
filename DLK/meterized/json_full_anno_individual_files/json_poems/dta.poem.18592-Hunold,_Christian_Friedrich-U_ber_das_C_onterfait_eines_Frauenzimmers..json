{"dta.poem.18592": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "U ber das  C onterfait eines  \n Frauenzimmers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dieses Conterfait dir welche Sch\u00f6nheit zeiget/", "tokens": ["Wenn", "die\u00b7ses", "Con\u00b7ter\u00b7fait", "dir", "wel\u00b7che", "Sch\u00f6n\u00b7heit", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "PWAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So dencke/ da\u00df es doch den sch\u00f6nsten Theil vermist", "tokens": ["So", "den\u00b7cke", "/", "da\u00df", "es", "doch", "den", "sch\u00f6ns\u00b7ten", "Theil", "ver\u00b7mist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil das Original die Kunst noch \u00fcbersteiget/", "tokens": ["Weil", "das", "O\u00b7rig\u00b7i\u00b7nal", "die", "Kunst", "noch", "\u00fc\u00b7bers\u00b7tei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Das nur bewundrens werth doch nicht zu mahlen ist.", "tokens": ["Das", "nur", "be\u00b7wund\u00b7rens", "werth", "doch", "nicht", "zu", "mah\u00b7len", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJD", "ADV", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}