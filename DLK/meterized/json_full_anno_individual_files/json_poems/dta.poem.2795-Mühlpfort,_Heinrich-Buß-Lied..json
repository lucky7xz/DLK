{"dta.poem.2795": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bu\u00df-Lied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje kan ich/ HErr/ die Missethat verh\u00f6len/", "tokens": ["Wie", "kan", "ich", "/", "Herr", "/", "die", "Mis\u00b7se\u00b7that", "ver\u00b7h\u00f6\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "$(", "NN", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und meine S\u00fcnde decken zu?", "tokens": ["Und", "mei\u00b7ne", "S\u00fcn\u00b7de", "de\u00b7cken", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie l\u00e4st nicht dem Gewissen Ruh/", "tokens": ["Sie", "l\u00e4st", "nicht", "dem", "Ge\u00b7wis\u00b7sen", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ist ein schneidend Schwerdt in meiner Seelen.", "tokens": ["Und", "ist", "ein", "schnei\u00b7dend", "Schwerdt", "in", "mei\u00b7ner", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drumb fallen alle Kr\u00e4fften hin/", "tokens": ["Drumb", "fal\u00b7len", "al\u00b7le", "Kr\u00e4ff\u00b7ten", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin nicht mehr/ der ich gewesen bin.", "tokens": ["Ich", "bin", "nicht", "mehr", "/", "der", "ich", "ge\u00b7we\u00b7sen", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "$(", "PRELS", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gleichwie ein Brunn/ so trocknet mein Gebeine/", "tokens": ["Gleich\u00b7wie", "ein", "Brunn", "/", "so", "trock\u00b7net", "mein", "Ge\u00b7bei\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich sehe schon den Todten gleich/", "tokens": ["Ich", "se\u00b7he", "schon", "den", "Tod\u00b7ten", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mein faulend Fleisch ist eine Leich.", "tokens": ["Mein", "fau\u00b7lend", "Fleisch", "ist", "ei\u00b7ne", "Leich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die Schuld druckt mich wie grosse Felsen-Steine.", "tokens": ["Die", "Schuld", "druckt", "mich", "wie", "gros\u00b7se", "Fel\u00b7sen\u00b7Stei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Es seufzt mein Hertz bey Tag und Nacht/", "tokens": ["Es", "seufzt", "mein", "Hertz", "bey", "Tag", "und", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und der zerknirschte Geist ist fast verschmacht.", "tokens": ["Und", "der", "zer\u00b7knirschte", "Geist", "ist", "fast", "ver\u00b7schmacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Drumb will ich frey die Ubertrettung sagen.", "tokens": ["Drumb", "will", "ich", "frey", "die", "U\u00b7bert\u00b7ret\u00b7tung", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ich habe dein Gebot veracht/", "tokens": ["Ich", "ha\u00b7be", "dein", "Ge\u00b7bot", "ver\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Nicht deinen Worten nachgedacht", "tokens": ["Nicht", "dei\u00b7nen", "Wor\u00b7ten", "nach\u00b7ge\u00b7dacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Den Eyffer dich zu ehren ausgeschlagen.", "tokens": ["Den", "Eyf\u00b7fer", "dich", "zu", "eh\u00b7ren", "aus\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Nun f\u00fchl ich meiner B\u00fcrden Last/", "tokens": ["Nun", "f\u00fchl", "ich", "mei\u00b7ner", "B\u00fcr\u00b7den", "Last", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Wormit du mich/ mein GOtt/ beleget hast.", "tokens": ["Wor\u00b7mit", "du", "mich", "/", "mein", "Gott", "/", "be\u00b7le\u00b7get", "hast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "$(", "PPOSAT", "NN", "$(", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Wer wird mich nun von deinem Fluch befreyen?", "tokens": ["Wer", "wird", "mich", "nun", "von", "dei\u00b7nem", "Fluch", "be\u00b7fre\u00b7yen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Mein eitel Opffer stinckt mich an.", "tokens": ["Mein", "ei\u00b7tel", "Opf\u00b7fer", "stinckt", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und weil ich niemals Guts gethan/", "tokens": ["Und", "weil", "ich", "nie\u00b7mals", "Guts", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "So mu\u00df ich \u00fcber mich erb\u00e4rmlich schreyen/", "tokens": ["So", "mu\u00df", "ich", "\u00fc\u00b7ber", "mich", "er\u00b7b\u00e4rm\u00b7lich", "schre\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Da\u00df ich der Welt ein Schand-Altar", "tokens": ["Da\u00df", "ich", "der", "Welt", "ein", "Schan\u00b7dAl\u00b7tar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Gebaut/ und dir zu opffern s\u00e4umig war.", "tokens": ["Ge\u00b7baut", "/", "und", "dir", "zu", "opf\u00b7fern", "s\u00e4u\u00b7mig", "war", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$(", "KON", "PPER", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ich rauche noch vom Brand der tollen L\u00fcste/", "tokens": ["Ich", "rau\u00b7che", "noch", "vom", "Brand", "der", "tol\u00b7len", "L\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Man sp\u00fcrt noch der Begierden Feur/", "tokens": ["Man", "sp\u00fcrt", "noch", "der", "Be\u00b7gier\u00b7den", "Feur", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der schn\u00f6den S\u00fcnden Ungeheur/", "tokens": ["Der", "schn\u00f6\u00b7den", "S\u00fcn\u00b7den", "Un\u00b7ge\u00b7heur", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Ach/ da\u00df mich doch die Erde decken m\u00fcste.", "tokens": ["Ach", "/", "da\u00df", "mich", "doch", "die", "Er\u00b7de", "de\u00b7cken", "m\u00fcs\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Wie aber will ich dir entfliehn/", "tokens": ["Wie", "a\u00b7ber", "will", "ich", "dir", "ent\u00b7fliehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und der verdienten Strafe mich entziehn?", "tokens": ["Und", "der", "ver\u00b7dien\u00b7ten", "Stra\u00b7fe", "mich", "ent\u00b7ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Du wollst doch nicht das Hals-Gerichte h\u00e4gen/", "tokens": ["Du", "wollst", "doch", "nicht", "das", "Hals\u00b7Ge\u00b7rich\u00b7te", "h\u00e4\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Das ich hoch-peinlich leiden mu\u00df/", "tokens": ["Das", "ich", "hoch\u00b7pein\u00b7lich", "lei\u00b7den", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Mich treten unter deinen Fu\u00df.", "tokens": ["Mich", "tre\u00b7ten", "un\u00b7ter", "dei\u00b7nen", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Dein Sohn erwarb vor Strafe mir den Seegen.", "tokens": ["Dein", "Sohn", "er\u00b7warb", "vor", "Stra\u00b7fe", "mir", "den", "See\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Ach la\u00df Genade gehn f\u00fcr Recht/", "tokens": ["Ach", "la\u00df", "Ge\u00b7na\u00b7de", "gehn", "f\u00fcr", "Recht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und sieh doch an barmhertzig deinen Knecht.", "tokens": ["Und", "sieh", "doch", "an", "barm\u00b7hert\u00b7zig", "dei\u00b7nen", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "APPR", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Wie soll ich armer Mensch seyn unbeflecket/", "tokens": ["Wie", "soll", "ich", "ar\u00b7mer", "Mensch", "seyn", "un\u00b7be\u00b7fle\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Die Himmel selbsten sind nicht rein?", "tokens": ["Die", "Him\u00b7mel", "selbs\u00b7ten", "sind", "nicht", "rein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Der Schlu\u00df ist wahr und allgemein/", "tokens": ["Der", "Schlu\u00df", "ist", "wahr", "und", "all\u00b7ge\u00b7mein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Da\u00df in des Menschen Hertz nur Thorheit stecket.", "tokens": ["Da\u00df", "in", "des", "Men\u00b7schen", "Hertz", "nur", "Thor\u00b7heit", "ste\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Und so du uns zeuchst f\u00fcr Gericht/", "tokens": ["Und", "so", "du", "uns", "zeuchst", "f\u00fcr", "Ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PPER", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.42": {"text": "Herr/ so besteht kein Mensch der lebet/ nicht.", "tokens": ["Herr", "/", "so", "be\u00b7steht", "kein", "Mensch", "der", "le\u00b7bet", "/", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "ADV", "VVFIN", "PIAT", "NN", "ART", "VVFIN", "$(", "PTKNEG", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.43": {"text": "Ach sprich mich lo\u00df/ erla\u00df mich meiner Schulden/", "tokens": ["Ach", "sprich", "mich", "lo\u00df", "/", "er\u00b7la\u00df", "mich", "mei\u00b7ner", "Schul\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PTKVZ", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Weil du gen\u00e4dig bist und heist!", "tokens": ["Weil", "du", "ge\u00b7n\u00e4\u00b7dig", "bist", "und", "heist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": 