{"textgrid.poem.59495": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Weh Valencia!", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eingeschlossen war Valencia,", "tokens": ["Ein\u00b7ge\u00b7schlos\u00b7sen", "war", "Va\u00b7len\u00b7cia", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Konnte kaum sich l\u00e4nger wahren,", "tokens": ["Konn\u00b7te", "kaum", "sich", "l\u00e4n\u00b7ger", "wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil sich die Almoraviden", "tokens": ["Weil", "sich", "die", "Al\u00b7mo\u00b7ra\u00b7vi\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Z\u00f6gernd nicht zum Beistand wandten.", "tokens": ["Z\u00f6\u00b7gernd", "nicht", "zum", "Bei\u00b7stand", "wand\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da dies sah ein alter Maure:", "tokens": ["Da", "dies", "sah", "ein", "al\u00b7ter", "Mau\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Auf des h\u00f6chsten Turmes Warte", "tokens": ["Auf", "des", "h\u00f6chs\u00b7ten", "Tur\u00b7mes", "War\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Stieg er schweigend da, noch einmal", "tokens": ["Stieg", "er", "schwei\u00b7gend", "da", ",", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "ADJD", "PTKVZ", "$,", "ADV", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Zu beschauen Stadt und Lande.", "tokens": ["Zu", "be\u00b7schau\u00b7en", "Stadt", "und", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und wie sie herauf so leuchten,", "tokens": ["Und", "wie", "sie", "her\u00b7auf", "so", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Brach das Herz ihm bei dem Glanze;", "tokens": ["Brach", "das", "Herz", "ihm", "bei", "dem", "Glan\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Gramvoll mit prophet'schem Munde", "tokens": ["Gram\u00b7voll", "mit", "pro\u00b7phet'\u00b7schem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Also von dem Turme sprach er:", "tokens": ["Al\u00b7so", "von", "dem", "Tur\u00b7me", "sprach", "er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "\u00bbo Valencia, o Valencia,", "tokens": ["\u00bb", "o", "Va\u00b7len\u00b7cia", ",", "o", "Va\u00b7len\u00b7cia", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "W\u00fcrd'ge Herrscherin der Lande,", "tokens": ["W\u00fcrd'\u00b7ge", "Herr\u00b7sche\u00b7rin", "der", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Deine heitre Pracht mu\u00df sinken,", "tokens": ["Dei\u00b7ne", "heit\u00b7re", "Pracht", "mu\u00df", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "So sich Gott nicht dein erbarmet!", "tokens": ["So", "sich", "Gott", "nicht", "dein", "er\u00b7bar\u00b7met", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "NN", "PTKNEG", "PPOSAT", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Die vier Felsen, drauf du thronest,", "tokens": ["Die", "vier", "Fel\u00b7sen", ",", "drauf", "du", "thro\u00b7nest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "W\u00fcrden, wenn sie k\u00f6nnten, klagen,", "tokens": ["W\u00fcr\u00b7den", ",", "wenn", "sie", "k\u00f6nn\u00b7ten", ",", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "VMFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Deine festen Mauern seh ich", "tokens": ["Dei\u00b7ne", "fes\u00b7ten", "Mau\u00b7ern", "seh", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Von dem wilden Anlauf wanken,", "tokens": ["Von", "dem", "wil\u00b7den", "An\u00b7lauf", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Deine T\u00fcrme, die so trostreich", "tokens": ["Dei\u00b7ne", "T\u00fcr\u00b7me", ",", "die", "so", "trost\u00b7reich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "\u00dcber Land und V\u00f6lker ragen,", "tokens": ["\u00dc\u00b7ber", "Land", "und", "V\u00f6l\u00b7ker", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Werden unaufhaltsam st\u00fcrzen,", "tokens": ["Wer\u00b7den", "un\u00b7auf\u00b7halt\u00b7sam", "st\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Deine Zinnen, gleich Kristallen,", "tokens": ["Dei\u00b7ne", "Zin\u00b7nen", ",", "gleich", "Kris\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Ihren Wunderglanz verl\u00f6schen,", "tokens": ["Ih\u00b7ren", "Wun\u00b7der\u00b7glanz", "ver\u00b7l\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Und dein m\u00e4cht'ger Guadalaviar", "tokens": ["Und", "dein", "m\u00e4cht'\u00b7ger", "Gua\u00b7da\u00b7la\u00b7vi\u00b7ar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Wird aus seinen Ufern steigen,", "tokens": ["Wird", "aus", "sei\u00b7nen", "U\u00b7fern", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Tr\u00fcben jeden Bach im Lande.", "tokens": ["Tr\u00fc\u00b7ben", "je\u00b7den", "Bach", "im", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "In den trocknen Wasserk\u00fcnsten", "tokens": ["In", "den", "trock\u00b7nen", "Was\u00b7ser\u00b7k\u00fcns\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Funkeln nimmermehr die Strahlen,", "tokens": ["Fun\u00b7keln", "nim\u00b7mer\u00b7mehr", "die", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Rings in deinen sch\u00f6nen G\u00e4rten,", "tokens": ["Rings", "in", "dei\u00b7nen", "sch\u00f6\u00b7nen", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Die fortan verwildernd ranken,", "tokens": ["Die", "for\u00b7tan", "ver\u00b7wil\u00b7dernd", "ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Werden Hirsche einsam grasen,", "tokens": ["Wer\u00b7den", "Hir\u00b7sche", "ein\u00b7sam", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Alles fr\u00f6hl'che Gr\u00fcn zernagend.", "tokens": ["Al\u00b7les", "fr\u00f6hl'\u00b7che", "Gr\u00fcn", "zer\u00b7na\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Keinen Duft mehr haucht die Luft her,", "tokens": ["Kei\u00b7nen", "Duft", "mehr", "haucht", "die", "Luft", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wo vieltausend Blumen standen,", "tokens": ["Wo", "viel\u00b7tau\u00b7send", "Blu\u00b7men", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Mu\u00df das Gl\u00fchen all verbl\u00fchen;", "tokens": ["Mu\u00df", "das", "Gl\u00fc\u00b7hen", "all", "ver\u00b7bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PIAT", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Wo jetzt Schiffe kommen, fahren,", "tokens": ["Wo", "jetzt", "Schif\u00b7fe", "kom\u00b7men", ",", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Liegt ver\u00f6det Strand und Hafen,", "tokens": ["Liegt", "ver\u00b7\u00f6\u00b7det", "Strand", "und", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Und vom weiten Bergeskranze,", "tokens": ["Und", "vom", "wei\u00b7ten", "Ber\u00b7ges\u00b7kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Den du m\u00e4chtig einst beherrschtest,", "tokens": ["Den", "du", "m\u00e4ch\u00b7tig", "einst", "be\u00b7herrschtest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Schlagen blutrot auf die Flammen,", "tokens": ["Schla\u00b7gen", "blut\u00b7rot", "auf", "die", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Da\u00df das Qualmen dich erblindet", "tokens": ["Da\u00df", "das", "Qual\u00b7men", "dich", "er\u00b7blin\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Rings von deiner L\u00e4nder Brande,", "tokens": ["Rings", "von", "dei\u00b7ner", "L\u00e4n\u00b7der", "Bran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Bis, als eine Todeswunde", "tokens": ["Bis", ",", "als", "ei\u00b7ne", "To\u00b7des\u00b7wun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["APPR"