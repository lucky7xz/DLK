{"dta.poem.23725": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Z\u00e4nker.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es zanken sich die Redner,", "tokens": ["Es", "zan\u00b7ken", "sich", "die", "Red\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um W\u00f6rter und um Silben,", "tokens": ["Um", "W\u00f6r\u00b7ter", "und", "um", "Sil\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schimpfen, wenn sie zanken.", "tokens": ["Und", "schimp\u00b7fen", ",", "wenn", "sie", "zan\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es zanken sich die Prinzen,", "tokens": ["Es", "zan\u00b7ken", "sich", "die", "Prin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Um Gold und veste St\u00e4dte,", "tokens": ["Um", "Gold", "und", "ves\u00b7te", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sterben, eh sie siegen.", "tokens": ["Und", "ster\u00b7ben", ",", "eh", "sie", "sie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es zanken sich die Weisen", "tokens": ["Es", "zan\u00b7ken", "sich", "die", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um gut und b\u00f6se Welten,", "tokens": ["Um", "gut", "und", "b\u00f6\u00b7se", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und bauen keine bessre.", "tokens": ["Und", "bau\u00b7en", "kei\u00b7ne", "bess\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Zankt immer, tapfre Z\u00e4nker,", "tokens": ["Zankt", "im\u00b7mer", ",", "tapf\u00b7re", "Z\u00e4n\u00b7ker", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Zankt tapfer, eh ihr sterbet!", "tokens": ["Zankt", "tap\u00b7fer", ",", "eh", "ihr", "ster\u00b7bet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Um L\u00e4nder, oder Reime,", "tokens": ["Um", "L\u00e4n\u00b7der", ",", "o\u00b7der", "Rei\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Will ich mit euch nicht zanken.", "tokens": ["Will", "ich", "mit", "euch", "nicht", "zan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.14": {"text": "Doch, wolt ihr mit mir zanken;", "tokens": ["Doch", ",", "wolt", "ihr", "mit", "mir", "zan\u00b7ken", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "So tadelt diese B\u00f6hmin.", "tokens": ["So", "ta\u00b7delt", "die\u00b7se", "B\u00f6h\u00b7min", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}