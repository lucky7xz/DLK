{"dta.poem.11614": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Tag und Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Schlank und sch\u00f6n ein Mohrenknabe", "tokens": ["Schlank", "und", "sch\u00f6n", "ein", "Moh\u00b7ren\u00b7kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bringt in himmelblauer Sch\u00fcrze", "tokens": ["Bringt", "in", "him\u00b7mel\u00b7blau\u00b7er", "Sch\u00fcr\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Manche wundersame Gabe,", "tokens": ["Man\u00b7che", "wun\u00b7der\u00b7sa\u00b7me", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchlen Duft und s\u00fc\u00dfe W\u00fcrze.", "tokens": ["K\u00fch\u00b7len", "Duft", "und", "s\u00fc\u00b7\u00dfe", "W\u00fcr\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Abendl\u00fcfte wehen,", "tokens": ["Wenn", "die", "A\u00b7ben\u00b7dl\u00fcf\u00b7te", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Naht er sachte, kaum gesehen,", "tokens": ["Naht", "er", "sach\u00b7te", ",", "kaum", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat ein Harfenspiel zur Hand.", "tokens": ["Hat", "ein", "Har\u00b7fen\u00b7spiel", "zur", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch der Saiten sanftes T\u00f6nen", "tokens": ["Auch", "der", "Sai\u00b7ten", "sanf\u00b7tes", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann man n\u00e4chtlich lauschend h\u00f6ren;", "tokens": ["Kann", "man", "n\u00e4cht\u00b7lich", "lau\u00b7schend", "h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch scheint Alles seiner Sch\u00f6nen,", "tokens": ["Doch", "scheint", "Al\u00b7les", "sei\u00b7ner", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ungetreuen zu geh\u00f6ren.", "tokens": ["Un\u00b7ge\u00b7treu\u00b7en", "zu", "ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und er wandelt, bis am Haine,", "tokens": ["Und", "er", "wan\u00b7delt", ",", "bis", "am", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis am See und Wiesenraine", "tokens": ["Bis", "am", "See", "und", "Wie\u00b7sen\u00b7rai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Er die Spur der Liebsten fand.", "tokens": ["Er", "die", "Spur", "der", "Liebs\u00b7ten", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl ein L\u00e4cheln mag sich leise", "tokens": ["Wohl", "ein", "L\u00e4\u00b7cheln", "mag", "sich", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VMFIN", "PRF", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dann ins ernste Antlitz neigen,", "tokens": ["Dann", "ins", "erns\u00b7te", "Ant\u00b7litz", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00dfe Z\u00e4hne, schneeig-wei\u00dfe,", "tokens": ["Wei\u00b7\u00dfe", "Z\u00e4h\u00b7ne", ",", "schne\u00b7e\u00b7ig\u00b7wei\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sich wie Sternenlichter zeigen.", "tokens": ["Sich", "wie", "Ster\u00b7nen\u00b7lich\u00b7ter", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch ihn fa\u00dft ein reizend Bangen,", "tokens": ["Doch", "ihn", "fa\u00dft", "ein", "rei\u00b7zend", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt von Ferne Sie gegangen,", "tokens": ["Kommt", "von", "Fer\u00b7ne", "Sie", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und er sucht sein dunkel Haus.", "tokens": ["Und", "er", "sucht", "sein", "dun\u00b7kel", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Liebchen tritt von Bergesh\u00f6hen", "tokens": ["Lieb\u00b7chen", "tritt", "von", "Ber\u00b7ges\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das Thal: da wird es Freude!", "tokens": ["In", "das", "Thal", ":", "da", "wird", "es", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wald und Flur wie neu erstehen", "tokens": ["Wald", "und", "Flur", "wie", "neu", "er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KOKOM", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem Kind im Rosenkleide;", "tokens": ["Vor", "dem", "Kind", "im", "Ro\u00b7sen\u00b7klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles dr\u00e4ngt sich nach der S\u00fc\u00dfen,", "tokens": ["Al\u00b7les", "dr\u00e4ngt", "sich", "nach", "der", "S\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alt und Jung will sie begr\u00fc\u00dfen,", "tokens": ["Alt", "und", "Jung", "will", "sie", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur der Knabe bleibet aus.", "tokens": ["Nur", "der", "Kna\u00b7be", "blei\u00b7bet", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und doch ist ein tiefes Ahnen", "tokens": ["Und", "doch", "ist", "ein", "tie\u00b7fes", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem Fremdling ihr geblieben;", "tokens": ["Von", "dem", "Fremd\u00b7ling", "ihr", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein Traum will sie's gemahnen", "tokens": ["Wie", "ein", "Traum", "will", "sie's", "ge\u00b7mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VMFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An ein fr\u00fch gehegtes Lieben.", "tokens": ["An", "ein", "fr\u00fch", "ge\u00b7heg\u00b7tes", "Lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gl\u00e4nzen dann auf allen Wegen", "tokens": ["Gl\u00e4n\u00b7zen", "dann", "auf", "al\u00b7len", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schmuck und Perlen ihr entgegen,", "tokens": ["Schmuck", "und", "Per\u00b7len", "ihr", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denkt sie wohl, wer es gebracht.", "tokens": ["Denkt", "sie", "wohl", ",", "wer", "es", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schnell den Schleier vorgezogen,", "tokens": ["Schnell", "den", "Schlei\u00b7er", "vor\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht das T\u00f6chterchen in Thr\u00e4nen,", "tokens": ["Steht", "das", "T\u00f6ch\u00b7ter\u00b7chen", "in", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Mutter Friedensbogen", "tokens": ["Und", "der", "Mut\u00b7ter", "Frie\u00b7dens\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neigt sich thauend ihrem Sehnen;", "tokens": ["Neigt", "sich", "thau\u00b7end", "ih\u00b7rem", "Seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Erd' und Himmel haben Frieden,", "tokens": ["Erd'", "und", "Him\u00b7mel", "ha\u00b7ben", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber ach, ", "tokens": ["A\u00b7ber", "ach", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "XY", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sind getrennt, wie ", "tokens": ["Sind", "ge\u00b7trennt", ",", "wie"], "token_info": ["word", "word", "punct", "word"], "pos": ["VAFIN", "VVPP", "$,", "PWAV"], "meter": "+-+-", "measure": "trochaic.di"}}}}}