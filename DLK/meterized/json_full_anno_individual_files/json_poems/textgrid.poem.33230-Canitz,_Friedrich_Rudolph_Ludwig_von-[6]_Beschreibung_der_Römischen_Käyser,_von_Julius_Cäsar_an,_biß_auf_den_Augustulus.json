{"textgrid.poem.33230": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "[6] Beschreibung der R\u00f6mischen K\u00e4yser, von Julius C\u00e4sar an, bi\u00df auf den Augustulus", "genre": "verse", "period": "N.A.", "pub_year": 1676, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erst macht sich Julius Roms Freyheit unterthan,", "tokens": ["Erst", "macht", "sich", "Ju\u00b7lius", "Roms", "Frey\u00b7heit", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "NE", "NE", "NN", "ADJD", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In dem verwirrten Reich folgt ihm Octavian.", "tokens": ["In", "dem", "ver\u00b7wirr\u00b7ten", "Reich", "folgt", "ihm", "Oc\u00b7ta\u00b7vi\u00b7an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Tiberius, nach ihm, ist voll von b\u00f6sen T\u00fccken,", "tokens": ["Ti\u00b7be\u00b7ri\u00b7us", ",", "nach", "ihm", ",", "ist", "voll", "von", "b\u00f6\u00b7sen", "T\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "PPER", "$,", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Und an Caligula sonst wenig zu erblicken,", "tokens": ["Und", "an", "Ca\u00b7li\u00b7gu\u00b7la", "sonst", "we\u00b7nig", "zu", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADV", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.5": {"text": "Als Grimm und Aberwitz. Der dumme Claudius,", "tokens": ["Als", "Grimm", "und", "A\u00b7ber\u00b7witz", ".", "Der", "dum\u00b7me", "Clau\u00b7di\u00b7us", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "$.", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So gleichfals ein Tyrann, erlebet den Verdru\u00df,", "tokens": ["So", "gleich\u00b7fals", "ein", "Ty\u00b7rann", ",", "er\u00b7le\u00b7bet", "den", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Da\u00df sein verbuhltes Weib mit andern sich verm\u00e4hlet.", "tokens": ["Da\u00df", "sein", "ver\u00b7buhl\u00b7tes", "Weib", "mit", "an\u00b7dern", "sich", "ver\u00b7m\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PIS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie wird der Christen-Schaar zu Nerons Zeit gequ\u00e4let!", "tokens": ["Wie", "wird", "der", "Chris\u00b7ten\u00b7Schaar", "zu", "Ne\u00b7rons", "Zeit", "ge\u00b7qu\u00e4\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der ietzt durch Mutter-Mord, durch angelegten Brand", "tokens": ["Der", "ietzt", "durch", "Mut\u00b7ter\u00b7Mord", ",", "durch", "an\u00b7ge\u00b7leg\u00b7ten", "Brand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und tausend Grausamkeit der Nachwelt noch bekannt.", "tokens": ["Und", "tau\u00b7send", "Grau\u00b7sam\u00b7keit", "der", "Nach\u00b7welt", "noch", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als Galba f\u00e4llt durch Geitz, wird Otto zwar erkohren,", "tokens": ["Als", "Gal\u00b7ba", "f\u00e4llt", "durch", "Geitz", ",", "wird", "Ot\u00b7to", "zwar", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "APPR", "NN", "$,", "VAFIN", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der aus Verzweifflung doch, nachdem die Schlacht verlohren,", "tokens": ["Der", "aus", "Ver\u00b7zweif\u00b7flung", "doch", ",", "nach\u00b7dem", "die", "Schlacht", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "$,", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sein eigner M\u00f6rder ist. Vitellius, verha\u00dft,", "tokens": ["Sein", "eig\u00b7ner", "M\u00f6r\u00b7der", "ist", ".", "Vi\u00b7tel\u00b7lius", ",", "ver\u00b7ha\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$.", "FM.la", "$,", "ADJD", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Weil er in Schlemmerey viel Gut und Blut verpra\u00dft,", "tokens": ["Weil", "er", "in", "Schlem\u00b7me\u00b7rey", "viel", "Gut", "und", "Blut", "ver\u00b7pra\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "ADJD", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wird, wie ein Aa\u00df, geschleppt. Vespasianus G\u00fcte", "tokens": ["Wird", ",", "wie", "ein", "Aa\u00df", ",", "ge\u00b7schleppt", ".", "Ves\u00b7pa\u00b7si\u00b7a\u00b7nus", "G\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "$,", "PWAV", "ART", "NN", "$,", "VVPP", "$.", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Begl\u00fcckt das K\u00e4yserthum. In Titus gro\u00df Gem\u00fcthe", "tokens": ["Be\u00b7gl\u00fcckt", "das", "K\u00e4y\u00b7ser\u00b7thum", ".", "In", "Ti\u00b7tus", "gro\u00df", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "APPR", "NE", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ist alle Welt verliebt; wiewol die heilge Stadt", "tokens": ["Ist", "al\u00b7le", "Welt", "ver\u00b7liebt", ";", "wie\u00b7wol", "die", "heil\u00b7ge", "Stadt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$.", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Des Himmels schwehren Zorn durch ihn empfunden hat.", "tokens": ["Des", "Him\u00b7mels", "schweh\u00b7ren", "Zorn", "durch", "ihn", "emp\u00b7fun\u00b7den", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "PPER", "VVPP", "VAFIN", "$."], "