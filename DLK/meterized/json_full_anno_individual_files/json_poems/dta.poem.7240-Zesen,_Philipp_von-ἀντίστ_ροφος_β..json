{"dta.poem.7240": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "\u1f00\u03bd\u03c4\u03af\u03c3\u03c4 \u03c1\u03bf\u03c6\u03bf\u03c2 \u03b2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1640", "urn": "urn:nbn:de:kobv:b4-20685-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ey Jhr Menschen kompt vnd grabet", "tokens": ["Ey", "Ihr", "Men\u00b7schen", "kompt", "vnd", "gra\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jhm ein Sanfftes Ruhebett", "tokens": ["Jhm", "ein", "Sanff\u00b7tes", "Ru\u00b7he\u00b7bett"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "vnd bereitet solche stett", "tokens": ["vnd", "be\u00b7rei\u00b7tet", "sol\u00b7che", "stett"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "mit dem Balsam den Jhr habet/", "tokens": ["mit", "dem", "Bal\u00b7sam", "den", "Ihr", "ha\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "PPER", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der da giebet allezeit", "tokens": ["Der", "da", "gie\u00b7bet", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "vns die vnverwe\u00dfligkeit;", "tokens": ["vns", "die", "vn\u00b7ver\u00b7we\u00df\u00b7lig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "vnd mit blumen ziert die Bahre/", "tokens": ["vnd", "mit", "blu\u00b7men", "ziert", "die", "Bah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "die gebl\u00fcht in diesem Jahre:", "tokens": ["die", "ge\u00b7bl\u00fcht", "in", "die\u00b7sem", "Jah\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nehmet weissen Marmelstein/", "tokens": ["Neh\u00b7met", "weis\u00b7sen", "Mar\u00b7mel\u00b7stein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Last Jhn diese Grabschrifft haben", "tokens": ["Last", "Jhn", "die\u00b7se", "Grab\u00b7schrifft", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "In sich zierlich aus gegraben/", "tokens": ["In", "sich", "zier\u00b7lich", "aus", "ge\u00b7gra\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJD", "APPR", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie es m\u00fcglich auch mag seyn.", "tokens": ["Wie", "es", "m\u00fcg\u00b7lich", "auch", "mag", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADV", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}