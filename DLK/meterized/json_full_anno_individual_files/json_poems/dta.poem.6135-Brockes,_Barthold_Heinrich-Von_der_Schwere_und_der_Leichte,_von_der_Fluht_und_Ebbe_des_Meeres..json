{"dta.poem.6135": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bey'm Neu- und Vollen Monden-Schein,", "tokens": ["Bey'm", "Neu", "und", "Vol\u00b7len", "Mon\u00b7den\u00b7Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird allezeit das Meer geschwollner seyn.", "tokens": ["Wird", "al\u00b7le\u00b7zeit", "das", "Meer", "ge\u00b7schwoll\u00b7ner", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn Tag und N\u00e4chte gleich, erhebt es sich noch mehr.", "tokens": ["Wenn", "Tag", "und", "N\u00e4ch\u00b7te", "gleich", ",", "er\u00b7hebt", "es", "sich", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "$,", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zwar daher:", "tokens": ["Und", "zwar", "da\u00b7her", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Weil zu derselben Zeit", "tokens": ["Weil", "zu", "der\u00b7sel\u00b7ben", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die Fluht gedr\u00fccket wird, mit gr\u00f6ssrer Hefftigkeit.", "tokens": ["Die", "Fluht", "ge\u00b7dr\u00fc\u00b7cket", "wird", ",", "mit", "gr\u00f6ss\u00b7rer", "Heff\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Mond w\u00fcrckt auf die Lufft, die wieder auf die Fluht,", "tokens": ["Der", "Mond", "w\u00fcrckt", "auf", "die", "Lufft", ",", "die", "wie\u00b7der", "auf", "die", "Fluht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und nimmer siehet man, da\u00df sie es anders thut.", "tokens": ["Und", "nim\u00b7mer", "sie\u00b7het", "man", ",", "da\u00df", "sie", "es", "an\u00b7ders", "thut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}