{"textgrid.poem.41650": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo er auch weilen mag, zu Land wie auf dem Meere,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo er auch weilen mag, zu Land wie auf dem Meere,", "tokens": ["Wo", "er", "auch", "wei\u00b7len", "mag", ",", "zu", "Land", "wie", "auf", "dem", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMFIN", "$,", "APPR", "NN", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In hei\u00dfer Tropenglut, auf wei\u00dfbesonntem Firn,", "tokens": ["In", "hei\u00b7\u00dfer", "Tro\u00b7pen\u00b7glut", ",", "auf", "wei\u00df\u00b7be\u00b7sonn\u00b7tem", "Firn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mag er als Jesu Knecht, als H\u00f6fling der Cythere,", "tokens": ["Mag", "er", "als", "Je\u00b7su", "Knecht", ",", "als", "H\u00f6f\u00b7ling", "der", "Cy\u00b7the\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOUS", "NE", "NE", "$,", "KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als finstrer Bettelmann, als schlauer Kr\u00f6sus irrn,", "tokens": ["Als", "finst\u00b7rer", "Bet\u00b7tel\u00b7mann", ",", "als", "schlau\u00b7er", "Kr\u00f6\u00b7sus", "irrn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", 