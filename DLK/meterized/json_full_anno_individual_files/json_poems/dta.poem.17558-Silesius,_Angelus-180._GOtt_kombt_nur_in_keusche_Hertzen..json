{"dta.poem.17558": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "180.  GOtt kombt nur in keusche Hertzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Den Br\u00e4utgam deiner Seel verlanget ein zu ziehen/", "tokens": ["Den", "Br\u00e4ut\u00b7gam", "dei\u00b7ner", "Seel", "ver\u00b7lan\u00b7get", "ein", "zu", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPOSAT", "NN", "VVFIN", "ART", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bl\u00fch auf; er kommet nicht bi\u00df da\u00df die Lilgen bl\u00fchen.", "tokens": ["Bl\u00fch", "auf", ";", "er", "kom\u00b7met", "nicht", "bi\u00df", "da\u00df", "die", "Lil\u00b7gen", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}}}}