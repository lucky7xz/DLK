{"dta.poem.13916": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "l .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es wechseln viel' Geschlechter", "tokens": ["Es", "wech\u00b7seln", "viel'", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sinken in die Nacht \u2014", "tokens": ["Und", "sin\u00b7ken", "in", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steh' fest, Du treuer W\u00e4chter,", "tokens": ["Steh'", "fest", ",", "Du", "treu\u00b7er", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nimm Dein Land in Acht!", "tokens": ["Und", "nimm", "Dein", "Land", "in", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}