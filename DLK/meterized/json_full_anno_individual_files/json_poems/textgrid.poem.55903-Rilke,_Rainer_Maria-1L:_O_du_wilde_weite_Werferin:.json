{"textgrid.poem.55903": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du wilde weite Werferin:", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du wilde weite Werferin:", "tokens": ["O", "du", "wil\u00b7de", "wei\u00b7te", "Wer\u00b7fe\u00b7rin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ein Speer bei andern Dingen", "tokens": ["Wie", "ein", "Speer", "bei", "an\u00b7dern", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "lag ich bei den Meinen. Dein Erklingen", "tokens": ["lag", "ich", "bei", "den", "Mei\u00b7nen", ".", "Dein", "Er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "warf mich weit. Ich wei\u00df nicht ", "tokens": ["warf", "mich", "weit", ".", "Ich", "wei\u00df", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "PTKNEG"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mich kann keiner wiederbringen.", "tokens": ["Mich", "kann", "kei\u00b7ner", "wie\u00b7der\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meine Schwestern denken mich und weben,", "tokens": ["Mei\u00b7ne", "Schwes\u00b7tern", "den\u00b7ken", "mich", "und", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "KON", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und das Haus ist voll vertrauter Schritte.", "tokens": ["und", "das", "Haus", "ist", "voll", "ver\u00b7trau\u00b7ter", "Schrit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ich allein bin fern und fortgegeben,", "tokens": ["Ich", "al\u00b7lein", "bin", "fern", "und", "fort\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "KON", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und ich zittere wie eine Bitte;", "tokens": ["und", "ich", "zit\u00b7te\u00b7re", "wie", "ei\u00b7ne", "Bit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "denn die sch\u00f6ne G\u00f6ttin in der Mitte", "tokens": ["denn", "die", "sch\u00f6\u00b7ne", "G\u00f6t\u00b7tin", "in", "der", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "ihrer Mythen gl\u00fcht und lebt mein Leben.", "tokens": ["ih\u00b7rer", "My\u00b7then", "gl\u00fcht", "und", "lebt", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "s