{"dta.poem.7753": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit meinem Meister gieng ich pilgern \u00fcber Land,", "tokens": ["Mit", "mei\u00b7nem", "Meis\u00b7ter", "gieng", "ich", "pil\u00b7gern", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir w\u00e4hlten einen Baum zur Rast im Mittagsbrand.", "tokens": ["Wir", "w\u00e4hl\u00b7ten", "ei\u00b7nen", "Baum", "zur", "Rast", "im", "Mit\u00b7tags\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein wilder Tiger kam vom Wald daher im Lauf,", "tokens": ["Ein", "wil\u00b7der", "Ti\u00b7ger", "kam", "vom", "Wald", "da\u00b7her", "im", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besinnungslose Furcht trieb mich den Baum hinauf.", "tokens": ["Be\u00b7sin\u00b7nungs\u00b7lo\u00b7se", "Furcht", "trieb", "mich", "den", "Baum", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich sah von obenher, wie jener drunten sa\u00df,", "tokens": ["Ich", "sah", "von", "o\u00b7ben\u00b7her", ",", "wie", "je\u00b7ner", "drun\u00b7ten", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "$,", "PWAV", "PDS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seinen Grimm vor ihm das wilde Thier verga\u00df.", "tokens": ["Und", "sei\u00b7nen", "Grimm", "vor", "ihm", "das", "wil\u00b7de", "Thier", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NE", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es wedelte geschmiegt alswie ein H\u00fcndlein zahm,", "tokens": ["Es", "we\u00b7del\u00b7te", "ge\u00b7schmiegt", "als\u00b7wie", "ein", "H\u00fcnd\u00b7lein", "zahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wandelte zur\u00fcck zum Wald, aus dem es kam.", "tokens": ["Und", "wan\u00b7del\u00b7te", "zu\u00b7r\u00fcck", "zum", "Wald", ",", "aus", "dem", "es", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich stieg besch\u00e4mt herab, wir aber zogen weiter,", "tokens": ["Ich", "stieg", "be\u00b7sch\u00e4mt", "her\u00b7ab", ",", "wir", "a\u00b7ber", "zo\u00b7gen", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,", "PPER", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Obdach suchten wir bei Nacht als m\u00fcde Schreiter.", "tokens": ["Ein", "Ob\u00b7dach", "such\u00b7ten", "wir", "bei", "Nacht", "als", "m\u00fc\u00b7de", "Schrei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da war's nach Mitternacht, als eine M\u00fccke stach", "tokens": ["Da", "wa\u00b7r's", "nach", "Mit\u00b7ter\u00b7nacht", ",", "als", "ei\u00b7ne", "M\u00fc\u00b7cke", "stach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Den Meister, da\u00df er st\u00f6hnt', und ich verwundert sprach:", "tokens": ["Den", "Meis\u00b7ter", ",", "da\u00df", "er", "st\u00f6hnt'", ",", "und", "ich", "ver\u00b7wun\u00b7dert", "sprach", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ein Tigerrachen lie\u00df dich gestern unverletzt,", "tokens": ["Ein", "Ti\u00b7ger\u00b7ra\u00b7chen", "lie\u00df", "dich", "ge\u00b7stern", "un\u00b7ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie nun verwundet dich ein M\u00fcckenstachel jetzt?", "tokens": ["Wie", "nun", "ver\u00b7wun\u00b7det", "dich", "ein", "M\u00fc\u00b7cken\u00b7sta\u00b7chel", "jetzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PRF", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Er aber sprach: Das Herz hat zwei verschiedne St\u00e4nde;", "tokens": ["Er", "a\u00b7ber", "sprach", ":", "Das", "Herz", "hat", "zwei", "ver\u00b7schied\u00b7ne", "St\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$.", "ART", "NN", "VAFIN", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O gl\u00fccklich, wenn es stets in einem sich bef\u00e4nde.", "tokens": ["O", "gl\u00fcck\u00b7lich", ",", "wenn", "es", "stets", "in", "ei\u00b7nem", "sich", "be\u00b7f\u00e4n\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Am Tage gestern war mein Herz im bessern Stand,", "tokens": ["Am", "Ta\u00b7ge", "ge\u00b7stern", "war", "mein", "Herz", "im", "bes\u00b7sern", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es stand in Gottes, nun steht es in meiner Hand.", "tokens": ["Es", "stand", "in", "Got\u00b7tes", ",", "nun", "steht", "es", "in", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}