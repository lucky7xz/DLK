{"textgrid.poem.58622": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eigne Werke nutzen nichts; willst du Kanans Ruh ererben,", "tokens": ["Eig\u00b7ne", "Wer\u00b7ke", "nut\u00b7zen", "nichts", ";", "willst", "du", "Ka\u00b7nans", "Ruh", "er\u00b7er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PIS", "$.", "VMFIN", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Lerne deinem eignen Tun durch gelass'nes Leiden sterben,", "tokens": ["Ler\u00b7ne", "dei\u00b7nem", "eig\u00b7nen", "Tun", "durch", "ge\u00b7lass'\u00b7nes", "Lei\u00b7den", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "So entbl\u00f6\u00dfet fliehe dann mit Vertrauen in Gott ein,", "tokens": ["So", "ent\u00b7bl\u00f6\u00b7\u00dfet", "flie\u00b7he", "dann", "mit", "Ver\u00b7trau\u00b7en", "in", "Gott", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "ADV", "APPR", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bis das s\u00fc\u00dfe Friedensland, Gottes heil'ger Berg, ist dein!", "tokens": ["Bis", "das", "s\u00fc\u00b7\u00dfe", "Frie\u00b7dens\u00b7land", ",", "Got\u00b7tes", "heil'\u00b7ger", "Berg", ",", "ist", "dein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "NN", "ADJA", "NN", "$,", "VAFIN", "PPOSAT", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Sch\u00f6ner Berg der Heiligkeit, da das Auge Gott nur siehet,", "tokens": ["Sch\u00f6\u00b7ner", "Berg", "der", "Hei\u00b7lig\u00b7keit", ",", "da", "das", "Au\u00b7ge", "Gott", "nur", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,", "KOUS", "ART", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Stilles Land der Ewigkeit, da der Friede Gottes bl\u00fchet,", "tokens": ["Stil\u00b7les", "Land", "der", "E\u00b7wig\u00b7keit", ",", "da", "der", "Frie\u00b7de", "Got\u00b7tes", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Da der lang bedr\u00e4ngte Geist findet Raum und offne Bahn", "tokens": ["Da", "der", "lang", "be\u00b7dr\u00e4ng\u00b7te", "Geist", "fin\u00b7det", "Raum", "und", "off\u00b7ne", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN", "VVFIN", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Und im Unerme\u00dflichen ohne Ansto\u00df leben kann!", "tokens": ["Und", "im", "Un\u00b7er\u00b7me\u00df\u00b7li\u00b7chen", "oh\u00b7ne", "An\u00b7sto\u00df", "le\u00b7ben", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "--