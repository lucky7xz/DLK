, "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was den zu laben scheint, das macht ihm nur Betr\u00fcben,", "tokens": ["Was", "den", "zu", "la\u00b7ben", "scheint", ",", "das", "macht", "ihm", "nur", "Be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PTKZU", "VVINF", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "der allzeit alles hat und niemals nichts doch kriegt.", "tokens": ["der", "all\u00b7zeit", "al\u00b7les", "hat", "und", "nie\u00b7mals", "nichts", "doch", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VAFIN", "KON", "ADV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich st\u00fcrbe mirs denn ab, so hoff' ichs zu erleben,", "tokens": ["Ich", "st\u00fcr\u00b7be", "mirs", "denn", "ab", ",", "so", "hoff'", "ichs", "zu", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "PTKVZ", "$,", "ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "da\u00df, wenn ich diesen Lauf zu Ende habe bracht,", "tokens": ["da\u00df", ",", "wenn", "ich", "die\u00b7sen", "Lauf", "zu", "En\u00b7de", "ha\u00b7be", "bracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PDAT", "NN", "APPR", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "ich dir den ersten Ku\u00df, o Landsmannin, will geben.", "tokens": ["ich", "dir", "den", "ers\u00b7ten", "Ku\u00df", ",", "o", "Lands\u00b7man\u00b7nin", ",", "will", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "ADJA", "NN", "$,", "FM", "NN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Was ferner kan geschehn, das la\u00df ich ungedacht.", "tokens": ["Was", "fer\u00b7ner", "kan", "ge\u00b7schehn", ",", "das", "la\u00df", "ich", "un\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "VVINF", "$,", "PRELS", "VVIMP", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Vor Terkii der Zirkassen, 1636. den 9. November.", "tokens": ["Vor", "Ter\u00b7kii", "der", "Zir\u00b7kas\u00b7sen", ",", "16\u00b736", ".", "den", "9.", "No\u00b7vem\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "number", "punct", "word", "ordinal", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$,", "CARD", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}