{"textgrid.poem.48474": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "48. Elegie an sein Vaterland", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! da\u00df ich mich einmal doch wieder solt' erfrischen an deiner reichen Lust, du edler ", "tokens": ["Ach", "!", "da\u00df", "ich", "mich", "ein\u00b7mal", "doch", "wie\u00b7der", "solt'", "er\u00b7fri\u00b7schen", "an", "dei\u00b7ner", "rei\u00b7chen", "Lust", ",", "du", "ed\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "VMFIN", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJA"], "meter": "-+-+-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "da du so sanfte gehst in bergichten Gep\u00fcschen,", "tokens": ["da", "du", "so", "sanf\u00b7te", "gehst", "in", "ber\u00b7gich\u00b7ten", "Ge\u00b7p\u00fcsc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "da, da mein ", "tokens": ["da", ",", "da", "mein"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wie jung, wie klein ich auch ward jener Zeit genommen", "tokens": ["Wie", "jung", ",", "wie", "klein", "ich", "auch", "ward", "je\u00b7ner", "Zeit", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "PPER", "ADV", "VAFIN", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "aus deiner s\u00fc\u00dfen Scho\u00df, so f\u00e4llt mirs doch noch ein,", "tokens": ["aus", "dei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Scho\u00df", ",", "so", "f\u00e4llt", "mirs", "doch", "noch", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "NE", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wie oft ich lustig hab' in deiner Flut geschwommen.", "tokens": ["wie", "oft", "ich", "lus\u00b7tig", "hab'", "in", "dei\u00b7ner", "Flut", "ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mir tr\u00e4umet ofte noch, als solt' ich um dich sein.", "tokens": ["Mir", "tr\u00e4u\u00b7met", "of\u00b7te", "noch", ",", "als", "solt'", "ich", "um", "dich", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "VMFIN", "PPER", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Itzt wolt' ich mir erst Lust und dir Erg\u00f6tzung schaffen:", "tokens": ["Itzt", "wolt'", "ich", "mir", "erst", "Lust", "und", "dir", "Er\u00b7g\u00f6t\u00b7zung", "schaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "NN", "KON", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "indem ich nach der Kunst, die mich und dich erhebt,", "tokens": ["in\u00b7dem", "ich", "nach", "der", "Kunst", ",", "die", "mich", "und", "dich", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ein unerh\u00f6rtes Lied, nicht von Gradivus Waffen,", "tokens": ["ein", "un\u00b7er\u00b7h\u00f6r\u00b7tes", "Lied", ",", "nicht", "von", "Gra\u00b7di\u00b7vus", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKNEG", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "f\u00fcr dem du nun, Gottlob! itzund hast ausgebebt,", "tokens": ["f\u00fcr", "dem", "du", "nun", ",", "Gott\u00b7lob", "!", "it\u00b7zund", "hast", "aus\u00b7ge\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "$,", "NN", "$.", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ein Lied von stiller Ruh' und sanftem Leben spielte,", "tokens": ["ein", "Lied", "von", "stil\u00b7ler", "Ruh'", "und", "sanf\u00b7tem", "Le\u00b7ben", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "wie unser Maro itzt bei seinem Bober tut,", "tokens": ["wie", "un\u00b7ser", "Ma\u00b7ro", "itzt", "bei", "sei\u00b7nem", "Bo\u00b7ber", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "ein Lied, das Himmel h\u00e4tt' und etwas solches f\u00fchlte,", "tokens": ["ein", "Lied", ",", "das", "Him\u00b7mel", "h\u00e4tt'", "und", "et\u00b7was", "sol\u00b7ches", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "KON", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "das nach der Gottheit schmeck' und rege Mut und Blut,", "tokens": ["das", "nach", "der", "Got\u00b7theit", "schmeck'", "und", "re\u00b7ge", "Mut", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "als ich denn pflag zu tun vor sieben halben Jahren,", "tokens": ["als", "ich", "denn", "pflag", "zu", "tun", "vor", "sie\u00b7ben", "hal\u00b7ben", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PTKZU", "VVINF", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "(wo ist sie itzund nur, die liebe sch\u00f6ne Zeit!)", "tokens": ["(", "wo", "ist", "sie", "it\u00b7zund", "nur", ",", "die", "lie\u00b7be", "sch\u00f6\u00b7ne", "Zeit", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "$,", "ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "da ich so helle sang bei ", "tokens": ["da", "ich", "so", "hel\u00b7le", "sang", "bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "VVFIN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "da\u00df sich mein Ton erschwung bis an die Ewigkeit.", "tokens": ["da\u00df", "sich", "mein", "Ton", "er\u00b7schwung", "bis", "an", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich sang der Deutschen Ruhm und ihrer teuren Prinzen,", "tokens": ["Ich", "sang", "der", "Deut\u00b7schen", "Ruhm", "und", "ih\u00b7rer", "teu\u00b7ren", "Prin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "bis Mars mich da treib' aus, der Unhold aller Kunst.", "tokens": ["bis", "Mars", "mich", "da", "treib'", "aus", ",", "der", "Un\u00b7hold", "al\u00b7ler", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "VVFIN", "PTKVZ", "$,", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Da macht' ich mich belobt bei vielerlei Provinzen,", "tokens": ["Da", "macht'", "ich", "mich", "be\u00b7lobt", "bei", "vie\u00b7ler\u00b7lei", "Pro\u00b7vin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "das Lief- und Ru\u00dfland auch mir boten ihre Gunst.", "tokens": ["das", "Lie\u00b7f", "und", "Ru\u00df\u00b7land", "auch", "mir", "bo\u00b7ten", "ih\u00b7re", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NE", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Rubelle, von Gestalt und Sitten hochgenamt,", "tokens": ["Ru\u00b7bel\u00b7le", ",", "von", "Ge\u00b7stalt", "und", "Sit\u00b7ten", "hoch\u00b7ge\u00b7namt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "dieselbe hatte mir die Pest auch aufgerieben,", "tokens": ["die\u00b7sel\u00b7be", "hat\u00b7te", "mir", "die", "Pest", "auch", "auf\u00b7ge\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "doch hat sich ihre Frucht in mir sehr reich besamt.", "tokens": ["doch", "hat", "sich", "ih\u00b7re", "Frucht", "in", "mir", "sehr", "reich", "be\u00b7samt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Die wei\u00dfe ", "tokens": ["Die", "wei\u00b7\u00dfe"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "Zeus itzt auch w\u00fcrde noch, fing mich mit ihrer Zier.", "tokens": ["Zeus", "itzt", "auch", "w\u00fcr\u00b7de", "noch", ",", "fing", "mich", "mit", "ih\u00b7rer", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VAFIN", "ADV", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Nach dieser wurd' mir hold die lange ", "tokens": ["Nach", "die\u00b7ser", "wurd'", "mir", "hold", "die", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "VAFIN", "PPER", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ach aber, ach! wie weit hin ich von beiden hier!", "tokens": ["Ach", "a\u00b7ber", ",", "ach", "!", "wie", "weit", "hin", "ich", "von", "bei\u00b7den", "hier", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ITJ", "$.", "PWAV", "ADJD", "PTKVZ", "PPER", "APPR", "PIAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Zwar es verstattet mir das kaspische Gestade,", "tokens": ["Zwar", "es", "ver\u00b7stat\u00b7tet", "mir", "das", "kas\u00b7pi\u00b7sche", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "da\u00df ich um seinen Strand mag ungehindert gehn;", "tokens": ["da\u00df", "ich", "um", "sei\u00b7nen", "Strand", "mag", "un\u00b7ge\u00b7hin\u00b7dert", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "auch bittet mich zur Zeit zu ihrem sch\u00f6nen Bade", "tokens": ["auch", "bit\u00b7tet", "mich", "zur", "Zeit", "zu", "ih\u00b7rem", "sch\u00f6\u00b7nen", "Ba\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "auf Urlaub des Hyrkans manch' Asische Siren'.", "tokens": ["auf", "Ur\u00b7laub", "des", "Hyr\u00b7kans", "man\u00b7ch'", "A\u00b7sisc\u00b7he", "Si\u00b7ren'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.35": {"text": "Ich bin den Nymphen lieb, den weichen Zirkassinnen,", "tokens": ["Ich", "bin", "den", "Nym\u00b7phen", "lieb", ",", "den", "wei\u00b7chen", "Zir\u00b7kas\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "dieweil ich ihnen fremd und nicht zu h\u00e4\u00dflich bin,", "tokens": ["die\u00b7weil", "ich", "ih\u00b7nen", "fremd", "und", "nicht", "zu", "h\u00e4\u00df\u00b7lich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "KON", "PTKNEG", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "und ob einander wir schon nicht verstehen k\u00f6nnen,", "tokens": ["und", "ob", "ein\u00b7an\u00b7der", "wir", "schon", "nicht", "ver\u00b7ste\u00b7hen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPER", "ADV", "PTKNEG", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "so kan ihr Auge doch mich g\u00fcnstig nach ihr ziehn.", "tokens": ["so", "kan", "ihr", "Au\u00b7ge", "doch", "mich", "g\u00fcns\u00b7tig", "nach", "ihr", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ADV", "PPER", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was aber soll ich so und auf der Flucht nur lieben?", "tokens": ["Was", "a\u00b7ber", "soll", "ich", "so", "und", "auf", "der", "Flucht", "nur", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "ADV", "KON", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Kupido wird durch nichts, als St\u00e4tigkeit vergn\u00fcgt.", "tokens": ["Ku\u00b7pi\u00b7do", "wird", "durch", "nichts", ",", "als", "St\u00e4\u00b7tig\u00b7keit", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PIS", "$,", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Was den zu laben scheint, das macht ihm nur Betr\u00fcben,", "tokens": ["Was", "den", "zu", "la\u00b7ben", "scheint", ",", "das", "macht", "ihm", "nur", "Be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PTKZU", "VVINF", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "der allzeit alles hat und niemals nichts doch kriegt.", "tokens": ["der", "all\u00b7zeit", "al\u00b7les", "hat", "und", "nie\u00b7mals", "nichts", "doch", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIS", "VAFIN", "KON", "ADV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich st\u00fcrbe mirs denn ab, so hoff' ichs zu erleben,", "tokens": ["Ich", "st\u00fcr\u00b7be", "mirs", "denn", "ab", ",", "so", "hoff'", "ichs", "zu", "er\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "PTKVZ", "$,", "ADV", "VVFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "da\u00df, wenn ich diesen Lauf zu Ende habe bracht,", "tokens": ["da\u00df", ",", "wenn", "ich", "die\u00b7sen", "Lauf", "zu", "En\u00b7de", "ha\u00b7be", "bracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PDAT", "NN", "APPR", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "ich dir den ersten Ku\u00df, o Landsmannin, will geben.", "tokens": ["ich", "dir", "den", "ers\u00b7ten", "Ku\u00df", ",", "o", "Lands\u00b7man\u00b7nin", ",", "will", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "ADJA", "NN", "$,", "FM", "NN", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Was ferner kan geschehn, das la\u00df ich ungedacht.", "tokens": ["Was", "fer\u00b7ner", "kan", "ge\u00b7schehn", ",", "das", "la\u00df", "ich", "un\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "VVINF", "$,", "PRELS", "VVIMP", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vor Terkii der Zirkassen, 1636. den 9. November.", "tokens": ["Vor", "Ter\u00b7kii", "der", "Zir\u00b7kas\u00b7sen", ",", "16\u00b736", ".", "den", "9.", "No\u00b7vem\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "number", "punct", "word", "ordinal", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$,", "CARD", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ach! da\u00df ich mich einmal doch wieder solt' erfrischen an deiner reichen Lust, du edler ", "tokens": ["Ach", "!", "da\u00df", "ich", "mich", "ein\u00b7mal", "doch", "wie\u00b7der", "solt'", "er\u00b7fri\u00b7schen", "an", "dei\u00b7ner", "rei\u00b7chen", "Lust", ",", "du", "ed\u00b7ler"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "VMFIN", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJA"], "meter": "-+-+-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "da du so sanfte gehst in bergichten Gep\u00fcschen,", "tokens": ["da", "du", "so", "sanf\u00b7te", "gehst", "in", "ber\u00b7gich\u00b7ten", "Ge\u00b7p\u00fcsc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "da, da mein ", "tokens": ["da", ",", "da", "mein"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Wie jung, wie klein ich auch ward jener Zeit genommen", "tokens": ["Wie", "jung", ",", "wie", "klein", "ich", "auch", "ward", "je\u00b7ner", "Zeit", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "PPER", "ADV", "VAFIN", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "aus deiner s\u00fc\u00dfen Scho\u00df, so f\u00e4llt mirs doch noch ein,", "tokens": ["aus", "dei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Scho\u00df", ",", "so", "f\u00e4llt", "mirs", "doch", "noch", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVFIN", "NE", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wie oft ich lustig hab' in deiner Flut geschwommen.", "tokens": ["wie", "oft", "ich", "lus\u00b7tig", "hab'", "in", "dei\u00b7ner", "Flut", "ge\u00b7schwom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mir tr\u00e4umet ofte noch, als solt' ich um dich sein.", "tokens": ["Mir", "tr\u00e4u\u00b7met", "of\u00b7te", "noch", ",", "als", "solt'", "ich", "um", "dich", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KOUS", "VMFIN", "PPER", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Itzt wolt' ich mir erst Lust und dir Erg\u00f6tzung schaffen:", "tokens": ["Itzt", "wolt'", "ich", "mir", "erst", "Lust", "und", "dir", "Er\u00b7g\u00f6t\u00b7zung", "schaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "NN", "KON", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "indem ich nach der Kunst, die mich und dich erhebt,", "tokens": ["in\u00b7dem", "ich", "nach", "der", "Kunst", ",", "die", "mich", "und", "dich", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ein unerh\u00f6rtes Lied, nicht von Gradivus Waffen,", "tokens": ["ein", "un\u00b7er\u00b7h\u00f6r\u00b7tes", "Lied", ",", "nicht", "von", "Gra\u00b7di\u00b7vus", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PTKNEG", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "f\u00fcr dem du nun, Gottlob! itzund hast ausgebebt,", "tokens": ["f\u00fcr", "dem", "du", "nun", ",", "Gott\u00b7lob", "!", "it\u00b7zund", "hast", "aus\u00b7ge\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "$,", "NN", "$.", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "ein Lied von stiller Ruh' und sanftem Leben spielte,", "tokens": ["ein", "Lied", "von", "stil\u00b7ler", "Ruh'", "und", "sanf\u00b7tem", "Le\u00b7ben", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "wie unser Maro itzt bei seinem Bober tut,", "tokens": ["wie", "un\u00b7ser", "Ma\u00b7ro", "itzt", "bei", "sei\u00b7nem", "Bo\u00b7ber", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "ein Lied, das Himmel h\u00e4tt' und etwas solches f\u00fchlte,", "tokens": ["ein", "Lied", ",", "das", "Him\u00b7mel", "h\u00e4tt'", "und", "et\u00b7was", "sol\u00b7ches", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "KON", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "das nach der Gottheit schmeck' und rege Mut und Blut,", "tokens": ["das", "nach", "der", "Got\u00b7theit", "schmeck'", "und", "re\u00b7ge", "Mut", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "als ich denn pflag zu tun vor sieben halben Jahren,", "tokens": ["als", "ich", "denn", "pflag", "zu", "tun", "vor", "sie\u00b7ben", "hal\u00b7ben", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PTKZU", "VVINF", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "(wo ist sie itzund nur, die liebe sch\u00f6ne Zeit!)", "tokens": ["(", "wo", "ist", "sie", "it\u00b7zund", "nur", ",", "die", "lie\u00b7be", "sch\u00f6\u00b7ne", "Zeit", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "$,", "ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "da ich so helle sang bei ", "tokens": ["da", "ich", "so", "hel\u00b7le", "sang", "bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "VVFIN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "da\u00df sich mein Ton erschwung bis an die Ewigkeit.", "tokens": ["da\u00df", "sich", "mein", "Ton", "er\u00b7schwung", "bis", "an", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich sang der Deutschen Ruhm und ihrer teuren Prinzen,", "tokens": ["Ich", "sang", "der", "Deut\u00b7schen", "Ruhm", "und", "ih\u00b7rer", "teu\u00b7ren", "Prin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "bis Mars mich da treib' aus, der Unhold aller Kunst.", "tokens": ["bis", "Mars", "mich", "da", "treib'", "aus", ",", "der", "Un\u00b7hold", "al\u00b7ler", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "VVFIN", "PTKVZ", "$,", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Da macht' ich mich belobt bei vielerlei Provinzen,", "tokens": ["Da", "macht'", "ich", "mich", "be\u00b7lobt", "bei", "vie\u00b7ler\u00b7lei", "Pro\u00b7vin\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "das Lief- und Ru\u00dfland auch mir boten ihre Gunst.", "tokens": ["das", "Lie\u00b7f", "und", "Ru\u00df\u00b7land", "auch", "mir", "bo\u00b7ten", "ih\u00b7re", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NE", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Rubelle, von Gestalt und Sitten hochgenamt,", "tokens": ["Ru\u00b7bel\u00b7le", ",", "von", "Ge\u00b7stalt", "und", "Sit\u00b7ten", "hoch\u00b7ge\u00b7namt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "dieselbe hatte mir die Pest auch aufgerieben,", "tokens": ["die\u00b7sel\u00b7be", "hat\u00b7te", "mir", "die", "Pest", "auch", "auf\u00b7ge\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "doch hat sich ihre Frucht in mir sehr reich besamt.", "tokens": ["doch", "hat", "sich", "ih\u00b7re", "Frucht", "in", "mir", "sehr", "reich", "be\u00b7samt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Die wei\u00dfe ", "tokens": ["Die", "wei\u00b7\u00dfe"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.28": {"text": "Zeus itzt auch w\u00fcrde noch, fing mich mit ihrer Zier.", "tokens": ["Zeus", "itzt", "auch", "w\u00fcr\u00b7de", "noch", ",", "fing", "mich", "mit", "ih\u00b7rer", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VAFIN", "ADV", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Nach dieser wurd' mir hold die lange ", "tokens": ["Nach", "die\u00b7ser", "wurd'", "mir", "hold", "die", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "VAFIN", "PPER", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Ach aber, ach! wie weit hin ich von beiden hier!", "tokens": ["Ach", "a\u00b7ber", ",", "ach", "!", "wie", "weit", "hin", "ich", "von", "bei\u00b7den", "hier", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ITJ", "$.", "PWAV", "ADJD", "PTKVZ", "PPER", "APPR", "PIAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Zwar es verstattet mir das kaspische Gestade,", "tokens": ["Zwar", "es", "ver\u00b7stat\u00b7tet", "mir", "das", "kas\u00b7pi\u00b7sche", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "da\u00df ich um seinen Strand mag ungehindert gehn;", "tokens": ["da\u00df", "ich", "um", "sei\u00b7nen", "Strand", "mag", "un\u00b7ge\u00b7hin\u00b7dert", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "auch bittet mich zur Zeit zu ihrem sch\u00f6nen Bade", "tokens": ["auch", "bit\u00b7tet", "mich", "zur", "Zeit", "zu", "ih\u00b7rem", "sch\u00f6\u00b7nen", "Ba\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "auf Urlaub des Hyrkans manch' Asische Siren'.", "tokens": ["auf", "Ur\u00b7laub", "des", "Hyr\u00b7kans", "man\u00b7ch'", "A\u00b7sisc\u00b7he", "Si\u00b7ren'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.35": {"text": "Ich bin den Nymphen lieb, den weichen Zirkassinnen,", "tokens": ["Ich", "bin", "den", "Nym\u00b7phen", "lieb", ",", "den", "wei\u00b7chen", "Zir\u00b7kas\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "dieweil ich ihnen fremd und nicht zu h\u00e4\u00dflich bin,", "tokens": ["die\u00b7weil", "ich", "ih\u00b7nen", "fremd", "und", "nicht", "zu", "h\u00e4\u00df\