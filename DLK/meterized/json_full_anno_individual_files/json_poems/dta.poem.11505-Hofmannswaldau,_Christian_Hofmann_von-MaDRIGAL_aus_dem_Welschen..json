{"dta.poem.11505": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "MaDRIGAL  \n  aus dem Welschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! lerne doch dem buhler nicht vertraun,", "tokens": ["Ach", "!", "ler\u00b7ne", "doch", "dem", "buh\u00b7ler", "nicht", "ver\u00b7traun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der seine treu auf freche schw\u00fcre gr\u00fcndet,", "tokens": ["Der", "sei\u00b7ne", "treu", "auf", "fre\u00b7che", "schw\u00fc\u00b7re", "gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und die fl\u00fcgel seiner liebe nur an leichte worte bindet;", "tokens": ["Und", "die", "fl\u00fc\u00b7gel", "sei\u00b7ner", "lie\u00b7be", "nur", "an", "leich\u00b7te", "wor\u00b7te", "bin\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "VVFIN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Denn dieses hei\u00dft ein haus auf sand und wellen baun.", "tokens": ["Denn", "die\u00b7ses", "hei\u00dft", "ein", "haus", "auf", "sand", "und", "wel\u00b7len", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "APPR", "VVFIN", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Verma\u00df sich Thyrsis nicht, dir ewig treu zu seyn,", "tokens": ["Ver\u00b7ma\u00df", "sich", "Thyr\u00b7sis", "nicht", ",", "dir", "e\u00b7wig", "treu", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "NE", "PTKNEG", "$,", "PPER", "ADJD", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als er mit angenommnen klagen", "tokens": ["Als", "er", "mit", "an\u00b7ge\u00b7nomm\u00b7nen", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dich n\u00e4chst dahin gebracht, ihm gegen-liebe zuzusagen?", "tokens": ["Dich", "n\u00e4chst", "da\u00b7hin", "ge\u00b7bracht", ",", "ihm", "ge\u00b7gen\u00b7lie\u00b7be", "zu\u00b7zu\u00b7sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "VVPP", "$,", "PPER", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Jtzt schaut er dich kaum \u00fcber achsel an.", "tokens": ["Jtzt", "schaut", "er", "dich", "kaum", "\u00fc\u00b7ber", "ach\u00b7sel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Allzugetreues hertze!", "tokens": ["All\u00b7zu\u00b7ge\u00b7treu\u00b7es", "hert\u00b7ze", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.10": {"text": "Werd\u2019 einst mit schaden klug!", "tokens": ["Werd'", "einst", "mit", "scha\u00b7den", "klug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Einmahl betrogen seyn, ist warlich gar genug.", "tokens": ["Ein\u00b7mahl", "be\u00b7tro\u00b7gen", "seyn", ",", "ist", "war\u00b7lich", "gar", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$,", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Ach! Floris! soltest du noch dermahleinst erfahren,", "tokens": ["Ach", "!", "Flo\u00b7ris", "!", "sol\u00b7test", "du", "noch", "der\u00b7mah\u00b7le\u00b7inst", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NE", "$.", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie deiner sch\u00f6nheit licht mein hertz in asche kehrt;", "tokens": ["Wie", "dei\u00b7ner", "sch\u00f6n\u00b7heit", "licht", "mein", "hertz", "in", "asc\u00b7he", "kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "PPOSAT", "NN", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df, du w\u00fcrdest noch,", "tokens": ["Ich", "wei\u00df", ",", "du", "w\u00fcr\u00b7dest", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wenn meine glieder sich schon mit dem sande paaren,", "tokens": ["Wenn", "mei\u00b7ne", "glie\u00b7der", "sich", "schon", "mit", "dem", "san\u00b7de", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADV", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sie der w\u00fcrmer mund verzehrt,", "tokens": ["Und", "sie", "der", "w\u00fcr\u00b7mer", "mund", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mitleiden mit mir tragen.", "tokens": ["Mit\u00b7lei\u00b7den", "mit", "mir", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "K\u00f6nnt\u2019 es doch m\u00f6glich seyn,", "tokens": ["K\u00f6nnt'", "es", "doch", "m\u00f6g\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und dir, wo nicht ein mensch, doch nur ein kalter stein,", "tokens": ["Und", "dir", ",", "wo", "nicht", "ein", "mensch", ",", "doch", "nur", "ein", "kal\u00b7ter", "stein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PWAV", "PTKNEG", "ART", "NN", "$,", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die sch\u00f6ne nachricht sagen,", "tokens": ["Die", "sch\u00f6\u00b7ne", "nach\u00b7richt", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Warum dein Seladon so bald verfallen ist!", "tokens": ["Wa\u00b7rum", "dein", "Se\u00b7la\u00b7don", "so", "bald", "ver\u00b7fal\u00b7len", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Allein, es geht unm\u00f6glich an,", "tokens": ["Al\u00b7lein", ",", "es", "geht", "un\u00b7m\u00f6g\u00b7lich", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Weil das, was niemand wei\u00df, auch niemand melden kan.", "tokens": ["Weil", "das", ",", "was", "nie\u00b7mand", "wei\u00df", ",", "auch", "nie\u00b7mand", "mel\u00b7den", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PRELS", "PIS", "VVFIN", "$,", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wiewohl, das ist mein trost, den ich noch hier erwerbe,", "tokens": ["Wie\u00b7wohl", ",", "das", "ist", "mein", "trost", ",", "den", "ich", "noch", "hier", "er\u00b7wer\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der mich in stiller lust dem blassen tode giebt:", "tokens": ["Der", "mich", "in", "stil\u00b7ler", "lust", "dem", "blas\u00b7sen", "to\u00b7de", "giebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df, weil mein k\u00fchner geist dich, sch\u00f6nstes kind! geliebt,", "tokens": ["Da\u00df", ",", "weil", "mein", "k\u00fch\u00b7ner", "geist", "dich", ",", "sch\u00f6ns\u00b7tes", "kind", "!", "ge\u00b7liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "PPER", "$,", "ADJA", "NN", "$.", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich blos aus tieffer ehr-furcht sterbe.", "tokens": ["Ich", "blos", "aus", "tief\u00b7fer", "ehr\u00b7furcht", "ster\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}