{"textgrid.poem.34509": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "3. Ninive", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und zum zweitenmal geschah des Herren", "tokens": ["Und", "zum", "zwei\u00b7ten\u00b7mal", "ge\u00b7schah", "des", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wort zu Jona: Mach dich auf und wandre", "tokens": ["Wort", "zu", "Jo\u00b7na", ":", "Mach", "dich", "auf", "und", "wand\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "$.", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "in die grosse Ninive und predige", "tokens": ["in", "die", "gros\u00b7se", "Ni\u00b7ni\u00b7ve", "und", "pre\u00b7di\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ADJA"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "von dem heiligen Zorne deines Gottes,", "tokens": ["von", "dem", "hei\u00b7li\u00b7gen", "Zor\u00b7ne", "dei\u00b7nes", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "denn es ist vor mich heraufgekommen", "tokens": ["denn", "es", "ist", "vor", "mich", "her\u00b7auf\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "ihre Bosheit, und ich will sie strafen.", "tokens": ["ih\u00b7re", "Bos\u00b7heit", ",", "und", "ich", "will", "sie", "stra\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPER", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Jona h\u00f6rte und gehorchte. Eilends", "tokens": ["Jo\u00b7na", "h\u00f6r\u00b7te", "und", "ge\u00b7horch\u00b7te", ".", "Ei\u00b7lends"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "brach er auf zum Lande der Assyrer.", "tokens": ["brach", "er", "auf", "zum", "Lan\u00b7de", "der", "As\u00b7sy\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und er kam nach Ninive, der grossen,", "tokens": ["Und", "er", "kam", "nach", "Ni\u00b7ni\u00b7ve", ",", "der", "gros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NE", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "die drei Tagereisen lang sich ausdehnt.", "tokens": ["die", "drei", "Ta\u00b7ge\u00b7rei\u00b7sen", "lang", "sich", "aus\u00b7dehnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "ADJD", "PRF", "VVPP", "$."], "meter": "-+--+-+---", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Stumm ging Jona eine Tagereise,", "tokens": ["Stumm", "ging", "Jo\u00b7na", "ei\u00b7ne", "Ta\u00b7ge\u00b7rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NE", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "aber dann erhob er seine Stimme,", "tokens": ["a\u00b7ber", "dann", "er\u00b7hob", "er", "sei\u00b7ne", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "predigte und sprach: Noch vierzig Tage", "tokens": ["pre\u00b7dig\u00b7te", "und", "sprach", ":", "Noch", "vier\u00b7zig", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "$.", "ADV", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "wird die stolze Ninive sich br\u00fcsten \u2013", "tokens": ["wird", "die", "stol\u00b7ze", "Ni\u00b7ni\u00b7ve", "sich", "br\u00fcs\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PRF", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "doch nach vierzig Tagen wird der Herr sie", "tokens": ["doch", "nach", "vier\u00b7zig", "Ta\u00b7gen", "wird", "der", "Herr", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "CARD", "NN", "VAFIN", "ART", "NN", "PPER"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "z\u00fcchtigen zu ihrer S\u00fcnden Ernte", "tokens": ["z\u00fcch\u00b7ti\u00b7gen", "zu", "ih\u00b7rer", "S\u00fcn\u00b7den", "Ern\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "und zerreissen ihrer Mauern Kranz.", "tokens": ["und", "zer\u00b7reis\u00b7sen", "ih\u00b7rer", "Mau\u00b7ern", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Da die Leute solche Predigt h\u00f6rten,", "tokens": ["Da", "die", "Leu\u00b7te", "sol\u00b7che", "Pre\u00b7digt", "h\u00f6r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "zog die Furcht in ihre raschen Herzen,", "tokens": ["zog", "die", "Furcht", "in", "ih\u00b7re", "ra\u00b7schen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und sie glaubten Gott. Alsbald erhub sich", "tokens": ["und", "sie", "glaub\u00b7ten", "Gott", ".", "Als\u00b7bald", "er\u00b7hub", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN", "$.", "ADV", "VVFIN", "PRF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wehgeschrei und Klagen durch die Strassen.", "tokens": ["Weh\u00b7ge\u00b7schrei", "und", "Kla\u00b7gen", "durch", "die", "Stras\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und die Kunde kam auch vor den K\u00f6nig.", "tokens": ["Und", "die", "Kun\u00b7de", "kam", "auch", "vor", "den", "K\u00f6\u00b7nig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der stand auf von seinem goldnen Throne,", "tokens": ["Der", "stand", "auf", "von", "sei\u00b7nem", "gold\u00b7nen", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "legte seinen Purpur ab und h\u00fcllte", "tokens": ["leg\u00b7te", "sei\u00b7nen", "Pur\u00b7pur", "ab", "und", "h\u00fcll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "sich in einen Sack. Drauf liess er ausschrein", "tokens": ["sich", "in", "ei\u00b7nen", "Sack", ".", "Drauf", "liess", "er", "aus\u00b7schrein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "$.", "PAV", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "als Befehl und aus Gewalt des K\u00f6nigs:", "tokens": ["als", "Be\u00b7fehl", "und", "aus", "Ge\u00b7walt", "des", "K\u00f6\u00b7nigs", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dass vielleicht sich Gottes Zorn noch wende,", "tokens": ["Dass", "viel\u00b7leicht", "sich", "Got\u00b7tes", "Zorn", "noch", "wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "NN", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "sollen alle, alle Wesen fasten,", "tokens": ["sol\u00b7len", "al\u00b7le", ",", "al\u00b7le", "We\u00b7sen", "fas\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gross und Klein und Mensch wie Thier. Sie sollen", "tokens": ["Gross", "und", "Klein", "und", "Mensch", "wie", "Thier", ".", "Sie", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "KOKOM", "NN", "$.", "PPER", "VMFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "alle sich in h\u00e4rene S\u00e4cke h\u00fcllen", "tokens": ["al\u00b7le", "sich", "in", "h\u00e4\u00b7re\u00b7ne", "S\u00e4\u00b7cke", "h\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "PRF", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.5": {"text": "und sich niederwerfen in die Asche.", "tokens": ["und", "sich", "nie\u00b7der\u00b7wer\u00b7fen", "in", "die", "A\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Darben soll was Odem haucht und niemand", "tokens": ["Dar\u00b7ben", "soll", "was", "O\u00b7dem", "haucht", "und", "nie\u00b7mand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PIS", "ADJA", "NN", "KON", "PIS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "soll sich selber Speis und Trank gew\u00e4hren", "tokens": ["soll", "sich", "sel\u00b7ber", "Speis", "und", "Trank", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ADV", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "noch ein Thier zur Tr\u00e4nk und Weide treiben.", "tokens": ["noch", "ein", "Thier", "zur", "Tr\u00e4nk", "und", "Wei\u00b7de", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Sondern jeder soll vom b\u00f6sen Wege", "tokens": ["Son\u00b7dern", "je\u00b7der", "soll", "vom", "b\u00f6\u00b7sen", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "ab sich kehren und von seiner H\u00e4nde", "tokens": ["ab", "sich", "keh\u00b7ren", "und", "von", "sei\u00b7ner", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "VVINF", "KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Frevel! \u2013 Dass vielleicht wir Gnade f\u00e4nden,", "tokens": ["Fre\u00b7vel", "!", "\u2013", "Dass", "viel\u00b7leicht", "wir", "Gna\u00b7de", "f\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PDS", "ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "dass vielleicht sich Gottes Zorn noch wende!", "tokens": ["dass", "viel\u00b7leicht", "sich", "Got\u00b7tes", "Zorn", "noch", "wen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Als nun Gott die Werke ihrer Reue", "tokens": ["Als", "nun", "Gott", "die", "Wer\u00b7ke", "ih\u00b7rer", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "sah, erbarmte sich sein Herz des Volkes,", "tokens": ["sah", ",", "er\u00b7barm\u00b7te", "sich", "sein", "Herz", "des", "Vol\u00b7kes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "und das \u00dcbel, das er durch die Stimme", "tokens": ["und", "das", "\u00dc\u00b7bel", ",", "das", "er", "durch", "die", "Stim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jonas, seines Dieners \u00fcber jene", "tokens": ["Jo\u00b7nas", ",", "sei\u00b7nes", "Die\u00b7ners", "\u00fc\u00b7ber", "je\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "NN", "APPR", "PDAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "schon verh\u00e4ngte \u2013 that er ihnen nicht an.", "tokens": ["schon", "ver\u00b7h\u00e4ng\u00b7te", "\u2013", "that", "er", "ih\u00b7nen", "nicht", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Und zum zweitenmal geschah des Herren", "tokens": ["Und", "zum", "zwei\u00b7ten\u00b7mal", "ge\u00b7schah", "des", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wort zu Jona: Mach dich auf und wandre", "tokens": ["Wort", "zu", "Jo\u00b7na", ":", "Mach", "dich", "auf", "und", "wand\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "$.", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "in die grosse Ninive und predige", "tokens": ["in", "die", "gros\u00b7se", "Ni\u00b7ni\u00b7ve", "und", "pre\u00b7di\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ADJA"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "von dem heiligen Zorne deines Gottes,", "tokens": ["von", "dem", "hei\u00b7li\u00b7gen", "Zor\u00b7ne", "dei\u00b7nes", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "denn es ist vor mich heraufgekommen", "tokens": ["denn", "es", "ist", "vor", "mich", "her\u00b7auf\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "ihre Bosheit, und ich will sie strafen.", "tokens": ["ih\u00b7re", "Bos\u00b7heit", ",", "und", "ich", "will", "sie", "stra\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPER", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Jona h\u00f6rte und gehorchte. Eilends", "tokens": ["Jo\u00b7na", "h\u00f6r\u00b7te", "und", "ge\u00b7horch\u00b7te", ".", "Ei\u00b7lends"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "$.", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "brach er 