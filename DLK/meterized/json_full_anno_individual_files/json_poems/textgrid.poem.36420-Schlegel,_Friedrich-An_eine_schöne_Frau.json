{"textgrid.poem.36420": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An eine sch\u00f6ne Frau", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich noch lebe, wird der Reim dir zeigen,", "tokens": ["Da\u00df", "ich", "noch", "le\u00b7be", ",", "wird", "der", "Reim", "dir", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der mir erlaubt, dir W\u00fcrd'ge! du zu sagen.", "tokens": ["Der", "mir", "er\u00b7laubt", ",", "dir", "W\u00fcrd'\u00b7ge", "!", "du", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "PPER", "NN", "$.", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich schreibe, um ein W\u00f6rtlein nur zu fragen;", "tokens": ["Ich", "schrei\u00b7be", ",", "um", "ein", "W\u00f6rt\u00b7lein", "nur", "zu", "fra\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was ich empfinde, davon la\u00df mich schweigen!", "tokens": ["Was", "ich", "emp\u00b7fin\u00b7de", ",", "da\u00b7von", "la\u00df", "mich", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PAV", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn ich jetzt spr\u00e4che, k\u00f6nntest du dich neigen?", "tokens": ["Wenn", "ich", "jetzt", "spr\u00e4\u00b7che", ",", "k\u00f6nn\u00b7test", "du", "dich", "nei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "VMFIN", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dort schwieg der Mund, nun soll's die Feder wagen:", "tokens": ["Dort", "schwieg", "der", "Mund", ",", "nun", "soll's", "die", "Fe\u00b7der", "wa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wollt' in jener Stunde dir es klagen,", "tokens": ["Ich", "wollt'", "in", "je\u00b7ner", "Stun\u00b7de", "dir", "es", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PDAT", "NN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als vor uns schwebte froher M\u00e4dchen Reigen.", "tokens": ["Als", "vor", "uns", "schweb\u00b7te", "fro\u00b7her", "M\u00e4d\u00b7chen", "Rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich hab' im Stillen wahrlich viel gelitten.", "tokens": ["Ich", "hab'", "im", "Stil\u00b7len", "wahr\u00b7lich", "viel", "ge\u00b7lit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erst lobt' ich das Geschick, dann mu\u00dft' ich schelten,", "tokens": ["Erst", "lobt'", "ich", "das", "Ge\u00b7schick", ",", "dann", "mu\u00dft'", "ich", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df du so sch\u00f6n und ich so schnell gefangen.", "tokens": ["Da\u00df", "du", "so", "sch\u00f6n", "und", "ich", "so", "schnell", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dein Auge l\u00e4chle freundlich meinen Bitten;", "tokens": ["Dein", "Au\u00b7ge", "l\u00e4ch\u00b7le", "freund\u00b7lich", "mei\u00b7nen", "Bit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wird das fromme Schweigen mir vergelten", "tokens": ["So", "wird", "das", "from\u00b7me", "Schwei\u00b7gen", "mir", "ver\u00b7gel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein s\u00fc\u00dfer Ku\u00df. Mehr darf ich nicht verlangen.", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Ku\u00df", ".", "Mehr", "darf", "ich", "nicht", "ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PIS", "VMFIN", "PPER", "PTKNE