{"dta.poem.14731": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schnee .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wann der kalte Schnee zergangen,", "tokens": ["Wann", "der", "kal\u00b7te", "Schnee", "zer\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehst Du drau\u00dfen in der Th\u00fcr,", "tokens": ["Stehst", "Du", "drau\u00b7\u00dfen", "in", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt ein Knabe sch\u00f6n gegangen,", "tokens": ["Kommt", "ein", "Kna\u00b7be", "sch\u00f6n", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stellt sich freundlich da zu Dir,", "tokens": ["Stellt", "sich", "freund\u00b7lich", "da", "zu", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lobet Deine frischen Wangen,", "tokens": ["Lo\u00b7bet", "Dei\u00b7ne", "fri\u00b7schen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dunkle Locken, Augen licht,", "tokens": ["Dunk\u00b7le", "Lo\u00b7cken", ",", "Au\u00b7gen", "licht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wann der kalte Schnee zergangen", "tokens": ["Wann", "der", "kal\u00b7te", "Schnee", "zer\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Glaub dem falschen Herzen nicht!", "tokens": ["Glaub", "dem", "fal\u00b7schen", "Her\u00b7zen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}