{"textgrid.poem.49307": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Auf den herannahenden Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei, nun wil ich lassen schwinden", "tokens": ["Ei", ",", "nun", "wil", "ich", "las\u00b7sen", "schwin\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "VVINF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Sorg' und Traurigkeit,", "tokens": ["Al\u00b7le", "Sor\u00b7g'", "und", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Weil die sch\u00f6ne Fr\u00fchlingszeit", "tokens": ["Weil", "die", "sch\u00f6\u00b7ne", "Fr\u00fch\u00b7lings\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich nun bald wird lassen finden,", "tokens": ["Sich", "nun", "bald", "wird", "las\u00b7sen", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VAFIN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil der Winter wird vergehen,", "tokens": ["Weil", "der", "Win\u00b7ter", "wird", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eis und Schnee zu Wasser wird,", "tokens": ["Eis", "und", "Schnee", "zu", "Was\u00b7ser", "wird", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die G\u00e4rten wolgeziert", "tokens": ["Und", "die", "G\u00e4r\u00b7ten", "wol\u00b7ge\u00b7ziert"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind sehr lieblich anzusehen.", "tokens": ["Sind", "sehr", "lieb\u00b7lich", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Hievon thut die Zeitung bringen", "tokens": ["Hie\u00b7von", "thut", "die", "Zei\u00b7tung", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aller V\u00f6gel Fr\u00f6lichkeit,", "tokens": ["Al\u00b7ler", "V\u00f6\u00b7gel", "Fr\u00f6\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Die zu dieser Fr\u00fchlingszeit", "tokens": ["Die", "zu", "die\u00b7ser", "Fr\u00fch\u00b7lings\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Ihre Stimmlein lassen klingen,", "tokens": ["Ih\u00b7re", "Stimm\u00b7lein", "las\u00b7sen", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Da die Lerchen sich erfreuen,", "tokens": ["Da", "die", "Ler\u00b7chen", "sich", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Da der Baur zu Felde zeucht", "tokens": ["Da", "der", "Baur", "zu", "Fel\u00b7de", "zeucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Und aus Scheu'r und St\u00e4llen kreucht", "tokens": ["Und", "aus", "Scheu'r", "und", "St\u00e4l\u00b7len", "kreucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Der Menalkas mit den S\u00e4uen.", "tokens": ["Der", "Me\u00b7nal\u00b7kas", "mit", "den", "S\u00e4u\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Alles thut jetzt mutig werden,", "tokens": ["Al\u00b7les", "thut", "jetzt", "mu\u00b7tig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Es komt wieder an den Tag,", "tokens": ["Es", "komt", "wie\u00b7der", "an", "den", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Was zuvor verborgen lag", "tokens": ["Was", "zu\u00b7vor", "ver\u00b7bor\u00b7gen", "lag"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "VVPP", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "In dem tiefen Kot der Erden;", "tokens": ["In", "dem", "tie\u00b7fen", "Kot", "der", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Man sicht alles hervor kriechen,", "tokens": ["Man", "sicht", "al\u00b7les", "her\u00b7vor", "krie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Kraut und Blumen mannigfalt,", "tokens": ["Kraut", "und", "Blu\u00b7men", "man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Die so lieblich von Gestalt", "tokens": ["Die", "so", "lieb\u00b7lich", "von", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Und anmutig sind zu riechen.", "tokens": ["Und", "an\u00b7mu\u00b7tig", "sind", "zu", "rie\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Ei, so wil ich in den Garten", "tokens": ["Ei", ",", "so", "wil", "ich", "in", "den", "Gar\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Mit dem sch\u00f6nen Saitenspiel", "tokens": ["Mit", "dem", "sch\u00f6\u00b7nen", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Und der andern Kurzweil viel", "tokens": ["Und", "der", "an\u00b7dern", "Kurz\u00b7weil", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Nur der Fr\u00f6lichkeit abwarten.", "tokens": ["Nur", "der", "Fr\u00f6\u00b7lich\u00b7keit", "ab\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+++-", "measure": "unknown.measure.penta"}, "line.29": {"text": "Ich wil suchen solche Gsellen,", "tokens": ["Ich", "wil", "su\u00b7chen", "sol\u00b7che", "Gsel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Die da wissen Lust und Freud'", "tokens": ["Die", "da", "wis\u00b7sen", "Lust", "und", "Freud'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "In der gr\u00fcnen Fr\u00fchlingszeit", "tokens": ["In", "der", "gr\u00fc\u00b7nen", "Fr\u00fch\u00b7lings\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Fein geb\u00fchrlich anzustellen.", "tokens": ["Fein", "ge\u00b7b\u00fchr\u00b7lich", "an\u00b7zu\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "La\u00dft uns guten Wein hergeben;", "tokens": ["La\u00dft", "uns", "gu\u00b7ten", "Wein", "her\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Lauten, Geigen, Jungfr\u00e4ulein", "tokens": ["Lau\u00b7ten", ",", "Gei\u00b7gen", ",", "Jung\u00b7fr\u00e4u\u00b7lein"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "M\u00fcssen alle bei uns sein;", "tokens": ["M\u00fcs\u00b7sen", "al\u00b7le", "bei", "uns", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIS", "APPR", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Das ist recht Studentenleben.", "tokens": ["Das", "ist", "recht", "Stu\u00b7den\u00b7ten\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Wer solt das nicht lieber wollen,", "tokens": ["Wer", "solt", "das", "nicht", "lie\u00b7ber", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDS", "PTKNEG", "ADV", "VMFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Als arbeiten Nacht und Tag,", "tokens": ["Als", "ar\u00b7bei\u00b7ten", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.39": {"text": "Stetig f\u00fchren gro\u00dfe Klag?", "tokens": ["Ste\u00b7tig", "f\u00fch\u00b7ren", "gro\u00b7\u00dfe", "Klag", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Wer wei\u00df, wann wir sterben sollen!", "tokens": ["Wer", "wei\u00df", ",", "wann", "wir", "ster\u00b7ben", "sol\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", 