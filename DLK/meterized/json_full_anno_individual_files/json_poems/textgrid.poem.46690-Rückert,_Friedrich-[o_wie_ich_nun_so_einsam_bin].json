{"textgrid.poem.46690": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[o wie ich nun so einsam bin]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie ich nun so einsam bin,", "tokens": ["O", "wie", "ich", "nun", "so", "ein\u00b7sam", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn meine Gro\u00dfen sind zur Schule,", "tokens": ["Wenn", "mei\u00b7ne", "Gro\u00b7\u00dfen", "sind", "zur", "Schu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sonst mir blieb mein kleiner Buhle,", "tokens": ["Wo", "sonst", "mir", "blieb", "mein", "klei\u00b7ner", "Buh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Nebenbuhlerin,", "tokens": ["Und", "mei\u00b7ne", "Ne\u00b7ben\u00b7buh\u00b7le\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die beiden, stets in Einigkeit,", "tokens": ["Die", "bei\u00b7den", ",", "stets", "in", "Ei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00fcber nichts als das im Streit,", "tokens": ["Und", "\u00fc\u00