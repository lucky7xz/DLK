{"dta.poem.9672": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Alchimie.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist die Alchimie/ als eine kunst zu l\u00fcgen?", "tokens": ["Was", "ist", "die", "Al\u00b7chi\u00b7mie", "/", "als", "ei\u00b7ne", "kunst", "zu", "l\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$(", "KOUS", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was dient sie anders wohl/ als menschen zu betr\u00fcgen?", "tokens": ["Was", "dient", "sie", "an\u00b7ders", "wohl", "/", "als", "men\u00b7schen", "zu", "be\u00b7tr\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was bringt sie dir/ mein freund/ als asche/ seuffzer/ schwei\u00df/", "tokens": ["Was", "bringt", "sie", "dir", "/", "mein", "freund", "/", "als", "asc\u00b7he", "/", "seuff\u00b7zer", "/", "schwei\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$(", "PPOSAT", "NN", "$(", "KOUS", "ADJA", "$(", "NE", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als hoffnung/ leeren wind/ und schande vor den flei\u00df?", "tokens": ["Als", "hoff\u00b7nung", "/", "lee\u00b7ren", "wind", "/", "und", "schan\u00b7de", "vor", "den