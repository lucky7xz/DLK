{"textgrid.poem.48835": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "60. An den Steinbruch zu Revel", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Zaum des frechen ", "tokens": ["Du", "Zaum", "des", "fre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "sich m\u00e4nlich setzet vor, da\u00df sich die Wellen brechen", "tokens": ["sich", "m\u00e4n\u00b7lich", "set\u00b7zet", "vor", ",", "da\u00df", "sich", "die", "Wel\u00b7len", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVFIN", "PTKVZ", "$,", "KOUS", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und in sich umgewandt sich an sich m\u00fcssen r\u00e4chen", "tokens": ["und", "in", "sich", "um\u00b7ge\u00b7wandt", "sich", "an", "sich", "m\u00fcs\u00b7sen", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "VVFIN", "PRF", "APPR", "PRF", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und kehrn den schwachen Zorn in leichten Sand und Wust,", "tokens": ["und", "kehrn", "den", "schwa\u00b7chen", "Zorn", "in", "leich\u00b7ten", "Sand", "und", "Wust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "der du dem Lande Schutz, der Stadt Zier geben mu\u00dft,", "tokens": ["der", "du", "dem", "Lan\u00b7de", "Schutz", ",", "der", "Stadt", "Zier", "ge\u00b7ben", "mu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "NN", "$,", "ART", "NN", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Stadt, so jenseit ist so reich an s\u00fc\u00dfen B\u00e4chen,", "tokens": ["der", "Stadt", ",", "so", "jen\u00b7seit", "ist", "so", "reich", "an", "s\u00fc\u00b7\u00dfen", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "VAFIN", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "hier an gesalzner See, an H\u00f6hen und an Fl\u00e4chen,", "tokens": ["hier", "an", "ge\u00b7salz\u00b7ner", "See", ",", "an", "H\u00f6\u00b7hen", "und", "an", "Fl\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "darinnen ", "tokens": ["da\u00b7rin\u00b7nen"], "token_info": ["word"], "pos": ["ADV"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Ich ginge zu dir ein, du Lustberg der Silenen,", "tokens": ["Ich", "gin\u00b7ge", "zu", "dir", "ein", ",", "du", "Lust\u00b7berg", "der", "Si\u00b7le\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mich meiner Liebesangst ein wenig zu entw\u00f