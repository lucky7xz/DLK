{"dta.poem.16652": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "11.  Die beste Sicherheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Schlaf meine Seele schlaf: Dan\u0304 in de\u00df Liebsten Wund\u0113", "tokens": ["Schlaf", "mei\u00b7ne", "See\u00b7le", "schlaf", ":", "Dan\u0304", "in", "de\u00df", "Liebs\u00b7ten", "Wun\u00b7d\u0113"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+++-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Hastu die sicherheit und volle Ruh gefunden.", "tokens": ["Has\u00b7tu", "die", "si\u00b7cher\u00b7heit", "und", "vol\u00b7le", "Ruh", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}