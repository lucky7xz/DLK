{"textgrid.poem.33776": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Das Halali", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sein Jagdschlo\u00df war auf viele Meilen weit", "tokens": ["Sein", "Jagd\u00b7schlo\u00df", "war", "auf", "vie\u00b7le", "Mei\u00b7len", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Forst umgeben, und er jagte da", "tokens": ["Von", "Forst", "um\u00b7ge\u00b7ben", ",", "und", "er", "jag\u00b7te", "da"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "KON", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom ersten Fr\u00fchrot bis zur Dunkelheit", "tokens": ["Vom", "ers\u00b7ten", "Fr\u00fch\u00b7rot", "bis", "zur", "Dun\u00b7kel\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf alles, was er leben sah.", "tokens": ["Auf", "al\u00b7les", ",", "was", "er", "le\u00b7ben", "sah", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er scho\u00df den Hirsch, das Reh, den Reiher,", "tokens": ["Er", "scho\u00df", "den", "Hirsch", ",", "das", "Reh", ",", "den", "Rei\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Falken und die Entenschar im Weiher.", "tokens": ["Den", "Fal\u00b7ken", "und", "die", "En\u00b7ten\u00b7schar", "im", "Wei\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Parforcejagd war sein H\u00f6chstes, oder Wild", "tokens": ["Par\u00b7for\u00b7ce\u00b7jagd", "war", "sein", "H\u00f6chs\u00b7tes", ",", "o\u00b7der", "Wild"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "$,", "KON", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Im Bau gepackt vom Hund. Den Tro\u00df, die Meute", "tokens": ["Im", "Bau", "ge\u00b7packt", "vom", "Hund", ".", "Den", "Tro\u00df", ",", "die", "Meu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "APPRART", "NN", "$.", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und das Halali stellte dar manch Bild", "tokens": ["Und", "das", "Ha\u00b7la\u00b7li", "stell\u00b7te", "dar", "manch", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "PIAT", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Im Schlafgemach, im Speisesaal; die Beute", "tokens": ["Im", "Schlaf\u00b7ge\u00b7mach", ",", "im", "Spei\u00b7se\u00b7saal", ";", "die", "Beu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Dar\u00fcber h\u00e4ngend zeigte rings Geweih", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "h\u00e4n\u00b7gend", "zeig\u00b7te", "rings", "Ge\u00b7weih"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADJD", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und seine Lust und Augenweid' in Schr\u00e4nken:", "tokens": ["Und", "sei\u00b7ne", "Lust", "und", "Au\u00b7gen\u00b7weid'", "in", "Schr\u00e4n\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Gewehre, Pulverh\u00f6rner, Haufen Blei.", "tokens": ["Ge\u00b7weh\u00b7re", ",", "Pul\u00b7ver\u00b7h\u00f6r\u00b7ner", ",", "Hau\u00b7fen", "Blei", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Gebreitet lagen \u00fcber Tisch und B\u00e4nken", "tokens": ["Ge\u00b7brei\u00b7tet", "la\u00b7gen", "\u00fc\u00b7ber", "Tisch", "und", "B\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Erlegter Tiere Decken und darauf", "tokens": ["Er\u00b7leg\u00b7ter", "Tie\u00b7re", "De\u00b7cken", "und", "da\u00b7rauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "KON", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die Becher und die Leuchter aus Gebeinen;", "tokens": ["Die", "Be\u00b7cher", "und", "die", "Leuch\u00b7ter", "aus", "Ge\u00b7bei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Denn au\u00dfer Jagd und Mast von wilden Schweinen", "tokens": ["Denn", "au\u00b7\u00dfer", "Jagd", "und", "Mast", "von", "wil\u00b7den", "Schwei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "War noch das Zechgelag sein Lebenslauf.", "tokens": ["War", "noch", "das", "Zech\u00b7ge\u00b7lag", "sein", "Le\u00b7bens\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es kam der Krieg, und alsobald", "tokens": ["Es", "kam", "der", "Krieg", ",", "und", "al\u00b7so\u00b7bald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward aufgebrochen und ins Feld gezogen.", "tokens": ["Ward", "auf\u00b7ge\u00b7bro\u00b7chen", "und", "ins", "Feld", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch da war oft die Szene noch der Wald,", "tokens": ["Auch", "da", "war", "oft", "die", "Sze\u00b7ne", "noch", "der", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADV", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch kam auf ihn das Blei jetzt hergeflogen,", "tokens": ["Doch", "kam", "auf", "ihn", "das", "Blei", "jetzt", "her\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und einst \u2013 sieh da \u2013 traf eine Kugel ihn,", "tokens": ["Und", "einst", "\u2013", "sieh", "da", "\u2013", "traf", "ei\u00b7ne", "Ku\u00b7gel", "ihn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "VVIMP", "ADV", "$(", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Den J\u00e4ger, in die Brust. Er schleppte lechzend", "tokens": ["Den", "J\u00e4\u00b7ger", ",", "in", "die", "Brust", ".", "Er", "schlepp\u00b7te", "lech\u00b7zend"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nach einem Bach sich unter Tannen hin", "tokens": ["Nach", "ei\u00b7nem", "Bach", "sich", "un\u00b7ter", "Tan\u00b7nen", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und hauchte dort, nur hie und da noch \u00e4chzend,", "tokens": ["Und", "hauch\u00b7te", "dort", ",", "nur", "hie", "und", "da", "noch", "\u00e4ch\u00b7zend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADV", "ADV", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ganz einsam still sein Leben aus,", "tokens": ["Ganz", "ein\u00b7sam", "still", "sein", "Le\u00b7ben", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ganz wie das Wild, dem er so oft zu Haus", "tokens": ["Ganz", "wie", "das", "Wild", ",", "dem", "er", "so", "oft", "zu", "Haus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das gleiche Los bereitet hatte. Ferne", "tokens": ["Das", "glei\u00b7che", "Los", "be\u00b7rei\u00b7tet", "hat\u00b7te", ".", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Verhallte das Get\u00f6s der Schlacht,", "tokens": ["Ver\u00b7hall\u00b7te", "das", "Ge\u00b7t\u00f6s", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und durch die Tannen schien das Licht der Sterne.", "tokens": ["Und", "durch", "die", "Tan\u00b7nen", "schien", "das", "Licht", "der", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da schlich ein Leichenr\u00e4uber durch die Nacht;", "tokens": ["Da", "schlich", "ein", "Lei\u00b7chen\u00b7r\u00e4u\u00b7ber", "durch", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er sah den Toten liegen, bog", "tokens": ["Er", "sah", "den", "To\u00b7ten", "lie\u00b7gen", ",", "bog"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich \u00fcber ihn und zog", "tokens": ["Sich", "\u00fc\u00b7ber", "ihn", "und", "zog"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "PPER", "KON", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Weidmannsmesser aus der Tasche, schlitzte", "tokens": ["Ein", "Weid\u00b7manns\u00b7mes\u00b7ser", "aus", "der", "Ta\u00b7sche", ",", "schlitz\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Koller auf, aus dem es rann", "tokens": ["Das", "Kol\u00b7ler", "auf", ",", "aus", "dem", "es", "rann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Wellen laut'res Golds, und schnitt sodann", "tokens": ["Wie", "Wel\u00b7len", "laut'\u00b7res", "Golds", ",", "und", "schnitt", "so\u00b7dann"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJA", "NN", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den Finger durch, an dem ein Demant blitzte.", "tokens": ["Den", "Fin\u00b7ger", "durch", ",", "an", "dem", "ein", "De\u00b7mant", "blitz\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "