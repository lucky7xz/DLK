{"dta.poem.4289": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sau-Jagd.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welch ein ungewisser Kampf, welch ein zweifelhaftes", "tokens": ["Welch", "ein", "un\u00b7ge\u00b7wis\u00b7ser", "Kampf", ",", "welch", "ein", "zwei\u00b7fel\u00b7haf\u00b7tes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "$,", "PWAT", "ART", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Kriegen!", "tokens": ["Krie\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wird das Heer der Hunde hier, oder wird der Hauer", "tokens": ["Wird", "das", "Heer", "der", "Hun\u00b7de", "hier", ",", "o\u00b7der", "wird", "der", "Hau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADV", "$,", "KON", "VAFIN", "ART", "NN"], "meter": "--+-+----+-+-", "measure": "anapaest.init"}, "line.4": {"text": "siegen?", "tokens": ["sie\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ein entsetzliches Gew\u00fchl, ein bet\u00e4ubendes Geschrey,", "tokens": ["Ein", "ent\u00b7setz\u00b7li\u00b7ches", "Ge\u00b7w\u00fchl", ",", "ein", "be\u00b7t\u00e4u\u00b7ben\u00b7des", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Meynt der Geist, im Blick get\u00e4uscht, nicht zu seh\u2019n nur,", "tokens": ["Meynt", "der", "Geist", ",", "im", "Blick", "ge\u00b7t\u00e4uscht", ",", "nicht", "zu", "seh'n", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,", "PTKNEG", "PTKZU", "VVINF", "ADV", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "gar zu h\u00f6ren.", "tokens": ["gar", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Seht, wie w\u00fctend st\u00fcrzt der Schwarm der ergrimmten", "tokens": ["Seht", ",", "wie", "w\u00fc\u00b7tend", "st\u00fcrzt", "der", "Schwarm", "der", "er\u00b7grimm\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Hund' herbey!", "tokens": ["Hund'", "her\u00b7bey", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.10": {"text": "Seht den angefallnen Eber sich recht heldenm\u00fchtig weh-", "tokens": ["Seht", "den", "an\u00b7ge\u00b7fall\u00b7nen", "E\u00b7ber", "sich", "recht", "hel\u00b7den\u00b7m\u00fch\u00b7tig", "weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF", "ADV", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "ren!", "tokens": ["ren", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Seht, wie er die regen Waffen, als zwey Todes-Sensen,", "tokens": ["Seht", ",", "wie", "er", "die", "re\u00b7gen", "Waf\u00b7fen", ",", "als", "zwey", "To\u00b7des\u00b7Sen\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "$,", "KOUS", "CARD", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "dreht,", "tokens": ["dreht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.14": {"text": "Und, was er ereilen kann, ungehemmt darnieder m\u00e4ht!", "tokens": ["Und", ",", "was", "er", "er\u00b7ei\u00b7len", "kann", ",", "un\u00b7ge\u00b7hemmt", "dar\u00b7nie\u00b7der", "m\u00e4ht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,", "ADJD", "PAV", "VVFIN", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Seines Muhtes, seiner Kraft, Blut-bespr\u00fctzte Sieges-", "tokens": ["Sei\u00b7nes", "Muh\u00b7tes", ",", "sei\u00b7ner", "Kraft", ",", "Blut\u00b7be\u00b7spr\u00fctz\u00b7te", "Sie\u00b7ges"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ADJA", "TRUNC"], "meter": "+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.16": {"text": "Zeichen", "tokens": ["Zei\u00b7chen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.17": {"text": "Zeigt er, zu der J\u00e4ger Gram, in der Hunde offnen B\u00e4u-", "tokens": ["Zeigt", "er", ",", "zu", "der", "J\u00e4\u00b7ger", "Gram", ",", "in", "der", "Hun\u00b7de", "off\u00b7nen", "B\u00e4u"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NN", "NE", "$,", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "chen.", "tokens": ["chen", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}, "line.19": {"text": "Zwischen eines wilden Helden und des Hauers Tap-", "tokens": ["Zwi\u00b7schen", "ei\u00b7nes", "wil\u00b7den", "Hel\u00b7den", "und", "des", "Hau\u00b7ers", "Tap"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.20": {"text": "ferkeit", "tokens": ["fer\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Find ich, mit dem grossen ", "tokens": ["Find", "ich", ",", "mit", "dem", "gros\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.22": {"text": "Unterscheid.", "tokens": ["Un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}