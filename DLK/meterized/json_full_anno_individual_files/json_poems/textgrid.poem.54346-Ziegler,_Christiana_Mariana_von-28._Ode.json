asure": "iambic.di.relaxed"}, "line.9": {"text": "R\u00fchrt mich der sanfte Thon.", "tokens": ["R\u00fchrt", "mich", "der", "sanf\u00b7te", "Thon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dein Wunsch ist eingetroffen,", "tokens": ["Dein", "Wunsch", "ist", "ein\u00b7ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df dich mein Herze liebt.", "tokens": ["Da\u00df", "dich", "mein", "Her\u00b7ze", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So kannst du alles hoffen", "tokens": ["So", "kannst", "du", "al\u00b7les", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was dir Vergn\u00fcgen giebt.", "tokens": ["Was", "dir", "Ver\u00b7gn\u00fc\u00b7gen", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn was du mir willst geben,", "tokens": ["Denn", "was", "du", "mir", "willst", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bekommst du schon von mir.", "tokens": ["Be\u00b7kommst", "du", "schon", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich habe nur ein Leben,", "tokens": ["Ich", "ha\u00b7be", "nur", "ein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ach nur ein einzig Leben;", "tokens": ["Ach", "nur", "ein", "ein\u00b7zig", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und das geh\u00f6ret dir.", "tokens": ["Und", "das", "ge\u00b7h\u00f6\u00b7ret", "dir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}