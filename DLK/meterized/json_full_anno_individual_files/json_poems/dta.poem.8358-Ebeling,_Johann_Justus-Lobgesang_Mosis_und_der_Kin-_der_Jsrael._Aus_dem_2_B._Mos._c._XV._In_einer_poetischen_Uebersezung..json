ord", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJA", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du hast uns durch deine Krafft zu dem Lande heim-", "tokens": ["Du", "hast", "uns", "durch", "dei\u00b7ne", "Krafft", "zu", "dem", "Lan\u00b7de", "heim"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das uns als ein heilges Land, als der V\u00e4ter Erb", "tokens": ["Das", "uns", "als", "ein", "heil\u00b7ges", "Land", ",", "als", "der", "V\u00e4\u00b7ter", "Erb"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "KOUS", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Da der Auszug ruchtbar ward, der durch deine\nHand geschehn,", "tokens": ["Da", "der", "Aus\u00b7zug", "rucht\u00b7bar", "ward", ",", "der", "durch", "dei\u00b7ne", "Hand", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "--+-+----+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Bebete der V\u00f6lker Herz dahin wir nun solten gehn,", "tokens": ["Be\u00b7be\u00b7te", "der", "V\u00f6l\u00b7ker", "Herz", "da\u00b7hin", "wir", "nun", "sol\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PAV", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Philister zitterten, ihr Gem\u00fcthe ward beklom-", "tokens": ["Die", "Phi\u00b7lis\u00b7ter", "zit\u00b7ter\u00b7ten", ",", "ihr", "Ge\u00b7m\u00fc\u00b7the", "ward", "be\u00b7klom"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "TRUNC"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Denn sie sahen zum voraus da\u00df ihr Land schon weg-", "tokens": ["Denn", "sie", "sa\u00b7hen", "zum", "vo\u00b7raus", "da\u00df", "ihr", "Land", "schon", "weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADV", "KOUS", "PPOSAT", "NN", "ADV", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Edoms F\u00fcrsten wurden Angst, als sie h\u00f6rten\ndas Ger\u00fccht,", "tokens": ["E\u00b7doms", "F\u00fcrs\u00b7ten", "wur\u00b7den", "Angst", ",", "als", "sie", "h\u00f6r\u00b7ten", "das", "Ge\u00b7r\u00fccht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "NN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da die ganze Gegend HErr! jezt von deinen Wun-", "tokens": ["Da", "die", "gan\u00b7ze", "Ge\u00b7gend", "Herr", "!", "jezt", "von", "dei\u00b7nen", "Wun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$.", "ADV", "APPR", "PPOSAT", "TRUNC"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und ein Zittern \u00fcberfiel alle die in Moab thronen:", "tokens": ["Und", "ein", "Zit\u00b7tern", "\u00fc\u00b7berf\u00b7iel", "al\u00b7le", "die", "in", "Mo\u00b7ab", "thro\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIS", "ART", "APPR", "NN", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Alle V\u00f6lker wurden feig, die in Canans Grenzen", "tokens": ["Al\u00b7le", "V\u00f6l\u00b7ker", "wur\u00b7den", "feig", ",", "die", "in", "Ca\u00b7nans", "Gren\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$,", "PRELS", "APPR", "NE", "NN"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.16": {"line.1": {"text": "La\u00df das Schrekken auf sie falln HErr! durch\ndeinen grossen Arm,", "tokens": ["La\u00df", "das", "Schrek\u00b7ken", "auf", "sie", "falln", "Herr", "!", "durch", "dei\u00b7nen", "gros\u00b7sen", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "PPER", "VVFIN", "NN", "$.", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und bet\u00e4ube mit der Furcht dieser V\u00f6lker wilden", "tokens": ["Und", "be\u00b7t\u00e4u\u00b7be", "mit", "der", "Furcht", "die\u00b7ser", "V\u00f6l\u00b7ker", "wil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PDAT", "NN", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Da\u00df sie wie ein Stein erstarrn, bis dein Volk hin-", "tokens": ["Da\u00df", "sie", "wie", "ein", "Stein", "er\u00b7starrn", ",", "bis", "dein", "Volk", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "ART", "NN", "VVINF", "$,", "KOUS", "PPOSAT", "NN", "TRUNC"], "meter": "--+-+-+-++-", "measure": 