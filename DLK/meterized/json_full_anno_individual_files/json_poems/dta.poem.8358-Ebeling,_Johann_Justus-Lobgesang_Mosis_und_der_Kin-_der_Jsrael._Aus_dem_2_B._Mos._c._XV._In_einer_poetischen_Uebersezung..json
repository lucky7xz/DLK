{"dta.poem.8358": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Lobgesang Mosis und der Kin-  \n der Jsrael.  \n Aus dem 2 B. Mos. c.  XV.  \n In einer poetischen Uebersezung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir besingen deine That, grosser Herr-\nscher aller Welt,", "tokens": ["Wir", "be\u00b7sin\u00b7gen", "dei\u00b7ne", "That", ",", "gros\u00b7ser", "Herr", "scher", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADJA", "TRUNC", "ADJA", "PIAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da durch dich dein Jsrael Leben", "tokens": ["Da", "durch", "dich", "dein", "Js\u00b7rael", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "PPOSAT", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Feinde sind gest\u00fcrzt, und die grossen Krieges-", "tokens": ["Uns\u00b7re", "Fein\u00b7de", "sind", "ge\u00b7st\u00fcrzt", ",", "und", "die", "gros\u00b7sen", "Krie\u00b7ge\u00b7s"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "KON", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ro\u00df und Wagen fliessen dort, todt zerbrochen in dem", "tokens": ["Ro\u00df", "und", "Wa\u00b7gen", "flies\u00b7sen", "dort", ",", "todt", "zer\u00b7bro\u00b7chen", "in", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "$,", "ADJD", "VVPP", "APPR", "ART"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Du bist unsre St\u00e4rke HErr! unser Heil, der\nLieder Klang", "tokens": ["Du", "bist", "uns\u00b7re", "St\u00e4r\u00b7ke", "Herr", "!", "un\u00b7ser", "Heil", ",", "der", "Lie\u00b7der", "Klang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "$.", "PPOSAT", "NN", "$,", "ART", "NN", "NN"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bringet dir mit Herz und Mund einen frohen Lob-", "tokens": ["Brin\u00b7get", "dir", "mit", "Herz", "und", "Mund", "ei\u00b7nen", "fro\u00b7hen", "Lob"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Du bist unser Schuz und ", "tokens": ["Du", "bist", "un\u00b7ser", "Schuz", "und"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "KON"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Du bist unsrer V\u00e4ter ", "tokens": ["Du", "bist", "uns\u00b7rer", "V\u00e4\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Gott der ist ein grosser Held, zieht er in den\nKrieg und Streit,", "tokens": ["Gott", "der", "ist", "ein", "gros\u00b7ser", "Held", ",", "zieht", "er", "in", "den", "Krieg", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VAFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "So wird gleich im Augenblik, seiner Feinde Macht", "tokens": ["So", "wird", "gleich", "im", "Au\u00b7gen\u00b7blik", ",", "sei\u00b7ner", "Fein\u00b7de", "Macht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "APPRART", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Er ist ein gewaltger HErr, l\u00e4st er seinen Donner", "tokens": ["Er", "ist", "ein", "ge\u00b7walt\u00b7ger", "Herr", ",", "l\u00e4st", "er", "sei\u00b7nen", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So mu\u00df alles durch den Grim seiner Blizen pl\u00f6zlich", "tokens": ["So", "mu\u00df", "al\u00b7les", "durch", "den", "Grim", "sei\u00b7ner", "Bli\u00b7zen", "pl\u00f6z\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PIS", "APPR", "ART", "NN", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "O! die Wagen Pharao und sein grosses Krie-\nges Heer", "tokens": ["O", "!", "die", "Wa\u00b7gen", "Pha\u00b7rao", "und", "sein", "gros\u00b7ses", "Krie", "ges", "Heer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ART", "NN", "NN", "KON", "PPOSAT", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Warf er pl\u00f6zlich durch den Sturz in das aufgeschwol-", "tokens": ["Warf", "er", "pl\u00f6z\u00b7lich", "durch", "den", "Sturz", "in", "das", "auf\u00b7ge\u00b7schwol"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Seine Helden sind dahin, die Hauptleute sind ver-", "tokens": ["Sei\u00b7ne", "Hel\u00b7den", "sind", "da\u00b7hin", ",", "die", "Haupt\u00b7leu\u00b7te", "sind", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PAV", "$,", "ART", "NN", "VAFIN", "TRUNC"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und in den beschilften Schlund, in den tieffen Schlam", "tokens": ["Und", "in", "den", "be\u00b7schilf\u00b7ten", "Schlund", ",", "in", "den", "tief\u00b7fen", "Schlam"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Dieser Tieffen weiter Bauch hat sie f\u00fcrchterlich\nverstekt,", "tokens": ["Die\u00b7ser", "Tief\u00b7fen", "wei\u00b7ter", "Bauch", "hat", "sie", "f\u00fcrch\u00b7ter\u00b7lich", "vers\u00b7tekt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da sie durch den nassen Schaum des Gew\u00e4ssers \u00fc-", "tokens": ["Da", "sie", "durch", "den", "nas\u00b7sen", "Schaum", "des", "Ge\u00b7w\u00e4s\u00b7sers", "\u00fc"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "TRUNC"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Pl\u00f6zlich fielen sie zur Grund, wie die Steine die mit", "tokens": ["Pl\u00f6z\u00b7lich", "fie\u00b7len", "sie", "zur", "Grund", ",", "wie", "die", "Stei\u00b7ne", "die", "mit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "ART", "APPR"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wenn man sie ins Wasser schmeist durch die Fluth", "tokens": ["Wenn", "man", "sie", "ins", "Was\u00b7ser", "schmeist", "durch", "die", "Fluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Herre! deine rechte Hand, die uns deine All-\nmacht zeigt,", "tokens": ["Her\u00b7re", "!", "dei\u00b7ne", "rech\u00b7te", "Hand", ",", "die", "uns", "dei\u00b7ne", "All", "macht", "zeigt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "TRUNC", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Die so grosse Wunder thut, und die stolzen Feinde", "tokens": ["Die", "so", "gros\u00b7se", "Wun\u00b7der", "thut", ",", "und", "die", "stol\u00b7zen", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Herre! deine Wundermacht hat die Feinde, Ro\u00df", "tokens": ["Her\u00b7re", "!", "dei\u00b7ne", "Wun\u00b7der\u00b7macht", "hat", "die", "Fein\u00b7de", ",", "Ro\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Alles was sich wiedersezt in den schnellen Grim zer-", "tokens": ["Al\u00b7les", "was", "sich", "wie\u00b7der\u00b7sezt", "in", "den", "schnel\u00b7len", "Grim", "zer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "PRELS", "PRF", "ADV", "APPR", "ART", "ADJA", "NE", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Deine grosse Herrlichkeit bringt den Widerstand\nzum Fall,", "tokens": ["Dei\u00b7ne", "gros\u00b7se", "Herr\u00b7lich\u00b7keit", "bringt", "den", "Wi\u00b7der\u00b7stand", "zum", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wenn du deinen Grim ausschie\u00dft; so entsteht ein", "tokens": ["Wenn", "du", "dei\u00b7nen", "Grim", "aus\u00b7schie\u00dft", ";", "so", "ent\u00b7steht", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ART"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "So entsteht ein strenger Bliz, der die Feinde gleich", "tokens": ["So", "ent\u00b7steht", "ein", "stren\u00b7ger", "Bliz", ",", "der", "die", "Fein\u00b7de", "gleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "ADV"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und durch ein entglomnes Feur wie die Stoppeln,", "tokens": ["Und", "durch", "ein", "ent\u00b7glom\u00b7nes", "Feur", "wie", "die", "Stop\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Durch dein Blasen th\u00e4ten sich die Gew\u00e4sser schleu-\nnig auf,", "tokens": ["Durch", "dein", "Bla\u00b7sen", "th\u00e4\u00b7ten", "sich", "die", "Ge\u00b7w\u00e4s\u00b7ser", "schleu", "nig", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "TRUNC", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Und die Fluthen stunden still als wie Mauren, in", "tokens": ["Und", "die", "Flut\u00b7hen", "stun\u00b7den", "still", "als", "wie", "Mau\u00b7ren", ",", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "KOKOM", "KOKOM", "NN", "$,", "APPR"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mitten in dem tieffen Meer sahe man das rege Wal-", "tokens": ["Mit\u00b7ten", "in", "dem", "tief\u00b7fen", "Meer", "sa\u00b7he", "man", "das", "re\u00b7ge", "Wa\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "PIS", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Von einander gleichsam gehn, in den Sprudel r\u00fck-", "tokens": ["Von", "ein\u00b7an\u00b7der", "gleich\u00b7sam", "gehn", ",", "in", "den", "Spru\u00b7del", "r\u00fc\u00b7k"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ADJD", "VVINF", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Da gedachte unser Feind, da\u00df er uns mit seiner\nJagd,", "tokens": ["Da", "ge\u00b7dach\u00b7te", "un\u00b7ser", "Feind", ",", "da\u00df", "er", "uns", "mit", "sei\u00b7ner", "Jagd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Auf der schnellen Flucht erhascht, und bereits zu Nicht", "tokens": ["Auf", "der", "schnel\u00b7len", "Flucht", "er\u00b7hascht", ",", "und", "be\u00b7reits", "zu", "Nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "KON", "ADV", "APPR", "PTKNEG"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Er gedachte seinen Muth noch zulezt an uns zu k\u00fch-", "tokens": ["Er", "ge\u00b7dach\u00b7te", "sei\u00b7nen", "Muth", "noch", "zu\u00b7lezt", "an", "uns", "zu", "k\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "PPER", "APPR", "TRUNC"], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Und wir solten Hand und Schwerd pl\u00f6zlich zum Ver-", "tokens": ["Und", "wir", "sol\u00b7ten", "Hand", "und", "Schwerd", "pl\u00f6z\u00b7lich", "zum", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "NN", "KON", "NN", "ADJD", "APPRART", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.10": {"line.1": {"text": "Aber HErr du Zebaoth! dem die Fluthen,\nMeer und Wind,", "tokens": ["A\u00b7ber", "Herr", "du", "Ze\u00b7bao\u00b7th", "!", "dem", "die", "Flut\u00b7hen", ",", "Meer", "und", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "NE", "$.", "ART", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wie es nur dein Wink gebeut, alsobald gehorsam", "tokens": ["Wie", "es", "nur", "dein", "Wink", "ge\u00b7beut", ",", "al\u00b7so\u00b7bald", "ge\u00b7hor\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Du bedektest sie im Meer, da\u00df sie wie das Blei ver-", "tokens": ["Du", "be\u00b7dek\u00b7test", "sie", "im", "Meer", ",", "da\u00df", "sie", "wie", "das", "Blei", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "KOKOM", "ART", "NN", "TRUNC"], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In der starken Wirbelfluth bis zum Abgrund sind", "tokens": ["In", "der", "star\u00b7ken", "Wir\u00b7bel\u00b7fluth", "bis", "zum", "Ab\u00b7grund", "sind"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "APPRART", "NN", "VAFIN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Herr! wer ist dir von der Zahl eingebildter\nG\u00f6tter gleich,", "tokens": ["Herr", "!", "wer", "ist", "dir", "von", "der", "Zahl", "ein\u00b7ge\u00b7bild\u00b7ter", "G\u00f6t\u00b7ter", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "ADV", "$,"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Wer ist an Vollkommenheit, in dem weiten Erden", "tokens": ["Wer", "ist", "an", "Voll\u00b7kom\u00b7men\u00b7heit", ",", "in", "dem", "wei\u00b7ten", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "NN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Der dir gleich an Heiligkeit, an gerechter Macht zu", "tokens": ["Der", "dir", "gleich", "an", "Hei\u00b7lig\u00b7keit", ",", "an", "ge\u00b7rech\u00b7ter", "Macht", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "APPR"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wer ist Dir an Ruhm und That noch woll an die", "tokens": ["Wer", "ist", "Dir", "an", "Ruhm", "und", "That", "noch", "woll", "an", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "NN", "KON", "NN", "ADV", "ADV", "APPR", "ART"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.12": {"line.1": {"text": "Keiner ist in aller Welt, der wie du allm\u00e4chtig\nheist,", "tokens": ["Kei\u00b7ner", "ist", "in", "al\u00b7ler", "Welt", ",", "der", "wie", "du", "all\u00b7m\u00e4ch\u00b7tig", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PIAT", "NN", "$,", "PRELS", "KOKOM", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-++-+-+", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Dessen Arm so wunderbahr alle Macht zu Boden", "tokens": ["Des\