{"dta.poem.7745": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein kleiner F\u00fcrst im Land, den gro\u00df ich preise,", "tokens": ["Es", "ist", "ein", "klei\u00b7ner", "F\u00fcrst", "im", "Land", ",", "den", "gro\u00df", "ich", "prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "ART", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den, weil er nicht will laut gelobt seyn, lob' ich leise.", "tokens": ["Den", ",", "weil", "er", "nicht", "will", "laut", "ge\u00b7lobt", "seyn", ",", "lob'", "ich", "lei\u00b7se", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PTKNEG", "VMFIN", "ADJD", "VVPP", "VAINF", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er hat die F\u00fcrstlichkeit erkannt in ihrem Wesen,", "tokens": ["Er", "hat", "die", "F\u00fcrst\u00b7lich\u00b7keit", "er\u00b7kannt", "in", "ih\u00b7rem", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und will den Titelprunk nicht h\u00f6ren und nicht lesen.", "tokens": ["Und", "will", "den", "Ti\u00b7tel\u00b7prunk", "nicht", "h\u00f6\u00b7ren", "und", "nicht", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "KON", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Schranken hat er weggehoben zwischen sich", "tokens": ["Die", "Schran\u00b7ken", "hat", "er", "weg\u00b7ge\u00b7ho\u00b7ben", "zwi\u00b7schen", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "APPR", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seinem Volk, da\u00df frei ihm nahn darf m\u00e4nniglich.", "tokens": ["Und", "sei\u00b7nem", "Volk", ",", "da\u00df", "frei", "ihm", "nahn", "darf", "m\u00e4n\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "KOUS", "ADJD", "PPER", "ADJA", "VMFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er will den Zugang nicht zu seinem Ohr vertheuert,", "tokens": ["Er", "will", "den", "Zu\u00b7gang", "nicht", "zu", "sei\u00b7nem", "Ohr", "ver\u00b7theu\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Erlaubnis ihn zu bitten, unbesteuert.", "tokens": ["Und", "die", "Er\u00b7laub\u00b7nis", "ihn", "zu", "bit\u00b7ten", ",", "un\u00b7be\u00b7steu\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er will beweisen, da\u00df ein F\u00fcrst noch mit Vertraun", "tokens": ["Er", "will", "be\u00b7wei\u00b7sen", ",", "da\u00df", "ein", "F\u00fcrst", "noch", "mit", "Ver\u00b7traun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "KOUS", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kann auf sein Volk, ein Volk auf seinen F\u00fcrsten schaun.", "tokens": ["Kann", "auf", "sein", "Volk", ",", "ein", "Volk", "auf", "sei\u00b7nen", "F\u00fcrs\u00b7ten", "schaun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "O m\u00f6g' er den Beweis, der noth thut, gl\u00e4nzend f\u00fchren,", "tokens": ["O", "m\u00f6g'", "er", "den", "Be\u00b7weis", ",", "der", "noth", "thut", ",", "gl\u00e4n\u00b7zend", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dieser Zeit, wo sich des Mistrauns Feuer sch\u00fcren.", "tokens": ["In", "die\u00b7ser", "Zeit", ",", "wo", "sich", "des", "Mis\u00b7trauns", "Feu\u00b7er", "sch\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PRF", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ihr gr\u00f6\u00dfern, schaut auf ihn, und nehmt von ihm ein Zeichen!", "tokens": ["Ihr", "gr\u00f6\u00b7\u00dfern", ",", "schaut", "auf", "ihn", ",", "und", "nehmt", "von", "ihm", "ein", "Zei\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "VVFIN", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie m\u00fc\u00dft ihr wachsen noch, wenn ihr ihn wollt erreichen!", "tokens": ["Wie", "m\u00fc\u00dft", "ihr", "wach\u00b7sen", "noch", ",", "wenn", "ihr", "ihn", "wollt", "er\u00b7rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}