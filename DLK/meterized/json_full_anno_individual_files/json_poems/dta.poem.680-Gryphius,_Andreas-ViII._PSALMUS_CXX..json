{"dta.poem.680": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n PSALMUS CXX.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "So offt der grimme schmertz/ ", "tokens": ["So", "offt", "der", "grim\u00b7me", "schmertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So offt der herbe schertz/", "tokens": ["So", "offt", "der", "her\u00b7be", "schertz", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So offt sich auff mein hertz", "tokens": ["So", "offt", "sich", "auff", "mein", "hertz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd Seel der bleiche Todt verschworen:", "tokens": ["Vnd", "Seel", "der", "blei\u00b7che", "Todt", "ver\u00b7schwo\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn aller trost verschwand:", "tokens": ["Wenn", "al\u00b7ler", "trost", "ver\u00b7schwand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Hab ich Hertz/ Aug vnd hand ", "tokens": ["Hab", "ich", "Hertz", "/", "Aug", "vnd", "hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "$(", "NN", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Zu GOtt/ der helffer heist/ gewendet? ", "tokens": ["Zu", "Gott", "/", "der", "helf\u00b7fer", "heist", "/", "ge\u00b7wen\u00b7det", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die seufftzer die ich jhm gesendet/", "tokens": ["Die", "seufft\u00b7zer", "die", "ich", "jhm", "ge\u00b7sen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRELS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die stiegen jhm zu beiden ohren.", "tokens": ["Die", "stie\u00b7gen", "jhm", "zu", "bei\u00b7den", "oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Mein seufftzen/ meine bitt\u2019", "tokens": ["Mein", "seufft\u00b7zen", "/", "mei\u00b7ne", "bitt'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "VVINF", "$(", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Erweichte sein gem\u00fctt/ ", "tokens": ["Er\u00b7weich\u00b7te", "sein", "ge\u00b7m\u00fctt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er der Brunn der g\u00fctt/ ", "tokens": ["Da\u00df", "er", "der", "Brunn", "der", "g\u00fctt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Vom Himmel auff mein elend sahe.", "tokens": ["Vom", "Him\u00b7mel", "auff", "mein", "e\u00b7lend", "sa\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es sahe meinen schmertz ", "tokens": ["Es", "sa\u00b7he", "mei\u00b7nen", "schmertz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sein ewigtrewes hertz/", "tokens": ["Sein", "e\u00b7wig\u00b7tre\u00b7wes", "hertz", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er zog mich aus der wehm\u00fct stricken. ", "tokens": ["Er", "zog", "mich", "aus", "der", "weh\u00b7m\u00fct", "stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ja wenn ich wolt' in angst ersticken: ", "tokens": ["Ja", "wenn", "ich", "wolt'", "in", "angst", "er\u00b7sti\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "KOUS", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "War er mit seinem beystandt nahe.", "tokens": ["War", "er", "mit", "sei\u00b7nem", "beys\u00b7tandt", "na\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. HERR der du mich erh\u00f6rt/", "tokens": ["HeRR", "der", "du", "mich", "er\u00b7h\u00f6rt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "PRF", "VVPP", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wenn dich mein Geist geehrt.", "tokens": ["Wenn", "dich", "mein", "Geist", "ge\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie das mich jtzt versehrt/", "tokens": ["Wie", "das", "mich", "jtzt", "ver\u00b7sehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Natterzungen tolles zischen?", "tokens": ["Der", "Nat\u00b7ter\u00b7zun\u00b7gen", "tol\u00b7les", "zi\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Soll mich denn jede stundt/ ", "tokens": ["Soll", "mich", "denn", "je\u00b7de", "stundt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der falschen l\u00e4ster mundt", "tokens": ["Der", "fal\u00b7schen", "l\u00e4s\u00b7ter", "mundt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das l\u00fcgen-reiche maul verletzen?", "tokens": ["Das", "l\u00fc\u00b7gen\u00b7rei\u00b7che", "maul", "ver\u00b7let\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein GOtt! wenn wirstu mich ergetzen", "tokens": ["Mein", "Gott", "!", "wenn", "wirs\u00b7tu", "mich", "er\u00b7get\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KOUS", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vnd diese threnen mir abwischen?", "tokens": ["Vnd", "die\u00b7se", "thre\u00b7nen", "mir", "ab\u00b7wi\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "4. Mag was mit dieser pein/", "tokens": ["Mag", "was", "mit", "die\u00b7ser", "pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wol zuvergleichen seyn?", "tokens": ["Wol", "zu\u00b7ver\u00b7glei\u00b7chen", "seyn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sie rennt durch marck vnd bein/", "tokens": ["Sie", "rennt", "durch", "marck", "vnd", "bein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als wenn ein Pfeil vom bogen fehret.", "tokens": ["Als", "wenn", "ein", "Pfeil", "vom", "bo\u00b7gen", "feh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie wenn die lichte macht", "tokens": ["Wie", "wenn", "die", "lich\u00b7te", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der donnerflamm\u2019 erkracht/", "tokens": ["Der", "don\u00b7ner\u00b7flamm'", "er\u00b7kracht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vnd die Wacholder streu", "tokens": ["Vnd", "die", "Wa\u00b7chol\u00b7der", "streu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Das eilendt ast vnd Laub verschwindet/ ", "tokens": ["Das", "ei\u00b7lendt", "ast", "vnd", "Laub", "ver\u00b7schwin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vnd strump/ vnd wurtzel gantz verzehret.", "tokens": ["Vnd", "strump", "/", "vnd", "wurt\u00b7zel", "gantz", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "5. A", "tokens": ["A"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "In das du mich verbannt/", "tokens": ["In", "das", "du", "mich", "ver\u00b7bannt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da als dein grim\u2019 entbrandt", "tokens": ["Da", "als", "dein", "grim'", "ent\u00b7brandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "ADJD", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mein Heilandt l\u00e4nger no", "tokens": ["Mein", "Hei\u00b7landt", "l\u00e4n\u00b7ger", "no"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O f\u00fchre mich von hier:", "tokens": ["O", "f\u00fch\u00b7re", "mich", "von", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "HeRR soll ich f\u00fcr vnd f\u00fcr", "tokens": ["HeRR", "soll", "ich", "f\u00fcr", "vnd", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "APPR", "KON", "APPR"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Bey Mesech vnd bey Kedar sitzen! ", "tokens": ["Bey", "Me\u00b7sech", "vnd", "bey", "Ke\u00b7dar", "sit\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was kan dir ", "tokens": ["Was", "kan", "dir"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Ach ", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "6 ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "In frembder dinstbarkeit/", "tokens": ["In", "fremb\u00b7der", "dinst\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In wehmut/ ach vnd leidt/", "tokens": ["In", "weh\u00b7mut", "/", "ach", "vnd", "leidt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "XY", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bi\u00df auff den augenblick verschwendet/", "tokens": ["Bi\u00df", "auff", "den", "au\u00b7gen\u00b7blick", "ver\u00b7schwen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sehne mich nach ruh/ ", "tokens": ["Ich", "seh\u00b7ne", "mich", "nach", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sie richten hader zu ", "tokens": ["Sie", "rich\u00b7ten", "ha\u00b7der", "zu"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKZU"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kom f\u00fchre ", "tokens": ["Kom", "f\u00fch\u00b7re"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Nur kan zu deinem dinst hingeben/", "tokens": ["Nur", "kan", "zu", "dei\u00b7nem", "dinst", "hin\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bi\u00df meine Bilgramschafft vollendet.", "tokens": ["Bi\u00df", "mei\u00b7ne", "Bilg\u00b7ramsc\u00b7hafft", "voll\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}