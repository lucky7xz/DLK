{"textgrid.poem.26473": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei stumm, mein Freund, so sprachst du leise", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei stumm, mein Freund, so sprachst du leise", "tokens": ["Sei", "stumm", ",", "mein", "Freund", ",", "so", "sprachst", "du", "lei\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und k\u00fc\u00dftest mich mit sanftem Mund,", "tokens": ["Und", "k\u00fc\u00df\u00b7test", "mich", "mit", "sanf\u00b7tem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Wort, kein Hauch, kein Blick erweise,", "tokens": ["Kein", "Wort", ",", "kein", "Hauch", ",", "kein", "Blick", "er\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir vereint zu holdem Bund.", "tokens": ["Da\u00df", "wir", "ver\u00b7eint", "zu", "hol\u00b7dem", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Err\u00e4th die Welt, da\u00df ich dein eigen,", "tokens": ["Er\u00b7r\u00e4\u00b7th", "die", "Welt", ",", "da\u00df", "ich", "dein", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "ADJD", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bin ich verloren \u2013 zitterst du?", "tokens": ["Bin", "ich", "ver\u00b7lo\u00b7ren", "\u2013", "zit\u00b7terst", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$(", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "D'rum decke seliges Verschweigen", "tokens": ["D'\u00b7rum", "de\u00b7cke", "se\u00b7li\u00b7ges", "Ver\u00b7schwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das seligste Geheimni\u00df zu!", "tokens": ["Das", "se\u00b7ligs\u00b7te", "Ge\u00b7heim\u00b7ni\u00df", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Du wei\u00dft, ich hab' es dir versprochen", "tokens": ["Du", "wei\u00dft", ",", "ich", "hab'", "es", "dir", "ver\u00b7spro\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit heil'gem Schwur noch, da ich schied;", "tokens": ["Mit", "heil'\u00b7gem", "Schwur", "noch", ",", "da", "ich", "schied", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ach, schon ist er auch gebrochen,", "tokens": ["Doch", "ach", ",", "schon", "ist", "er", "auch", "ge\u00b7bro\u00b7chen", "