{"dta.poem.13378": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten  \n von gelehrten Schriften.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist es auch m\u00f6glich, da\u00df aus Welt und Sternen", "tokens": ["Ist", "es", "auch", "m\u00f6g\u00b7lich", ",", "da\u00df", "aus", "Welt", "und", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sehende Menschen Gott verkennen lernen?", "tokens": ["Se\u00b7hen\u00b7de", "Men\u00b7schen", "Gott", "ver\u00b7ken\u00b7nen", "ler\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVINF", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Da\u00df wir den Sch\u00f6pfer unter Kraut und Thieren", "tokens": ["Da\u00df", "wir", "den", "Sch\u00f6p\u00b7fer", "un\u00b7ter", "Kraut", "und", "Thie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "G\u00e4ntzlich verlieren?", "tokens": ["G\u00e4ntz\u00b7lich", "ver\u00b7lie\u00b7ren", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}