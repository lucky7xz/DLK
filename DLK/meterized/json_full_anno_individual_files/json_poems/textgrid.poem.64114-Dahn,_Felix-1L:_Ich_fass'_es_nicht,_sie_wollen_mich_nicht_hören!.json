: "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und kann ich nicht den Gang des Schicksals wenden", "tokens": ["Und", "kann", "ich", "nicht", "den", "Gang", "des", "Schick\u00b7sals", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ist der Jugendtraum der Welt verloren,", "tokens": ["Und", "ist", "der", "Ju\u00b7gend\u00b7traum", "der", "Welt", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Will ich mit allem, was da sch\u00f6n ist, enden.", "tokens": ["Will", "ich", "mit", "al\u00b7lem", ",", "was", "da", "sch\u00f6n", "ist", ",", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PIS", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Horch! H\u00f6rnerklang! das sind Barbarenheere!", "tokens": ["Horch", "!", "H\u00f6r\u00b7ner\u00b7klang", "!", "das", "sind", "Bar\u00b7ba\u00b7ren\u00b7hee\u00b7re", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "PDS", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun folgt mir, Hellas' Schwung und Romas Tugend!", "tokens": ["Nun", "folgt", "mir", ",", "Hel\u00b7las'", "Schwung", "und", "Ro\u00b7mas", "Tu\u00b7gend", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NE", "NN", "KON", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ph\u00f6bos Apoll, du Gott, den ich verehre,", "tokens": ["Ph\u00f6\u00b7bos", "A\u00b7poll", ",", "du", "Gott", ",", "den", "ich", "ver\u00b7eh\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPER", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Gib mir den sch\u00f6nen, raschen Tod der Jugend!", "tokens": ["Gib", "mir", "den", "sch\u00f6\u00b7nen", ",", "ra\u00b7schen", "Tod", "der", "Ju\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Triff mich im Heldenkampf, im Siegesflug,", "tokens": ["Triff", "mich", "im", "Hel\u00b7den\u00b7kampf", ",", "im", "Sie\u00b7ges\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Triff mich wie den Peliden am Skamandros:", "tokens": ["Triff", "mich", "wie", "den", "Pe\u00b7li\u00b7den", "am", "Ska\u00b7man\u00b7dros", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Dann f\u00fcr den Griechen, der die Perser schlug,", "tokens": ["Dann", "f\u00fcr", "den", "Grie\u00b7chen", ",", "der", "die", "Per\u00b7ser", "schlug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schafft Raum im Hades neben Alexandros!", "tokens": ["Schafft", "Raum", "im", "Ha\u00b7des", "ne\u00b7ben", "A\u00b7le\u00b7xan\u00b7dros", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "APPR", "NE", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}}}}}