{"textgrid.poem.36285": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Epilog", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fabel leichtes Bild ist nun vollendet;", "tokens": ["Der", "Fa\u00b7bel", "leich\u00b7tes", "Bild", "ist", "nun", "voll\u00b7en\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Besorgnis hat, erst zweifelvoll verschlungen,", "tokens": ["Be\u00b7sorg\u00b7nis", "hat", ",", "erst", "zwei\u00b7fel\u00b7voll", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Freud' und Wiederfinden sich gewendet.", "tokens": ["In", "Freud'", "und", "Wie\u00b7der\u00b7fin\u00b7den", "sich", "ge\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Geschwister staunen Brust an Brust umschlungen,", "tokens": ["Ge\u00b7schwis\u00b7ter", "stau\u00b7nen", "Brust", "an", "Brust", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Lebens k\u00fchnes Spiel ist nicht verloren,", "tokens": ["Des", "Le\u00b7bens", "k\u00fch\u00b7nes", "Spiel", "ist", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den alten Ring hat Glaube neu errungen;", "tokens": ["Den", "al\u00b7ten", "Ring", "hat", "Glau\u00b7be", "neu", "er\u00b7run\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da\u00df jeder Rechte sei von Gott erkoren,", "tokens": ["Da\u00df", "je\u00b7der", "Rech\u00b7te", "sei", "von", "Gott", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VAFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der rechte Glaube hat uns all' vereinet,", "tokens": ["Der", "rech\u00b7te", "Glau\u00b7be", "hat", "uns", "all'", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gottheit sinnend wir im Staub geboren.", "tokens": ["Die", "Got\u00b7theit", "sin\u00b7nend", "wir", "im", "Staub", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So schwinde Tr\u00fcbsinn, den wir erst beweinet;", "tokens": ["So", "schwin\u00b7de", "Tr\u00fcb\u00b7sinn", ",", "den", "wir", "erst", "be\u00b7wei\u00b7net", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und la\u00dft, von allem Dunkel frei, uns fragen,", "tokens": ["Und", "la\u00dft", ",", "von", "al\u00b7lem", "Dun\u00b7kel", "frei", ",", "uns", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PIS", "NN", "PTKVZ", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was jenes lichte R\u00e4tselbild nun meinet?", "tokens": ["Was", "je\u00b7nes", "lich\u00b7te", "R\u00e4t\u00b7sel\u00b7bild", "nun", "mei\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Den Frieden kommt der Held uns anzutragen,", "tokens": ["Den", "Frie\u00b7den", "kommt", "der", "Held", "uns", "an\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erkenntnis soll der Zwietracht Kinder t\u00f6ten,", "tokens": ["Er\u00b7kennt\u00b7nis", "soll", "der", "Zwiet\u00b7racht", "Kin\u00b7der", "t\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Lichtes Gunst will uns die Dichtkunst sagen;", "tokens": ["Des", "Lich\u00b7tes", "Gunst", "will", "uns", "die", "Dicht\u00b7kunst", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Des reinen Lichtes, dessen Morgenr\u00f6ten,", "tokens": ["Des", "rei\u00b7nen", "Lich\u00b7tes", ",", "des\u00b7sen", "Mor\u00b7gen\u00b7r\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In ewig gleicher Sonneneinheit strahlen,", "tokens": ["In", "e\u00b7wig", "glei\u00b7cher", "Son\u00b7nen\u00b7ein\u00b7heit", "strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Befreit von ird'scher D\u00e4mmerung und N\u00f6ten.", "tokens": ["Be\u00b7freit", "von", "ird'\u00b7scher", "D\u00e4m\u00b7me\u00b7rung", "und", "N\u00f6\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Dies Eine war das Ziel von seinen Wahlen,", "tokens": ["Dies", "Ei\u00b7ne", "war", "das", "Ziel", "von", "sei\u00b7nen", "Wah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dies Eine, das die alte Zeit erkannte,", "tokens": ["Dies", "Ei\u00b7ne", ",", "das", "die", "al\u00b7te", "Zeit", "er\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dies Eine wollt' er neu lebendig malen;", "tokens": ["Dies", "Ei\u00b7ne", "wollt'", "er", "neu", "le\u00b7ben\u00b7dig", "ma\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "PPER", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Denn mancher Hohe, den die Welt verkannte", "tokens": ["Denn", "man\u00b7cher", "Ho\u00b7he", ",", "den", "die", "Welt", "ver\u00b7kann\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hatt' es wohl fest, doch also hoch gegr\u00fcndet", "tokens": ["Hatt'", "es", "wohl", "fest", ",", "doch", "al\u00b7so", "hoch", "ge\u00b7gr\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PTKVZ", "$,", "ADV", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df sich geblendet weg die Schwachheit wandte.", "tokens": ["Da\u00df", "sich", "ge\u00b7blen\u00b7det", "weg", "die", "Schwach\u00b7heit", "wand\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVPP", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Der Dichter h\u00e4tt' es allen gern verk\u00fcndet,", "tokens": ["Der", "Dich\u00b7ter", "h\u00e4tt'", "es", "al\u00b7len", "gern", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des P\u00f6bels Wahnsinn ewig ausgerottet,", "tokens": ["Des", "P\u00f6\u00b7bels", "Wahn\u00b7sinn", "e\u00b7wig", "aus\u00b7ge\u00b7rot\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Gottes Klarheit alle ganz verb\u00fcndet;", "tokens": ["In", "Got\u00b7tes", "Klar\u00b7heit", "al\u00b7le", "ganz", "ver\u00b7b\u00fcn\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PIAT", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Drum wird mit bitterm Spotte hier verspottet,", "tokens": ["Drum", "wird", "mit", "bit\u00b7term", "Spot\u00b7te", "hier", "ver\u00b7spot\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was gegen jenes Licht die Sonn' umschwirrend,", "tokens": ["Was", "ge\u00b7gen", "je\u00b7nes", "Licht", "die", "Sonn'", "um\u00b7schwir\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PDAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In dumpfem Sinn sich dumpf zusammenrottet.", "tokens": ["In", "dum\u00b7pfem", "Sinn", "sich", "dumpf", "zu\u00b7sam\u00b7men\u00b7rot\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Es webt und gr\u00e4bt der Geist nur tiefer irrend,", "tokens": ["Es", "webt", "und", "gr\u00e4bt", "der", "Geist", "nur", "tie\u00b7fer", "ir\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So lang er noch im Irdischen beschr\u00e4nket,", "tokens": ["So", "lang", "er", "noch", "im", "Ir\u00b7di\u00b7schen", "be\u00b7schr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Denken auch bedr\u00e4ngt, sich selbst verwirrend;", "tokens": ["Im", "Den\u00b7ken", "auch", "be\u00b7dr\u00e4ngt", ",", "sich", "selbst", "ver\u00b7wir\u00b7rend", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "$,", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Doch wenn er hoffnungslos im Schmerz versenket,", "tokens": ["Doch", "wenn", "er", "hoff\u00b7nungs\u00b7los", "im", "Schmerz", "ver\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat schnell er oft des Friedens Born gefunden,", "tokens": ["Hat", "schnell", "er", "oft", "des", "Frie\u00b7dens", "Born", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "ADV", "ART", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn tief Gef\u00fchl in sich zur Quell' ihn lenket!", "tokens": ["Wenn", "tief", "Ge\u00b7f\u00fchl", "in", "sich", "zur", "Quell'", "ihn", "len\u00b7ket", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "NN", "APPR", "PRF", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Erwacht von Traum ist er mit Gott verbunden,", "tokens": ["Er\u00b7wacht", "von", "Traum", "ist", "er", "mit", "Gott", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vernimmt nicht mehr, wie tr\u00fcber Zweifel h\u00f6hnet,", "tokens": ["Ver\u00b7nimmt", "nicht", "mehr", ",", "wie", "tr\u00fc\u00b7ber", "Zwei\u00b7fel", "h\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "$,", "PWAV", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Kraft und Mut und Licht die Stirn umwunden;", "tokens": ["Von", "Kraft", "und", "Mut", "und", "Licht", "die", "Stirn", "um\u00b7wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Mit andern ist er nun, mit sich vers\u00f6hnet,", "tokens": ["Mit", "an\u00b7dern", "ist", "er", "nun", ",", "mit", "sich", "ver\u00b7s\u00f6h\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ADV", "$,", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Herzens Trieb' und St\u00e4rke neu erstanden,", "tokens": ["Des", "Her\u00b7zens", "Trieb'", "und", "St\u00e4r\u00b7ke", "neu", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In immer h\u00f6herm Licht der Geist versch\u00f6net.", "tokens": ["In", "im\u00b7mer", "h\u00f6\u00b7herm", "Licht", "der", "Geist", "ver\u00b7sch\u00f6\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Selig der Mann, der schauend das verstanden,", "tokens": ["Se\u00b7lig", "der", "Mann", ",", "der", "schau\u00b7end", "das", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "ADJD", "ART", "ADJA", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Welten all' erkannt in jenem Einen,", "tokens": ["Die", "Wel\u00b7ten", "all'", "er\u00b7kannt", "in", "je\u00b7nem", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVPP", "APPR", "PDAT", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich selbst befreit von allen ird'schen Banden!", "tokens": ["Sich", "selbst", "be\u00b7freit", "von", "al\u00b7len", "ird'\u00b7schen", "Ban\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Nun strebt mit Gott den Geist er zu vereinen,", "tokens": ["Nun", "strebt", "mit", "Gott", "den", "Geist", "er", "zu", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn ihr Menschen alle ihn verlachtet,", "tokens": ["Und", "wenn", "ihr", "Men\u00b7schen", "al\u00b7le", "ihn", "ver\u00b7lach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PIS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihn preis' ich einzig selig, anders keinen.", "tokens": ["Ihn", "preis'", "ich", "ein\u00b7zig", "se\u00b7lig", ",", "an\u00b7ders", "kei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADJD", "$,", "ADV", "PIAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Der Denker, Dichter, den wir jetzt betrachtet,", "tokens": ["Der", "Den\u00b7ker", ",", "Dich\u00b7ter", ",", "den", "wir", "jetzt", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat auch mit solchem Bild uns vorgeleuchtet,", "tokens": ["Hat", "auch", "mit", "sol\u00b7chem", "Bild", "uns", "vor\u00b7ge\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Ziel erreicht, wonach die Forschung trachtet.", "tokens": ["Das", "Ziel", "er\u00b7reicht", ",", "wo\u00b7nach", "die", "For\u00b7schung", "trach\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.