iambic.penta"}}, "stanza.40": {"line.1": {"text": "La\u00dft denn das K\u00f6stliche uns sorgsam pflegen,", "tokens": ["La\u00dft", "denn", "das", "K\u00f6st\u00b7li\u00b7che", "uns", "sorg\u00b7sam", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo unbewu\u00dft noch H\u00f6h'res angedeutet,", "tokens": ["Wo", "un\u00b7be\u00b7wu\u00dft", "noch", "H\u00f6h'\u00b7res", "an\u00b7ge\u00b7deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von gr\u00f6\u00dfern Zeiten, ferner Weisheit Wegen,", "tokens": ["Von", "gr\u00f6\u00b7\u00dfern", "Zei\u00b7ten", ",", "fer\u00b7ner", "Weis\u00b7heit", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.41": {"line.1": {"text": "Da Stern und Blum' und Erde Himmel deutet,", "tokens": ["Da", "Stern", "und", "Blum'", "und", "Er\u00b7de", "Him\u00b7mel", "deu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Geist nicht mehr im Kampf mit seinen Zeichen,", "tokens": ["Der", "Geist", "nicht", "mehr", "im", "Kampf", "mit", "sei\u00b7nen", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der ew