{"textgrid.poem.41591": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "Der Albatros.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft f\u00e4ngt die Mannschaft auf den Schiffen zum Vergn\u00fcgen", "tokens": ["Oft", "f\u00e4ngt", "die", "Mann\u00b7schaft", "auf", "den", "Schif\u00b7fen", "zum", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich Albatrosse ein, Seev\u00f6gel k\u00fchnbeschwingt,", "tokens": ["Sich", "Al\u00b7bat\u00b7ros\u00b7se", "ein", ",", "See\u00b7v\u00f6\u00b7gel", "k\u00fchn\u00b7be\u00b7schwingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "NE", "PTKVZ", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+--++-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die still und ruhevoll auf ihren weite Z\u00fcgen", "tokens": ["Die", "still", "und", "ru\u00b7he\u00b7voll", "auf", "ih\u00b7ren", "wei\u00b7te", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Fahrzeug folgen, wie es durch die Salzflut dringt.", "tokens": ["Dem", "Fahr\u00b7zeug", "fol\u00b7gen", ",", "wie", "es", "durch", "die", "Salz\u00b7flut", "dringt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sobald auf das Verdeck sie die Gefangnen bringen,", "tokens": ["So\u00b7bald", "auf", "das", "Ver\u00b7deck", "sie", "die", "Ge\u00b7fang\u00b7nen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00e4ngen voller Scham, verst\u00f6rt und ungeschickt,", "tokens": ["So", "h\u00e4n\u00b7gen", "vol\u00b7ler", "Scham", ",", "ver\u00b7st\u00f6rt", "und", "un\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die K\u00f6n'ge des Azurs die m\u00e4chtgen, wei\u00dfen Schwingen", "tokens": ["Die", "K\u00f6n'\u00b7ge", "des", "A\u00b7zurs", "die", "m\u00e4cht\u00b7gen", ",", "wei\u00b7\u00dfen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie Ruder rechts und links, hinschleifend und geknickt.", "tokens": ["Wie", "Ru\u00b7der", "rechts", "und", "links", ",", "hin\u00b7schlei\u00b7fend", "und", "ge\u00b7knickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "KON", "ADV", "$,", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Wandrer, leicht beschwingt, da\u00df er die Luft durchschweife,", "tokens": ["Der", "Wand\u00b7rer", ",", "leicht", "be\u00b7schwingt", ",", "da\u00df", "er", "die", "Luft", "durch\u00b7schwei