{"textgrid.poem.65151": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schon ist es Abend. Schwermutvoll", "tokens": ["Schon", "ist", "es", "A\u00b7bend", ".", "Schwer\u00b7mut\u00b7voll"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denkt Dugald an das nahe Scheiden,", "tokens": ["Denkt", "Du\u00b7gald", "an", "das", "na\u00b7he", "Schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch mutig, wie der Mann es soll,", "tokens": ["Doch", "mu\u00b7tig", ",", "wie", "der", "Mann", "es", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verhehlt entschlossen er sein Leiden.", "tokens": ["Ver\u00b7hehlt", "ent\u00b7schlos\u00b7sen", "er", "sein", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er birgt sein Weh, mag's auch in's Mark,", "tokens": ["Er", "birgt", "sein", "Weh", ",", "mag's", "auch", "in's", "Mark", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In's tiefste, ihm des Lebens gehen.", "tokens": ["In's", "tiefs\u00b7te", ",", "ihm", "des", "Le\u00b7bens", "ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sieht er die Mutter doch so stark,", "tokens": ["Sieht", "er", "die", "Mut\u00b7ter", "doch", "so", "stark", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die er vorerst so schwach gesehen!", "tokens": ["Die", "er", "vor\u00b7erst", "so", "schwach", "ge\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Hause schafft sie r\u00fcstig, frisch,", "tokens": ["Im", "Hau\u00b7se", "schafft", "sie", "r\u00fcs\u00b7tig", ",", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie's ihre altgewohnte Weise.", "tokens": ["Wie's", "ih\u00b7re", "alt\u00b7ge\u00b7wohn\u00b7te", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann r\u00fcckt zum Herde sie den Tisch", "tokens": ["Dann", "r\u00fcckt", "zum", "Her\u00b7de", "sie", "den", "Tisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar wohl besetzt mit Trank und Speise.", "tokens": ["Gar", "wohl", "be\u00b7setzt", "mit", "Trank", "und", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbsag, Dugald! wei\u00dft du denn noch, was", "tokens": ["\u00bb", "sag", ",", "Du\u00b7gald", "!", "wei\u00dft", "du", "denn", "noch", ",", "was"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "VVFIN", "$,", "NE", "$.", "VVFIN", "PPER", "ADV", "ADV", "$,", "PWS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Brauch erheischt in unserm Lande?\u00ab", "tokens": ["Der", "Brauch", "er\u00b7heischt", "in", "un\u00b7serm", "Lan\u00b7de", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und mit des Whiskys Feuerna\u00df", "tokens": ["Und", "mit", "des", "Whis\u00b7kys", "Feu\u00b7er\u00b7na\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcllt seinen Becher sie zum Rande.", "tokens": ["F\u00fcllt", "sei\u00b7nen", "Be\u00b7cher", "sie", "zum", "Ran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und rasch, mit einem Zuge leert", "tokens": ["Und", "rasch", ",", "mit", "ei\u00b7nem", "Zu\u00b7ge", "leert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er die ihm dargebotne Schale.", "tokens": ["Er", "die", "ihm", "dar\u00b7ge\u00b7bot\u00b7ne", "Scha\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was ist's, das pl\u00f6tzlich ihn durchf\u00e4hrt?", "tokens": ["Was", "ist's", ",", "das", "pl\u00f6tz\u00b7lich", "ihn", "durch\u00b7f\u00e4hrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wird ihm nur mit einem Male?", "tokens": ["Wie", "wird", "ihm", "nur", "mit", "ei\u00b7nem", "Ma\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihm ist, als h\u00e4tte fl\u00fcss'ge Glut", "tokens": ["Ihm", "ist", ",", "als", "h\u00e4t\u00b7te", "fl\u00fcss'\u00b7ge", "Glut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus jenem Becher er getrunken,", "tokens": ["Aus", "je\u00b7nem", "Be\u00b7cher", "er", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als spr\u00fchten, statt der roten Flut,", "tokens": ["Als", "spr\u00fch\u00b7ten", ",", "statt", "der", "ro\u00b7ten", "Flut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "$,", "KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch seine Adern lohe Funken.", "tokens": ["Durch", "sei\u00b7ne", "A\u00b7dern", "lo\u00b7he", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Nebelflor sein Aug' umh\u00fcllt,", "tokens": ["Ein", "Ne\u00b7bel\u00b7flor", "sein", "Aug'", "um\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es schwinden Sinn ihm und Gedanke.", "tokens": ["Es", "schwin\u00b7den", "Sinn", "ihm", "und", "Ge\u00b7dan\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wieder seinen Becher f\u00fcllt", "tokens": ["Und", "wie\u00b7der", "sei\u00b7nen", "Be\u00b7cher", "f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Mutter ihm mit jenem Tranke.", "tokens": ["Die", "Mut\u00b7ter", "ihm", "mit", "je\u00b7nem", "Tran\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Abwehrend weist er ihn zur\u00fcck.", "tokens": ["Ab\u00b7weh\u00b7rend", "weist", "er", "ihn", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbich mu\u00df Euch den Bescheid versagen.\u00ab", "tokens": ["\u00bb", "ich", "mu\u00df", "Euch", "den", "Be\u00b7scheid", "ver\u00b7sa\u00b7gen", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "\u00bbo trink' auf unser k\u00fcnft'ges Gl\u00fcck,", "tokens": ["\u00bb", "o", "trink'", "auf", "un\u00b7ser", "k\u00fcnft'\u00b7ges", "Gl\u00fcck", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf Wiedersehn in bessern Tagen!\u00ab", "tokens": ["Auf", "Wie\u00b7der\u00b7sehn", "in", "bes\u00b7sern", "Ta\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbgilt's dies, dann sei es, wie Ihr wollt!", "tokens": ["\u00bb", "gilt's", "dies", ",", "dann", "sei", "es", ",", "wie", "Ihr", "wollt", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDS", "$,", "ADV", "VAFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich trink' Euch zu von ganzer Seele!\u00ab", "tokens": ["Ich", "trink'", "Euch", "zu", "von", "gan\u00b7zer", "See\u00b7le", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bet\u00e4ubend, \u00fcberw\u00e4ltigend rollt", "tokens": ["Be\u00b7t\u00e4u\u00b7bend", ",", "\u00fc\u00b7berw\u00b7\u00e4l\u00b7ti\u00b7gend", "rollt"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "VVPP", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Feuertrank ihm durch die Kehle.", "tokens": ["Der", "Feu\u00b7er\u00b7trank", "ihm", "durch", "die", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Zustand w\u00fcster Traum ihm d\u00fcnkt,", "tokens": ["Sein", "Zu\u00b7stand", "w\u00fcs\u00b7ter", "Traum", "ihm", "d\u00fcnkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Zittern fliegt durch seine Glieder,", "tokens": ["Ein", "Zit\u00b7tern", "fliegt", "durch", "sei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Erheben will er sich und sinkt", "tokens": ["Er\u00b7he\u00b7ben", "will", "er", "sich", "und", "sinkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PRF", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf seinen Sitz bewu\u00dftlos nieder.", "tokens": ["Auf", "sei\u00b7nen", "Sitz", "be\u00b7wu\u00dft\u00b7los", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das ist nicht Schlaf, der lind und sacht", "tokens": ["Das", "ist", "nicht", "Schlaf", ",", "der", "lind", "und", "sacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PTKNEG", "NN", "$,", "PRELS", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Ruhbed\u00fcrft'ge niedergleitet!", "tokens": ["Auf", "Ruh\u00b7be\u00b7d\u00fcrft'\u00b7ge", "nie\u00b7derg\u00b7lei\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bet\u00e4ubung ist's, wie sie die Macht", "tokens": ["Be\u00b7t\u00e4u\u00b7bung", "ist's", ",", "wie", "sie", "die", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des st\u00e4rksten Opiats bereitet.", "tokens": ["Des", "st\u00e4rks\u00b7ten", "O\u00b7piats", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sein Antlitz bleich und unbelebt,", "tokens": ["Sein", "Ant\u00b7litz", "bleich", "und", "un\u00b7be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von kaltem Schwei\u00df feucht seine Locken!", "tokens": ["Von", "kal\u00b7tem", "Schwei\u00df", "feucht", "sei\u00b7ne", "Lo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie \u00e4ngstlich seine Brust sich hebt,", "tokens": ["Wie", "\u00e4ngst\u00b7lich", "sei\u00b7ne", "Brust", "sich", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie seine Pulse z\u00f6gernd stocken! \u2013", "tokens": ["Wie", "sei\u00b7ne", "Pul\u00b7se", "z\u00f6\u00b7gernd", "sto\u00b7cken", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Zu ihres Sohnes H\u00e4upten kniet", "tokens": ["Zu", "ih\u00b7res", "Soh\u00b7nes", "H\u00e4up\u00b7ten", "kniet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Meg Nora nieder auf den Boden,", "tokens": ["Meg", "No\u00b7ra", "nie\u00b7der", "auf", "den", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie k\u00fc\u00dft sein dunkles Augenlid,", "tokens": ["Sie", "k\u00fc\u00dft", "sein", "dunk\u00b7les", "Au\u00b7gen\u00b7lid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie lauschet seines Mundes Odem.", "tokens": ["Sie", "lau\u00b7schet", "sei\u00b7nes", "Mun\u00b7des", "O\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf ihren Scho\u00df st\u00fctzt sie gelind", "tokens": ["Auf", "ih\u00b7ren", "Scho\u00df", "st\u00fctzt", "sie", "ge\u00b7lind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN