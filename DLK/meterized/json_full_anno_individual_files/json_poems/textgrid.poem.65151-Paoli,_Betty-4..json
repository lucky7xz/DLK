ext": "Erheben will er sich und sinkt", "tokens": ["Er\u00b7he\u00b7ben", "will", "er", "sich", "und", "sinkt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PRF", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf seinen Sitz bewu\u00dftlos nieder.", "tokens": ["Auf", "sei\u00b7nen", "Sitz", "be\u00b7wu\u00dft\u00b7los", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Das ist nicht Schlaf, der lind und sacht", "tokens": ["Das", "ist", "nicht", "Schlaf", ",", "der", "lind", "und", "sacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PTKNEG", "NN", "$,", "PRELS", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Ruhbed\u00fcrft'ge niedergleitet!", "tokens": ["Auf", "Ruh\u00b7be\u00b7d\u00fcrft'\u00b7ge", "nie\u00b7derg\u00b7lei\u00b7tet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bet\u00e4ubung ist's, wie sie die Macht", "tokens": ["Be\u00b7t\u00e4u\u00b7bung", "ist's", ",", "wie", "sie", "die", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des st\u00e4rksten Opiats bereitet.", "tokens": ["Des", "st\u00e4rks\u00b7ten", "O\u00b7piats", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sein Antlitz bleich und unbelebt,", "tokens": ["Sein", "Ant\u00b7litz", "bleich", "und", "un\u00b7be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von kaltem Schwei\u00df feucht seine Locken!", "tokens": ["Von", "kal\u00b7tem", "Schwei\u00df", "feucht", "sei\u00b7ne", "Lo\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie \u00e4ngstlich seine Brust sich hebt,", "tokens": ["Wie", "\u00e4ngst\u00b7lich", "sei\u00b7ne", "Brust", "sich", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie seine Pulse z\u00f6gernd stocken! \u2013", "tokens": ["Wie", "sei\u00b7ne", "Pul\u00b7se", "z\u00f6\u00b7gernd", "sto\u00b7cken", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Zu ihres Sohnes H\u00e4upten kniet", "tokens": ["Zu", "ih\u00b7res", "Soh\u00b7nes", "H\u00e4up\u00b7ten", "kniet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Meg Nora nieder auf den Boden,", "tokens": ["Meg", "No\u00b7ra", "nie\u00b7der", "auf", "den", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie k\u00fc\u00dft sein dunkles Augenlid,", "tokens": ["Sie", "k\u00fc\u00dft", "sein", "dunk\u00b7les", "Au\u00b7gen\u00b7lid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie lauschet seines Mundes Odem.", "tokens": ["Sie", "lau\u00b7schet", "sei\u00b7nes", "Mun\u00b7des", "O\u00b7dem", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf ihren Scho\u00df st\u00fctzt sie gelind", "tokens": ["Auf", "ih\u00b7ren", "Scho\u00df", "st\u00fctzt", "sie", "ge\u00b7lind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein sch\u00f6nes Haupt, das ohnmachtschwache,", "tokens": ["Sein", "sch\u00f6\u00b7nes", "Haupt", ",", "das", "ohn\u00b7macht\u00b7schwa\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und h\u00e4lt bei ihrem lieben Kind", "tokens": ["Und", "h\u00e4lt", "bei", "ih\u00b7rem", "lie\u00b7ben", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Getreu und unverdrossen Wache.", "tokens": ["Ge\u00b7treu", "und", "un\u00b7ver\u00b7dros\u00b7sen", "Wa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}