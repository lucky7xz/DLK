{"textgrid.poem.48826": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "51. Auf seiner Bulschaft Verreisen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieb gedenket weg. Was w\u00fcndsch ich ihr vor Gl\u00fccke?", "tokens": ["Mein", "Lieb", "ge\u00b7den\u00b7ket", "weg", ".", "Was", "w\u00fcndsch", "ich", "ihr", "vor", "Gl\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie meines Gl\u00fcckes Wundsch, mein Gl\u00fccke selbst zeucht hin,", "tokens": ["Sie", "mei\u00b7nes", "Gl\u00fc\u00b7ckes", "Wund\u00b7sch", ",", "mein", "Gl\u00fc\u00b7cke", "selbst", "zeucht", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "$,", "PPOSAT", "NN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "mit ihr auch wird mein Sinn und ganze Seele ziehn,", "tokens": ["mit", "ihr", "auch", "wird", "mein", "Sinn", "und", "gan\u00b7ze", "See\u00b7le", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und ehe nicht sein hier, als bis sie k\u00f6mmt zur\u00fccke.", "tokens": ["und", "e\u00b7he", "nicht", "sein", "hier", ",", "als", "bis", "sie", "k\u00f6mmt", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "VAINF", "ADV", "$,", "KOUS", "APPR", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Tuts, geht, begleitet sie, ihr Seufzer und ihr Blicke.", "tokens": ["Tuts", ",", "geht", ",", "be\u00b7glei\u00b7tet", "sie", ",", "ihr", "Seuf\u00b7zer", "und", "ihr", "Bli\u00b7cke", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sagts, sagts, in was f\u00fcr Angst ich augenblicklich bin,", "tokens": ["Sagts", ",", "sagts", ",", "in", "was", "f\u00fcr", "Angst", "ich", "au\u00b7gen\u00b7blick\u00b7lich", "bin", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "APPR", "PRELS", "APPR", "NN", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so lange sie ist weg. Di\u00df blo\u00df sei mein Gewinn,", "tokens": ["so", "lan\u00b7ge", "sie", "ist", "weg", ".", "Di\u00df", "blo\u00df", "sei", "mein", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VAFIN", "ADV", "$.", "PDS", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wenn sie aufs Ehste sich zu ihrer R\u00fcckkunft schicke.", "tokens": ["wenn", "sie", "aufs", "Ehs\u00b7te", "sich", "zu", "ih\u00b7rer", "R\u00fcck\u00b7kunft", "schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Immittels werd' ich oft vor diesem Fenster stehn,", "tokens": ["Im\u00b7mit\u00b7tels", "werd'", "ich", "oft", "vor", "die\u00b7sem", "Fens\u00b7ter", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "den Weg auch, den sie k\u00f6mmt, oft auf und niedergehn,", "tokens": ["den", "Weg", "auch", ",", "den", "sie", "k\u00f6mmt", ",", "oft", "auf", "und", "nie\u00b7der\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADV", "PTKVZ", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und kranken Sehnens voll nach meiner Sonnen sehen.", "tokens": ["und", "kran\u00b7ken", "Seh\u00b7nens", "voll", "nach", "mei\u00b7ner", "Son\u00b7nen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "K\u00f6mmt sie, sehts Alle denn, es ist mir kein Verdru\u00df,", "tokens": ["K\u00f6mmt", "sie", ",", "sehts", "Al\u00b7le", "denn", ",", "es", "ist", "mir", "kein", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "PIAT", "ADV", "$,", "PPER", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+