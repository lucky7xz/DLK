{"dta.poem.10985": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "N\u00f6thige Erkl\u00e4rung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So oft wir in der Bibel lesen,", "tokens": ["So", "oft", "wir", "in", "der", "Bi\u00b7bel", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Gott, im Busch, mit Mose sprach:", "tokens": ["Da\u00df", "Gott", ",", "im", "Busch", ",", "mit", "Mo\u00b7se", "sprach", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "APPRART", "NN", "$,", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So scheints, denkt man nicht weiter nach,", "tokens": ["So", "scheints", ",", "denkt", "man", "nicht", "wei\u00b7ter", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "PIS", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ob w\u00e4re Gott dort ganz gewesen.", "tokens": ["Ob", "w\u00e4\u00b7re", "Gott", "dort", "ganz", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "NN", "ADV", "ADV", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie von dem Bilde, ja von einem alten Mann,", "tokens": ["Wie", "von", "dem", "Bil\u00b7de", ",", "ja", "von", "ei\u00b7nem", "al\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man wohl nichts bessers denken kann.", "tokens": ["Man", "wohl", "nichts", "bes\u00b7sers", "den\u00b7ken", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PIS", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Allein:", "tokens": ["Al\u00b7lein", ":"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Wie trieget uns ein solcher Schein!", "tokens": ["Wie", "trie\u00b7get", "uns", "ein", "sol\u00b7cher", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Millionen Sonn- und Erden,", "tokens": ["Der", "Mil\u00b7lion\u00b7en", "Sonn", "und", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "TRUNC", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Der aller Himmel Himmel f\u00fcllt,", "tokens": ["Der", "al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Kann durch ein solches winzigs Bild,", "tokens": ["Kann", "durch", "ein", "sol\u00b7ches", "win\u00b7zigs", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit Recht nicht abgebildet werden,", "tokens": ["Mit", "Recht", "nicht", "ab\u00b7ge\u00b7bil\u00b7det", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Jm Busch nicht eingeschlossen seyn.", "tokens": ["Jm", "Busch", "nicht", "ein\u00b7ge\u00b7schlos\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Mit solchen schm\u00e4lernden Jdeen,", "tokens": ["Mit", "sol\u00b7chen", "schm\u00e4\u00b7lern\u00b7den", "Jdeen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Die gro\u00dfe Gottheit anzusehn,", "tokens": ["Die", "gro\u00b7\u00dfe", "Got\u00b7theit", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und was allgegenw\u00e4rtig, sich,", "tokens": ["Und", "was", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ",", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "$,", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "So eng begr\u00e4nzet, vorzustellen,", "tokens": ["So", "eng", "be\u00b7gr\u00e4n\u00b7zet", ",", "vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ist str\u00e4flich, l\u00e4ch- und l\u00e4sterlich.", "tokens": ["Ist", "str\u00e4f\u00b7lich", ",", "l\u00e4ch", "und", "l\u00e4s\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "TRUNC", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Man kann kein ander Urtheil f\u00e4llen,", "tokens": ["Man", "kann", "kein", "an\u00b7der", "Ur\u00b7theil", "f\u00e4l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Wenn wir, mit dem Begriff, es lesen,", "tokens": ["Wenn", "wir", ",", "mit", "dem", "Be\u00b7griff", ",", "es", "le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Als da\u00df die gro\u00dfe Gottheit da,", "tokens": ["Als", "da\u00df", "die", "gro\u00b7\u00dfe", "Got\u00b7theit", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Wie Moses ihn im Busche sah,", "tokens": ["Wie", "Mo\u00b7ses", "ihn", "im", "Bu\u00b7sche", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Nur blo\u00df allein in Asia,", "tokens": ["Nur", "blo\u00df", "al\u00b7lein", "in", "A\u00b7sia", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Und in Europa nicht, gewesen.", "tokens": ["Und", "in", "Eu\u00b7ro\u00b7pa", "nicht", ",", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "NE", "PTKNEG", "$,", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Du sprichst: Wer wird doch so gedenken?", "tokens": ["Du", "sprichst", ":", "Wer", "wird", "doch", "so", "ge\u00b7den\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VAFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Ich sprech: Ein jeder, dem das Bild,", "tokens": ["Ich", "sprech", ":", "Ein", "je\u00b7der", ",", "dem", "das", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "PIS", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Vom alten Mann, sein Hirn erf\u00fcllt,", "tokens": ["Vom", "al\u00b7ten", "Mann", ",", "sein", "Hirn", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wird Gott im Busche ganz verschrenken.", "tokens": ["Wird", "Gott", "im", "Bu\u00b7sche", "ganz", "ver\u00b7schren\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Man mu\u00df dahero, Gott zu ehren,", "tokens": ["Man", "mu\u00df", "da\u00b7he\u00b7ro", ",", "Gott", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PAV", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Die Stelle mit Vernunft erkl\u00e4ren.", "tokens": ["Die", "Stel\u00b7le", "mit", "Ver\u00b7nunft", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}