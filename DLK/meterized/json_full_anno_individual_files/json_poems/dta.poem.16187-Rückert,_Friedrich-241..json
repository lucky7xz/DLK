{"dta.poem.16187": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "241.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gar manche glauben, sprach ein Weiser wohlbeflissen,", "tokens": ["Gar", "man\u00b7che", "glau\u00b7ben", ",", "sprach", "ein", "Wei\u00b7ser", "wohl\u00b7be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "$,", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht minder ihrem Wahn, als andre ihrem Wissen.", "tokens": ["Nicht", "min\u00b7der", "ih\u00b7rem", "Wahn", ",", "als", "and\u00b7re", "ih\u00b7rem", "Wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPOSAT", "NN", "$,", "KOUS", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das eben ist der Wahn, der was zu wissen meint,", "tokens": ["Das", "e\u00b7ben", "ist", "der", "Wahn", ",", "der", "was", "zu", "wis\u00b7sen", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da wahres Wissen sich unwissend immer scheint.", "tokens": ["Da", "wah\u00b7res", "Wis\u00b7sen", "sich", "un\u00b7wis\u00b7send", "im\u00b7mer", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PRF", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum w\u00e4chst das Wissen, das nie gnug zu wissen glaubt,", "tokens": ["Drum", "w\u00e4chst", "das", "Wis\u00b7sen", ",", "das", "nie", "gnug", "zu", "wis\u00b7sen", "glaubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Fortschritts aber hat der Wahn sich selbst beraubt.", "tokens": ["Des", "Fort\u00b7schritts", "a\u00b7ber", "hat", "der", "Wahn", "sich", "selbst", "be\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}