{"textgrid.poem.49278": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "20. Auff da\u00df Creutz des Herrn", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist dieses hier das Holtz, damit wir Christen prangen,", "tokens": ["Ist", "die\u00b7ses", "hier", "das", "Holtz", ",", "da\u00b7mit", "wir", "Chris\u00b7ten", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach dem der S\u00fcnden Last ist von uns abgethan?", "tokens": ["Nach", "dem", "der", "S\u00fcn\u00b7den", "Last", "ist", "von", "uns", "ab\u00b7ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NE", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist di\u00df das Holtz, an dem der rechte Pelican", "tokens": ["Ist", "di\u00df", "das", "Holtz", ",", "an", "dem", "der", "rech\u00b7te", "Pe\u00b7li\u00b7can"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Uns lebendig gemacht, da als die List der Schlangen", "tokens": ["Uns", "le\u00b7ben\u00b7dig", "ge\u00b7macht", ",", "da", "als", "die", "List", "der", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVPP", "$,", "KOUS", "KOKOM", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Uns s\u00e4mptlich hat erw\u00fcrgt? Ist dieser hier gehangen,", "tokens": ["Uns", "s\u00e4mpt\u00b7lich", "hat", "er\u00b7w\u00fcrgt", "?", "Ist", "die\u00b7ser", "hier", "ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAFIN", "VVPP", "$.", "VAFIN", "PDS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Himmel, Erde, Lufft und Meer regieren kan,", "tokens": ["Der", "Him\u00b7mel", ",", "Er\u00b7de", ",", "Lufft", "und", "Meer", "re\u00b7gie\u00b7ren", "kan", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der wieder Hell' und Todt schwingt seine Siegesfahn", "tokens": ["Der", "wie\u00b7der", "Hell'", "und", "Todt", "schwingt", "sei\u00b7ne", "Sie\u00b7ges\u00b7fahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und den, der uns vor hielt, jetzt selber helt gefangen?", "tokens": ["Und", "den", ",", "der", "uns", "vor", "hielt", ",", "jetzt", "sel\u00b7ber", "helt", "ge\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PPER", "APPR", "VVFIN", "$,", "ADV", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ists hier, wo Gottes Zorn und Grimm verloschen sind", "tokens": ["Ists", "hier", ",", "wo", "Got\u00b7tes", "Zorn", "und", "Grimm", "ver\u00b7lo\u00b7schen", "sind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PWAV", "NN", "NN", "KON", "NE", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und wo er seine Lieb' hat wieder angez\u00fcndt?", "tokens": ["Und", "wo", "er", "sei\u00b7ne", "Lieb'", "hat", "wie\u00b7der", "an\u00b7ge\u00b7z\u00fcndt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Trugst du die edle Last? hat dich das Land genetzet,", "tokens": ["Trugst", "du", "die", "ed\u00b7le", "Last", "?", "hat", "dich", "das", "Land", "ge\u00b7net\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das alle S\u00fcnden tr\u00e4gt? O grosse Wunderthat!", "tokens": ["Das", "al\u00b7le", "S\u00fcn\u00b7den", "tr\u00e4gt", "?", "O", "gros\u00b7se", "Wun\u00b7der\u00b7that", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Geheiligt ist der Wald, der dich getragen hat!", "tokens": ["Ge\u00b7hei\u00b7ligt", "ist", "der", "Wald", ",", "der", "dich", "ge\u00b7tra\u00b7gen", "hat", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Noch heiliger der Berg, auff den man dich gesetzet.", "tokens": ["Noch", "hei\u00b7li\u00b7ger", "der", "Berg", ",", "auff", "den", "man", "dich", "ge\u00b7set\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ART", "NN", "$,", "APPR", "ART", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist dieses hier das Holtz, damit wir Christen prangen,", "tokens": ["Ist", "die\u00b7ses", "hier", "das", "Holtz", ",", "da\u00b7mit", "wir", "Chris\u00b7ten", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach dem der S\u00fcnden Last ist von uns abgethan?", "tokens": ["Nach", "dem", "der", "S\u00fcn\u00b7den", "Last", "ist", "von", "uns", "ab\u00b7ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NE", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist di\u00df das Holtz, an dem der rechte Pelican", "tokens": ["Ist", "di\u00df", "das", "Holtz", ",", "an", "dem", "der", "rech\u00b7te", "Pe\u00b7li\u00b7can"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "APPR", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Uns lebendig gemacht, da als die List der Schlangen", "tokens": ["Uns", "le\u00b7ben\u00b7dig", "ge\u00b7macht", ",", "da", "als", "die", "List", 