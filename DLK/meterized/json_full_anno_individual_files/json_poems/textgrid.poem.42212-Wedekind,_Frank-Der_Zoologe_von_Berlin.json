{"textgrid.poem.42212": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Der Zoologe von Berlin", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt ihr Kinder, wie es j\u00fcngst ergangen", "tokens": ["H\u00f6rt", "ihr", "Kin\u00b7der", ",", "wie", "es", "j\u00fcngst", "er\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einem Zoologen in Berlin!", "tokens": ["Ei\u00b7nem", "Zoo\u00b7lo\u00b7gen", "in", "Ber\u00b7lin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Pl\u00f6tzlich f\u00fchrt ein Schutzmann ihn gefangen", "tokens": ["Pl\u00f6tz\u00b7lich", "f\u00fchrt", "ein", "Schutz\u00b7mann", "ihn", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Vor den Untersuchungsrichter hin.", "tokens": ["Vor", "den", "Un\u00b7ter\u00b7su\u00b7chungs\u00b7rich\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Dieser tritt ihm kr\u00e4ftig auf die Zehen,", "tokens": ["Die\u00b7ser", "tritt", "ihm", "kr\u00e4f\u00b7tig", "auf", "die", "Ze\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nimmt ihn hochnotpeinlich ins Gebet", "tokens": ["Nimmt", "ihn", "hoch\u00b7not\u00b7pein\u00b7lich", "ins", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Und empfiehlt ihm, schlankweg zu gestehen,", "tokens": ["Und", "emp\u00b7fiehlt", "ihm", ",", "schlank\u00b7weg", "zu", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "--+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Da\u00df beleidigt er die Majest\u00e4t.", "tokens": ["Da\u00df", "be\u00b7lei\u00b7digt", "er", "die", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dieser sprach: \u00bbHerr Richter, ungeheuer", "tokens": ["Die\u00b7ser", "sprach", ":", "\u00bb", "Herr", "Rich\u00b7ter", ",", "un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "VVFIN", "$.", "$(", "NN", "NN", "$,", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist die Schuld, die man mir unterlegt;", "tokens": ["Ist", "die", "Schuld", ",", "die", "man", "mir", "un\u00b7ter\u00b7legt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn da\u00df eine Kuh ein Wiederk\u00e4uer,", "tokens": ["Denn", "da\u00df", "ei\u00b7ne", "Kuh", "ein", "Wie\u00b7der\u00b7k\u00e4u\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Hat noch nirgends \u00c4rgernis erregt.", "tokens": ["Hat", "noch", "nir\u00b7gends", "\u00c4r\u00b7ger\u00b7nis", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Soweit ist die Wissenschaft gediehen,", "tokens": ["So\u00b7weit", "ist", "die", "Wis\u00b7sen\u00b7schaft", "ge\u00b7die\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da\u00df es l\u00e4ngst in Kinderb\u00fcchern steht.", "tokens": ["Da\u00df", "es", "l\u00e4ngst", "in", "Kin\u00b7der\u00b7b\u00fc\u00b7chern", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Dann beleidigen ", "tokens": ["Dann", "be\u00b7lei\u00b7di\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Vor der Majest\u00e4t, das kann ich schw\u00f6ren,", "tokens": ["Vor", "der", "Ma\u00b7jes\u00b7t\u00e4t", ",", "das", "kann", "ich", "schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hegt ich stets den schuldigsten Respekt;", "tokens": ["Hegt", "ich", "stets", "den", "schul\u00b7digs\u00b7ten", "Res\u00b7pekt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ja, es freut mich oft sogar zu h\u00f6ren,", "tokens": ["Ja", ",", "es", "freut", "mich", "oft", "so\u00b7gar", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn man den Beleidiger entdeckt;", "tokens": ["Wenn", "man", "den", "Be\u00b7lei\u00b7di\u00b7ger", "ent\u00b7deckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Denn dann wird die Majest\u00e4t erst sehen,", "tokens": ["Denn", "dann", "wird", "die", "Ma\u00b7jes\u00b7t\u00e4t", "erst", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ob sie majest\u00e4tisch nach Geb\u00fchr.", "tokens": ["Ob", "sie", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "nach", "Ge\u00b7b\u00fchr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Deshalb ist ein Mops, das bleibt bestehen,", "tokens": ["Des\u00b7halb", "ist", "ein", "Mops", ",", "das", "bleibt", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "$,", "PDS", "VVFIN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zweifelsohne doch ein S\u00e4ugetier.", "tokens": ["Zwei\u00b7fel\u00b7soh\u00b7ne", "doch", "ein", "S\u00e4u\u00b7ge\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ebenso hab vor den Staatsgewalten", "tokens": ["E\u00b7ben\u00b7so", "hab", "vor", "den", "Staats\u00b7ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ich mich vorschriftsm\u00e4\u00dfig stets geduckt,", "tokens": ["Ich", "mich", "vor\u00b7schrifts\u00b7m\u00e4\u00b7\u00dfig", "stets", "ge\u00b7duckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auf Kommando oft das Maul gehalten", "tokens": ["Auf", "Kom\u00b7man\u00b7do", "oft", "das", "Maul", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und vor Anarchisten ausgespuckt.", "tokens": ["Und", "vor", "An\u00b7ar\u00b7chis\u00b7ten", "aus\u00b7ge\u00b7spuckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Auch wo Spitzel horchen in Vereinen,", "tokens": ["Auch", "wo", "Spit\u00b7zel", "hor\u00b7chen", "in", "Ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Sprach ich immer harmlos wie ein Kind.", "tokens": ["Sprach", "ich", "im\u00b7mer", "harm\u00b7los", "wie", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aber deshalb kann ich von den Schweinen", "tokens": ["A\u00b7ber", "des\u00b7halb", "kann", "ich", "von", "den", "Schwei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Doch nicht sagen, da\u00df es Menschen sind.", "tokens": ["Doch", "nicht", "sa\u00b7gen", ",", "da\u00df", "es", "Men\u00b7schen", "sind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Viel Respekt hab ich vor dir, o Richter,", "tokens": ["Viel", "Res\u00b7pekt", "hab", "ich", "vor", "dir", ",", "o", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "APPR", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unbegrenzten menschlichen Respekt!", "tokens": ["Un\u00b7be\u00b7grenz\u00b7ten", "menschli\u00b7chen", "Res\u00b7pekt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "L\u00e4\u00dft du doch die \u00e4rgsten B\u00f6sewichter", "tokens": ["L\u00e4\u00dft", "du", "doch", "die", "\u00e4rgs\u00b7ten", "B\u00f6\u00b7se\u00b7wich\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "In Berlin gew\u00f6hnlich unentdeckt.", "tokens": ["In", "Ber\u00b7lin", "ge\u00b7w\u00f6hn\u00b7lich", "un\u00b7ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "ADJD", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Doch wenn hochzurufen ich mich sehne", "tokens": ["Doch", "wenn", "hoch\u00b7zu\u00b7ru\u00b7fen", "ich", "mich", "seh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "PPER", "PRF", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Von dem Schwarzwald bis nach Kiautschau,", "tokens": ["Von", "dem", "Schwarz\u00b7wald", "bis", "nach", "Ki\u00b7aut\u00b7schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPR", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Bleibt deshalb gestreift nicht die Hy\u00e4ne?", "tokens": ["Bleibt", "des\u00b7halb", "ge\u00b7streift", "nicht", "die", "Hy\u00b7\u00e4\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Nicht ein sch\u00f6nes Federvieh der Pfau?\u00ab", "tokens": ["Nicht", "ein", "sch\u00f6\u00b7nes", "Fe\u00b7der\u00b7vieh", "der", "Pfau", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Also war das Wort des Zoologen,", "tokens": ["Al\u00b7so", "war", "das", "Wort", "des", "Zoo\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Doch dann sprach der hohe Staatsanwalt;", "tokens": ["Doch", "dann", "sprach", "der", "ho\u00b7he", "Staats\u00b7an\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und nachdem man alles wohl erwogen,", "tokens": ["Und", "nach\u00b7dem", "man", "al\u00b7les", "wohl", "er\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ward der Mann zu einem Jahr verknallt.", "tokens": ["Ward", "der", "Mann", "zu", "ei\u00b7nem", "Jahr", "ver\u00b7knallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Deshalb vor Zoologie-Studieren", "tokens": ["Des\u00b7halb", "vor", "Zoo\u00b7lo\u00b7gie\u00b7Stu\u00b7die\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PAV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00fcte sich ein jeder, wenn er jung;", "tokens": ["H\u00fc\u00b7te", "sich", "ein", "je\u00b7der", ",", "wenn", "er", "jung", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "PIS", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Denn es schlummert in den meisten Tieren", "tokens": ["Denn", "es", "schlum\u00b7mert", "in", "den", "meis\u00b7ten", "Tie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Eine Majest\u00e4tsbeleidigung.", "tokens": ["Ei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4ts\u00b7be\u00b7lei\u00b7di\u00b7gung", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "H\u00f6rt ihr Kinder, wie es j\u00fcngst ergangen", "tokens": ["H\u00f6rt", "ihr", "Kin\u00b7der", ",", "wie", "es", "j\u00fcngst", "er\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einem Zoologen in Berlin!", "tokens": ["Ei\u00b7nem", "Zoo\u00b7lo\u00b7gen", "in", "Ber\u00b7lin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Pl\u00f6tzlich f\u00fchrt ein Schutzmann ihn gefangen", "tokens": ["Pl\u00f6tz\u00b7lich", "f\u00fchrt", "ein", "Schutz\u00b7mann", "ihn", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Vor den Untersuchungsrichter hin.", "tokens": ["Vor", "den", "Un\u00b7ter\u00b7su\u00b7chungs\u00b7rich\u00b7ter", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Dieser tritt ihm kr\u00e4ftig auf die Zehen,", "tokens": ["Die\u00b7ser", "tritt", "ihm", "kr\u00e4f\u00b7tig", "auf", "die", "Ze\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word",