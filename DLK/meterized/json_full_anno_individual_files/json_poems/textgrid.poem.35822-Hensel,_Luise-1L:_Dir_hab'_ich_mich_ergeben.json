{"textgrid.poem.35822": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir hab' ich mich ergeben", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir hab' ich mich ergeben", "tokens": ["Dir", "hab'", "ich", "mich", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Allem, was ich bin;", "tokens": ["Mit", "Al\u00b7lem", ",", "was", "ich", "bin", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So nimm mein ganzes Leben,", "tokens": ["So", "nimm", "mein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Heiland! nimm es hin.", "tokens": ["Mein", "Hei\u00b7land", "!", "nimm", "es", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dich einzig will ich lieben,", "tokens": ["Dich", "ein\u00b7zig", "will", "ich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein einzig will ich sein.", "tokens": ["Dein", "ein\u00b7zig", "will", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nichts ist f\u00fcr mich geblieben,", "tokens": ["Nichts", "ist", "f\u00fcr", "mich", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ist Alles Dein, nur Dein.", "tokens": ["Ist", "Al\u00b7les", "Dein", ",", "nur", "Dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "$,", "ADV", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So mu\u00df ich immer sprechen,", "tokens": ["So", "mu\u00df", "ich", "im\u00b7mer", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn Sein mein Herz gedenkt;", "tokens": ["Wenn", "Sein", "mein", "Herz", "ge\u00b7denkt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will das Wort nicht brechen,", "tokens": ["Ich", "will", "das", "Wort", "nicht", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ich Ihm einst geschenkt,", "tokens": ["Das", "ich", "Ihm", "einst", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch flie\u00dfen meine Thr\u00e4nen", "tokens": ["Doch", "flie\u00b7\u00dfen", "mei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Oft, wenn ich einsam bin,", "tokens": ["Oft", ",", "wenn", "ich", "ein\u00b7sam", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und zieht mich schmerzlich Sehnen,", "tokens": ["Und", "zieht", "mich", "schmerz\u00b7lich", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ich wei\u00df es nicht, wohin.", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "wo\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Freund! o Herr! wie lange,", "tokens": ["O", "Freund", "!", "o", "Herr", "!", "wie", "lan\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "FM", "NN", "$.", "PWAV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie lange harre ich!", "tokens": ["Wie", "lan\u00b7ge", "har\u00b7re", "ich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O komm, mir wird so bange,", "tokens": ["O", "komm", ",", "mir", "wird", "so", "ban\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm und erl\u00f6se mich! \u2013", "tokens": ["Komm", "und", "er\u00b7l\u00f6\u00b7se", "mich", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Herz aus meinem Herzen,", "tokens": ["Das", "Herz", "aus", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich gab es treu Dir hin,", "tokens": ["Ich", "gab", "es", "treu", "Dir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df Du in Lieb' und Schmerzen", "tokens": ["Da\u00df", "Du", "in", "Lieb'", "und", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mich m\u00f6chtest zu Dir ziehn.", "tokens": ["Mich", "m\u00f6ch\u00b7test", "zu", "Dir", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich habe nichts behalten,", "tokens": ["Ich", "ha\u00b7be", "nichts", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ich im Busen trug,", "tokens": ["Was", "ich", "im", "Bu\u00b7sen", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lie\u00df das Herz erkalten,", "tokens": ["Und", "lie\u00df", "das", "Herz", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das z\u00e4rtlich f\u00fcr mich schlug.", "tokens": ["Das", "z\u00e4rt\u00b7lich", "f\u00fcr", "mich", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch nun im fremden Lande,", "tokens": ["Doch", "nun", "im", "frem\u00b7den", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der trauten Heimath fern,", "tokens": ["Der", "trau\u00b7ten", "Hei\u00b7math", "fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie sind so los' die Bande", "tokens": ["Wie", "sind", "so", "los'", "die", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des liebevollsten Herrn!", "tokens": ["Des", "lie\u00b7be\u00b7volls\u00b7ten", "Herrn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gest\u00fctzt auf Seine Treue", "tokens": ["Ge\u00b7st\u00fctzt", "auf", "Sei\u00b7ne", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich das Werk gethan,", "tokens": ["Hab'", "ich", "das", "Werk", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie, nun fiele Reue", "tokens": ["Und", "wie", ",", "nun", "fie\u00b7le", "Reu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "$,", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die schwache Seele an?", "tokens": ["Die", "schwa\u00b7che", "See\u00b7le", "an", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jetzt kehrte sich mein Sehnen", "tokens": ["Jetzt", "kehr\u00b7te", "sich", "mein", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zur eiteln Welt zur\u00fcck?", "tokens": ["Zur", "ei\u00b7teln", "Welt", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nun fl\u00f6ssen meine Thr\u00e4nen", "tokens": ["Nun", "fl\u00f6s\u00b7sen", "mei\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Erde kurzem Gl\u00fcck?", "tokens": ["Der", "Er\u00b7de", "kur\u00b7zem", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O nein, ein rein'res Leben,", "tokens": ["O", "nein", ",", "ein", "rein'\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein h\u00f6h'res Vaterland", "tokens": ["Ein", "h\u00f6h'\u00b7res", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Liebe wird Er geben,", "tokens": ["Und", "Lie\u00b7be", "wird", "Er", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem sich mein Herz verband.", "tokens": ["Dem", "sich", "mein", "Herz", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm, s\u00fc\u00dfer Freund! und mehre", "tokens": ["Komm", ",", "s\u00fc\u00b7\u00dfer", "Freund", "!", "und", "meh\u00b7re"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "KON", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Gluth der Lieb' in mir,", "tokens": ["Die", "Gluth", "der", "Lieb'", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df sich mein Herz verzehre", "tokens": ["Da\u00df", "sich", "mein", "Herz", "ver\u00b7zeh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In flammender Begier!", "tokens": ["In", "flam\u00b7men\u00b7der", "Be\u00b7gier", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und Alles, was mich kettet,", "tokens": ["Und", "Al\u00b7les", ",", "was", "mich", "ket\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sei dieser Flamme Raub,", "tokens": ["Sei", "die\u00b7ser", "Flam\u00b7me", "Raub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bis sich die Seele rettet", "tokens": ["Bis", "sich", "die", "See\u00b7le", "ret\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Nacht und Erdenstaub,", "tokens": ["Aus", "Nacht", "und", "Er\u00b7dens\u00b7taub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bis sich zum ew'gen Gl\u00fccke", "tokens": ["Bis", "sich", "zum", "ew'\u00b7gen", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRF", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gereint die Seele schwingt,", "tokens": ["Ge\u00b7reint", "die", "See\u00b7le", "schwingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und still der Leib zur\u00fccke", "tokens": ["Und", "still", "der", "Leib", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In Todesschatten sinkt.", "tokens": ["In", "To\u00b7des\u00b7schat\u00b7ten", "sinkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dir hab' ich mich ergeben", "tokens": ["Dir", "hab'", "ich", "mich", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Allem, was ich bin;", "tokens": ["Mit", "Al\u00b7lem", ",", "was", "ich", "bin", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So nimm mein ganzes Leben,", "tokens": ["So", "nimm", "mein", "gan\u00b7zes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Heiland! nimm es hin.", "tokens": ["Mein", "Hei\u00b7land", "!", "nimm", "es", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dich einzig will ich lieben,", "tokens": ["Dich", "ein\u00b7zig", "will", "ich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dein einzig will ich sein.", "tokens": ["Dein", "ein\u00b7zig", "will", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nichts ist f\u00fcr mich geblieben,", "tokens": ["Nichts", "ist", "f\u00fcr", "mich", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", 