ord"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Liebe wird Er geben,", "tokens": ["Und", "Lie\u00b7be", "wird", "Er", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem sich mein Herz verband.", "tokens": ["Dem", "sich", "mein", "Herz", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm, s\u00fc\u00dfer Freund! und mehre", "tokens": ["Komm", ",", "s\u00fc\u00b7\u00dfer", "Freund", "!", "und", "meh\u00b7re"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "KON", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Gluth der Lieb' in mir,", "tokens": ["Die", "Gluth", "der", "Lieb'", "in", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df sich mein Herz verzehre", "tokens": ["Da\u00df", "sich", "mein", "Herz", "ver\u00b7zeh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In flammender Begier!", "tokens": ["In", "flam\u00b7men\u00b7der", "Be\u00b7gier", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und Alles, was mich kettet,", "tokens": ["Und", "Al\u00b7les", ",", "was", "mich", "ket\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON