{"textgrid.poem.61282": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun wandelt von den Bergen sacht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wandelt von den Bergen sacht", "tokens": ["Nun", "wan\u00b7delt", "von", "den", "Ber\u00b7gen", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum See herab die Sommernacht,", "tokens": ["Zum", "See", "her\u00b7ab", "die", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und tr\u00e4umerisch mit hei\u00dfem Sinn", "tokens": ["Und", "tr\u00e4u\u00b7me\u00b7risch", "mit", "hei\u00b7\u00dfem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch ihre Schatten schreit' ich hin.", "tokens": ["Durch", "ih\u00b7re", "Schat\u00b7ten", "schreit'", "ich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Berauschend schwimmt im Strom der Luft", "tokens": ["Be\u00b7rau\u00b7schend", "schwimmt", "im", "Strom", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Daher der Rebenbl\u00fcte Duft,", "tokens": ["Da\u00b7her", "der", "Re\u00b7ben\u00b7bl\u00fc\u00b7te", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Gl\u00fchwurm webt die lichte Bahn", "tokens": ["Der", "Gl\u00fch\u00b7wurm", "webt", "die", "lich\u00b7te", "Bahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Im Dunkel an des Turms Gem\u00e4uer,", "tokens": ["Im", "Dun\u00b7kel", "an", "des", "Turms", "Ge\u00b7m\u00e4u\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und droben gl\u00fchn mit tiefem Feuer", "tokens": ["Und", "dro\u00b7ben", "gl\u00fchn", "mit", "tie\u00b7fem", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Sterne r\u00e4tselhaft mich an.", "tokens": ["Die", "Ster\u00b7ne", "r\u00e4t\u00b7sel\u00b7haft", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies ist die Stunde, da das Lied", "tokens": ["Dies", "ist", "die", "Stun\u00b7de", ",", "da", "das", "Lied"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sehnsucht durch die L\u00fcfte zieht,", "tokens": ["Der", "Sehn\u00b7sucht", "durch", "die", "L\u00fcf\u00b7te", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die tief in Wald, Gestein und Flur", "tokens": ["Die", "tief", "in", "Wald", ",", "Ge\u00b7stein", "und", "Flur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kern ist aller Kreatur:", "tokens": ["Der", "Kern", "ist", "al\u00b7ler", "Kre\u00b7a\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sehnsucht, die durch Felsen dicht", "tokens": ["Der", "Sehn\u00b7sucht", ",", "die", "durch", "Fel\u00b7sen", "dicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Quell emporzwingt an das Licht,", "tokens": ["Den", "Quell", "em\u00b7por\u00b7zwingt", "an", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die nach dem Himmel aus dem Wald", "tokens": ["Die", "nach", "dem", "Him\u00b7mel", "aus", "dem", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit tausend gr\u00fcnen Armen greift,", "tokens": ["Mit", "tau\u00b7send", "gr\u00fc\u00b7nen", "Ar\u00b7men", "greift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Aus hartem Stein als Echo hallt,", "tokens": ["Aus", "har\u00b7tem", "Stein", "als", "E\u00b7cho", "hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Im irren Wind die Welt umschweift,", "tokens": ["Im", "ir\u00b7ren", "Wind", "die", "Welt", "um\u00b7schweift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die aus der Nachtigallen Kehle", "tokens": ["Die", "aus", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Im Silberton hinperlend quillt", "tokens": ["Im", "Sil\u00b7ber\u00b7ton", "hin\u00b7per\u00b7lend", "quillt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und aus der Blumen Auge mild", "tokens": ["Und", "aus", "der", "Blu\u00b7men", "Au\u00b7ge", "mild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Dich anschaut mit der stummen Seele.", "tokens": ["Dich", "an\u00b7schaut", "mit", "der", "stum\u00b7men", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Sehnsucht, die du wie ein Kind,", "tokens": ["O", "Sehn\u00b7sucht", ",", "die", "du", "wie", "ein", "Kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Schlaf gelullt durch s\u00fc\u00dfe Lieder,", "tokens": ["In", "Schlaf", "ge\u00b7lullt", "durch", "s\u00fc\u00b7\u00dfe", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch stets aufs neu' erwachst und wieder", "tokens": ["Doch", "stets", "aufs", "neu'", "er\u00b7wachst", "und", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "VVFIN", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu weinen anhebst leis und lind,", "tokens": ["Zu", "wei\u00b7nen", "an\u00b7hebst", "leis", "und", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie nimmst du heut mir Herz und Sinn", "tokens": ["Wie", "nimmst", "du", "heut", "mir", "Herz", "und", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit deiner Klage ganz dahin!", "tokens": ["Mit", "dei\u00b7ner", "Kla\u00b7ge", "ganz", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mir ist's, ich m\u00fc\u00dfte Fl\u00fcgel heben", "tokens": ["Mir", "ist's", ",", "ich", "m\u00fc\u00df\u00b7te", "Fl\u00fc\u00b7gel", "he\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VMFIN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und k\u00f6rperlos ins Weite schweben,", "tokens": ["Und", "k\u00f6r\u00b7per\u00b7los", "ins", "Wei\u00b7te", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Verschenken m\u00fc\u00dft' ich wonniglich", "tokens": ["Ver\u00b7schen\u00b7ken", "m\u00fc\u00dft'", "ich", "won\u00b7nig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mein bestes Sein, mein tiefstes Ich;", "tokens": ["Mein", "bes\u00b7tes", "Sein", ",", "mein", "tiefs\u00b7tes", "Ich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Den ganzen Schatz der vollen Brust,", "tokens": ["Den", "gan\u00b7zen", "Schatz", "der", "vol\u00b7len", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Andacht und Liebe, Schmerz und Lust,", "tokens": ["An\u00b7dacht", "und", "Lie\u00b7be", ",", "Schmerz", "und", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der innersten Gedanken Hort,", "tokens": ["Der", "in\u00b7ners\u00b7ten", "Ge\u00b7dan\u00b7ken", "Hort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Ich m\u00fc\u00dft' ihn in ein einzig Wort", "tokens": ["Ich", "m\u00fc\u00dft'", "ihn", "in", "ein", "ein\u00b7zig", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Als wie in g\u00fcldnen Kelch beschlie\u00dfen,", "tokens": ["Als", "wie", "in", "g\u00fcld\u00b7nen", "Kelch", "be\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Um ihn verschwendrisch hinzugie\u00dfen.", "tokens": ["Um", "ihn", "ver\u00b7schwend\u00b7risch", "hin\u00b7zu\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Umsonst! Kein Wort, sei's noch so gro\u00df,", "tokens": ["Um\u00b7sonst", "!", "Kein", "Wort", ",", "sei's", "noch", "so", "gro\u00df", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PIAT", "NN", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht dich des tiefen Dranges los,", "tokens": ["Macht", "dich", "des", "tie\u00b7fen", "Dran\u00b7ges", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den hei\u00dfen Durst der Seele stillt", "tokens": ["Den", "hei\u00b7\u00dfen", "Durst", "der", "See\u00b7le", "stillt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Brunnen, der auf Erden quillt.", "tokens": ["Kein", "Brun\u00b7nen", ",", "der", "auf", "Er\u00b7den", "quillt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl w\u00e4hnt' ich einst in goldnen Stunden,", "tokens": ["Wohl", "w\u00e4hnt'", "ich", "einst", "in", "gold\u00b7nen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In meines Herzens Maienzeit,", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "Mai\u00b7en\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des R\u00e4tsels L\u00f6sung sei gefunden,", "tokens": ["Des", "R\u00e4t\u00b7sels", "L\u00f6\u00b7sung", "sei", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Minne heile jedes Leid;", "tokens": ["Und", "Min\u00b7ne", "hei\u00b7le", "je\u00b7des", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch was so hoch mir war, so lieb,", "tokens": ["Doch", "was", "so", "hoch", "mir", "war", ",", "so", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "PPER", "VAFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mir ward es - und die Sehnsucht ", "tokens": ["Mir", "ward", "es", "und", "die", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$(", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Darum zur Ruh', mein wild Gem\u00fct!", "tokens": ["Da\u00b7rum", "zur", "Ruh'", ",", "mein", "wild", "Ge\u00b7m\u00fct", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "APPRART", "NN", "$,", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht alles wird hier Frucht, was bl\u00fcht;", "tokens": ["Nicht", "al\u00b7les", "wird", "hier", "Frucht", ",", "was", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VAFIN", "ADV", "NN", "$,", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du tr\u00e4gst, der Erde stummer Gast,", "tokens": ["Du", "tr\u00e4gst", ",", "der", "Er\u00b7de", "stum\u00b7mer", "Gast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dir, was nur der Himmel fa\u00dft.", "tokens": ["In", "dir", ",", "was", "nur", "der", "Him\u00b7mel", "fa\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was f\u00fcr und f\u00fcr so ruhelos", "tokens": ["Was", "f\u00fcr", "und", "f\u00fcr", "so", "ru\u00b7he\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word"], 