{"dta.poem.17006": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "107.  Ein unbeflekter Mensch ist \u00fcber  \n die Engel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Engel seyn ist viel; Noch mehr ein Mensch auf", "tokens": ["Ein", "En\u00b7gel", "seyn", "ist", "viel", ";", "Noch", "mehr", "ein", "Mensch", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAINF", "VAFIN", "ADV", "$.", "ADV", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erden/", "tokens": ["Er\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und nicht mit jhrem wust und Koth besudelt werden.", "tokens": ["Und", "nicht", "mit", "jhrem", "wust", "und", "Koth", "be\u00b7su\u00b7delt", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPOSAT", "NN", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}