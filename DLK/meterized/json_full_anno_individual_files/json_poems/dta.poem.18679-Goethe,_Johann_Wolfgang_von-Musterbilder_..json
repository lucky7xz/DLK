{"dta.poem.18679": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Musterbilder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r\u2019 und bewahre", "tokens": ["H\u00f6r'", "und", "be\u00b7wah\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sechs Liebespaare.", "tokens": ["Sechs", "Lie\u00b7be\u00b7spaa\u00b7re", "."], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wortbild entz\u00fcndet, Liebe sch\u00fcrt zu:", "tokens": ["Wort\u00b7bild", "ent\u00b7z\u00fcn\u00b7det", ",", "Lie\u00b7be", "sch\u00fcrt", "zu", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "VVFIN", "PTKVZ", "$."], "meter": "---+-+---", "measure": "unknown.measure.di"}, "line.4": {"text": "Rustan und Rodawu.", "tokens": ["Rus\u00b7tan", "und", "Ro\u00b7da\u00b7wu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Unbekannte sind sich nah:", "tokens": ["Un\u00b7be\u00b7kann\u00b7te", "sind", "sich", "nah", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jussuph und Suleika.", "tokens": ["Jus\u00b7suph", "und", "Sul\u00b7ei\u00b7ka", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Liebe nicht Liebesgewinn:", "tokens": ["Lie\u00b7be", "nicht", "Lie\u00b7bes\u00b7ge\u00b7winn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Ferhad und Schirin.", "tokens": ["Fer\u00b7had", "und", "Schi\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Nur f\u00fcr einander da:", "tokens": ["Nur", "f\u00fcr", "ein\u00b7an\u00b7der", "da", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Medschnun und Leila.", "tokens": ["Medsc\u00b7hnun", "und", "Lei\u00b7la", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Liebend im Alter sah", "tokens": ["Lie\u00b7bend", "im", "Al\u00b7ter", "sah"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Dschemil auf Boteinah.", "tokens": ["Dsche\u00b7mil", "auf", "Bo\u00b7tei\u00b7nah", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.13": {"text": "S\u00fcsse Liebeslaune,", "tokens": ["S\u00fcs\u00b7se", "Lie\u00b7bes\u00b7lau\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Salomo und die Braune!", "tokens": ["Sa\u00b7lo\u00b7mo", "und", "die", "Brau\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.15": {"text": "Hast du sie wohl vermerkt,", "tokens": ["Hast", "du", "sie", "wohl", "ver\u00b7merkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Bist im Lieben gest\u00e4rkt.", "tokens": ["Bist", "im", "Lie\u00b7ben", "ge\u00b7st\u00e4rkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}