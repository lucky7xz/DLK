{"textgrid.poem.48706": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "7. Auf Herrn Tobias H\u00fcbners Ableben", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der deutsche Helikon hatt' einen schwarzen Flor", "tokens": ["Der", "deut\u00b7sche", "He\u00b7li\u00b7kon", "hatt'", "ei\u00b7nen", "schwar\u00b7zen", "Flor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "um sich gezogen her, die tr\u00fcbe Hippokrene", "tokens": ["um", "sich", "ge\u00b7zo\u00b7gen", "her", ",", "die", "tr\u00fc\u00b7be", "Hip\u00b7po\u00b7kre\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "PRF", "VVPP", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "flo\u00df durch den jungen Mo\u00df ein seufzendes Get\u00f6ne.", "tokens": ["flo\u00df", "durch", "den", "jun\u00b7gen", "Mo\u00df", "ein", "seuf\u00b7zen\u00b7des", "Ge\u00b7t\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Melpomene, die blie\u00df in ihr betr\u00fcbtes Rohr,", "tokens": ["Mel\u00b7po\u00b7me\u00b7ne", ",", "die", "blie\u00df", "in", "ihr", "be\u00b7tr\u00fcb\u00b7tes", "Rohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"te