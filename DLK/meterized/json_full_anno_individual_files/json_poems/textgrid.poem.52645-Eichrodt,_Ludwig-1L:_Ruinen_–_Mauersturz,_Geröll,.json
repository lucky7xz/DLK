{"textgrid.poem.52645": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ruinen \u2013 Mauersturz, Ger\u00f6ll,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ruinen \u2013 Mauersturz, Ger\u00f6ll,", "tokens": ["Ru\u00b7i\u00b7nen", "\u2013", "Mau\u00b7er\u00b7sturz", ",", "Ge\u00b7r\u00f6ll", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Getr\u00fcmmer, Schutt und Raub;", "tokens": ["Ge\u00b7tr\u00fcm\u00b7mer", ",", "Schutt", "und", "Raub", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch allw\u00e4rts spinnt der bl\u00fchende Dorn,", "tokens": ["Doch", "all\u00b7w\u00e4rts", "spinnt", "der", "bl\u00fc\u00b7hen\u00b7de", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und fr\u00f6hlich spro\u00dft das Laub.", "tokens": ["Und", "fr\u00f6h\u00b7lich", "spro\u00dft", "das", "Laub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es klafft der Fels ins weite Thal", "tokens": ["Es", "klafft", "der", "Fels", "ins", "wei\u00b7te", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hinab, hoch ist der Berg,", "tokens": ["Hin\u00b7ab", ",", "hoch", "ist", "der", "Berg", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die riesigen Tannen \u00fcberkr\u00f6nt", "tokens": ["Die", "rie\u00b7si\u00b7gen", "Tan\u00b7nen", "\u00fc\u00b7ber\u00b7kr\u00f6nt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das alte Menschenwerk.", "tokens": ["Das", "al\u00b7te", "Men\u00b7schen\u00b7werk", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dort sitzt ein runzlig K\u00f6hlerweib", "tokens": ["Dort", "sitzt", "ein", "runz\u00b7lig", "K\u00f6h\u00b7ler\u00b7weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Strauch, dort ist ein Haus,", "tokens": ["Am", "Strauch", ",", "dort", "ist", "ein", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die stolzen Steine der Herrenburg", "tokens": ["Die", "stol\u00b7zen", "Stei\u00b7ne", "der", "Her\u00b7ren\u00b7burg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie halfen der Nothdurft aus.", "tokens": ["Sie", "hal\u00b7fen", "der", "Noth\u00b7durft", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Das M\u00fctterlein ist ganz vergn\u00fcgt,", "tokens": ["Das", "M\u00fct\u00b7ter\u00b7lein", "ist", "ganz", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel ist ihr auch blau,", "tokens": ["Der", "Him\u00b7mel", "ist", "ihr", "auch", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Sommer hat bunt mit Blumen gef\u00fcllt", "tokens": ["Der", "Som\u00b7mer", "hat", "bunt", "mit", "Blu\u00b7men", "ge\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den Garten der alten Frau.", "tokens": ["Den", "Gar\u00b7ten", "der", "al\u00b7ten", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Da wuchert es wild von Rosmarin,", "tokens": ["Da", "wu\u00b7chert", "es", "wild", "von", "Ros\u00b7ma\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von Nelken und Hagebutt,", "tokens": ["Von", "Nel\u00b7ken", "und", "Ha\u00b7ge\u00b7butt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gro\u00dfk\u00f6pfige Sonnenblumen genug", "tokens": ["Gro\u00df\u00b7k\u00f6p\u00b7fi\u00b7ge", "Son\u00b7nen\u00b7blu\u00b7men", "ge\u00b7nug"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wiegen sich \u00fcber dem Schutt.", "tokens": ["Wie\u00b7gen", "sich", "\u00fc\u00b7ber", "dem", "Schutt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Wies gr\u00fcnt und bl\u00fcht, wies farbig spricht!", "tokens": ["Wies", "gr\u00fcnt", "und", "bl\u00fcht", ",", "wies", "far\u00b7big", "spricht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welch \u00fcppige Blumengluth!", "tokens": ["Welch", "\u00fcp\u00b7pi\u00b7ge", "Blu\u00b7men\u00b7gluth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein guter Boden ist Graben und Wall", "tokens": ["Ein", "gu\u00b7ter", "Bo\u00b7den", "ist", "Gra\u00b7ben", "und", "Wall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "KON", "NE"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u2013 Ged\u00fcngt mit Menschenblut.", "tokens": ["\u2013", "Ge\u00b7d\u00fcngt", "mit", "Men\u00b7schen\u00b7blut", "."], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Da ist nicht Ordnung, und ist kein Pfad,", "tokens": ["Da", "ist", "nicht", "Ord\u00b7nung", ",", "und", "ist", "kein", "Pfad", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "NN", "$,", "KON", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Zum sanften Spazierengehn,", "tokens": ["Zum", "sanf\u00b7ten", "Spa\u00b7zie\u00b7ren\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Unkraut bl\u00fcht gl\u00fcckselig mit,", "tokens": ["Das", "Un\u00b7kraut", "bl\u00fcht", "gl\u00fcck\u00b7se\u00b7lig", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man kann es wachsen sehn.", "tokens": ["Man", "kann", "es", "wach\u00b7sen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du gutes Weib! verk\u00fcmmern soll", "tokens": ["Du", "gu\u00b7tes", "Weib", "!", "ver\u00b7k\u00fcm\u00b7mern", "soll"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In deiner Sch\u00f6pfung kein Trieb.", "tokens": ["In", "dei\u00b7ner", "Sch\u00f6p\u00b7fung", "kein", "Trieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Kinder des warmen Sommers sind", "tokens": ["Die", "Kin\u00b7der", "des", "war\u00b7men", "Som\u00b7mers", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dir alle sch\u00f6n und lieb!", "tokens": ["Dir", "al\u00b7le", "sch\u00f6n", "und", "lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O paradiesischer Wohlgeruch!", "tokens": ["O", "pa\u00b7ra\u00b7die\u00b7si\u00b7scher", "Wohl\u00b7ge\u00b7ruch", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Krafts\u00fc\u00dfer Honigduft,", "tokens": ["Kraft\u00b7s\u00fc\u00b7\u00dfer", "Ho\u00b7nig\u00b7duft", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie labst du mich, wie lieb ich dich", "tokens": ["Wie", "labst", "du", "mich", ",", "wie", "lieb", "ich", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "PWAV", "ADJD", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In der himmelreinen Luft!", "tokens": ["In", "der", "him\u00b7mel\u00b7rei\u00b7nen", "Luft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Sonne scheint. Doch Alles still.", "tokens": ["Die", "Son\u00b7ne", "scheint", ".", "Doch", "Al\u00b7les", "still", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Nachtigallenschlag.", "tokens": ["Kein", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schlag", "."], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur wispernd leise fliegt ein Traum", "tokens": ["Nur", "wis\u00b7pernd", "lei\u00b7se", "fliegt", "ein", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hin\u00fcber den Bl\u00fcthenhag.", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "den", "Bl\u00fc\u00b7then\u00b7hag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Was ist das? Welcher r\u00fchrende Reiz", "tokens": ["Was", "ist", "das", "?", "Wel\u00b7cher", "r\u00fch\u00b7ren\u00b7de", "Reiz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "$.", "PWAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Durchweht den einsamen Ort?", "tokens": ["Durch\u00b7weht", "den", "ein\u00b7sa\u00b7men", "Ort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In einem M\u00e4hrchen wandle ich \u2013", "tokens": ["In", "ei\u00b7nem", "M\u00e4hr\u00b7chen", "wand\u00b7le", "ich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist eine Fee die dort?", "tokens": ["Ist", "ei\u00b7ne", "Fee", "die", "dort", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ruinen \u2013 Mauersturz, Ger\u00f6ll,", "tokens": ["Ru\u00b7i\u00b7nen", "\u2013", "Mau\u00b7er\u00b7sturz", ",", "Ge\u