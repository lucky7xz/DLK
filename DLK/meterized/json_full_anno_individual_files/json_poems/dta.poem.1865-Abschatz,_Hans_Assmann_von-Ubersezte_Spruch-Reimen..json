{"dta.poem.1865": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht die Mauren/ nicht die W\u00e4nde/", "tokens": ["Nicht", "die", "Mau\u00b7ren", "/", "nicht", "die", "W\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$(", "PTKNEG", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sondern tapffre Menschen-H\u00e4nde/", "tokens": ["Son\u00b7dern", "tapf\u00b7fre", "Men\u00b7schen\u00b7H\u00e4n\u00b7de", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind die beste Gegenwehr/", "tokens": ["Sind", "die", "bes\u00b7te", "Ge\u00b7gen\u00b7wehr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist das Schlo\u00df von diesem leer/", "tokens": ["Ist", "das", "Schlo\u00df", "von", "die\u00b7sem", "leer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PDAT", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df es sonder Widerstreben", "tokens": ["Mu\u00df", "es", "son\u00b7der", "Wi\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich in kurtzer Zeit ergeben.", "tokens": ["Sich", "in", "kurt\u00b7zer", "Zeit", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}