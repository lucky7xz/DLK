{"dta.poem.20488": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Climene/ pr\u00fcfe fleisch und blut/", "tokens": ["Cli\u00b7me\u00b7ne", "/", "pr\u00fc\u00b7fe", "fleisch", "und", "blut", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "ADJD", "KON", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und straffe meine liebes-glut/", "tokens": ["Und", "straf\u00b7fe", "mei\u00b7ne", "lie\u00b7bes\u00b7glut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht nach der schw\u00e4che deiner flammen;", "tokens": ["Nicht", "nach", "der", "schw\u00e4\u00b7che", "dei\u00b7ner", "flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein feuer k\u00f6mmt aus adams-schoo\u00df/", "tokens": ["Mein", "feu\u00b7er", "k\u00f6mmt", "aus", "a\u00b7dams\u00b7schoo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Darein der himmel selber flo\u00df;", "tokens": ["Da\u00b7rein", "der", "him\u00b7mel", "sel\u00b7ber", "flo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie kan dein menschlich hertz doch meine glut verdammen.", "tokens": ["Wie", "kan", "dein", "menschlich", "hertz", "doch", "mei\u00b7ne", "glut", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "ADJD", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Du bist/ wie Eva/ fleisch und bein/", "tokens": ["Du", "bist", "/", "wie", "E\u00b7va", "/", "fleisch", "und", "bein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "KOKOM", "NN", "$(", "ADJD", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum kanst du auch kein engel seyn/", "tokens": ["Drum", "kanst", "du", "auch", "kein", "en\u00b7gel", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PIAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ausser menschen dich verlieben/", "tokens": ["Und", "aus\u00b7ser", "men\u00b7schen", "dich", "ver\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und das gesetze der natur/", "tokens": ["Und", "das", "ge\u00b7set\u00b7ze", "der", "na\u00b7tur", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das mit dem athem in uns fuhr/", "tokens": ["Das", "mit", "dem", "at\u00b7hem", "in", "uns", "fuhr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat auch in deine brust: Seyd fruchtbar; eingeschrieben.", "tokens": ["Hat", "auch", "in", "dei\u00b7ne", "brust", ":", "Seyd", "frucht\u00b7bar", ";", "ein\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$.", "VAIMP", "ADJD", "$.", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer sich in stiller glut verbrennt/", "tokens": ["Wer", "sich", "in", "stil\u00b7ler", "glut", "ver\u00b7brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und menschen-liebe s\u00fcnde nennt/", "tokens": ["Und", "men\u00b7schen\u00b7lie\u00b7be", "s\u00fcn\u00b7de", "nennt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "VVFIN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Mu\u00df auch das paradie\u00df verdammen;", "tokens": ["Mu\u00df", "auch", "das", "pa\u00b7ra\u00b7die\u00df", "ver\u00b7dam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Evens weisse marmol-haut", "tokens": ["Denn", "E\u00b7vens", "weis\u00b7se", "mar\u00b7mol\u00b7haut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "War kaum aus knochen auffgebaut/", "tokens": ["War", "kaum", "aus", "kno\u00b7chen", "auff\u00b7ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So f\u00fchlte Adams-hertz schon s\u00fcsse liebes-flammen.", "tokens": ["So", "f\u00fchl\u00b7te", "A\u00b7dams\u00b7hertz", "schon", "s\u00fcs\u00b7se", "lie\u00b7bes\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Climene/ drum bedencke dich/", "tokens": ["Cli\u00b7me\u00b7ne", "/", "drum", "be\u00b7den\u00b7cke", "dich", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PAV", "VVFIN", "PPER", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du kanst hier ohne dornen-stich", "tokens": ["Du", "kanst", "hier", "oh\u00b7ne", "dor\u00b7nen\u00b7stich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die sch\u00f6nsten zucker-rosen brechen.", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "zu\u00b7cke\u00b7rro\u00b7sen", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein mensch mu\u00df wie die \u00e4rtzte seyn/", "tokens": ["Ein", "mensch", "mu\u00df", "wie", "die", "\u00e4rtz\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "KOKOM", "ART", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und eher nicht von todes-pein/", "tokens": ["Und", "e\u00b7her", "nicht", "von", "to\u00b7des\u00b7pein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als von der lebens-krafft der starcken vele sprechen.", "tokens": ["Als", "von", "der", "le\u00b7bens\u00b7krafft", "der", "star\u00b7cken", "ve\u00b7le", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-----+-+-+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Schau/ meine seele schmeltzt in mir/", "tokens": ["Schau", "/", "mei\u00b7ne", "see\u00b7le", "schmeltzt", "in", "mir", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und alle glieder folgen dir/", "tokens": ["Und", "al\u00b7le", "glie\u00b7der", "fol\u00b7gen", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleich wie die blumen ihrer sonnen;", "tokens": ["Gleich", "wie", "die", "blu\u00b7men", "ih\u00b7rer", "son\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bist du mir nun mein sonnen-schein/", "tokens": ["Bist", "du", "mir", "nun", "mein", "son\u00b7nen\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So la\u00df mich deine blume seyn/", "tokens": ["So", "la\u00df", "mich", "dei\u00b7ne", "blu\u00b7me", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bi\u00df meine wurtzel grund in deinem schoo\u00df gewonnen.", "tokens": ["Bi\u00df", "mei\u00b7ne", "wurt\u00b7zel", "grund", "in", "dei\u00b7nem", "schoo\u00df", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}