{"textgrid.poem.54546": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm Pfand der Eintracht komm/ die grimmen V\u00f6lcker w\u00fctten/", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm Pfand der Eintracht komm/ die grimmen V\u00f6lcker w\u00fctten/", "tokens": ["Komm", "Pfand", "der", "Ein\u00b7tracht", "komm", "/", "die", "grim\u00b7men", "V\u00f6l\u00b7cker", "w\u00fct\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In rasend vollem Zanck/ die Kirche wird getrennt/", "tokens": ["In", "ra\u00b7send", "vol\u00b7lem", "Zanck", "/", "die", "Kir\u00b7che", "wird", "ge\u00b7trennt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und zwar von dieser Schaar/ die sich von Christo nennt/", "tokens": ["Und", "zwar", "von", "die\u00b7ser", "Schaar", "/", "die", "sich", "von", "Chris\u00b7to", "nennt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PDAT", "NN", "$(", "PRELS", "PRF", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du findest nichts allhier/ als ungebundne Sitten/", "tokens": ["Du", "fin\u00b7dest", "nichts", "all\u00b7hier", "/", "als", "un\u00b7ge\u00b7bund\u00b7ne", "Sit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "w