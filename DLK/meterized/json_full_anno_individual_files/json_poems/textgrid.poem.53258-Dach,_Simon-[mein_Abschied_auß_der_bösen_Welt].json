 "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wo jhr die S\u00fcnde werdet fliehn", "tokens": ["Wo", "jhr", "die", "S\u00fcn\u00b7de", "wer\u00b7det", "fliehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vnd Christo euch ergeben.", "tokens": ["Vnd", "Chris\u00b7to", "euch", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Denn das gew\u00fcnschte Himmel-gut", "tokens": ["Denn", "das", "ge\u00b7w\u00fcnschte", "Him\u00b7mel\u00b7gut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ererben alle Frommen,", "tokens": ["Er\u00b7er\u00b7ben", "al\u00b7le", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Christum angenommen,", "tokens": ["Die", "Chris\u00b7tum", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die hie sich gr\u00fcnden auff sein Blut,", "tokens": ["Die", "hie", "sich", "gr\u00fcn\u00b7den", "auff", "sein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In seiner Furcht sich \u00fcben", "tokens": ["In", "sei\u00b7ner", "Furcht", "sich", "\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measu