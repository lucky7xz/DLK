{"textgrid.poem.53258": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[mein Abschied au\u00df der b\u00f6sen Welt]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Abschied au\u00df der b\u00f6sen Welt", "tokens": ["Mein", "Ab\u00b7schied", "au\u00df", "der", "b\u00f6\u00b7sen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd au\u00df den schweren Banden", "tokens": ["Vnd", "au\u00df", "den", "schwe\u00b7ren", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist nun einmal vorhanden,", "tokens": ["Ist", "nun", "ein\u00b7mal", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bin dem Tode vorgestellt,", "tokens": ["Ich", "bin", "dem", "To\u00b7de", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd mu\u00df das Reich zu erben", "tokens": ["Vnd", "mu\u00df", "das", "Reich", "zu", "er\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gleich wie ein Opffer sterben.", "tokens": ["Gleich", "wie", "ein", "Opf\u00b7fer", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich habe ritterlich gek\u00e4mpfft", "tokens": ["Ich", "ha\u00b7be", "rit\u00b7ter\u00b7lich", "ge\u00b7k\u00e4mpfft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vnd meinen Lauff vollendet,", "tokens": ["Vnd", "mei\u00b7nen", "Lauff", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der Feinde w\u00fctten ist gedempfft,", "tokens": ["Der", "Fein\u00b7de", "w\u00fct\u00b7ten", "ist", "ge\u00b7dem\u00b7pfft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Vnd alle Noht geendet.", "tokens": ["Vnd", "al\u00b7le", "Noht", "ge\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In diesem Lauff vnd harten Streit", "tokens": ["In", "die\u00b7sem", "Lauff", "vnd", "har\u00b7ten", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat mir der Feind den Glauben", "tokens": ["Hat", "mir", "der", "Feind", "den", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dennoch nicht k\u00f6nnen rauben.", "tokens": ["Den\u00b7noch", "nicht", "k\u00f6n\u00b7nen", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Krone der Gerechtigkeit,", "tokens": ["Die", "Kro\u00b7ne", "der", "Ge\u00b7rech\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die jenes Leben heget,", "tokens": ["Die", "je\u00b7nes", "Le\u00b7ben", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist schon mir beygeleget,", "tokens": ["Ist", "schon", "mir", "bey\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Got, der im letzten Welt-gericht", "tokens": ["Got", ",", "der", "im", "letz\u00b7ten", "Welt\u00b7ge\u00b7richt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Das Richter-Ampt wird f\u00fchren,", "tokens": ["Das", "Rich\u00b7ter\u00b7Ampt", "wird", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wird selbst mich in dem wahren Liecht", "tokens": ["Wird", "selbst", "mich", "in", "dem", "wah\u00b7ren", "Liecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mit solcher Krone zieren.", "tokens": ["Mit", "sol\u00b7cher", "Kro\u00b7ne", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drumb, meine Liebsten, lasset ab", "tokens": ["Drumb", ",", "mei\u00b7ne", "Liebs\u00b7ten", ",", "las\u00b7set", "ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PAV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel j\u00e4mmerliches klagen", "tokens": ["Viel", "j\u00e4m\u00b7mer\u00b7li\u00b7ches", "kla\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vmb meinen Tod zu tragen,", "tokens": ["Vmb", "mei\u00b7nen", "Tod", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Di\u00df Sterben, dieses finstre Grab", "tokens": ["Di\u00df", "Ster\u00b7ben", ",", "die\u00b7ses", "finst\u00b7re", "Grab"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "$,", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist mir au\u00df allem leiden", "tokens": ["Ist", "mir", "au\u00df", "al\u00b7lem", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Richtsteig zu den frewden.", "tokens": ["Der", "Richt\u00b7steig", "zu", "den", "frew\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ihr m\u00fcsset auch von hinnen ziehn,", "tokens": ["Ihr", "m\u00fcs\u00b7set", "auch", "von", "hin\u00b7nen", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch bleibet euch das Leben,", "tokens": ["Doch", "blei\u00b7bet", "euch", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wo jhr die S\u00fcnde werdet fliehn", "tokens": ["Wo", "jhr", "die", "S\u00fcn\u00b7de", "wer\u00b7det", "fliehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vnd Christo euch ergeben.", "tokens": ["Vnd", "Chris\u00b7to", "euch", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn das gew\u00fcnschte Himmel-gut", "tokens": ["Denn", "das", "ge\u00b7w\u00fcnschte", "Him\u00b7mel\u00b7gut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ererben alle Frommen,", "tokens": ["Er\u00b7er\u00b7ben", "al\u00b7le", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Christum angenommen,", "tokens": ["Die", "Chris\u00b7tum", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die hie sich gr\u00fcnden auff sein Blut,", "tokens": ["Die", "hie", "sich", "gr\u00fcn\u00b7den", "auff", "sein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "w