{"dta.poem.2700": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bey Beerdigung Hn. H. F. den.  11.  \n Julii 1677.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bist du/ Seeliger/ nun deiner Qual entbunden?", "tokens": ["So", "bist", "du", "/", "See\u00b7li\u00b7ger", "/", "nun", "dei\u00b7ner", "Qual", "ent\u00b7bun\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "NN", "$(", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So macht ein sanffter Tod dich von den Fesseln lo\u00df?", "tokens": ["So", "macht", "ein", "sanff\u00b7ter", "Tod", "dich", "von", "den", "Fes\u00b7seln", "lo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach so viel rauhem Sturm hast du den Port gefunden", "tokens": ["Nach", "so", "viel", "rau\u00b7hem", "Sturm", "hast", "du", "den", "Port", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "VAFIN", "PPER", "ART", "NE", "VVPP"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und gehst mit Seegen in der alten Mutter Scho\u00df.", "tokens": ["Und", "gehst", "mit", "See\u00b7gen", "in", "der", "al\u00b7ten", "Mut\u00b7ter", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach! abgelebter ", "tokens": ["Ach", "!", "ab\u00b7ge\u00b7leb\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$.", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Wenn auf die Folter-Banck die strenge Gicht dich warff?", "tokens": ["Wenn", "auf", "die", "Fol\u00b7ter\u00b7Banck", "die", "stren\u00b7ge", "Gicht", "dich", "warff", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn ihre Tyranney die Glieder dir durchschnitten/", "tokens": ["Wenn", "ih\u00b7re", "Ty\u00b7ran\u00b7ney", "die", "Glie\u00b7der", "dir", "durch\u00b7schnit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn ihr zerreissend Weh mehr als ein Messer scharff.", "tokens": ["Wenn", "ihr", "zer\u00b7reis\u00b7send", "Weh", "mehr", "als", "ein", "Mes\u00b7ser", "scharff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "NN", "PIAT", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das Alter an sich selbst war eine Last zu tragen/", "tokens": ["Das", "Al\u00b7ter", "an", "sich", "selbst", "war", "ei\u00b7ne", "Last", "zu", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ADV", "VAFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als das ein Sammel-Platz der bittern Schmertzen hie\u00df/", "tokens": ["Als", "das", "ein", "Sam\u00b7mel\u00b7Platz", "der", "bit\u00b7tern", "Schmert\u00b7zen", "hie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und was versp\u00fcrt man mehr bey den beschneyten Tagen", "tokens": ["Und", "was", "ver\u00b7sp\u00fcrt", "man", "mehr", "bey", "den", "be\u00b7schney\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PIS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Als st\u00fcndiich neue Noth/ Angst/ Eckel und Verdrie\u00df?", "tokens": ["Als", "st\u00fcn\u00b7diich", "neu\u00b7e", "Noth", "/", "Angst", "/", "E\u00b7ckel", "und", "Ver\u00b7drie\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJA", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Du bist/ ", "tokens": ["Du", "bist", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$("], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Nein/ wie viel Jahre hat der Tod dich nicht besucht?", "tokens": ["Nein", "/", "wie", "viel", "Jah\u00b7re", "hat", "der", "Tod", "dich", "nicht", "be\u00b7sucht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KOKOM", "PIAT", "NN", "VAFIN", "ART", "NN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da schon die Lebens-Kr\u00e4fft/ und Geister abgewichen", "tokens": ["Da", "schon", "die", "Le\u00b7bens\u00b7Kr\u00e4fft", "/", "und", "Geis\u00b7ter", "ab\u00b7ge\u00b7wi\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$(", "KON", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ja selbst die Seele stand als wie auff schneller Flucht.", "tokens": ["Ja", "selbst", "die", "See\u00b7le", "stand", "als", "wie", "auff", "schnel\u00b7ler", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVFIN", "KOUS", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie sah\u2019 ihr Wohnhau\u00df da mit seinen S\u00e4ulen sincken/", "tokens": ["Sie", "sah'", "ihr", "Wohn\u00b7hau\u00df", "da", "mit", "sei\u00b7nen", "S\u00e4u\u00b7len", "sin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Eymer war zulechst/ die R\u00e4der morsch entzwey/", "tokens": ["Der", "Ey\u00b7mer", "war", "zu\u00b7lechst", "/", "die", "R\u00e4\u00b7der", "morsch", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Es wolte nicht das Licht mehr durch die Fenster blincken", "tokens": ["Es", "wol\u00b7te", "nicht", "das", "Licht", "mehr", "durch", "die", "Fens\u00b7ter", "blin\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die Sprache zog farm Ohr als wie ein Gast vorbey.", "tokens": ["Die", "Spra\u00b7che", "zog", "farm", "Ohr", "als", "wie", "ein", "Gast", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "KOUS", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach was elender N\u00e4cht\u2019 hast du nicht da empfunden", "tokens": ["Ach", "was", "e\u00b7len\u00b7der", "N\u00e4cht'", "hast", "du", "nicht", "da", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PWS", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Und mit dem Hiob dir gew\u00fcnscht erl\u00f6st zu seyn?", "tokens": ["Und", "mit", "dem", "Hiob", "dir", "ge\u00b7w\u00fcnscht", "er\u00b7l\u00f6st", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NE", "PPER", "ADJD", "VVPP", "PTKZU", "VAINF", "$."], "meter": "---+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "War\u2019st du nicht an dein Bett als einen Pfahl gebunden?", "tokens": ["Wa\u00b7r'st", "du", "nicht", "an", "dein", "Bett", "als", "ei\u00b7nen", "Pfahl", "ge\u00b7bun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "+---+-+-+-+-+-", "measure": "dactylic.init"}, "line.24": {"text": "Und mit dem Morgen-Liecht erschien auch neue Pein?", "tokens": ["Und", "mit", "dem", "Mor\u00b7gen\u00b7Liecht", "er\u00b7schien", "auch", "neu\u00b7e", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der Martern Grausamkeit und aller Hencker reissen/", "tokens": ["Der", "Mar\u00b7tern", "Grau\u00b7sam\u00b7keit", "und", "al\u00b7ler", "Hen\u00b7cker", "reis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was Eccelin erfand/ und Nero hat erdacht/", "tokens": ["Was", "E\u00b7cce\u00b7lin", "er\u00b7fand", "/", "und", "Ne\u00b7ro", "hat", "er\u00b7dacht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$(", "KON", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sind nur ein Kinderspiel f\u00fcr deiner Gicht zu heissen.", "tokens": ["Sind", "nur", "ein", "Kin\u00b7der\u00b7spiel", "f\u00fcr", "dei\u00b7ner", "Gicht", "zu", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die dich noch lebenden Gerippen gleich gemacht.", "tokens": ["Die", "dich", "noch", "le\u00b7ben\u00b7den", "Ge\u00b7rip\u00b7pen", "gleich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.29": {"text": "Dein Lager war gewi\u00df von nichts als Dorn und Hecken/", "tokens": ["Dein", "La\u00b7ger", "war", "ge\u00b7wi\u00df", "von", "nichts", "als", "Dorn", "und", "He\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPR", "PIS", "KOKOM", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein Nothstall/ den Japan nicht schlimmer zeigen kan:", "tokens": ["Ein", "Noth\u00b7stall", "/", "den", "Ja\u00b7pan", "nicht", "schlim\u00b7mer", "zei\u00b7gen", "kan", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Noch liest du keine Qual so deinen Geist erschrecken", "tokens": ["Noch", "liest", "du", "kei\u00b7ne", "Qual", "so", "dei\u00b7nen", "Geist", "er\u00b7schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df er nicht Glaubens-voll sich schwunge Himmel an.", "tokens": ["Da\u00df", "er", "nicht", "Glau\u00b7bens\u00b7voll", "sich", "schwun\u00b7ge", "Him\u00b7mel", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "PRF", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Je mehr der Leib erlag/ je hurtiger die Sinnen", "tokens": ["Je", "mehr", "der", "Leib", "er\u00b7lag", "/", "je", "hur\u00b7ti\u00b7ger", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$(", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mit feurigem Gebet den h\u00f6chsten GOtt vers\u00f6hnt.", "tokens": ["Mit", "feu\u00b7ri\u00b7gem", "Ge\u00b7bet", "den", "h\u00f6chs\u00b7ten", "Gott", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du liest die Ungedult nicht Oberhand gewinnen", "tokens": ["Du", "liest", "die", "Un\u00b7ge\u00b7dult", "nicht", "O\u00b7ber\u00b7hand", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und rufftest: Wer wol k\u00e4mpfft/ der wird zuletzt gekr\u00f6nt.", "tokens": ["Und", "ruff\u00b7test", ":", "Wer", "wol", "k\u00e4mpfft", "/", "der", "wird", "zu\u00b7letzt", "ge\u00b7kr\u00f6nt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWS", "ADV", "VVFIN", "$(", "ART", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nun ist dein Feind erlegt/ dein Leiden hat ein Ende/", "tokens": ["Nun", "ist", "dein", "Feind", "er\u00b7legt", "/", "dein", "Lei\u00b7den", "hat", "ein", "En\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$(", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dein wol geplagter Leib geneust der s\u00fcssen Ruh.", "tokens": ["Dein", "wol", "ge\u00b7plag\u00b7ter", "Leib", "ge\u00b7neust", "der", "s\u00fcs\u00b7sen", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Du bist auch so begl\u00fcckt/ da\u00df dir der Kinder H\u00e4nde", "tokens": ["Du", "bist", "auch", "so", "be\u00b7gl\u00fcckt", "/", "da\u00df", "dir", "der", "Kin\u00b7der", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$(", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "In heisser Liebes-Pflicht die Augen dr\u00fccken zu.", "tokens": ["In", "heis\u00b7ser", "Lie\u00b7bes\u00b7Pflicht", "die", "Au\u00b7gen", "dr\u00fc\u00b7cken", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wem wird wol als wie dir von GOtt das Heil bescheret", "tokens": ["Wem", "wird", "wol", "als", "wie", "dir", "von", "Gott", "das", "Heil", "be\u00b7sche\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "KOUS", "KOKOM", "PPER", "APPR", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Da\u00df Kind und Kindes Kind er ", "tokens": ["Da\u00df", "Kind", "und", "Kin\u00b7des", "Kind", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "p