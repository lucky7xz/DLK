{"textgrid.poem.52662": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Gefa\u00dft", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der ersten Liebe Feiertage", "tokens": ["Der", "ers\u00b7ten", "Lie\u00b7be", "Fei\u00b7er\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unsterblich, maienvolle Zeit,", "tokens": ["U\u00b7nsterb\u00b7lich", ",", "mai\u00b7en\u00b7vol\u00b7le", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So lebt ihr noch in meiner Klage,", "tokens": ["So", "lebt", "ihr", "noch", "in", "mei\u00b7ner", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit ihr so schnell entschwunden seid?", "tokens": ["Seit", "ihr", "so", "schnell", "ent\u00b7schwun\u00b7den", "seid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Schicksal hat mich fortgetrieben,", "tokens": ["Ein", "Schick\u00b7sal", "hat", "mich", "fort\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich ging, ich weinte bitterlich,", "tokens": ["Ich", "ging", ",", "ich", "wein\u00b7te", "bit\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verlernen sollt ich dich zu lieben,", "tokens": ["Ver\u00b7ler\u00b7nen", "sollt", "ich", "dich", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ach! vergessen sollt ich dich.", "tokens": ["Und", "ach", "!", "ver\u00b7ges\u00b7sen", "sollt", "ich", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "VVINF", "VMFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vergessen nicht, nein, nicht vergessen!", "tokens": ["Ver\u00b7ges\u00b7sen", "nicht", ",", "nein", ",", "nicht", "ver\u00b7ges\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PTKANT", "$,", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ich dich nur verlassen mu\u00dft;", "tokens": ["Da", "ich", "dich", "nur", "ver\u00b7las\u00b7sen", "mu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl konnt es mir die Wange n\u00e4ssen,", "tokens": ["Wohl", "konnt", "es", "mir", "die", "Wan\u00b7ge", "n\u00e4s\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nicht besiegen diese Brust.", "tokens": ["Doch", "nicht", "be\u00b7sie\u00b7gen", "die\u00b7se", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und durft ich keine H\u00fctten bauen,", "tokens": ["Und", "durft", "ich", "kei\u00b7ne", "H\u00fct\u00b7ten", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drein Gottes Segen strahlend bricht,", "tokens": ["Drein", "Got\u00b7tes", "Se\u00b7gen", "strah\u00b7lend", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So blieb mir manch ein k\u00fchn Vertrauen", "tokens": ["So", "blieb", "mir", "manch", "ein", "k\u00fchn", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und manche stolze Zuversicht!", "tokens": ["Und", "man\u00b7che", "stol\u00b7ze", "Zu\u00b7ver\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der ersten Liebe Feiertage", "tokens": ["Der", "ers\u00b7ten", "Lie\u00b7be", "Fei\u00b7er\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unsterblich, maienvolle Zeit,", "tokens": ["U\u00b7nsterb\u00b7lich", ",", "mai\u00b7en\u00b7vol\u00b7le", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "