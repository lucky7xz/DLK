{"textgrid.poem.60490": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Bei Gelegenheit einer l\u00e4ndlichen Unterhaltung in Bordacs", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was rauscht durch diese Pappeln? \u2013 horchet, Br\u00fcder!", "tokens": ["Was", "rauscht", "durch", "die\u00b7se", "Pap\u00b7peln", "?", "\u2013", "hor\u00b7chet", ",", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PDAT", "NN", "$.", "$(", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als naht' ein Genius aus Himmelsh\u00f6hn", "tokens": ["Als", "naht'", "ein", "Ge\u00b7nius", "aus", "Him\u00b7mels\u00b7h\u00f6hn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und senkte sich auf ihre Wipfel nieder,", "tokens": ["Und", "senk\u00b7te", "sich", "auf", "ih\u00b7re", "Wip\u00b7fel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So rauscht es durch den Hain mit leisem Wehn.", "tokens": ["So", "rauscht", "es", "durch", "den", "Hain", "mit", "lei\u00b7sem", "Wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Welch Schimmer! ha! mich fa\u00dft ein s\u00fc\u00dfes Bangen!", "tokens": ["Welch", "Schim\u00b7mer", "!", "ha", "!", "mich", "fa\u00dft", "ein", "s\u00fc\u00b7\u00dfes", "Ban\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "ITJ", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein M\u00e4dchen seh ich dort am Schattenrand", "tokens": ["Ein", "M\u00e4d\u00b7chen", "seh", "ich", "dort", "am", "Schat\u00b7ten\u00b7rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit g\u00fcldnem Fittig, rosenroten Wangen,", "tokens": ["Mit", "g\u00fcld\u00b7nem", "Fit\u00b7tig", ",", "ro\u00b7sen\u00b7ro\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Antlitz ist uns l\u00e4chelnd zugewandt.", "tokens": ["Ihr", "Ant\u00b7litz", "ist", "uns", "l\u00e4\u00b7chelnd", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die G\u00f6ttin ists des Gl\u00fccks! o Br\u00fcder, eilet", "tokens": ["Die", "G\u00f6t\u00b7tin", "ists", "des", "Gl\u00fccks", "!", "o", "Br\u00fc\u00b7der", ",", "ei\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "FM", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und rafft ihn auf, den frohen Augenblick,", "tokens": ["Und", "rafft", "ihn", "auf", ",", "den", "fro\u00b7hen", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Solange noch ihr rascher Fl\u00fcgel weilet;", "tokens": ["So\u00b7lan\u00b7ge", "noch", "ihr", "ra\u00b7scher", "Fl\u00fc\u00b7gel", "wei\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn der verlorne kehret nicht zur\u00fcck!", "tokens": ["Denn", "der", "ver\u00b7lor\u00b7ne", "keh\u00b7ret", "nicht", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es kommt ein Tag, die frohe Lust verklinget,", "tokens": ["Es", "kommt", "ein", "Tag", ",", "die", "fro\u00b7he", "Lust", "ver\u00b7klin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es zieht die G\u00f6ttin fort im schnellen Flug;", "tokens": ["Es", "zieht", "die", "G\u00f6t\u00b7tin", "fort", "im", "schnel\u00b7len", "Flug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und diese Hand, die jetzt den Becher schwinget,", "tokens": ["Und", "die\u00b7se", "Hand", ",", "die", "jetzt", "den", "Be\u00b7cher", "schwin\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4lt bebend den betr\u00e4nten Aschenkrug.", "tokens": ["H\u00e4lt", "be\u00b7bend", "den", "be\u00b7tr\u00e4n\u00b7ten", "A\u00b7schen\u00b7krug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Drum soll, solang das M\u00e4dchen dort uns l\u00e4chelt", "tokens": ["Drum", "soll", ",", "so\u00b7lang", "das", "M\u00e4d\u00b7chen", "dort", "uns", "l\u00e4\u00b7chelt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "$,", "KOUS", "ART", "NN", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und manches andre noch, solang der Wein", "tokens": ["Und", "man\u00b7ches", "and\u00b7re", "noch", ",", "so\u00b7lang", "der", "Wein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "PIS", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch schmeckt, die Wange Fr\u00fchlingsluft umf\u00e4chelt,", "tokens": ["Noch", "schmeckt", ",", "die", "Wan\u00b7ge", "Fr\u00fch\u00b7lings\u00b7luft", "um\u00b7f\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der eitle Gram von uns ge\u00e4chtet sein!", "tokens": ["Der", "eit\u00b7le", "Gram", "von", "uns", "ge\u00e4ch\u00b7tet", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "