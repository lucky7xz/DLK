{"dta.poem.8560": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Horche nicht hinter die Dinge. Zergr\u00fcble dich nicht. Suche nicht nach dir selbst.", "tokens": ["Hor\u00b7che", "nicht", "hin\u00b7ter", "die", "Din\u00b7ge", ".", "Zer\u00b7gr\u00fcb\u00b7le", "dich", "nicht", ".", "Su\u00b7che", "nicht", "nach", "dir", "selbst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "ART", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$.", "NN", "PTKNEG", "APPR", "PPER", "ADV", "$."], "meter": "+--+--+--+-+-+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Du bist nicht!", "tokens": ["Du", "bist", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Du bist der blaue, verschwebende Rauch, der sich aus deiner Cigarre ringelt,", "tokens": ["Du", "bist", "der", "blau\u00b7e", ",", "ver\u00b7schwe\u00b7ben\u00b7de", "Rauch", ",", "der", "sich", "aus", "dei\u00b7ner", "Ci\u00b7gar\u00b7re", "rin\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "der Tropfen, der eben aufs Fensterblech fiel,", "tokens": ["der", "Trop\u00b7fen", ",", "der", "e\u00b7ben", "aufs", "Fens\u00b7ter\u00b7blech", "fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "das leise, knisternde Lied, das durch die Stille deine Lampe singt.", "tokens": ["das", "lei\u00b7se", ",", "knis\u00b7tern\u00b7de", "Lied", ",", "das", "durch", "die", "Stil\u00b7le", "dei\u00b7ne", "Lam\u00b7pe", "singt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}