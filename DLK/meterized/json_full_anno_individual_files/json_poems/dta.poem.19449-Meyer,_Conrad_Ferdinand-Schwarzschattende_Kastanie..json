{"dta.poem.19449": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Schwarzschattende Kastanie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Schwarzschattende Kastanie,", "tokens": ["Schwarz\u00b7schat\u00b7ten\u00b7de", "Kas\u00b7ta\u00b7nie", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein windgeregtes Sommerzelt,", "tokens": ["Mein", "wind\u00b7ge\u00b7reg\u00b7tes", "Som\u00b7mer\u00b7zelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du senkst zur Flut dein weit Ge\u00e4st,", "tokens": ["Du", "senkst", "zur", "Flut", "dein", "weit", "Ge\u00b7\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Laub, es durstet und es trinkt,", "tokens": ["Dein", "Laub", ",", "es", "durs\u00b7tet", "und", "es", "trinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVPP", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schwarzschattende Kastanie!", "tokens": ["Schwarz\u00b7schat\u00b7ten\u00b7de", "Kas\u00b7ta\u00b7nie", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Im Porte badet junge Brut", "tokens": ["Im", "Por\u00b7te", "ba\u00b7det", "jun\u00b7ge", "Brut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Hader oder Lustgeschrei,", "tokens": ["Mit", "Ha\u00b7der", "o\u00b7der", "Lust\u00b7ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Kinder schwimmen leuchtend wei\u00df", "tokens": ["Und", "Kin\u00b7der", "schwim\u00b7men", "leuch\u00b7tend", "wei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Im Gitter deines Bl\u00e4tterwerks,", "tokens": ["Im", "Git\u00b7ter", "dei\u00b7nes", "Bl\u00e4t\u00b7ter\u00b7werks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Schwarzschattende Kastanie!", "tokens": ["Schwarz\u00b7schat\u00b7ten\u00b7de", "Kas\u00b7ta\u00b7nie", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "Und d\u00e4mmern See und Ufer ein", "tokens": ["Und", "d\u00e4m\u00b7mern", "See", "und", "U\u00b7fer", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und rauscht vorbei das Abendboot,", "tokens": ["Und", "rauscht", "vor\u00b7bei", "das", "A\u00b7bend\u00b7boot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "So zuckt aus rother Schiffslatern", "tokens": ["So", "zuckt", "aus", "ro\u00b7ther", "Schiffs\u00b7la\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Ein Blitz und wandert auf dem Schwung", "tokens": ["Ein", "Blitz", "und", "wan\u00b7dert", "auf", "dem", "Schwung"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Flut, gebrochnen Lettern gleich,", "tokens": ["Der", "Flut", ",", "ge\u00b7broch\u00b7nen", "Let\u00b7tern", "gleich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Bis unter deinem Laub erlischt", "tokens": ["Bis", "un\u00b7ter", "dei\u00b7nem", "Laub", "er\u00b7lischt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Die r\u00e4thselhafte Flammenschrift,", "tokens": ["Die", "r\u00e4th\u00b7sel\u00b7haf\u00b7te", "Flam\u00b7men\u00b7schrift", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Schwarzschattende Kastanie!", "tokens": ["Schwarz\u00b7schat\u00b7ten\u00b7de", "Kas\u00b7ta\u00b7nie", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}