{"dta.poem.2823": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Geistes Flitterstaat, mein Sohn, ist Neubegierde,", "tokens": ["Des", "Geis\u00b7tes", "Flit\u00b7ter\u00b7staat", ",", "mein", "Sohn", ",", "ist", "Neu\u00b7be\u00b7gier\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein die Wi\u00dfbegier ist seine wahre Zierde.", "tokens": ["Al\u00b7lein", "die", "Wi\u00df\u00b7be\u00b7gier", "ist", "sei\u00b7ne", "wah\u00b7re", "Zier\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Neubegier ist aufs Besondre gleich beflissen,", "tokens": ["Die", "Neu\u00b7be\u00b7gier", "ist", "aufs", "Be\u00b7sond\u00b7re", "gleich", "be\u00b7flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wi\u00dfbegierde will erst das Gemeinste wissen.", "tokens": ["Die", "Wi\u00df\u00b7be\u00b7gier\u00b7de", "will", "erst", "das", "Ge\u00b7meins\u00b7te", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Neubegierde spielt, die Wi\u00dfbegierde zielt;", "tokens": ["Die", "Neu\u00b7be\u00b7gier\u00b7de", "spielt", ",", "die", "Wi\u00df\u00b7be\u00b7gier\u00b7de", "zielt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wi\u00dfbegierde schaut, die Neubegierde schielt.", "tokens": ["Die", "Wi\u00df\u00b7be\u00b7gier\u00b7de", "schaut", ",", "die", "Neu\u00b7be\u00b7gier\u00b7de", "schielt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Des Strebens Unterschied, haupt- oder nebens\u00e4chlich,", "tokens": ["Des", "Stre\u00b7bens", "Un\u00b7ter\u00b7schied", ",", "haup\u00b7t", "o\u00b7der", "ne\u00b7ben\u00b7s\u00e4ch\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "TRUNC", "KON", "ADJD", "$,"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Macht gr\u00fcndlich Wi\u00dfbegier und Neugier oberfl\u00e4chlich.", "tokens": ["Macht", "gr\u00fcnd\u00b7lich", "Wi\u00df\u00b7be\u00b7gier", "und", "Neu\u00b7gier", "o\u00b7berf\u00b7l\u00e4ch\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}