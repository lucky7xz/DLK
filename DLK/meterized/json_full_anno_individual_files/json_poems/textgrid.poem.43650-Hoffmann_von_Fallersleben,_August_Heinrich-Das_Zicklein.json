{"textgrid.poem.43650": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Zicklein", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Mutter hat gesagt,", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "hat", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich soll das Zicklein h\u00fcten.", "tokens": ["Ich", "soll", "das", "Zic\u00b7klein", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zicklein klimmt so hoch hinan,", "tokens": ["Zic\u00b7klein", "klimmt", "so", "hoch", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich ihm nicht folgen kann", "tokens": ["Da\u00df", "ich", "ihm", "nicht", "fol\u00b7gen", "kann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwischen Laub und Bl\u00fcthen.", "tokens": ["Zwi\u00b7schen", "Laub", "und", "Bl\u00fc\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Meine Mutter hat gesagt:", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "hat", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bring' mir das Zicklein wieder!", "tokens": ["Bring'", "mir", "das", "Zic\u00b7klein", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zicklein bleibt am Abhang stehn,", "tokens": ["Zic\u00b7klein", "bleibt", "am", "Ab\u00b7hang", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Will sich mal die Welt besehn,", "tokens": ["Will", "sich", "mal", "die", "Welt", "be\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zicklein mag nicht nieder.", "tokens": ["Zic\u00b7klein", "mag", "nicht", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Meine Mutter hat gesagt:", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "hat", "ge\u00b7sagt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn's immer will entlaufen,", "tokens": ["Wenn's", "im\u00b7mer", "will", "ent\u00b7lau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Soll's nicht l\u00e4nger unser sein,", "tokens": ["Soll's", "nicht", "l\u00e4n\u00b7ger", "un\u00b7ser", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG