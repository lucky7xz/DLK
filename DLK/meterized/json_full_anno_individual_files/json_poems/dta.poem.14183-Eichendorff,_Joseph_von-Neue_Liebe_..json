{"dta.poem.14183": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Neue Liebe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und ich hab' die Fenster offen,", "tokens": ["Und", "ich", "hab'", "die", "Fens\u00b7ter", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neu zieh in die Welt hinein", "tokens": ["Neu", "zieh", "in", "die", "Welt", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Altes Bangen, altes Hoffen!", "tokens": ["Al\u00b7tes", "Ban\u00b7gen", ",", "al\u00b7tes", "Hof\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fchling, Fr\u00fchling soll es sein!", "tokens": ["Fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", "soll", "es", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}