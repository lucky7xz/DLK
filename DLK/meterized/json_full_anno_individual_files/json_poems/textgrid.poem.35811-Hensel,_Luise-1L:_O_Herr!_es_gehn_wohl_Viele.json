: ["NE", "NN", "$.", "APPR", "PWAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hast Du Dich mir versenkt?", "tokens": ["Hast", "Du", "Dich", "mir", "ver\u00b7senkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo soll mein Herz nun finden", "tokens": ["Wo", "soll", "mein", "Herz", "nun", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Quell, der rein mich tr\u00e4nkt?", "tokens": ["Den", "Quell", ",", "der", "rein", "mich", "tr\u00e4nkt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Oft mu\u00df ich Blinde fragen,", "tokens": ["Oft", "mu\u00df", "ich", "Blin\u00b7de", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die viel am Wege stehn;", "tokens": ["Die", "viel", "am", "We\u00b7ge", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie k\u00f6nnen mir's nicht sagen,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "mir's", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weil sie das Licht nicht sehn.", "tokens": ["Weil", "sie", "das", "Licht", "nicht", "sehn", "."], "token_in