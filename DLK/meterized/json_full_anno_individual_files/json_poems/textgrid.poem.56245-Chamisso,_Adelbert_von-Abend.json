{"textgrid.poem.56245": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Abend", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df, Kind, la\u00df meinen Weg mich ziehen,", "tokens": ["La\u00df", ",", "Kind", ",", "la\u00df", "mei\u00b7nen", "Weg", "mich", "zie\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "VVIMP", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es wird schon sp\u00e4t, es wird schon kalt,", "tokens": ["Es", "wird", "schon", "sp\u00e4t", ",", "es", "wird", "schon", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es neiget sich der Tag zu Ende,", "tokens": ["Es", "nei\u00b7get", "sich", "der", "Tag", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und erst dort unten mach ich Halt.", "tokens": ["Und", "erst", "dort", "un\u00b7ten", "mach", "ich", "Halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wozu mir deine Lieder singen?", "tokens": ["Wo\u00b7zu", "mir", "dei\u00b7ne", "Lie\u00b7der", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie treffen mich mit fremdem Klang. \u2013", "tokens": ["Sie", "tref\u00b7fen", "mich", "mit", "frem\u00b7dem", "Klang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie war das Wort? war's Liebe? Liebe!", "tokens": ["Wie", "war", "das", "Wort", "?", "wa\u00b7r's", "Lie\u00b7be", "?", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$.", "VAFIN", "NN", "$.", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vergessen hatt ich es schon lang.", "tokens": ["Ver\u00b7ges\u00b7sen", "hatt", "ich", "es", "schon", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und doch, gedenk ich ferner Zeiten,", "tokens": ["Und", "doch", ",", "ge\u00b7denk", "ich", "fer\u00b7ner", "Zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich d\u00fcnkt, es war ein s\u00fc\u00dfes Wort.", "tokens": ["Mich", "d\u00fcnkt", ",", "es", "war", "ein", "s\u00fc\u00b7\u00dfes", "Wort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt aber zieh ich meiner Stra\u00dfe,", "tokens": ["Jetzt", "a\u00b7ber", "zieh", "ich", 