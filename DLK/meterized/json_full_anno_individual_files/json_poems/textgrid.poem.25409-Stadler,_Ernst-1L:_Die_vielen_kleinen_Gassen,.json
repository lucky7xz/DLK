{"textgrid.poem.25409": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die vielen kleinen Gassen,", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die vielen kleinen Gassen,", "tokens": ["Die", "vie\u00b7len", "klei\u00b7nen", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "die die langgestreckte Hauptstra\u00dfe \u00fcberqueren,", "tokens": ["die", "die", "lang\u00b7ge\u00b7streck\u00b7te", "Haupt\u00b7stra\u00b7\u00dfe", "\u00fc\u00b7berq\u00b7ue\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Laufen alle ins Gr\u00fcne.", "tokens": ["Lau\u00b7fen", "al\u00b7le", "ins", "Gr\u00fc\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "\u00dcberall f\u00e4ngt Land an.", "tokens": ["\u00dc\u00b7be\u00b7rall", "f\u00e4ngt", "Land", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "\u00dcberall str\u00f6mt Himmel ein und Geruch von B\u00e4umen", "tokens": ["\u00dc\u00b7be\u00b7rall", "str\u00f6mt", "Him\u00b7mel", "ein", "und", "Ge\u00b7ruch", "von", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "KON", "NN", "APPR", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "und der starke Duft der \u00c4cker