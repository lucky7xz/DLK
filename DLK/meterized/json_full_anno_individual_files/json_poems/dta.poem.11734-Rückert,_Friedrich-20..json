{"dta.poem.11734": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Knabe steht am Berg und lauscht in stiller Wonne,", "tokens": ["Der", "Kna\u00b7be", "steht", "am", "Berg", "und", "lauscht", "in", "stil\u00b7ler", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil gegen\u00fcber ihm aufgehen will die Sonne.", "tokens": ["Weil", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "ihm", "auf\u00b7ge\u00b7hen", "will", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VVINF", "VMFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Die h\u00f6chsten Spitzen sieht von Hoffnung er ger\u00f6thet,", "tokens": ["Die", "h\u00f6chs\u00b7ten", "Spit\u00b7zen", "sieht", "von", "Hoff\u00b7nung", "er", "ge\u00b7r\u00f6\u00b7thet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und h\u00f6rt von Lerchenlied den Sieg des Lichts gefl\u00f6tet.", "tokens": ["Und", "h\u00f6rt", "von", "Ler\u00b7chen\u00b7lied", "den", "Sieg", "des", "Lichts", "ge\u00b7fl\u00f6\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch immer will sie selbst noch kommen nicht empor,", "tokens": ["Doch", "im\u00b7mer", "will", "sie", "selbst", "noch", "kom\u00b7men", "nicht", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "ADV", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seiner Sehnsucht schiebt sich eine Wolke vor.", "tokens": ["Und", "sei\u00b7ner", "Sehn\u00b7sucht", "schiebt", "sich", "ei\u00b7ne", "Wol\u00b7ke", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da fa\u00dft ihn Ungeduld: wie lange will sie s\u00e4umen?", "tokens": ["Da", "fa\u00dft", "ihn", "Un\u00b7ge\u00b7duld", ":", "wie", "lan\u00b7ge", "will", "sie", "s\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$.", "PWAV", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sonn' entgegen geht er vorwerts in den R\u00e4umen.", "tokens": ["Der", "Sonn'", "ent\u00b7ge\u00b7gen", "geht", "er", "vor\u00b7werts", "in", "den", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPO", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er geht den Berg hinab, er stand am Bergabhange,", "tokens": ["Er", "geht", "den", "Berg", "hin\u00b7ab", ",", "er", "stand", "am", "Ber\u00b7gab\u00b7han\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entgegen berghinab geht er dem Sonnaufgange.", "tokens": ["Ent\u00b7ge\u00b7gen", "berg\u00b7hi\u00b7nab", "geht", "er", "dem", "Son\u00b7nauf\u00b7gan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Und immer schw\u00e4cher wird um ihn der Morgenschein,", "tokens": ["Und", "im\u00b7mer", "schw\u00e4\u00b7cher", "wird", "um", "ihn", "der", "Mor\u00b7gen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie tiefer in die Nacht des Thals er geht hinein.", "tokens": ["Wie", "tie\u00b7fer", "in", "die", "Nacht", "des", "Thals", "er", "geht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und aus der Schlucht, wo ihm der letzte Schein verglimmt,", "tokens": ["Und", "aus", "der", "Schlucht", ",", "wo", "ihm", "der", "letz\u00b7te", "Schein", "ver\u00b7glimmt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieht er zur\u00fcck, wie rings in Glanz die Sch\u00f6pfung schwimmt;", "tokens": ["Sieht", "er", "zu\u00b7r\u00fcck", ",", "wie", "rings", "in", "Glanz", "die", "Sch\u00f6p\u00b7fung", "schwimmt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ADV", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und sieht denselben Platz, von dem er ausgegangen,", "tokens": ["Und", "sieht", "den\u00b7sel\u00b7ben", "Platz", ",", "von", "dem", "er", "aus\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$,", "APPR", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom hellsten Sonnenstral, den er ersehnt, umfangen.", "tokens": ["Vom", "hells\u00b7ten", "Son\u00b7nen\u00b7stral", ",", "den", "er", "er\u00b7sehnt", ",", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}