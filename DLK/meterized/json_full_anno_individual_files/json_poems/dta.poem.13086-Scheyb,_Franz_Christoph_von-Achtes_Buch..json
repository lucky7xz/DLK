{"dta.poem.13086": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.57", "af:0.28", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eja! stell du dir ihn vor, wie man ihn streiten sah,", "tokens": ["\u201e", "ja", "!", "stell", "du", "dir", "ihn", "vor", ",", "wie", "man", "ihn", "strei\u00b7ten", "sah", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "ADJD", "PPER", "PPER", "PPER", "PTKVZ", "$,", "PWAV", "PIS", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewas Unerschrockenheit er f\u00fcr ", "tokens": ["\u201e", "was", "Un\u00b7er\u00b7schro\u00b7cken\u00b7heit", "er", "f\u00fcr"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PWS", "NN", "PPER", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eund f\u00fcr das Vaterland in seinen Thaten wiese;", "tokens": ["\u201e", "und", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "in", "sei\u00b7nen", "Tha\u00b7ten", "wie\u00b7se", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewie selbst des Feindes Heer sein Thun und Lassen priese.", "tokens": ["\u201e", "wie", "selbst", "des", "Fein\u00b7des", "Heer", "sein", "Thun", "und", "Las\u00b7sen", "prie\u00b7se", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "ART", "NN", "NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "185\u201dBewaffnet und zu Pferd mit seinem Krieges-Stab,", "tokens": ["\"", "Be\u00b7waff\u00b7net", "und", "zu", "Pferd", "mit", "sei\u00b7nem", "Krie\u00b7ge\u00b7sStab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "KON", "APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewie selber ich ihn dort im Feld gesehen hab,", "tokens": ["\u201e", "wie", "sel\u00b7ber", "ich", "ihn", "dort", "im", "Feld", "ge\u00b7se\u00b7hen", "hab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "PPER", "PPER", "ADV", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eals er in Ei\u00df und Schnee mit Herz und Gro\u00dfmuth fochte,", "tokens": ["\u201e", "als", "er", "in", "Ei\u00df", "und", "Schnee", "mit", "Herz", "und", "Gro\u00df\u00b7muth", "foch\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eden Einfall hintertrieb, womit der Gegner pochte.", "tokens": ["\u201e", "den", "Ein\u00b7fall", "hin\u00b7ter\u00b7trieb", ",", "wo\u00b7mit", "der", "Geg\u00b7ner", "poch\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ekraft, Munterkeit, Vernunft, Geist, Hoheit und Gewalt,", "tokens": ["\u201e", "kraft", ",", "Mun\u00b7ter\u00b7keit", ",", "Ver\u00b7nunft", ",", "Geist", ",", "Ho\u00b7heit", "und", "Ge\u00b7walt", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "190\u201dJa was ein Held im Krieg durch die Gesichts-Gestallt", "tokens": ["\"", "Ja", "was", "ein", "Held", "im", "Krieg", "durch", "die", "Ge\u00b7sichts\u00b7Ge\u00b7stallt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "PRELS", "ART", "NN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201edem Heer vor Augen stellt, wollt ich in Marmel hauen;", "tokens": ["\u201e", "dem", "Heer", "vor", "Au\u00b7gen", "stellt", ",", "wollt", "ich", "in", "Mar\u00b7mel", "hau\u00b7en", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "NN", "VVFIN", "$,", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201edie\u00df alles solltest du darinnen lebhaft schauen.", "tokens": ["\u201e", "die\u00df", "al\u00b7les", "soll\u00b7test", "du", "da\u00b7rin\u00b7nen", "leb\u00b7haft", "schau\u00b7en", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "PIS", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eich streute rings herum die Sieges-Zeichen hin,", "tokens": ["\u201e", "ich", "streu\u00b7te", "rings", "he\u00b7rum", "die", "Sie\u00b7ges\u00b7Zei\u00b7chen", "hin", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APZR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201emit welchen wir ihn sahn in Feindes L\u00e4nder ziehn.", "tokens": ["\u201e", "mit", "wel\u00b7chen", "wir", "ihn", "sahn", "in", "Fein\u00b7des", "L\u00e4n\u00b7der", "ziehn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWAT", "PPER", "PPER", "VVFIN", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "195\u201dIch schm\u00fcckte seine Brust mit jenen Anmuths-Strahlen", "tokens": ["\"", "Ich", "schm\u00fcck\u00b7te", "sei\u00b7ne", "Brust", "mit", "je\u00b7nen", "An\u00b7muths\u00b7Strah\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201edie sonsten das Gesicht der K\u00f6niginn bemahlen.", "tokens": ["\u201e", "die", "sons\u00b7ten", "das", "Ge\u00b7sicht", "der", "K\u00f6\u00b7ni\u00b7ginn", "be\u00b7mah\u00b7len", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201edas Bildnis m\u00fc\u00dfte sich zu seines Lebens Schuz,", "tokens": ["\u201e", "das", "Bild\u00b7nis", "m\u00fc\u00df\u00b7te", "sich", "zu", "sei\u00b7nes", "Le\u00b7bens", "Schuz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201edes Feindes Heer zur Furcht, desselben Groll zu Truz,", "tokens": ["\u201e", "des", "Fein\u00b7des", "Heer", "zur", "Furcht", ",", "des\u00b7sel\u00b7ben", "Groll", "zu", "Truz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "APPRART", "NN", "$,", "PRELAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201eder treuen Krieger Schaar zu der Erweckung zeigen;", "tokens": ["\u201e", "der", "treu\u00b7en", "Krie\u00b7ger", "Schaar", "zu", "der", "Er\u00b7we\u00b7ckung", "zei\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "200\u201dDas Pferd in Majest\u00e4t mit stolzen Schritten steigen.", "tokens": ["\"", "Das", "Pferd", "in", "Ma\u00b7jes\u00b7t\u00e4t", "mit", "stol\u00b7zen", "Schrit\u00b7ten", "stei\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}