{"dta.poem.11102": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sinn-Getichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Stoltze T\u00fcrcken, freuet euch! K\u00fchne Frantzen, jauchtzt", "tokens": ["Stolt\u00b7ze", "T\u00fcr\u00b7cken", ",", "freu\u00b7et", "euch", "!", "K\u00fch\u00b7ne", "Frant\u00b7zen", ",", "jauchtzt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$.", "NN", "NN", "$,", "VVFIN"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "und springet!", "tokens": ["und", "sprin\u00b7get", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Denn der alle welt erschreckt, liegt nun auf der todten-bahr.", "tokens": ["Denn", "der", "al\u00b7le", "welt", "er\u00b7schreckt", ",", "liegt", "nun", "auf", "der", "tod\u00b7ten\u00b7bahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VVPP", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jedoch freut, ihr Teutschen, euch! Denn der grosse Friedrich", "tokens": ["Je\u00b7doch", "freut", ",", "ihr", "Teut\u00b7schen", ",", "euch", "!", "Denn", "der", "gros\u00b7se", "Fried\u00b7rich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "$.", "KON", "ART", "ADJA", "NE"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "bringet", "tokens": ["brin\u00b7get"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Wieder durch die kunst ans licht, was schon staub und", "tokens": ["Wie\u00b7der", "durch", "die", "kunst", "ans", "licht", ",", "was", "schon", "staub", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "ADV", "ADJD", "KON"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "asche war.", "tokens": ["asc\u00b7he", "war", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Freuet euch, ihr tugenden, und du gantzer musen-orden!", "tokens": ["Freu\u00b7et", "euch", ",", "ihr", "tu\u00b7gen\u00b7den", ",", "und", "du", "gant\u00b7zer", "mu\u00b7sen\u00b7or\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "KON", "PPER", "ADJA", "NN", "$."], "meter": "----+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Der ein f\u00fcrsten-vater hie\u00df, ist ein k\u00f6nigs-vater worden.", "tokens": ["Der", "ein", "f\u00fcrs\u00b7ten\u00b7va\u00b7ter", "hie\u00df", ",", "ist", "ein", "k\u00f6\u00b7nigs\u00b7va\u00b7ter", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "VVFIN", "$,", "VAFIN", "ART", "NN", "VAPP", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}