{"dta.poem.18573": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "U ber ihre  U ntreue.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Immer hin/", "tokens": ["Im\u00b7mer", "hin", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Falsches Hertze/ leichter Sinn!", "tokens": ["Fal\u00b7sches", "Hert\u00b7ze", "/", "leich\u00b7ter", "Sinn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lesche nur die starcken Kertzen", "tokens": ["Le\u00b7sche", "nur", "die", "star\u00b7cken", "Kert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den sonst entflammten Hertzen/", "tokens": ["In", "den", "sonst", "ent\u00b7flamm\u00b7ten", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil ich es zu frieden bin.", "tokens": ["Weil", "ich", "es", "zu", "frie\u00b7den", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Immer hin/", "tokens": ["Im\u00b7mer", "hin", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Falsches Hertze/ leichter Sinn!", "tokens": ["Fal\u00b7sches", "Hert\u00b7ze", "/", "leich\u00b7ter", "Sinn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$(", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwur und Treu", "tokens": ["Schwur", "und", "Treu"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Sind Betrug und Heucheley.", "tokens": ["Sind", "Be\u00b7trug", "und", "Heu\u00b7che\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch die allersch\u00f6nsten Decken", "tokens": ["Auch", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "De\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind gar selten ohne Flecken/", "tokens": ["Sind", "gar", "sel\u00b7ten", "oh\u00b7ne", "Fle\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Damen einerley.", "tokens": ["Und", "die", "Da\u00b7men", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwur und Treu", "tokens": ["Schwur", "und", "Treu"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Sind Betrug und Heucheley.", "tokens": ["Sind", "Be\u00b7trug", "und", "Heu\u00b7che\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wie sch\u00f6n", "tokens": ["Doch", "wie", "sch\u00f6n"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Wissen sie sich vorzusehn.", "tokens": ["Wis\u00b7sen", "sie", "sich", "vor\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Muschel ist gebrochen/", "tokens": ["Wenn", "die", "Mu\u00b7schel", "ist", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Perle draus gestochen/", "tokens": ["Und", "die", "Per\u00b7le", "draus", "ge\u00b7sto\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll sie erst verschlossen stehn.", "tokens": ["Soll", "sie", "erst", "ver\u00b7schlos\u00b7sen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch wie sch\u00f6n", "tokens": ["Doch", "wie", "sch\u00f6n"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Wissen sie sich vorzusehn.", "tokens": ["Wis\u00b7sen", "sie", "sich", "vor\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dr\u00fcm mein Geist/", "tokens": ["Dr\u00fcm", "mein", "Geist", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Suche was unsterblich heist/", "tokens": ["Su\u00b7che", "was", "uns\u00b7terb\u00b7lich", "heist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe wo die sch\u00f6ne Jugend", "tokens": ["Lie\u00b7be", "wo", "die", "sch\u00f6\u00b7ne", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich durch Klugheit und durch Tugend", "tokens": ["Dich", "durch", "Klug\u00b7heit", "und", "durch", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ewig mit Vergn\u00fcgung speist.", "tokens": ["E\u00b7wig", "mit", "Ver\u00b7gn\u00fc\u00b7gung", "speist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dr\u00fcm mein Geist", "tokens": ["Dr\u00fcm", "mein", "Geist"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Suche was unsterblich heist.", "tokens": ["Su\u00b7che", "was", "uns\u00b7terb\u00b7lich", "heist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}