{"dta.poem.987": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "A. V.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr ahnt die linien unsrer hellen welten                 ", "tokens": ["Ihr", "ahnt", "die", "li\u00b7ni\u00b7en", "uns\u00b7rer", "hel\u00b7len", "wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die bunten halden mit den rebenkronen                 ", "tokens": ["Die", "bun\u00b7ten", "hal\u00b7den", "mit", "den", "re\u00b7ben\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den zefir der durch grade pappeln fl&#252;stert                 ", "tokens": ["Den", "ze\u00b7fir", "der", "durch", "gra\u00b7de", "pap\u00b7peln", "fl", "&#252;", "stert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "NN", "ART", "APPR", "ADV", "VVFIN", "NE", "$(", "VVFIN"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und Tiburs wasser weich wie liebesfl\u00f6ten?                 ", "tokens": ["Und", "Ti\u00b7burs", "was\u00b7ser", "weich", "wie", "lie\u00b7bes\u00b7fl\u00f6\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJD", "ADJD", "KOKOM", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da hebt sich euer blondes haupt: kennt IHR                 ", "tokens": ["Da", "hebt", "sich", "eu\u00b7er", "blon\u00b7des", "haupt", ":", "kennt", "IhR"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$.", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der nebel tanz im moore grenzenlos                 ", "tokens": ["Der", "ne\u00b7bel", "tanz", "im", "moo\u00b7re", "gren\u00b7zen\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Im d&#252;nenried der st&#252;rme orgelton                 ", "tokens": ["Im", "d", "&#252;", "nen\u00b7ried", "der", "st", "&#252;", "rme", "or\u00b7gel\u00b7ton"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und das ger&#228;usch der ungeheuren see?               ", "tokens": ["Und", "das", "ger", "&#228;", "usch", "der", "un\u00b7ge\u00b7heu\u00b7ren", "see", "?"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$(", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}