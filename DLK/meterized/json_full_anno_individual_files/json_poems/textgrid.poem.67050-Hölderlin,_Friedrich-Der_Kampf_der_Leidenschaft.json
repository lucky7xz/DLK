{"textgrid.poem.67050": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Kampf der Leidenschaft", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ras ich ewig? noch nicht ausgestritten", "tokens": ["Ras", "ich", "e\u00b7wig", "?", "noch", "nicht", "aus\u00b7ge\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "ADJD", "$.", "ADV", "PTKNEG", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist der hei\u00dfe Streit der Leidenschaft?", "tokens": ["Ist", "der", "hei\u00b7\u00dfe", "Streit", "der", "Lei\u00b7den\u00b7schaft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hab ich Armer nicht genug gelitten?", "tokens": ["Hab", "ich", "Ar\u00b7mer", "nicht", "ge\u00b7nug", "ge\u00b7lit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie ist hin \u2013 ist hin \u2013 des K\u00e4mpfers Kraft.", "tokens": ["Sie", "ist", "hin", "\u2013", "ist", "hin", "\u2013", "des", "K\u00e4mp\u00b7fers", "Kraft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "VAFIN", "ADV", "$(", "ART", "NN", "NN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Engelsauge! immer um mich schweben \u2013", "tokens": ["En\u00b7gel\u00b7sau\u00b7ge", "!", "im\u00b7mer", "um", "mich", "schwe\u00b7ben", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "O warum? warum? du liebe Grausame!", "tokens": ["O", "wa\u00b7rum", "?", "wa\u00b7rum", "?", "du", "lie\u00b7be", "Grau\u00b7sa\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "$.", "PWAV", "$.", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Schone! schone! sieh! dies schwache Beben!", "tokens": ["Scho\u00b7ne", "!", "scho\u00b7ne", "!", "sieh", "!", "dies", "schwa\u00b7che", "Be\u00b7ben", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "$.", "VVIMP", "$.", "PDS", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Weibertr\u00e4nen weint der \u00dcberwundene.", "tokens": ["Wei\u00b7ber\u00b7tr\u00e4\u00b7nen", "weint", "der", "\u00dc\u00b7berw\u00b7un\u00b7de\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+--", "measure": "unknown.measure.penta"}}, "stanza.2": {"line.1": {"text": "Weibertr\u00e4nen weinen? Weibertr\u00e4nen?", "tokens": ["Wei\u00b7ber\u00b7tr\u00e4\u00b7nen", "wei\u00b7nen", "?", "Wei\u00b7ber\u00b7tr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$.", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wirklich? wein ich wirklich, Zauberin?", "tokens": ["Wirk\u00b7lich", "?", "wein", "ich", "wirk\u00b7lich", ",", "Zau\u00b7be\u00b7rin", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "$.", "PWAV", "PPER", "ADJD", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und dies Klopfen, dieses bange Sehnen,", "tokens": ["Und", "dies", "Klop\u00b7fen", ",", "die\u00b7ses", "ban\u00b7ge", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ists um Luzias Umarmungen?", "tokens": ["Ists", "um", "Lu\u00b7zi\u00b7as", "Um\u00b7ar\u00b7mun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Nein! ich kann nicht! will nicht! diese Tr\u00e4nen", "tokens": ["Nein", "!", "ich", "kann", "nicht", "!", "will", "nicht", "!", "die\u00b7se", "Tr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VMFIN", "PTKNEG", "$.", "VMFIN", "PTKNEG", "$.", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Stie\u00df der Zorn ins Auge, sie vergo\u00df der Grimm;", "tokens": ["Stie\u00df", "der", "Zorn", "ins", "Au\u00b7ge", ",", "sie", "ver\u00b7go\u00df", "der", "Grimm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "ART", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "O! mich schmelzen keine M\u00e4dchenmienen,", "tokens": ["O", "!", "mich", "schmel\u00b7zen", "kei\u00b7ne", "M\u00e4d\u00b7chen\u00b7mie\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Nur der Freiheit brauste dieses Ungest\u00fcm.", "tokens": ["Nur", "der", "Frei\u00b7heit", "braus\u00b7te", "die\u00b7ses", "Un\u00b7ge\u00b7st\u00fcm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Aber wie? dein Stolz hat sich betrogen,", "tokens": ["A\u00b7ber", "wie", "?", "dein", "Stolz", "hat", "sich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "PPOSAT", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Siehe! L\u00fcgen straft die Liebe mich;", "tokens": ["Sie\u00b7he", "!", "L\u00fc\u00b7gen", "straft", "die", "Lie\u00b7be", "mich", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00e4nnergr\u00f6\u00dfe hat dein Herz gelogen,", "tokens": ["M\u00e4n\u00b7ner\u00b7gr\u00f6\u00b7\u00dfe", "hat", "dein", "Herz", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und im schwachen Kampf verkennst du dich.", "tokens": ["Und", "im", "schwa\u00b7chen", "Kampf", "ver\u00b7kennst", "du", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Stolz verschm\u00e4hst du alle M\u00e4dchenherzen,", "tokens": ["Stolz", "ver\u00b7schm\u00e4hst", "du", "al\u00b7le", "M\u00e4d\u00b7chen\u00b7her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Weil dir Luzia ihr gro\u00dfes Herz nicht gibt,", "tokens": ["Weil", "dir", "Lu\u00b7zia", "ihr", "gro\u00b7\u00dfes", "Herz", "nicht", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kindisch heuchelst du verbi\u00dfne Schmerzen,", "tokens": ["Kin\u00b7disch", "heu\u00b7chelst", "du", "ver\u00b7bi\u00df\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Armer Heuchler! weil dich Luzia nicht liebt.", "tokens": ["Ar\u00b7mer", "Heuch\u00b7ler", "!", "weil", "dich", "Lu\u00b7zia", "nicht", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KOUS", "PPER", "NE", "PTKNEG", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Weh! sie kann, sie kann mich nimmer lieben,", "tokens": ["Weh", "!", "sie", "kann", ",", "sie", "kann", "mich", "nim\u00b7mer", "lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mir geraubt durch ein tyrannisch Joch,", "tokens": ["Mir", "ge\u00b7raubt", "durch", "ein", "ty\u00b7ran\u00b7nisch", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nur die Wunde noc