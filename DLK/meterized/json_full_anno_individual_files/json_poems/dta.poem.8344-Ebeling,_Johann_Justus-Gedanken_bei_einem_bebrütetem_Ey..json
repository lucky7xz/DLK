{"dta.poem.8344": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gedanken  \n bei einem bebr\u00fctetem Ey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo GOtt das Leben giebt, da giebt\ner Nahrung auch,", "tokens": ["Wo", "Gott", "das", "Le\u00b7ben", "giebt", ",", "da", "giebt", "er", "Nah\u00b7rung", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versorgt die Kreatur; wenn sie", "tokens": ["Ver\u00b7sorgt", "die", "Kre\u00b7a\u00b7tur", ";", "wenn", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der sie gebiehrt, verstekt: Wie solt er uns das", "tokens": ["Der", "sie", "ge\u00b7biehrt", ",", "vers\u00b7tekt", ":", "Wie", "solt", "er", "uns", "das"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVPP", "$,", "VVPP", "$.", "PWAV", "VMFIN", "PPER", "PRF", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da er der Vater ist, ohn Lebens Nahrung geben?", "tokens": ["Da", "er", "der", "Va\u00b7ter", "ist", ",", "ohn", "Le\u00b7bens", "Nah\u00b7rung", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "KOUI", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das thut der Sch\u00f6pfer nicht, der m\u00e4chtig, g\u00fctig,", "tokens": ["Das", "thut", "der", "Sch\u00f6p\u00b7fer", "nicht", ",", "der", "m\u00e4ch\u00b7tig", ",", "g\u00fc\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "$,", "PRELS", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo er das Leben giebt, legt er die Nahrung bei.", "tokens": ["Wo", "er", "das", "Le\u00b7ben", "giebt", ",", "legt", "er", "die", "Nah\u00b7rung", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wir k\u00f6nnen dieses klar an denen Eyern sehen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "die\u00b7ses", "klar", "an", "de\u00b7nen", "Ey\u00b7ern", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDAT", "ADJD", "APPR", "PRELS", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Woraus die junge Brut der K\u00fcchelein entstehen.", "tokens": ["Wo\u00b7raus", "die", "jun\u00b7ge", "Brut", "der", "K\u00fc\u00b7chel\u00b7ein", "ent\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Seh ich ein Ey recht an, und was darinnen stekt,", "tokens": ["Seh", "ich", "ein", "Ey", "recht", "an", ",", "und", "was", "da\u00b7rin\u00b7nen", "stekt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$,", "KON", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So wird in meiner Seel Bewunderung erwekt:", "tokens": ["So", "wird", "in", "mei\u00b7ner", "Seel", "Be\u00b7wun\u00b7de\u00b7rung", "er\u00b7wekt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man kan nicht ohne Lust die Theile all erwegen,", "tokens": ["Man", "kan", "nicht", "oh\u00b7ne", "Lust", "die", "Thei\u00b7le", "all", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PTKNEG", "APPR", "NN", "ART", "NN", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die sich in einem Ey, uns vor die Augen legen.", "tokens": ["Die", "sich", "in", "ei\u00b7nem", "Ey", ",", "uns", "vor", "die", "Au\u00b7gen", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "$,", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein gelber Dotter der recht in der Mitte liegt,", "tokens": ["Ein", "gel\u00b7ber", "Dot\u00b7ter", "der", "recht", "in", "der", "Mit\u00b7te", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.14": {"text": "Ist an den Enden fest ins Weisse angef\u00fcgt", "tokens": ["Ist", "an", "den", "En\u00b7den", "fest", "ins", "Weis\u00b7se", "an\u00b7ge\u00b7f\u00fcgt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Durch B\u00e4nder welche man, die Hagelkn\u00f6tchen", "tokens": ["Durch", "B\u00e4n\u00b7der", "wel\u00b7che", "man", ",", "die", "Ha\u00b7gel\u00b7kn\u00f6t\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PRELS", "PIS", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und die verlieren sich, wo sich das Wei\u00df ergeusset.", "tokens": ["Und", "die", "ver\u00b7lie\u00b7ren", "sich", ",", "wo", "sich", "das", "Wei\u00df", "er\u00b7geus\u00b7set", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PRF", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein doppelt Eyerwei\u00df f\u00fcllt denn die Schalen an,", "tokens": ["Ein", "dop\u00b7pelt", "Ey\u00b7er\u00b7wei\u00df", "f\u00fcllt", "denn", "die", "Scha\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Wie man im Augenblik mit Lust ansehen kan,", "tokens": ["Wie", "man", "im", "Au\u00b7gen\u00b7blik", "mit", "Lust", "an\u00b7se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPRART", "NN", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn man sie nur zerbricht. Das eine ist verd\u00fcn-", "tokens": ["Wenn", "man", "sie", "nur", "zer\u00b7bricht", ".", "Das", "ei\u00b7ne", "ist", "ver\u00b7d\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN", "$.", "ART", "PIS", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das wenn die Schale bricht, wie milchicht Wasser", "tokens": ["Das", "wenn", "die", "Scha\u00b7le", "bricht", ",", "wie", "milc\u00b7hicht", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "KOUS", "ART", "NN", "VVFIN", "$,", "PWAV", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Das andre Wei\u00df ist steif, am Dotter angeklebt,", "tokens": ["Das", "and\u00b7re", "Wei\u00df", "ist", "steif", ",", "am", "Dot\u00b7ter", "an\u00b7ge\u00b7klebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und durch die Kn\u00f6tchens fest und gleichsam durch-", "tokens": ["Und", "durch", "die", "Kn\u00f6t\u00b7chens", "fest", "und", "gleich\u00b7sam", "durch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "KON", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Der Dotter und das Wei\u00df sind zwiefach ganz um-", "tokens": ["Der", "Dot\u00b7ter", "und", "das", "Wei\u00df", "sind", "zwie\u00b7fach", "ganz", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Von doppelt zarter Haut, worin sie sanfte schwe-", "tokens": ["Von", "dop\u00b7pelt", "zar\u00b7ter", "Haut", ",", "wo\u00b7rin", "sie", "sanf\u00b7te", "schwe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und diese wiederum sind sicher, wohl verwahrt,", "tokens": ["Und", "die\u00b7se", "wie\u00b7de\u00b7rum", "sind", "si\u00b7cher", ",", "wohl", "ver\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VAFIN", "ADJD", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Durch eine dichte Schal, die rundlicht, fest und", "tokens": ["Durch", "ei\u00b7ne", "dich\u00b7te", "Schal", ",", "die", "rund\u00b7licht", ",", "fest", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "$,", "ADJD", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die harte Schale h\u00e4ngt an denen d\u00fcnnen H\u00e4uten", "tokens": ["Die", "har\u00b7te", "Scha\u00b7le", "h\u00e4ngt", "an", "de\u00b7nen", "d\u00fcn\u00b7nen", "H\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PRELS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Nur an dem Ende nicht, wo sich die Eyr ausbrei-", "tokens": ["Nur", "an", "dem", "En\u00b7de", "nicht", ",", "wo", "sich", "die", "Eyr", "aus\u00b7brei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PTKNEG", "$,", "PWAV", "PRF", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Woselbst ein kleines Loch mit Luft gef\u00fcllet bleibt,", "tokens": ["Wo\u00b7selbst", "ein", "klei\u00b7nes", "Loch", "mit", "Luft", "ge\u00b7f\u00fcl\u00b7let", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die Schale und die Haut ein wenig abwerts treibt.", "tokens": ["Die", "Scha\u00b7le", "und", "die", "Haut", "ein", "we\u00b7nig", "ab\u00b7werts", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der Dotter fa\u00dft in sich der Fr\u00fcchte ersten Saa-", "tokens": ["Der", "Dot\u00b7ter", "fa\u00dft", "in", "sich", "der", "Fr\u00fcch\u00b7te", "ers\u00b7ten", "Saa"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "ART", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Den wir den Urstof sonst, die Urbildung benah-", "tokens": ["Den", "wir", "den", "Ur\u00b7stof", "sonst", ",", "die", "Ur\u00b7bil\u00b7dung", "be\u00b7nah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Der wie ein weisser Ring an seiner Fl\u00e4che flie\u00dft,", "tokens": ["Der", "wie", "ein", "weis\u00b7ser", "Ring", "an", "sei\u00b7ner", "Fl\u00e4\u00b7che", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So lang das Ey noch frisch und unbebr\u00fctet ist.", "tokens": ["So", "lang", "das", "Ey", "noch", "frisch", "und", "un\u00b7be\u00b7br\u00fc\u00b7tet", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wenn es bebr\u00fctet wird; und sich der Ring aus-", "tokens": ["Wenn", "es", "be\u00b7br\u00fc\u00b7tet", "wird", ";", "und", "sich", "der", "Ring", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$.", "KON", "PRF", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "So wird daraus die Frucht recht wunderbahr ge-", "tokens": ["So", "wird", "da\u00b7raus", "die", "Frucht", "recht", "wun\u00b7der\u00b7bahr", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PAV", "ART", "NN", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Das d\u00fcnne Eyerwei\u00df n\u00e4hrt solche K\u00fcchelein,", "tokens": ["Das", "d\u00fcn\u00b7ne", "Ey\u00b7er\u00b7wei\u00df", "n\u00e4hrt", "sol\u00b7che", "K\u00fc\u00b7chel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So lange sie annoch in denen Schalen seyn,", "tokens": ["So", "lan\u00b7ge", "sie", "an\u00b7noch", "in", "de\u00b7nen", "Scha\u00b7len", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "APPR", "PRELS", "NN", "VAINF", "$,"], "meter": "-+--+-++-+-+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Es flie\u00dft die R\u00f6hren durch, die in dem Ey zu fin-", "tokens": ["Es", "flie\u00dft", "die", "R\u00f6h\u00b7ren", "durch", ",", "die", "in", "dem", "Ey", "zu", "fin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und durch die Nabelschnur sich mit der Frucht", "tokens": ["Und", "durch", "die", "Na\u00b7bel\u00b7schnur", "sich", "mit", "der", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Die Luftgef\u00fcllte H\u00f6h wird durch die W\u00e4rm erregt,", "tokens": ["Die", "Luft\u00b7ge\u00b7f\u00fcll\u00b7te", "H\u00f6h", "wird", "durch", "die", "W\u00e4rm", "er\u00b7regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die denn den Nahrungssaft zur Fl\u00fc\u00dfigkeit bewegt", "tokens": ["Die", "denn", "den", "Nah\u00b7rungs\u00b7saft", "zur", "Fl\u00fc\u00b7\u00dfig\u00b7keit", "be\u00b7wegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die R\u00f6hren offen h\u00e4lt, wodurch derselbe fliess