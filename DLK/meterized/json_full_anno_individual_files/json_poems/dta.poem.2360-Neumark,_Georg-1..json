{"dta.poem.2360": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Halt inn/ o gro\u00dfer Gott/ zu strafen/", "tokens": ["Halt", "inn", "/", "o", "gro\u00b7\u00dfer", "Gott", "/", "zu", "stra\u00b7fen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$(", "FM", "ADJA", "NN", "$(", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sich dein ", "tokens": ["Wenn", "sich", "dein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wenn Huld und Gunst bey Dir entschlafen/", "tokens": ["Wenn", "Huld", "und", "Gunst", "bey", "Dir", "ent\u00b7schla\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So straf doch nicht dein ", "tokens": ["So", "straf", "doch", "nicht", "dein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "PPOSAT"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ach schone meiner liebster ", "tokens": ["Ach", "scho\u00b7ne", "mei\u00b7ner", "liebs\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Schau wie Ich bin mit Angst beladen/", "tokens": ["Schau", "wie", "Ich", "bin", "mit", "Angst", "be\u00b7la\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie Ich mein b\u00f6ses ", "tokens": ["Wie", "Ich", "mein", "b\u00f6\u00b7ses"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ach Gott erbarme dich aus Gnaden/", "tokens": ["Ach", "Gott", "er\u00b7bar\u00b7me", "dich", "aus", "Gna\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich lege Mich zu deinen ", "tokens": ["Ich", "le\u00b7ge", "Mich", "zu", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ach H", "tokens": ["Ach", "H"], "token_info": ["word", "word"], "pos": ["ITJ", "XY"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ach nim Mich auf und sieh nicht an/", "tokens": ["Ach", "nim", "Mich", "auf", "und", "sieh", "nicht", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PTKVZ", "KON", "VVIMP", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Ich so wieder Dich gethan.", "tokens": ["Da\u00df", "Ich", "so", "wie\u00b7der", "Dich", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Errette Mich aus diesen N\u00f6hten/", "tokens": ["Er\u00b7ret\u00b7te", "Mich", "aus", "die\u00b7sen", "N\u00f6h\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rei\u00df Mich aus dieser ", "tokens": ["Rei\u00df", "Mich", "aus", "die\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die Mich ged\u00e4nket fast zu t\u00f6dten/", "tokens": ["Die", "Mich", "ge\u00b7d\u00e4n\u00b7ket", "fast", "zu", "t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Ich darinn nicht untergeh\u2019/", "tokens": ["Da\u00df", "Ich", "da\u00b7rinn", "nicht", "un\u00b7ter\u00b7geh'", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jm fall Du nicht wirst bey Mir stehn", "tokens": ["Jm", "fall", "Du", "nicht", "wirst", "bey", "Mir", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "VAFIN", "APPR", "NE", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So ist es bald mit Mir geschehn.", "tokens": ["So", "ist", "es", "bald", "mit", "Mir", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich mu\u00df es zwar mein Gott bekennen/", "tokens": ["Ich", "mu\u00df", "es", "zwar", "mein", "Gott", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df meine S\u00fcnd\u2019 und Missethat", "tokens": ["Da\u00df", "mei\u00b7ne", "S\u00fcnd'", "und", "Mis\u00b7se\u00b7that"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die rechte Quelle sey zu nennen/", "tokens": ["Die", "rech\u00b7te", "Quel\u00b7le", "sey", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "De\u00df/ was Mich nun befallen hat/", "tokens": ["De\u00df", "/", "was", "Mich", "nun", "be\u00b7fal\u00b7len", "hat", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWS", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich hat gest\u00fcrtzt in dieses ", "tokens": ["Mich", "hat", "ge\u00b7st\u00fcrtzt", "in", "die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PDAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dr\u00fcm will Ich gerne stille halten/", "tokens": ["Dr\u00fcm", "will", "Ich", "ger\u00b7ne", "stil\u00b7le", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein liebster Vater/ deine Huld/", "tokens": ["Mein", "liebs\u00b7ter", "Va\u00b7ter", "/", "dei\u00b7ne", "Huld", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Ich auch wieder werde froh.", "tokens": ["Da\u00df", "Ich", "auch", "wie\u00b7der", "wer\u00b7de", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}