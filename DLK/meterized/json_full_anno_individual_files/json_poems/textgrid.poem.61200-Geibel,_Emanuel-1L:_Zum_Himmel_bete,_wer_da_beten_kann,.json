{"textgrid.poem.61200": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zum Himmel bete, wer da beten kann,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Himmel bete, wer da beten kann,", "tokens": ["Zum", "Him\u00b7mel", "be\u00b7te", ",", "wer", "da", "be\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "PWS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wer nicht aufw\u00e4rts blickt nach einem Horte,", "tokens": ["Und", "wer", "nicht", "auf\u00b7w\u00e4rts", "blickt", "nach", "ei\u00b7nem", "Hor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der sag's dem Sturm, da\u00df er von Ort zu Orte", "tokens": ["Der", "sag's", "dem", "Sturm", ",", "da\u00df", "er", "von", "Ort", "zu", "Or\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es weiter trag' als einen Zauberbann.", "tokens": ["Es", "wei\u00b7ter", "trag'", "als", "ei\u00b7nen", "Zau\u00b7ber\u00b7bann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der S\u00e4ugling, der zu stammeln kaum begann,", "tokens": ["Der", "S\u00e4ug\u00b7ling", ",", "der", "zu", "stam\u0