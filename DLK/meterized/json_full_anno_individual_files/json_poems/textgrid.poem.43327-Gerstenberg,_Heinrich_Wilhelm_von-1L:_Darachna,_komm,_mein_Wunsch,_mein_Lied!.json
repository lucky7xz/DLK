en_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "KOUS", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus H\u00f6hlen oder W\u00e4ldern droht!", "tokens": ["Aus", "H\u00f6h\u00b7len", "o\u00b7der", "W\u00e4l\u00b7dern", "droht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn eine Schlange sie umflicht,", "tokens": ["Wenn", "ei\u00b7ne", "Schlan\u00b7ge", "sie", "um\u00b7flicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Crocodill sie hascht, ein Scorpion sie sticht!", "tokens": ["Ein", "Cro\u00b7co\u00b7dill", "sie", "hascht", ",", "ein", "Scor\u00b7pi\u00b7on", "sie", "sticht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eh treff ein Donner euch! Scheusale! wagt es nicht.", "tokens": ["Eh", "treff", "ein", "Don\u00b7ner", "euch", "!", "Scheu\u00b7sa\u00b7le", "!", "wagt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "$.", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.13": {"line.1": {"text": "Mein Herz, mein Herz fleucht ihr entgegen:", "tokens": ["Mein", "Herz", ",", "mein", "Herz", "fleucht", "ihr", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will an ihre Brust mich legen,", "tokens": ["Ich", "will", "an", "ih\u00b7re", "Brust", "mich", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das kleinste R\u00f6cheln sp\u00e4hn, und horchen, wie sie schl\u00e4gt,", "tokens": ["Das", "kleins\u00b7te", "R\u00f6\u00b7cheln", "sp\u00e4hn", ",", "und", "hor\u00b7chen", ",", "wie", "sie", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und forschen, wo der Tod sich regt.", "tokens": ["Und", "for\u00b7schen", ",", "wo", "der", "Tod", "sich", "reg\u00b7t."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "PRF", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wie Ambraduft will ich dich, Tod!", "tokens": ["Wie", "Am\u00b7bra\u00b7duft", "will", "ich", "dich", ",", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "VMFIN", "PPER", "PRF", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit jedem Odemzug aus ihren Adern trinken,", "tokens": ["Mit", "je\u00b7dem", "O\u00b7dem\u00b7zug", "aus", "ih\u00b7ren", "A\u00b7dern", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf ihren matten Busen sinken,", "tokens": ["Auf", "ih\u00b7ren", "mat\u00b7ten", "Bu\u00b7sen", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit ihm sterben \u2013 s\u00fc\u00dfer Tod!", "tokens": ["Und", "mit", "ihm", "ster\u00b7ben", "\u2013", "s\u00fc\u00b7\u00dfer", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVINF", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}