ra"}, "line.2": {"text": "Rausch mirs her\u00fcber, wo sie ist.", "tokens": ["Rausch", "mirs", "her\u00b7\u00fc\u00b7ber", ",", "wo", "sie", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Du rauschend Laub in Cederw\u00e4ldern,", "tokens": ["Du", "rau\u00b7schend", "Laub", "in", "Ce\u00b7der\u00b7w\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sag mir es, wo mein M\u00e4dchen ist.", "tokens": ["Sag", "mir", "es", ",", "wo", "mein", "M\u00e4d\u00b7chen", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich harre f\u00fchllos, da\u00df der Sand", "tokens": ["Ich", "har\u00b7re", "f\u00fchl\u00b7los", ",", "da\u00df", "der", "Sand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fersen mir verzehrt, und meine Seufzer wecken", "tokens": ["Die", "Fer\u00b7sen", "mir", "ver\u00b7zehrt", ",", "und", "mei\u00b7ne", "Seuf\u00b7zer", "we\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVPP", "$,", "KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Tieger dieses Hayns, die, durch den Durst entbrannt,", "tokens": ["Die", "Tie\u00b7ger", "die\u00b7ses", "Hayns", ",", "die", ",", "durch", "den", "Durst", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,", "PRELS", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weh mir! mein Blut von ferne lecken.", "tokens": ["Weh", "mir", "!", "mein", "Blut", "von", "fer\u00b7ne", "le\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPOSAT", "NN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Sonne! wenn auch ihr der Tod", "tokens": ["O", "Son\u00b7ne", "!", "wenn", "auch", "ihr", "der", "Tod"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "KOUS", "ADV", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus H\u00f6hlen oder W\u00e4ldern droht!", "tokens": ["Aus", "H\u00f6h\u00b7len", "o\u00b7der", "W\u00e4l\u00b7dern", "droht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn eine Schlange sie umflicht,", "tokens": ["Wenn", "ei\u00b7ne", "Schlan\u00b7ge", "sie", "um\u00b7flicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Crocodill sie hascht, ein Scorpion sie sticht!", "tokens": ["Ein", "Cro\u00b7co\u00b7dill", "sie", "hascht", ",", "ein", "Scor\u00b7pi\u00b7on", "sie", "sticht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eh treff ein Donner euch! Scheusale! wagt es nicht.", "tokens": ["Eh", "treff", "ein", "Don\u00b7ner", "euch", "!", "Scheu\u00b7sa\u00b7le", "!", "wagt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "$.", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.6": {"line.1": {"text": "Mein Herz, mein Herz fleucht ihr entgegen:", "tokens": ["Mein", "Herz", ",", "mein", "Herz", "fleucht", "ihr", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich will an ihre Brust mich legen,", "tokens": ["Ich", "will", "an", "ih\u00b7re", "Brust", "mich", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das kleinste R\u00f6cheln sp\u00e4hn, und horchen, wie sie schl\u00e4gt,", "tokens": ["Das", "kleins\u00b7te", "R\u00f6\u00b7cheln", "sp\u00e4hn", ",", "und", "hor\u00b7chen", ",", "wie", "sie", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und forschen, wo der Tod sich regt.", "tokens": ["Und", "for\u00b7schen", ",", "wo", "der", "Tod", "sich", "reg\u00b7t."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["