{"dta.poem.2263": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer ist, o Pamela, wer ist, der dich mit Recht", "tokens": ["Wer", "ist", ",", "o", "Pa\u00b7me\u00b7la", ",", "wer", "ist", ",", "der", "dich", "mit", "Recht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "FM", "NE", "$,", "PWS", "VAFIN", "$,", "PRELS", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der niedern Abkunft wegen tadelt?", "tokens": ["Der", "nie\u00b7dern", "Ab\u00b7kunft", "we\u00b7gen", "ta\u00b7delt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Weiser sieht auf kein Geschlecht:", "tokens": ["Ein", "Wei\u00b7ser", "sieht", "auf", "kein", "Ge\u00b7schlecht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begl\u00fcckter Edelmann, den Pamela geadelt!", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "E\u00b7del\u00b7mann", ",", "den", "Pa\u00b7me\u00b7la", "ge\u00b7a\u00b7delt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}