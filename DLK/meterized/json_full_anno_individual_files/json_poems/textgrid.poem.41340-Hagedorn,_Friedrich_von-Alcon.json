{"textgrid.poem.41340": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Alcon", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Apollo stand bet\u00e4ubt durch S\u00f6hne seiner Kunst,", "tokens": ["A\u00b7pol\u00b7lo", "stand", "be\u00b7t\u00e4ubt", "durch", "S\u00f6h\u00b7ne", "sei\u00b7ner", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVPP", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn jeder singt ihm Dank, oft f\u00fcr weit gr\u00f6\u00dfre Gunst,", "tokens": ["Denn", "je\u00b7der", "singt", "ihm", "Dank", ",", "oft", "f\u00fcr", "weit", "gr\u00f6\u00df\u00b7re", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "NN", "$,", "ADV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als ihm der Gott gew\u00e4hrt, und nach verrauschten Ch\u00f6ren", "tokens": ["Als", "ihm", "der", "Gott", "ge\u00b7w\u00e4hrt", ",", "und", "nach", "ver\u00b7rauschten", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bat Alcon insgeheim Apoll um neue Lehren.", "tokens": ["Bat", "Al\u00b7con", "ins\u00b7ge\u00b7heim", "A\u00b7poll", "um", "neu\u00b7e", "Leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Er kam, vergn\u00fcgt, zur\u00fcck. Gleich denkt die ganze Schaar:", "tokens": ["Er", "kam", ",", "ver\u00b7gn\u00fcgt", ",", "zu\u00b7r\u00fcck", ".", "Gleich", "denkt", "die", "gan\u00b7ze", "Schaar", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was wird denn eben dem, vor andern, offenbar?", "tokens": ["Was", "wird", "denn", "e\u00b7ben", "dem", ",", "vor", "an\u00b7dern", ",", "of\u00b7fen\u00b7bar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "$,", "APPR", "PIS", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und einer rief ihm zu: Nun bist du, frei von Fehde,", "tokens": ["Und", "ei\u00b7ner", "rief", "ihm", "zu", ":", "Nun", "bist", "du", ",", "frei", "von", "Feh\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Voll Gottheit, voll Olymp. Umstirnt mit Wahrheit, rede", "tokens": ["Voll", "Got\u00b7theit", ",", "voll", "O\u00b7lymp", ".", "Um\u00b7stirnt", "mit", "Wahr\u00b7heit", ",", "re\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "NN", "$,", "ADJD", "NN", "$.", "VVPP", "APPR", "NN", "$,", "VVFIN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Aetherisc