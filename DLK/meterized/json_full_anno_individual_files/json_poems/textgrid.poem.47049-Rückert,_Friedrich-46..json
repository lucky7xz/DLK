{"textgrid.poem.47049": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "46.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte dich in Sammet und in Seide", "tokens": ["Ich", "hat\u00b7te", "dich", "in", "Sam\u00b7met", "und", "in", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "NN", "KON", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geh\u00fcllt, dich angethan mit Purpurzonen;", "tokens": ["Ge\u00b7h\u00fcllt", ",", "dich", "an\u00b7ge\u00b7than", "mit", "Pur\u00b7pur\u00b7zo\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich hatte dir aufs Haupt gesetzet Kronen,", "tokens": ["Ich", "hat\u00b7te", "dir", "aufs", "Haupt", "ge\u00b7set\u00b7zet", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir um die Brust geleget Goldgeschmeide.", "tokens": ["Dir", "um", "die", "Brust", "ge\u00b7le\u00b7get", "Gold\u00b7ge\u00b7schmei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Thu' von dir den geborgten Schmuck, entkleide", "tokens": ["Thu'", "von", "dir", "den", "ge\u00b7borg\u00b7ten", "Schmuck", ",", "ent\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "