 "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "Beim Anblick seiner vollen Tenne", "tokens": ["Beim", "An\u00b7blick", "sei\u00b7ner", "vol\u00b7len", "Ten\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Frischgefallenen Segens,", "tokens": ["Frischge\u00b7fal\u00b7le\u00b7nen", "Se\u00b7gens", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Unterm Sichelschnitt gefallen,", "tokens": ["Un\u00b7term", "Si\u00b7chel\u00b7schnitt", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "J\u00e4h, weinend,", "tokens": ["J\u00e4h", ",", "wei\u00b7nend", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Wer h\u00f6rte das Weinen gem\u00e4heter Halme?", "tokens": ["Wer", "h\u00f6r\u00b7te", "das", "Wei\u00b7nen", "ge\u00b7m\u00e4\u00b7he\u00b7ter", "Hal\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.21": {"text": "Wie der Bauer,", "tokens": ["Wie", "der", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.22": {"text": "So lacht meine Seele und freut sich", "tokens": ["So", "lacht", "mei\u00b7ne", "See\u00b7le", "und", "freut", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PRF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.23": {"text": "Ihres mehrenden Reichtums.", "tokens": ["Ih\u00b7res", "meh\u00b7ren\u00b7den", "Reich\u00b7tums", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.24": {"text": "Wie vieles danke ich euch!", "tokens": ["Wie", "vie\u00b7les", "dan\u00b7ke", "ich", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "Alles vielleicht! \u2013", "tokens": ["Al\u00b7les", "viel\u00b7leicht", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["PIS", "ADV", "$.", "$("], "meter": "+--+", "measure": "iambic.di.chol"}}}}}