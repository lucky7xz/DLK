{"textgrid.poem.55324": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Howards Ehrenged\u00e4chtnis", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Gottheit ", "tokens": ["Wenn", "Got\u00b7theit"], "token_info": ["word", "word"], "pos": ["KOUS", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Durch L\u00fcfte schwankend wandelt leicht und schwer,", "tokens": ["Durch", "L\u00fcf\u00b7te", "schwan\u00b7kend", "wan\u00b7delt", "leicht", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Schleiers Falten sammelt, sie zerstreut,", "tokens": ["Des", "Schlei\u00b7ers", "Fal\u00b7ten", "sam\u00b7melt", ",", "sie", "zer\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Am Wechsel der Gestalten sich erfreut,", "tokens": ["Am", "Wech\u00b7sel", "der", "Ge\u00b7stal\u00b7ten", "sich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jetzt starr sich h\u00e4lt, dann schwindet wie ein Traum,", "tokens": ["Jetzt", "starr", "sich", "h\u00e4lt", ",", "dann", "schwin\u00b7det", "wie", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVFIN", "$,", "ADV", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da staunen wir und traun dem Auge kaum;", "tokens": ["Da", "stau\u00b7nen", "wir", "und", "traun", "dem", "Au\u00b7ge", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun regt sich k\u00fchn des eignen Bildens Kraft,", "tokens": ["Nun", "regt", "sich", "k\u00fchn", "des", "eig\u00b7nen", "Bil\u00b7dens", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Unbestimmtes zu Bestimmtem schafft;", "tokens": ["Die", "Un\u00b7be\u00b7stimm\u00b7tes", "zu", "Be\u00b7stimm\u00b7tem", "schafft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da droht ein Leu, dort wogt ein Elefant,", "tokens": ["Da", "droht", "ein", "Leu", ",", "dort", "wogt", "ein", "E\u00b7le\u00b7fant", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kameles Hals, zum Drachen umgewandt,", "tokens": ["Ka\u00b7me\u00b7les", "Hals", ",", "zum", "Dra\u00b7chen", "um\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Heer zieht an, doch triumphiert es nicht,", "tokens": ["Ein", "Heer", "zieht", "an", ",", "doch", "tri\u00b7um\u00b7phiert", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da es die Macht am steilen Felsen bricht;", "tokens": ["Da", "es", "die", "Macht", "am", "stei\u00b7len", "Fel\u00b7sen", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der treuste Wolkenbote selbst zerstiebt,", "tokens": ["Der", "treus\u00b7te", "Wol\u00b7ken\u00b7bo\u00b7te", "selbst", "zer\u00b7stiebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Eh er die Fern erreicht, wohin man liebt.", "tokens": ["Eh", "er", "die", "Fern", "er\u00b7reicht", ",", "wo\u00b7hin", "man", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er aber, Howard, gibt mit reinem Sinn", "tokens": ["Er", "a\u00b7ber", ",", "Ho\u00b7ward", ",", "gibt", "mit", "rei\u00b7nem", "Sinn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "NE", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uns neuer Lehre herrlichsten Gewinn.", "tokens": ["Uns", "neu\u00b7er", "Leh\u00b7re", "herr\u00b7lichs\u00b7ten", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was sich nicht halten, nicht erreichen l\u00e4\u00dft,", "tokens": ["Was", "sich", "nicht", "hal\u00b7ten", ",", "nicht", "er\u00b7rei\u00b7chen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er fa\u00dft es an, er h\u00e4lt zuerst es fest;", "tokens": ["Er", "fa\u00dft", "es", "an", ",", "er", "h\u00e4lt", "zu\u00b7erst", "es", "fest", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bestimmt das Unbestimmte, schr\u00e4nkt es ein,", "tokens": ["Be\u00b7stimmt", "das", "Un\u00b7be\u00b7stimm\u00b7te", ",", "schr\u00e4nkt", "es", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Benennt es treffend! \u2013 Sei die Ehre dein! \u2013", "tokens": ["Be\u00b7nennt", "es", "tref\u00b7fend", "!", "\u2013", "Sei", "die", "Eh\u00b7re", "dein", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "$(", "VAFIN", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie Streife steigt, sich ballt, zerflattert, f\u00e4llt,", "tokens": ["Wie", "Strei\u00b7fe", "steigt", ",", "sich", "ballt", ",", "zer\u00b7flat\u00b7tert", ",", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct