{"dta.poem.21711": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Atlantis .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Insel der Uranionen,", "tokens": ["In\u00b7sel", "der", "U\u00b7ra\u00b7ni\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wo in Thetis blauer Schooss", "tokens": ["Wo", "in", "The\u00b7tis", "blau\u00b7er", "Scho\u00b7oss"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Heroen selig wohnen,", "tokens": ["Die", "He\u00b7roen", "se\u00b7lig", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Harmlos, m\u00fchlos, wandellos;", "tokens": ["Harm\u00b7los", ",", "m\u00fch\u00b7los", ",", "wan\u00b7del\u00b7los", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heymath unbescholtner Triebe,", "tokens": ["Hey\u00b7math", "un\u00b7be\u00b7scholt\u00b7ner", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freystatt, die kein Zwist entweiht,", "tokens": ["Freys\u00b7tatt", ",", "die", "kein", "Zwist", "ent\u00b7weiht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.7": {"text": "Wo sich nimmer mit dem Triebe", "tokens": ["Wo", "sich", "nim\u00b7mer", "mit", "dem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "St\u00f6rrig das Gesetz entzweyt;", "tokens": ["St\u00f6r\u00b7rig", "das", "Ge\u00b7setz", "ent\u00b7zweyt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo sich schliessen alle Wunden,", "tokens": ["Wo", "sich", "schlies\u00b7sen", "al\u00b7le", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Jede Quetschung sich vernarbt \u2014", "tokens": ["Je\u00b7de", "Quet\u00b7schung", "sich", "ver\u00b7narbt"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Hab' ich, Insel, dich gefunden,", "tokens": ["Hab'", "ich", ",", "In\u00b7sel", ",", "dich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wo nicht Sinn noch Seele darbt?", "tokens": ["Wo", "nicht", "Sinn", "noch", "See\u00b7le", "darbt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Welche K\u00fchle, welche Frische", "tokens": ["Wel\u00b7che", "K\u00fch\u00b7le", ",", "wel\u00b7che", "Fri\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAT", "NN", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der balsamreichen Luft!", "tokens": ["In", "der", "bal\u00b7sam\u00b7rei\u00b7chen", "Luft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welches S\u00e4useln im Geb\u00fcsche!", "tokens": ["Wel\u00b7ches", "S\u00e4u\u00b7seln", "im", "Ge\u00b7b\u00fc\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welches Hauchen! welch ein Duft!", "tokens": ["Wel\u00b7ches", "Hau\u00b7chen", "!", "welch", "ein", "Duft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$.", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Murmelnd durch bebl\u00fcmte Matten", "tokens": ["Mur\u00b7melnd", "durch", "be\u00b7bl\u00fcm\u00b7te", "Mat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schl\u00e4ngelt sich des Bachs Krystall,", "tokens": ["Schl\u00e4n\u00b7gelt", "sich", "des", "Bachs", "Krys\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rings erklingt aus gr\u00fcnen Schatten", "tokens": ["Rings", "er\u00b7klingt", "aus", "gr\u00fc\u00b7nen", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wachtelschlag und Drosselschall.", "tokens": ["Wach\u00b7tel\u00b7schlag", "und", "Dros\u00b7sel\u00b7schall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wahrlich, dieses \u00c4thers Schimmer", "tokens": ["Wahr\u00b7lich", ",", "die\u00b7ses", "\u00c4\u00b7thers", "Schim\u00b7mer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "W\u00f6lkte nie des Nebels Grau.", "tokens": ["W\u00f6lk\u00b7te", "nie", "des", "Ne\u00b7bels", "Grau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nimmer tr\u00fcbten St\u00fcrme, nimmer", "tokens": ["Nim\u00b7mer", "tr\u00fcb\u00b7ten", "St\u00fcr\u00b7me", ",", "nim\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dieser Fluthen spiegelnd Blau.", "tokens": ["Die\u00b7ser", "Flut\u00b7hen", "spie\u00b7gelnd", "Blau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Schmeichell\u00fcfte, lau und linde,", "tokens": ["Schmei\u00b7chel\u00b7l\u00fcf\u00b7te", ",", "lau", "und", "lin\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lullen Sinn und Seel' in Rast;", "tokens": ["Lul\u00b7len", "Sinn", "und", "Seel'", "in", "Rast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dem Aug' entrollt die Binde,", "tokens": ["Und", "dem", "Aug'", "ent\u00b7rollt", "die", "Bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vom Nacken sinkt die Last.", "tokens": ["Und", "vom", "Na\u00b7cken", "sinkt", "die", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In dem reinern Elemente", "tokens": ["In", "dem", "rei\u00b7nern", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.6": {"text": "W\u00f6lbet m\u00e4cht'ger sich die Brust.", "tokens": ["W\u00f6l\u00b7bet", "m\u00e4cht'\u00b7ger", "sich", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aus dem labenden Nepenthe", "tokens": ["Aus", "dem", "la\u00b7ben\u00b7den", "Ne\u00b7pen\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.8": {"text": "Schl\u00fcrft der Geist D\u00e4monenlust.", "tokens": ["Schl\u00fcrft", "der", "Geist", "D\u00e4\u00b7mo\u00b7nen\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Jedes Jammers wird vergessen,", "tokens": ["Je\u00b7des", "Jam\u00b7mers", "wird", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Jeder Sorge Dunst verfliegt.", "tokens": ["Je\u00b7der", "Sor\u00b7ge", "Dunst", "ver\u00b7fliegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Alle Schranke ward durchmessen,", "tokens": ["Al\u00b7le", "Schran\u00b7ke", "ward", "durch\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und das Kleinod ist ersiegt.", "tokens": ["Und", "das", "Klei\u00b7nod", "ist", "er\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6her schl\u00e4gt das Herz und freyer,", "tokens": ["H\u00f6\u00b7her", "schl\u00e4gt", "das", "Herz", "und", "frey\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer den schw\u00fclen Kampf bestand.", "tokens": ["Wer", "den", "schw\u00fc\u00b7len", "Kampf", "be\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wahrheit l\u00fcpft den Wolkenschleyer,", "tokens": ["Wahr\u00b7heit", "l\u00fcpft", "den", "Wol\u00b7ken\u00b7schle\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6nheit ihm ihr Duftgewand.", "tokens": ["Sch\u00f6n\u00b7heit", "ihm", "ihr", "Duft\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes Dunkel ist gelichtet,", "tokens": ["Je\u00b7des", "Dun\u00b7kel", "ist", "ge\u00b7lich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gar zerflattert Trug und Wahn.", "tokens": ["Gar", "zer\u00b7flat\u00b7tert", "Trug", "und", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jede Zwietracht ist geschlichtet,", "tokens": ["Je\u00b7de", "Zwiet\u00b7racht", "ist", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aller Hader abgethan.", "tokens": ["Al\u00b7ler", "Ha\u00b7der", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "In den Frohsinn leichter Jugend", "tokens": ["In", "den", "Froh\u00b7sinn", "leich\u00b7ter", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Grollt nicht hie des Alters Harm,", "tokens": ["Grollt", "nicht", "hie", "des", "Al\u00b7ters", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und der Wollust sinkt die Tugend", "tokens": ["Und", "der", "Wol\u00b7lust", "sinkt", "die", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Liebend in den Schwesterarm.", "tokens": ["Lie\u00b7bend", "in", "den", "Schwes\u00b7ter\u00b7arm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nein, der Sch\u00f6nheit Heiligthume", "tokens": ["Nein", ",", "der", "Sch\u00f6n\u00b7heit", "Hei\u00b7lig\u00b7thu\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Naht kein Feind des Erebus.", "tokens": ["Naht", "kein", "Feind", "des", "E\u00b7re\u00b7bus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Des Genusses zarte Blume", "tokens": ["Des", "Ge\u00b7nus\u00b7ses", "zar\u00b7te", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Knickt nicht hier der \u00dcberdruss.", "tokens": ["Knickt", "nicht", "hier", "der", "\u00dc\u00b7berd\u00b7russ", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nie wird Wehmuth hier zur Schwermuth.", "tokens": ["Nie", "wird", "Weh\u00b7muth", "hier", "zur", "Schwer\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das Entz\u00fccken nie zur Pein;", "tokens": ["Das", "Ent\u00b7z\u00fc\u00b7cken", "nie", "zur", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nie verf\u00e4lscht der Reue Wermuth", "tokens": ["Nie", "ver\u00b7f\u00e4lscht", "der", "Reu\u00b7e", "Wer\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Unsrer Freuden edlen Wein.", "tokens": ["Uns\u00b7rer", "Freu\u00b7den", "ed\u00b7len", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Nein, die Frucht der Hesperiden", "tokens": ["Nein", ",", "die", "Frucht", "der", "Hes\u00b7pe\u00b7ri\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "H\u00fctet hier kein H\u00f6llenhund;", "tokens": ["H\u00fc\u00b7tet", "hier", "kein", "H\u00f6l\u00b7len\u00b7hund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und die furchtbarn Eumeniden", "tokens": ["Und", "die", "furcht\u00b7barn", "Eu\u00b7me\u00b7ni\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Scheuen den geweihten Grund.", "tokens": ["Scheu\u00b7en", "den", "ge\u00b7weih\u00b7ten", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.