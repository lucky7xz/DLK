{"dta.poem.1902": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Kein Ubel ist so rasch und schnell/ als das Geschrey/", "tokens": ["Kein", "U\u00b7bel", "ist", "so", "rasch", "und", "schnell", "/", "als", "das", "Ge\u00b7schrey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$(", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Wachsthum ist ein Lauff/ Bewegung ist sein Leben/", "tokens": ["Sein", "Wach\u00b7sthum", "ist", "ein", "Lauff", "/", "Be\u00b7we\u00b7gung", "ist", "sein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sein erste Frucht ist klein/ bald lernt es sich erheben/", "tokens": ["Sein", "ers\u00b7te", "Frucht", "ist", "klein", "/", "bald", "lernt", "es", "sich", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$(", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geht auff der Erd und sezt das Haubt den Wolcken bey.", "tokens": ["Geht", "auff", "der", "Erd", "und", "sezt", "das", "Haubt", "den", "Wol\u00b7cken", "bey", "."], "toke