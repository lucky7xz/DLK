{"textgrid.poem.42931": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Kuttel Daddeldu \u00fcber Nobile", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So gro\u00dfe Kerle gingen tot.", "tokens": ["So", "gro\u00b7\u00dfe", "Ker\u00b7le", "gin\u00b7gen", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott wei\u00df, was fern in h\u00f6chster Not", "tokens": ["Gott", "wei\u00df", ",", "was", "fern", "in", "h\u00f6chs\u00b7ter", "Not"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PRELS", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch heute k\u00e4mpft, vom Eis umklammert,", "tokens": ["Noch", "heu\u00b7te", "k\u00e4mpft", ",", "vom", "Eis", "um\u00b7klam\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr dieses Gro\u00dfmaul, das jetzt jammert", "tokens": ["F\u00fcr", "die\u00b7ses", "Gro\u00df\u00b7maul", ",", "das", "jetzt", "jam\u00b7mert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Um seinen angequetschten Zeh.", "tokens": ["Um", "sei\u00b7nen", "an\u00b7ge\u00b7quetschten", "Zeh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Wann hat ein Captain je in See", "tokens": ["Wann", "hat", "ein", "Cap\u00b7ta\u00b7in", "je", "in", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als erster seine Crew verlassen?!", "tokens": ["Als", "ers\u00b7ter", "sei\u00b7ne", "Crew", "ver\u00b7las\u00b7sen", "?!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem m\u00f6cht ich in die Kiemen fassen!", "tokens": ["Dem", "m\u00f6cht", "ich", "in", "die", "Kie\u00b7men", "fas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, da\u00df sie den gerettet haben!", "tokens": ["Ach", ",", "da\u00df", "sie", "den", "ge\u00b7ret\u00b7tet", "ha\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ART", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er m\u00fc\u00dfte, tief ins Eis gegraben,", "tokens": ["Er", "m\u00fc\u00df\u00b7te", ",", "tief", "ins", "Eis", "ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einem Lorbeerstock im Hintern,", "tokens": ["Mit", "ei\u00b7nem", "Lor\u00b7beer\u00b7stock", "im", "Hin\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Solang die Welt steht, \u00fcberwintern.", "tokens": ["So\u00b7lang", "die", "Welt", "steht", ",", "\u00fc\u00b7berw\u00b7in\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Verflucht, ich kann nicht richtig beten,", "tokens": ["Ver\u00b7flucht", ",", "ich", "kann", "nicht", "rich\u00b7tig", "be\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch hab ich eine solche Wut.", "tokens": ["Doch", "hab", "ich", "ei\u00b7ne", "sol\u00b7che", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gott sei zu Amundsen recht gut.", "tokens": ["Gott", "sei", "zu", "A\u00b7mund\u00b7sen", "recht", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NE", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Und wenn mir Nobile begegnet,", "tokens": ["Und", "wenn", "mir", "No\u00b7bi\u00b7le", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Will ich ihm das Ged\u00e4rm zerkneten", "tokens": ["Will", "ich", "ihm", "das", "Ge\u00b7d\u00e4rm", "zer\u00b7kne\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihn und sein ihm teures Leben", "tokens": ["Und", "ihn", "und", "sein", "ihm", "teu\u00b7res", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "KON", "PPOSAT", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "An andre F\u00e4uste weitergeben,", "tokens": ["An", "and\u00b7re", "F\u00e4us\u00b7te", "wei\u00b7ter\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So, da\u00df er Luft und Wasser segnet.", "tokens": ["So", ",", "da\u00df", "er", "Luft", "und", "Was\u00b7ser", "seg\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So gro\u00dfe Kerle gingen tot.", "tokens": ["So", "gro\u00b7\u00dfe", "Ker\u00b7le", "gin\u00b7gen", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott wei\u00df, was fern in h\u00f6chster Not", "tokens": ["Gott", "wei\u00df", ",", "was", "fern", "in", "h\u00f6chs\u00b7ter", "Not"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PRELS", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch heute k\u00e4mpft, vom Eis umklammert,", "tokens": ["Noch", "heu\u00b7te", "k\u00e4mpft", ",", "vom", "Eis", "um\u00b7klam\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "m