{"textgrid.poem.40544": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: S\u00fc\u00df mag das Aug' des Sterbenden sich schlie\u00dfen,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df mag das Aug' des Sterbenden sich schlie\u00dfen,", "tokens": ["S\u00fc\u00df", "mag", "das", "Aug'", "des", "Ster\u00b7ben\u00b7den", "sich", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Freundesthr\u00e4nen auf der Stirne f\u00fchlt,", "tokens": ["Der", "Freun\u00b7dest\u00b7hr\u00e4\u00b7nen", "auf", "der", "Stir\u00b7ne", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die drauf wie eine Todestaufe flie\u00dfen,", "tokens": ["Die", "drauf", "wie", "ei\u00b7ne", "To\u00b7de\u00b7stau\u00b7fe", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "KOKOM", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sich der bange Schwei\u00df des Sterbens k\u00fchlt.", "tokens": ["Da\u00df", "sich", "der", "ban\u00b7ge", "Schwei\u00df", "des", "Ster\u00b7bens", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch G\u00f6tterloos ist's, unbeweint zu scheiden,", "tokens": ["Doch", "G\u00f6t\u00b7ter\u00b7loos", "ist's", ",", "un\u00b7be\u00b7weint", "zu", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn man der Thr\u00e4nen und der Trauer werth!", "tokens": ["Wenn", "man", "der", "Thr\u00e4\u00b7nen", "und", "der", "Trau\u00b7er", "werth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wozu soll eine Seele um sie leiden,", "tokens": ["Wo\u00b7zu", "soll", "ei\u00b7ne", "See\u00b7le", "um", "sie", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn die Vollendung zu den Sternen f\u00e4hrt?", "tokens": ["Wenn", "die", "Vol\u00b7len\u00b7dung", "zu", "den", "Ster\u00b7nen", "f\u00e4hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ja, G\u00f6tterloos ist's, unbeweint zu scheiden,", "tokens": ["Ja", ",", "G\u00f6t\u00b7ter\u00b7loos", "ist's", ",", "un\u00b7be\u00b7weint", "zu", "schei\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NE", "NE", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu scheiden wie der Tag im Abendroth.", "tokens": ["Zu", "schei\u00b7den", "wie", "der", "Tag", "im", "A\u00b7ben\u00b7droth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er gab uns W\u00e4rme, Licht genug und Freuden,", "tokens": ["Er", "gab", "uns", "W\u00e4r\u00b7me", ",", "Licht", "ge\u00b7nug", "und", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und zieht dahin, weil seine Zeit gebot!", "tokens": ["Und", "zieht", "da\u00b7hin", ",", "weil", "sei\u00b7ne", "Zeit", "ge\u00b7bot", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zu fallen wie ein Feld voll goldner Aehren,", "tokens": ["Zu", "fal\u00b7len", "wie", "ein", "Feld", "voll", "gold\u00b7ner", "A\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOKOM", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die schlank gewallt im gr\u00fcnen Jugendkleid,", "tokens": ["Die", "schlank", "ge\u00b7wallt", "im", "gr\u00fc\u00b7nen", "Ju\u00b7gend\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nun ihr lastend Haupt zur Erde kehren.", "tokens": ["Doch", "nun", "ihr", "las\u00b7tend", "Haupt", "zur", "Er\u00b7de", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "ADJD", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer weint darob, da\u00df es nun Erntezeit?", "tokens": ["Wer", "weint", "da\u00b7rob", ",", "da\u00df", "es", "nun", "Ern\u00b7te\u00b7zeit", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PAV", "$,", "KOUS", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In Nacht zu sinken wie des Meeres Wogen,", "tokens": ["In", "Nacht", "zu", "sin\u00b7ken", "wie", "des", "Mee\u00b7res", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Drauf Sonnenglanz, Goldwimpel, reiche Fracht,", "tokens": ["Drauf", "Son\u00b7nen\u00b7glanz", ",", "Gold\u00b7wim\u00b7pel", ",", "rei\u00b7che", "Fracht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "NN", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gesang und Schw\u00e4ne tages\u00fcber zogen \u2013", "tokens": ["Ge\u00b7sang", "und", "Schw\u00e4\u00b7ne", "ta\u00b7ges\u00b7\u00fc\u00b7ber", "zo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Zeit ist um, ihr Recht will auch die Nacht!", "tokens": ["Die", "Zeit", "ist", "um", ",", "ihr", "Recht", "will", "auch", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "$,", "PPOSAT", "NN", "VMFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und zu zerst\u00e4uben wie die fl\u00fccht'ge Wolke!", "tokens": ["Und", "zu", "zer\u00b7st\u00e4u\u00b7ben", "wie", "die", "fl\u00fccht'\u00b7ge", "Wol\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie hat Gedeihn geregnet auf die Flur,", "tokens": ["Sie", "hat", "Ge\u00b7deihn", "ge\u00b7reg\u00b7net", "auf", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Friedensbogen hell gezeigt dem Volke,", "tokens": ["Den", "Frie\u00b7dens\u00b7bo\u00b7gen", "hell", "ge\u00b7zeigt", "dem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00f6st sich nun in leuchtenden Azur.", "tokens": ["Und", "l\u00f6st", "sich", "nun", "in", "leuch\u00b7ten\u00b7den", "A\u00b7zur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "So schied auch Er, der nun dahingegangen,", "tokens": ["So", "schied", "auch", "Er", ",", "der", "nun", "da\u00b7hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "PRELS", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der hohe Mann, der kr\u00e4ft'ge Dichtergreis,", "tokens": ["Der", "ho\u00b7he", "Mann", ",", "der", "kr\u00e4ft'\u00b7ge", "Dich\u00b7ter\u00b7greis", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf dessen Lipp', auf dessen bleichen Wangen", "tokens": ["Auf", "des\u00b7sen", "Lipp'", ",", "auf", "des\u00b7sen", "blei\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "$,", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Ku\u00df des Gl\u00fccks noch jetzt vergl\u00fchet leis.", "tokens": ["Der", "Ku\u00df", "des", "Gl\u00fccks", "noch", "jetzt", "ver\u00b7gl\u00fc\u00b7het", "leis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Ein kalter starrer Arm, reglos gebeuget,", "tokens": ["Ein", "kal\u00b7ter", "star\u00b7rer", "Arm", ",", "reg\u00b7los", "ge\u00b7beu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dem die goldne Leier lichtvoll blitzt;", "tokens": ["In", "dem", "die", "gold\u00b7ne", "Lei\u00b7er", "licht\u00b7voll", "blitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein greises Silberhaupt, im Tod geneiget,", "tokens": ["Ein", "grei\u00b7ses", "Sil\u00b7ber\u00b7haupt", ",", "im", "Tod", "ge\u00b7nei\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drauf immergr\u00fcn der frische Lorbeer sitzt!", "tokens": ["Drauf", "im\u00b7mer\u00b7gr\u00fcn", "der", "fri\u00b7sche", "Lor\u00b7beer", "sitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sah die\u00df mein Aug', nie konnt' es Thr\u00e4nen thauen!", "tokens": ["Sah", "die\u00df", "mein", "Aug'", ",", "nie", "konnt'", "es", "Thr\u00e4\u00b7nen", "thau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PPOSAT", "NN", "$,", "ADV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein, stillbefriedigt, ruhig, glanzerhellt", "tokens": ["Nein", ",", "still\u00b7be\u00b7frie\u00b7digt", ",", "ru\u00b7hig", ",", "glan\u00b7zer\u00b7hellt"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00dft' unabwendbar drauf es niederschauen, \u2013", "tokens": ["Mu\u00dft'", "un\u00b7ab\u00b7wend\u00b7bar", "drauf", "es", "nie\u00b7der\u00b7schau\u00b7en", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ADJD", "PAV", "PPER", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcrwahr, durch eine Thr\u00e4ne w\u00e4r's entstellt!", "tokens": ["F\u00fcr\u00b7wahr", ",", "durch", "ei\u00b7ne", "Thr\