{"dta.poem.12879": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen guten freund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du r\u00fchmest diesen ort, in den dich Amor f\u00fchret,", "tokens": ["Du", "r\u00fch\u00b7mest", "die\u00b7sen", "ort", ",", "in", "den", "dich", "A\u00b7mor", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "APPR", "ART", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von wegen seiner sch\u00e4tz, als eine neue welt.", "tokens": ["Von", "we\u00b7gen", "sei\u00b7ner", "sch\u00e4tz", ",", "als", "ei\u00b7ne", "neu\u00b7e", "welt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ich gesteh es selbst, wer, was Rosetten zieret,", "tokens": ["Und", "ich", "ge\u00b7steh", "es", "selbst", ",", "wer", ",", "was", "Ro\u00b7set\u00b7ten", "zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "PWS", "$,", "PWS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht vor was sonderlich-und ungemeines h\u00e4lt,", "tokens": ["Nicht", "vor", "was", "son\u00b7der\u00b7lich\u00b7\u00b7und", "un\u00b7ge\u00b7mei\u00b7nes", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PRELS", "ADJD", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der hat der sch\u00f6nheit werth noch nicht genau erkannt.", "tokens": ["Der", "hat", "der", "sch\u00f6n\u00b7heit", "werth", "noch", "nicht", "ge\u00b7nau", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJD", "ADV", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch prale nicht zu viel! darff deine freye hand", "tokens": ["Doch", "pra\u00b7le", "nicht", "zu", "viel", "!", "darff", "dei\u00b7ne", "frey\u00b7e", "hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "PTKA", "PIS", "$.", "VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In diesem Indien gleich gold und perlen lesen:", "tokens": ["In", "die\u00b7sem", "In\u00b7di\u00b7en", "gleich", "gold", "und", "per\u00b7len", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ADJD", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Genug, da\u00df ich allhier Columbus bin gewesen.", "tokens": ["Ge\u00b7nug", ",", "da\u00df", "ich", "all\u00b7hier", "Co\u00b7lum\u00b7bus", "bin", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "NE", "VAFIN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}