{"textgrid.poem.38321": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "2. Erziehung durch Geschichte", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00f6wen la\u00dft euch wieder finden,", "tokens": ["L\u00f6\u00b7wen", "la\u00dft", "euch", "wie\u00b7der", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie im ersten Christenthum;", "tokens": ["Wie", "im", "ers\u00b7ten", "Chris\u00b7ten\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die nichts konnte \u00fcberwinden,", "tokens": ["Die", "nichts", "konn\u00b7te", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seht nur an ihr Marterthum.", "tokens": ["Seht", "nur", "an", "ihr", "Mar\u00b7ter\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie in Lieb sie gl\u00fchen,", "tokens": ["Wie", "in", "Lieb", "sie", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wie sie Feuer spieen;", "tokens": ["Wie", "sie", "Feu\u00b7er", "spie\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da sich vor der Sterbenslust", "tokens": ["Da", "sich", "vor", "der", "Ster\u00b7bens\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Selbst der Satan f\u00fcrchten must.", "tokens": ["Selbst", "der", "Sa\u00b7tan", "f\u00fcrch\u00b7ten", "must", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In Gefahren unerschrocken,", "tokens": ["In", "Ge\u00b7fah\u00b7ren", "un\u00b7er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und von L\u00fcsten unber\u00fchrt;", "tokens": ["Und", "von", "L\u00fcs\u00b7ten", "un\u00b7be\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die aufs Eitle konnten locken,", "tokens": ["Die", "aufs", "Eit\u00b7le", "konn\u00b7ten", "lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles sie zum Himmel f\u00fchrt.", "tokens": ["Al\u00b7les", "sie", "zum", "Him\u00b7mel", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keine Furcht in ihnen,", "tokens": ["Kei\u00b7ne", "Furcht", "in", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Auf die Kampfschaub\u00fchnen", "tokens": ["Auf", "die", "Kampf\u00b7schau\u00b7b\u00fch\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sprangen sie mit Freudigkeit,", "tokens": ["Spran\u00b7gen", "sie", "mit", "Freu\u00b7dig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hielten mit den Thieren Streit.", "tokens": ["Hiel\u00b7ten", "mit", "den", "Thie\u00b7ren", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ey wohlan, nur fein standhaftig,", "tokens": ["Ey", "wo\u00b7hlan", ",", "nur", "fein", "stand\u00b7haf\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O ihr Br\u00fcder tapfer drauf;", "tokens": ["O", "ihr", "Br\u00fc\u00b7der", "tap\u00b7fer", "drauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasset uns doch recht herzhaftig", "tokens": ["Las\u00b7set", "uns", "doch", "recht", "herz\u00b7haf\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folgen jener Zeugen Hauf!", "tokens": ["Fol\u00b7gen", "je\u00b7ner", "Zeu\u00b7gen", "Hauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur den Leib ber\u00fchrets,", "tokens": ["Nur", "den", "Leib", "be\u00b7r\u00fch\u00b7rets", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Was ihm so geb\u00fchret;", "tokens": ["Was", "ihm", "so", "ge\u00b7b\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Er hats Leiden wohl verdient,", "tokens": ["Er", "hats", "Lei\u00b7den", "wohl", "ver\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Seel darunter gr\u00fcnt.", "tokens": ["Und", "die", "Seel", "da\u00b7run\u00b7ter", "gr\u00fcnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fort weg mit dem Sinn der Griechen,", "tokens": ["Fort", "weg", "mit", "dem", "Sinn", "der", "Grie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Denen Kreutz ein Thorheit ist;", "tokens": ["De\u00b7nen", "Kreutz", "ein", "Thor\u00b7heit", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O la\u00dft uns zur\u00fcck nicht kriechen,", "tokens": ["O", "la\u00dft", "uns", "zu\u00b7r\u00fcck", "nicht", "krie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wenn ans Kreutz soll Jesu Christ!", "tokens": ["Wenn", "ans", "Kreutz", "soll", "Je\u00b7su", "Christ", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "VMFIN", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Reiht euch dicht zusammen,", "tokens": ["Reiht", "euch", "dicht", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wenn der Schlange Samen", "tokens": ["Wenn", "der", "Schlan\u00b7ge", "Sa\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sich dem Glauben widersetzt,", "tokens": ["Sich", "dem", "Glau\u00b7ben", "wi\u00b7der\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und das Schlachtschwerdt auf uns wetzt.", "tokens": ["Und", "das", "Schlacht\u00b7schwerdt", "auf", "uns", "wetzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schw\u00e4ngre vor, o goldner Regen,", "tokens": ["Schw\u00e4ng\u00b7re", "vor", ",", "o", "gold\u00b7ner", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns dein d\u00fcrres Erb und Erd;", "tokens": ["Uns", "dein", "d\u00fcr\u00b7res", "Erb", "und", "Erd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir dir getreu seyn m\u00f6gen,", "tokens": ["Da\u00df", "wir", "dir", "ge\u00b7treu", "seyn", "m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und nicht achten Feuer, Schwerdt.", "tokens": ["Und", "nicht", "ach\u00b7ten", "Feu\u00b7er", ",", "Schwerdt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJA", "NN", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als in Liebe trunken,", "tokens": ["Als", "in", "Lie\u00b7be", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und in dir versunken;", "tokens": ["Und", "in", "dir", "ver\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mach die Kirch an Liebe reich,", "tokens": ["Mach", "die", "Kirch", "an", "Lie\u00b7be", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df das End dem Anfang gleich.", "tokens": ["Da\u00df", "das", "End", "dem", "An\u00b7fang", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV",