{"textgrid.poem.52041": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du die Verse liest, dis bilde dir nicht ein,", "tokens": ["Der", "du", "die", "Ver\u00b7se", "liest", ",", "dis", "bil\u00b7de", "dir", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sammt meine Galle solt auch deinen Stand bespr\u00fctzen,", "tokens": ["Sammt", "mei\u00b7ne", "Gal\u00b7le", "solt", "auch", "dei\u00b7nen", "Stand", "be\u00b7spr\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Furcht ist dir zu hoch, du kanst was seichter sitzen,", "tokens": ["Die", "Furcht", "ist", "dir", "zu", "hoch", ",", "du", "kanst", "was", "seich\u00b7ter", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$,", "PPER", "VMFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und darffst um deinen Ruff hier nicht bem\u00fchet seyn.", "tokens": ["Und", "darffst", "um", "dei\u00b7nen", "Ruff", "hier", "nicht", "be\u00b7m\u00fc\u00b7het", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "APPR", "PPOSAT", "NN", "ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da wo der Kramer wohnt und Pfeffer D\u00fctten dreht,", "tokens": ["Da", "wo", "der", "Kra\u00b7mer", "wohnt", "und", "Pfef\u00b7fer", "D\u00fct\u00b7ten", "dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "VVFIN", "KON", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was der Koch erkaufft zum Futter der Pasteten,", "tokens": ["Und", "was", "der", "Koch", "er\u00b7kaufft", "zum", "Fut\u00b7ter", "der", "Pas\u00b7te\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NE", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da geh und hol ein Blat und frage nach Poeten:", "tokens": ["Da", "geh", "und", "hol", "ein", "Blat", "und", "fra\u00b7ge", "nach", "Po\u00b7et\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die liest man, wann der Wind aus faulen L\u00f6chern weht.", "tokens": ["Die", "liest", "man", ",", "wann", "der", "Wind", "aus", "fau\u00b7len", "L\u00f6\u00b7chern", "weht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der hinterm Offen sitzt, und B