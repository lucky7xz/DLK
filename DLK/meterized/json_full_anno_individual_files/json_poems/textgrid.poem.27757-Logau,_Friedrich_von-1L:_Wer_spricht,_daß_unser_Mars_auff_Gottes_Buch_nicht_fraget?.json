{"textgrid.poem.27757": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer spricht, da\u00df unser Mars auff Gottes Buch nicht fraget?", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer spricht, da\u00df unser Mars auff Gottes Buch nicht fraget?", "tokens": ["Wer", "spricht", ",", "da\u00df", "un\u00b7ser", "Mars", "auff", "Got\u00b7tes", "Buch", "nicht", "fra\u00b7get", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat genau gemerckt, was K\u00f6nig Saul dort saget:", "tokens": ["Er", "hat", "ge\u00b7nau", "ge\u00b7merckt", ",", "was", "K\u00f6\u00b7nig", "Saul", "dort", "sa\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "VVPP", "$,", "PWS", "NN", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer mir nicht nach zeucht au\u00df, de\u00df Rinder soll man st\u00fccken,", "tokens": ["Wer", "mir", "nicht", "nach", "zeucht", "au\u00df", ",", "de\u00df", "Rin\u00b7der", "soll", "man", "st\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "APPR", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und also hin und her durch alle Gr\u00e4ntzen schicken.", "tokens": ["Und", "al\u00b7so", "hin", "und", "her", "durch", "al\u00b7le", "Gr\u00e4nt\u00b7zen", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKVZ", "KON", "ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum ist nun alles Vieh verschickt, zerst\u00fcckt, vertrieben,", "tokens": ["Drum", "ist", "nun", "al\u00b7les", "Vieh", "ver\u00b7schickt", ",", "zer\u00b7st\u00fcckt", ",", "ver\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PAV", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df uns von ihnen kaum der lehre Stall ist blieben.", "tokens": ["Da\u00df", "uns", "von", "ih\u00b7nen", "kaum", "der", "leh\u00b7re", "Stall", "ist", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADV", "ART", "NN", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer spricht, da\u00df unser Mars auff Gottes Buch nicht fraget?", "tokens": ["Wer", "spricht", ",", "da\u00df", "un\u00b7ser", "Mars", "auff", "Got\u00b7tes", "Buch", "nicht", "fra\u00b7get", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er hat genau gemerckt, was K\u00f6nig Saul dort saget:", "tokens": ["Er", "hat", "ge\u00b7nau", "ge\u00b7merckt", ",", "was", "K\u00f6\u00b7nig", "Saul", "dort", "sa\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "p