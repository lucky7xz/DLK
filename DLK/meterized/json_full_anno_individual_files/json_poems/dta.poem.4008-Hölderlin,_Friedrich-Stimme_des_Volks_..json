{"dta.poem.4008": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Stimme des Volks .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Du seyest Gottes Stimme, so ahndet' ich", "tokens": ["Du", "se\u00b7yest", "Got\u00b7tes", "Stim\u00b7me", ",", "so", "ahn\u00b7det'", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "In heil'ger Jugend; ja, und ich sag' es noch. \u2014", "tokens": ["In", "heil'\u00b7ger", "Ju\u00b7gend", ";", "ja", ",", "und", "ich", "sag'", "es", "noch", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PTKANT", "$,", "KON", "PPER", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Um meine Weisheit unbek\u00fcmmert", "tokens": ["Um", "mei\u00b7ne", "Weis\u00b7heit", "un\u00b7be\u00b7k\u00fcm\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Rauschen die Wasser doch auch, und dennoch", "tokens": ["Rau\u00b7schen", "die", "Was\u00b7ser", "doch", "auch", ",", "und", "den\u00b7noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "ADV", "$,", "KON", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "H\u00f6r' ich sie gern, und \u00f6fters bewegen sie", "tokens": ["H\u00f6r'", "ich", "sie", "gern", ",", "und", "\u00f6f\u00b7ters", "be\u00b7we\u00b7gen", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "KON", "ADV", "VVFIN", "PPER"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und st\u00e4rken mir das Herz, die Gewaltigen;", "tokens": ["Und", "st\u00e4r\u00b7ken", "mir", "das", "Herz", ",", "die", "Ge\u00b7wal\u00b7ti\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und meine Bahn nicht, aber richtig", "tokens": ["Und", "mei\u00b7ne", "Bahn", "nicht", ",", "a\u00b7ber", "rich\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wandeln in's Meer sie die Bahn hinunter.", "tokens": ["Wan\u00b7deln", "in's", "Meer", "sie", "die", "Bahn", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}