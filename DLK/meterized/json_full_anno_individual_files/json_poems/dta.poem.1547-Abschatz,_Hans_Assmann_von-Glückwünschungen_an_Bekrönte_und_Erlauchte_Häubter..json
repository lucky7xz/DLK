{"dta.poem.1547": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckw\u00fcnschungen  \n an  \n Bekr\u00f6nte  \n und  \n Erlauchte H\u00e4ubter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "O neues Licht von Oesterreich/", "tokens": ["O", "neu\u00b7es", "Licht", "von", "O\u00b7es\u00b7ter\u00b7reich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NE", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Sonne gieng dir auff/ als du die Welt beschienen;", "tokens": ["Die", "Son\u00b7ne", "gieng", "dir", "auff", "/", "als", "du", "die", "Welt", "be\u00b7schie\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "$(", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Monden wurde bleich", "tokens": ["Der", "Mon\u00b7den", "wur\u00b7de", "bleich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Stambols Monde weich/", "tokens": ["Und", "Stam\u00b7bols", "Mon\u00b7de", "weich", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zum Zeichen/ da\u00df dir Sonn und Monde sollen dienen!", "tokens": ["Zum", "Zei\u00b7chen", "/", "da\u00df", "dir", "Sonn", "und", "Mon\u00b7de", "sol\u00b7len", "die\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "KOUS", "PPER", "NN", "KON", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}