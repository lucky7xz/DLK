{"dta.poem.1317": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die tr\u00fcben Tage.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Des Lebens Nebel ist ein kurtz bew\u00f6lckter Tag/", "tokens": ["Des", "Le\u00b7bens", "Ne\u00b7bel", "ist", "ein", "kurtz", "be\u00b7w\u00f6lck\u00b7ter", "Tag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dem man wenig Licht und Sonne sehen mag.", "tokens": ["An", "dem", "man", "we\u00b7nig", "Licht", "und", "Son\u00b7ne", "se\u00b7hen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ART", "PIS", "PIAT", "NN", "KON", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der eiteln Dinge Schnee blendt bl\u00f6der Augen Licht/", "tokens": ["Der", "ei\u00b7teln", "Din\u00b7ge", "Schnee", "blendt", "bl\u00f6\u00b7der", "Au\u00b7gen", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "ADJD", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man traut auff glattes Ei\u00df/ da\u00df unterm Fusse bricht/", "tokens": ["Man", "traut", "auff", "glat\u00b7tes", "Ei\u00df", "/", "da\u00df", "un\u00b7term", "Fus\u00b7se", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "$(", "KOUS", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit Sturm und Frost vergeht der Winter dieser Zeit:", "tokens": ["Mit", "Sturm", "und", "Frost", "ver\u00b7geht", "der", "Win\u00b7ter", "die\u00b7ser", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Christen Sommer ist in froher Ewigkeit.", "tokens": ["Der", "Chris\u00b7ten", "Som\u00b7mer", "ist", "in", "fro\u00b7her", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}