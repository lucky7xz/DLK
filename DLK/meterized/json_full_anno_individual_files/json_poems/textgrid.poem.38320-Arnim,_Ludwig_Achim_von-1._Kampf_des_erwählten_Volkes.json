{"textgrid.poem.38320": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1. Kampf des erw\u00e4hlten Volkes", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, auf, auf ihr Helden, waget Gut und Blut,", "tokens": ["Auf", ",", "auf", ",", "auf", "ihr", "Hel\u00b7den", ",", "wa\u00b7get", "Gut", "und", "Blut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "W\u00fcrget mit vereinten Kr\u00e4ften Babels Brut!", "tokens": ["W\u00fcr\u00b7get", "mit", "ver\u00b7ein\u00b7ten", "Kr\u00e4f\u00b7ten", "Ba\u00b7bels", "Brut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NE", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Eure Feldposaunen,", "tokens": ["Eu\u00b7re", "Feld\u00b7po\u00b7sau\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Trommeln und Kartaunen,", "tokens": ["Trom\u00b7meln", "und", "Kar\u00b7tau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Lasset t\u00f6nen und erwecken L\u00f6wenmuth.", "tokens": ["Las\u00b7set", "t\u00f6\u00b7nen", "und", "er\u00b7we\u00b7cken", "L\u00f6\u00b7wen\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann die Blutfahn flieget, so seyd unverzagt,", "tokens": ["Wann", "die", "Blut\u00b7fahn", "flie\u00b7get", ",", "so", "seyd", "un\u00b7ver\u00b7zagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Josua hat vor euch schon den Feind verjagt!", "tokens": ["Jo\u00b7sua", "hat", "vor", "euch", "schon", "den", "Feind", "ver\u00b7jagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Unser L\u00f6we br\u00fcllet,", "tokens": ["Un\u00b7ser", "L\u00f6\u00b7we", "br\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und mit Schrecken f\u00fcllet", "tokens": ["Und", "mit", "Schre\u00b7cken", "f\u00fcl\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Das Heer der Assyrer, so sich an uns wagt.", "tokens": ["Das", "Heer", "der", "As\u00b7sy\u00b7rer", ",", "so", "sich", "an", "uns", "wagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADV", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Auf, auf, zuckt die Schwerdter, schlaget muthig drein,", "tokens": ["Auf", ",", "auf", ",", "zuckt", "die", "Schwerd\u00b7ter", ",", "schla\u00b7get", "mut\u00b7hig", "drein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "St\u00fcrmt die Th\u00fcrme Babels, rei\u00dft die Mauern ein.", "tokens": ["St\u00fcrmt", "die", "Th\u00fcr\u00b7me", "Ba\u00b7bels", ",", "rei\u00dft", "die", "Mau\u00b7ern", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Auf, sie sollen fallen,", "tokens": ["Auf", ",", "sie", "sol\u00b7len", "fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn Posaunen schallen,", "tokens": ["Wenn", "Po\u00b7sau\u00b7nen", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Denn die Stunde, sie zu richten, bricht herein.", "tokens": ["Denn", "die", "Stun\u00b7de", ",", "sie", "zu", "rich\u00b7ten", ",", "bricht", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Du o Jesu f\u00fchre selbsten deinen Krieg,", "tokens": ["Du", "o", "Je\u00b7su", "f\u00fch\u00b7re", "selbs\u00b7ten", "dei\u00b7nen", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "FM", "NE", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "In uns, durch uns, mit uns, da\u00df der Feind erlieg.", "tokens": ["In", "uns", ",", "durch", "uns", ",", "mit", "uns", ",", "da\u00df", "der", "Feind", "er\u00b7lieg", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "$,", "APPR", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "In der Kraft erscheinen", "tokens": ["In", "der", "Kraft", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wir nun als die deinen,", "tokens": ["Wir", "nun", "als", "die", "dei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOUS", "ART", "PPOSAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "K\u00f6nnen triumphiren nach erlangtem Sieg.", "tokens": ["K\u00f6n\u00b7nen", "tri\u00b7um\u00b7phi\u00b7ren", "nach", "er\u00b7lang\u00b7tem", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Preis, Kraft, Macht und St\u00e4rke sey dir starker Hort,", "tokens": ["Preis", ",", "Kraft", ",", "Macht", "und", "St\u00e4r\u00b7ke", "sey", "dir", "star\u00b7ker", "Hort", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Von uns zubereitet immer fort und fort.", "tokens": ["Von", "uns", "zu\u00b7be\u00b7rei\u00b7tet", "im\u00b7mer", "fort", "und", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Jo, Jo, Jo, durch Sterben", "tokens": ["Jo", ",", "Jo", ",", "Jo", ",", "durch", "Ster\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wollen wir erwerben", "tokens": ["Wol\u00b7len", "wir", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Deine Siegeskrone bei dem Friedensport.", "tokens": ["Dei\u00b7ne", "Sie\u00b7ges\u00b7kro\u00b7ne", "bei", "dem", "Frie\u00b7dens\u00b7port", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Dann wird erst ert\u00f6nen der Trompeten Hall,", "tokens": ["Dann", "wird", "erst", "er\u00b7t\u00f6\u00b7nen", "der", "Trom\u00b7pe\u00b7ten", "Hall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn wir werden jauchzen \u00fcber Babels Fall.", "tokens": ["Wenn", "wir", "wer\u00b7den", "jauch\u00b7zen", "\u00fc\u00b7ber", "Ba\u00b7bels", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVINF", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Da wir k\u00f6nnen springen,", "tokens": ["Da", "wir", "k\u00f6n\u00b7nen", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Neue Lieder singen;", "tokens": ["Neu\u00b7e", "Lie\u00b7der", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mit erhabnen Stimmen bei dem Jubelschall.", "tokens": ["Mit", "er\u00b7hab\u00b7nen", "Stim\u00b7men", "bei", "dem", "Ju\u00b7bel\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Auf, auf, auf ihr Helden, waget Gut und Blut,", "tokens": ["Auf", ",", "auf", ",", "auf", "ihr", "Hel\u00b7den", ",", "wa\u00b7get", "Gut", "und", "Blut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "W\u00fcrget mit vereinten Kr\u00e4ften Babels Brut!", "tokens": ["W\u00fcr\u00b7get", "mit", "ver\u00b7ein\u00b7ten", "Kr\u00e4f\u00b7ten", "Ba\u00b7bels", "Brut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "NE", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Eure Feldposaunen,", "tokens": ["Eu\u00b7re", "Feld\u00b7po\u00b7sau\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Trommeln und Kartaunen,", "tokens": ["Trom\u00b7meln", "und", "Kar\u00b7tau\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.