nct"], "pos": ["VMFIN", "$,", "NE", "$,", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich kl\u00e4glich sol vergehen?", "tokens": ["Da\u00df", "ich", "kl\u00e4g\u00b7lich", "sol", "ver\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohnet nicht mehr G\u00fct' vnd Trew", "tokens": ["Woh\u00b7net", "nicht", "mehr", "G\u00fct'", "vnd", "Trew"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinem Vater-Hertzen bey?", "tokens": ["Dei\u00b7nem", "Va\u00b7ter\u00b7Hert\u00b7zen", "bey", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ey ich wil dennoch bekleiben,", "tokens": ["Ey", "ich", "wil", "den\u00b7noch", "be\u00b7klei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd an Dir, GOTT, allzeit bleiben,", "tokens": ["Vnd", "an", "Dir", ",", "GoTT", ",", "all\u00b7zeit", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "NE", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn Du h\u00e4ltest meinen Stand", "tokens": ["Denn", "Du", "h\u00e4l\u00b7test", "mei\u00b7nen", "Stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fest an deiner Rechten Hand.", "tokens": ["Fest", "an", "dei\u00b7ner", "Rech\u00b7ten", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Niemals wird mein Fu\u00df-tritt gleiten,", "tokens": ["Nie\u00b7mals", "wird", "mein", "Fu\u00df\u00b7tritt", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich sehe dich mich leiten", "tokens": ["Denn", "ich", "se\u00b7he", "dich", "mich", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Richtig vnd nach deinem Raht,", "tokens": ["Rich\u00b7tig", "vnd", "nach", "dei\u00b7nem", "Raht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gieng ich auch des Todtes pfad.", "tokens": ["Gieng", "ich", "auch", "des", "Tod\u00b7tes", "pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Endlich, wenn mein Leid sich endet,", "tokens": ["End\u00b7lich", ",", "wenn", "mein", "Leid", "sich", "en\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df dein Zorn sich hat gewendet,", "tokens": ["Da\u00df", "dein", "Zorn", "sich", "hat", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der vers\u00fchnt noch werden kan,", "tokens": ["Der", "ver\u00b7s\u00fchnt", "noch", "wer\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nimbstu mich mit Ehren an.", "tokens": ["Nimbs\u00b7tu", "mich", "mit", "Eh\u00b7ren", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich wil nichts den Himmel achten,", "tokens": ["Ich", "wil", "nichts", "den", "Him\u00b7mel", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wil nach keiner Erden trachten,", "tokens": ["Wil", "nach", "kei\u00b7ner", "Er\u00b7den", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' ich dich, herr, mein Gewinn,", "tokens": ["Hab'", "ich", "dich", ",", "herr", ",", "mein", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Allzeit nur in meinem Sinn;", "tokens": ["All\u00b7zeit", "nur", "in", "mei\u00b7nem", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es sey nichts an mir zu finden,", "tokens": ["Es", "sey", "nichts", "an", "mir", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Lass mir Leib vnd Seele schwinden,", "tokens": ["Lass", "mir", "Leib", "vnd", "See\u00b7le", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So bist Du doch, GOTT, mein Heyl,", "tokens": ["So", "bist", "Du", "doch", ",", "GoTT", ",", "mein", "Heyl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Meines Hertzens Trost vnd Theil.", "tokens": ["Mei\u00b7nes", "Hert\u00b7zens", "Trost", "vnd", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}