{"textgrid.poem.53308": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herr, wohin sol ich mich kehren?", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, wohin sol ich mich kehren?", "tokens": ["Herr", ",", "wo\u00b7hin", "sol", "ich", "mich", "keh\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer kan H\u00fclffe mir gewehren", "tokens": ["Wer", "kan", "H\u00fclf\u00b7fe", "mir", "ge\u00b7weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Angst, die mich umbgiebt,", "tokens": ["In", "der", "Angst", ",", "die", "mich", "umb\u00b7giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd bis auf den Tod betr\u00fcbt?", "tokens": ["Vnd", "bis", "auf", "den", "Tod", "be\u00b7tr\u00fcbt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich kan selber mir nicht rathen,", "tokens": ["Ich", "kan", "sel\u00b7ber", "mir", "nicht", "ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wegen meiner Missethaten,", "tokens": ["We\u00b7gen", "mei\u00b7ner", "Mis\u00b7se\u00b7tha\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Menschen helffen auch mir nicht,", "tokens": ["Men\u00b7schen", "helf\u00b7fen", "auch", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Welchen Rettung selbs gebricht.", "tokens": ["Wel\u00b7chen", "Ret\u00b7tung", "selbs", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nichts mag auff der weiten Erden,", "tokens": ["Nichts", "mag", "auff", "der", "wei\u00b7ten", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nichts im Himmel funden werden,", "tokens": ["Nichts", "im", "Him\u00b7mel", "fun\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPRART", "NN", "VVFIN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das die Seel' in mir befreyht", "tokens": ["Das", "die", "Seel'", "in", "mir", "be\u00b7freyht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer Furcht vnd Trawrigheit;", "tokens": ["Ih\u00b7rer", "Furcht", "vnd", "Traw\u00b7rig\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00fcnde, Todt, vnd selbs die Helle", "tokens": ["S\u00fcn\u00b7de", ",", "Todt", ",", "vnd", "selbs", "die", "Hel\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KON", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd des Jammers tausent F\u00e4lle,", "tokens": ["Vnd", "des", "Jam\u00b7mers", "tau\u00b7sent", "F\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was mir Angst vnd Schmertzen bringt,", "tokens": ["Was", "mir", "Angst", "vnd", "Schmert\u00b7zen", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Halten s\u00e4mptlich mich umbringt.", "tokens": ["Hal\u00b7ten", "s\u00e4mpt\u00b7lich", "mich", "um\u00b7bringt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kanstu, GOTT, es dann gestehen,", "tokens": ["Kans\u00b7tu", ",", "GoTT", ",", "es", "dann", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "NE", "$,", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich kl\u00e4glich sol vergehen?", "tokens": ["Da\u00df", "ich", "kl\u00e4g\u00b7lich", "sol", "ver\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohnet nicht mehr G\u00fct' vnd Trew", "tokens": ["Woh\u00b7net", "nicht", "mehr", "G\u00fct'", "vnd", "Trew"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinem Vater-Hertzen bey?", "tokens": ["Dei\u00b7nem", "Va\u00b7ter\u00b7Hert\u00b7zen", "bey", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ey ich wil dennoch bekleiben,", "tokens": ["Ey", "ich", "wil", "den\u00b7noch", "be\u00b7klei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vnd an Dir, GOTT, allzeit bleiben,", "tokens": ["Vnd", "an", "Dir", ",", "GoTT", ",", "all\u00b7zeit", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "NE", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn Du h\u00e4ltest meinen Stand", "tokens": ["Denn", "Du", "h\u00e4l\u00b7test", "mei\u00b7nen", "Stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fest an deiner Rechten Hand.", "tokens": ["Fest", "an", "dei\u00b7ner", "Rech\u00b7ten", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Niemals wird mein Fu\u00df-tritt gleiten,", "tokens": ["Nie\u00b7mals", "wird", "mein", "Fu\u00df\u00b7tritt", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich sehe dich mich leiten", "tokens": ["Denn", "ich", "se\u00b7he", "dich", "mich", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Richtig vnd nach deinem Raht,", "tokens": ["Rich\u00b7tig", "vnd", "nach", "dei\u00b7nem", "Raht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gieng ich auch des Todtes pfad.", "tokens": ["Gieng", "ich", "auch", "des", "Tod\u00b7tes", "pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Endlich, wenn mein Leid sich endet,", "tokens": ["End\u00b7lich", ",", "wenn", "mein", "Leid", "sich", "en\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df dein Zorn sich hat gewendet,", "tokens": ["Da\u00df", "dein", "Zorn", "sich", "hat", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der vers\u00fchnt noch werden kan,", "tokens": ["Der", "ver\u00b7s\u00fchnt", "noch", "wer\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VAINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nimbstu mich mit Ehren an.", "tokens": ["Nimbs\u00b7tu", "mich", "mit", "Eh\u00b7ren", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich wil nichts den Himmel achten,", "tokens": ["Ich", "wil", "nichts", "den", "Him\u00b7mel", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wil nach keiner Erden trachten,", "tokens": ["Wil", "nach", "kei\u00b7ner", "Er\u00b7den", "trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hab' ich dich, herr, mein Gewinn,", "tokens": ["Hab'", "ich", "dich", ",", "herr", ",", "mein", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Allzeit nur in meinem Sinn;", "tokens": ["All\u00b7zeit", "nur", "in", "mei\u00b7nem", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es sey nichts an mir zu finden,", "tokens": ["Es", "sey", "nichts", "an", "mir", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Lass mir Leib vnd Seele schwinden,", "tokens": ["Lass", "mir", "Leib", "vnd", "See\u00b7le", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So bist Du doch, GOTT, mein Heyl,", "tokens": ["So", "bist", "Du", "doch", ",", "GoTT", ",", "mein", "Heyl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Meines Hertzens Trost vnd Theil.", "tokens": ["Mei\u00b7nes", "Hert\u00b7zens", "Trost", "vnd", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr, wohin sol ich mich kehren?", "tokens": ["Herr", ",", "wo\u00b7hin", "sol", "ich", "mich", "keh\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wer kan H\u00fclffe mir gewehren", "tokens": ["Wer", "kan", "H\u00fclf\u00b7fe", "mir", "ge\u00b7weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der Angst, die mich umbgiebt,", "tokens": ["In", "der", "Angst", ",", "die", "mich", "umb\u00b7giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd bis auf den Tod betr\u00fcbt?", "tokens": ["Vnd", "bis", "auf", "den", "Tod", "be\u00b7tr\u00fcbt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"]