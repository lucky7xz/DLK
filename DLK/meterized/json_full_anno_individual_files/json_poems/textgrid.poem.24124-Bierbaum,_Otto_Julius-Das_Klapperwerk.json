{"textgrid.poem.24124": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Das Klapperwerk", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor meinem Fenster drau\u00df,", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter", "drau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf dem kahlen Pappelbaum", "tokens": ["Auf", "dem", "kah\u00b7len", "Pap\u00b7pel\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzt ein Gespenst;", "tokens": ["Sitzt", "ein", "Ge\u00b7spenst", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Das sieht scheu\u00dflich aus.", "tokens": ["Das", "sieht", "scheu\u00df\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sein Auge, das droht,", "tokens": ["Sein", "Au\u00b7ge", ",", "das", "droht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ist innewendig rot,", "tokens": ["Ist", "in\u00b7ne\u00b7wen\u00b7dig", "rot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Maul, das trenst.", "tokens": ["Sein", "Maul", ",", "das", "trenst", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ach, und wies redet und gestikuliert,", "tokens": ["Ach", ",", "und", "wies", "re\u00b7det", "und", "ges\u00b7ti\u00b7ku\u00b7liert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VVFIN", "VVFIN", "KON", "VVPP", "$,"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Jedwedes W\u00f6rtel mit Salbe beschmiert;", "tokens": ["Jed\u00b7we\u00b7des", "W\u00f6r\u00b7tel", "mit", "Sal\u00b7be", "be\u00b7schmiert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.10": {"text": "Schnappt \u00fcber auch oft in Gezeter.", "tokens": ["Schnappt", "\u00fc\u00b7ber", "auch", "oft", "in", "Ge\u00b7ze\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Sei nicht so unversch\u00e4mt, wertes Gespenst;", "tokens": ["Sei", "nicht", "so", "un\u00b7ver\u00b7sch\u00e4mt", ",", "wer\u00b7tes", "Ge\u00b7spenst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Siehe, mein Pappelbaum ist kein Katheder,", "tokens": ["Sie\u00b7he", ",", "mein", "Pap\u00b7pel\u00b7baum", "ist", "kein", "Ka\u00b7the\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und ich bin kein Schulbube, wie du mich kennst.", "tokens": ["Und", "ich", "bin", "kein", "Schul\u00b7bu\u00b7be", ",", "wie", "du", "mich", "kennst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "--+-+---+-+", "measure": "anapaest.init"}, "line.14": {"text": "Hab ich nicht rite hinaus dich geschmissen", "tokens": ["Hab", "ich", "nicht", "ri\u00b7te", "hin\u00b7aus", "dich", "ge\u00b7schmis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "VVFIN", "APZR", "PPER", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.15": {"text": "Aus meinem Leben, du ledernes Scheuel?", "tokens": ["Aus", "mei\u00b7nem", "Le\u00b7ben", ",", "du", "le\u00b7der\u00b7nes", "Scheu\u00b7el", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Du bist mir widriger als der Tod,", "tokens": ["Du", "bist", "mir", "wid\u00b7ri\u00b7ger", "als", "der", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Und eine Fahne flammfeuerrot", "tokens": ["Und", "ei\u00b7ne", "Fah\u00b7ne", "flamm\u00b7feu\u00b7er\u00b7rot"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Will ich auf meiner Pappel hissen,", "tokens": ["Will", "ich", "auf", "mei\u00b7ner", "Pap\u00b7pel", "his\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Da\u00df sie dir droht", "tokens": ["Da\u00df", "sie", "dir", "droht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Und dich bannt, oh du Greuel.", "tokens": ["Und", "dich", "bannt", ",", "oh", "du", "Greu\u00b7el", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "NE", "$."], "meter": "-+++-+-", "measure": "unknown.measure.tetra"}, "line.21": {"text": "Du bist die gelehrte K\u00fcmmerlichkeit,", "tokens": ["Du", "bist", "die", "ge\u00b7lehr\u00b7te", "K\u00fcm\u00b7mer\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Armselig weise, kr\u00fcppelgescheidt,", "tokens": ["Arm\u00b7se\u00b7lig", "wei\u00b7se", ",", "kr\u00fcp\u00b7pel\u00b7ge\u00b7scheidt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Die nichts vermag,", "tokens": ["Die", "nichts", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Als Nacht und Tag", "tokens": ["Als", "Nacht", "und", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Dem Leben dekretieren,", "tokens": ["Dem", "Le\u00b7ben", "de\u00b7kre\u00b7tie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Wies bl\u00fchen soll und wachsen soll", "tokens": ["Wies", "bl\u00fc\u00b7hen", "soll", "und", "wach\u00b7sen", "soll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VMFIN", "KON", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und ja nicht excedieren.", "tokens": ["Und", "ja", "nicht", "ex\u00b7ce\u00b7die\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Macht einer vor Begeisterung", "tokens": ["Macht", "ei\u00b7ner", "vor", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Jach in die L\u00fcfte einen Sprung,", "tokens": ["Jach", "in", "die", "L\u00fcf\u00b7te", "ei\u00b7nen", "Sprung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Gleich kr\u00e4hst du miserere,", "tokens": ["Gleich", "kr\u00e4hst", "du", "mi\u00b7se\u00b7re\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Thust immer, als ob Gotteswelt", "tokens": ["Thust", "im\u00b7mer", ",", "als", "ob", "Got\u00b7tes\u00b7welt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOKOM", "KOUS", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ein tristes Geometerfeld", "tokens": ["Ein", "tris\u00b7tes", "Geo\u00b7me\u00b7ter\u00b7feld"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.33": {"text": "Aus graden Linien w\u00e4re;", "tokens": ["Aus", "gra\u00b7den", "Li\u00b7ni\u00b7en", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Speist alles an, was freudig ist", "tokens": ["Speist", "al\u00b7les", "an", ",", "was", "freu\u00b7dig", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "PRELS", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Und bist voll eitel Hinterlist", "tokens": ["Und", "bist", "voll", "ei\u00b7tel", "Hin\u00b7ter\u00b7list"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Mit Regeln und mit Fallen,", "tokens": ["Mit", "Re\u00b7geln", "und", "mit", "Fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Und manchen frohen Uebermut", "tokens": ["Und", "man\u00b7chen", "fro\u00b7hen", "Ue\u00b7ber\u00b7mut"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Hat deine kalte Regelwut", "tokens": ["Hat", "dei\u00b7ne", "kal\u00b7te", "Re\u00b7gel\u00b7wut"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Zerdr\u00fcckt in ihren Krallen.", "tokens": ["Zer\u00b7dr\u00fcckt", "in", "ih\u00b7ren", "Kral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Du bist der Deutschen Erbgespenst,", "tokens": ["Du", "bist", "der", "Deut\u00b7schen", "Erb\u00b7ge\u00b7spenst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und wenn du dich Professor nennst,", "tokens": ["Und", "wenn", "du", "dich", "Pro\u00b7fes\u00b7sor", "nennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Gleich werden zahm die Kecken,", "tokens": ["Gleich", "wer\u00b7den", "zahm", "die", "Ke\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Und hei\u00dft du gar Geheimer Rat,", "tokens": ["Und", "hei\u00dft", "du", "gar", "Ge\u00b7hei\u00b7mer", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Mu\u00df sich die beste, frohste That", "tokens": ["Mu\u00df", "sich", "die", "bes\u00b7te", ",", "frohs\u00b7te", "That"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PRF", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Vor deinem Spruch verstecken.", "tokens": ["Vor", "dei\u00b7nem", "Spruch", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie meinem Zorn ich genug gethan", "tokens": ["Wie", "mei\u00b7nem", "Zorn", "ich", "ge\u00b7nug", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "ADV", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sah ich das Ding mir genauer an:", "tokens": ["Sah", "ich", "das", "Ding", "mir", "ge\u00b7nau\u00b7er", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da wars ein Klapperscheuchwerk nur,", "tokens": ["Da", "wars", "ein", "Klap\u00b7per\u00b7scheuch\u00b7werk", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von einem Geiste keine Spur;", "tokens": ["Von", "ei\u00b7nem", "Geis\u00b7te", "kei\u00b7ne", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Oh zornige Verblendung!", "tokens": ["Oh", "zor\u00b7ni\u00b7ge", "Ver\u00b7blen\u00b7dung", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Indes, mich deucht, wens nicht verdrie\u00dft,", "tokens": ["In\u00b7des", ",", "mich", "deucht", ",", "wens", "nicht", "ver\u00b7drie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df er aus dieser M\u00e4re liest", "tokens": ["Da\u00df", "er", "aus", "die\u00b7ser", "M\u00e4\u00b7re", "liest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wol eine Nutzanwendung.", "tokens": ["Wol", "ei\u00b7ne", "Nut\u00b7zan\u00b7wen\u00b7dung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Vor meinem Fenster drau\u00df,", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter", "drau\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf dem kahlen Pappelbaum", "tokens": ["Auf", "dem", "kah\u00b7len", "Pap\u00b7pel\u00b7baum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzt ein Gespenst;", "tokens": ["Sitzt", "ein", "Ge\u00b7spenst", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Das sieht scheu\u00dflich aus.", "tokens": ["Das", "sieht", "scheu\u00df\u00b7lich", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sein Auge, das droht,", "tokens": ["Sein", "Au\u00b7ge", ",", "das", "droht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ist innewendig rot,", "tokens": ["Ist", "in\u00b7ne\u00b7wen\u00b7dig", "rot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sein Maul, das trenst.", "tokens": ["Sein", "Maul", ",", "das", "trenst", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+