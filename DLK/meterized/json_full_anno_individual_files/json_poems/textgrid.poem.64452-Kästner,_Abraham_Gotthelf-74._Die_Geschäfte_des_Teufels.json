{"textgrid.poem.64452": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "74. Die Gesch\u00e4fte des Teufels", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Teufels Bosheit nur verg\u00e4llt des Menschen Leben;", "tokens": ["Des", "Teu\u00b7fels", "Bos\u00b7heit", "nur", "ver\u00b7g\u00e4llt", "des", "Men\u00b7schen", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Krieg entsteht durch ihn, er macht die Erde beben,", "tokens": ["Der", "Krieg", "ent\u00b7steht", "durch", "ihn", ",", "er", "macht", "die", "Er\u00b7de", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von ihm kommt Fieber, Pest, de Hypochonders Stich,", "tokens": ["Von", "ihm", "kommt", "Fie\u00b7ber", ",", "Pest", ",", "de", "Hy\u00b7po\u00b7chon\u00b7ders", "Stich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "$,", "NN", "$,", "NE", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er spricht aus Rasenden, und - \u00fcbersetzt durch dich.", "tokens": ["Er", "spricht", "aus", "Ra\u00b7sen\u00b7den", ",", "und", "\u00fc\u00b7bers\u00b7etzt", "durch", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos":