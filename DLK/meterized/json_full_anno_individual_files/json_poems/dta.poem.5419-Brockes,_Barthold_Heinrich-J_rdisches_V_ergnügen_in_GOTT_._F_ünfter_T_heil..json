{"dta.poem.5419": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder, nur, der der Sonnen Menge", "tokens": ["\u201e", "der", ",", "nur", ",", "der", "der", "Son\u00b7nen", "Men\u00b7ge"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "$,", "ADV", "$,", "PRELS", "ART", "NN", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "\u201esammt den Schaaren aller Welt,", "tokens": ["\u201e", "sammt", "den", "Schaa\u00b7ren", "al\u00b7ler", "Welt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201ein so herrlichem Gepr\u00e4nge,", "tokens": ["\u201e", "in", "so", "herr\u00b7li\u00b7chem", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201ein so richtger Ordnung, h\u00e4lt,", "tokens": ["\u201e", "in", "so", "richt\u00b7ger", "Ord\u00b7nung", ",", "h\u00e4lt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "APPR", "ADV", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201esie so wunderbar regiret,", "tokens": ["\u201e", "sie", "so", "wun\u00b7der\u00b7bar", "re\u00b7gi\u00b7ret", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201eist, dem ewig Preis geb\u00fchret!", "tokens": ["\u201e", "ist", ",", "dem", "e\u00b7wig", "Preis", "ge\u00b7b\u00fch\u00b7ret", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "$,", "PRELS", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}