{"textgrid.poem.65335": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsahnung", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schwalbe k\u00f6mmt, die Schwalbe k\u00f6mmt, sie k\u00f6mmt vom wei\u00dfen Meer,", "tokens": ["Die", "Schwal\u00b7be", "k\u00f6mmt", ",", "die", "Schwal\u00b7be", "k\u00f6mmt", ",", "sie", "k\u00f6mmt", "vom", "wei\u00b7\u00dfen", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Sie fliegt heran, sie sieht sich um, als ob's nicht sicher w\u00e4r'.", "tokens": ["Sie", "fliegt", "he\u00b7ran", ",", "sie", "sieht", "sich", "um", ",", "als", "ob's", "nicht", "si\u00b7cher", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PRF", "PTKVZ", "$,", "KOUS", "NE", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "O M\u00e4rz, o M\u00e4rz, mein sch\u00f6ner Freund, ich f\u00fchl's, du bist mir nah!", "tokens": ["O", "M\u00e4rz", ",", "o", "M\u00e4rz", ",", "mein", "sch\u00f6\u00b7ner", "Freund", ",", "ich", "f\u00fchl's", ",", "du", "bist", "mir", "nah", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "