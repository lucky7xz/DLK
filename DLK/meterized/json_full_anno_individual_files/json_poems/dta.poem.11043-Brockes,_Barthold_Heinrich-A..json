{"dta.poem.11043": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich weis es: Aber weist du auch, da\u00df es ja den Orientalen", "tokens": ["Ich", "weis", "es", ":", "A\u00b7ber", "weist", "du", "auch", ",", "da\u00df", "es", "ja", "den", "O\u00b7rien\u00b7ta\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "PPER", "$.", "KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ganz eigen ist, den Sinn der Wahrheit in vielen Bildern", "tokens": ["Ganz", "ei\u00b7gen", "ist", ",", "den", "Sinn", "der", "Wahr\u00b7heit", "in", "vie\u00b7len", "Bil\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "ART", "NN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die werden in der ganzen Schrift, da\u00df solch ein Bild was", "tokens": ["Die", "wer\u00b7den", "in", "der", "gan\u00b7zen", "Schrift", ",", "da\u00df", "solch", "ein", "Bild", "was"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PIAT", "ART", "NN", "PWS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch die vern\u00fcnftgen Geistlichen vern\u00fcnftig \u00fcberall erkl\u00e4rt.", "tokens": ["Durch", "die", "ver\u00b7n\u00fcnft\u00b7gen", "Geist\u00b7li\u00b7chen", "ver\u00b7n\u00fcnf\u00b7tig", "\u00fc\u00b7be\u00b7rall", "er\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "Soll, bey so viel verbl\u00fcmten Stellen, die von der H\u00f6llen,", "tokens": ["Soll", ",", "bey", "so", "viel", "ver\u00b7bl\u00fcm\u00b7ten", "Stel\u00b7len", ",", "die", "von", "der", "H\u00f6l\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "APPR", "ADV", "PIAT", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "In einem eigentlichen Sinn und anders nicht erkl\u00e4ret seyn?", "tokens": ["In", "ei\u00b7nem", "ei\u00b7gent\u00b7li\u00b7chen", "Sinn", "und", "an\u00b7ders", "nicht", "er\u00b7kl\u00e4\u00b7ret", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ADV", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}