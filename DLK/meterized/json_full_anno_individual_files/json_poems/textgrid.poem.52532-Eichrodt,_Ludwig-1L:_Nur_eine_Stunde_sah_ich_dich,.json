{"textgrid.poem.52532": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nur eine Stunde sah ich dich,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur eine Stunde sah ich dich,", "tokens": ["Nur", "ei\u00b7ne", "Stun\u00b7de", "sah", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sprach kein Wort mit dir,", "tokens": ["Und", "sprach", "kein", "Wort", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch haben deine Z\u00fcge sich", "tokens": ["Doch", "ha\u00b7ben", "dei\u00b7ne", "Z\u00fc\u00b7ge", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tiefeingepr\u00e4gt in mir.", "tokens": ["Tie\u00b7fein\u00b7ge\u00b7pr\u00e4gt", "in", "mir", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Was du beginnst, wie du dich giebst,", "tokens": ["Was", "du", "be\u00b7ginnst", ",", "wie", "du", "dich", "giebst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Zauber liegt darin \u2013", "tokens": ["Ein", "Zau\u00b7ber", "liegt", "da\u00b7rin", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Eine, den du sicher liebst,", "tokens": ["Der", "Ei\u00b7ne", ",", "den", "du", "si\u00b7cher", "liebst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verwirrt mir ganz den Sinn.", "tokens": ["Ver\u00b7wirrt", "mir", "ganz", "den", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich will nicht fragen, wer du bist,", "tokens": ["Ich", "will", "nicht", "fra\u00b7gen", ",", "wer", "du", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es bleibe besser fern,", "tokens": ["Es", "blei\u00b7be", "bes\u00b7ser", "fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin ein leidlich guter Christ,", "tokens": ["Ich", "bin", "ein", "leid\u00b7lich", "gu\u00b7ter", "Christ", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mache Verse gern;", "tokens": ["Und", "ma\u00b7che", "Ver\u00b7se", "gern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer dich erblickt, der schuldet dir", "tokens": ["Wer", "dich", "er\u00b7bl