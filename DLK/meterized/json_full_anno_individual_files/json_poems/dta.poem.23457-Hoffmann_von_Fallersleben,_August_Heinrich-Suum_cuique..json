{"dta.poem.23457": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Suum cuique.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir haben's wahrlich trefflich weit gebracht:", "tokens": ["Wir", "ha\u00b7ben's", "wahr\u00b7lich", "treff\u00b7lich", "weit", "ge\u00b7bracht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zur Strafe ward der B\u00fcrgerstand gemacht.", "tokens": ["Zur", "Stra\u00b7fe", "ward", "der", "B\u00fcr\u00b7ger\u00b7stand", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verwirkt sein Adelthum ein Edelmann,", "tokens": ["Ver\u00b7wirkt", "sein", "A\u00b7del\u00b7thum", "ein", "E\u00b7del\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So wird und ist er b\u00fcrgerlich fortan.", "tokens": ["So", "wird", "und", "ist", "er", "b\u00fcr\u00b7ger\u00b7lich", "for\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KON", "VAFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie kommt zu solchem Eingriff doch der Staat?", "tokens": ["Wie", "kommt", "zu", "sol\u00b7chem", "Ein\u00b7griff", "doch", "der", "Staat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PIAT", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Adel soll behalten was er hat;", "tokens": ["Der", "A\u00b7del", "soll", "be\u00b7hal\u00b7ten", "was", "er", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie er seine Tugend tr\u00e4gt allein,", "tokens": ["Und", "wie", "er", "sei\u00b7ne", "Tu\u00b7gend", "tr\u00e4gt", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Soll er auch seines Lasters Tr\u00e4ger sein.", "tokens": ["Soll", "er", "auch", "sei\u00b7nes", "Las\u00b7ters", "Tr\u00e4\u00b7ger", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hat man den Pranger nur f\u00fcr uns erdacht?", "tokens": ["Hat", "man", "den", "Pran\u00b7ger", "nur", "f\u00fcr", "uns", "er\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Zuchthaus nur f\u00fcr unser eins gemacht?", "tokens": ["Das", "Zucht\u00b7haus", "nur", "f\u00fcr", "un\u00b7ser", "eins", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPOSAT", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "I nun, Herr Graf kann auch am Pranger steh'n,", "tokens": ["I", "nun", ",", "Herr", "Graf", "kann", "auch", "am", "Pran\u00b7ger", "steh'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "$,", "NN", "NE", "VMFIN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und Herr Baron kann auch in's Zuchthaus geh'n.", "tokens": ["Und", "Herr", "Ba\u00b7ron", "kann", "auch", "in's", "Zucht\u00b7haus", "geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "NN", "NN", "VMFIN", "ADV", "APPRART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir sind doch in Sibirien noch nicht,", "tokens": ["Wir", "sind", "doch", "in", "Si\u00b7bi\u00b7ri\u00b7en", "noch", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "NE", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo der Verbrecher eine Nummer kriegt!", "tokens": ["Wo", "der", "Ver\u00b7bre\u00b7cher", "ei\u00b7ne", "Num\u00b7mer", "kriegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Individuell' ist noch zur Zeit", "tokens": ["Das", "In\u00b7di\u00b7vi\u00b7du\u00b7ell'", "ist", "noch", "zur", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die sch\u00f6nste deutsche Eigenth\u00fcmlichkeit.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "deut\u00b7sche", "Ei\u00b7gent\u00b7h\u00fcm\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es klingt auch h\u00fcbsch, historisch obendrein,", "tokens": ["Es", "klingt", "auch", "h\u00fcbsch", ",", "his\u00b7to\u00b7risch", "o\u00b7ben\u00b7drein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn man im Zuchthaus aufruft Gro\u00df und Klein:", "tokens": ["Wenn", "man", "im", "Zucht\u00b7haus", "auf\u00b7ruft", "Gro\u00df", "und", "Klein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPRART", "NN", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mandube! Schinderhans! Lips Tullian!", "tokens": ["Man\u00b7du\u00b7be", "!", "Schin\u00b7der\u00b7hans", "!", "Lips", "Tul\u00b7li\u00b7an", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "$.", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Baron von Habenix! Graf Tummerjan!", "tokens": ["Ba\u00b7ron", "von", "Ha\u00b7be\u00b7nix", "!", "Graf", "Tum\u00b7mer\u00b7jan", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$.", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}