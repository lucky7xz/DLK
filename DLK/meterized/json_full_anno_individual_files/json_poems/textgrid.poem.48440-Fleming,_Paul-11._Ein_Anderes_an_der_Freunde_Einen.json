{"textgrid.poem.48440": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11. Ein Anderes an der Freunde Einen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df den Tag deine sein, doch deine nicht alleine:", "tokens": ["La\u00df", "den", "Tag", "dei\u00b7ne", "sein", ",", "doch", "dei\u00b7ne", "nicht", "al\u00b7lei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "VAINF", "$,", "KON", "PPOSAT", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gib uns ein Teil darvon, uns, die wir auch sind deine,", "tokens": ["gib", "uns", "ein", "Teil", "dar\u00b7von", ",", "uns", ",", "die", "wir", "auch", "sind", "dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PAV", "$,", "PPER", "$,", "PRELS", "PPER", "ADV", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "als wie du unser bist! Verschlei\u00df' die liebe Zeit", "tokens": ["als", "wie", "du", "un\u00b7ser", "bist", "!", "Ver\u00b7schlei\u00df'", "die", "lie\u00b7be", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PWAV", "PPER", "ADJD", "VAFIN", "$.", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit angenehmer Lust und leichter Fr\u00f6hlichkeit!", "tokens": ["mit", "an\u00b7ge\u00b7neh\u00b7mer", "Lust", "und", "leich\u00b7ter", "Fr\u00f6h\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "p