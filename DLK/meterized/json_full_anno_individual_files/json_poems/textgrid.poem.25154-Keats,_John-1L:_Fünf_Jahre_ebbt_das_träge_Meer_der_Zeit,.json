rd", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So zeigt Erinnrung deiner Augen Glanz,", "tokens": ["So", "zeigt", "E\u00b7rinn\u00b7rung", "dei\u00b7ner", "Au\u00b7gen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seh ich rosiger Rosen zarten Kranz,", "tokens": ["Und", "seh", "ich", "ro\u00b7si\u00b7ger", "Ro\u00b7sen", "zar\u00b7ten", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Denkt meine Seele nur an dein Gesicht.", "tokens": ["Denkt", "mei\u00b7ne", "See\u00b7le", "nur", "an", "dein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Kein Knospenschwellen kann mein Auge sehen,", "tokens": ["Kein", "Knos\u00b7pen\u00b7schwel\u00b7len", "kann", "mein", "Au\u00b7ge", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ohn' da\u00df mein t\u00f6richt Ohr sich neigt und lauscht,", "tokens": ["Ohn'", "da\u00df", "mein", "t\u00f6\u00b7richt", "Ohr", "sich", "neigt", "und", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "VVFIN", "NN", "PRF", "VVFIN", "KON", 