{"dta.poem.9289": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das vierdte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Ein wundersch\u00f6nes ding ist glatt und kugelrund/", "tokens": ["Ein", "wun\u00b7der\u00b7sch\u00f6\u00b7nes", "ding", "ist", "glatt", "und", "ku\u00b7gel\u00b7rund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur mitten giebet sich ein kleines w\u00e4rtzgen kund.", "tokens": ["Nur", "mit\u00b7ten", "gie\u00b7bet", "sich", "ein", "klei\u00b7nes", "w\u00e4rtz\u00b7gen", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jm anfang ist es hart: Doch wo mans gar zu viel", "tokens": ["Jm", "an\u00b7fang", "ist", "es", "hart", ":", "Doch", "wo", "mans", "gar", "zu", "viel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$.", "KON", "PWAV", "PIS", "ADV", "PTKA", "PIS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zur kurtzweil oder sonst zum handspiel brauchen wil/", "tokens": ["Zur", "kurt\u00b7zweil", "o\u00b7der", "sonst", "zum", "hand\u00b7spiel", "brau\u00b7chen", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird es ziemlich weich/ und schlappert ungefehr/", "tokens": ["So", "wird", "es", "ziem\u00b7lich", "weich", "/", "und", "schlap\u00b7pert", "un\u00b7ge\u00b7fehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$(", "KON", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als wie ein sack voll teig bald hin bald wieder her.", "tokens": ["Als", "wie", "ein", "sack", "voll", "teig", "bald", "hin", "bald", "wie\u00b7der", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ADJD", "NN", "ADV", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man hat es gro\u00df und klein: Und wers zu kauffe tr\u00e4gt/", "tokens": ["Man", "hat", "es", "gro\u00df", "und", "klein", ":", "Und", "wers", "zu", "kauf\u00b7fe", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$.", "KON", "PIS", "PTKZU", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der hat es vor den kram zur schau heraus gelegt.", "tokens": ["Der", "hat", "es", "vor", "den", "kram", "zur", "schau", "he\u00b7raus", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}