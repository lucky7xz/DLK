{"dta.poem.724": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "X.  Der Welt Wollust.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wo Lust ist/ da ist angst; wo Frewd' ist/ da ist klagen. ", "tokens": ["Wo", "Lust", "ist", "/", "da", "ist", "angst", ";", "wo", "Fre\u00b7wd'", "ist", "/", "da", "ist", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$(", "ADV", "VAFIN", "VVPP", "$.", "PWAV", "NN", "VAFIN", "$(", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wer sch\u00f6ne Rosen sieht/ siht Dornen nur dabey", "tokens": ["Wer", "sch\u00f6\u00b7ne", "Ro\u00b7sen", "sieht", "/", "siht", "Dor\u00b7nen", "nur", "da\u00b7bey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "$(", "VVFIN", "NN", "ADV", "PAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein stand/ kein ort/ kein Mensch ist seines Creutzes frey.", "tokens": ["Kein", "stand", "/", "kein", "ort", "/", "kein", "Mensch", "ist", "sei\u00b7nes", "Creut\u00b7zes", "frey", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "$(", "PIAT", "NN", "$(", "PIAT", "NN", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wer lacht; f\u00fchlt wenn er lacht im Hertzen tausend plagen.", "tokens": ["Wer", "lacht", ";", "f\u00fchlt", "wenn", "er", "lacht", "im", "Hert\u00b7zen", "tau\u00b7send", "pla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "VVFIN", "KOUS", "PPER", "VVFIN", "APPRART", "NN", "CARD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer hoch in Ehren sitzt/ mus hohe sorgen tragen.", "tokens": ["Wer", "hoch", "in", "Eh\u00b7ren", "sitzt", "/", "mus", "ho\u00b7he", "sor\u00b7gen", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "NN", "VVFIN", "$(", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer ist der/ Reichthumb acht\u2019/ vnd lo\u00df von kummer sey?", "tokens": ["Wer", "ist", "der", "/", "Reicht\u00b7humb", "acht'", "/", "vnd", "lo\u00df", "von", "kum\u00b7mer", "sey", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "$(", "XY", "XY", "$(", "KON", "ADV", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Armut ist; ist noht. wer kennt wie mancherley", "tokens": ["Wo", "Ar\u00b7mut", "ist", ";", "ist", "noht", ".", "wer", "kennt", "wie", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VAFIN", "$.", "VAFIN", "NN", "$.", "PWS", "VVFIN", "KOKOM", "PIAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Trawrw\u00fcrmer vns die Seel vnd matte ", "tokens": ["Trawr\u00b7w\u00fcr\u00b7mer", "vns", "die", "Seel", "vnd", "mat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich red\u2019 es offenbahr/ so lang als ", "tokens": ["Ich", "red'", "es", "of\u00b7fen\u00b7bahr", "/", "so", "lang", "als"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$(", "ADV", "ADJD", "KOKOM"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Himmel ab bestralt/ mein bleiches Angesicht", "tokens": ["Vom", "Him\u00b7mel", "ab", "be\u00b7stralt", "/", "mein", "blei\u00b7ches", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "VVFIN", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist mir noch nie ein Tag/ der gantz ohn angst bescheret:", "tokens": ["Ist", "mir", "noch", "nie", "ein", "Tag", "/", "der", "gantz", "ohn", "angst", "be\u00b7sche\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$(", "ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O Welt du Threuen thal! recht seelig wird gesch\u00e4tzt;", "tokens": ["O", "Welt", "du", "Threu\u00b7en", "thal", "!", "recht", "see\u00b7lig", "wird", "ge\u00b7sch\u00e4tzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "NN", "VVFIN", "$.", "ADV", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der eh er einen Fus/ hin auff die Erden setzt/ ", "tokens": ["Der", "eh", "er", "ei\u00b7nen", "Fus", "/", "hin", "auff", "die", "Er\u00b7den", "setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PPER", "ART", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bald au\u00df der ", "tokens": ["Bald", "au\u00df", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "ART"], "meter": "++-", "measure": "unknown.measure.di"}}}}}