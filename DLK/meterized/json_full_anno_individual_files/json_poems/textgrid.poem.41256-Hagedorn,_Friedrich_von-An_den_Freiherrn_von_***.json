{"textgrid.poem.41256": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Freiherrn von ***", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der, unverf\u00fchrt von Freuden und von Sorgen,", "tokens": ["Der", ",", "un\u00b7ver\u00b7f\u00fchrt", "von", "Freu\u00b7den", "und", "von", "Sor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie herzlich weinet oder lacht;", "tokens": ["Nie", "herz\u00b7lich", "wei\u00b7net", "o\u00b7der", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der, jede Nacht und jeden Morgen,", "tokens": ["Der", ",", "je\u00b7de", "Nacht", "und", "je\u00b7den", "Mor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ohn' alle Tr\u00e4ume schl\u00e4ft; nur, wann er soll, erwacht;", "tokens": ["Ohn'", "al\u00b7le", "Tr\u00e4u\u00b7me", "schl\u00e4ft", ";", "nur", ",", "wann", "er", "soll", ",", "er\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$.", "ADV", "$,", "PWAV", "PPER", "VMFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der, gleich entfernt von Witz und Unverstande,", "tokens": ["Der", ",", "gleich", "ent\u00b7fernt", "von", "Witz", "und", "Un\u00b7ver\u00b7stan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sich nicht versteigt, auch nicht versteigen kann:", "tokens": ["Sich", "nicht", "ver\u00b7steigt", ",", "auch", "nicht", "ver\u00b7stei\u00b7gen", "kann", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "VVPP", "$,", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Trifft man in dem den gr\u00f6\u00dften Geist nicht an;", "tokens": ["Trifft", "man", "in", "dem", "den", "gr\u00f6\u00df\u00b7ten", "Geist", "nicht", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So ist er doch vielleicht der Gl\u00fccklichste im Lande.", "tokens": ["So", "ist", "er", "doch", "viel\u00b7leicht", "der", "Gl\u00fcck\u00b7lichs\u00b7te", "im", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der, unverf\u00fchrt von Freuden und von Sorgen,", "tokens": ["Der", ",", "un\u00b7ver\u00b7f\u00fchrt", "von", "Freu\u00b7den", "und", "von", "Sor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nie herzlich weinet oder lacht;", "toke