{"dta.poem.2043": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Wolf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint, der Wolf sey mehr zur Strafe, als zum Ver-", "tokens": ["Es", "scheint", ",", "der", "Wolf", "sey", "mehr", "zur", "Stra\u00b7fe", ",", "als", "zum", "Ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NE", "VAFIN", "ADV", "APPRART", "NN", "$,", "KOUS", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gn\u00fcgen, auf der Welt;", "tokens": ["gn\u00fc\u00b7gen", ",", "auf", "der", "Welt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn er ist nicht nur m\u00f6rdrisch, grausam, wild, t\u00fcckisch,", "tokens": ["Denn", "er", "ist", "nicht", "nur", "m\u00f6rd\u00b7risch", ",", "grau\u00b7sam", ",", "wild", ",", "t\u00fc\u00b7ckisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "blutbegierig, gr\u00e4\u00dflich,", "tokens": ["blut\u00b7be\u00b7gie\u00b7rig", ",", "gr\u00e4\u00df\u00b7lich", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und sonderlich fatal den Schafen, er ist dazu noch scheu\u00dflich,", "tokens": ["Und", "son\u00b7der\u00b7lich", "fa\u00b7tal", "den", "Scha\u00b7fen", ",", "er", "ist", "da\u00b7zu", "noch", "scheu\u00df\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ART", "NN", "$,", "PPER", "VAFIN", "PAV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "h\u00e4\u00dflich,", "tokens": ["h\u00e4\u00df\u00b7lich", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Dabey auch f\u00fcrchterlich zu h\u00f6ren, wenn er im Winter heu-", "tokens": ["Da\u00b7bey", "auch", "f\u00fcrch\u00b7ter\u00b7lich", "zu", "h\u00f6\u00b7ren", ",", "wenn", "er", "im", "Win\u00b7ter", "heu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADJD", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "lend bellt;", "tokens": ["lend", "bellt", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.9": {"text": "So, da\u00df man fast bey diesem Thier\u2019 auf die Gedanken kom-", "tokens": ["So", ",", "da\u00df", "man", "fast", "bey", "die\u00b7sem", "Thier'", "auf", "die", "Ge\u00b7dan\u00b7ken", "kom"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PIS", "ADV", "APPR", "PDAT", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "men sollte,", "tokens": ["men", "soll\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Gott w\u00fcrd\u2019 im Wolfe nicht geehrt, und wenn man ihn auch", "tokens": ["Gott", "w\u00fcrd'", "im", "Wol\u00b7fe", "nicht", "ge\u00b7ehrt", ",", "und", "wenn", "man", "ihn", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPRART", "NN", "PTKNEG", "VVPP", "$,", "KON", "KOUS", "PIS", "PPER", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "ehren wollte,", "tokens": ["eh\u00b7ren", "woll\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Weil der zu h\u00e4\u00dflich und zu sch\u00e4dlich. Allein, man mu\u00df", "tokens": ["Weil", "der", "zu", "h\u00e4\u00df\u00b7lich", "und", "zu", "sch\u00e4d\u00b7lich", ".", "Al\u00b7lein", ",", "man", "mu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "PTKA", "ADJD", "KON", "PTKA", "ADJD", "$.", "ADV", "$,", "PIS", "VMFIN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "hier wohl erw\u00e4gen,", "tokens": ["hier", "wohl", "er\u00b7w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Da\u00df, ob bey ihm des Sch\u00f6pfers Wege sich nicht so klar", "tokens": ["Da\u00df", ",", "ob", "bey", "ihm", "des", "Sch\u00f6p\u00b7fers", "We\u00b7ge", "sich", "nicht", "so", "klar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "APPR", "PPER", "ART", "NN", "NN", "PRF", "PTKNEG", "ADV", "ADJD"], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "zu Tage legen,", "tokens": ["zu", "Ta\u00b7ge", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Wir darum gleich nicht schlie\u00dfen m\u00fcssen: Wenn auf der", "tokens": ["Wir", "da\u00b7rum", "gleich", "nicht", "schlie\u00b7\u00dfen", "m\u00fcs\u00b7sen", ":", "Wenn", "auf", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PAV", "ADV", "PTKNEG", "VVINF", "VMINF", "$.", "KOUS", "APPR", "ART"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Welt kein Wolf vorhanden,", "tokens": ["Welt", "kein", "Wolf", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NE", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "So w\u00e4r\u2019 es besser, oder denken, vielleicht w\u00e4r\u2019 er von", "tokens": ["So", "w\u00e4r'", "es", "bes\u00b7ser", ",", "o\u00b7der", "den\u00b7ken", ",", "viel\u00b7leicht", "w\u00e4r'", "er", "von"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KON", "VVINF", "$,", "ADV", "VAFIN", "PPER", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.20": {"text": "selbst entstanden.", "tokens": ["selbst", "ent\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "O nein! Denn da\u00df wir es nicht wissen, wozu er eigentlich", "tokens": ["O", "nein", "!", "Denn", "da\u00df", "wir", "es", "nicht", "wis\u00b7sen", ",", "wo\u00b7zu", "er", "ei\u00b7gent\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "KON", "KOUS", "PPER", "PPER", "PTKNEG", "VVINF", "$,", "PWAV", "PPER", "ADV"], "meter": "-+-+-+-+-++--+-", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "gemacht,", "tokens": ["ge\u00b7macht", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Zeigt deutlich unsern Unverstand, umschr\u00e4nkten Geist,", "tokens": ["Zeigt", "deut\u00b7lich", "un\u00b7sern", "Un\u00b7ver\u00b7stand", ",", "um\u00b7schr\u00e4nk\u00b7ten", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "und Unbedacht,", "tokens": ["und", "Un\u00b7be\u00b7dacht", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Doch keinen Fehl der Sch\u00f6pfung an. Zudem, wenn wir", "tokens": ["Doch", "kei\u00b7nen", "Fehl", "der", "Sch\u00f6p\u00b7fung", "an", ".", "Zu\u00b7dem", ",", "wenn", "wir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "PTKVZ", "$.", "PAV", "$,", "KOUS", "PPER"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.26": {"text": "es wohl ergr\u00fcnden,", "tokens": ["es", "wohl", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "Sind auch in W\u00f6lfen viele Dinge zu unserm Nutzen noch", "tokens": ["Sind", "auch", "in", "W\u00f6l\u00b7fen", "vie\u00b7le", "Din\u00b7ge", "zu", "un\u00b7serm", "Nut\u00b7zen", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "zu finden.", "tokens": ["zu", "fin\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Wir haben nicht nur ihrer B\u00e4lge im scharfen Frost uns", "tokens": ["Wir", "ha\u00b7ben", "nicht", "nur", "ih\u00b7rer", "B\u00e4l\u00b7ge", "im", "schar\u00b7fen", "Frost", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "zu erfreuen;", "tokens": ["zu", "er\u00b7freu\u00b7en", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.31": {"text": "Es dienen ihrer Glieder viele zu gro\u00dfem Nutz in Arzeneyen.", "tokens": ["Es", "die\u00b7nen", "ih\u00b7rer", "Glie\u00b7der", "vie\u00b7le", "zu", "gro\u00b7\u00dfem", "Nutz", "in", "Ar\u00b7ze\u00b7ne\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PIS", "APPR", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-+-+--+", "measure": "iambic.octa.plus.relaxed"}}}}}