{"dta.poem.20123": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1851", "urn": "urn:nbn:de:kobv:b4-200905191321", "language": ["de:0.99"], "booktitle": "Fontane, Theodor: Gedichte. Berlin, 1851."}, "poem": {"stanza.1": {"line.1": {"text": "In England w\u00fcthen zwei Tyrannen:", "tokens": ["In", "En\u00b7gland", "w\u00fct\u00b7hen", "zwei", "Ty\u00b7ran\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "CARD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der K\u00f6nig Jacob und die Pest,", "tokens": ["Der", "K\u00f6\u00b7nig", "Ja\u00b7cob", "und", "die", "Pest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und Jener immer raft von dannen,", "tokens": ["Und", "Je\u00b7ner", "im\u00b7mer", "raft", "von", "dan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADV", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was diese noch am Leben l\u00e4\u00dft.", "tokens": ["Was", "die\u00b7se", "noch", "am", "Le\u00b7ben", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gesetz und Recht \u2014 des Volkes Pathen", "tokens": ["Ge\u00b7setz", "und", "Recht", "des", "Vol\u00b7kes", "Pa\u00b7then"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$(", "ART", "NN", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Sind jedes H\u00f6flings Spiel und Spott,", "tokens": ["Sind", "je\u00b7des", "H\u00f6f\u00b7lings", "Spiel", "und", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schon seufzen gilt als hochverrathen,", "tokens": ["Schon", "seuf\u00b7zen", "gilt", "als", "hoch\u00b7ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "KOUS", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und f\u00fchrt zu Kerker und Schaffott.", "tokens": ["Und", "f\u00fchrt", "zu", "Ker\u00b7ker", "und", "Schaf\u00b7fott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Staube liegt die heilge Sache", "tokens": ["Im", "Stau\u00b7be", "liegt", "die", "heil\u00b7ge", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Volks, und bettelt vor dem Thron,", "tokens": ["Des", "Volks", ",", "und", "bet\u00b7telt", "vor", "dem", "Thron", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schon aber weben Ha\u00df und Rache", "tokens": ["Schon", "a\u00b7ber", "we\u00b7ben", "Ha\u00df", "und", "Ra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Siegeskleid \u2014 Revolution.", "tokens": ["Dein", "Sie\u00b7ges\u00b7kleid", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Schon schlummert Er in goldner Wiege,", "tokens": ["Schon", "schlum\u00b7mert", "Er", "in", "gold\u00b7ner", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "De\u00df Stirne jenen Stempel tr\u00e4gt,", "tokens": ["De\u00df", "Stir\u00b7ne", "je\u00b7nen", "Stem\u00b7pel", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den auf des Mordgeweihten Z\u00fcge", "tokens": ["Den", "auf", "des", "Mord\u00b7ge\u00b7weih\u00b7ten", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Jugend auf das Schicksal pr\u00e4gt;", "tokens": ["Von", "Ju\u00b7gend", "auf", "das", "Schick\u00b7sal", "pr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Schon athmet Cromwell, schon allnachtens", "tokens": ["Schon", "ath\u00b7met", "Crom\u00b7well", ",", "schon", "all\u00b7nach\u00b7tens"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Tritt Englands Zukunft vor ihn hin,", "tokens": ["Tritt", "En\u00b7glands", "Zu\u00b7kunft", "vor", "ihn", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und legt die Keime k\u00fcnftgen Trachtens", "tokens": ["Und", "legt", "die", "Kei\u00b7me", "k\u00fcnft\u00b7gen", "Trach\u00b7tens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In seinen ruhmbegiergen Sinn;", "tokens": ["In", "sei\u00b7nen", "ruhm\u00b7be\u00b7gier\u00b7gen", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Schon graut der Tag, nur noch ein Kurzes", "tokens": ["Schon", "graut", "der", "Tag", ",", "nur", "noch", "ein", "Kur\u00b7zes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "So steigt die Sonne blutigroth,", "tokens": ["So", "steigt", "die", "Son\u00b7ne", "blu\u00b7ti\u00b7groth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Doch f\u00fcr die Zeichen nahnden Sturzes", "tokens": ["Doch", "f\u00fcr", "die", "Zei\u00b7chen", "nahn\u00b7den", "Stur\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ist jede F\u00fcrstenseele todt.", "tokens": ["Ist", "je\u00b7de", "F\u00fcrs\u00b7ten\u00b7see\u00b7le", "todt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "An Jacobs Hof dr\u00fcckt ihren Stempel", "tokens": ["An", "Ja\u00b7cobs", "Hof", "dr\u00fcckt", "ih\u00b7ren", "Stem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die Lust noch auf jedwede Stirn,", "tokens": ["Die", "Lust", "noch", "auf", "jed\u00b7we\u00b7de", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch ist sein Schlo\u00df ein Bacchustempel:", "tokens": ["Noch", "ist", "sein", "Schlo\u00df", "ein", "Bac\u00b7chus\u00b7tem\u00b7pel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Flasche gilt, es gilt die Dirn\u2019;", "tokens": ["Die", "Fla\u00b7sche", "gilt", ",", "es", "gilt", "die", "Dirn'", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl rast die Pest, doch jedes Opfer", "tokens": ["Wohl", "rast", "die", "Pest", ",", "doch", "je\u00b7des", "Op\u00b7fer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Scheint nur zu rufen: \u201eFrisch gelebt!", "tokens": ["Scheint", "nur", "zu", "ru\u00b7fen", ":", "\u201e", "Frisch", "ge\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKZU", "VVINF", "$.", "$(", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer wei\u00df es ob der Tod den Klopfer", "tokens": ["Wer", "wei\u00df", "es", "ob", "der", "Tod", "den", "Klop\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht bald an Deiner Th\u00fcre hebt.\u201c", "tokens": ["Nicht", "bald", "an", "Dei\u00b7ner", "Th\u00fc\u00b7re", "hebt", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Es ist, als ob das nahe Sterben", "tokens": ["Es", "ist", ",", "als", "ob", "das", "na\u00b7he", "Ster\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dem Leben tausend Reize leiht,", "tokens": ["Dem", "Le\u00b7ben", "tau\u00b7send", "Rei\u00b7ze", "leiht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Man jagt um seine Lust zu werben;", "tokens": ["Man", "jagt", "um", "sei\u00b7ne", "Lust", "zu", "wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}}, "stanza.4": {"line.1": {"text": "Bei Hof ist Ball; schau, \u2014 scheint nicht eben", "tokens": ["Bei", "Hof", "ist", "Ball", ";", "schau", ",", "scheint", "nicht", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "VAFIN", "NN", "$.", "PTKVZ", "$,", "$(", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Die Sch\u00f6nheit selbst daher zu schweben?", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "selbst", "da\u00b7her", "zu", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer anders kann sie sein die Schlanke,", "tokens": ["Wer", "an\u00b7ders", "kann", "sie", "sein", "die", "Schlan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu der, wenn sie vor\u00fcberrauscht,", "tokens": ["Zu", "der", ",", "wenn", "sie", "vor\u00b7\u00fc\u00b7berr\u00b7auscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein jeder Sinn sich und Gedanke", "tokens": ["Ein", "je\u00b7der", "Sinn", "sich", "und", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "PRF", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hinneiget und gefangen lauscht!", "tokens": ["Hin\u00b7nei\u00b7get", "und", "ge\u00b7fan\u00b7gen", "lauscht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "An ihrer Sch\u00f6nheit stumpft der Hohn.", "tokens": ["An", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "stumpft", "der", "Hohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mehr als ein K\u00f6nig auf dem Thron,", "tokens": ["Mehr", "als", "ein", "K\u00f6\u00b7nig", "auf", "dem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn seine Blicke zornig irren,", "tokens": ["Wenn", "sei\u00b7ne", "Bli\u00b7cke", "zor\u00b7nig", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Vermag ", "tokens": ["Ver\u00b7mag"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Das blo\u00dfe Flattern ihrer Locken", "tokens": ["Das", "blo\u00b7\u00dfe", "Flat\u00b7tern", "ih\u00b7rer", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Macht schon des H\u00f6flings Zunge stocken,", "tokens": ["Macht", "schon", "des", "H\u00f6f\u00b7lings", "Zun\u00b7ge", "sto\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "Und selbst der Neid auf den sie sp\u00e4ht,", "tokens": ["Und", "selbst", "der", "Neid", "auf", "den", "sie", "sp\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Bewundert solche Majest\u00e4t.", "tokens": ["Be\u00b7wun\u00b7dert", "sol\u00b7che", "Ma\u00b7jes\u00b7t\u00e4t", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was ist\u2019s, das bis in\u2019s tiefste Herze", "tokens": ["Was", "ist's", ",", "das", "bis", "in's", "tiefs\u00b7te", "Her\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Selbst das Geschmei\u00df am Hof durchbebt,", "tokens": ["Selbst", "das", "Ge\u00b7schmei\u00df", "am", "Hof", "durch\u00b7bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wenn anmuthvoll, mit leichtem Scherze,", "tokens": ["Wenn", "an\u00b7muth\u00b7voll", ",", "mit", "leich\u00b7tem", "Scher\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lady Essex n\u00e4her schwebt!", "tokens": ["Die", "La\u00b7dy", "Es\u00b7sex", "n\u00e4\u00b7her", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist es das junogleiche Haupt,", "tokens": ["Ist", "es", "das", "ju\u00b7no\u00b7glei\u00b7che", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-