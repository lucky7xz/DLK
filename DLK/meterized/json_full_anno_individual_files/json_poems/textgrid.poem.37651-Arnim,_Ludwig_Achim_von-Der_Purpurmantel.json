{"textgrid.poem.37651": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Purpurmantel", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00f6nig ging vertrieben", "tokens": ["Der", "K\u00f6\u00b7nig", "ging", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch ein Feindesland,", "tokens": ["Durch", "ein", "Fein\u00b7des\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die mit ihm sind geblieben,", "tokens": ["Die", "mit", "ihm", "sind", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bindet da kein Band.", "tokens": ["Bin\u00b7det", "da", "kein", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da sind in Noth nur blieben,", "tokens": ["Da", "sind", "in", "Noth", "nur", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die er oft verkannt,", "tokens": ["Die", "er", "oft", "ver\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn streng sind, die uns lieben,", "tokens": ["Denn", "streng", "sind", ",", "die", "uns", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fest ist der Liebe Band.", "tokens": ["Fest", "ist", "der", "Lie\u00b7be", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Er sah wie seine Feinde", "tokens": ["Er", "sah", "wie", "sei\u00b7ne", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort am Ufer stehn,", "tokens": ["Dort", "am", "U\u00b7fer", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "An Freundes Busen weinte,", "tokens": ["An", "Freun\u00b7des", "Bu\u00b7sen", "wein\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wollte schier vergehn.", "tokens": ["Woll\u00b7te", "schier", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbich habe nichts zu geben,", "tokens": ["\u00bb", "ich", "ha\u00b7be", "nichts", "zu", "ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als den Mantel mein,", "tokens": ["Als", "den", "Man\u00b7tel", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der gab mir Noth im Leben,", "tokens": ["Der", "gab", "mir", "Noth", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bald auch Todespein.\u00ab", "tokens": ["Bald", "auch", "To\u00b7de\u00b7spein", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbwar meine Noth begl\u00fccken", "tokens": ["\u00bb", "war", "mei\u00b7ne", "Noth", "be\u00b7gl\u00fc\u00b7cken"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eurer Tage Preis,", "tokens": ["Eu\u00b7rer", "Ta\u00b7ge", "Preis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Den Purpur rei\u00dft in St\u00fccken,", "tokens": ["Den", "Pur\u00b7pur", "rei\u00dft", "in", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geb' ihn allen preis!\u00ab", "tokens": ["Geb'", "ihn", "al\u00b7len", "preis", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PIS", "PTKVZ", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Er fa\u00dft, so viel er konnte,", "tokens": ["Er", "fa\u00dft", ",", "so", "viel", "er", "konn\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jeder ri\u00df sein St\u00fcck,", "tokens": ["Je\u00b7der", "ri\u00df", "sein", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Es auf dem Herzen sonnte,", "tokens": ["Es", "auf", "dem", "Her\u00b7zen", "sonn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie ein Stern im Gl\u00fcck.", "tokens": ["Wie", "ein", "Stern", "im", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbnun flieht!\u00ab befiehlt der K\u00f6nig,", "tokens": ["\u00bb", "nun", "flieht", "!", "\u00ab", "be\u00b7fiehlt", "der", "K\u00f6\u00b7nig", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$.", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bblasset mich und flieht!\u00ab", "tokens": ["\u00bb", "las\u00b7set", "mich", "und", "flieht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "KON", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zum erstenmal der K\u00f6nig", "tokens": ["Zum", "ers\u00b7ten\u00b7mal", "der", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ungehorsam sieht.", "tokens": ["Un\u00b7ge\u00b7hor\u00b7sam", "sieht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbder Purpur ist zerrissen,", "tokens": ["\u00bb", "der", "Pur\u00b7pur", "ist", "zer\u00b7ris\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus ist nun dein Reich,", "tokens": ["Aus", "ist", "nun", "dein", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wir handeln nach Gewissen,", "tokens": ["Wir", "han\u00b7deln", "nach", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind nun alle gleich.\u00ab", "tokens": ["Sind", "nun", "al\u00b7le", "gleich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PIS", "ADV", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbwir stehn am j\u00fcngsten Tage", "tokens": ["\u00bb", "wir", "stehn", "am", "j\u00fcng\u00b7sten", "Ta\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon dem Weltgericht,", "tokens": ["Schon", "dem", "Welt\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auf hoher Himmelswage;", "tokens": ["Auf", "ho\u00b7her", "Him\u00b7mels\u00b7wa\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "K\u00f6nig der K\u00f6nige richt!\u00ab", "tokens": ["K\u00f6\u00b7nig", "der", "K\u00f6\u00b7ni\u00b7ge", "richt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbder Purpurstern kann blitzen,", "tokens": ["\u00bb", "der", "Pur\u00b7purs\u00b7tern", "kann", "blit\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "W\u00e4rmen euer Herz,", "tokens": ["W\u00e4r\u00b7men", "eu\u00b7er", "Herz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nicht wie ein Harnisch sch\u00fctzen! \u2013", "tokens": ["Nicht", "wie", "ein", "Har\u00b7nisch", "sch\u00fct\u00b7zen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "K\u00e4ltend deckt das Erz!\u00ab \u2013", "tokens": ["K\u00e4l\u00b7tend", "deckt", "das", "Erz", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Die Feinde sahn sie blicken,", "tokens": ["Die", "Fein\u00b7de", "sahn", "sie", "bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sahn die Sterne hell,", "tokens": ["Sahn", "die", "Ster\u00b7ne", "hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und ihre Pfeile dr\u00fccken", "tokens": ["Und", "ih\u00b7re", "Pfei\u00b7le", "dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In die Herzen schnell.", "tokens": ["In", "die", "Her\u00b7zen", "schnell", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Die purpurrothen St\u00fccke", "tokens": ["Die", "pur\u00b7pur\u00b7ro\u00b7then", "St\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tauchte in Blutes Farb,", "tokens": ["Tauch\u00b7te", "in", "Blu\u00b7tes", "Farb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Feind ging \u00fcber die Br\u00fccke,", "tokens": ["Der", "Feind", "ging", "\u00fc\u00b7ber", "die", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nicht den Schmuck verdarb.", "tokens": ["Nicht", "den", "Schmuck", "ver\u00b7darb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Es stiftet einen Orden", "tokens": ["Es", "stif\u00b7tet", "ei\u00b7nen", "Or\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Diesem nach der Feind,", "tokens": ["Die\u00b7sem", "nach", "der", "Feind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der ordnet seine Horden,", "tokens": ["Der", "ord\u00b7net", "sei\u00b7ne", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie durch Treu' vereint.", "tokens": ["Sie", "durch", "Treu'", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Der K\u00f6nig ging vertrieben", "tokens": ["Der", "K\u00f6\u00b7nig", "ging", "ver\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch ein Feindesland,", "tokens": ["Durch", "ein", "Fein\u00b7des\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die mit ihm sind geblieben,", "tokens": ["Die", "mit", "ihm", "sind", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bindet da kein Band.", "tokens": ["Bin\u00b7det", "da", "kein", "Band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Da sind in Noth nur blieben,", "tokens": ["Da", "sind", "in", "Noth", "nur", "blie\u00b7ben", ",