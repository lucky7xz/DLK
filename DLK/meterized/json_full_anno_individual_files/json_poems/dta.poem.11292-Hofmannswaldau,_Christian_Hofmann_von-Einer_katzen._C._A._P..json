{"dta.poem.11292": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Einer katzen.  \n C. A. P.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.71", "fr:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So streckst du endlich alle viere nu,", "tokens": ["So", "streckst", "du", "end\u00b7lich", "al\u00b7le", "vie\u00b7re", "nu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIAT", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du wackrer m\u00e4use-fischer du!", "tokens": ["Du", "wack\u00b7rer", "m\u00e4u\u00b7se\u00b7fi\u00b7scher", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schleu\u00dft den thor-weg deines lebens;", "tokens": ["Und", "schleu\u00dft", "den", "thor\u00b7weg", "dei\u00b7nes", "le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die worte sind anitzo nur vergebens.", "tokens": ["Die", "wor\u00b7te", "sind", "a\u00b7nit\u00b7zo", "nur", "ver\u00b7ge\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die m\u00e4use sollen dich, als einen biedermann,", "tokens": ["Die", "m\u00e4u\u00b7se", "sol\u00b7len", "dich", ",", "als", "ei\u00b7nen", "bie\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit trauer-m\u00e4nteln nachbegleiten,", "tokens": ["Mit", "trau\u00b7e\u00b7rm\u00e4n\u00b7teln", "nach\u00b7be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die katzen-compagnie soll mauen, was sie kan,", "tokens": ["Die", "kat\u00b7zen\u00b7com\u00b7pa\u00b7gnie", "soll", "mau\u00b7en", ",", "was", "sie", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dir dein grab bereiten;", "tokens": ["Und", "dir", "dein", "grab", "be\u00b7rei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Aus lauter m\u00e4use-fellen", "tokens": ["Aus", "lau\u00b7ter", "m\u00e4u\u00b7se\u00b7fel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Bey deinen grabe-stellen", "tokens": ["Bey", "dei\u00b7nen", "gra\u00b7be\u00b7stel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Soll dich mit lob auszieren", "tokens": ["Soll", "dich", "mit", "lob", "aus\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der grosse kater, und dir parentiren.", "tokens": ["Der", "gros\u00b7se", "ka\u00b7ter", ",", "und", "dir", "pa\u00b7ren\u00b7ti\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}