{"textgrid.poem.32704": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Deutsches Trinklied", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, ihr meine deutschen Br\u00fcder!", "tokens": ["Auf", ",", "ihr", "mei\u00b7ne", "deut\u00b7schen", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feiern wollen wir die Nacht!", "tokens": ["Fei\u00b7ern", "wol\u00b7len", "wir", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schallen sollen frohe Lieder,", "tokens": ["Schal\u00b7len", "sol\u00b7len", "fro\u00b7he", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis der Morgenstern erwacht!", "tokens": ["Bis", "der", "Mor\u00b7gens\u00b7tern", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "--+---+", "measure": "anapaest.init"}, "line.5": {"text": "La\u00dft die Stunden uns befl\u00fcgeln!", "tokens": ["La\u00dft", "die", "Stun\u00b7den", "uns", "be\u00b7fl\u00fc\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier ist echter, deutscher Wein,", "tokens": ["Hier", "ist", "ech\u00b7ter", ",", "deut\u00b7scher", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ausgepre\u00dft auf deutschen H\u00fcgeln,", "tokens": ["Aus\u00b7ge\u00b7pre\u00dft", "auf", "deut\u00b7schen", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und gereift am alten Rhein!", "tokens": ["Und", "ge\u00b7reift", "am", "al\u00b7ten", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer im fremden Tranke prasset,", "tokens": ["Wer", "im", "frem\u00b7den", "Tran\u00b7ke", "pras\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meide dieses freie Land!", "tokens": ["Mei\u00b7de", "die\u00b7ses", "frei\u00b7e", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer des Rheines Gabe hasset,", "tokens": ["Wer", "des", "Rhei\u00b7nes", "Ga\u00b7be", "has\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Trink', als Knecht, am Marnestrand!", "tokens": ["Trink'", ",", "als", "Knecht", ",", "am", "Mar\u00b7nes\u00b7trand", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Singt in lauten Wechselch\u00f6ren!", "tokens": ["Singt", "in", "lau\u00b7ten", "Wech\u00b7selc\u00b7h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ebert, Hagedorn und Gleim", "tokens": ["E\u00b7bert", ",", "Ha\u00b7ge\u00b7dorn", "und", "Gleim"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sollen uns Ges\u00e4nge lehren;", "tokens": ["Sol\u00b7len", "uns", "Ge\u00b7s\u00e4n\u00b7ge", "leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn wir lieben deutschen Reim.", "tokens": ["Denn", "wir", "lie\u00b7ben", "deut\u00b7schen", "Reim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Trotz geboten allen denen,", "tokens": ["Trotz", "ge\u00b7bo\u00b7ten", "al\u00b7len", "de\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PIAT", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die, mit Galliens Gezier,", "tokens": ["Die", ",", "mit", "Gal\u00b7li\u00b7ens", "Ge\u00b7zier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Nervensprache h\u00f6hnen!", "tokens": ["Uns\u00b7re", "Ner\u00b7ven\u00b7spra\u00b7che", "h\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrer spotten wollen wir!", "tokens": ["Ih\u00b7rer", "spot\u00b7ten", "wol\u00b7len", "wir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VMFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihrer spotten! Aber, Br\u00fcder!", "tokens": ["Ih\u00b7rer", "spot\u00b7ten", "!", "A\u00b7ber", ",", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "$.", "KON", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stark und deutsch, wie dieser Wein,", "tokens": ["Stark", "und", "deutsch", ",", "wie", "die\u00b7ser", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "PWAV", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sollen immer unsre Lieder", "tokens": ["Sol\u00b7len", "im\u00b7mer", "uns\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bei Gelag und Mahlen sein.", "tokens": ["Bei", "Ge\u00b7lag", "und", "Mah\u00b7len", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unser Kaiser Joseph lebe!", "tokens": ["Un\u00b7ser", "Kai\u00b7ser", "Jo\u00b7se\u00b7ph", "le\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Biedermann und deutsch ist er.", "tokens": ["Bie\u00b7der\u00b7mann", "und", "deutsch", "ist", "er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPER", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Hermanns hoher Schatten schwebe", "tokens": ["Her\u00b7manns", "ho\u00b7her", "Schat\u00b7ten", "schwe\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Waltend um den Enkel her,", "tokens": ["Wal\u00b7tend", "um", "den", "En\u00b7kel", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df er, mutig in Gefahren,", "tokens": ["Da\u00df", "er", ",", "mu\u00b7tig", "in", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich dem Vaterlande weih',", "tokens": ["Sich", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "weih'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, in Kindeskinder-Jahren,", "tokens": ["Und", ",", "in", "Kin\u00b7des\u00b7kin\u00b7der\u00b7Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Muster aller Kaiser sei!", "tokens": ["Mus\u00b7ter", "al\u00b7ler", "Kai\u00b7ser", "sei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jeder F\u00fcrst im Lande lebe,", "tokens": ["Je\u00b7der", "F\u00fcrst", "im", "Lan\u00b7de", "le\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der es treu und redlich meint!", "tokens": ["Der", "es", "treu", "und", "red\u00b7lich", "meint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedem wackern Deutschen gebe", "tokens": ["Je\u00b7dem", "wa\u00b7ckern", "Deut\u00b7schen", "ge\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott den w\u00e4rmsten Herzensfreund,", "tokens": ["Gott", "den", "w\u00e4rms\u00b7ten", "Her\u00b7zens\u00b7freund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein Weib in seine H\u00fctte,", "tokens": ["Und", "ein", "Weib", "in", "sei\u00b7ne", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das ihm sei ein Himmelreich,", "tokens": ["Das", "ihm", "sei", "ein", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und ihm Kinder geb', an Sitte", "tokens": ["Und", "ihm", "Kin\u00b7der", "geb'", ",", "an", "Sit\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "NN", "VVFIN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seinen braven V\u00e4tern gleich!", "tokens": ["Sei\u00b7nen", "bra\u00b7ven", "V\u00e4\u00b7tern", "gleich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Leben sollen alle Sch\u00f6nen,", "tokens": ["Le\u00b7ben", "sol\u00b7len", "al\u00b7le", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die, von fremder Thorheit rein,", "tokens": ["Die", ",", "von", "frem\u00b7der", "Thor\u00b7heit", "rein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur des Vaterlandes S\u00f6hnen", "tokens": ["Nur", "des", "Va\u00b7ter\u00b7lan\u00b7des", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren keuschen Busen weihn!", "tokens": ["Ih\u00b7ren", "keu\u00b7schen", "Bu\u00b7sen", "weihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deutsche Redlichkeit und Treue", "tokens": ["Deut\u00b7sche", "Red\u00b7lich\u00b7keit", "und", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht uns ihrer Liebe wert;", "tokens": ["Macht", "uns", "ih\u00b7rer", "Lie\u00b7be", "wert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Drum, wohlauf! der Tugend weihe", "tokens": ["Drum", ",", "wohl\u00b7auf", "!", "der", "Tu\u00b7gend", "wei\u00b7he"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "PWAV", "$.", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Jeder sich, der sie begehrt!", "tokens": ["Je\u00b7der", "sich", ",", "der", "sie", "be\u00b7gehrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auf, ihr meine deutschen Br\u00fcder!", "tokens": ["Auf", ",", "ihr", "mei\u00b7ne", "deut\u00b7schen", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feiern wollen wir die Nacht!", "tokens": ["Fei\u00b7ern", "wol\u00b7len", "wir", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schallen sollen frohe Lieder,", "tokens": ["Schal\u00b7len", "sol\u00b7len", "fro\u00b7he", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis der Morgenstern erwacht!", "tokens": ["Bis", "der", "Mor\u00b7gens\u00b7tern", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "--+---+", "measure": "anapaest.init"}, "line.5": {"text": "La\u00dft die Stunden uns befl\u00fcgeln!", "tokens": ["La\u00dft", "die", "Stun\u00b7den", "uns", "be\u00b7fl\u00fc\u00b7geln", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier ist echter, deutscher Wein,", "tokens": ["Hier", "ist", "ech\u00b7ter", ",", "deut\u00b7scher", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ausgepre\u00dft auf deutschen H\u00fcgeln,", "tokens": ["Aus\u00b7ge\u00b7pre\u00dft", "auf", "deut\u00b7schen", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und gereift am alten Rhein!", "tokens": ["Und", "ge\u00b7reift", "am", "al\u00b7ten", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wer im fremden Tranke prasset,", "tokens": ["Wer", "im", "frem\u00b7den", "Tran\u00b7ke", "pras\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meide dieses freie Land!", "tokens": ["Mei\u00b7de", "die\u00b7ses", "frei\u00b7e", "Land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer des Rheines Gabe hasset,", "tokens": ["Wer", "des", "Rhei\u00b7nes", "Ga\u00b7be", "has\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Trink', als Knecht, am Marnestrand!", "tokens": ["Trink'", ",", "als", "Knecht", ",", "am", "Mar\u00b7nes\u00b7trand", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Singt in lauten Wechselch\u00f6ren!", "tokens": ["Singt", "in", "lau\u00b7ten", "Wech\u00b7selc\u00b7h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ebert, Hagedorn und Gleim", "tokens": ["E\u00b7bert", ",", "Ha\u00b7ge\u00b7dorn", "und", "Gleim"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sollen uns Ges\u00e4nge lehren;", "tokens": ["Sol\u00b7len", "uns", "Ge\u00b7s\u00e4n\u00b7ge", "leh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn wir lieben deutschen Reim.", "tokens": ["Denn", "wir", "lie\u00b7ben", "deut\u00b7schen", "Reim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Trotz geboten allen denen,", "tokens": ["Trotz", "ge\u00b7bo\u00b7ten", "al\u00b7len", "de\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PIAT", "PDS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die, mit Galliens Gezier,", "tokens": ["Die", ",", "mit", "Gal\u00b7li\u00b7ens", "Ge\u00b7zier", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": 