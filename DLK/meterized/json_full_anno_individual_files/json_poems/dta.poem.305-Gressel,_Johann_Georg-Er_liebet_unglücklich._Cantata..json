{"dta.poem.305": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er liebet ungl\u00fccklich.  \n  Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Soll ich ein Ball", "tokens": ["Soll", "ich", "ein", "Ball"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Des falschen Gl\u00fcckes seyn?", "tokens": ["Des", "fal\u00b7schen", "Gl\u00fc\u00b7ckes", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der bald erh\u00f6ht zur blauen H\u00f6he steiget", "tokens": ["Der", "bald", "er\u00b7h\u00f6ht", "zur", "blau\u00b7en", "H\u00f6\u00b7he", "stei\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald zu der Grufft der H\u00f6llen sich hin neiget.", "tokens": ["Bald", "zu", "der", "Grufft", "der", "H\u00f6l\u00b7len", "sich", "hin", "nei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O herbe Quaal!", "tokens": ["O", "her\u00b7be", "Qua\u00b7al", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Soll ich ein Ball", "tokens": ["Soll", "ich", "ein", "Ball"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Des falschen Gl\u00fcckes seyn?", "tokens": ["Des", "fal\u00b7schen", "Gl\u00fc\u00b7ckes", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich mu\u00df verbannt", "tokens": ["Ich", "mu\u00df", "ver\u00b7bannt"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Von meiner sch\u00f6nen leben:", "tokens": ["Von", "mei\u00b7ner", "sch\u00f6\u00b7nen", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ander bey dem Wollust-Strand/", "tokens": ["Da", "an\u00b7der", "bey", "dem", "Wol\u00b7lust\u00b7Strand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In reichen Freuden schweben:", "tokens": ["In", "rei\u00b7chen", "Freu\u00b7den", "schwe\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn ander in der Lust", "tokens": ["Wenn", "an\u00b7der", "in", "der", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Auf Zucker-Rosen gehn/", "tokens": ["Auf", "Zu\u00b7cke\u00b7rRo\u00b7sen", "gehn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So mu\u00df ich in dem Wust", "tokens": ["So", "mu\u00df", "ich", "in", "dem", "Wust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der scharffen Dornen stehn.", "tokens": ["Der", "scharf\u00b7fen", "Dor\u00b7nen", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gedult mein Geist!", "tokens": ["Ge\u00b7dult", "mein", "Geist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "La\u00df nur den Himmel sorgen.", "tokens": ["La\u00df", "nur", "den", "Him\u00b7mel", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nach langer Nacht geht auf", "tokens": ["Nach", "lan\u00b7ger", "Nacht", "geht", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der offt erw\u00fcnschte Morgen;", "tokens": ["Der", "offt", "er\u00b7w\u00fcnschte", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Das Schiff beschliest den Lauff", "tokens": ["Das", "Schiff", "be\u00b7schliest", "den", "Lauff"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn sich der Haven weist.", "tokens": ["Wenn", "sich", "der", "Ha\u00b7ven", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gedult mein Geist!", "tokens": ["Ge\u00b7dult", "mein", "Geist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "La\u00df nur den Himmel sorgen.", "tokens": ["La\u00df", "nur", "den", "Him\u00b7mel", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So wirst du auch", "tokens": ["So", "wirst", "du", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Nach langen Qu\u00e4hlen", "tokens": ["Nach", "lan\u00b7gen", "Qu\u00e4h\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die frohe Stunde z\u00e4hlen/", "tokens": ["Die", "fro\u00b7he", "Stun\u00b7de", "z\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da nach der Liebe Brauch", "tokens": ["Da", "nach", "der", "Lie\u00b7be", "Brauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Galle wird vers\u00fc\u00dft", "tokens": ["Die", "Gal\u00b7le", "wird", "ver\u00b7s\u00fc\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wenn dich dein Leben k\u00fc\u00dft.", "tokens": ["Wenn", "dich", "dein", "Le\u00b7ben", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum bleib getreu", "tokens": ["Drum", "bleib", "ge\u00b7treu"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Trotz\u2019 aller Winde Rasen/", "tokens": ["Trotz'", "al\u00b7ler", "Win\u00b7de", "Ra\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df keine Glei\u00dfnerey", "tokens": ["La\u00df", "kei\u00b7ne", "Glei\u00df\u00b7ne\u00b7rey"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In deine Seegel blasen:", "tokens": ["In", "dei\u00b7ne", "See\u00b7gel", "bla\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So wird das falsche Gl\u00fcck", "tokens": ["So", "wird", "das", "fal\u00b7sche", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Besch\u00e4met seiner T\u00fcck", "tokens": ["Be\u00b7sch\u00e4\u00b7met", "sei\u00b7ner", "T\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der treuen Liebe weichen/", "tokens": ["Der", "treu\u00b7en", "Lie\u00b7be", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und du den Port erreichen.", "tokens": ["Und", "du", "den", "Port", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Drum bleib getreu", "tokens": ["Drum", "bleib", "ge\u00b7treu"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Trotz\u2019 aller Winde Rasen.", "tokens": ["Trotz'", "al\u00b7ler", "Win\u00b7de", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}