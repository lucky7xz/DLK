{"dta.poem.18335": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Nicht wahr?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Und ihre K\u00f6nige sind's nicht minder,", "tokens": ["Und", "ih\u00b7re", "K\u00f6\u00b7ni\u00b7ge", "sin\u00b7d's", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Lachen und weinen im selben Nu,", "tokens": ["La\u00b7chen", "und", "wei\u00b7nen", "im", "sel\u00b7ben", "Nu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "APPRART", "ADJA", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Spielen mitunter auch Blindekuh", "tokens": ["Spie\u00b7len", "mi\u00b7tun\u00b7ter", "auch", "Blin\u00b7de\u00b7kuh"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Und ihre Fibel", "tokens": ["Und", "ih\u00b7re", "Fi\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Benennt sich Bibel!", "tokens": ["Be\u00b7nennt", "sich", "Bi\u00b7bel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}