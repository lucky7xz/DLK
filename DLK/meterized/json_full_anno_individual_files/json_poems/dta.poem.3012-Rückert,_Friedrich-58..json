{"dta.poem.3012": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei, die sich lieben, sind einander so un\u00e4hnlich,", "tokens": ["Zwei", ",", "die", "sich", "lie\u00b7ben", ",", "sind", "ein\u00b7an\u00b7der", "so", "un\u00b7\u00e4hn\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "PRELS", "PRF", "VVINF", "$,", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da\u00df der Verstand nicht wei\u00df, was sie bewegt so sehnlich,", "tokens": ["Da\u00df", "der", "Ver\u00b7stand", "nicht", "wei\u00df", ",", "was", "sie", "be\u00b7wegt", "so", "sehn\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und endlich meint, da\u00df von Un\u00e4hnlichkeit getrieben", "tokens": ["Und", "end\u00b7lich", "meint", ",", "da\u00df", "von", "Un\u00b7\u00e4hn\u00b7lich\u00b7keit", "ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sey'n, einander zur Ver\u00e4hnlichung zu lieben.", "tokens": ["Sie", "sey'n", ",", "ein\u00b7an\u00b7der", "zur", "Ver\u00b7\u00e4hn\u00b7li\u00b7chung", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Allein mit K\u00fcnstlerblick, mit liebesf\u00e4higem Auge,", "tokens": ["Al\u00b7lein", "mit", "K\u00fcnst\u00b7ler\u00b7blick", ",", "mit", "lie\u00b7bes\u00b7f\u00e4\u00b7hi\u00b7gem", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sieh recht die beiden an, und ihre Seelen sauge;", "tokens": ["Sieh", "recht", "die", "bei\u00b7den", "an", ",", "und", "ih\u00b7re", "See\u00b7len", "sau\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "PIAT", "PTKVZ", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So siehst du aus der Z\u00fcg' Un\u00e4hnlichkeiten steigen", "tokens": ["So", "siehst", "du", "aus", "der", "Z\u00fcg'", "Un\u00b7\u00e4hn\u00b7lich\u00b7kei\u00b7ten", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geistige Aehnlichkeit, wie Bl\u00fctenduft sich zeigen;", "tokens": ["Geis\u00b7ti\u00b7ge", "A\u00b7ehn\u00b7lich\u00b7keit", ",", "wie", "Bl\u00fc\u00b7ten\u00b7duft", "sich", "zei\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "NN", "PRF", "VVINF", "$."], "meter": "+---+-+-+-+-+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Der, wenn Einbildungskraft ihn walten und entfalten", "tokens": ["Der", ",", "wenn", "Ein\u00b7bil\u00b7dungs\u00b7kraft", "ihn", "wal\u00b7ten", "und", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "NN", "PPER", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich l\u00e4\u00dft, die Beiden wird zu Einem umgestalten.", "tokens": ["Sich", "l\u00e4\u00dft", ",", "die", "Bei\u00b7den", "wird", "zu", "Ei\u00b7nem", "um\u00b7ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "PRELS", "PIS", "VAFIN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wenn ich ein Maler w\u00e4r', und h\u00e4tt' ein Lieb ein feines,", "tokens": ["Wenn", "ich", "ein", "Ma\u00b7ler", "w\u00e4r'", ",", "und", "h\u00e4tt'", "ein", "Lieb", "ein", "fei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "KON", "VAFIN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich malt' uns ohne Zwang als zwei zugleich und eines.", "tokens": ["Ich", "malt'", "uns", "oh\u00b7ne", "Zwang", "als", "zwei", "zu\u00b7gleich", "und", "ei\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KOKOM", "CARD", "ADV", "KON", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}