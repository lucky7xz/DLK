{"dta.poem.7720": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dich nehm' ich heute nicht zum Tischgenossen an,", "tokens": ["Dich", "nehm'", "ich", "heu\u00b7te", "nicht", "zum", "Tischge\u00b7nos\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-++-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Wenn du nicht deiner Pflicht erst hast genuggethan.", "tokens": ["Wenn", "du", "nicht", "dei\u00b7ner", "Pflicht", "erst", "hast", "ge\u00b7nug\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PPOSAT", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der wicht'gen gro\u00dfen Pflicht, die Federn mir zu schneiden,", "tokens": ["Der", "wicht'\u00b7gen", "gro\u00b7\u00dfen", "Pflicht", ",", "die", "Fe\u00b7dern", "mir", "zu", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit ich ewige Gedanken will bekleiden.", "tokens": ["Wo\u00b7mit", "ich", "e\u00b7wi\u00b7ge", "Ge\u00b7dan\u00b7ken", "will", "be\u00b7klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn das ist dein Beruf, die Pfeile mir zu schnitzen,", "tokens": ["Denn", "das", "ist", "dein", "Be\u00b7ruf", ",", "die", "Pfei\u00b7le", "mir", "zu", "schnit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ich verschie\u00dfe sie mit oder ohne Spitzen.", "tokens": ["Und", "ich", "ver\u00b7schie\u00b7\u00dfe", "sie", "mit", "o\u00b7der", "oh\u00b7ne", "Spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was, fragt ein Leser, der nach Versen Hunger litt,", "tokens": ["Was", ",", "fragt", "ein", "Le\u00b7ser", ",", "der", "nach", "Ver\u00b7sen", "Hun\u00b7ger", "litt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schreibt ", "tokens": ["Schreibt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}}}}}