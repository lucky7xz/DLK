{"textgrid.poem.39887": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den Wassern Babylons", "tokens": ["Auf", "den", "Was\u00b7sern", "Ba\u00b7by\u00b7lons"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Flutet Alexanders Nachen,", "tokens": ["Flu\u00b7tet", "A\u00b7lex\u00b7an\u00b7ders", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Samt den Freunden seines Throns,", "tokens": ["Samt", "den", "Freun\u00b7den", "sei\u00b7nes", "Throns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die des Siegers Haupt bewachen.", "tokens": ["Die", "des", "Sie\u00b7gers", "Haupt", "be\u00b7wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch er lies't ein tr\u00fcb Geschick", "tokens": ["Doch", "er", "lies't", "ein", "tr\u00fcb", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In dem Flug des Wellenschaumes,", "tokens": ["In", "dem", "Flug", "des", "Wel\u00b7len\u00b7schau\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in seinem finstern Blick", "tokens": ["Und", "in", "sei\u00b7nem", "fins\u00b7tern", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schwebt das Mi\u00dftraun eines Traumes.", "tokens": ["Schwebt", "das", "Mi\u00df\u00b7traun", "ei\u00b7nes", "Trau\u00b7mes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seines Diademes Pracht", "tokens": ["Sei\u00b7nes", "Di\u00b7a\u00b7de\u00b7mes", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Leuchtet in den braunen Locken;", "tokens": ["Leuch\u00b7tet", "in", "den", "brau\u00b7nen", "Lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses schaute j\u00fcngst zu Nacht", "tokens": ["Die\u00b7ses", "schau\u00b7te", "j\u00fcngst", "zu", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Er auf fremdem Haupt erschrocken.", "tokens": ["Er", "auf", "frem\u00b7dem", "Haupt", "er\u00b7schro\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Widerfunkelt's aus der Flut", "tokens": ["Wi\u00b7der\u00b7fun\u00b7kelt's", "aus", "der", "Flut"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jetzt im Schimmer der Gesteine:", "tokens": ["Jetzt", "im", "Schim\u00b7mer", "der", "Ge\u00b7stei\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch sein Auge fragend ruht", "tokens": ["Doch", "sein", "Au\u00b7ge", "fra\u00b7gend", "ruht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf der Stirn, ob's auch die seine?", "tokens": ["Auf", "der", "Stirn", ",", "ob's", "auch", "die", "sei\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "ADV", "ART", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In der Woge Spiegel sieht", "tokens": ["In", "der", "Wo\u00b7ge", "Spie\u00b7gel", "sieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Er den Himmel dunkler br\u00fcten", "tokens": ["Er", "den", "Him\u00b7mel", "dunk\u00b7ler", "br\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwanke V\u00f6gel ohne Lied", "tokens": ["Schwan\u00b7ke", "V\u00f6\u00b7gel", "oh\u00b7ne", "Lied"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Uferweiden traurig h\u00fcten.", "tokens": ["U\u00b7fer\u00b7wei\u00b7den", "trau\u00b7rig", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ein K\u00f6nigsgrabmal sp\u00fclt", "tokens": ["Und", "ein", "K\u00f6\u00b7nigs\u00b7grab\u00b7mal", "sp\u00fclt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihm die Flut im Bild entgegen,", "tokens": ["Ihm", "die", "Flut", "im", "Bild", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Rauschende Gezweige f\u00fchlt", "tokens": ["Rau\u00b7schen\u00b7de", "Ge\u00b7zwei\u00b7ge", "f\u00fchlt"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Er in seinem Haar sich regen.", "tokens": ["Er", "in", "sei\u00b7nem", "Haar", "sich", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PRF", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und der Nachen schie\u00dft vorbei: \u2013", "tokens": ["Und", "der", "Na\u00b7chen", "schie\u00dft", "vor\u00b7bei", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun erst merkt der F\u00fcrst mit Bangen,", "tokens": ["Nun", "erst", "merkt", "der", "F\u00fcrst", "mit", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sein Haupt des Schmuckes frei,", "tokens": ["Da\u00df", "sein", "Haupt", "des", "Schmu\u00b7ckes", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht ihn in den B\u00fcschen hangen.", "tokens": ["Sieht", "ihn", "in", "den", "B\u00fc\u00b7schen", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueber dem zerfallnen Grab", "tokens": ["Ue\u00b7ber", "dem", "zer\u00b7fall\u00b7nen", "Grab"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwebt die junge K\u00f6nigsbinde,", "tokens": ["Schwebt", "die", "jun\u00b7ge", "K\u00f6\u00b7nigs\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie erreicht mit Speer und Stab", "tokens": ["Sie", "er\u00b7reicht", "mit", "Speer", "und", "Stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Keiner von dem Hofgesinde.", "tokens": ["Kei\u00b7ner", "von", "dem", "Hof\u00b7ge\u00b7sin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Von des Helden Seite schwingt", "tokens": ["Von", "des", "Hel\u00b7den", "Sei\u00b7te", "schwingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzt ein Freund sich in die Wellen,", "tokens": ["Jetzt", "ein", "Freund", "sich", "in", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der sich durch die Wogen ringt", "tokens": ["Der", "sich", "durch", "die", "Wo\u00b7gen", "ringt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis sie ihn an's Ufer schwellen.", "tokens": ["Bis", "sie", "ihn", "an's", "U\u00b7fer", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und, das Kleinod unbenetzt,", "tokens": ["Und", ",", "das", "Klei\u00b7nod", "un\u00b7be\u00b7netzt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gl\u00e4nzend, seinem Herrn zu reichen,", "tokens": ["Gl\u00e4n\u00b7zend", ",", "sei\u00b7nem", "Herrn", "zu", "rei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Hat er selbst sich's aufgesetzt,", "tokens": ["Hat", "er", "selbst", "sich's", "auf\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df der K\u00f6nig mu\u00df erbleichen.", "tokens": ["Da\u00df", "der", "K\u00f6\u00b7nig", "mu\u00df", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der erkennt das stolze Haupt,", "tokens": ["Der", "er\u00b7kennt", "das", "stol\u00b7ze", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie er es im Traum gesehen;", "tokens": ["Wie", "er", "es", "im", "Traum", "ge\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus dem Wasser, flutumstaubt,", "tokens": ["Aus", "dem", "Was\u00b7ser", ",", "flu\u00b7tums\u00b7taubt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sieht gekr\u00f6nt er's auferstehen.", "tokens": ["Sieht", "ge\u00b7kr\u00f6nt", "er's", "auf\u00b7er\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PIS", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nun h\u00f6rt er sich in's Ohr", "tokens": ["Und", "nun", "h\u00f6rt", "er", "sich", "in's", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch des Sehers Stimme beben:", "tokens": ["Auch", "des", "Se\u00b7hers", "Stim\u00b7me", "be\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00bbherr! welch Zeichen! sieh dich vor!", "tokens": ["\u00bb", "herr", "!", "welch", "Zei\u00b7chen", "!", "sieh", "dich", "vor", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "PWAT", "NN", "$.", "VVIMP", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00df den Kronendieb nicht leben!\u00ab", "tokens": ["La\u00df", "den", "Kro\u00b7nen\u00b7dieb", "nicht", "le\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sinnend starr der K\u00f6nig sitzt,", "tokens": ["Sin\u00b7nend", "starr", "der", "K\u00f6\u00b7nig", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es jagen sich Gedanken,", "tokens": ["Und", "es", "ja\u00b7gen", "sich", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis ihm's durch die Seele blitzt,", "tokens": ["Bis", "ihm's", "durch", "die", "See\u00b7le", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Durch der Bilder wildes Schwanken.", "tokens": ["Durch", "der", "Bil\u00b7der", "wil\u00b7des", "Schwan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jenes Freundes Blutgestalt,", "tokens": ["Je\u00b7nes", "Freun\u00b7des", "Blut\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den er l\u00e4ngst bei'm Trunk erstochen,", "tokens": ["Den", "er", "l\u00e4ngst", "bei'm", "Trunk", "er\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Naht, aus dunklem Aufenthalt", "tokens": ["Naht", ",", "aus", "dunk\u00b7lem", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Seines Innern vorgebrochen.", "tokens": ["Sei\u00b7nes", "In\u00b7nern", "vor\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "In geheimer Tiefe regt", "tokens": ["In", "ge\u00b7hei\u00b7mer", "Tie\u00b7fe", "regt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich's von Scham und bittern Schmerzen;", "tokens": ["Sich's", "von", "Scham", "und", "bit\u00b7tern", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem blut'gen Schatten pflegt", "tokens": ["Mit", "dem", "blut'\u00b7gen", "Schat\u00b7ten", "pflegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stille Zwiesprach er