", "nichts", "auf", "wei\u00b7ter", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Zerst\u00f6ren nichts, gehn gar genau,", "tokens": ["Zer\u00b7st\u00f6\u00b7ren", "nichts", ",", "gehn", "gar", "ge\u00b7nau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Sie rauben sanft; der s\u00fc\u00dfe Raub", "tokens": ["Sie", "rau\u00b7ben", "sanft", ";", "der", "s\u00fc\u00b7\u00dfe", "Raub"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wird Honig und war Blumenstaub.", "tokens": ["Wird", "Ho\u00b7nig", "und", "war", "Blu\u00b7men\u00b7staub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Sie schwirren fort \u2013 die Sonn' erwacht!", "tokens": ["Sie", "schwir\u00b7ren", "fort", "\u2013", "die", "Sonn'", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Sieh, wie die Aue lacht!", "tokens": ["Sieh", ",", "wie", "die", "Au\u00b7e", "lacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}