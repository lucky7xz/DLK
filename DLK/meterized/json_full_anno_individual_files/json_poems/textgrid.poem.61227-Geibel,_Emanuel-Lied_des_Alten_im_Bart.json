{"textgrid.poem.61227": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Lied des Alten im Bart", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch tiefe Nacht ein Brausen zieht,", "tokens": ["Durch", "tie\u00b7fe", "Nacht", "ein", "Brau\u00b7sen", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und beugt die knospenden Reiser,", "tokens": ["Und", "beugt", "die", "knos\u00b7pen\u00b7den", "Rei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Winde klingt ein altes Lied,", "tokens": ["Im", "Win\u00b7de", "klingt", "ein", "al\u00b7tes", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Lied vom deutschen Kaiser.", "tokens": ["Das", "Lied", "vom", "deut\u00b7schen", "Kai\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Sinn ist wild, mein Sinn ist schwer,", "tokens": ["Mein", "Sinn", "ist", "wild", ",", "mein", "Sinn", "ist", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kann nicht lassen vom Lauschen;", "tokens": ["Ich", "kann", "nicht", "las\u00b7sen", "vom", "Lau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es klingt, als z\u00f6g' in den Wolken ein Heer,", "tokens": ["Es", "klingt", ",", "als", "z\u00f6g'", "in", "den", "Wol\u00b7ken", "ein", "Heer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es klingt wie Adlers Rauschen.", "tokens": ["Es", "klingt", "wie", "Ad\u00b7lers", "Rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Viel tausend Herzen sind entfacht", "tokens": ["Viel", "tau\u00b7send", "Her\u00b7zen", "sind", "ent\u00b7facht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und harren wie das meine,", "tokens": ["Und", "har\u00b7ren", "wie", "das", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf allen Bergen halten sie Wacht,", "tokens": ["Auf", "al\u00b7len", "Ber\u00b7gen", "hal\u00b7ten", "sie", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ob rot der Tag erscheine.", "tokens": ["Ob", "rot", "der", "Tag", "er\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Deutschland, die sch\u00f6n geschm\u00fcckte Braut,", "tokens": ["Deutschland", ",", "die", "sch\u00f6n", "ge\u00b7schm\u00fcck\u00b7te", "Braut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon schl\u00e4ft sie leis und leiser -", "tokens": ["Schon", "schl\u00e4ft", "sie", "leis", "und", "lei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wann weckst du sie mit Trompetenlaut,", "tokens": ["Wann", "weckst", "du", "sie", "mit", "Trom\u00b7pe\u00b7ten\u00b7laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wann f\u00fchrst du sie heim, mein Kaiser!", "tokens": ["Wann", "f\u00fchrst", "du", "sie", "heim", ",", "mein", "Kai\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Durch tiefe Nacht ein Brausen zieht,", "tokens": ["Durch", "tie\u00b7fe", "Nacht", "ein", "Brau\u00b7sen", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und beugt die knospenden Reiser,", "tokens": ["