{"textgrid.poem.36800": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Engl\u00e4nder im Miserere", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist ein Jammer, mit Lady und Mi\u00df erst durch sich zu k\u00e4mpfen,", "tokens": ["Das", "ist", "ein", "Jam\u00b7mer", ",", "mit", "La\u00b7dy", "und", "Mi\u00df", "erst", "durch", "sich", "zu", "k\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "ADV", "APPR", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+--++-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und drei Stunden und mehr steh' ich und wart' ich nun schon.", "tokens": ["Und", "drei", "Stun\u00b7den", "und", "mehr", "steh'", "ich", "und", "wart'", "ich", "nun", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "KON", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Traun das Reisen ist doch beschwerlich, ich w\u00e4r' es zufrieden,", "tokens": ["Traun", "das", "Rei\u00b7sen", "ist", "doch", "be\u00b7schwer\u00b7lich", ",", "ich", "w\u00e4r'", "es", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "W\u00e4r' es doch einmal vorbei, h\u00e4tt' ich's doch einmal geh\u00f6rt.", "tokens": ["W\u00e4r'", "es", "doch"