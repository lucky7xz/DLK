{"dta.poem.15244": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "79.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was hab' ich nun erk\u00e4mpft, da\u00df stumpf sind meine Waffen?", "tokens": ["Was", "hab'", "ich", "nun", "er\u00b7k\u00e4mpft", ",", "da\u00df", "stumpf", "sind", "mei\u00b7ne", "Waf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich habe viel geschafft, und habe nichts geschaffen.", "tokens": ["Ich", "ha\u00b7be", "viel", "ge\u00b7schafft", ",", "und", "ha\u00b7be", "nichts", "ge\u00b7schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "KON", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}