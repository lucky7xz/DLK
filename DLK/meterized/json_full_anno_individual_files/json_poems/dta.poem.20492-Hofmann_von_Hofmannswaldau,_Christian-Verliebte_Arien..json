{"dta.poem.20492": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Parthenie/ du strenge meiner seelen/", "tokens": ["Par\u00b7the\u00b7nie", "/", "du", "stren\u00b7ge", "mei\u00b7ner", "see\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vor der mein hertz in f\u00e4sseln schmacht/", "tokens": ["Vor", "der", "mein", "hertz", "in", "f\u00e4s\u00b7seln", "schmacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "APPR", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie lange soll mein tr\u00fcber geist sich qv\u00e4len/", "tokens": ["Wie", "lan\u00b7ge", "soll", "mein", "tr\u00fc\u00b7ber", "geist", "sich", "qv\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den du um seine freyheit bracht?", "tokens": ["Den", "du", "um", "sei\u00b7ne", "frey\u00b7heit", "bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie lange soll ich mich betr\u00fcben/", "tokens": ["Wie", "lan\u00b7ge", "soll", "ich", "mich", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verschwiegen und mit schmertzen lieben?", "tokens": ["Ver\u00b7schwie\u00b7gen", "und", "mit", "schmert\u00b7zen", "lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kan ja kein ach in deine seele dringen/", "tokens": ["Kan", "ja", "kein", "ach", "in", "dei\u00b7ne", "see\u00b7le", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das g\u00f6ttin dir dein sclave schickt;", "tokens": ["Das", "g\u00f6t\u00b7tin", "dir", "dein", "scla\u00b7ve", "schickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So h\u00f6re doch die schweren f\u00e4ssel klingen/", "tokens": ["So", "h\u00f6\u00b7re", "doch", "die", "schwe\u00b7ren", "f\u00e4s\u00b7sel", "klin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wovor der abgrund selbst erschrickt.", "tokens": ["Wo\u00b7vor", "der", "ab\u00b7grund", "selbst", "er\u00b7schrickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wiltu den diamanten gleichen/", "tokens": ["Wil\u00b7tu", "den", "di\u00b7a\u00b7man\u00b7ten", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So kan dich nichts als blut erweichen.", "tokens": ["So", "kan", "dich", "nichts", "als", "blut", "er\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So siehe denn hier blut und thr\u00e4nen rinnen/", "tokens": ["So", "sie\u00b7he", "denn", "hier", "blut", "und", "thr\u00e4\u00b7nen", "rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das pfand so ich dir liefern kan/", "tokens": ["Das", "pfand", "so", "ich", "dir", "lie\u00b7fern", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du wirst es nicht von dir verstossen k\u00f6nnen/", "tokens": ["Du", "wirst", "es", "nicht", "von", "dir", "ver\u00b7stos\u00b7sen", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der himmel nimmt solch opffer an;", "tokens": ["Der", "him\u00b7mel", "nimmt", "solch", "opf\u00b7fer", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum wird ja auch vor deinen augen", "tokens": ["Drum", "wird", "ja", "auch", "vor", "dei\u00b7nen", "au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein himmel-reines opffer taugen.", "tokens": ["Ein", "him\u00b7mel\u00b7rei\u00b7nes", "opf\u00b7fer", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es wird die welt dir s\u00fcssen weyrauch brennen/", "tokens": ["Es", "wird", "die", "welt", "dir", "s\u00fcs\u00b7sen", "wey\u00b7rauch", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn du/ o sch\u00f6nstes g\u00f6tter-kind/", "tokens": ["Wenn", "du", "/", "o", "sch\u00f6ns\u00b7tes", "g\u00f6t\u00b7ter\u00b7kind", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "FM", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auff deinem thron dich l\u00e4st barmhertzig nennen/", "tokens": ["Auff", "dei\u00b7nem", "thron", "dich", "l\u00e4st", "barm\u00b7hert\u00b7zig", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn man vor dir genade find/", "tokens": ["Wenn", "man", "vor", "dir", "ge\u00b7na\u00b7de", "find", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wird dein thun den engeln gleichen/", "tokens": ["So", "wird", "dein", "thun", "den", "en\u00b7geln", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "VVINF", "ART", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dein erbarmen sie erweichen.", "tokens": ["Und", "dein", "er\u00b7bar\u00b7men", "sie", "er\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Verzeihe mir/ o g\u00f6ttin/ meiner seelen/", "tokens": ["Ver\u00b7zei\u00b7he", "mir", "/", "o", "g\u00f6t\u00b7tin", "/", "mei\u00b7ner", "see\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "FM", "NE", "$(", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sich ein knecht so viel erk\u00fchnt/", "tokens": ["Da\u00df", "sich", "ein", "knecht", "so", "viel", "er\u00b7k\u00fchnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df ein mensch/ mit hertzens angst und qv\u00e4len/", "tokens": ["Und", "da\u00df", "ein", "mensch", "/", "mit", "hert\u00b7zens", "angst", "und", "qv\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$(", "APPR", "ADV", "VVPP", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um deine himmels sch\u00f6nheit dient/", "tokens": ["Um", "dei\u00b7ne", "him\u00b7mels", "sch\u00f6n\u00b7heit", "dient", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du gleichst mit deiner pracht der sonnen/", "tokens": ["Du", "gleichst", "mit", "dei\u00b7ner", "pracht", "der", "son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von der auch staub den schein genommen.", "tokens": ["Von", "der", "auch", "staub", "den", "schein", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "VVFIN", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}