ambic.tetra"}}, "stanza.4": {"line.1": {"text": "O steh mir immerdar im R\u00fccken,", "tokens": ["O", "steh", "mir", "im\u00b7mer\u00b7dar", "im", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich im Feld mit meiner Zeit!", "tokens": ["Bin", "ich", "im", "Feld", "mit", "mei\u00b7ner", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deinen hellen Mutterblicken", "tokens": ["Mit", "dei\u00b7nen", "hel\u00b7len", "Mut\u00b7ter\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruh auf mir, auch im w\u00e4rmsten Streit!", "tokens": ["Ruh", "auf", "mir", ",", "auch", "im", "w\u00e4rms\u00b7ten", "Streit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sollte mich mein St\u00fcndlein finden,", "tokens": ["Und", "soll\u00b7te", "mich", "mein", "St\u00fcnd\u00b7lein", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schnell decke mich mit Rasen zu!", "tokens": ["Schnell", "de\u00b7cke", "mich", "mit", "Ra\u00b7sen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O selig Sterben und Verschwinden", "tokens": ["O", "se\u00b7lig", "Ster\u00b7ben", "und", "Ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "In deines Urgrunds tiefste Ruh!", "tokens": ["In", "dei\u00b7nes", "Ur\u00b7grunds", "tiefs\u00b7te", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00fcll mich in deine gr\u00fcnen Decken", "tokens": ["H\u00fcll", "mich", "in", "dei\u00b7ne", "gr\u00fc\u00b7nen", "De\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lulle mich mit Liedern ein!", "tokens": ["Und", "lul\u00b7le", "mich", "mit", "Lie\u00b7dern", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei guter Zeit magst du mich wecken", "tokens": ["Bei", "gu\u00b7ter", "Zeit", "magst", "du", "mich", "we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit eines jungen Tages Schein!", "tokens": ["Mit", "ei\u00b7nes", "jun\u00b7gen", "Ta\u00b7ges", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hab mich m\u00fcd in dir ergangen,", "tokens": ["Ich", "hab", "mich", "m\u00fcd", "in", "dir", "