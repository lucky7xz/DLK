", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Herz verdorben oder krank!", "tokens": ["Mein", "Herz", "ver\u00b7dor\u00b7ben", "o\u00b7der", "krank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "O steh mir immerdar im R\u00fccken,", "tokens": ["O", "steh", "mir", "im\u00b7mer\u00b7dar", "im", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bin ich im Feld mit meiner Zeit!", "tokens": ["Bin", "ich", "im", "Feld", "mit", "mei\u00b7ner", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit deinen hellen Mutterblicken", "tokens": ["Mit", "dei\u00b7nen", "hel\u00b7len", "Mut\u00b7ter\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ruh auf mir, auch im w\u00e4rmsten Streit!", "tokens": ["Ruh", "auf", "mir", ",", "auch", "im", "w\u00e4rms\u00b7ten", "Streit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sollte mich mein St\u00fcndlein finden,", "tokens": ["Und", "soll\u00b7te", "mich", "mein", "St\u00fcnd\u00b7lein", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schnell decke mich mit Rasen zu!", "tokens": ["Schnell", "de\u00b7cke", "mich", "mit", "Ra\u00b7sen", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O selig Sterben und Verschwinden", "tokens": ["O", "se\u00b7lig", "Ster\u00b7ben", "und", "Ver\u00b7schwin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "In deines Urgrunds tiefste Ruh!", "tokens": ["In", "dei\u00b7nes", "Ur\u00b7grunds", "tiefs\u00b7te", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}