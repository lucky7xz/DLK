{"textgrid.poem.50712": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Abendlied an die Natur", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00fcll mich in deine gr\u00fcnen Decken", "tokens": ["H\u00fcll", "mich", "in", "dei\u00b7ne", "gr\u00fc\u00b7nen", "De\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lulle mich mit Liedern ein!", "tokens": ["Und", "lul\u00b7le", "mich", "mit", "Lie\u00b7dern", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei guter Zeit magst du mich wecken", "tokens": ["Bei", "gu\u00b7ter", "Zeit", "magst", "du", "mich", "we\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit eines jungen Tages Schein!", "tokens": ["Mit", "ei\u00b7nes", "jun\u00b7gen", "Ta\u00b7ges", "Schein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich hab mich m\u00fcd in dir ergangen,", "tokens": ["Ich", "hab", "mich", "m\u00fcd", "in", "dir", "er\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Aug ist matt von deiner Pracht;", "tokens": ["Mein", "Aug", "i