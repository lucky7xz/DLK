{"dta.poem.8783": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00fcllen Reben dich in Lauben ein;", "tokens": ["H\u00fcl\u00b7len", "Re\u00b7ben", "dich", "in", "Lau\u00b7ben", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn am H\u00fcgel dich umf\u00e4ngt der Schlaf,", "tokens": ["Wenn", "am", "H\u00fc\u00b7gel", "dich", "um\u00b7f\u00e4ngt", "der", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Girren dich verliebte Tauben ein;", "tokens": ["Gir\u00b7ren", "dich", "ver\u00b7lieb\u00b7te", "Tau\u00b7ben", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn du liebst, so stellen Engel sich,", "tokens": ["Wenn", "du", "liebst", ",", "so", "stel\u00b7len", "En\u00b7gel", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "ADJA", "NN", "PRF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Die der Sorge dich berauben, ein;", "tokens": ["Die", "der", "Sor\u00b7ge", "dich", "be\u00b7rau\u00b7ben", ",", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVINF", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Da die Weisheit m\u00fchevoll du fandst,", "tokens": ["Da", "die", "Weis\u00b7heit", "m\u00fc\u00b7he\u00b7voll", "du", "fandst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "B\u00fc\u00dftest doch du nicht den Glauben ein.", "tokens": ["B\u00fc\u00df\u00b7test", "doch", "du", "nicht", "den", "Glau\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}