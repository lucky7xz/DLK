{"dta.poem.8598": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Gefangene .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling ist zu Berg und Thal gekommen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "zu", "Berg", "und", "Thal", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Freudenruf ist durch die Luft erklungen;", "tokens": ["Sein", "Freu\u00b7den\u00b7ruf", "ist", "durch", "die", "Luft", "er\u00b7klun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kaum hat die Erd' im Schlafe ihn vernommen,", "tokens": ["Kaum", "hat", "die", "Erd'", "im", "Schla\u00b7fe", "ihn", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist sie vom Wintertraum emporgesprungen,", "tokens": ["Ist", "sie", "vom", "Win\u00b7ter\u00b7traum", "em\u00b7por\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der ihren Busen deckte bang und kalt.", "tokens": ["Der", "ih\u00b7ren", "Bu\u00b7sen", "deck\u00b7te", "bang", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In alle Fernen ist der Ruf gedrungen", "tokens": ["In", "al\u00b7le", "Fer\u00b7nen", "ist", "der", "Ruf", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mit freundlicher, s\u00fc\u00dflockender Gewalt,", "tokens": ["Mit", "freund\u00b7li\u00b7cher", ",", "s\u00fc\u00df\u00b7lo\u00b7cken\u00b7der", "Ge\u00b7walt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da\u00df ihres Nest's die Schwalbe nun gedenket,", "tokens": ["Da\u00df", "ih\u00b7res", "Nest's", "die", "Schwal\u00b7be", "nun", "ge\u00b7den\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Weit \u00fcber's Meer zur trauten H\u00fctte wallt,", "tokens": ["Weit", "\u00fc\u00b7ber's", "Meer", "zur", "trau\u00b7ten", "H\u00fct\u00b7te", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df seinen Flug der Storch nun heimw\u00e4rts lenket,", "tokens": ["Da\u00df", "sei\u00b7nen", "Flug", "der", "Storch", "nun", "heim\u00b7w\u00e4rts", "len\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Verlassend schnell das Schilf im fernen S\u00fcden.", "tokens": ["Ver\u00b7las\u00b7send", "schnell", "das", "Schilf", "im", "fer\u00b7nen", "S\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADJD", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die Blume bl\u00fcht, der bunte Falter senket", "tokens": ["Die", "Blu\u00b7me", "bl\u00fcht", ",", "der", "bun\u00b7te", "Fal\u00b7ter", "sen\u00b7ket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Auf sie die Fl\u00fcgel hin, die wonnem\u00fcden.", "tokens": ["Auf", "sie", "die", "Fl\u00fc\u00b7gel", "hin", ",", "die", "won\u00b7ne\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Mit Bl\u00fcthen haben sich geschm\u00fcckt die B\u00e4ume,", "tokens": ["Mit", "Bl\u00fc\u00b7then", "ha\u00b7ben", "sich", "ge\u00b7schm\u00fcckt", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PRF", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Da\u00df sie zu Lieb' und Sang die S\u00e4nger l\u00fcden.", "tokens": ["Da\u00df", "sie", "zu", "Lieb'", "und", "Sang", "die", "S\u00e4n\u00b7ger", "l\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Schon singt und bringt uns Paradiesestr\u00e4ume", "tokens": ["Schon", "singt", "und", "bringt", "uns", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Im Bl\u00fcthenstrauche dort die Nachtigall;", "tokens": ["Im", "Bl\u00fc\u00b7then\u00b7strau\u00b7che", "dort", "die", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Melodisch zieht der Bach durch Waldesr\u00e4ume.", "tokens": ["Me\u00b7lo\u00b7disch", "zieht", "der", "Bach", "durch", "Wal\u00b7des\u00b7r\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Der Hirte fl\u00f6tet und der Wiederhall;", "tokens": ["Der", "Hir\u00b7te", "fl\u00f6\u00b7tet", "und", "der", "Wie\u00b7der\u00b7hall", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Zur gr\u00fcnen Alpe kehrt die Herde wieder,", "tokens": ["Zur", "gr\u00fc\u00b7nen", "Al\u00b7pe", "kehrt", "die", "Her\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Weithin ert\u00f6nt der frohe Glockenschall.", "tokens": ["Weit\u00b7hin", "er\u00b7t\u00f6nt", "der", "fro\u00b7he", "Glo\u00b7cken\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Der Wildbach st\u00fcrzt vom Klippenhange nieder,", "tokens": ["Der", "Wild\u00b7bach", "st\u00fcrzt", "vom", "Klip\u00b7pen\u00b7han\u00b7ge", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ein Freudenthr\u00e4nenstrom, dem Lenz entgegen;", "tokens": ["Ein", "Freu\u00b7dent\u00b7hr\u00e4\u00b7nen\u00b7strom", ",", "dem", "Lenz", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Es sonnen sich der Alpe Felsenglieder", "tokens": ["Es", "son\u00b7nen", "sich", "der", "Al\u00b7pe", "Fel\u00b7sen\u00b7glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Im warmen Schein, der Fr\u00fchling klimmt verwegen", "tokens": ["Im", "war\u00b7men", "Schein", ",", "der", "Fr\u00fch\u00b7ling", "klimmt", "ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Zum Schneeberg auf und ruft ihn jubelnd wach:", "tokens": ["Zum", "Schnee\u00b7berg", "auf", "und", "ruft", "ihn", "ju\u00b7belnd", "wach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Der sch\u00fcttelt sich den Winter ab, den tr\u00e4gen,", "tokens": ["Der", "sch\u00fct\u00b7telt", "sich", "den", "Win\u00b7ter", "ab", ",", "den", "tr\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und schleudert ihm Lawinendonner nach;", "tokens": ["Und", "schleu\u00b7dert", "ihm", "La\u00b7wi\u00b7nen\u00b7don\u00b7ner", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Voll Sehnsucht harrt er schon der Alpenrose,", "tokens": ["Voll", "Sehn\u00b7sucht", "harrt", "er", "schon", "der", "Al\u00b7pen\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Der holden Freundin, die der Lenz versprach,", "tokens": ["Der", "hol\u00b7den", "Freun\u00b7din", ",", "die", "der", "Lenz", "ver\u00b7sprach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Die j\u00e4hrlich ihn beschleicht auf weichem Moose. \u2014", "tokens": ["Die", "j\u00e4hr\u00b7lich", "ihn", "be\u00b7schleicht", "auf", "wei\u00b7chem", "Moo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "So zieht der Lenz herum in allen Gauen,", "tokens": ["So", "zieht", "der", "Lenz", "he\u00b7rum", "in", "al\u00b7len", "Gau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APZR", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Verschwendet rings die sch\u00f6nen Freudenloose;", "tokens": ["Ver\u00b7schwen\u00b7det", "rings", "die", "sch\u00f6\u00b7nen", "Freu\u00b7den\u00b7loo\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Doch Einen wei\u00df ich, der ihn darf nicht schauen,", "tokens": ["Doch", "Ei\u00b7nen", "wei\u00df", "ich", ",", "der", "ihn", "darf", "nicht", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und nicht, was Gott durch ihn gesandt, genie\u00dfen,", "tokens": ["Und", "nicht", ",", "was", "Gott", "durch", "ihn", "ge\u00b7sandt", ",", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PRELS", "NN", "APPR", "PPER", "VVPP", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Weil \u00f6de Kerkerw\u00e4nde ihn umgrauen,", "tokens": ["Weil", "\u00f6\u00b7de", "Ker\u00b7ker\u00b7w\u00e4n\u00b7de", "ihn", "um\u00b7grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Schmachvolle Fesseln ehern ihn umschlie\u00dfen.", "tokens": ["Schmach\u00b7vol\u00b7le", "Fes\u00b7seln", "e\u00b7hern", "ihn", "um\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KOUS", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.38": {"text": "Nicht h\u00f6rt er Vogelsang im Walde t\u00f6nen,", "tokens": ["Nicht", "h\u00f6rt", "er", "Vo\u00b7gel\u00b7sang", "im", "Wal\u00b7de", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Nicht sieht er, wie so sch\u00f6n die Blumen sprie\u00dfen.", "tokens": ["Nicht", "sieht", "er", ",", "wie", "so", "sch\u00f6n", "die", "Blu\u00b7men", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Er h\u00f6rt nur seinen eignen Jammer st\u00f6hnen,", "tokens": ["Er", "h\u00f6rt", "nur", "sei\u00b7nen", "eig\u00b7nen", "Jam\u00b7mer", "st\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "F\u00fcr Nachtigallensang und Taubengirren", "tokens": ["F\u00fcr", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7sang", "und", "Tau\u00b7ben\u00b7gir\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "H\u00f6rt er die Wand sein Klagen wiederh\u00f6hnen,", "tokens": ["H\u00f6rt", "er", "die", "Wand", "sein", "Kla\u00b7gen", "wie\u00b7der\u00b7h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Und, regt er sich, die Eisenkette klirren.", "tokens": ["Und", ",", "regt", "er", "sich", ",", "die", "Ei\u00b7sen\u00b7ket\u00b7te", "klir\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Kein Strahl des Fr\u00fchlings konnte mit Erbarmen,", "tokens": ["Kein", "Strahl", "des", "Fr\u00fch\u00b7lings", "konn\u00b7te", "mit", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Ein s\u00fc\u00dfer Tr\u00f6ster, sich zu ihm verirren;", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Tr\u00f6s\u00b7ter", ",", "sich", "zu", "ihm", "ver\u00b7ir\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Er darf an Gottes Sonne nicht erwarmen,", "tokens": ["Er", "darf", "an", "Got\u00b7tes", "Son\u00b7ne", "nicht", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Nur Nacht, nur Nacht, das schwarze Ungeheuer,", "tokens": ["Nur", "Nacht", ",", "nur", "Nacht", ",", "das", "schwar\u