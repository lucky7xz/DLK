20": {"text": "Da\u00df sie verfolge Trauerscenen nur,", "tokens": ["Da\u00df", "sie", "ver\u00b7fol\u00b7ge", "Trau\u00b7er\u00b7sce\u00b7nen", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.121": {"text": "Und sich f\u00fcr Blumen sammle bittre Z\u00e4hren;", "tokens": ["Und", "sich", "f\u00fcr", "Blu\u00b7men", "samm\u00b7le", "bitt\u00b7re", "Z\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.122": {"text": "Und in den Kerker dort zu Jenem wandre,", "tokens": ["Und", "in", "den", "Ker\u00b7ker", "dort", "zu", "Je\u00b7nem", "wand\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "APPR", "PDAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.123": {"text": "Dem Dulder, bis der Tod, sein hei\u00df Begehren,", "tokens": ["Dem", "Dul\u00b7der", ",", "bis", "der", "Tod", ",", "sein", "hei\u00df", "Be\u00b7geh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.124": {"text": "Aus einer Nacht ihn senket in die andre.", "tokens": ["Aus", "ei\u00b7ner", "Nacht", "ihn", "sen\u00b7ket", "in", "die", "and\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}