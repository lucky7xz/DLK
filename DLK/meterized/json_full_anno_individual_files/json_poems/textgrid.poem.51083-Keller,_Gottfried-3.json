{"textgrid.poem.51083": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von alter Zeit her war des Hauses Wand", "tokens": ["Von", "al\u00b7ter", "Zeit", "her", "war", "des", "Hau\u00b7ses", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APZR", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von wuchernd dichtem Efeu \u00fcberspannt;", "tokens": ["Von", "wu\u00b7chernd", "dich\u00b7tem", "E\u00b7feu", "\u00fc\u00b7bers\u00b7pannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den liebt' der Bauer, sonst so liebeleer,", "tokens": ["Den", "liebt'", "der", "Bau\u00b7er", ",", "sonst", "so", "lie\u00b7be\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Weil er so gierig, alt und z\u00e4h, wie ", "tokens": ["Weil", "er", "so", "gie\u00b7rig", ",", "alt", "und", "z\u00e4h", ",", "wie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "PWAV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun brennt das dunkle Unkraut lichterloh", "tokens": ["Nun", "brennt", "das", "dunk\u00b7le", "Un\u00b7kraut", "lich\u00b7ter\u00b7loh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und flackert in der Luft wie leichtes Stroh;", "tokens": ["Und", "fla\u00b7ckert", "in", "der", "Luft", "wie", "leich\u00b7tes", "Stroh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer glaubte, da\u00df der alte, schwere Kranz", "tokens": ["Wer", "glaub\u00b7te", ",", "da\u00df", "der", "al\u00b7te", ",", "schwe\u00b7re", "Kranz"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So lustig hielte seinen Totentanz?", "tokens": ["So", "lus\u00b7tig", "hiel\u00b7te", "sei\u00b7nen", "To\u00b7ten\u00b7tanz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Oho, was fliegt f\u00fcr Ungeziefer aus?", "tokens": ["O\u00b7ho", ",", "was", "fliegt", "f\u00fcr", "Un\u00b7ge\u00b7zie\u00b7fer", "aus", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In ganzen Schw\u00e4rmen flieht die Fledermaus!", "tokens": ["In", "gan\u00b7zen", "Schw\u00e4r\u00b7men", "flieht", "die", "Fle\u00b7der\u00b7maus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kreuzspinnen, K\u00e4fer, was da kriechen mag,", "tokens": ["Kreuz\u00b7spin\u00b7nen", ",", "K\u00e4\u00b7fer", ",", "was", "da", "krie\u00b7chen", "mag", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Erlebt im Feuer seinen J\u00fcngsten Tag.", "tokens": ["Er\u00b7lebt", "im", "Feu\u00b7er", "sei\u00b7nen", "J\u00fcng\u00b7sten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Was von Gespenstern und von Koboldsbrut,", "tokens": ["Was", "von", "Ge\u00b7spens\u00b7tern", "und", "von", "Ko\u00b7bolds\u00b7brut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von alten S\u00fcnden auf dem Hause ruht,", "tokens": ["Von", "al\u00b7ten", "S\u00fcn\u00b7den", "auf", "dem", "Hau\u00b7se", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und was es sonst f\u00fcr Spuk und Sagen gab,", "tokens": ["Und", "was", "es", "sonst", "f\u00fcr", "Spuk", "und", "Sa\u00b7gen", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Brennt mit den dicken Efeuranken ab.", "tokens": ["Brennt", "mit", "den", "di\u00b7cken", "E\u00b7feu\u00b7ran\u00b7ken", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Was mag wohl schimmern dort, und, seh ich recht?", "tokens": ["Was", "mag", "wohl", "schim\u00b7mern", "dort", ",", "und", ",", "seh", "ich", "recht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "VVFIN", "ADV", "$,", "KON", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was l\u00f6st sich aus dem brennenden Geflecht", "tokens": ["Was", "l\u00f6st", "sich", "aus", "dem", "bren\u00b7nen\u00b7den", "Ge\u00b7flecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und poltert da zu meinen F\u00fc\u00dfen her?", "tokens": ["Und", "pol\u00b7tert", "da", "zu", "mei\u00b7nen", "F\u00fc\u00b7\u00dfen", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein t\u00fcchtig Kruzifix, von Silber schwer!", "tokens": ["Ein", "t\u00fcch\u00b7tig", "Kru\u00b7zi\u00b7fix", ",", "von", "Sil\u00b7ber", "schwer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Einst ri\u00df der Ahn, es sind dreihundert Jahr,", "tokens": ["Einst", "ri\u00df", "der", "Ahn", ",", "es", "sind", "drei\u00b7hun\u00b7dert", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PPER", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Bild als Bilderst\u00fcrmer vom Altar;", "tokens": ["Das", "Bild", "als", "Bil\u00b7der\u00b7st\u00fcr\u00b7mer", "vom", "Al\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es blieb im gr\u00fcnen Rankenwerk versteckt,", "tokens": ["Es", "blieb", "im", "gr\u00fc\u00b7nen", "Ran\u00b7ken\u00b7werk", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nun endlich hat's das Feuer aufgedeckt.", "tokens": ["Nun", "end\u00b7lich", "hat's", "das", "Feu\u00b7er", "auf\u00b7ge\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Zwar munkelt man, da\u00df in verschlo\u00dfner Brust", "tokens": ["Zwar", "mun\u00b7kelt", "man", ",", "da\u00df", "in", "ver\u00b7schlo\u00df\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Enkel jederzeit davon gewu\u00dft:", "tokens": ["Die", "En\u00b7kel", "je\u00b7der\u00b7zeit", "da\u00b7von", "ge\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie h\u00e4tten's n\u00e4chtlich auf den Tisch gesetzt", "tokens": ["Sie", "h\u00e4t\u00b7ten's", "n\u00e4cht\u00b7lich", "auf", "den", "Tisch", "ge\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sich an dem Geflunker oft erg\u00f6tzt.", "tokens": ["Und", "sich", "an", "dem", "Ge\u00b7flun\u00b7ker", "oft", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Eins tut mir leid: manch zierlich Schwalbennest", "tokens": ["Eins", "tut", "mir", "leid", ":", "manch", "zier\u00b7lich", "Schwal\u00b7ben\u00b7nest"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "$.", "PIAT", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hing traulich in den wirren Ranken fest;", "tokens": ["Hing", "trau\u00b7lich", "in", "den", "wir\u00b7ren", "Ran\u00b7ken", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn nun die liebe Schwalbe wiederkehrt,", "tokens": ["Wenn", "nun", "die", "lie\u00b7be", "Schwal\u00b7be", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So findet sie ihr kleines Haus verheert.", "tokens": ["So", "fin\u00b7det", "sie", "ihr", "klei\u00b7nes", "Haus", "ver\u00b7heert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Doch tr\u00f6ste dich, o V\u00f6glein altvertraut,", "tokens": ["Doch", "tr\u00f6s\u00b7te", "dich", ",", "o", "V\u00f6\u00b7glein", "alt\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "FM", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist erst der neue Giebel aufgebaut,", "tokens": ["Ist", "erst", "der", "neu\u00b7e", "Gie\u00b7bel", "auf\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "G'nug Winkel noch und Ecken findest du,", "tokens": ["G'\u00b7nug", "Win\u00b7kel", "noch", "und", "E\u00b7cken", "fin\u00b7dest", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Daran du bauen kannst in guter Ruh!", "tokens": ["Da\u00b7ran", "du", "bau\u00b7en", "kannst", "in", "gu\u00b7ter", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Von alter Zeit her war des Hauses Wand", "tokens": ["Von", "al\u00b7ter", "Zeit", "her", "war", "des", "Hau\u00b7ses", "Wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APZR", "VAFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von wuchernd dichtem Efeu \u00fcberspannt;", "tokens": ["Von", "wu\u00b7chernd", "dich\u00b7tem", "E\u00b7feu", "\u00fc\u00b7