{"dta.poem.8744": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxIII.  \n  Gesang der Todten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Beneiden wir so sehr,", "tokens": ["Be\u00b7nei\u00b7den", "wir", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du gehst von Luft umwoben,", "tokens": ["Du", "gehst", "von", "Luft", "um\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hauchst im Aethermeer.", "tokens": ["Du", "hauchst", "im", "A\u00b7e\u00b7ther\u00b7meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Wir sind zu Staub verwandelt", "tokens": ["Wir", "sind", "zu", "Staub", "ver\u00b7wan\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In dumpfer Gr\u00fcfte Schoos:", "tokens": ["In", "dum\u00b7pfer", "Gr\u00fcf\u00b7te", "Schoos", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O selig, wer noch wandelt,", "tokens": ["O", "se\u00b7lig", ",", "wer", "noch", "wan\u00b7delt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie preisen wir sein Loos!", "tokens": ["Wie", "prei\u00b7sen", "wir", "sein", "Loos", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vom Sonnenstrahl umschw\u00e4rmet,", "tokens": ["Vom", "Son\u00b7nen\u00b7strahl", "um\u00b7schw\u00e4r\u00b7met", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ergehst du dich im Licht,", "tokens": ["Er\u00b7gehst", "du", "dich", "im", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch was die Fl\u00e4chen w\u00e4rmet,", "tokens": ["Doch", "was", "die", "Fl\u00e4\u00b7chen", "w\u00e4r\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Tiefe w\u00e4rmt es nicht.", "tokens": ["Die", "Tie\u00b7fe", "w\u00e4rmt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dir flimmert gleich Gestirnen", "tokens": ["Dir", "flim\u00b7mert", "gleich", "Ge\u00b7stir\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Blumen bunter Glanz,", "tokens": ["Der", "Blu\u00b7men", "bun\u00b7ter", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An unsern nackten Stirnen", "tokens": ["An", "un\u00b7sern", "nack\u00b7ten", "Stir\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Klebt ein verst\u00e4ubter Kranz.", "tokens": ["Klebt", "ein", "ver\u00b7st\u00e4ub\u00b7ter", "Kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wir horchen, ach! wir lauschen,", "tokens": ["Wir", "hor\u00b7chen", ",", "ach", "!", "wir", "lau\u00b7schen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "$.", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo nie ein Schall sich regt,", "tokens": ["Wo", "nie", "ein", "Schall", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir klingt der Quell, es rauschen", "tokens": ["Dir", "klingt", "der", "Quell", ",", "es", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Bl\u00e4tter sturmbewegt.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "sturm\u00b7be\u00b7wegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vom H\u00fcgel aus die Lande", "tokens": ["Vom", "H\u00fc\u00b7gel", "aus", "die", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergn\u00fcgt beschaust du dir,", "tokens": ["Ver\u00b7gn\u00fcgt", "be\u00b7schaust", "du", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch unter seinem Sande,", "tokens": ["Doch", "un\u00b7ter", "sei\u00b7nem", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du Guter, schlafen wir.", "tokens": ["Du", "Gu\u00b7ter", ",", "schla\u00b7fen", "wir", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}