{"textgrid.poem.65654": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Bobolina", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bobolina, Bobolina, K\u00f6nigin der Meeresfluth!", "tokens": ["Bo\u00b7bo\u00b7li\u00b7na", ",", "Bo\u00b7bo\u00b7li\u00b7na", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Mee\u00b7res\u00b7fluth", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Wie ergl\u00fchen rings die Wogen um dich her so roth von Blut!", "tokens": ["Wie", "er\u00b7gl\u00fc\u00b7hen", "rings", "die", "Wo\u00b7gen", "um", "dich", "her", "so", "roth", "von", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ART", "NN", "APPR", "PRF", "APZR", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie dein schwarzer Witwenschleier stolz als Kriegsflagge weht,", "tokens": ["Wie", "dein", "schwar\u00b7zer", "Wit\u00b7wensc\u00b7hlei\u00b7er", "stolz", "als", "Kriegs\u00b7flag\u00b7ge", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "ADJD", "KOKOM", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Und mit tausend Argusaugen auf dem Mast die Rache steht!", "tokens": ["Und", "mit", "tau\u00b7send", "Ar\u00b7gus\u00b7au\u00b7gen", "auf", "dem", "Mast", "die", "Ra\u00b7che", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Um sich sp\u00e4ht sie durch die Meere, durch die Inseln, durch das Land,", "tokens": ["Um", "sich", "sp\u00e4ht", "sie", "durch", "die", "Mee\u00b7re", ",", "durch", "die", "In\u00b7seln", ",", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Und es weint ihr jedes Auge, das noch keine Beute fand.", "tokens": ["Und", "es", "weint", "ihr", "je\u00b7des", "Au\u00b7ge", ",", "das", "noch", "kei\u00b7ne", "Beu\u00b7te", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Bobolina, Bobolina! Durstig ist die Meeresfluth,", "tokens": ["Bo\u00b7bo\u00b7li\u00b7na", ",", "Bo\u00b7bo\u00b7li\u00b7na", "!", "Durs\u00b7tig", "ist", "die", "Mee\u00b7res\u00b7fluth", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Durstig sind des Schiffes Balken, durstig sind wir all' nach Blut.", "tokens": ["Durs\u00b7tig", "sind", "des", "Schif\u00b7fes", "Bal\u00b7ken", ",", "durs\u00b7tig", "sind", "wir", "all'", "nach", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,", "ADJD", "VAFIN", "PPER", "PIS", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Horch, und aus der Wogen Grunde hallt ein dumpfer Geisterlaut:", "tokens": ["Horch", ",", "und", "aus", "der", "Wo\u00b7gen", "Grun\u00b7de", "hallt", "ein", "dum\u00b7pfer", "Geis\u00b7ter\u00b7laut", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sch\u00fctte Blut mir in die Tiefe, Bobolina, meine Braut!", "tokens": ["Sch\u00fct\u00b7te", "Blut", "mir", "in", "die", "Tie\u00b7fe", ",", "Bo\u00b7bo\u00b7li\u00b7na", ",", "mei\u00b7ne", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "APPR", "ART", "NN", "$,", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+--++-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Einen Bach f\u00fcr jeden Tropfen, der aus meinem Herzen sprang,", "tokens": ["Ei\u00b7nen", "Bach", "f\u00fcr", "je\u00b7den", "Trop\u00b7fen", ",", "der", "aus", "mei\u00b7nem", "Her\u00b7zen", "sprang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Als der Dolch der Henkersknechte des Tyrannen es durchdrang.", "tokens": ["Als", "der", "Dolch", "der", "Hen\u00b7kers\u00b7knech\u00b7te", "des", "Ty\u00b7ran\u00b7nen", "es", "durch\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "wor