{"dta.poem.18083": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Zustande der deutschen  \n Poesie bey Ankunft Martin Opitzens.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach da\u00df ich m\u00f6chte seyn die sch\u00f6nste Blum der Erden", "tokens": ["Ach", "da\u00df", "ich", "m\u00f6ch\u00b7te", "seyn", "die", "sch\u00f6ns\u00b7te", "Blum", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "KOUS", "PPER", "VMFIN", "PPOSAT", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voll k\u00f6stliches Geruchs, ach da\u00df ich k\u00f6nte werden", "tokens": ["Voll", "k\u00f6st\u00b7li\u00b7ches", "Ge\u00b7ruchs", ",", "ach", "da\u00df", "ich", "k\u00f6n\u00b7te", "wer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "XY", "KOUS", "PPER", "VMFIN", "VAINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Weide g\u00e4ntzlich gleich, die Flora aus ihr Schoo\u00df,", "tokens": ["Der", "Wei\u00b7de", "g\u00e4ntz\u00b7lich", "gleich", ",", "die", "Flo\u00b7ra", "aus", "ihr", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da Jo war in Noth, nach Jovis Will ausgo\u00df!", "tokens": ["Da", "Jo", "war", "in", "Noth", ",", "nach", "Jo\u00b7vis", "Will", "aus\u00b7go\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VAFIN", "APPR", "NN", "$,", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ach da\u00df ich jetzund h\u00e4tt der Bl\u00e4tter Art und Tugend,", "tokens": ["Ach", "da\u00df", "ich", "je\u00b7tzund", "h\u00e4tt", "der", "Bl\u00e4t\u00b7ter", "Art", "und", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "ADV", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Damit Cupido selbst in seiner Mutter Jugend", "tokens": ["Da\u00b7mit", "Cu\u00b7pi\u00b7do", "selbst", "in", "sei\u00b7ner", "Mut\u00b7ter", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "NE", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Polster hat gef\u00fcllt, darauf die erste Nacht", "tokens": ["Den", "Pols\u00b7ter", "hat", "ge\u00b7f\u00fcllt", ",", "da\u00b7rauf", "die", "ers\u00b7te", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,", "PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An ihre Seiten zart Adonis ward gebracht!", "tokens": ["An", "ih\u00b7re", "Sei\u00b7ten", "zart", "A\u00b7do\u00b7nis", "ward", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie wolt ich mich in Eil so lieblich riechend geben,", "tokens": ["Wie", "wolt", "ich", "mich", "in", "Eil", "so", "lieb\u00b7lich", "rie\u00b7chend", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "APPR", "NN", "ADV", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und rund um den Altar mit s\u00fcsser Kraft umschweben,", "tokens": ["Und", "rund", "um", "den", "Al\u00b7tar", "mit", "s\u00fcs\u00b7ser", "Kraft", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Um den Altar, der ziert ihrs Antlitz Tempel klar", "tokens": ["Um", "den", "Al\u00b7tar", ",", "der", "ziert", "ihrs", "Ant\u00b7litz", "Tem\u00b7pel", "klar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mehr als die andere, doch kundbar Sch\u00f6nheit, Schar.", "tokens": ["Mehr", "als", "die", "an\u00b7de\u00b7re", ",", "doch", "kund\u00b7bar", "Sch\u00f6n\u00b7heit", ",", "Schar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "KOKOM", "ART", "ADJA", "$,", "ADV", "ADJD", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vielleicht w\u00fcrd mich dann auch Fortun so g\u00fcnstig f\u00fchren,", "tokens": ["Viel\u00b7leicht", "w\u00fcrd", "mich", "dann", "auch", "For\u00b7tun", "so", "g\u00fcns\u00b7tig", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "NE", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ich k\u00f6nt ungefehr den Purpur-Bogen r\u00fchren,", "tokens": ["Da\u00df", "ich", "k\u00f6nt", "un\u00b7ge\u00b7fehr", "den", "Pur\u00b7pur\u00b7Bo\u00b7gen", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Welchen Cupido l\u00e4ngst f\u00fcr seinen hat begehrt,", "tokens": ["Wel\u00b7chen", "Cu\u00b7pi\u00b7do", "l\u00e4ngst", "f\u00fcr", "sei\u00b7nen", "hat", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "APPR", "PPOSAT", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Wofehrn nicht Daphnes Reu und Ph\u00f6bi Treu gewehrt.", "tokens": ["Wo\u00b7fehrn", "nicht", "Daph\u00b7nes", "Reu", "und", "Ph\u00f6\u00b7bi", "Treu", "ge\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "NE", "NE", "KON", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ach wie bin ich umsonst! Jezt alles thut erkalten,", "tokens": ["Ach", "wie", "bin", "ich", "um\u00b7sonst", "!", "Jezt", "al\u00b7les", "thut", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VAFIN", "PPER", "ADV", "$.", "ADV", "PIS", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Des Winters Boreas kein Bl\u00fcmlein kan erhalten,", "tokens": ["Des", "Win\u00b7ters", "Bo\u00b7re\u00b7as", "kein", "Bl\u00fcm\u00b7lein", "kan", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Keiner Viol Geruch und keiner Nasen schon", "tokens": ["Kei\u00b7ner", "Vi\u00b7ol", "Ge\u00b7ruch", "und", "kei\u00b7ner", "Na\u00b7sen", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "NE", "NN", "KON", "PIAT", "NN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Mein Wunsch erf\u00fcllen mag, dann ach ich mu\u00df darvon.", "tokens": ["Mein", "Wunsch", "er\u00b7f\u00fcl\u00b7len", "mag", ",", "dann", "ach", "ich", "mu\u00df", "dar\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$,", "ADV", "XY", "PPER", "VMFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}