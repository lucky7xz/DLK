{"textgrid.poem.56080": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Leser", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer kennt ihn, diesen, welcher sein Gesicht", "tokens": ["Wer", "kennt", "ihn", ",", "die\u00b7sen", ",", "wel\u00b7cher", "sein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PDAT", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wegsenkte aus dem Sein zu einem zweiten,", "tokens": ["weg\u00b7senk\u00b7te", "aus", "dem", "Sein", "zu", "ei\u00b7nem", "zwei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das nur das schnelle Wenden voller Seiten", "tokens": ["das", "nur", "das", "schnel\u00b7le", "Wen\u00b7den", "vol\u00b7ler", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "manchmal gewaltsam unterbricht?", "tokens": ["manch\u00b7mal", "ge\u00b7walt\u00b7sam", "un\u00b7ter\u00b7bricht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Selbst seine Mutter w\u00e4re nicht gewi\u00df,", "tokens": ["Selbst", "sei\u00b7ne", "Mut\u00b7ter", "w\u00e4\u00b7re", "nicht", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "$,"], "