{"textgrid.poem.54416": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich k\u00f6nnte dich, mein Kind, noch endlich lieben;", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich k\u00f6nnte dich, mein Kind, noch endlich lieben;", "tokens": ["Ich", "k\u00f6nn\u00b7te", "dich", ",", "mein", "Kind", ",", "noch", "end\u00b7lich", "lie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch gehet es so gleich nicht an.", "tokens": ["Doch", "ge\u00b7het", "es", "so", "gleich", "nicht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein mehr als t\u00e4glich Wanken,", "tokens": ["Dein", "mehr", "als", "t\u00e4g\u00b7lich", "Wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PIAT", "KOKOM", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ich nicht leiden kann,", "tokens": ["Das", "ich", "nicht", "lei\u00b7den", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Entzieht dir noch mein Herze:", "tokens": ["Ent\u00b7zieht", "dir", "noch", "mein", "Her\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich kenne dich im Ernst, und auch im Scherze.", "tokens": ["Ich", "ken\u00b7ne", "dich", "im", "Ernst", ",", "und", "auch", "im", "Scher\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dein Blick ist sch\u00f6n, und dringet in die Brust;", "tokens": ["Dein", "Blick", "ist", "sch\u00f6n", ",", "und", "drin\u00b7get", "in", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mich bringt er nicht auf andere Gedanken?", "tokens": ["Mich", "bringt", "er", "nicht", "auf", "an\u00b7de\u00b7re", "Ge\u00b7dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Denn meine gr\u00f6ste Lust,", "tokens": ["Denn", "mei\u00b7ne", "gr\u00f6s\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wenn ich ja lieben soll, ist blo\u00df die Treue;", "tokens": ["Wenn", "ich", "ja", "lie\u00b7ben", "soll", ",", "ist", "blo\u00df", "die", "Treu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Damit ich nicht die Wahl zu sp\u00e4t bereue.",