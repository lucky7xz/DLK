], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Meer im Sturmesflug", "tokens": ["Auf", "dem", "Meer", "im", "Stur\u00b7mes\u00b7flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weht der Freiheit Odemzug.", "tokens": ["Weht", "der", "Frei\u00b7heit", "O\u00b7dem\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Hab' ich doch mein Schwert behalten", "tokens": ["Hab'", "ich", "doch", "mein", "Schwert", "be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den Arm, der stark es fa\u00dft;", "tokens": ["Und", "den", "Arm", ",", "der", "stark", "es", "fa\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Des verfemten Banners Falten", "tokens": ["Des", "ver\u00b7fem\u00b7ten", "Ban\u00b7ners", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flattern schwarzgesengt vom Mast;", "tokens": ["Flat\u00b7tern", "schwarz\u00b7ge\u00b7sengt", "vom", "Mast", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weh dem K\u00fchnen, der's bedroht!", "tokens": ["Weh", "dem", "K\u00fch\u00b7nen", ",", "der's", "be\u00b7droht", "!"], "token_info": ["word", "word", "word", 