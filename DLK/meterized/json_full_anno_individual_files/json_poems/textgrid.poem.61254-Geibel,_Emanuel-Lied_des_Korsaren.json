{"textgrid.poem.61254": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Lied des Korsaren", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gut der Wind, und fest das Steuer,", "tokens": ["Gut", "der", "Wind", ",", "und", "fest", "das", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leuchtend Silbergr\u00fcn das Meer,", "tokens": ["Leuch\u00b7tend", "Sil\u00b7ber\u00b7gr\u00fcn", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber uns der Sterne Feuer -", "tokens": ["\u00dc\u00b7ber", "uns", "der", "Ster\u00b7ne", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gebt die Mandoline her!", "tokens": ["Gebt", "die", "Man\u00b7do\u00b7li\u00b7ne", "her", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Syrakuser schenkt mir ein!", "tokens": ["Sy\u00b7ra\u00b7ku\u00b7ser", "schenkt", "mir", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hei\u00dfer Sinn will hei\u00dfen Wein.", "tokens": ["Hei\u00b7\u00dfer", "Sinn", "will", "hei\u00b7\u00dfen", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "VVINF", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ging mein Schlo\u00df in j\u00e4hem Brande", "tokens": ["Ging", "mein", "Schlo\u00df", "in", "j\u00e4\u00b7hem", "Bran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lodernd auf um Mitternacht,", "tokens": ["Lo\u00b7dernd", "auf", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwirrt auf Rabenschwing' am Lande", "tokens": ["Schwirrt", "auf", "Ra\u00b7ben\u00b7schwing'", "am", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um mein Haupt des Reiches Acht:", "tokens": ["Um", "mein", "Haupt", "des", "Rei\u00b7ches", "Acht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Meer im Sturmesflug", "tokens": ["Auf", "dem", "Meer", "im", "Stur\u00b7mes\u00b7flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Weht der Freiheit Odemzug.", "tokens": ["Weht", "der", "Frei\u00b7heit", "O\u00b7dem\u00b7zug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hab' ich doch mein Schwert behalten", "tokens": ["Hab'", "ich", "doch", "mein", "Schwert", "be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den Arm, der stark es fa\u00dft;", "tokens": ["Und", "den", "Arm", ",", "der", "stark", "es", "fa\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Des verfemten Banners Falten", "tokens": ["Des", "ver\u00b7fem\u00b7ten", "Ban\u00b7ners", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flattern schwarzgesengt vom Mast;", "tokens": ["Flat\u00b7tern", "schwarz\u00b7ge\u00b7sengt", "vom", "Mast", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weh dem K\u00fchnen, der's bedroht!", "tokens": ["Weh", "dem", "K\u00fch\u00b7nen", ",", "der's", "be\u00b7droht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine Antwort lautet: Tod.", "tokens": ["Sei\u00b7ne", "Ant\u00b7wort", "lau\u00b7tet", ":", "Tod", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$.", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seit das Schiff ich frei bestiegen,", "tokens": ["Seit", "das", "Schiff", "ich", "frei", "be\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haus' ich jedem F\u00fcrsten gleich;", "tokens": ["Haus'", "ich", "je\u00b7dem", "F\u00fcrs\u00b7ten", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weit, so weit die Winde fliegen,", "tokens": ["Weit", ",", "so", "weit", "die", "Win\u00b7de", "flie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt mein flutend K\u00f6nigreich.", "tokens": ["Liegt", "mein", "flu\u00b7tend", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Blanker Stahl ist mein Wardein,", "tokens": ["Blan\u00b7ker", "Stahl", "ist", "mein", "War\u00b7dein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Treib' ich meine Schatzung ein.", "tokens": ["Treib'", "ich", "mei\u00b7ne", "Schat\u00b7zung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "S\u00e4ckel, die von Gold sich br\u00fcsten,", "tokens": ["S\u00e4\u00b7ckel", ",", "die", "von", "Gold", "sich", "br\u00fcs\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ferner Zonen seltne Fracht,", "tokens": ["Fer\u00b7ner", "Zo\u00b7nen", "selt\u00b7ne", "Fracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Klosterwein von sonn'gen K\u00fcsten", "tokens": ["Klos\u00b7ter\u00b7wein", "von", "sonn'\u00b7gen", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Becher von Smaragd,", "tokens": ["Und", "den", "Be\u00b7cher", "von", "Sma\u00b7ragd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Was nur Sinn und Herz begehrt,", "tokens": ["Was", "nur", "Sinn", "und", "Herz", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kauft im Schlachtgew\u00fchl mein Schwert.", "tokens": ["Kauft", "im", "Schlacht\u00b7ge\u00b7w\u00fchl", "mein", "Schwert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wie reizend ist die Dirne,", "tokens": ["Und", "wie", "rei\u00b7zend", "ist", "die", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sie vor dem R\u00e4uber steht,", "tokens": ["Wenn", "sie", "vor", "dem", "R\u00e4u\u00b7ber", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und um ihre blonde Stirne", "tokens": ["Und", "um", "ih\u00b7re", "blon\u00b7de", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Gl\u00fchend Ha\u00df und Neigung weht!", "tokens": ["Gl\u00fc\u00b7hend", "Ha\u00df", "und", "Nei\u00b7gung", "weht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Scham und Lust - o s\u00fc\u00dfer Krieg!", "tokens": ["Scham", "und", "Lust", "o", "s\u00fc\u00b7\u00dfer", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch dem K\u00fchnen bleibt der Sieg.", "tokens": ["Doch", "dem", "K\u00fch\u00b7nen", "bleibt", "der", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Heil dir, Meer, du Feld des Mutes!", "tokens": ["Heil", "dir", ",", "Meer", ",", "du", "Feld", "des", "Mu\u00b7tes", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heil dir, Freiheit, meine Braut!", "tokens": ["Heil", "dir", ",", "Frei\u00b7heit", ",", "mei\u00b7ne", "Braut", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir mit jedem Tropfen Blutes,", "tokens": ["Dir", "mit", "je\u00b7dem", "Trop\u00b7fen", "Blu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir allein bin ich getraut,", "tokens": ["Dir", "al\u00b7lein", "bin", "ich", "ge\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Treu auch dann, wenn mich umdroht", "tokens": ["Treu", "auch", "dann", ",", "wenn", "mich", "um\u00b7droht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Einst im Kampf die letzte Not.", "tokens": ["Einst", "im", "Kampf", "die", "letz\u00b7te", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Dann kein Ach, kein feiger Jammer!", "tokens": ["Dann", "kein", "Ach", ",", "kein", "fei\u00b7ger", "Jam\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoch die Wimpel, hoch das Beil!", "tokens": ["Hoch", "die", "Wim\u00b7pel", ",", "hoch", "das", "Beil", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der engen Pulverkammer", "tokens": ["In", "der", "en\u00b7gen", "Pul\u00b7ver\u00b7kam\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Schl\u00e4ft beisammen Rach' und Heil;", "tokens": ["Schl\u00e4ft", "bei\u00b7sam\u00b7men", "Rach'", "und", "Heil", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Stolz im Blitze fahr' ich dann", "tokens": ["Stolz", "im", "Blit\u00b7ze", "fahr'", "ich", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In den Tod als freier Mann.", "tokens": ["In", "den", "Tod", "als", "frei\u00b7er", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOUS", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Gut der Wind, und fest das Steuer,", "tokens": ["Gut", "der", "Wind", ",", "und", "fest", "das", "Steu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leuchtend Silbergr\u00fcn das Meer,", "tokens": ["Leuch\u00b7tend", "Sil\u00b7ber\u00b7gr\u00fcn", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber uns der Sterne Feuer -", "tokens": ["\u00dc\u00b7ber", "uns", "der", "Ster\u00b7ne", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text"