{"textgrid.poem.39253": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[von Rothenburg die Edelfrau]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von Rothenburg die Edelfrau,", "tokens": ["Von", "Ro\u00b7then\u00b7burg", "die", "E\u00b7del\u00b7frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die weint' in Schmerz und Stolze,", "tokens": ["Die", "weint'", "in", "Schmerz", "und", "Stol\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schritt allein durch die Halle grau \u2013", "tokens": ["Sie", "schritt", "al\u00b7lein", "durch", "die", "Hal\u00b7le", "grau", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Junker schweift im Holze!", "tokens": ["Der", "Jun\u00b7ker", "schweift", "im", "Hol\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbnun helfe mir Gott auf seinem Thron,", "tokens": ["\u00bb", "nun", "hel\u00b7fe", "mir", "Gott", "auf", "sei\u00b7nem", "Thron", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Ende hat der Zweifel,", "tokens": ["Ein", "En\u00b7de", "hat", "der", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe gesehn den eigenen Sohn", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7sehn", "den", "ei\u00b7ge\u00b7nen", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Umarmen den schlimmen Teufel!\u00ab", "tokens": ["Um\u00b7ar\u00b7men", "den", "schlim\u00b7men", "Teu\u00b7fel", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich hab' sie gesehen, die Hexenbraut,", "tokens": ["Ich", "hab'", "sie", "ge\u00b7se\u00b7hen", ",", "die", "He\u00b7xen\u00b7braut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie hat zwei Augen wie R\u00e4der,", "tokens": ["Sie", "hat", "zwei", "Au\u00b7gen", "wie", "R\u00e4\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Durch ihre glei\u00dfende Schwanenhaut", "tokens": ["Durch", "ih\u00b7re", "glei\u00b7\u00dfen\u00b7de", "Schwa\u00b7nen\u00b7haut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Durchscheint das blaue Ge\u00e4der.\u00ab", "tokens": ["Durch\u00b7scheint", "das", "blau\u00b7e", "Ge\u00b7\u00e4\u00b7der", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbsie tat ihn mit beiden Armen fest", "tokens": ["\u00bb", "sie", "tat", "ihn", "mit", "bei\u00b7den", "Ar\u00b7men", "fest"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "APPR", "PIAT", "NN", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Umringeln und umgattern,", "tokens": ["Um\u00b7rin\u00b7geln", "und", "um\u00b7gat\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir war's, als schlief er im Schlangennest", "tokens": ["Mir", "wa\u00b7r's", ",", "als", "schlief", "er", "im", "Schlan\u00b7gen\u00b7nest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und um ihn gerollt die Nattern.\u00ab", "tokens": ["Und", "um", "ihn", "ge\u00b7rollt", "die", "Nat\u00b7tern", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUI", "PPER", "VVPP", "ART", "NN", "$.", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "\u00bbdie Glocken klangen so feierlich,", "tokens": ["\u00bb", "die", "Glo\u00b7cken", "klan\u00b7gen", "so", "fei\u00b7er\u00b7lich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er schlief gleich einem Tauber,", "tokens": ["Er", "schlief", "gleich", "ei\u00b7nem", "Tau\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat vergessen auf Gott und mich,", "tokens": ["Er", "hat", "ver\u00b7ges\u00b7sen", "auf", "Gott", "und", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "KON", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ich aber breche den Zauber!\u00ab", "tokens": ["Ich", "a\u00b7ber", "bre\u00b7che", "den", "Zau\u00b7ber", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Die Freifrau ritt zu Walde flink,", "tokens": ["Die", "Frei\u00b7frau", "ritt", "zu", "Wal\u00b7de", "flink", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr folgten die Trabanten,", "tokens": ["Ihr", "folg\u00b7ten", "die", "Tra\u00b7ban\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie ritten zusammen den Elfenring,", "tokens": ["Sie", "rit\u00b7ten", "zu\u00b7sam\u00b7men", "den", "El\u00b7fen\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das Gras sie niederbrannten.", "tokens": ["Das", "Gras", "sie", "nie\u00b7der\u00b7brann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie pfl\u00fcgten den Boden stumm und schnell,", "tokens": ["Sie", "pfl\u00fcg\u00b7ten", "den", "Bo\u00b7den", "stumm", "und", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Salz s\u00e4ten sie in die Ritze,", "tokens": ["Salz", "s\u00e4\u00b7ten", "sie", "in", "die", "Rit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Drauf t\u00fcrmten sie Schutt und Mauerger\u00f6ll'", "tokens": ["Drauf", "t\u00fcrm\u00b7ten", "sie", "Schutt", "und", "Mau\u00b7er\u00b7ge\u00b7r\u00f6ll'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "KON", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und pflanzten ein Kreuz zur Spitze.", "tokens": ["Und", "pflanz\u00b7ten", "ein", "Kreuz", "zur", "Spit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Die Burgfrau warf den ersten Stein,", "tokens": ["Die", "Burg\u00b7frau", "warf", "den", "ers\u00b7ten", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Stein ihr sank vom Herzen:", "tokens": ["Ein", "Stein", "ihr", "sank", "vom", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbmaria, s\u00fc\u00dfe Magedein,", "tokens": ["\u00bb", "ma\u00b7ria", ",", "s\u00fc\u00b7\u00dfe", "Ma\u00b7ge\u00b7dein", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir weih' ich zweihundert Kerzen!\u00ab", "tokens": ["Dir", "weih'", "ich", "zwei\u00b7hun\u00b7dert", "Ker\u00b7zen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "CARD", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbzweihundert Kerzen bl\u00fctenwei\u00df", "tokens": ["\u00bb", "zwei\u00b7hun\u00b7dert", "Ker\u00b7zen", "bl\u00fc\u00b7ten\u00b7wei\u00df"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Allj\u00e4hrlich ich Dir weihe; \u2013", "tokens": ["All\u00b7j\u00e4hr\u00b7lich", "ich", "Dir", "wei\u00b7he", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe gesprengt den Zauberkreis", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7sprengt", "den", "Zau\u00b7ber\u00b7kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und habe gebannt die Feie!\u00ab \u2013", "tokens": ["Und", "ha\u00b7be", "ge\u00b7bannt", "die", "Fei\u00b7e", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VAFIN", "VVPP", "ART", "NN", "$.", "$(", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Von Rothenburg die Edelfrau,", "tokens": ["Von", "Ro\u00b7then\u00b7burg", "die", "E\u00b7del\u00b7frau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die weint' in Schmerz und Stolze,", "tokens": ["Die", "weint'", "in", "Schmerz", "und", "Stol\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schritt allein durch die Halle grau \u2013", "tokens": ["Sie", "schritt", "al\u00b7lein", "durch", "die", "Hal\u00b7le", "grau", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Junker schweift im Holze!", "tokens": ["Der", "Jun\u00b7ker", "schweift", "im", "Hol\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbnun helfe mir Gott auf seinem Thron,", "tokens": ["\u00bb", "nun", "hel\u00b7fe", "mir", "Gott", "auf", "sei\u00b7nem", "Thron", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Ende hat der Zweifel,", "tokens": ["Ein", "En\u00b7de", "hat", "der", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter":