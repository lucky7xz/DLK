{"textgrid.poem.23981": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Rabenflug", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mattheller Wintertag. Wie goldene Bronce", "tokens": ["Mat\u00b7thel\u00b7ler", "Win\u00b7ter\u00b7tag", ".", "Wie", "gol\u00b7de\u00b7ne", "Bron\u00b7ce"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWAV", "ADJA", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Liegt auf dem Schnee der Sonne schwacher Schein.", "tokens": ["Liegt", "auf", "dem", "Schnee", "der", "Son\u00b7ne", "schwa\u00b7cher", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Leben schl\u00e4ft in tr\u00e4umender Agonie.", "tokens": ["Das", "Le\u00b7ben", "schl\u00e4ft", "in", "tr\u00e4u\u00b7men\u00b7der", "A\u00b7go\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "War Fr\u00fchling einst? In dieser grauen Luft", "tokens": ["War", "Fr\u00fch\u00b7ling", "einst", "?", "In", "die\u00b7ser", "grau\u00b7en", "Luft"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADV", "$.", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hat farbiges Falterschwingenspiel geweht", "tokens": ["Hat", "far\u00b7bi\u00b7ges", "Fal\u00b7ter\u00b7schwin\u00b7gen\u00b7spiel", "ge\u00b7weht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und Blumend\u00fcften? Wo das kalte Wei\u00df", "tokens": ["Und", "Blu\u00b7men\u00b7d\u00fcf\u00b7ten", "?", "Wo", "das", "kal\u00b7te", "Wei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$.", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Starr liegt und eben, wogte Maiengr\u00fcn,", "tokens": ["Starr", "liegt", "und", "e\u00b7ben", ",", "wog\u00b7te", "Mai\u00b7en\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ADV", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von buntem Blumensternenschmelz, durchflockt?", "tokens": ["Von", "bun\u00b7tem", "Blu\u00b7mens\u00b7ter\u00b7nen\u00b7schmelz", ",", "durch\u00b7flockt", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie ist es still geworden, todesahnungsstill ...", "tokens": ["Wie", "ist", "es", "still", "ge\u00b7wor\u00b7den", ",", "to\u00b7de\u00b7sah\u00b7nungs\u00b7still", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "VAPP", "$,", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Park ist offen. Niemand trat durchs Thor.", "tokens": ["Der", "Park", "ist", "of\u00b7fen", ".", "Nie\u00b7mand", "trat", "durchs", "Thor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So einsam ists, als w\u00e4rs die Toteninsel.", "tokens": ["So", "ein\u00b7sam", "ists", ",", "als", "w\u00e4rs", "die", "To\u00b7ten\u00b7in\u00b7sel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOKOM", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "Die Marmorg\u00f6tter auf den hoben Sockeln,", "tokens": ["Die", "Mar\u00b7mor\u00b7g\u00f6t\u00b7ter", "auf", "den", "ho\u00b7ben", "So\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Von Schnee behaubt, stehn da wie Gr\u00e4bermale;", "tokens": ["Von", "Schnee", "be\u00b7haubt", ",", "stehn", "da", "wie", "Gr\u00e4\u00b7ber\u00b7ma\u00b7le", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "VVFIN", "ADV", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die Tannallee, schnurgrad hinausgezogen", "tokens": ["Die", "Tan\u00b7nal\u00b7lee", ",", "schnur\u00b7grad", "hin\u00b7aus\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Vom wei\u00dfen Schlo\u00df bis an die Mauert\u00fcrme,", "tokens": ["Vom", "wei\u00b7\u00dfen", "Schlo\u00df", "bis", "an", "die", "Mau\u00b7er\u00b7t\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Ist eine schwarze, steife Leichengarde,", "tokens": ["Ist", "ei\u00b7ne", "schwar\u00b7ze", ",", "stei\u00b7fe", "Lei\u00b7chen\u00b7gar\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "In Reih und Glied zum Trauern kommandiert.", "tokens": ["In", "Reih", "und", "Glied", "zum", "Trau\u00b7ern", "kom\u00b7man\u00b7diert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Von jedem Schritte knistert, wie in Schmerz, der Schnee,", "tokens": ["Von", "je\u00b7dem", "Schrit\u00b7te", "knis\u00b7tert", ",", "wie", "in", "Schmerz", ",", "der", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,", "PWAV", "APPR", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Mein Hauch dampft aus in grauen Nebelw\u00f6lckchen;", "tokens": ["Mein", "Hauch", "dampft", "aus", "in", "grau\u00b7en", "Ne\u00b7bel\u00b7w\u00f6l\u00b7ck\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.20": {"text": "Bin ich allein das Leben in dem Tod?", "tokens": ["Bin", "ich", "al\u00b7lein", "das", "Le\u00b7ben", "in", "dem", "Tod", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Mein warmes Herz, du nimmer m\u00fcder Quell", "tokens": ["Mein", "war\u00b7mes", "Herz", ",", "du", "nim\u00b7mer", "m\u00fc\u00b7der", "Quell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Voll roten, hei\u00dfen Lebensweines, str\u00f6me", "tokens": ["Voll", "ro\u00b7ten", ",", "hei\u00b7\u00dfen", "Le\u00b7bens\u00b7wei\u00b7nes", ",", "str\u00f6\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "ADJA", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Die Purpurwogen voller Liebe aus,", "tokens": ["Die", "Pur\u00b7pur\u00b7wo\u00b7gen", "vol\u00b7ler", "Lie\u00b7be", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Gie\u00df aus durch meinen Leib die Flut der Liebe,", "tokens": ["Gie\u00df", "aus", "durch", "mei\u00b7nen", "Leib", "die", "Flut", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Denn leben will ich, hei\u00df in Liebe leben!", "tokens": ["Denn", "le\u00b7ben", "will", "ich", ",", "hei\u00df", "in", "Lie\u00b7be", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$,", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Wo ist die Bank, da die Syringentrauben", "tokens": ["Wo", "ist", "die", "Bank", ",", "da", "die", "Sy\u00b7rin\u00b7gen\u00b7trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Gesch\u00e4mig blau aus dunklem Laube winkten?", "tokens": ["Ge\u00b7sch\u00e4\u00b7mig", "blau", "aus", "dunk\u00b7lem", "Lau\u00b7be", "wink\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Im hellen Lindgezweig, das dr\u00fcber dachte,", "tokens": ["Im", "hel\u00b7len", "Lind\u00b7ge\u00b7zweig", ",", "das", "dr\u00fc\u00b7ber", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Barg sich ein Finkenpaar im kleinen Nest,", "tokens": ["Barg", "sich", "ein", "Fin\u00b7ken\u00b7paar", "im", "klei\u00b7nen", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Ein Marmorfaun auf rotem Porphyrsitze", "tokens": ["Ein", "Mar\u00b7mor\u00b7faun", "auf", "ro\u00b7tem", "Por\u00b7phyr\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Lie\u00df sich die Liebe einer kleinen Nymphe,", "tokens": ["Lie\u00df", "sich", "die", "Lie\u00b7be", "ei\u00b7ner", "klei\u00b7nen", "Nym\u00b7phe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.32": {"text": "Die eng sich schmiegte seinem feisten Leib,", "tokens": ["Die", "eng", "sich", "schmieg\u00b7te", "sei\u00b7nem", "feis\u00b7ten", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Mit Grinsen wohlthun ... Suchend geh ich schneller", "tokens": ["Mit", "Grin\u00b7sen", "wohl\u00b7thun", "...", "Su\u00b7chend", "geh", "ich", "schnel\u00b7ler"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "$(", "NE", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und finde meine Laube. \u2013 Armer Faun!", "tokens": ["Und", "fin\u00b7de", "mei\u00b7ne", "Lau\u00b7be", ".", "\u2013", "Ar\u00b7mer", "Faun", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Die kalte Flockenm\u00fctze sitzt ihm schief,", "tokens": ["Die", "kal\u00b7te", "Flo\u00b7cken\u00b7m\u00fct\u00b7ze", "sitzt", "ihm", "schief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Sein armes Nymphchen ist ihm schier verdeckt,", "tokens": ["Sein", "ar\u00b7mes", "Nymph\u00b7chen", "ist", "ihm", "schier", "ver\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Ihr Schmiegen sieht mir gar nicht mehr wie Liebe,", "tokens": ["Ihr", "Schmie\u00b7gen", "sieht", "mir", "gar", "nicht", "mehr", "wie", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Ach sieht nur noch wie bittres Frieren aus.", "tokens": ["Ach", "sieht", "nur", "noch", "wie", "bitt\u00b7res", "Frie\u00b7ren", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "ADV", "ADV", "KOKOM", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Das Finkenpaar? Ein alter Rabe sitzt", "tokens": ["Das", "Fin\u00b7ken\u00b7paar", "?", "Ein", "al\u00b7ter", "Ra\u00b7be", "sitzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Im krummen Knorrge\u00e4st der kahlen Linde", "tokens": ["Im", "krum\u00b7men", "Knorr\u00b7ge\u00e4st", "der", "kah\u00b7len", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "Und pre\u00dft die Fl\u00fcgel an den kalten Balg.", "tokens": ["Und", "pre\u00dft", "die", "Fl\u00fc\u00b7gel", "an", "den", "kal\u00b7ten", "Balg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Du schwarzer Leichenbitter, kannst du sagen,", "tokens": ["Du", "schwar\u00b7zer", "Lei\u00b7chen\u00b7bit\u00b7ter", ",", "kannst", "du", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Wo jetzt die Liebe weilt? Er hebt die Fl\u00fcgel,", "tokens": ["Wo", "jetzt", "die", "Lie\u00b7be", "weilt", "?", "Er", "hebt", "die", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Und kr\u00e4chzend, schwanken Fluges, schwebt er fort", "tokens": ["Und", "kr\u00e4ch\u00b7zend", ",", "schwan\u00b7ken", "Flu\u00b7ges", ",", "schwebt", "er", "fort"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Und fliegt zur Stadt. \u2013 Schnell bin ich nachgegangen", "tokens": ["Und", "fliegt", "zur", "Stadt", ".", "\u2013", "Schnell", "bin", "ich", "nach\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$.", "$(", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Der Richtung seines Flugs. Und sollt mans glauben?", "tokens": ["Der", "Rich\u00b7tung", "sei\u00b7nes", "Flugs", ".", "Und", "sollt", "mans", "glau\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "KON", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Ich fand auf dieses alten Raben Weg", "tokens": ["Ich", "fand", "auf", "die\u00b7ses", "al\u00b7ten", "Ra\u00b7ben", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "ADJA", "NN"