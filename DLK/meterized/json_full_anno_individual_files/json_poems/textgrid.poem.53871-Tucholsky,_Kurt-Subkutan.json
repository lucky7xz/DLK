{"textgrid.poem.53871": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Subkutan", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich geh mit etwas weichen Knien", "tokens": ["Ich", "geh", "mit", "et\u00b7was", "wei\u00b7chen", "Kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und tr\u00e4umerisch durch ganz Berlin", "tokens": ["und", "tr\u00e4u\u00b7me\u00b7risch", "durch", "ganz", "Ber\u00b7lin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADV", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "leicht angeknockt und ein wenig schwach:", "tokens": ["leicht", "an\u00b7ge\u00b7knockt", "und", "ein", "we\u00b7nig", "schwach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "ART", "PIS", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "ernsten Berufsgesch\u00e4ften nach.", "tokens": ["erns\u00b7ten", "Be\u00b7rufs\u00b7ge\u00b7sch\u00e4f\u00b7ten", "nach", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Der Ordner hie\u00df \u203aHelvetia\u2039;", "tokens": ["Der", "Ord\u00b7ner", "hie\u00df", "\u203a", "Hel\u00b7ve\u00b7tia", "\u2039", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "NE", "$(", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "von den Packpapierbogen ist nichts mehr da;", "tokens": ["von", "den", "Pack\u00b7pa\u00b7pier\u00b7bo\u00b7gen", "ist", "nichts", "mehr", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIS", "ADV", "ADV", "$."], "meter": "--+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "die Lieferung h\u00e4tten wir noch ergattert \u2013", "tokens": ["die", "Lie\u00b7fe\u00b7rung", "h\u00e4t\u00b7ten", "wir", "noch", "er\u00b7gat\u00b7tert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Telefon schnurrt, Schreibmaschine schnattert . . .", "tokens": ["Te\u00b7le\u00b7fon", "schnurrt", ",", "Schreib\u00b7ma\u00b7schi\u00b7ne", "schnat\u00b7tert", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "VVFIN", "$.", "$.", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Chinesisch-fett ruht mein Gesicht,", "tokens": ["Chi\u00b7ne\u00b7sischfett", "ruht", "mein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "und was gestern war, wei\u00df keiner nicht.", "tokens": ["und", "was", "ge\u00b7stern", "war", ",", "wei\u00df", "kei\u00b7ner", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VAFIN", "$,", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Da gibt es im M\u00e4rchen einen Zwerg,", "tokens": ["Da", "gibt", "es", "im", "M\u00e4r\u00b7chen", "ei\u00b7nen", "Zwerg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "der glaubt sich mit allem l\u00e4ngst \u00fcber den Berg;", "tokens": ["der", "glaubt", "sich", "mit", "al\u00b7lem", "l\u00e4ngst", "\u00fc\u00b7ber", "den", "Berg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PRF", "APPR", "PIS", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "an einem unbewachten Ort", "tokens": ["an", "ei\u00b7nem", "un\u00b7be\u00b7wach\u00b7ten", "Ort"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sagt das Dummchen sein Zauberwort", "tokens": ["sagt", "das", "Dumm\u00b7chen", "sein", "Zau\u00b7ber\u00b7wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "und tanzt dazu auf einem Bein", "tokens": ["und", "tanzt", "da\u00b7zu", "auf", "ei\u00b7nem", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und steht nicht an, vor sich hin zu schrein:", "tokens": ["und", "steht", "nicht", "an", ",", "vor", "sich", "hin", "zu", "schrein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKVZ", "$,", "APPR", "PRF", "APZR", "PTKZU", "VAINF", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "\u00bbach, wie sch\u00f6n, da\u00df niemand wei\u00df,", "tokens": ["\u00bb", "ach", ",", "wie", "sch\u00f6n", ",", "da\u00df", "nie\u00b7mand", "wei\u00df", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PWAV", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "da\u00df ich Rumpelstilzchen heiss \u2013!\u00ab", "tokens": ["da\u00df", "ich", "Rum\u00b7pel\u00b7stilz\u00b7chen", "heiss", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "NE", "NE", "$(", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vor mir schreibt ein gebeugter Scheitel . . .", "tokens": ["Vor", "mir", "schreibt", "ein", "ge\u00b7beug\u00b7ter", "Schei\u00b7tel", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "M\u00e4nner sind manchmal bodenlos eitel.", "tokens": ["M\u00e4n\u00b7ner", "sind", "manch\u00b7mal", "bo\u00b7den\u00b7los", "ei\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "ADJD", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und in mir gluckert ein Freudengebraus:", "tokens": ["Und", "in", "mir", "glu\u00b7ckert", "ein", "Freu\u00b7den\u00b7ge\u00b7braus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "ich hab euch allen etwas voraus!", "tokens": ["ich", "hab", "euch", "al\u00b7len", "et\u00b7was", "vo\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "PIS", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und beschaulich, in guter Ruh,", "tokens": ["Und", "be\u00b7schau\u00b7lich", ",", "in", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "seh ich den Geisteskranken zu,", "tokens": ["seh", "ich", "den", "Geis\u00b7tes\u00b7kran\u00b7ken", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "die sich im Reichstag wichtig machen,", "tokens": ["die", "sich", "im", "Reichs\u00b7tag", "wich\u00b7tig", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "h\u00f6r still erfreut die Schlagzeilen krachen", "tokens": ["h\u00f6r", "still", "er\u00b7freut", "die", "Schlag\u00b7zei\u00b7len", "kra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "von Morgen\u2013, Mittag- und Nachtausgabe . . .", "tokens": ["von", "Mor\u00b7gen", "\u2013", ",", "Mit\u00b7tag", "und", "Nach\u00b7taus\u00b7ga\u00b7be", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "$(", "$,", "TRUNC", "KON", "NN", "$.", "$.", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Macht, macht . . . Ich pers\u00f6nlich habe", "tokens": ["Macht", ",", "macht", ".", ".", ".", "Ich", "per\u00b7s\u00f6n\u00b7lich", "ha\u00b7be"], "token_info": ["word", "punct", "word", "punct", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$.", "$.", "$.", "PPER", "ADJD", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "meinen Teil weg. Und bin angenehm matt.", "tokens": ["mei\u00b7nen", "Teil", "weg", ".", "Und", "bin", "an\u00b7ge\u00b7nehm", "matt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "KON", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.12": {"text": "Wer hat, hat.", "tokens": ["Wer", "hat", ",", "hat", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "VAFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Nur kein Neid.", "tokens": ["Nur", "kein", "Neid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Das ist die sch\u00f6nste Tageszeit:", "tokens": ["Das", "ist", "die", "sch\u00f6ns\u00b7te", "Ta\u00b7ges\u00b7zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die nach der Erf\u00fcllung. Da l\u00e4\u00dft man sich treiben,", "tokens": ["die", "nach", "der", "Er\u00b7f\u00fcl\u00b7lung", ".", "Da", "l\u00e4\u00dft", "man", "sich", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PIS", "PRF", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "m\u00f6chte immerzu die H\u00e4nde reiben", "tokens": ["m\u00f6ch\u00b7te", "im\u00b7mer\u00b7zu", "die", "H\u00e4n\u00b7de", "rei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "und hat zu eignem Privatgebrauch", "tokens": ["und", "hat", "zu", "eig\u00b7nem", "Pri\u00b7vat\u00b7ge\u00b7brauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "so etwas wie Schadenfreude im Bauch.", "tokens": ["so", "et\u00b7was", "wie", "Scha\u00b7den\u00b7freu\u00b7de", "im", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Denn jeder Kerl glaubt dann und wann,", "tokens": ["Denn", "je\u00b7der", "Kerl", "glaubt", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "KON", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "er sei ganz alleine ein Mann.", "tokens": ["er", "sei", "ganz", "al\u00b7lei\u00b7ne", "ein", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Kein Feuer, keine Kohle", "tokens": ["Kein", "Feu\u00b7er", ",", "kei\u00b7ne", "Koh\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "kann brennen so hei\u00df", "tokens": ["kann", "bren\u00b7nen", "so", "hei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "ADV", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "wie die heimliche Liebe,", "tokens": ["wie", "die", "heim\u00b7li\u00b7che", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "von der niemand nichts wei\u00df.", "tokens": ["von", "der", "nie\u00b7mand", "nichts", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "PIS", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Kennst du das?", "tokens": ["Kennst", "du", "das", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Zu dem, was an solchem Tage geschieht,", "tokens": ["Zu", "dem", ",", "was", "an", "sol\u00b7chem", "Ta\u00b7ge", "ge\u00b7schieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "zu allem, was dein Auge sieht,", "tokens": ["zu", "al\u00b7lem", ",", "was", "dein", "Au\u00b7ge", "sieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "zu allen Reden und Diskussionen,", "tokens": ["zu", "al\u00b7len", "Re\u00b7den", "und", "Dis\u00b7kus\u00b7si\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "zu allen Reichsgerichts-Konstruktionen;", "tokens": ["zu", "al\u00b7len", "Reichs\u00b7ge\u00b7richts\u00b7Kon\u00b7struk\u00b7ti\u00b7o\u00b7nen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "zu Vollb\u00e4rten, die sich gebildet bekleckern \u2013:", "tokens": ["zu", "Voll\u00b7b\u00e4r\u00b7ten", ",", "die", "sich", "ge\u00b7bil\u00b7det", "be\u00b7kle\u00b7ckern", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PRF", "VVPP", "VVINF", "$(", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "immer h\u00f6rst du ein Stimmchen meckern:", "tokens": ["im\u00b7mer", "h\u00f6rst", "du", "ein", "Stimm\u00b7chen", "me\u00b7ckern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "\u00bbach, wie sch\u00f6n, da\u00df niemand wei\u00df,", "tokens": ["\u00bb", "ach", ",", "wie", "sch\u00f6n", ",", "da\u00df", "nie\u00b7mand", "wei\u00df", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PWAV", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "da\u00df ich Rumpelstilzchen hei\u00df \u2013!\u00ab", "tokens": ["da\u00df", "ich", "Rum\u00b7pel\u00b7stilz\u00b7chen", "hei\u00df", "\u2013", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "NN", "ADJD", "$(", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Mensch, sei diskret! Ein Dummkopf, wer sich spreizt.", "tokens": ["Mensch", ",", "sei", "dis\u00b7kret", "!", "Ein", "Dumm\u00b7kopf", ",", "wer", "sich", "spreizt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "VVFIN", "$.", "ART", "NN", "$,", "PWS", "PRF", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambi