{"textgrid.poem.49987": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "19. Auf Chlorinde", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Chlorinde traurt um ihren Mann,", "tokens": ["Chlo\u00b7rin\u00b7de", "traurt", "um", "ih\u00b7ren", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den sie in Wort und Werck, ", "tokens": ["Den", "sie", "in", "Wort", "und", "Werck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat ein zierlich Traur-Kleid an,", "tokens": ["Sie", "hat", "ein", "zier\u00b7lich", "Traur\u00b7Kleid", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Zimmer ist mit Boy biss auf den Grund bezogen;", "tokens": ["Ihr", "Zim\u00b7mer", "ist", "mit", "Boy", "biss", "auf", "den", "Grund", "be\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "NE", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schwartz ist die Deck' auf ihrem Bett',", "tokens": ["Schwartz", "ist", "die", "Deck'", "auf", "ih\u00b7rem", "Bett'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Brieftr\u00e4ger, Kuppler, Knecht, Magd, Kutscher, Pferd und Wagen", "tokens": ["Brief\u00b7tr\u00e4\u00b7ger", ",", "Kupp\u00b7ler", ",", "Knecht", ",", "Magd", ",", "Kut\u00b7scher", ",", "Pferd", "und", "Wa\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die m\u00fcssen alle Trauer tragen:", "tokens": ["Die", "m\u00fcs\u00b7sen", "al\u00b7le", "Trau\u00b7er", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es scheint, als ob sie ", "tokens": ["Es", "scheint", ",", "als", "ob", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "KOUS", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Chlorinde traurt um ihren Mann,", "tokens": ["Chlo\u00b7rin\u00b7de", "traurt", "um", "ih\u00b7ren", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den sie in Wort und Werck, ", "tokens": ["Den", "sie", "in", "Wort", "und", "Werck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat ein zierlich Traur-Kleid an,", "tokens": ["Sie", "hat", "ein", "zier\u00b7lich", "Traur\u00b7Kleid", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+