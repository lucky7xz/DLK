{"dta.poem.21565": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Spruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird auf unserer viellieben Erden,               ", "tokens": ["Es", "wird", "auf", "un\u00b7se\u00b7rer", "viel\u00b7lie\u00b7ben", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So lange sie wandert, nicht anders werden:", "tokens": ["So", "lan\u00b7ge", "sie", "wan\u00b7dert", ",", "nicht", "an\u00b7ders", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Reichen treiben mit Himmel und Gott", "tokens": ["Die", "Rei\u00b7chen", "trei\u00b7ben", "mit", "Him\u00b7mel", "und", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Fr\u00f6hlich ihren lachenden Spott;", "tokens": ["Fr\u00f6h\u00b7lich", "ih\u00b7ren", "la\u00b7chen\u00b7den", "Spott", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Schmausen und zechen Fasanen und Wein \u2014", "tokens": ["Schmau\u00b7sen", "und", "ze\u00b7chen", "Fa\u00b7sa\u00b7nen", "und", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "KON", "NN", "$("], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Lassen das Philosophiren h\u00fcbsch sein.", "tokens": ["Las\u00b7sen", "das", "Phi\u00b7lo\u00b7so\u00b7phi\u00b7ren", "h\u00fcbsch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Sie besitzen die Weisheit der Welt \u2014", "tokens": ["Sie", "be\u00b7sit\u00b7zen", "die", "Weis\u00b7heit", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Die Weisheit hat Klang und nennt sich Geld.", "tokens": ["Die", "Weis\u00b7heit", "hat", "Klang", "und", "nennt", "sich", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "VVFIN", "PRF", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}