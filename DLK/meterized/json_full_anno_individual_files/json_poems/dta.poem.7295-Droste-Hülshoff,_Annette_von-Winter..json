{"dta.poem.7295": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Schneegest\u00e4ub' und Nebelqualm", "tokens": ["Aus", "Schnee\u00b7ge\u00b7st\u00e4ub'", "und", "Ne\u00b7bel\u00b7qualm"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bricht endlich doch ein klarer Tag;", "tokens": ["Bricht", "end\u00b7lich", "doch", "ein", "kla\u00b7rer", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da fliegen alle Fenster auf,", "tokens": ["Da", "flie\u00b7gen", "al\u00b7le", "Fens\u00b7ter", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Jeder sp\u00e4ht, was er vermag.", "tokens": ["Ein", "Je\u00b7der", "sp\u00e4ht", ",", "was", "er", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob jene Bl\u00f6cke H\u00e4user sind?", "tokens": ["Ob", "je\u00b7ne", "Bl\u00f6\u00b7cke", "H\u00e4u\u00b7ser", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Weiher jener ebne Raum?", "tokens": ["Ein", "Wei\u00b7her", "je\u00b7ner", "eb\u00b7ne", "Raum", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcrwahr, in dieser Uniform", "tokens": ["F\u00fcr\u00b7wahr", ",", "in", "die\u00b7ser", "U\u00b7ni\u00b7form"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Glockenthurm erkennt man kaum;", "tokens": ["Den", "Glo\u00b7cken\u00b7thurm", "er\u00b7kennt", "man", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und alles Leben liegt zerdr\u00fcckt,", "tokens": ["Und", "al\u00b7les", "Le\u00b7ben", "liegt", "zer\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie unterm Leichentuch erstickt.", "tokens": ["Wie", "un\u00b7term", "Lei\u00b7chen\u00b7tuch", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schau! an Horizontes Rand", "tokens": ["Doch", "schau", "!", "an", "Ho\u00b7ri\u00b7zon\u00b7tes", "Rand"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Begegnet mir lebend'ges Land.", "tokens": ["Be\u00b7geg\u00b7net", "mir", "le\u00b7ben\u00b7d'\u00b7ges", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Du starrer W\u00e4chter, lass' ihn los", "tokens": ["Du", "star\u00b7rer", "W\u00e4ch\u00b7ter", ",", "lass'", "ihn", "los"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den F\u00f6hn aus deiner Kerker Schoo\u00df!", "tokens": ["Den", "F\u00f6hn", "aus", "dei\u00b7ner", "Ker\u00b7ker", "Schoo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo schw\u00e4rzlich jene Riffe spalten,", "tokens": ["Wo", "schw\u00e4rz\u00b7lich", "je\u00b7ne", "Rif\u00b7fe", "spal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da mu\u00df er Quarantaine halten,", "tokens": ["Da", "mu\u00df", "er", "Qua\u00b7ran\u00b7tai\u00b7ne", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Fremdling aus der Lombardei;", "tokens": ["Der", "Fremd\u00b7ling", "aus", "der", "Lom\u00b7bar\u00b7dei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O S\u00e4ntis, gib den Thauwind frei!", "tokens": ["O", "S\u00e4n\u00b7tis", ",", "gib", "den", "Thau\u00b7wind", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}