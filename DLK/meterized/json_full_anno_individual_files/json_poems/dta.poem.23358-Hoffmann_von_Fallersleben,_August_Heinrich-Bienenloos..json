{"dta.poem.23358": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Bienenloos.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir geben und der K\u00f6nig nimmt,", "tokens": ["Wir", "ge\u00b7ben", "und", "der", "K\u00f6\u00b7nig", "nimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird sind zum Geben nur bestimmt,", "tokens": ["Wird", "sind", "zum", "Ge\u00b7ben", "nur", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir sind nichts weiter als die Bienen,", "tokens": ["Wir", "sind", "nichts", "wei\u00b7ter", "als", "die", "Bie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Arbeiten m\u00fcssen wir und dienen.", "tokens": ["Ar\u00b7bei\u00b7ten", "m\u00fcs\u00b7sen", "wir", "und", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und statt des Stachels gab Natur", "tokens": ["Und", "statt", "des", "Sta\u00b7chels", "gab", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns eine stumpfe Zunge nur,", "tokens": ["Uns", "ei\u00b7ne", "stump\u00b7fe", "Zun\u00b7ge", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die d\u00fcrfen wir nie unsertwegen", "tokens": ["Die", "d\u00fcr\u00b7fen", "wir", "nie", "un\u00b7sert\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nur im Dienst des K\u00f6nigs regen.", "tokens": ["Und", "nur", "im", "Dienst", "des", "K\u00f6\u00b7nigs", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}