{"textgrid.poem.59283": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Schalk", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4uten kaum die Maienglocken", "tokens": ["L\u00e4u\u00b7ten", "kaum", "die", "Mai\u00b7en\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leise durch den lauen Wind,", "tokens": ["Lei\u00b7se", "durch", "den", "lau\u00b7en", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebt ein Knabe froh erschrocken", "tokens": ["Hebt", "ein", "Kna\u00b7be", "froh", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Grase sich geschwind,", "tokens": ["Aus", "dem", "Gra\u00b7se", "sich", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00fcttelt in den Bl\u00fctenflocken", "tokens": ["Sch\u00fct\u00b7telt", "in", "den", "Bl\u00fc\u00b7ten\u00b7flo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seine feinen blonden Locken,", "tokens": ["Sei\u00b7ne", "fei\u00b7nen", "blon\u00b7den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schelmisch sinnend wie ein Kind.", "tokens": ["Schel\u00b7misch", "sin\u00b7nend", "wie", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und nun wehen Lerchenlieder,", "tokens": ["Und", "nun", "we\u00b7hen", "Ler\u00b7chen\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es schl\u00e4gt die Nachtigall,", "tokens": ["Und", "es", "schl\u00e4gt", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rauschend von den Bergen nieder", "tokens": ["Rau\u00b7schend", "von", "den", "Ber\u00b7gen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt der k\u00fchle Wasserfall,", "tokens": ["Kommt", "der", "k\u00fch\u00b7le", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rings im Walde bunt Gefieder: \u2013", "tokens": ["Rings", "im", "Wal\u00b7de", "bunt", "Ge\u00b7fie\u00b7der", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00fchling, Fr\u00fchling ist es wieder", "tokens": ["Fr\u00fch\u00b7ling", ",", "Fr\u00fch\u00b7ling", "ist", "es", "wie\u00b7der"], "token_info": ["word", "punct", "wo