{"dta.poem.15046": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von den vortrefflichen Umst\u00e4nden  \n f\u00fcr die Poesie unter den Kaisern aus  \n dem schw\u00e4bischen Hause.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "die zwene kamen ouch aldar", "tokens": ["die", "zwe\u00b7ne", "ka\u00b7men", "ouch", "al\u00b7dar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die fuorten beide in lichter schar", "tokens": ["die", "fu\u00b7or\u00b7ten", "bei\u00b7de", "in", "lich\u00b7ter", "schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PIS", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "manegen ritterlichen degen", "tokens": ["ma\u00b7ne\u00b7gen", "rit\u00b7ter\u00b7li\u00b7chen", "de\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.4": {"text": "der K&#250;nec rich von Norwegen", "tokens": ["der", "K", "&#250;", "nec", "rich", "von", "Nor\u00b7we\u00b7gen"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "NE", "$(", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "vnd der von Orchadie", "tokens": ["vnd", "der", "von", "Or\u00b7cha\u00b7die"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "mit grozer massenie", "tokens": ["mit", "gro\u00b7zer", "mas\u00b7se\u00b7nie"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "zuo ritten unde randen", "tokens": ["zuo", "rit\u00b7ten", "un\u00b7de", "ran\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "der K&#250;nec von Irlanden", "tokens": ["der", "K", "&#250;", "nec", "von", "Ir\u00b7lan\u00b7den"], "token_info": ["word", "word", "XML_entity", "word", "word", "word"], "pos": ["ART", "NE", "$(", "ADV", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "vnd der von Tenemarken", "tokens": ["vnd", "der", "von", "Te\u00b7ne\u00b7mar\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "vuoren dar mit starken", "tokens": ["vu\u00b7o\u00b7ren", "dar", "mit", "star\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "rotten uf die gruenen wisen", "tokens": ["rot\u00b7ten", "uf", "die", "gru\u00b7e\u00b7nen", "wi\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "ouch wart gesehen da mit disen", "tokens": ["ouch", "wart", "ge\u00b7se\u00b7hen", "da", "mit", "di\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "VVPP", "ADV", "APPR", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "der K&#250;nec von Yspanie", "tokens": ["der", "K", "&#250;", "nec", "von", "Y\u00b7spa\u00b7nie"], "token_info": ["word", "word", "XML_entity", "word", "word", "word"], "pos": ["ART", "NE", "$(", "ADV", "APPR", "NE"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.14": {"text": "des Riches von Britanye", "tokens": ["des", "Ri\u00b7ches", "von", "Bri\u00b7ta\u00b7nye"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Pfleger waz da hin gefarn", "tokens": ["Pfle\u00b7ger", "waz", "da", "hin", "ge\u00b7farn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "ADV", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "der brahte dar in sinen scharn", "tokens": ["der", "brah\u00b7te", "dar", "in", "si\u00b7nen", "scharn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "vil manegen werden Britun", "tokens": ["vil", "ma\u00b7ne\u00b7gen", "wer\u00b7den", "Bri\u00b7tun"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VAFIN", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "da was der K&#250;nec von Arragun", "tokens": ["da", "was", "der", "K", "&#250;", "nec", "von", "Ar\u00b7ra\u00b7gun"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["ADV", "PWS", "ART", "NE", "$(", "FM.la", "FM.la", "FM.la"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "und der Kerlingere Vogt", "tokens": ["und", "der", "Ker\u00b7lin\u00b7ge\u00b7re", "Vogt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "von Nafarre dar gezogt", "tokens": ["von", "Na\u00b7far\u00b7re", "dar", "ge\u00b7zogt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "kam ein K&#250;nec wit erkant", "tokens": ["kam", "ein", "K", "&#250;", "nec", "wit", "er\u00b7kant"]