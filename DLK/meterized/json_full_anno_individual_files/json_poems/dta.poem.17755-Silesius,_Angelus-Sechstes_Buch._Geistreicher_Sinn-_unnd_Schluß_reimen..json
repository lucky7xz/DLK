{"dta.poem.17755": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "Sechstes Buch.  \n Geistreicher Sinn- unnd  \n Schlu\u00df reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Dein ansehn/ deine Pracht ist sch\u00f6ner alls das Kleid", "tokens": ["Dein", "an\u00b7sehn", "/", "dei\u00b7ne", "Pracht", "ist", "sch\u00f6\u00b7ner", "alls", "das", "Kleid"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "VVINF", "$(", "PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des K\u00f6nigs Salamons in seiner Herrigkeit/", "tokens": ["Des", "K\u00f6\u00b7nigs", "Sa\u00b7la\u00b7mons", "in", "sei\u00b7ner", "Her\u00b7rig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dir mu\u00df der klare Blitz der Seraphine weichen:", "tokens": ["Dir", "mu\u00df", "der", "kla\u00b7re", "Blitz", "der", "Se\u00b7ra\u00b7phi\u00b7ne", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Edeler Geruch erquikt die gantze Welt/", "tokens": ["Dein", "E\u00b7de\u00b7ler", "Ge\u00b7ruch", "er\u00b7quikt", "die", "gant\u00b7ze", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und was sonst unsrem GOtt dem Herrn zu Fusse f\u00e4llt.", "tokens": ["Und", "was", "sonst", "uns\u00b7rem", "Gott", "dem", "Herrn", "zu", "Fus\u00b7se", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In dir findt man allein die Sch\u00f6nheit der Jungfrauen/", "tokens": ["In", "dir", "findt", "man", "al\u00b7lein", "die", "Sch\u00f6n\u00b7heit", "der", "Jung\u00b7frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "ADV", "ART", "NN", "ART", "NN", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Der M\u00e4rterer bestand/ und aller Heilgen Ziehr.", "tokens": ["Der", "M\u00e4r\u00b7te\u00b7rer", "be\u00b7stand", "/", "und", "al\u00b7ler", "Heil\u00b7gen", "Ziehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Drumb Edle Lilie komm und erquik mich hier/", "tokens": ["Drumb", "Ed\u00b7le", "Li\u00b7lie", "komm", "und", "er\u00b7quik", "mich", "hier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "VVFIN", "KON", "VVIMP", "PPER", "ADV", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Da\u00df ich m\u00f6g ewig dich und deinen Saamen schauen.", "tokens": ["Da\u00df", "ich", "m\u00f6g", "e\u00b7wig", "dich", "und", "dei\u00b7nen", "Saa\u00b7men", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "ADJD", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}