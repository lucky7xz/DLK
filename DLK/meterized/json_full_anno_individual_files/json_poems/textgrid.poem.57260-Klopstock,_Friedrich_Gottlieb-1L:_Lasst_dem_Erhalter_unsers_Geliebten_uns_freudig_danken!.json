{"textgrid.poem.57260": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lasst dem Erhalter unsers Geliebten uns freudig danken!", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lasst dem Erhalter unsers Geliebten uns freudig danken!", "tokens": ["Lasst", "dem", "Er\u00b7hal\u00b7ter", "un\u00b7sers", "Ge\u00b7lieb\u00b7ten", "uns", "freu\u00b7dig", "dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Du hasts allein gethan, o du des Lebens", "tokens": ["Du", "hasts", "al\u00b7lein", "ge\u00b7than", ",", "o", "du", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "FM", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herr! und Herr des Todes!", "tokens": ["Herr", "!", "und", "Herr", "des", "To\u00b7des", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dir sey der Ruhm, der Dank, der Preis, die Ehre,", "tokens": ["Dir", "sey", "der", "Ruhm", ",", "der", "Dank", ",", "der", "Preis", ",", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Grosser Erhalter unsers Geliebten!", "tokens": ["Gros\u00b7ser", "Er\u00b7hal\u00b7ter", "un\u00b7sers", "Ge\u00b7lieb\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Thr\u00e4nen der Wonne, dankende Thr\u00e4nen seyn unser Opfer!", "tokens": ["Thr\u00e4\u00b7nen", "der", "Won\u00b7ne", ",", "dan\u00b7ken\u00b7de", "Thr\u00e4\u00b7nen", "seyn", "un\u00b7ser", "Op\u00b7fer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "VAINF", "PPOSAT", "NN", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mit diesem Opfer fallet tiefanbetend", "tokens": ["Mit", "die\u00b7sem", "Op\u00b7fer", "fal\u00b7let", "tie\u00b7fan\u00b7be\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor dem Throne nieder,", "tokens": ["Vor", "dem", "Thro\u00b7ne", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Von dem des Rettenden Befehl' erschollen:", "tokens": ["Von", "dem", "des", "Ret\u00b7ten\u00b7den", "Be\u00b7fehl'", "er\u00b7schol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Leben, ja leben soll mein Gesalbter!", "tokens": ["Le\u00b7ben", ",", "ja", "le\u00b7ben", "soll", "mein", "Ge\u00b7salb\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVINF", "VMFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wunderbar hast du, Vater des Schicksals, uns ihn erhalten!", "tokens": ["Wun\u00b7der\u00b7bar", "hast", "du", ",", "Va\u00b7ter", "des", "Schick\u00b7sals", ",", "uns", "ihn", "er\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,", "PPER", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Zu viel, zu viel Barmherzigkeit, o Vater,", "tokens": ["Zu", "viel", ",", "zu", "viel", "Barm\u00b7her\u00b7zig\u00b7keit", ",", "o", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "APPR", "PIAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hast du uns gegeben!", "tokens": ["Hast", "du", "uns", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Steig oft, und stark, Gebet, viel ist der Gnade!", "tokens": ["Steig", "oft", ",", "und", "stark", ",", "Ge\u00b7bet", ",", "viel", "ist", "der", "Gna\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ADJD", "$,", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Steige mit Wonne auf zu dem Geber!", "tokens": ["Stei\u00b7ge", "mit", "Won\u00b7ne", "auf", "zu", "dem", "Ge\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Mengen erlagen, aber ihn r\u00fchrte sanft deine Hand nur,", "tokens": ["Men\u00b7gen", "er\u00b7la\u00b7gen", ",", "a\u00b7ber", "ihn", "r\u00fchr\u00b7te", "sanft", "dei\u00b7ne", "Hand", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "ADV", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "So sanft, dass wir sogar, wer kan hier danken?", "tokens": ["So", "sanft", ",", "dass", "wir", "so\u00b7gar", ",", "wer", "kan", "hier", "dan\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "$,", "PWS", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht einmal erschraken!", "tokens": ["Nicht", "ein\u00b7mal", "er\u00b7schra\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu viel, zu viel Barmherzigkeit, o Vater,", "tokens": ["Zu", "viel", ",", "zu", "viel", "Barm\u00b7her\u00b7zig\u00b7keit", ",", "o", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "APPR", "PIAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gab uns die Stunde deiner Errettung!", "tokens": ["Gab", "uns", "die", "Stun\u00b7de", "dei\u00b7ner", "Er\u00b7ret\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ach, den wir lieben, Vater, er lebet! und auch wir leben!", "tokens": ["Ach", ",", "den", "wir", "lie\u00b7ben", ",", "Va\u00b7ter", ",", "er", "le\u00b7bet", "!", "und", "auch", "wir", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PRELS", "PPER", "VVFIN", "$,", "NN", "$,", "PPER", "VVFIN", "$.", "KON", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Denn in der Stunde deiner reichen Gnade,", "tokens": ["Denn", "in", "der", "Stun\u00b7de", "dei\u00b7ner", "rei\u00b7chen", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da du ihn erhieltest,", "tokens": ["Da", "du", "ihn", "er\u00b7hiel\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Da r\u00fchrtest du auch uns mit sanfter Hand an.", "tokens": ["Da", "r\u00fchr\u00b7test", "du", "auch", "uns", "mit", "sanf\u00b7ter", "Hand", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vater, die Erde bebt', und wir leben!", "tokens": ["Va\u00b7ter", ",", "die", "Er\u00b7de", "bebt'", ",", "und", "wir", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Herr! da die Erde unter uns bebte, scholl deine Stimme,", "tokens": ["Herr", "!", "da", "die", "Er\u00b7de", "un\u00b7ter", "uns", "beb\u00b7te", ",", "scholl", "dei\u00b7ne", "Stim\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Nicht deines Zornes, deiner Liebe Stimme", "tokens": ["Nicht", "dei\u00b7nes", "Zor\u00b7nes", ",", "dei\u00b7ner", "Lie\u00b7be", "Stim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Scholl, uns aus dem Staube", "tokens": ["Scholl", ",", "uns", "aus", "dem", "Stau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PPER", "APPR", "ART", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Zu rufen, und gen Himmel schaun zu lehren,", "tokens": ["Zu", "ru\u00b7fen", ",", "und", "gen", "Him\u00b7mel", "schaun", "zu", "leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "APPR", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf zu des Lebens Herrn, und des Todes!", "tokens": ["Auf", "zu", "des", "Le\u00b7bens", "Herrn", ",", "und", "des", "To\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Noch mit Entz\u00fcckung h\u00f6r' ich der Erde gelindes Rauschen!", "tokens": ["Noch", "mit", "Ent\u00b7z\u00fc\u00b7ckung", "h\u00f6r'", "ich", "der", "Er\u00b7de", "ge\u00b7lin\u00b7des", "Rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Des Richters Arm, der \u00fcber andre V\u00f6lker", "tokens": ["Des", "Rich\u00b7ters", "Arm", ",", "der", "\u00fc\u00b7ber", "and\u00b7re", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcrchterlich sich ausstreckt,", "tokens": ["F\u00fcrch\u00b7ter\u00b7lich", "sich", "aus\u00b7streckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die St\u00e4dt' ersch\u00fcttert, dass sie voll Entsetzens", "tokens": ["Die", "St\u00e4dt'", "er\u00b7sch\u00fct\u00b7tert", ",", "dass", "sie", "voll", "Ent\u00b7set\u00b7zens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Donnern, und fallen, unterzugehen!", "tokens": ["Don\u00b7nern", ",", "und", "fal\u00b7len", ",", "un\u00b7ter\u00b7zu\u00b7ge\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KON", "VVINF", "$,", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Der jetzt die V\u00f6lker, dass es sie w\u00fcrge, dem Schwerte zuf\u00fchrt,", "tokens": ["Der", "jetzt", "die", "V\u00f6l\u00b7ker", ",", "dass", "es", "sie", "w\u00fcr\u00b7ge", ",", "dem", "Schwer\u00b7te", "zu\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Arm wird \u00fcber unserm Haupt erhoben,", "tokens": ["Der", "Arm", "wird", "\u00fc\u00b7ber", "un\u00b7serm", "Haupt", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach, damit er segne!", "tokens": ["Ach", ",", "da\u00b7mit", "er", "seg\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und dass wir auf des Segens F\u00fclle merken,", "tokens": ["Und", "dass", "wir", "auf", "des", "Se\u00b7gens", "F\u00fcl\u00b7le", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wecket er sanft uns auf von dem Schlummer.", "tokens": ["We\u00b7cket", "er", "sanft", "uns", "auf", "von", "dem", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Fallet mit Jauchzen vor dem Erbarmer aufs Antlitz nieder!", "tokens": ["Fal\u00b7let", "mit", "Jauch\u00b7zen", "vor", "dem", "Er\u00b7bar\u00b7mer", "aufs", "Ant\u00b7litz", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Lasst Aller Herz das Halleluja singen!", "tokens": ["Lasst", "Al\u00b7ler", "Herz", "das", "Hal\u00b7le\u00b7lu\u00b7ja", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herr, Herr, Gott, barmherzig!", "tokens": ["Herr", ",", "Herr", ",", "Gott", ",", "barm\u00b7her\u00b7zig", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Du Dulder, du Getreuer! Gnadevoller!", "tokens": ["Du", "Dul\u00b7der", ",", "du", "Ge\u00b7treu\u00b7er", "!", "Gna\u00b7de\u00b7vol\u00b7ler", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ehre dir! Preis dir! Dank dir, Erbarmer!", "tokens": ["Eh\u00b7re", "dir", "!", "Preis", "dir", "!", "Dank", "dir", ",", "Er\u00b7bar\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "PPER", "$.", "APPR", "PPER", "$,", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Ging nicht des Herschers Herlichkeit sichtbar vor uns vor\u00fcber?", "tokens": ["Ging", "nicht", "des", "Her\u00b7schers", "Her\u00b7lich\u00b7keit", "sicht\u00b7bar", "vor", "uns", "vor\u00b7\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-++--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Lasst uns anbetend ihr von ferne nachsehn!", "tokens": ["Lasst", "uns", "an\u00b7be\u00b7tend", "ihr", "von", "fer\u00b7ne", "nach\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "PPER", "APPR", "ADV", "VVINF", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ja, in unsrer Seele", "tokens": ["Ja", ",", "in", "uns\u00b7rer", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Soll dieses Heils Erinnrung ewig bleiben,", "tokens": ["Soll", "die\u00b7ses", "Heils", "E\u00b7rinn\u00b7rung", "e\u00b7wig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bleiben, ein Nachhall dessen, was Gott that!", "tokens": ["Blei\u00b7ben", ",", "ein", "Nach\u00b7hall", "des\u00b7sen", ",", "was", "Gott", "that", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "PDS", "$,", "PRELS", "NN", "VVFIN", "$."], "meter": "+--+-+-++-", "measure": "iambic.penta.invert"}}, "stanza.11": {"line.1": {"text": "Sagt es den Enkeln, V\u00e4ter, und lehrt sie gen Himmel schauen!", "tokens": ["Sagt", "es", "den", "En\u00b7keln", ",", "V\u00e4\u00b7ter", ",", "und", "lehrt", "sie", "gen", "Him\u00b7mel", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vernims, der Enkel Sohn, und lerne danken!", "tokens": ["Ver\u00b7nims", ",", "der", "En\u00b7kel", "Sohn", ",", "und", "ler\u00b7ne", "dan\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "$,", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kein Greis entschlumre,", "tokens": ["Und", "kein", "Greis", "ent\u00b7schlum\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der nicht noch Einmal Dank, wenn er entschlummert,", "tokens": ["Der", "nicht", "noch", "Ein\u00b7mal", "Dank", ",", "wenn", "er", "ent\u00b7schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADV", "NN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gott aus des Herzens Innerstem stamle.", "tokens": ["Gott", "aus", "des", "Her\u00b7zens", "In\u00b7ners\u00b7tem", "stam\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Dass wir dir danken, Vater, o gieb uns auch diese Gnade!", "tokens": ["Dass", "wir", "dir", "dan\u00b7ken", ",", "Va\u00b7ter", ",", "o", "gieb", "uns", "auch", "die\u00b7se", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "$,", "NN", "$,", "FM", "VVIMP", "PPER", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Herr, Herr! Preis, Ehr', und Ruhm sey, und Anbetung", "tokens": ["Herr", ",", "Herr", "!", "Preis", ",", "Ehr'", ",", "und", "Ruhm", "sey", ",", "und", "An\u00b7be\u00b7tung"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "NN", "$,", "NN", "$,", "KON", "NN", "VAFIN", "$,", "KON", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Deinem grossen Namen!", "tokens": ["Dei\u00b7nem", "gros\u00b7sen", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Im Himmel oben hubst du deinen Arm auf,", "tokens": ["Im", "Him\u00b7mel", "o\u00b7ben", "hubst", "du", "dei\u00b7nen", "Arm", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Herr! uns zu segnen, Herr! uns zu segnen!", "tokens": ["Herr", "!", "uns", "zu", "seg\u00b7nen", ",", "Herr", "!", "uns", "zu", "seg\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "PTKZU", "VVINF", "$,", "NN", "$.", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.13": {"line.1": {"text": "Lasst dem Erhalter unsers Geliebten uns freudig danken!", "tokens": ["Lasst", "dem", "Er\u00b7hal\u00b7ter", "un\u00b7sers", "Ge\u00b7lieb\u00b7ten", "uns", "freu\u00b7dig", "dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Du hasts allein gethan, o du des Lebens", "tokens": ["Du", "hasts", "al\u00b7lein", "ge\u00b7than", ",", "o", "du", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "FM", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herr! und Herr des Todes!", "tokens": ["Herr", "!", "und", "Herr", "des", "To\u00b7des", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Dir sey der Ruhm, der Dank, der Preis, die Ehre,", "tokens": ["Dir", "sey", "der", "Ruhm", ",", "der", "Dank", ",", "der", "Preis", ",", "die", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Grosser Erhalter unsers Geliebten!", "tokens": ["Gros\u00b7ser", "Er\u00b7hal\u00b7ter", "un\u00b7sers", "Ge\u00b7lieb\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.14": {"line.1": {"text": "Thr\u00e4nen der Wonne, dankende Thr\u00e4nen seyn unser Opfer!", "tokens": ["Thr\u00e4\u00b7nen", "der", "Won\u00b7ne", ",", "dan\u00b7ken\u00b7de", "Thr\u00e4\u00b7nen", "seyn", "un\u00b7ser", "Op\u00b7fer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "VAINF", "PPOSAT", "NN", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mit diesem Opfer fallet tiefanbetend", "tokens": ["Mit", "die\u00b7sem", "Op\u00b7fer", "fal\u00b7let", "tie\u00b7fan\u00b7be\u00b7tend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor dem Throne nieder,", "tokens": ["Vor", "dem", "Thro\u00b7ne", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Von dem des Rettenden Befehl' erschollen:", "tokens": ["Von", "dem", "des", "Ret\u00b7ten\u00b7den", "Be\u00b7fehl'", "er\u00b7schol\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Leben, ja leben soll mein Gesalbter!", "tokens": ["Le\u00b7ben", ",", "ja", "le\u00b7ben", "soll", "mein", "Ge\u00b7salb\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVINF", "VMFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.15": {"line.1": {"text": "Wunderbar hast du, Vater des Schicksals, uns ihn erhalten!", "tokens": ["Wun\u00b7der\u00b7bar", "hast", "du", ",", "Va\u00b7ter", "des", "Schick\u00b7sals", ",", "uns", "ihn", "er\u00b7hal\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,", "PPER", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Zu viel, zu viel Barmherzigkeit, o Vater,", "tokens": ["Zu", "viel", ",", "zu", "viel", "Barm\u00b7her\u00b7zig\u00b7keit", ",", "o", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "APPR", "PIAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hast du uns gegeben!", "tokens": ["Hast", "du", "uns", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Steig oft, und stark, Gebet, viel ist der Gnade!", "tokens": ["Steig", "oft", ",", "und", "stark", ",", "Ge\u00b7bet", ",", "viel", "ist", "der", "Gna\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ADJD", "$,", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Steige mit Wonne auf zu dem Geber!", "tokens": ["Stei\u00b7ge", "mit", "Won\u00b7ne", "auf", "zu", "dem", "Ge\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.16": {"line.1": {"text": "Mengen erlagen, aber ihn r\u00fchrte sanft deine Hand nur,", "tokens": ["Men\u00b7gen", "er\u00b7la\u00b7gen", ",", "a\u00b7ber", "ihn", "r\u00fchr\u00b7te", "sanft", "dei\u00b7ne", "Hand", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "ADV", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "So sanft, dass wir sogar, wer kan hier danken?", "tokens": ["So", "sanft", ",", "dass", "wir", "so\u00b7gar", ",", "wer", "kan", "hier", "dan\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "$,", "PWS", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht einmal erschraken!", "tokens": ["Nicht", "ein\u00b7mal", "er\u00b7schra\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu viel, zu viel Barmherzigkeit, o Vater,", "tokens": ["Zu", "viel", ",", "zu", "viel", "Barm\u00b7her\u00b7zig\u00b7keit", ",", "o", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "APPR", "PIAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gab uns die Stunde deiner Errettung!", "tokens": ["Gab", "uns", "die", "Stun\u00b7de", "dei\u00b7ner", "Er\u00b7ret\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.17": {"line.1": {"text": "Ach, den wir lieben, Vater, er lebet! und auch wir leben!", "tokens": ["Ach", ",", "den", "wir", "lie\u00b7ben", ",", "Va\u00b7ter", ",", "er", "le\u00b7bet", "!", "und", "auch", "wir", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PRELS", "PPER", "VVFIN", "$,", "NN", "$,", "PPER", "VVFIN", "$.", "KON", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Denn in der Stunde deiner reichen Gnade,", "tokens": ["Denn", "in", "der", "Stun\u00b7de", "dei\u00b7ner", "rei\u00b7chen", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da du ihn erhieltest,", "tokens": ["Da", "du", "ihn", "er\u00b7hiel\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Da r\u00fchrtest du auch uns mit sanfter Hand an.", "tokens": ["Da", "r\u00fchr\u00b7test", "du", "auch", "uns", "mit", "sanf\u00b7ter", "Hand", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vater, die Erde bebt', und wir leben!", "tokens": ["Va\u00b7ter", ",", "die", "Er\u00b7de", "bebt'", ",", "und", "wir", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "$,", "KON", "PPER", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.18": {"line.1": {"text": "Herr! da die Erde unter uns bebte, scholl deine Stimme,", "tokens": ["Herr", "!", "da", "die", "Er\u00b7de", "un\u00b7ter", "uns", "beb\u00b7te", ",", "scholl", "dei\u00b7ne", "Stim\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Nicht deines Zornes, deiner Liebe Stimme", "tokens": ["Nicht", "dei\u00b7nes", "Zor\u00b7nes", ",", "dei\u00b7ner", "Lie\u00b7be", "Stim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Scholl, uns aus dem Staube", "tokens": ["Scholl", ",", "uns", "aus", "dem", "Stau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PPER", "APPR", "ART", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Zu rufen, und gen Himmel schaun zu lehren,", "tokens": ["Zu", "ru\u00b7fen", ",", "und", "gen", "Him\u00b7mel", "schaun", "zu", "leh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KON", "APPR", "NN", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Auf zu des Lebens Herrn, und des Todes!", "tokens": ["Auf", "zu", "des", "Le\u00b7bens", "Herrn", ",", "und", "des", "To\u00b7des", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$,", "KON", "ART", "NN", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.19": {"line.1": {"text": "Noch mit Entz\u00fcckung h\u00f6r' ich der Erde gelindes Rauschen!", "tokens": ["Noch", "mit", "Ent\u00b7z\u00fc\u00b7ckung", "h\u00f6r'", "ich", "der", "Er\u00b7de", "ge\u00b7lin\u00b7des", "Rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Des Richters Arm, der \u00fcber andre V\u00f6lker", "tokens": ["Des", "Rich\u00b7ters", "Arm", ",", "der", "\u00fc\u00b7ber", "and\u00b7re", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcrchterlich sich ausstreckt,", "tokens": ["F\u00fcrch\u00b7ter\u00b7lich", "sich", "aus\u00b7streckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die St\u00e4dt' ersch\u00fcttert, dass sie voll Entsetzens", "tokens": ["Die", "St\u00e4dt'", "er\u00b7sch\u00fct\u00b7tert", ",", "dass", "sie", "voll", "Ent\u00b7set\u00b7zens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Donnern, und fallen, unterzugehen!", "tokens": ["Don\u00b7nern", ",", "und", "fal\u00b7len", ",", "un\u00b7ter\u00b7zu\u00b7ge\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KON", "VVINF", "$,", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.20": {"line.1": {"text": "Der jetzt die V\u00f6lker, dass es sie w\u00fcrge, dem Schwerte zuf\u00fchrt,", "tokens": ["Der", "jetzt", "die", "V\u00f6l\u00b7ker", ",", "dass", "es", "sie", "w\u00fcr\u00b7ge", ",", "dem", "Schwer\u00b7te", "zu\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Arm wird \u00fcber unserm Haupt erhoben,", "tokens": ["Der", "Arm", "wird", "\u00fc\u00b7ber", "un\u00b7serm", "Haupt", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach, damit er segne!", "tokens": ["Ach", ",", "da\u00b7mit", "er", "seg\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und dass wir auf des Segens F\u00fclle merken,", "tokens": ["Und", "dass", "wir", "auf", "des", "Se\u00b7gens", "F\u00fcl\u00b7le", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wecket er sanft uns auf von dem Schlummer.", "tokens": ["We\u00b7cket", "er", "sanft", "uns", "auf", "von", "dem", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.21": {"line.1": {"text": "Fallet mit Jauchzen vor dem Erbarmer aufs Antlitz nieder!", "tokens": ["Fal\u00b7let", "mit", "Jauch\u00b7zen", "vor", "dem", "Er\u00b7bar\u00b7mer", "aufs", "Ant\u00b7litz", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Lasst Aller Herz das Halleluja singen!", "tokens": ["Lasst", "Al\u00b7ler", "Herz", "das", "Hal\u00b7le\u00b7lu\u00b7ja", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herr, Herr, Gott, barmherzig!", "tokens": ["Herr", ",", "Herr", ",", "Gott", ",", "barm\u00b7her\u00b7zig", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Du Dulder, du Getreuer! Gnadevoller!", "tokens": ["Du", "Dul\u00b7der", ",", "du", "Ge\u00b7treu\u00b7er", "!", "Gna\u00b7de\u00b7vol\u00b7ler", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ehre dir! Preis dir! Dank dir, Erbarmer!", "tokens": ["Eh\u00b7re", "dir", "!", "Preis", "dir", "!", "Dank", "dir", ",", "Er\u00b7bar\u00b7mer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "NN", "PPER", "$.", "APPR", "PPER", "$,", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.22": {"line.1": {"text": "Ging nicht des Herschers Herlichkeit sichtbar vor uns vor\u00fcber?", "tokens": ["Ging", "nicht", "des", "Her\u00b7schers", "Her\u00b7lich\u00b7keit", "sicht\u00b7bar", "vor", "uns", "vor\u00b7\u00fc\u00b7ber", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-++--+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Lasst uns anbetend ihr von ferne nachsehn!", "tokens": ["Lasst", "uns", "an\u00b7be\u00b7tend", "ihr", "von", "fer\u00b7ne", "nach\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "PPER", "APPR", "ADV", "VVINF", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ja, in unsrer Seele", "tokens": ["Ja", ",", "in", "uns\u00b7rer", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Soll dieses Heils Erinnrung ewig bleiben,", "tokens": ["Soll", "die\u00b7ses", "Heils", "E\u00b7rinn\u00b7rung", "e\u00b7wig", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDAT", "NN", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bleiben, ein Nachhall dessen, was Gott that!", "tokens": ["Blei\u00b7ben", ",", "ein", "Nach\u00b7hall", "des\u00b7sen", ",", "was", "Gott", "that", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "PDS", "$,", "PRELS", "NN", "VVFIN", "$."], "meter": "+--+-+-++-", "measure": "iambic.penta.invert"}}, "stanza.23": {"line.1": {"text": "Sagt es den Enkeln, V\u00e4ter, und lehrt sie gen Himmel schauen!", "tokens": ["Sagt", "es", "den", "En\u00b7keln", ",", "V\u00e4\u00b7ter", ",", "und", "lehrt", "sie", "gen", "Him\u00b7mel", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vernims, der Enkel Sohn, und lerne danken!", "tokens": ["Ver\u00b7nims", ",", "der", "En\u00b7kel", "Sohn", ",", "und", "ler\u00b7ne", "dan\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "NN", "$,", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und kein Greis entschlumre,", "tokens": ["Und", "kein", "Greis", "ent\u00b7schlum\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Der nicht noch Einmal Dank, wenn er entschlummert,", "tokens": ["Der", "nicht", "noch", "Ein\u00b7mal", "Dank", ",", "wenn", "er", "ent\u00b7schlum\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADV", "NN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gott aus des Herzens Innerstem stamle.", "tokens": ["Gott", "aus", "des", "Her\u00b7zens", "In\u00b7ners\u00b7tem", "stam\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.24": {"line.1": {"text": "Dass wir dir danken, Vater, o gieb uns auch diese Gnade!", "tokens": ["Dass", "wir", "dir", "dan\u00b7ken", ",", "Va\u00b7ter", ",", "o", "gieb", "uns", "auch", "die\u00b7se", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "$,", "NN", "$,", "FM", "VVIMP", "PPER", "ADV", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Herr, Herr! Preis, Ehr', und Ruhm sey, und Anbetung", "tokens": ["Herr", ",", "Herr", "!", "Preis", ",", "Ehr'", ",", "und", "Ruhm", "sey", ",", "und", "An\u00b7be\u00b7tung"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "NN", "$,", "NN", "$,", "KON", "NN", "VAFIN", "$,", "KON", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Deinem grossen Namen!", "tokens": ["Dei\u00b7nem", "gros\u00b7sen", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Im Himmel oben hubst du deinen Arm auf,", "tokens": ["Im", "Him\u00b7mel", "o\u00b7ben", "hubst", "du", "dei\u00b7nen", "Arm", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Herr! uns zu segnen, Herr! uns zu segnen!", "tokens": ["Herr", "!", "uns", "zu", "seg\u00b7nen", ",", "Herr", "!", "uns", "zu", "seg\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "PTKZU", "VVINF", "$,", "NN", "$.", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}}}}