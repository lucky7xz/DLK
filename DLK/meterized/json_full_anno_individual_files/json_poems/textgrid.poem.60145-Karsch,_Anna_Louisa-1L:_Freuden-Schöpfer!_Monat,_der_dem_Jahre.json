a"}, "line.2": {"text": "Bilder machet, die, wie Menschen weinen,", "tokens": ["Bil\u00b7der", "ma\u00b7chet", ",", "die", ",", "wie", "Men\u00b7schen", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Werdest du gehauen auf sein Grab.", "tokens": ["Wer\u00b7dest", "du", "ge\u00b7hau\u00b7en", "auf", "sein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In Gestalt des M\u00e4dchens, die ihn dachte,", "tokens": ["In", "Ge\u00b7stalt", "des", "M\u00e4d\u00b7chens", ",", "die", "ihn", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mit dem Schoo\u00df voll Blumen, die sie brachte,", "tokens": ["Mit", "dem", "Schoo\u00df", "voll", "Blu\u00b7men", ",", "die", "sie", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zeichne dich des K\u00fcnstlers Meissel ab!", "tokens": ["Zeich\u00b7ne", "dich", "des", "K\u00fcnst\u00b7lers", "Meis\u00b7sel", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.19": {"line.1": {"text": "Wenn alsdann in sp\u00e4tgekommnen Tagen,", "tokens": ["Wenn", "als\u00b7dann", "in", "sp\u00e