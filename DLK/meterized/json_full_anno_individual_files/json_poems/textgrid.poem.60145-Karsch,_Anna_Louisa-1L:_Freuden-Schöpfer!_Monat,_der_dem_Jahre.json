{"textgrid.poem.60145": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freuden-Sch\u00f6pfer! Monat, der dem Jahre", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freuden-Sch\u00f6pfer! Monat, der dem Jahre", "tokens": ["Freu\u00b7den\u00b7Sch\u00f6p\u00b7fer", "!", "Mo\u00b7nat", ",", "der", "dem", "Jah\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zierath gab, und diese jungen Haare", "tokens": ["Zie\u00b7rath", "gab", ",", "und", "die\u00b7se", "jun\u00b7gen", "Haa\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf der B\u00e4ume kahlgestandnes Haupt;", "tokens": ["Auf", "der", "B\u00e4u\u00b7me", "kahl\u00b7ge\u00b7stand\u00b7nes", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Eile langsam mit gesenkten Schwingen!", "tokens": ["Ei\u00b7le", "lang\u00b7sam", "mit", "ge\u00b7senk\u00b7ten", "Schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Bleib noch! la\u00df mich deine Reitze singen,", "tokens": ["Bleib", "noch", "!", "la\u00df", "mich", "dei\u00b7ne", "Reit\u00b7ze", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eh' ein kriechend Gift den Baum entlaubt.", "tokens": ["Eh'", "ein", "krie\u00b7chend", "Gift", "den", "Baum", "ent\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Deiner Ankunft freuten sich die Hirten", "tokens": ["Dei\u00b7ner", "An\u00b7kunft", "freu\u00b7ten", "sich", "die", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und becr\u00e4nzt mit frischgebrochnen Myrten", "tokens": ["Und", "be\u00b7cr\u00b7\u00e4nzt", "mit", "frischge\u00b7broch\u00b7nen", "Myr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Stampften sie das jugendliche Gra\u00df;", "tokens": ["Stampf\u00b7ten", "sie", "das", "ju\u00b7gend\u00b7li\u00b7che", "Gra\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da indessen Damon ohne Zeugen", "tokens": ["Da", "in\u00b7des\u00b7sen", "Da\u00b7mon", "oh\u00b7ne", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "NE", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Unter krumgewachsnen dichten Zweigen,", "tokens": ["Un\u00b7ter", "krum\u00b7ge\u00b7wachs\u00b7nen", "dich\u00b7ten", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schlau verborgen, bey der Phillis sa\u00df!", "tokens": ["Schlau", "ver\u00b7bor\u00b7gen", ",", "bey", "der", "Phil\u00b7lis", "sa\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "APPR", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Du erscheinst mit ganzen Myriaden", "tokens": ["Du", "er\u00b7scheinst", "mit", "gan\u00b7zen", "My\u00b7ria\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Bunter Blumen um und um beladen,", "tokens": ["Bun\u00b7ter", "Blu\u00b7men", "um", "und", "um", "be\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "KON", "APPR", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die du auf der Erde Schoo\u00df gestreut;", "tokens": ["Die", "du", "auf", "der", "Er\u00b7de", "Schoo\u00df", "ge\u00b7streut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Deine weisse Silbergl\u00f6ckchen d\u00fcften", "tokens": ["Dei\u00b7ne", "weis\u00b7se", "Sil\u00b7ber\u00b7gl\u00f6ck\u00b7chen", "d\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ihren Balsam aus, und in den L\u00fcften", "tokens": ["Ih\u00b7ren", "Bal\u00b7sam", "aus", ",", "und", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Singen Lerchen deine Lieblichkeit.", "tokens": ["Sin\u00b7gen", "Ler\u00b7chen", "dei\u00b7ne", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Von der Liebe treulich unterrichtet", "tokens": ["Von", "der", "Lie\u00b7be", "treu\u00b7lich", "un\u00b7ter\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Singt ein Vogel, der wie Sapho dichtet,", "tokens": ["Singt", "ein", "Vo\u00b7gel", ",", "der", "wie", "Sa\u00b7pho", "dich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$,", "PRELS", "KOKOM", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ganze N\u00e4chte in der Ode Thon.", "tokens": ["Gan\u00b7ze", "N\u00e4ch\u00b7te", "in", "der", "O\u00b7de", "Thon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nachtigallen singen ihre Klagen,", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "sin\u00b7gen", "ih\u00b7re", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und der Sperling in den alten Tagen,", "tokens": ["Und", "der", "Sper\u00b7ling", "in", "den", "al\u00b7ten", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "H\u00fcpft und buhlt noch, wie Anacreon.", "tokens": ["H\u00fcpft", "und", "buhlt", "noch", ",", "wie", "A\u00b7na\u00b7cre\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "$,", "PWAV", "NE", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Du erweckst mit deinem sanften Hauche", "tokens": ["Du", "er\u00b7weckst", "mit", "dei\u00b7nem", "sanf\u00b7ten", "Hau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Alle Creaturen zum Gebrauche", "tokens": ["Al\u00b7le", "Crea\u00b7tu\u00b7ren", "zum", "Ge\u00b7brau\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ihres Lebens, das so bald verflieht;", "tokens": ["Ih\u00b7res", "Le\u00b7bens", ",", "das", "so", "bald", "ver\u00b7flieht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bienen summen, und die kalten Fr\u00f6sche", "tokens": ["Bie\u00b7nen", "sum\u00b7men", ",", "und", "die", "kal\u00b7ten", "Fr\u00f6\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sagen, durch ihr quackendes Gew\u00e4sche,", "tokens": ["Sa\u00b7gen", ",", "durch", "ihr", "qua\u00b7cken\u00b7des", "Ge\u00b7w\u00e4\u00b7sche", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da\u00df die Freude sie ans Ufer zieht.", "tokens": ["Da\u00df", "die", "Freu\u00b7de", "sie", "ans", "U\u00b7fer", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Gr\u00fcner machst du Bl\u00e4tter an den Zweigen", "tokens": ["Gr\u00fc\u00b7ner", "machst", "du", "Bl\u00e4t\u00b7ter", "an", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die sich um den Schlaf des J\u00fcnglings beugen,", "tokens": ["Die", "sich", "um", "den", "Schlaf", "des", "J\u00fcng\u00b7lings", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der im Marsfeld wie ein L\u00f6we stritt;", "tokens": ["Der", "im", "Mars\u00b7feld", "wie", "ein", "L\u00f6\u00b7we", "stritt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "KOKOM", "ART", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Alle Jahre kommest du mit neuen", "tokens": ["Al\u00b7le", "Jah\u00b7re", "kom\u00b7mest", "du", "mit", "neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Blumen, auf des Helden Grab zu streuen,", "tokens": ["Blu\u00b7men", ",", "auf", "des", "Hel\u00b7den", "Grab", "zu", "streu\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Dessen Faden fr\u00fch die Parce schnitt.", "tokens": ["Des\u00b7sen", "Fa\u00b7den", "fr\u00fch", "die", "Par\u00b7ce", "schnitt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Holder May, bey jenem Siz der Musen,", "tokens": ["Hol\u00b7der", "May", ",", "bey", "je\u00b7nem", "Siz", "der", "Mu\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die Oder ihren ofnen Busen", "tokens": ["Wo", "die", "O\u00b7der", "ih\u00b7ren", "of\u00b7nen", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NE", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit erschlagner Russen Blut geschw\u00e4rzt,", "tokens": ["Mit", "er\u00b7schlag\u00b7ner", "Rus\u00b7sen", "Blut", "ge\u00b7schw\u00e4rzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Liegt ein Dichter, der dich einst gesungen;", "tokens": ["Liegt", "ein", "Dich\u00b7ter", ",", "der", "dich", "einst", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hundert Seelen hat sein Tod durchdrungen,", "tokens": ["Hun\u00b7dert", "See\u00b7len", "hat", "sein", "Tod", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "O, er starb voll Wunden, und beherzt!", "tokens": ["O", ",", "er", "starb", "voll", "Wun\u00b7den", ",", "und", "be\u00b7herzt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADJD", "NN", "$,", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Von dem gr\u00f6\u00dften K\u00fcnstler der aus Steinen", "tokens": ["Von", "dem", "gr\u00f6\u00df\u00b7ten", "K\u00fcnst\u00b7ler", "der", "aus", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bilder machet, die, wie Menschen weinen,", "tokens": ["Bil\u00b7der", "ma\u00b7chet", ",", "die", ",", "wie", "Men\u00b7schen", "wei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Werdest du gehauen auf sein Grab.", "tokens": ["Wer\u00b7dest", "du", "ge\u00b7hau\u00b7en", "auf", "sein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In Gestalt des M\u00e4dchens, die ihn dachte,", "tokens": ["In", "Ge\u00b7stalt", "des", "M\u00e4d\u00b7chens", ",", "die", "ihn", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Mit dem Schoo\u00df voll Blumen, die sie brachte,", "tokens": ["Mit", "dem", "Schoo\u00df", "voll", "Blu\u00b7men", ",", "die", "sie", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zeichne dich des K\u00fcnstlers Meissel ab!", "tokens": ["Zeich\u00b7ne", "dich", "des", "K\u00fcnst\u00b7lers", "Meis\u00b7sel", "ab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Wenn alsdann in sp\u00e4tgekommnen Tagen,", "tokens": ["Wenn", "als\u00b7dann", "in", "sp\u00e4t\u00b7ge\u00b7komm\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wandrer nach des Grabes Nahmen fragen,", "tokens": ["Wand\u00b7rer", "nach", "des", "Gra\u00b7bes", "Nah\u00b7men", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nenn' ein Marmor-Schild den sanften Kleist,", "tokens": ["Nenn'", "ein", "Mar\u00b7mor\u00b7Schild", "den", "sanf\u00b7ten", "Kleist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der nur Zorn empfunden gegen Feinde;", "tokens": ["Der", "nur", "Zorn", "emp\u00b7fun\u00b7den", "ge\u00b7gen", "Fein\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Eine Tafel nenne seine Freunde,", "tokens": ["Ei\u00b7ne", "Ta\u00b7fel", "nen\u00b7ne", "sei\u00b7ne", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und berichte, wie das M\u00e4dchen hei\u00dft,", "tokens": ["Und", "be\u00b7rich\u00b7te", ",", "wie", "das", "M\u00e4d\u00b7chen", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Die, gereizet von des Helden Ruhme,", "tokens": ["Die", ",", "ge\u00b7rei\u00b7zet", "von", "des", "Hel\u00b7den", "Ruh\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seinem Staube, diesem Heiligthume,", "tokens": ["Sei\u00b7nem", "Stau\u00b7be", ",", "die\u00b7sem", "Hei\u00b7lig\u00b7thu\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tausend Fr\u00fchlings-Kinder opferte!", "tokens": ["Tau\u00b7send", "Fr\u00fch\u00b7lings\u00b7Kin\u00b7der", "op\u00b7fer\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Sch\u00f6ner Monat, komme oftmahls wieder!", "tokens": ["Sch\u00f6\u00b7ner", "Mo\u00b7nat", ",", "kom\u00b7me", "oft\u00b7mahls", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Streu aus deinem Schoosse Blumen nieder", "tokens": ["Streu", "aus", "dei\u00b7nem", "Schoos\u00b7se", "Blu\u00b7men", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "NN", "PTKVZ"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Vor dem M\u00e4dchen, da\u00df es sanfter geh'!", "tokens": ["Vor", "dem", "M\u00e4d\u00b7chen", ",", "da\u00df", "es", "sanf\u00b7ter", "geh'", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Freuden-Sch\u00f6pfer! Monat, der dem Jahre", "tokens": ["Freu\u00b7den\u00b7Sch\u00f6p\u00b7fer", "!", "Mo\u00b7nat", ",", "der", "dem", "Jah\u00b7re"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zierath gab, und diese jungen Haare", "tokens": ["Zie\u00b7rath", "gab", ",", "und", "die\u00b7se", "jun\u00b7gen", "Haa\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf der B\u00e4ume kahlgestandnes Haupt;", "tokens": ["Auf", "der", "B\u00e4u\u00b7me", "kahl\u00b7ge\u0