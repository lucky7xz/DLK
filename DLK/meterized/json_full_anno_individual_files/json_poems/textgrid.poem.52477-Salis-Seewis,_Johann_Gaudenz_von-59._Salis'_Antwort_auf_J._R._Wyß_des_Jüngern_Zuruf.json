{"textgrid.poem.52477": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "59. Salis' Antwort auf J. R. Wy\u00df des J\u00fcngern Zuruf", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Saitenspiel hing l\u00e4ngst an Weidenzweigen,", "tokens": ["Mein", "Sai\u00b7ten\u00b7spiel", "hing", "l\u00e4ngst", "an", "Wei\u00b7den\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mein Gem\u00fct verschlo\u00df, was ich empfand,", "tokens": ["Und", "mein", "Ge\u00b7m\u00fct", "ver\u00b7schlo\u00df", ",", "was", "ich", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als deine Muse, mir im Schwesterreigen", "tokens": ["Als", "dei\u00b7ne", "Mu\u00b7se", ",", "mir", "im", "Schwes\u00b7ter\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die freundlichste, an ferner Aare Strand", "tokens": ["Die", "freund\u00b7lichs\u00b7te", ",", "an", "fer\u00b7ner", "Aa\u00b7re", "Strand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wohlwollend r\u00fcgte mein zu tiefes Schweigen,", "tokens": ["Wohl\u00b7wol\u00b7lend", "r\u00fcg\u00b7te", "mein", "zu", "tie\u00b7fes", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Alpenblumen mir zum Kranze wand;", "tokens": ["Und", "Al\u00b7pen\u00b7blu\u00b7men", "mir", "zum", "Kran\u00b7ze", "wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Dann lockte, wie mit Nachtigallenschl\u00e4gen", "tokens": ["Dann", "lock\u00b7te", ",", "wie", "mit", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zum Abendlied den Landmann zu bewegen.", "tokens": ["Zum", "A\u00b7ben\u00b7dlied", "den", "Land\u00b7mann", "zu", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Sommertag schwand bei Gewitterschw\u00fcle,", "tokens": ["Mein", "Som\u00b7mer\u00b7tag", "schwand", "bei", "Ge\u00b7wit\u00b7ter\u00b7schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein heitres Abendrot ist bald erreicht.", "tokens": ["Sein", "heit\u00b7res", "A\u00b7ben\u00b7drot", "ist", "bald", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gleich Philomela sang ich nur Gef\u00fchle,", "tokens": ["Gleich", "Phi\u00b7lo\u00b7me\u00b7la", "sang", "ich", "nur", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mein Gesang hat schon sein Ziel erreicht;", "tokens": ["Und", "mein", "Ge\u00b7sang", "hat", "schon", "sein", "Ziel", "er\u00b7reicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch sie verstummt schon vor des Herbstes K\u00fchle. \u2013", "tokens": ["Auch", "sie", "ver\u00b7stummt", "schon", "vor", "des", "Herbs\u00b7tes", "K\u00fch\u00b7le", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn fr\u00fch sie jungen S\u00e4ngerch\u00f6ren weicht,", "tokens": ["Wenn", "fr\u00fch", "sie", "jun\u00b7gen", "S\u00e4n\u00b7ger\u00b7ch\u00f6\u00b7ren", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Birgt sie sich gern in stillen Finsternissen,", "tokens": ["Birgt", "sie", "sich", "gern", "in", "stil\u00b7len", "Fins\u00b7ter\u00b7nis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wo Menschen sie nicht kr\u00e4nken, noch vermissen.", "tokens": ["Wo", "Men\u00b7schen", "sie", "nicht", "kr\u00e4n\u00b7ken", ",", "noch", "ver\u00b7mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "PTKNEG", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es schwebet stets, nach alter Dichtung Sagen,", "tokens": ["Es", "schwe\u00b7bet", "stets", ",", "nach", "al\u00b7ter", "Dich\u00b7tung", "Sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um des Vergessens Strom ein Schwanenchor;", "tokens": ["Um", "des", "Ver\u00b7ges\u00b7sens", "Strom", "ein", "Schwa\u00b7nen\u00b7chor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo auf der Flut ein Name sinkt, den tragen", "tokens": ["Wo", "auf", "der", "Flut", "ein", "Na\u00b7me", "sinkt", ",", "den", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie zu des Nachruhms Tempel sanft empor;", "tokens": ["Sie", "zu", "des", "Nach\u00b7ruhms", "Tem\u00b7pel", "sanft", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch m\u00fcssen oft die Retter K\u00e4mpfe wagen,", "tokens": ["Doch", "m\u00fcs\u00b7sen", "oft", "die", "Ret\u00b7ter", "K\u00e4mp\u00b7fe", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Es grinst der Hohn, die Scheelsucht dr\u00e4ngt sich vor,", "tokens": ["Es", "grinst", "der", "Hohn", ",", "die", "Scheel\u00b7sucht", "dr\u00e4ngt", "sich", "vor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bis an des Ruhmes Kranz nur Dornen blieben: \u2013", "tokens": ["Bis", "an", "des", "Ruh\u00b7mes", "Kranz", "nur", "Dor\u00b7nen", "blie\u00b7ben", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ADV", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mein bester Ruhm ist, da\u00df mich Edle lieben.", "tokens": ["Mein", "bes\u00b7ter", "Ruhm", "ist", ",", "da\u00df", "mich", "Ed\u00b7le", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "$,", "KOUS", "PPER", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr edlen S\u00e4nger an der Aare Wogen,", "tokens": ["Ihr", "ed\u00b7len", "S\u00e4n\u00b7ger", "an", "der", "Aa\u00b7re", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr an der Limmat und des Rheines Strand,", "tokens": ["Ihr", "an", "der", "Lim\u00b7mat", "und", "des", "Rhei\u00b7nes", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ergreift die Harfen, spannt den goldnen Bogen!", "tokens": ["Er\u00b7greift", "die", "Har\u00b7fen", ",", "spannt", "den", "gold\u00b7nen", "Bo\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Eintracht schling' um euch ihr Bundesband,", "tokens": ["Die", "Ein\u00b7tracht", "schling'", "um", "euch", "ihr", "Bun\u00b7des\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch milden Sinn stets enger angezogen!", "tokens": ["Durch", "mil\u00b7den", "Sinn", "stets", "en\u00b7ger", "an\u00b7ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Schweizermuse hat ", "tokens": ["Die", "Schwei\u00b7zer\u00b7mu\u00b7se", "hat"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vielleicht, da\u00df beim Erwachen deiner Telle", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "beim", "Er\u00b7wa\u00b7chen", "dei\u00b7ner", "Tel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich einst, o Freund, zum Chore mich geselle! \u2013", "tokens": ["Ich", "einst", ",", "o", "Freund", ",", "zum", "Cho\u00b7re", "mich", "ge\u00b7sel\u00b7le", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "$,", "FM", "NN", "$,", "APPRART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mein Saitenspiel hing l\u00e4ngst an Weidenzweigen,", "tokens": ["Mein", "Sai\u00b7ten\u00b7spiel", "hing", "l\u00e4ngst", "an", "Wei\u00b7den\u00b7zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mein Gem\u00fct verschlo\u00df, was ich empfand,", "tokens": ["Und", "mein", "Ge\u00b7m\u00fct", "ver\u00b7schlo\u00df", ",", "was", "ich", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als deine Muse, mir im Schwesterreigen", "tokens": ["Als", "dei\u00b7ne", "Mu\u00b7se", ",", "mir", "im", "Schwes\u00b7ter\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die freundlichste, an ferner Aare Strand", "tokens": ["Die", "freund\u00b7lichs\u00b7te", ",", "an", "fer\u00b7ner", "Aa\u00b7re", "Strand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wohlwollend r\u00fcgte mein zu tiefes Schweigen,", "tokens": ["Wohl\u00b7wol\u00b7lend", "r