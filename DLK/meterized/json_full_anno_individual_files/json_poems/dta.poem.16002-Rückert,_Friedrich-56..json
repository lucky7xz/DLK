{"dta.poem.16002": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein alter Weiser sprach: Den Mann mag's auch erbauen,", "tokens": ["Ein", "al\u00b7ter", "Wei\u00b7ser", "sprach", ":", "Den", "Mann", "mag's", "auch", "er\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "NN", "NE", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit rechtem Sinne sich im Spiegel zu beschauen.", "tokens": ["Mit", "rech\u00b7tem", "Sin\u00b7ne", "sich", "im", "Spie\u00b7gel", "zu", "be\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sieht er sein Antlitz sch\u00f6n, so denk' er, etwas fehle,", "tokens": ["Sieht", "er", "sein", "Ant\u00b7litz", "sch\u00f6n", ",", "so", "denk'", "er", ",", "et\u00b7was", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nicht ein sch\u00f6ner Geist die sch\u00f6ne Form beseele.", "tokens": ["Wo", "nicht", "ein", "sch\u00f6\u00b7ner", "Geist", "die", "sch\u00f6\u00b7ne", "Form", "be\u00b7see\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wo Unlieblichkeit er sieht in seinen Z\u00fcgen,", "tokens": ["Und", "wo", "Un\u00b7lieb\u00b7lich\u00b7keit", "er", "sieht", "in", "sei\u00b7nen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00fct' er sich, hinzu unholden Sinn zu f\u00fcgen.", "tokens": ["So", "h\u00fct'", "er", "sich", ",", "hin\u00b7zu", "un\u00b7hol\u00b7den", "Sinn", "zu", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PTKVZ", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}