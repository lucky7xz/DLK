{"textgrid.poem.48169": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Traum", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einen Scharlachteppich schritt ich hin.", "tokens": ["Auf", "ei\u00b7nen", "Schar\u00b7lach\u00b7tep\u00b7pich", "schritt", "ich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Klirrendes Gold zerpre\u00dfte meine Glieder \u2013", "tokens": ["Klir\u00b7ren\u00b7des", "Gold", "zer\u00b7pre\u00df\u00b7te", "mei\u00b7ne", "Glie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "in Ketten war ich eine K\u00f6nigin.", "tokens": ["in", "Ket\u00b7ten", "war", "ich", "ei\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vor meinen Blicken schwankte auf und nieder", "tokens": ["Vor", "mei\u00b7nen", "Bli\u00b7cken", "schwank\u00b7te", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "im halben D\u00e4mmerlicht ein schmaler Sarg.", "tokens": ["im", "hal\u00b7ben", "D\u00e4m\u00b7mer\u00b7licht", "ein", "schma\u00b7ler", "Sarg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mit Veilchen rings besteckt und wei\u00dfem Flieder.", "tokens": ["mit", "Veil\u00b7chen", "rings", "be\u00b7steckt", "und", "wei\u00b7\u00dfem", "Flie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Kindersarg, der ein Gestorbnes barg \u2013", "tokens": ["Ein", "Kin\u00b7der\u00b7sarg", ",", "der", "ein", "Ge\u00b7storb\u00b7nes", "barg", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihn trug ein Mann. Und ich, mit wehen F\u00fc\u00dfen,", "tokens": ["Ihn", "trug", "ein", "Mann", ".", "Und", "ich", ",", "mit", "we\u00b7hen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "KON", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mit brennenden Augen folgte tr\u00e4nenkarg.", "tokens": ["mit", "bren\u00b7nen\u00b7den", "Au\u00b7gen", "folg\u00b7te", "tr\u00e4\u00b7nen\u00b7karg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Aus fahlen Wolken flo\u00df ein flimmernd Gr\u00fc\u00dfen,", "tokens": ["Aus", "fah\u00b7len", "Wol\u00b7ken", "flo\u00df", "ein", "flim\u00b7mernd", "Gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "als nun am Tor der stille Tr\u00e4ger stand. \u2013", "tokens": ["als", "nun", "am", "Tor", "der", "stil\u00b7le", "Tr\u00e4\u00b7ger", "stand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er wandte sich und hob mit einem s\u00fc\u00dfen", "tokens": ["Er", "wand\u00b7te", "sich", "und", "hob", "mit", "ei\u00b7nem", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "und klaren L\u00e4cheln seine Last zum Sand", "tokens": ["und", "kla\u00b7ren", "L\u00e4\u00b7cheln", "sei\u00b7ne", "Last", "zum", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hinab. Und hier am Eingang seiner Reiche \u2013", "tokens": ["hin\u00b7ab", ".", "Und", "hier", "am", "Ein\u00b7gang", "sei\u00b7ner", "Rei\u00b7che", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "KON", "ADV", "APPRART", "NN", "PPOSAT", "NE", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "bot er zum Willkomm mir die wei\u00dfe Hand.", "tokens": ["bot", "er", "zum", "Will\u00b7komm", "mir", "die", "wei\u00b7\u00dfe", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Laut schrie ich auf: \u00bbDein L\u00e4cheln l\u00fcgt, das bleiche,", "tokens": ["Laut", "schrie", "ich", "auf", ":", "\u00bb", "Dein", "L\u00e4\u00b7cheln", "l\u00fcgt", ",", "das", "blei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dein Schweigen l\u00fcgt, \u2013 und all dies klirrende Gold,", "tokens": ["dein", "Schwei\u00b7gen", "l\u00fcgt", ",", "\u2013", "und", "all", "dies", "klir\u00b7ren\u00b7de", "Gold", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "$(", "KON", "PIAT", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "der K\u00f6nigspurpur l\u00fcgt an dieser Leiche!", "tokens": ["der", "K\u00f6\u00b7nigs\u00b7pur\u00b7pur", "l\u00fcgt", "an", "die\u00b7ser", "Lei\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Erl\u00f6sung nur aus meiner Schmerzen Sold \u2013", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "nur", "aus", "mei\u00b7ner", "Schmer\u00b7zen", "Sold", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erl\u00f6sung will ich: Tr\u00e4nen! gib mir Tr\u00e4nen \u2013\u00ab", "tokens": ["Er\u00b7l\u00f6\u00b7sung", "will", "ich", ":", "Tr\u00e4\u00b7nen", "!", "gib", "mir", "Tr\u00e4\u00b7nen", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VMFIN", "PPER", "$.", "NN", "$.", "VVIMP", "PPER", "NN", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und er: \u00bbDir sei gew\u00e4hrt, was du gewollt!\u00ab", "tokens": ["und", "er", ":", "\u00bb", "Dir", "sei", "ge\u00b7w\u00e4hrt", ",", "was", "du", "ge\u00b7wollt", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "$.", "$(", "PPER", "VAFIN", "VVPP", "$,", "PWS", "PPER", "VMPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Da quoll's mir hoch, wie ein erstickend Sehnen,", "tokens": ["Da", "quoll's", "mir", "hoch", ",", "wie", "ein", "er\u00b7sti\u00b7ckend", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ART", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ich sp\u00fcrt im Hirn des Herzens wildes Klopfen", "tokens": ["ich", "sp\u00fcrt", "im", "Hirn", "des", "Her\u00b7zens", "wil\u00b7des", "Klop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u2013 und aus den Augen st\u00fcrzten meine Tr\u00e4nen", "tokens": ["\u2013", "und", "aus", "den", "Au\u00b7gen", "st\u00fcrz\u00b7ten", "mei\u00b7ne", "Tr\u00e4\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "erl\u00f6send, hei\u00dfe, rote, blutige Tropfen!", "to