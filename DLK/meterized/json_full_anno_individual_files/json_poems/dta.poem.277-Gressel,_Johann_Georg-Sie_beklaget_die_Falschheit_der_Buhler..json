{"dta.poem.277": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Sie beklaget die Falschheit der Buhler.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Jris, die betr\u00fcbte Sch\u00f6ne/", "tokens": ["Jris", ",", "die", "be\u00b7tr\u00fcb\u00b7te", "Sch\u00f6\u00b7ne", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach voller Angst mit weinenden Gesicht;", "tokens": ["Sprach", "vol\u00b7ler", "Angst", "mit", "wei\u00b7nen\u00b7den", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach geliebte ", "tokens": ["Ach", "ge\u00b7lieb\u00b7te"], "token_info": ["word", "word"], "pos": ["ITJ", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Trau nimmermehr der Buhler Schmeicheln nicht/", "tokens": ["Trau", "nim\u00b7mer\u00b7mehr", "der", "Buh\u00b7ler", "Schmei\u00b7cheln", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denn ihre Gunst ist nur auf Trug gericht/", "tokens": ["Denn", "ih\u00b7re", "Gunst", "ist", "nur", "auf", "Trug", "ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wann sie schon betrieglich sagen/", "tokens": ["Wann", "sie", "schon", "be\u00b7trieg\u00b7lich", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich liebe euch so viel ich immer kan/", "tokens": ["Ich", "lie\u00b7be", "euch", "so", "viel", "ich", "im\u00b7mer", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PPER", "ADV", "VMFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Must du \u00fcber sie doch klagen/", "tokens": ["Must", "du", "\u00fc\u00b7ber", "sie", "doch", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "ADV", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn schauen sie nur eine ander an/", "tokens": ["Denn", "schau\u00b7en", "sie", "nur", "ei\u00b7ne", "an\u00b7der", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wird gleich ihr Sinn durch Wechseln kund gethan.", "tokens": ["Wird", "gleich", "ihr", "Sinn", "durch", "Wech\u00b7seln", "kund", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "APPR", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}