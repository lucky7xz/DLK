{"dta.poem.12022": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vorm Menschen, welchen kein Gesetz der Lieb' und Treue", "tokens": ["Vorm", "Men\u00b7schen", ",", "wel\u00b7chen", "kein", "Ge\u00b7setz", "der", "Lieb'", "und", "Treu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAT", "PIAT", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beherrschet, habe mehr als vor dem Thiere Scheue!", "tokens": ["Be\u00b7herr\u00b7schet", ",", "ha\u00b7be", "mehr", "als", "vor", "dem", "Thie\u00b7re", "Scheu\u00b7e", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "PIAT", "KOKOM", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn auch dem Thiere fehlt Gem\u00fct, Vernunft und Liebe,", "tokens": ["Wenn", "auch", "dem", "Thie\u00b7re", "fehlt", "Ge\u00b7m\u00fct", ",", "Ver\u00b7nunft", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gehalten ist es doch vom Bande seiner Triebe.", "tokens": ["Ge\u00b7hal\u00b7ten", "ist", "es", "doch", "vom", "Ban\u00b7de", "sei\u00b7ner", "Trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "An diesem halt es fest, du darfst dich drauf verlassen;", "tokens": ["An", "die\u00b7sem", "halt", "es", "fest", ",", "du", "darfst", "dich", "drauf", "ver\u00b7las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VMFIN", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Menschen aber kannst du nirgend sicher fassen.", "tokens": ["Den", "Men\u00b7schen", "a\u00b7ber", "kannst", "du", "nir\u00b7gend", "si\u00b7cher", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Liebe Widerschein kannst du ins Thier meintwegen,", "tokens": ["Der", "Lie\u00b7be", "Wi\u00b7der\u00b7schein", "kannst", "du", "ins", "Thier", "meint\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch lieber in die fromm unschuld'ge Pflanze legen.", "tokens": ["Noch", "lie\u00b7ber", "in", "die", "fromm", "un\u00b7schuld'\u00b7ge", "Pflan\u00b7ze", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch in den Menschen, wo sie selber sollte seyn,", "tokens": ["Doch", "in", "den", "Men\u00b7schen", ",", "wo", "sie", "sel\u00b7ber", "soll\u00b7te", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kannst du, wo sie nicht ist, sie auch nicht legen ein.", "tokens": ["Kannst", "du", ",", "wo", "sie", "nicht", "ist", ",", "sie", "auch", "nicht", "le\u00b7gen", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PPER", "PTKNEG", "VAFIN", "$,", "PPER", "ADV", "PTKNEG", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}