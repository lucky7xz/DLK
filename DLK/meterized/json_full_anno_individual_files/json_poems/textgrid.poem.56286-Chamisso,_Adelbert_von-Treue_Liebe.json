{"textgrid.poem.56286": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Treue Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schallten muntre Lieder", "tokens": ["Es", "schall\u00b7ten", "mun\u00b7tre", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hell durch den Fichtenwald,", "tokens": ["Hell", "durch", "den", "Fich\u00b7ten\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es kam ein muntrer Reiter", "tokens": ["Es", "kam", "ein", "mun\u00b7trer", "Rei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum F\u00f6rsterhause bald.", "tokens": ["Zum", "F\u00f6rs\u00b7ter\u00b7hau\u00b7se", "bald", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Frau Muhme, guten Morgen,", "tokens": ["Frau", "Muh\u00b7me", ",", "gu\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo bleibt die Liebste mein? \u2013", "tokens": ["Wo", "bleibt", "die", "Liebs\u00b7te", "mein", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie lieget, krank zum Sterben,", "tokens": ["Sie", "lie\u00b7get", ",", "krank", "zum", "Ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im obern K\u00e4mmerlein.", "tokens": ["Im", "o\u00b7bern", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er stieg in bittern Tr\u00e4nen", "tokens": ["Er", "stieg", "in", "bit\u00b7tern", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Treppe wohl hinauf,", "tokens": ["Die", "Trep\u00b7pe", "wohl", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hemmte, vor der T\u00fcre", "tokens": ["Er", "hemm\u00b7te", ",", "vor", "der", "T\u00fc\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebsten, ihren Lauf.", "tokens": ["Der", "Liebs\u00b7ten", ",", "ih\u00b7ren", "Lauf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herein, herein, Geliebter,", "tokens": ["Her\u00b7ein", ",", "her\u00b7ein", ",", "Ge\u00b7lieb\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKVZ", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu schmerzlichem Besuch!", "tokens": ["Zu", "schmerz\u00b7li\u00b7chem", "Be\u00b7such", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die heim du holen wolltest,", "tokens": ["Die", "heim", "du", "ho\u00b7len", "woll\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKVZ", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Deckt bald das Leichentuch.", "tokens": ["Deckt", "bald", "das", "Lei\u00b7chen\u00b7tuch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie schl\u00e4ft in engem Sarge,", "tokens": ["Sie", "schl\u00e4ft", "in", "en\u00b7gem", "Sar\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Drauf liegt der Myrtenkranz;", "tokens": ["Drauf", "liegt", "der", "Myr\u00b7ten\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst nicht heim sie f\u00fchren,", "tokens": ["Du", "wirst", "nicht", "heim", "sie", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PTKVZ", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht bei Gesang und Tanz.", "tokens": ["Nicht", "bei", "Ge\u00b7sang", "und", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie werden fort mich tragen,", "tokens": ["Sie", "wer\u00b7den", "fort", "mich", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKVZ", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und tief mich scharren ein,", "tokens": ["Und", "tief", "mich", "schar\u00b7ren", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du wirst mir Tr\u00e4nen weinen,", "tokens": ["Du", "wirst", "mir", "Tr\u00e4\u00b7nen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eine andre frein. \u2013", "tokens": ["Und", "ei\u00b7ne", "and\u00b7re", "frein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die du mich nie betr\u00fcbet,", "tokens": ["Die", "du", "mich", "nie", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du meine Zier und Lust,", "tokens": ["Du", "mei\u00b7ne", "Zier", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie hast du jetzt geschnitten", "tokens": ["Wie", "hast", "du", "jetzt", "ge\u00b7schnit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir scharf in meine Brust!", "tokens": ["Mir", "scharf", "in", "mei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Drauf sahen zu einander", "tokens": ["Drauf", "sa\u00b7hen", "zu", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "PRF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die beiden ernst und mild,", "tokens": ["Die", "bei\u00b7den", "ernst", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verschlungen ihre H\u00e4nde,", "tokens": ["Ver\u00b7schlun\u00b7gen", "ih\u00b7re", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein sch\u00f6nes, bleiches Bild.", "tokens": ["Ein", "sch\u00f6\u00b7nes", ",", "blei\u00b7ches", "Bild", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da schied sie sanft hin\u00fcber,", "tokens": ["Da", "schied", "sie", "sanft", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er aber zog zur Stund", "tokens": ["Er", "a\u00b7ber", "zog", "zur", "Stund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Ringlein sich vom Finger", "tokens": ["Das", "Rin\u00b7glein", "sich", "vom", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und steckt's in ihren Mund.", "tokens": ["Und", "steckt's", "in", "ih\u00b7ren", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ob er geweinet habe,", "tokens": ["Ob", "er", "ge\u00b7wei\u00b7net", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als solches ist geschehn? \u2013", "tokens": ["Als", "sol\u00b7ches", "ist", "ge\u00b7schehn", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich selber flo\u00df in Tr\u00e4nen,", "tokens": ["Ich", "sel\u00b7ber", "flo\u00df", "in", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich hab es nicht gesehn.", "tokens": ["Ich", "hab", "es", "nicht", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es gr\u00e4bt der Totengr\u00e4ber", "tokens": ["Es", "gr\u00e4bt", "der", "To\u00b7ten\u00b7gr\u00e4\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Grab, und noch ein Grab:", "tokens": ["Ein", "Grab", ",", "und", "noch", "ein", "Grab", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er kommt an ihre Seite,", "tokens": ["Er", "kommt", "an", "ih\u00b7re", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der ihr das Ringlein gab.", "tokens": ["Der", "ihr", "das", "Rin\u00b7glein", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Es schallten muntre Lieder", "tokens": ["Es", "schall\u00b7ten", "mun\u00b7tre", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hell durch den Fichtenwald,", "tokens": ["Hell", "durch", "den", "Fich\u00b7ten\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es kam ein muntrer Reiter", "tokens": ["Es", "kam", "ein", "mun\u00b7trer", "Rei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum F\u00f6rsterhause bald.", "tokens": ["Zum", "F\u00f6rs\u00b7ter\u00b7hau\u00b7se", "bald", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Frau Muhme, guten Morgen,", "tokens": ["Frau", "Muh\u00b7me", ",", "gu\u00b7ten", "Mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-