{"dta.poem.13729": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An  \u2014", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie nach festen Felsenw\u00e4nden", "tokens": ["Wie", "nach", "fes\u00b7ten", "Fel\u00b7sen\u00b7w\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df ich in der Einsamkeit", "tokens": ["Mu\u00df", "ich", "in", "der", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stets auf Dich die Blicke wenden.", "tokens": ["Stets", "auf", "Dich", "die", "Bli\u00b7cke", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle, die in guter Zeit", "tokens": ["Al\u00b7le", ",", "die", "in", "gu\u00b7ter", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bei mir waren, sah ich scheiden", "tokens": ["Bei", "mir", "wa\u00b7ren", ",", "sah", "ich", "schei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit des falschen Gl\u00fcckes Schaum,", "tokens": ["Mit", "des", "fal\u00b7schen", "Gl\u00fc\u00b7ckes", "Schaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Du bliebst schweigend mir im Leiden,", "tokens": ["Du", "bliebst", "schwei\u00b7gend", "mir", "im", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPRART", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Wie ein treuer Tannenbaum,", "tokens": ["Wie", "ein", "treu\u00b7er", "Tan\u00b7nen\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ob die Felder lustig bl\u00fch'n,", "tokens": ["Ob", "die", "Fel\u00b7der", "lus\u00b7tig", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ob der Winter zieht heran,", "tokens": ["Ob", "der", "Win\u00b7ter", "zieht", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Immer finster, immer gr\u00fcn \u2014", "tokens": ["Im\u00b7mer", "fins\u00b7ter", ",", "im\u00b7mer", "gr\u00fcn"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Reich' die Hand mir, wackrer Mann!", "tokens": ["Reich'", "die", "Hand", "mir", ",", "wack\u00b7rer", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}