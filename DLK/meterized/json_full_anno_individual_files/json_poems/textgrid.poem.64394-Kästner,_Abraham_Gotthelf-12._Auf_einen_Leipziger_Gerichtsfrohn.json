{"textgrid.poem.64394": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "12. Auf einen Leipziger Gerichtsfrohn", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bem\u00fcht des Feuers Glut zu d\u00e4mpfen,", "tokens": ["Be\u00b7m\u00fcht", "des", "Feu\u00b7ers", "Glut", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "So wie ein Held, dem im erhitzten K\u00e4mpfen", "tokens": ["So", "wie", "ein", "Held", ",", "dem", "im", "er\u00b7hitz\u00b7ten", "K\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein zischend Bley den Siegerlauf verk\u00fcrzt.", "tokens": ["Ein", "zi\u00b7schend", "Bley", "den", "Sie\u00b7ger\u00b7lauf", "ver\u00b7k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Gleichni\u00df scheint vielleicht euch allzuviel gewagt.", "tokens": ["Das", "Gleich\u00b7ni\u00df", "scheint", "viel\u00b7leicht", "euch", "all\u00b7zu\u00b7viel", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Stir