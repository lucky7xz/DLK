{"textgrid.poem.28095": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gutes Wasser ist, das von der Bu\u00df entspringet;", "tokens": ["Ein", "gu\u00b7tes", "Was\u00b7ser", "ist", ",", "das", "von", "der", "Bu\u00df", "ent\u00b7sprin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein gutes Wasser ist, das Zucht vom Knaben bringet;", "tokens": ["Ein", "gu\u00b7tes", "Was\u00b7ser", "ist", ",", "das", "Zucht", "vom", "Kna\u00b7ben", "brin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein gutes Wasser ist, das unsre Kost bestellt;", "tokens": ["Ein", "gu\u00b7tes", "Was\u00b7ser", "ist", ",", "das", "uns\u00b7re", "Kost", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein gutes Wasser ist, das Aertzten bringet Geld.", "tokens": ["Ein", "gu\u00b7tes", "Was\u00b7ser", "ist", ",", "das", "A\u00b7ertz\u00b7ten", "brin\u00b7get", "Geld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Ein b\u00f6ses Wasser ist, da Menschen drinn ersauffen;", "tokens": ["Ein", "b\u00f6\u00b7ses", "Was\u00b7ser", "ist", ",", "da", "Men\u00b7schen", "drinn", "er\u00b7sauf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "KOUS", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein b\u00f6ses Wasser ist, das so, wie Wein, zu kauffen;", "tokens": ["Ein", "b\u00f6\u00b7ses", "Was\u00b7ser", "ist", ",", "das", "so", ",", "wie", "Wein", ",", "zu", "kauf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "ADV", "$,", "PWAV", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein b\u00f6ses Wasser ist, das au\u00df der Noth man trinckt;", "tokens": ["Ein", "b\u00f6\u00b7ses", "Was\u00b7ser", "ist", ",", "das", "au\u00df", "der", "Noth", "man", "trinckt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein b\u00f6ses Wasser ist, das Grimm au\u00df Augen zwingt.", "tokens": ["Ein", "b\u00f6\u00b7ses", "Was\u00b7ser", "ist", ",", "das", "Grimm", "au\u00df", "Au\u00b7gen", "zwingt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2