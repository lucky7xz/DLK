{"textgrid.poem.56168": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Giebt es wirklich die Zeit, die zerst\u00f6rende?", "tokens": ["Giebt", "es", "wirk\u00b7lich", "die", "Zeit", ",", "die", "zer\u00b7st\u00f6\u00b7ren\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wann, auf dem ruhenden Berg, zerbricht sie die Burg?", "tokens": ["Wann", ",", "auf", "dem", "ru\u00b7hen\u00b7den", "Berg", ",", "zer\u00b7bricht", "sie", "die", "Burg", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dieses Herz, das unendlich den G\u00f6ttern geh\u00f6rende,", "tokens": ["Die\u00b7ses", "Herz", ",", "das", "un\u00b7end\u00b7lich", "den", "G\u00f6t\u00b7tern", "ge\u00b7h\u00f6\u00b7ren\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "ADJA", "$,"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "wann vergewaltigts der Demiurg?", "tokens": ["wann", "ver\u00b7ge\u00b7wal\u00b7tigts", "der", "De\u00b7miurg", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Sind wir wirklich so \u00e4ngstlich Zerbrechliche,", "