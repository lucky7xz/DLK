{"textgrid.poem.40492": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Begr\u00fc\u00dfung des Meeres", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unerme\u00dflich und unendlich,", "tokens": ["Un\u00b7er\u00b7me\u00df\u00b7lich", "und", "un\u00b7end\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00e4nzend, ruhig, ahnungschwer,", "tokens": ["Gl\u00e4n\u00b7zend", ",", "ru\u00b7hig", ",", "ah\u00b7nung\u00b7schwer", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegst du vor mir ausgebreitet,", "tokens": ["Liegst", "du", "vor", "mir", "aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Altes, heil'ges, ew'ges Meer!", "tokens": ["Al\u00b7tes", ",", "heil'\u00b7ges", ",", "ew'\u00b7ges", "Meer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Soll ich dich mit Thr\u00e4nen gr\u00fc\u00dfen,", "tokens": ["Soll", "ich", "dich", "mit", "Thr\u00e4\u00b7nen", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Wehmuth sie vergie\u00dft,", "tokens": ["Wie", "die", "Weh\u00b7muth", "sie", "ver\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie trauernd auf dem Friedhof", "tokens": ["Wenn", "sie", "trau\u00b7ernd", "auf", "dem", "Fried\u00b7hof"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manch ein theures Grab begr\u00fc\u00dft?", "tokens": ["Manch", "ein", "theu\u00b7res", "Grab", "be\u00b7gr\u00fc\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn ein gro\u00dfer, stiller Friedhof,", "tokens": ["Denn", "ein", "gro\u00b7\u00dfer", ",", "stil\u00b7ler", "Fried\u00b7hof", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine weite Gruft bist du,", "tokens": ["Ei\u00b7ne", "wei\u00b7te", "Gruft", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manches Leben, manche Hoffnung", "tokens": ["Man\u00b7ches", "Le\u00b7ben", ",", "man\u00b7che", "Hoff\u00b7nung"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deckst du kalt und f\u00fchllos zu;", "tokens": ["Deckst", "du", "kalt", "und", "f\u00fchl\u00b7los", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Keinen Grabstein wahrst du ihnen,", "tokens": ["Kei\u00b7nen", "Grab\u00b7stein", "wahrst", "du", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht ein Kreuzlein, schlicht und schmal,", "tokens": ["Nicht", "ein", "Kreuz\u00b7lein", ",", "schlicht", "und", "schmal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur am Strande wandelt weinend", "tokens": ["Nur", "am", "Stran\u00b7de", "wan\u00b7delt", "wei\u00b7nend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manch ein lebend Trauermal.", "tokens": ["Manch", "ein", "le\u00b7bend", "Trau\u00b7er\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Soll ich dich mit Jubel gr\u00fc\u00dfen,", "tokens": ["Soll", "ich", "dich", "mit", "Ju\u00b7bel", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jubel, wie ihn Freude zollt,", "tokens": ["Ju\u00b7bel", ",", "wie", "ihn", "Freu\u00b7de", "zollt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ein weiter, reicher Garten", "tokens": ["Wenn", "ein", "wei\u00b7ter", ",", "rei\u00b7cher", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADV", "$,", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihrem Blick sich aufgerollt?", "tokens": ["Ih\u00b7rem", "Blick", "sich", "auf\u00b7ge\u00b7rollt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Denn ein unerme\u00df'ner Garten,", "tokens": ["Denn", "ein", "un\u00b7er\u00b7me\u00df'\u00b7ner", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine reiche Flur bist du,", "tokens": ["Ei\u00b7ne", "rei\u00b7che", "Flur", "bist", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Edle Keime deckt und Sch\u00e4tze", "tokens": ["Ed\u00b7le", "Kei\u00b7me", "deckt", "und", "Sch\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein kristallner Busen zu.", "tokens": ["Dein", "kris\u00b7tall\u00b7ner", "Bu\u00b7sen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Wie des Gartens \u00fcpp'ge Wiesen", "tokens": ["Wie", "des", "Gar\u00b7tens", "\u00fcpp'\u00b7ge", "Wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist dein Plan auch glatt und gr\u00fcn,", "tokens": ["Ist", "dein", "Plan", "auch", "glatt", "und", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Perlen und Korallenhaine", "tokens": ["Per\u00b7len", "und", "Ko\u00b7ral\u00b7len\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die Blumen, die dir bl\u00fchn.", "tokens": ["Sind", "die", "Blu\u00b7men", ",", "die", "dir", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie im Garten stille Wandler", "tokens": ["Wie", "im", "Gar\u00b7ten", "stil\u00b7le", "Wand\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehn die Schiffe durch das Meer,", "tokens": ["Ziehn", "die", "Schif\u00b7fe", "durch", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00e4tze fordernd, Sch\u00e4tze bringend,", "tokens": ["Sch\u00e4t\u00b7ze", "for\u00b7dernd", ",", "Sch\u00e4t\u00b7ze", "brin\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00dfend, hoffend, hin und her. \u2013", "tokens": ["Gr\u00fc\u00b7\u00dfend", ",", "hof\u00b7fend", ",", "hin", "und", "her", ".", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "ADV", "KON", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sollen Thr\u00e4nen, soll mein Jubel", "tokens": ["Sol\u00b7len", "Thr\u00e4\u00b7nen", ",", "soll", "mein", "Ju\u00b7bel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich begr\u00fc\u00dfen, Ozean?", "tokens": ["Dich", "be\u00b7gr\u00fc\u00b7\u00dfen", ",", "O\u00b7ze\u00b7an", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht'ger Zweifel, eitle Frage,", "tokens": ["Nicht'\u00b7ger", "Zwei\u00b7fel", ",", "eit\u00b7le", "Fra\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da ich doch nicht w\u00e4hlen kann!", "tokens": ["Da", "ich", "doch", "nicht", "w\u00e4h\u00b7len", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Da doch auch der h\u00f6chste Jubel", "tokens": ["Da", "doch", "auch", "der", "h\u00f6chs\u00b7te", "Ju\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Mir vom Aug' als Thr\u00e4ne rollt,", "tokens": ["Mir", "vom", "Aug'", "als", "Thr\u00e4\u00b7ne", "rollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "KOUS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So wie Abendschein und Fr\u00fchroth", "tokens": ["So", "wie", "A\u00b7bend\u00b7schein", "und", "Fr\u00fch\u00b7roth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets nur Thau den B\u00e4umen zollt.", "tokens": ["Stets", "nur", "Thau", "den", "B\u00e4u\u00b7men", "zollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Zu dem Herrn empor mit Thr\u00e4nen", "tokens": ["Zu", "dem", "Herrn", "em\u00b7por", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKVZ", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War mein Aug' im Dom gewandt;", "tokens": ["War", "mein", "Aug'", "im", "Dom", "ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Thr\u00e4nen gr\u00fc\u00dft' ich wieder", "tokens": ["Und", "mit", "Thr\u00e4\u00b7nen", "gr\u00fc\u00dft'", "ich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "J\u00fcngst mein sch\u00f6nes Vaterland;", "tokens": ["J\u00fcngst", "mein", "sch\u00f6\u00b7nes", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Weinend \u00f6ffnet' ich die Arme,", "tokens": ["Wei\u00b7nend", "\u00f6ff\u00b7net'", "ich", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ich der Geliebten nah;", "tokens": ["Als", "ich", "der", "Ge\u00b7lieb\u00b7ten", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weinend kniet' ich auf den H\u00f6hen,", "tokens": ["Wei\u00b7nend", "kniet'", "ich", "auf", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ich dich zuerst ersah.", "tokens": ["Wo", "ich", "dich", "zu\u00b7erst", "er\u00b7sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Unerme\u00dflich und unendlich,", "tokens": ["Un\u00b7er\u00b7me\u00df\u00b7lich", "und", "un\u00b7end\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00e4nzend, ruhig, ahnungschwer,", "tokens": ["Gl\u00e4n\u00b7zend", ",", "ru\u00b7hig", ",", "ah\u00b7nung\u00b7schwer", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegst du vor mir ausgebreitet,", "tokens": ["Liegst", "du", "vor", "mir", "aus\u00b7ge\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Altes, heil'ges, ew'ges Meer!", "tokens": ["Al\u00b7tes", ",", "heil'\u00b7ges", ",", "ew'\u00b7ges", "Meer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Soll ich dich mit Thr\u00e4nen gr\u00fc\u00dfen,", "tokens": ["Soll", "ich", "dich", "mit", "Thr\u00e4\u00b7nen", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die Wehmuth sie vergie\u00dft,", "tokens": ["Wie", "die", "Weh\u00b7muth", "sie", "ver\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sie trauernd auf dem Friedhof", "tokens": ["Wenn", "sie", "trau\u00b7ernd", "auf", "dem", "Fried\u00b7hof"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manch ein theures Grab begr\u00fc\u00dft?", "tokens": ["Manch", "ein", "theu\u00b7res", "Grab", "be\u00b7gr\u00fc\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Denn ein gro\u00dfer, stiller Friedhof,", "tokens": ["Denn", "ein", "gro\u00b7\u00dfer", ",", "stil\u00b7ler", "Fried\u00b7hof", ","], "token_info": ["word", "word", "word", "punct", 