, "line.4": {"text": "Als du jener Zeit gethan.", "tokens": ["Als", "du", "je\u00b7ner", "Zeit", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mein Verlangen war dein Scherz,", "tokens": ["Mein", "Ver\u00b7lan\u00b7gen", "war", "dein", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mein Vergn\u00fcgen ist dein Schmerz;", "tokens": ["Mein", "Ver\u00b7gn\u00fc\u00b7gen", "ist", "dein", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Deiner Thr\u00e4nen Fluth", "tokens": ["Dei\u00b7ner", "Thr\u00e4\u00b7nen", "Fluth"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "L\u00f6scht die erste Glut", "tokens": ["L\u00f6scht", "die", "ers\u00b7te", "Glut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Und erquickt mein lechzend Herz.", "tokens": ["Und", "er\u00b7quickt", "mein", "lech\u00b7zend", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hastu doch dein Theil erwehlet,", "tokens": ["Has\u00b7tu", "doch", "dein", "Theil", "er\u00b7weh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "