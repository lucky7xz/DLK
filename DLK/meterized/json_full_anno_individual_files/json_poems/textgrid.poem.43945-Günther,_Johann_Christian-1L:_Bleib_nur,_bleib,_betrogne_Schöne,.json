"Zei\u00b7get", "mei\u00b7ner", "W\u00fcnt\u00b7sche", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wirstu jezt verlacht", "tokens": ["Wirs\u00b7tu", "jezt", "ver\u00b7lacht"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Und in Angst gebracht,", "tokens": ["Und", "in", "Angst", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Dencke, wie es mir gefiel.", "tokens": ["Den\u00b7cke", ",", "wie", "es", "mir", "ge\u00b7fiel", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Spare nur die sp\u00e4ten Thr\u00e4nen,", "tokens": ["Spa\u00b7re", "nur", "die", "sp\u00e4\u00b7ten", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leide, bitte, schw\u00f6re, geh und fleuch;", "tokens": ["Lei\u00b7de", ",", "bit\u00b7te", ",", "schw\u00f6\u00b7re", ",", "geh", "und", "fleuch", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "VVFIN", "$,", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Deiner Wehmuth na\u00dfes Sehnen", "tokens": ["Dei\u00b7ner", "Weh\u00b7muth", "na\u00b7\u00dfes", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht mein Herze nicht mehr weich.", "tokens": ["Macht", "mein", "Her\u00b7ze", "nicht", "mehr", "weich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ich dir nur wohl gethan,", "tokens": ["Was", "ich", "dir", "nur", "wohl", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schreib ich mir zum Fehler an;", "tokens": ["Schreib", "ich", "mir", "zum", "Feh\u00b7ler", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zeigt doch schon das Weh", "tokens": ["Zeigt", "doch", "schon", "das", "Weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Deiner tollen Eh,", "tokens": ["Dei\u00b7ner", "tol\u00b7len", "Eh", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Was versto\u00dfne Liebe kan.", "tokens": ["Was", "ver\u00b7sto\u00df\u00b7ne", "Lie\u00b7be", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}