{"dta.poem.14090": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Freiwerber .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So wie du bist, verschlafen hei\u00df,", "tokens": ["So", "wie", "du", "bist", ",", "ver\u00b7schla\u00b7fen", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VAFIN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df allen Putz und Zier zu Haus,", "tokens": ["La\u00df", "al\u00b7len", "Putz", "und", "Zier", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Tritt nur herf\u00fcr im Hemdlein wei\u00df,", "tokens": ["Tritt", "nur", "her\u00b7f\u00fcr", "im", "Hemd\u00b7lein", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Siehst so gar sch\u00f6n verliebet aus.", "tokens": ["Siehst", "so", "gar", "sch\u00f6n", "ver\u00b7lie\u00b7bet", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}