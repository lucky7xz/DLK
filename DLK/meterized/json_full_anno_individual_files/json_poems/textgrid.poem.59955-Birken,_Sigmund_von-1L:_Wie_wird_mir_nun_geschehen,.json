{"textgrid.poem.59955": {"metadata": {"author": {"name": "Birken, Sigmund von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie wird mir nun geschehen,", "genre": "verse", "period": "N.A.", "pub_year": 1653, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie wird mir nun geschehen,", "tokens": ["Wie", "wird", "mir", "nun", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ich dich lassen mu\u00df,", "tokens": ["Da", "ich", "dich", "las\u00b7sen", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dich nicht mehr soll sehen?", "tokens": ["Und", "dich", "nicht", "mehr", "soll", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach, bittrer Scheidegru\u00df!", "tokens": ["Ach", ",", "bit\u00b7trer", "Schei\u00b7de\u00b7gru\u00df", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du rei\u00dfest dich von mir,", "tokens": ["Du", "rei\u00b7\u00dfest", "dich", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "O Hirtin, meine Zier!", "tokens": ["O", "Hir\u00b7tin", ",", "mei\u00b7ne", "Zier", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich m\u00f6cht' in Angst vergehen \u2013", "tokens": ["Ich", "m\u00f6cht'", "in", "Angst", "ver\u00b7ge\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Tod mich trennt von dir!", "tokens": ["Der", "Tod", "mich", "trennt", "von", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Gr\u00e4slein, helft mir klagen,", "tokens": ["Ihr", "Gr\u00e4s\u00b7lein", ",", "helft", "mir", "kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr W\u00e4slein allzumal,", "tokens": ["Ihr", "W\u00e4s\u00b7lein", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ihr sie oft getragen", "tokens": ["Die", "ihr", "sie", "oft", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier in dem Pegnitzthal.", "tokens": ["Hier", "in", "dem", "Peg\u00b7nitz\u00b7thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Ihr seht mich jetzt allein", "tokens": ["Ihr", "seht", "mich", "jetzt", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und traurig treten ein.", "tokens": ["Und", "trau\u00b7rig", "tre\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mich d\u00fcnkt, ich h\u00f6r' euch fragen:", "tokens": ["Mich", "d\u00fcnkt", ",", "ich", "h\u00f6r'", "euch", "fra\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo mag die Seine sein?", "tokens": ["Wo", "mag", "die", "Sei\u00b7ne", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie wird mir doch geschehen,", "tokens": ["Wie", "wird", "mir", "doch", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn ich sie nimmer werd',", "tokens": ["Wenn", "ich", "sie", "nim\u00b7mer", "werd'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ich sie sahe, sehen", "tokens": ["Wo", "ich", "sie", "sa\u00b7he", ",", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spaziren um die Heerd'?", "tokens": ["Spa\u00b7zi\u00b7ren", "um", "die", "He\u00b7erd'", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Wenn sie nicht mehr im Gras", "tokens": ["Wenn", "sie", "nicht", "mehr", "im", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wird sitzen, wo sie sa\u00df?", "tokens": ["Wird", "sit\u00b7zen", ",", "wo", "sie", "sa\u00df", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vor Leid werd' ich vergehen,", "tokens": ["Vor", "Leid", "werd'", "ich", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von Weinen werden na\u00df.", "tokens": ["Von", "Wei\u00b7nen", "wer\u00b7den", "na\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Bach, von meinen Z\u00e4hren", "tokens": ["Der", "Bach", ",", "von", "mei\u00b7nen", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vermehrt und tr\u00fcb' gemacht,", "tokens": ["Ver\u00b7mehrt", "und", "tr\u00fcb'", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird auch mein Weinen mehren,", "tokens": ["Wird", "auch", "mein", "Wei\u00b7nen", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was ich vorgebracht,", "tokens": ["Und", "was", "ich", "vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den bittern Scheidegru\u00df,", "tokens": ["Den", "bit\u00b7tern", "Schei\u00b7de\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wird er in schnellem Schu\u00df", "tokens": ["Wird", "er", "in", "schnel\u00b7lem", "Schu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nachlispelnd lassen h\u00f6ren", "tokens": ["Nach\u00b7lis\u00b7pelnd", "las\u00b7sen", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und tragen in den Flu\u00df.", "tokens": ["Und", "tra\u00b7gen", "in", "den", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr, die ihr sonst gesprungen,", "tokens": ["Ihr", ",", "die", "ihr", "sonst", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn meine Margaris", "tokens": ["Wenn", "mei\u00b7ne", "Mar\u00b7ga\u00b7ris"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur Tafel euch gesungen,", "tokens": ["Zur", "Ta\u00b7fel", "euch", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Grasmahl machte s\u00fc\u00df,", "tokens": ["Das", "Gras\u00b7mahl", "mach\u00b7te", "s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es wird euch keine Weid'", "tokens": ["Es", "wird", "euch", "kei\u00b7ne", "Weid'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wohl schmecken mehr vor Leid.", "tokens": ["Wohl", "schme\u00b7cken", "mehr", "vor", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo Lieder sonst erklungen,", "tokens": ["Wo", "Lie\u00b7der", "sonst", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wird heulen Traurigkeit.", "tokens": ["Wird", "heu\u00b7len", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ach, mir wird weh' geschehen.", "tokens": ["Ach", ",", "mir", "wird", "weh'", "ge\u00b7sche\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fahr' hin, verwich'ne Freud'!", "tokens": ["Fahr'", "hin", ",", "ver\u00b7wich'\u00b7ne", "Freud'", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm an, mich bla\u00df zu sehen!", "tokens": ["Komm", "an", ",", "mich", "bla\u00df", "zu", "se\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du k\u00f6mmst, du schwarzes Leid!", "tokens": ["Du", "k\u00f6mmst", ",", "du", "schwar\u00b7zes", "Leid", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Willkommen, Angst und Pein!", "tokens": ["Will\u00b7kom\u00b7men", ",", "Angst", "und", "Pein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich mag nicht fr\u00f6hlich sein.", "tokens": ["Ich", "mag", "nicht", "fr\u00f6h\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Licht mu\u00dft' untergehen,", "tokens": ["Mein", "Licht", "mu\u00dft'", "un\u00b7ter\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Drum, Nacht, brich du herein!", "tokens": ["Drum", ",", "Nacht", ",", "brich", "du", "her\u00b7ein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wie wird mir nun geschehen,", "tokens": ["Wie"