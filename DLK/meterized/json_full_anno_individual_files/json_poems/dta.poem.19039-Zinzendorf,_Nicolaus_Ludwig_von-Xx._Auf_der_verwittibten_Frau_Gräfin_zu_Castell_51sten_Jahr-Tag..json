{"dta.poem.19039": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Xx.  Auf der verwittibten Frau Gr\u00e4fin zu  \n Castell 51sten Jahr-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Herr JEsu! segne Sie um deines Nahmens Willen,", "tokens": ["Herr", "Je\u00b7su", "!", "seg\u00b7ne", "Sie", "um", "dei\u00b7nes", "Nah\u00b7mens", "Wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die unser beyder Hertz als Vaters Schwester ehrt.", "tokens": ["Die", "un\u00b7ser", "bey\u00b7der", "Hertz", "als", "Va\u00b7ters", "Schwes\u00b7ter", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "PIAT", "NN", "KOUS", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach! fahre fort den Geist zu setzen und zu stillen,", "tokens": ["Ach", "!", "fah\u00b7re", "fort", "den", "Geist", "zu", "set\u00b7zen", "und", "zu", "stil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PTKVZ", "ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den noch so mancherley von aussen her beschwehrt!", "tokens": ["Den", "noch", "so", "man\u00b7cher\u00b7ley", "von", "aus\u00b7sen", "her", "be\u00b7schwehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIS", "APPR", "NE", "APZR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O Liebe! hast du nicht f\u00fcr sie den Tod gelitten,", "tokens": ["O", "Lie\u00b7be", "!", "hast", "du", "nicht", "f\u00fcr", "sie", "den", "Tod", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht deiner Gottheit-Glantz mit Dunckelheit bedeckt,", "tokens": ["Nicht", "dei\u00b7ner", "Gott\u00b7heit\u00b7Glantz", "mit", "Dun\u00b7ckel\u00b7heit", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht mit dem hellen Schwarm der Schlangen-Brut ge-\nstritten,", "tokens": ["Nicht", "mit", "dem", "hel\u00b7len", "Schwarm", "der", "Schlan\u00b7gen\u00b7Brut", "ge", "strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "ART", "NN", "TRUNC", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Nicht dich in eigner Krafft f\u00fcr Sie auch auferweckt,", "tokens": ["Nicht", "dich", "in", "eig\u00b7ner", "Krafft", "f\u00fcr", "Sie", "auch", "au\u00b7fer\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PRF", "APPR", "ADJA", "NN", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nicht, da\u00df sie herrschete, dich selbst zum Knecht verkauffet,", "tokens": ["Nicht", ",", "da\u00df", "sie", "herr\u00b7sche\u00b7te", ",", "dich", "selbst", "zum", "Knecht", "ver\u00b7kauf\u00b7fet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und d\u00fcrftig arm gemacht, die Seele reich zu sehn,", "tokens": ["Und", "d\u00fcrf\u00b7tig", "arm", "ge\u00b7macht", ",", "die", "See\u00b7le", "reich", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVPP", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nicht dich mit Flamm und Brand des Zornes selbst getaufet,", "tokens": ["Nicht", "dich", "mit", "Flamm", "und", "Brand", "des", "Zor\u00b7nes", "selbst", "ge\u00b7tau\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PRF", "APPR", "NE", "KON", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Um Sie zu w\u00fcrdigen durch Meer und Feur zu gehn?", "tokens": ["Um", "Sie", "zu", "w\u00fcr\u00b7di\u00b7gen", "durch", "Meer", "und", "Feur", "zu", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ja, HErr! dis alles ist f\u00fcr Sie so wohl geschehen,", "tokens": ["Ja", ",", "Herr", "!", "dis", "al\u00b7les", "ist", "f\u00fcr", "Sie", "so", "wohl", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "PDS", "PIS", "VAFIN", "APPR", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Als uns und andere; Ja darum littest du:", "tokens": ["Als", "uns", "und", "an\u00b7de\u00b7re", ";", "Ja", "da\u00b7rum", "lit\u00b7test", "du", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PIS", "$.", "PTKANT", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie soll dein Antlitz einst vers\u00f6hnt im Friede sehen;", "tokens": ["Sie", "soll", "dein", "Ant\u00b7litz", "einst", "ver\u00b7s\u00f6hnt", "im", "Frie\u00b7de", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Durch deiner Arbeit Krafft, gedeyhet sie zur Ruh:", "tokens": ["Durch", "dei\u00b7ner", "Ar\u00b7beit", "Krafft", ",", "ge\u00b7dey\u00b7het", "sie", "zur", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ja: aber darum bist du nicht herab gekommen,", "tokens": ["Ja", ":", "a\u00b7ber", "da\u00b7rum", "bist", "du", "nicht", "her\u00b7ab", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "PAV", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df du nur blo\u00df allein der S\u00fcnden-Tr\u00e4ger seyst:", "tokens": ["Da\u00df", "du", "nur", "blo\u00df", "al\u00b7lein", "der", "S\u00fcn\u00b7den\u00b7Tr\u00e4\u00b7ger", "seyst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du scheinest Gnaden-Licht und Leitstern aller Frommen;", "tokens": ["Du", "schei\u00b7nest", "Gna\u00b7den\u00b7Licht", "und", "Leits\u00b7tern", "al\u00b7ler", "From\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Damit du uns zugleich von aller Nacht befreyst:", "tokens": ["Da\u00b7mit", "du", "uns", "zu\u00b7gleich", "von", "al\u00b7ler", "Nacht", "be\u00b7freyst", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dein Wandel solte uns, o GOtt-Mensch! deutlich weisen,", "tokens": ["Dein", "Wan\u00b7del", "sol\u00b7te", "uns", ",", "o", "Got\u00b7tMensch", "!", "deut\u00b7lich", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "$,", "FM", "NN", "$.", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wie jeder GOttes-Mensch in unbeflecktem Sinn,", "tokens": ["Wie", "je\u00b7der", "Got\u00b7tes\u00b7Mensch", "in", "un\u00b7be\u00b7fleck\u00b7tem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mit seinem Lebens-Lauf den Nahmen Christi preisen,", "tokens": ["Mit", "sei\u00b7nem", "Le\u00b7bens\u00b7Lauf", "den", "Nah\u00b7men", "Chris\u00b7ti", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und also streiten soll, da\u00df er den Crantz gewinn.", "tokens": ["Und", "al\u00b7so", "strei\u00b7ten", "soll", ",", "da\u00df", "er", "den", "Crantz", "ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Als Christ ist man nicht Graf, nicht F\u00fcrst, nicht edler Ritter;", "tokens": ["Als", "Christ", "ist", "man", "nicht", "Graf", ",", "nicht", "F\u00fcrst", ",", "nicht", "ed\u00b7ler", "Rit\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PIS", "PTKNEG", "NE", "$,", "PTKNEG", "NN", "$,", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dis d\u00fcnckt dem edlen Geist ein ungereimter Tand.", "tokens": ["Dis", "d\u00fcnckt", "dem", "ed\u00b7len", "Geist", "ein", "un\u00b7ge\u00b7reim\u00b7ter", "Tand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Jhr Nicht! ist Christi Wort: Die Lehre schmeckt wohl bitter,", "tokens": ["Ihr", "Nicht", "!", "ist", "Chris\u00b7ti", "Wort", ":", "Die", "Leh\u00b7re", "schmeckt", "wohl", "bit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$.", "VAFIN", "NE", "NN", "$.", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn man des Christen Staats-Gesetze nicht erkannt.", "tokens": ["Wenn", "man", "des", "Chris\u00b7ten", "Staats\u00b7Ge\u00b7set\u00b7ze", "nicht", "er\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Denn hiemit werden nicht die St\u00e4nde aufgehaben:", "tokens": ["Denn", "hie\u00b7mit", "wer\u00b7den", "nicht", "die", "St\u00e4n\u00b7de", "auf\u00b7ge\u00b7ha\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PTKNEG", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die sind in ihrer Art als wie ein Boten-Schild,", "tokens": ["Die", "sind", "in", "ih\u00b7rer", "Art", "als", "wie", "ein", "Bo\u00b7ten\u00b7Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Damit wir durch das Land der Cananiter traben,", "tokens": ["Da\u00b7mit", "wir", "durch", "das", "Land", "der", "Ca\u00b7na\u00b7ni\u00b7ter", "tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo als ein Passeport der Ehren-Titul gilt.", "tokens": ["Wo", "als", "ein", "Pas\u00b7se\u00b7port", "der", "Eh\u00b7ren\u00b7Ti\u00b7tul", "gilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOKOM", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie macht es denn ein Christ bey dem sich W\u00fcrde zeiget?", "tokens": ["Wie", "macht", "es", "denn", "ein", "Christ", "bey", "dem", "sich", "W\u00fcr\u00b7de", "zei\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PRELS", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Er braucht sich seiner H\u00f6h, in grosser Niedrigkeit;", "tokens": ["Er", "braucht", "sich", "sei\u00b7ner", "H\u00f6h", ",", "in", "gros\u00b7ser", "Nied\u00b7rig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sitzt er im F\u00fcrsten-Glantz, die Seele liegt gebeuget,", "tokens": ["Sitzt", "er", "im", "F\u00fcrs\u00b7ten\u00b7Glantz", ",", "die", "See\u00b7le", "liegt", "ge\u00b7beu\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und h\u00e4lts vor Tages-Last der letzten b\u00f6sen Zeit;", "tokens": ["Und", "h\u00e4lts", "vor", "Ta\u00b7ges\u00b7Last", "der", "letz\u00b7ten", "b\u00f6\u00b7sen", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Man hoft, wie David einst, mit denen die auf Erden", "tokens": ["Man", "hoft", ",", "wie", "Da\u00b7vid", "einst", ",", "mit", "de\u00b7nen", "die", "auf", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "NE", "ADV", "$,", "APPR", "PRELS", "ART", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Verachtetes Geschmei\u00df in Michals Augen sind,", "tokens": ["Ver\u00b7ach\u00b7te\u00b7tes", "Ge\u00b7schmei\u00df", "in", "Mic\u00b7hals", "Au\u00b7gen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NE", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Zum rechten Ehren-Schmuck hinauf ger\u00fcckt zu werden:", "tokens": ["Zum", "rech\u00b7ten", "Eh\u00b7ren\u00b7Schmuck", "hin\u00b7auf", "ge\u00b7r\u00fcckt", "zu", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und eben darum wird man hier ein kleines Kind.", "tokens": ["Und", "e\u00b7ben", "da\u00b7rum", "wird", "man", "hier", "ein", "klei\u00b7nes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "VAFIN", "PIS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Das Eine, was man noch vom hohen Stande haben,", "tokens": ["Das", "Ei\u00b7ne", ",", "was", "man", "noch", "vom", "ho\u00b7hen", "Stan\u00b7de", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "$,", "PRELS", "PIS", "ADV", "APPRART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Das, wie man ihn allein im Seegen f\u00fchren kan,", "tokens": ["Das", ",", "wie", "man", "ihn", "al\u00b7lein", "im", "See\u00b7gen", "f\u00fch\u00b7ren", "kan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PIS", "PPER", "ADV", "APPRART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ist: ", "tokens": ["Ist", ":"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.44": {"text": "So ist man droben gro\u00df, so hat der HErr gethan.", "tokens": ["So", "ist", "man", "dro\u00b7ben", "gro\u00df", ",", "so", "hat", "der", "Herr", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADJD", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Das sehen andere, die werden denn beweget,", "tokens": ["Das", "se\u00b7hen", "an\u00b7de\u00b7re", ",", "die", "wer\u00b7den", "denn", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "$,", "PRELS", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und solches schl\u00e4get uns zu lauter Palmen aus:", "tokens": ["Und", "sol\u00b7ches", "schl\u00e4\u00b7get", "uns", "zu", "lau\u00b7ter", "Pal\u00b7men", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ein Hertz, aus Eifecsucht zur Seeligkeit erreget,", "tokens": ["Ein", "Hertz", ",", "aus", "Ei\u00b7fe\u00b7csucht", "zur", "See\u00b7lig\u00b7keit", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Baut seinem F\u00f6rderer ein St\u00fcck ans Lebens-Hau\u00df.", "tokens": ["Baut", "sei\u00b7nem", "F\u00f6r\u00b7de\u00b7rer", "ein", "St\u00fcck", "ans", "Le\u00b7bens\u00b7Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Nun Hochgebohrne Frau, Sie heist hier nicht vergebens", "tokens": ["Nun", "Hoch\u00b7ge\u00b7bohr\u00b7ne", "Frau", ",", "Sie", "heist", "hier", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Und nach dem Schatten gro\u00df. Sie ist auch Obrigkeit,", "tokens": ["Und", "nach", "dem", "Schat\u00b7ten", "gro\u00df", ".", "Sie", "ist", "auch", "Ob\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$.", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Sie kennt die Last davon, die Pestilentz des Lebens;", "tokens": ["Sie", "kennt", "die", "Last", "da\u00b7von", ",", "die", "Pes\u00b7ti\u00b7lentz", "des", "Le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PAV", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "So mache sich Jhr Geist zur rechten H\u00f6h\u2019 bereit!", "tokens": ["So", "ma\u00b7che", "sich", "Ihr", "Geist", "zur", "rech\u00b7ten", "H\u00f6h'", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}