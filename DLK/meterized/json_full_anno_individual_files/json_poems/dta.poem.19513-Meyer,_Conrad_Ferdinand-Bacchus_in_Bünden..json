{"dta.poem.19513": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Bacchus in B\u00fcnden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wo st\u00fcrzend aus r\u00e4tischen Kl\u00fcften der Rhein", "tokens": ["Wo", "st\u00fcr\u00b7zend", "aus", "r\u00e4\u00b7ti\u00b7schen", "Kl\u00fcf\u00b7ten", "der", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ADJA", "NN", "ART", "NE"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Um silberne H\u00fcften sich g\u00fcrtet den Wein,", "tokens": ["Um", "sil\u00b7ber\u00b7ne", "H\u00fcf\u00b7ten", "sich", "g\u00fcr\u00b7tet", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ziehn paukende Masken mit Cymbelgel\u00e4ut:", "tokens": ["Ziehn", "pau\u00b7ken\u00b7de", "Mas\u00b7ken", "mit", "Cym\u00b7bel\u00b7ge\u00b7l\u00e4ut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u201edu Traube von Trimmis, dich wimmeln wir heut!\u201c", "tokens": ["\u201e", "du", "Trau\u00b7be", "von", "Trim\u00b7mis", ",", "dich", "wim\u00b7meln", "wir", "heut", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "NN", "APPR", "NE", "$,", "PPER", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Sie treten den Reigen, sie stampfen den Chor,", "tokens": ["Sie", "tre\u00b7ten", "den", "Rei\u00b7gen", ",", "sie", "stamp\u00b7fen", "den", "Chor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da dunkelt's und lodern die Fackeln empor:", "tokens": ["Da", "dun\u00b7kelt's", "und", "lo\u00b7dern", "die", "Fa\u00b7ckeln", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein Kranz in den L\u00fcften! Ein wirbelndes Paar!", "tokens": ["Ein", "Kranz", "in", "den", "L\u00fcf\u00b7ten", "!", "Ein", "wir\u00b7beln\u00b7des", "Paar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ein brennender Nacken! Ein purpurnes Haar!", "tokens": ["Ein", "bren\u00b7nen\u00b7der", "Na\u00b7cken", "!", "Ein", "pur\u00b7pur\u00b7nes", "Haar", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Die Fackeln verl\u00f6schen. Es hebt sich der Glanz", "tokens": ["Die", "Fa\u00b7ckeln", "ver\u00b7l\u00f6\u00b7schen", ".", "Es", "hebt", "sich", "der", "Glanz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$.", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Des schimmernden Monds und vergeistert den Tanz \u2014", "tokens": ["Des", "schim\u00b7mern\u00b7den", "Monds", "und", "ver\u00b7geis\u00b7tert", "den", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein adliger J\u00fcngling von fremder Gestalt", "tokens": ["Ein", "ad\u00b7li\u00b7ger", "J\u00fcng\u00b7ling", "von", "frem\u00b7der", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bemeistert den Reigen mit Herrschergewalt.", "tokens": ["Be\u00b7meis\u00b7tert", "den", "Rei\u00b7gen", "mit", "Herr\u00b7scher\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Er schwebt in der Mitte bekr\u00e4nzt und allein", "tokens": ["Er", "schwebt", "in", "der", "Mit\u00b7te", "be\u00b7kr\u00e4nzt", "und", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVPP", "KON", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit leuchtenden F\u00fc\u00dfen in himmlischem Schein,", "tokens": ["Mit", "leuch\u00b7ten\u00b7den", "F\u00fc\u00b7\u00dfen", "in", "himm\u00b7li\u00b7schem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Schulter umflattert getigertes Fell,", "tokens": ["Die", "Schul\u00b7ter", "um\u00b7flat\u00b7tert", "ge\u00b7ti\u00b7ger\u00b7tes", "Fell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Er tr\u00e4gt einen Scepter, der k\u00fchne Gesell.", "tokens": ["Er", "tr\u00e4gt", "ei\u00b7nen", "Scep\u00b7ter", ",", "der", "k\u00fch\u00b7ne", "Ge\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Er neigt ihn vor Irma, der tr\u00e4umenden Maid:", "tokens": ["Er", "neigt", "ihn", "vor", "Ir\u00b7ma", ",", "der", "tr\u00e4u\u00b7men\u00b7den", "Maid", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "\u201ein nachtdunkle Haare taugt blitzend Geschmeid!\u201c", "tokens": ["\u201e", "in", "nacht\u00b7dunk\u00b7le", "Haa\u00b7re", "taugt", "blit\u00b7zend", "Ge\u00b7schmeid", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "VVFIN", "ADJD", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er greift in den Himmel mit m\u00e4chtiger Hand,", "tokens": ["Er", "greift", "in", "den", "Him\u00b7mel", "mit", "m\u00e4ch\u00b7ti\u00b7ger", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er raubt aus den Sternen ein flimmerndes Band:", "tokens": ["Er", "raubt", "aus", "den", "Ster\u00b7nen", "ein", "flim\u00b7mern\u00b7des", "Band", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6n Irma schwebt hin mit dem Kr\u00f6nlein von Licht,", "tokens": ["Sch\u00f6n", "Ir\u00b7ma", "schwebt", "hin", "mit", "dem", "Kr\u00f6n\u00b7lein", "von", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Als fesselte f\u00fcrder die Erde sie nicht,", "tokens": ["Als", "fes\u00b7sel\u00b7te", "f\u00fcr\u00b7der", "die", "Er\u00b7de", "sie", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er schwingt ihr zu H\u00e4upten den Thyrsus umrankt", "tokens": ["Er", "schwingt", "ihr", "zu", "H\u00e4up\u00b7ten", "den", "Thyr\u00b7sus", "um\u00b7rankt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ART", "NN", "VVFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Mit \u00fcppigem Laube, von Trauben umschwankt ...", "tokens": ["Mit", "\u00fcp\u00b7pi\u00b7gem", "Lau\u00b7be", ",", "von", "Trau\u00b7ben", "um\u00b7schwankt", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "VVFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "Zw\u00f6lf Schl\u00e4ge verk\u00fcnden die Mitte der Nacht.", "tokens": ["Zw\u00f6lf", "Schl\u00e4\u00b7ge", "ver\u00b7k\u00fcn\u00b7den", "die", "Mit\u00b7te", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Reigen erm\u00fcdet. Das Fest ist vollbracht!", "tokens": ["Der", "Rei\u00b7gen", "er\u00b7m\u00fc\u00b7det", ".", "Das", "Fest", "ist", "voll\u00b7bracht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u201eherunter die Masken! So will es der Brauch!", "tokens": ["\u201e", "her\u00b7un\u00b7ter", "die", "Mas\u00b7ken", "!", "So", "will", "es", "der", "Brauch", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "$.", "ADV", "VMFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Du F\u00fchrer des Reigens, entlarve dich auch!", "tokens": ["Du", "F\u00fch\u00b7rer", "des", "Rei\u00b7gens", ",", "ent\u00b7lar\u00b7ve", "dich", "auch", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.8": {"line.1": {"text": "Wir sind unser zwanzig, und voll ist die Zahl!", "tokens": ["Wir", "sind", "un\u00b7ser", "zwan\u00b7zig", ",", "und", "voll", "ist", "die", "Zahl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "CARD", "$,", "KON", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wer bist du, der frech in die Gilde sich stahl?", "tokens": ["Wer", "bist", "du", ",", "der", "frech", "in", "die", "Gil\u00b7de", "sich", "stahl", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ein Gaukler? Ein Zaub'rer? Sprich wie du dich nennst!", "tokens": ["Ein", "Gauk\u00b7ler", "?", "Ein", "Zaub'\u00b7rer", "?", "Sprich", "wie", "du", "dich", "nennst", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "ADJD", "KOKOM", "PPER", "PRF", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sonst f\u00fcrcht' unsre Messer, bist du kein Gespenst!\u201c", "tokens": ["Sonst", "f\u00fcrcht'", "uns\u00b7re", "Mes\u00b7ser", ",", "bist", "du", "kein", "Ge\u00b7spenst", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "PIAT", "NN", "$.", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Ein M\u00f6nchlein, ein zechend entschlafnes, wird reg:", "tokens": ["Ein", "M\u00f6nch\u00b7lein", ",", "ein", "ze\u00b7chend", "ent\u00b7schlaf\u00b7nes", ",", "wird", "reg", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJD", "ADJA", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u201ewer bist du? Der Satan? Dir weis' ich den Weg!\u201c", "tokens": ["\u201e", "wer", "bist", "du", "?", "Der", "Sa\u00b7tan", "?", "Dir", "weis'", "ich", "den", "Weg", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "$.", "ART", "NN", "$.", "PPER", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Er zeichnet ein Kreuz. \u201eNun entmumme dich nur!", "tokens": ["Er", "zeich\u00b7net", "ein", "Kreuz", ".", "\u201e", "Nun", "ent\u00b7mum\u00b7me", "dich", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich bin der gelehrte Pancrazi von Cur!\u201c", "tokens": ["Ich", "bin", "der", "ge\u00b7lehr\u00b7te", "Pan\u00b7cr\u00b7a\u00b7zi", "von", "Cur", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "$.", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Der J\u00fcngling entlarvt ein von Eppich umlaubt,", "tokens": ["Der", "J\u00fcng\u00b7ling", "ent\u00b7larvt", "ein", "von", "Ep\u00b7pich", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "APPR", "NE", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ein hohes, ein mildes, ein gn\u00e4diges Haupt:", "tokens": ["Ein", "ho\u00b7hes", ",", "ein", "mil\u00b7des", ",", "ein", "gn\u00e4\u00b7di\u00b7ges", "Haupt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u201ezu F\u00fc\u00dfen dem Herrscher, vermessen Gesind!", "tokens": ["\u201e", "zu", "F\u00fc\u00b7\u00dfen", "dem", "Herr\u00b7scher", ",", "ver\u00b7mes\u00b7sen", "Ge\u00b7sind", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ich bin Dionysus, des Donnerers Kind!\u201c", "tokens": ["Ich", "bin", "Di\u00b7o\u00b7ny\u00b7sus", ",", "des", "Don\u00b7ne\u00b7rers", "Kind", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "NE", "$,", "ART", "NN", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.11": {"line.1": {"text": "Er l\u00e4chelt dem M\u00f6nch in das feiste Gesicht:", "tokens": ["Er", "l\u00e4\u00b7chelt", "dem", "M\u00f6nch", "in", "das", "feis\u00b7te", "Ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u201esilenos, Silenos, verleugne mich nicht!", "tokens": ["\u201e", "si\u00b7le\u00b7nos", ",", "Si\u00b7le\u00b7nos", ",", "ver\u00b7leug\u00b7ne", "mich", "nicht", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NE", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Mich hat seine Gnaden, der Bischof, gebannt", "tokens": ["Mich", "hat", "sei\u00b7ne", "Gna\u00b7den", ",", "der", "Bi\u00b7schof", ",", "ge\u00b7bannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "$,", "VVPP"], "meter": "-+--+-+-+-+", "m