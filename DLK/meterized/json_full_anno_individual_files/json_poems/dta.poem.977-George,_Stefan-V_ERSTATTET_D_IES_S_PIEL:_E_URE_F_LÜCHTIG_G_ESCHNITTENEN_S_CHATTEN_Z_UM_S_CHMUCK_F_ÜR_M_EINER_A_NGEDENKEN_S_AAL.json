{"dta.poem.977": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "V ERSTATTET  D IES  S PIEL:  E URE  F L\u00dcCHTIG  \n  G ESCHNITTENEN  S CHATTEN  Z UM  S CHMUCK  \n  F \u00dcR  M EINER  A NGEDENKEN  S AAL", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Soll nun der mund der von des eises bruch               ", "tokens": ["Soll", "nun", "der", "mund", "der", "von", "des", "ei\u00b7ses", "bruch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum neuen reife l&#228;ngst erstarkt im wehe               ", "tokens": ["Zum", "neu\u00b7en", "rei\u00b7fe", "l", "&#228;", "ngst", "er\u00b7starkt", "im", "we\u00b7he"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$(", "ADV", "VVPP", "APPRART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sich klagend \u00f6ffnen und nach welchem spruch               ", "tokens": ["Sich", "kla\u00b7gend", "\u00f6ff\u00b7nen", "und", "nach", "wel\u00b7chem", "spruch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "VVINF", "KON", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem kinde? unterbrich mich nicht &#8212; ich flehe.             ", "tokens": ["Dem", "kin\u00b7de", "?", "un\u00b7ter\u00b7brich", "mich", "nicht", "&#8212;", "ich", "fle\u00b7he", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ADJD", "PPER", 