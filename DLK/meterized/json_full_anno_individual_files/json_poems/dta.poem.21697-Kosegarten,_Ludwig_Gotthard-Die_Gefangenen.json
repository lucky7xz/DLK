{"dta.poem.21697": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Gefangenen", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es liegen drey Junggesellen", "tokens": ["Es", "lie\u00b7gen", "drey", "Jung\u00b7ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gefangen auf den Tod.", "tokens": ["Ge\u00b7fan\u00b7gen", "auf", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ewas haben wir denn B\u00f6ses begangen?", "tokens": ["\u201e", "was", "ha\u00b7ben", "wir", "denn", "B\u00f6\u00b7ses", "be\u00b7gan\u00b7gen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "ADV", "NE", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir liegen, wir liegen gefangen.", "tokens": ["Wir", "lie\u00b7gen", ",", "wir", "lie\u00b7gen", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Erbarmt euch unsrer Noth!\u201c", "tokens": ["Er\u00b7barmt", "euch", "uns\u00b7rer", "Noth", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Jungfr\u00e4ulein ging vor\u00fcber.", "tokens": ["Ein", "Jung\u00b7fr\u00e4u\u00b7lein", "ging", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es h\u00f6rt der Gefangnen Schreyn.", "tokens": ["Es", "h\u00f6rt", "der", "Ge\u00b7fang\u00b7nen", "Schreyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es gehet ihm tief zu Herzen,", "tokens": ["Es", "ge\u00b7het", "ihm", "tief", "zu", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Es machet ihm bittre Schmerzen,", "tokens": ["Es", "ma\u00b7chet", "ihm", "bitt\u00b7re", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dem zarten Jungfr\u00e4ulein.", "tokens": ["Dem", "zar\u00b7ten", "Jung\u00b7fr\u00e4u\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr Gefangnen, ich will f\u00fcr euch bitten,", "tokens": ["Ihr", "Ge\u00b7fang\u00b7nen", ",", "ich", "will", "f\u00fcr", "euch", "bit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bittet ihr indessen Gott.", "tokens": ["Bit\u00b7tet", "ihr", "in\u00b7des\u00b7sen", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich will die Herren sch\u00f6n gr\u00fcssen,", "tokens": ["Ich", "will", "die", "Her\u00b7ren", "sch\u00f6n", "gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich will ihnen fallen zu F\u00fcssen,", "tokens": ["Ich", "will", "ih\u00b7nen", "fal\u00b7len", "zu", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Euch l\u00f6sen vom bittern Tod.\u201c", "tokens": ["Euch", "l\u00f6\u00b7sen", "vom", "bit\u00b7tern", "Tod", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201egott gr\u00fcss euch, grossg\u00fcnstige Herren!", "tokens": ["\u201e", "gott", "gr\u00fcss", "euch", ",", "gross\u00b7g\u00fcns\u00b7ti\u00b7ge", "Her\u00b7ren", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Eine Bitte mir gew\u00e4hrt:", "tokens": ["Ei\u00b7ne", "Bit\u00b7te", "mir", "ge\u00b7w\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Gefangnen haben nichts begangen.", "tokens": ["Die", "Ge\u00b7fang\u00b7nen", "ha\u00b7ben", "nichts", "be\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lasst sie los, lasst los die Gefangnen,", "tokens": ["Lasst", "sie", "los", ",", "lasst", "los", "die", "Ge\u00b7fang\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dass Gott euch wieder erh\u00f6rt.\u201c \u2014", "tokens": ["Dass", "Gott", "euch", "wie\u00b7der", "er\u00b7h\u00f6rt", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "VVPP", "$.", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "\u201ewas du willst kann dir nicht werden,", "tokens": ["\u201e", "was", "du", "willst", "kann", "dir", "nicht", "wer\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "VMFIN", "VMFIN", "PPER", "PTKNEG", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du junge zarte Maid.", "tokens": ["Du", "jun\u00b7ge", "zar\u00b7te", "Maid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Gefangnen m\u00fcssen sterben.", "tokens": ["Die", "Ge\u00b7fang\u00b7nen", "m\u00fcs\u00b7sen", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottes Reich m\u00fcssen sie ererben,", "tokens": ["Got\u00b7tes", "Reich", "m\u00fcs\u00b7sen", "sie", "er\u00b7er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Dazu die ew'ge Freud.", "tokens": ["Da\u00b7zu", "die", "ew'\u00b7ge", "Freud", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201ewas die Gefangenen haben begangen,", "tokens": ["\u201e", "was", "die", "Ge\u00b7fan\u00b7ge\u00b7nen", "ha\u00b7ben", "be\u00b7gan\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Der Tod nur b\u00fcssen mag.", "tokens": ["Der", "Tod", "nur", "b\u00fcs\u00b7sen", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Schwer dr\u00fccken der S\u00fcnde Lasten!", "tokens": ["Schwer", "dr\u00fc\u00b7cken", "der", "S\u00fcn\u00b7de", "Las\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Lass sie ruhen, lass sie risten, lass sie rasten", "tokens": ["Lass", "sie", "ru\u00b7hen", ",", "lass", "sie", "ris\u00b7ten", ",", "lass", "sie", "ras\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVFIN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Bis an den j\u00fcngsten Tag.\u201c", "tokens": ["Bis", "an", "den", "j\u00fcng\u00b7sten", "Tag", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das M\u00e4dchen ging von dannen", "tokens": ["Das", "M\u00e4d\u00b7chen", "ging", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In bitterm Herzeleid.", "tokens": ["In", "bit\u00b7term", "Her\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eihr Gefangnen, ihr m\u00fcsst sterben,", "tokens": ["\u201e", "ihr", "Ge\u00b7fang\u00b7nen", ",", "ihr", "m\u00fcsst", "ster\u00b7ben", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottes Reich m\u00fcsst ihr ererben,", "tokens": ["Got\u00b7tes", "Reich", "m\u00fcsst", "ihr", "er\u00b7er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dazu die ew'ge Freud.", "tokens": ["Da\u00b7zu", "die", "ew'\u00b7ge", "Freud", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr Gefangnen, was ihr begangen,", "tokens": ["Ihr", "Ge\u00b7fang\u00b7nen", ",", "was", "ihr", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Das b\u00fcsset nur der Tod.", "tokens": ["Das", "b\u00fcs\u00b7set", "nur", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwer dr\u00fccken der S\u00fcnde Lasten.", "tokens": ["Schwer", "dr\u00fc\u00b7cken", "der", "S\u00fcn\u00b7de", "Las\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "S\u00fcss l\u00e4sst sichs risten und rasten.", "tokens": ["S\u00fcss", "l\u00e4sst", "sichs", "ris\u00b7ten", "und", "ras\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ihr Gefangnen befehlt euch Gott!\u201c", "tokens": ["Ihr", "Ge\u00b7fang\u00b7nen", "be\u00b7fehlt", "euch", "Gott", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Und als die Gefangenen kamen", "tokens": ["Und", "als", "die", "Ge\u00b7fan\u00b7ge\u00b7nen", "ka\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wohl auf den Richteplatz,", "tokens": ["Wohl", "auf", "den", "Rich\u00b7te\u00b7platz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das M\u00e4gdlein stand in dem Kreise.", "tokens": ["Das", "M\u00e4gd\u00b7lein", "stand", "in", "dem", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie winkten, sie gr\u00fcssten sie leise.", "tokens": ["Sie", "wink\u00b7ten", ",", "sie", "gr\u00fcs\u00b7sten", "sie", "lei\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Fahr wohl, allersch\u00f6nster Schatz.\u201c", "tokens": ["Fahr", "wohl", ",", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ter", "Schatz", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und als das Schwert sie gerichtet,", "tokens": ["Und", "als", "das", "Schwert", "sie", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das M\u00e4gdlein machtlos lag.", "tokens": ["Das", "M\u00e4gd\u00b7lein", "macht\u00b7los", "lag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201emich \u00e4ngstigen des Lebens Lasten.", "tokens": ["\u201e", "mich", "\u00e4ngs\u00b7ti\u00b7gen", "des", "Le\u00b7bens", "Las\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lasst mich ruhen, lasst mich risten, lasst mich rasten", "tokens": ["Lasst", "mich", "ru\u00b7hen", ",", "lasst", "mich", "ris\u00b7ten", ",", "lasst", "mich", "ras\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", 