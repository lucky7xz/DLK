{"textgrid.poem.67518": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Rettung", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G'nug der drohenden ungl\u00fcckschwangern St\u00fcrme,", "tokens": ["G'\u00b7nug", "der", "dro\u00b7hen\u00b7den", "un\u00b7gl\u00fcck\u00b7schwan\u00b7gern", "St\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "G'nug des giftigen Nebels, der den Freunden", "tokens": ["G'\u00b7nug", "des", "gif\u00b7ti\u00b7gen", "Ne\u00b7bels", ",", "der", "den", "Freun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Freunde birget und alte Treu in neuer", "tokens": ["Freun\u00b7de", "bir\u00b7get", "und", "al\u00b7te", "Treu", "in", "neu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KON", "ADJA", "NN", "APPR", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "H\u00f6llengestalt zeigt!", "tokens": ["H\u00f6l\u00b7len\u00b7ge\u00b7stalt", "zeigt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "G'nug des schrecklichen Hagels, der die Saaten", "tokens": ["G'\u00b7nug", "des", "schreck\u00b7li\u00b7chen", "Ha\u00b7gels", ",", "der", "die", "Saa\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+--+--+---+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Aller W\u00fcnsche zerschl\u00e4gt, hat uns das Schicksal", "tokens": ["Al\u00b7ler", "W\u00fcn\u00b7sche", "zer\u00b7schl\u00e4gt", ",", "hat", "uns", "das", "Schick\u00b7sal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "VAFIN", "PPER", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zugesendet, das j\u00fcngst auf Thron' und Reiche", "tokens": ["Zu\u00b7ge\u00b7sen\u00b7det", ",", "das", "j\u00fcngst", "auf", "Thron'", "und", "Rei\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PRELS", "ADV", "APPR", "NN", "KON", "NE"], "meter": "-----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Blitze geschleudert;", "tokens": ["Blit\u00b7ze", "ge\u00b7schleu\u00b7dert", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Hat die V\u00f6lker erschreckt mit jener Zeiten", "tokens": ["Hat", "die", "V\u00f6l\u00b7ker", "er\u00b7schreckt", "mit", "je\u00b7ner", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVFIN", "APPR", "PDAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "R\u00fcckkehr, da in Europa's dunkeln W\u00e4ldern", "tokens": ["R\u00fcck\u00b7kehr", ",", "da", "in", "Eu\u00b7ro\u00b7pa's", "dun\u00b7keln", "W\u00e4l\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "W\u00f6lfe heuleten und mit mehr als Wolfsgier", "tokens": ["W\u00f6l\u00b7fe", "heu\u00b7le\u00b7ten", "und", "mit", "mehr", "als", "Wolfs\u00b7gier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "APPR", "PIAT", "KOKOM", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Heere sich w\u00fcrgten.", "tokens": ["Hee\u00b7re", "sich", "w\u00fcrg\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Sahn wir, sehen wir nicht den Rhein, die Mosel,", "tokens": ["Sahn", "wir", ",", "se\u00b7hen", "wir", "nicht", "den", "Rhein", ",", "die", "Mo\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Maas und Rhone vom Blut unschuld'ger V\u00f6lker,", "tokens": ["Maas", "und", "Rho\u00b7ne", "vom", "Blut", "un\u00b7schuld'\u00b7ger", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Roth vom Blute der B\u00fcrger? im Gefilde", "tokens": ["Roth", "vom", "Blu\u00b7te", "der", "B\u00fcr\u00b7ger", "?", "im", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$.", "APPRART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Berge von Leichen?", "tokens": ["Ber\u00b7ge", "von", "Lei\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "V\u00e4ter, J\u00fcnglinge, Kinder f\u00fcllten Gr\u00e4ber", "tokens": ["V\u00e4\u00b7ter", ",", "J\u00fcng\u00b7lin\u00b7ge", ",", "Kin\u00b7der", "f\u00fcll\u00b7ten", "Gr\u00e4\u00b7ber"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVFIN", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Vor den Heeren, damit dar\u00fcber stiegen", "tokens": ["Vor", "den", "Hee\u00b7ren", ",", "da\u00b7mit", "da\u00b7r\u00fc\u00b7ber", "stie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "PAV", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Neue Heere der Br\u00fcder in die offne", "tokens": ["Neu\u00b7e", "Hee\u00b7re", "der", "Br\u00fc\u00b7der", "in", "die", "off\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "H\u00f6hle des Todes!", "tokens": ["H\u00f6h\u00b7le", "des", "To\u00b7des", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Und weswegen? Du wirst es h\u00f6ren, Nachwelt,", "tokens": ["Und", "wes\u00b7we\u00b7gen", "?", "Du", "wirst", "es", "h\u00f6\u00b7ren", ",", "Nach\u00b7welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$,", "NN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wenn vom Grimme der V\u00e4ter uns noch Enkel", "tokens": ["Wenn", "vom", "Grim\u00b7me", "der", "V\u00e4\u00b7ter", "uns", "noch", "En\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "PPER", "ADV", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Bleiben, h\u00f6ren und richten uns, entkommne", "tokens": ["Blei\u00b7ben", ",", "h\u00f6\u00b7ren", "und", "rich\u00b7ten", "uns", ",", "ent\u00b7komm\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "VVINF", "KON", "VVFIN", "PPER", "$,", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Weisere Nachwelt!", "tokens": ["Wei\u00b7se\u00b7re", "Nach\u00b7welt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Wen der G\u00f6tter, o wen soll unser Flehen", "tokens": ["Wen", "der", "G\u00f6t\u00b7ter", ",", "o", "wen", "soll", "un\u00b7ser", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "$,", "FM", "PWS", "VMFIN", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Niederrufen? Ihr heil'gen Vestalinnen,", "tokens": ["Nie\u00b7der\u00b7ru\u00b7fen", "?", "Ihr", "heil'\u00b7gen", "Ves\u00b7ta\u00b7lin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Treue Seelen, o wer soll unsern schweren", "tokens": ["Treu\u00b7e", "See\u00b7len", ",", "o", "wer", "soll", "un\u00b7sern", "schwe\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "FM", "PWS", "VMFIN", "PPOSAT", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Frevel ents\u00fchnen?", "tokens": ["Fre\u00b7vel", "ent\u00b7s\u00fch\u00b7nen", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Nicht der blutige Mavors, Kriege zeugen", "tokens": ["Nicht", "der", "blu\u00b7ti\u00b7ge", "Ma\u00b7vors", ",", "Krie\u00b7ge", "zeu\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Kriege; Cypria nicht, ihr Band um Thronen,", "tokens": ["Krie\u00b7ge", ";", "Cyp\u00b7ria", "nicht", ",", "ihr", "Band", "um", "Thro\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "PTKNEG", "$,", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fein und lose gespannt, verewigt unsre", "tokens": ["Fein", "und", "lo\u00b7se", "ge\u00b7spannt", ",", "ve\u00b7re\u00b7wigt", "uns\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "VVPP", "$,", "VVFIN", "PPOSAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sorge der Nachzeit;", "tokens": ["Sor\u00b7ge", "der", "Nach\u00b7zeit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Nicht die Herrscherin Juno, sie verschw\u00e4gert", "tokens": ["Nicht", "die", "Herr\u00b7sche\u00b7rin", "Ju\u00b7no", ",", "sie", "ver\u00b7schw\u00e4\u00b7gert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "NE", "$,", "PPER", "VVPP"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Nationen zu ihres Stolzes Zwietracht.", "tokens": ["Na\u00b7ti\u00b7o\u00b7nen", "zu", "ih\u00b7res", "Stol\u00b7zes", "Zwiet\u00b7racht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Komm hernieder, o Du, ein Strahlenj\u00fcngling,", "tokens": ["Komm", "her\u00b7nie\u00b7der", ",", "o", "Du", ",", "ein", "Strah\u00b7len\u00b7j\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Priester Apollo,", "tokens": ["Pries\u00b7ter", "A\u00b7pol\u00b7lo", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Mit dem lindesten Griff in Deine Saiten", "tokens": ["Mit", "dem", "lin\u00b7des\u00b7ten", "Griff", "in", "Dei\u00b7ne", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "B\u00e4ndigend der Entbrannten Wuth; ", "tokens": ["B\u00e4n\u00b7di\u00b7gend", "der", "Ent\u00b7brann\u00b7ten", "Wuth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Deines goldenen K\u00f6chers trenne jeden", "tokens": ["Dei\u00b7nes", "gol\u00b7de\u00b7nen", "K\u00f6\u00b7chers", "tren\u00b7ne", "je\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIAT"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "T\u00e4uschenden Nebel,", "tokens": ["T\u00e4u\u00b7schen\u00b7den", "Ne\u00b7bel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Da\u00df sich Br\u00fcder erkennen, da\u00df sich V\u00f6lker,", "tokens": ["Da\u00df", "sich", "Br\u00fc\u00b7der", "er\u00b7ken\u00b7nen", ",", "da\u00df", "sich", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "VVINF", "$,", "KOUS", "PRF", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie von Tr\u00e4umen erwacht, mit Hilf' umarmen!", "tokens": ["Wie", "von", "Tr\u00e4u\u00b7men", "er\u00b7wacht", ",", "mit", "Hil\u00b7f'", "um\u00b7ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVPP", "$,", "APPR", "NE", "VVINF", "$."], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Singe, singe den Menschen, Du der V\u00f6lker", "tokens": ["Sin\u00b7ge", ",", "sin\u00b7ge", "den", "Men\u00b7schen", ",", "Du", "der", "V\u00f6l\u00b7ker"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Einziger Hilfsgott,", "tokens": ["Ein\u00b7zi\u00b7ger", "Hilfs\u00b7gott", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Harmonieen des allgemeinen Wohllauts,", "tokens": ["Har\u00b7mo\u00b7ni\u00b7e\u00b7en", "des", "all\u00b7ge\u00b7mei\u00b7nen", "Wohl\u00b7lauts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "---+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die des niedrigen Neides, der an sich nagt,", "tokens": ["Die", "des", "nied\u00b7ri\u00b7gen", "Nei\u00b7des", ",", "der", "an", "sich", "nagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und der tollen Begier, die nie genie\u00dfet,", "tokens": ["Und", "der", "tol\u00b7len", "Be\u00b7gier", ",", "die", "nie", "ge\u00b7nie\u00b7\u00dfet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sch\u00e4ndliche T\u00f6chter,", "tokens": ["Sch\u00e4nd\u00b7li\u00b7che", "T\u00f6ch\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Habsucht, Sucht zu gebieten, in den Orcus", "tokens": ["Hab\u00b7sucht", ",", "Sucht", "zu", "ge\u00b7bie\u00b7ten", ",", "in", "den", "Or\u00b7cus"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PTKZU", "VVINF", "$,", "APPR", "ART", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Bannen; singe den K\u00f6nigen den sch\u00f6nsten", "tokens": ["Ban\u00b7nen", ";", "sin\u00b7ge", "den", "K\u00f6\u00b7ni\u00b7gen", "den", "sch\u00f6ns\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "K\u00f6nigsnamen, ", "tokens": ["K\u00f6\u00b7nigs\u00b7na\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Tief in das Herz ein!", "tokens": [