{"textgrid.poem.23985": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Alexandriner", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort lag der See gewellt, ein blauer Schimmerplan,", "tokens": ["Dort", "lag", "der", "See", "ge\u00b7wellt", ",", "ein", "blau\u00b7er", "Schim\u00b7mer\u00b7plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie wei\u00dfe M\u00f6ven drauf manch schneller Segelkahn;", "tokens": ["Wie", "wei\u00b7\u00dfe", "M\u00f6\u00b7ven", "drauf", "manch", "schnel\u00b7ler", "Se\u00b7gel\u00b7kahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PAV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Ufer dr\u00fcben hell, der Himmel dr\u00fcber klar,", "tokens": ["Das", "U\u00b7fer", "dr\u00fc\u00b7ben", "hell", ",", "der", "Him\u00b7mel", "dr\u00fc\u00b7ber", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ART", "NN", "PAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie das doch wundersam, gar heilig heiter war!", "tokens": ["Wie", "das", "doch", "wun\u00b7der\u00b7sam", ",", "gar", "hei\u00b7lig", "hei\u00b7ter", "war", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es tuschte noch der Herbst mit feiner K\u00fcnstlerhand", "tokens": ["Es", "tuschte", "noch", "der", "Herbst", "mit", "fei\u00b7ner", "K\u00fcnst\u00b7ler\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "In Sammetbraun und -Rot Wald, Wiese, Berg und Land.", "tokens": ["In", "Sam\u00b7met\u00b7braun", "und", "Wald", ",", "Wie\u00b7se", ",", "Berg", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Unendlich weit der Blick, und umri\u00dfreinlich, fein,", "tokens": ["Un\u00b7end\u00b7lich", "weit", "der", "Blick", ",", "und", "um\u00b7ri\u00df\u00b7rein\u00b7lich", ",", "fein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "$,", "KON", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Fiel Alles, fern und nah, dem satten Auge ein.", "tokens": ["Fiel", "Al\u00b7les", ",", "fern", "und", "nah", ",", "dem", "sat\u00b7ten", "Au\u00b7ge", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ADJD", "KON", "ADJD", "$,", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Zacken des Gebirgs scharf vor dem Himmelsblau,", "tokens": ["Die", "Za\u00b7cken", "des", "Ge\u00b7birgs", "scharf", "vor", "dem", "Him\u00b7mels\u00b7blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich sah der Schroffen Grat, der Schr\u00fcnde Spalt genau,", "tokens": ["Ich", "sah", "der", "Schrof\u00b7fen", "Grat", ",", "der", "Schr\u00fcn\u00b7de", "Spalt", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wenn zur D\u00e4mmerzeit der Mondkahn dr\u00fcber schwamm,", "tokens": ["Und", "wenn", "zur", "D\u00e4m\u00b7mer\u00b7zeit", "der", "Mond\u00b7kahn", "dr\u00fc\u00b7ber", "schwamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "War silber\u00fcberblitzt der blaue H\u00f6henkamm.", "tokens": ["War", "sil\u00b7be\u00b7r\u00fc\u00b7berb\u00b7litzt", "der", "blau\u00b7e", "H\u00f6\u00b7hen\u00b7kamm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der fernsten D\u00e4cher Rot, der weitsten W\u00e4lder Braun,", "tokens": ["Der", "ferns\u00b7ten", "D\u00e4\u00b7cher", "Rot", ",", "der", "weits\u00b7ten", "W\u00e4l\u00b7der", "Braun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich sah, wie weit es war, und konnt es nahe schaun,", "tokens": ["Ich", "sah", ",", "wie", "weit", "es", "war", ",", "und", "konnt", "es", "na\u00b7he", "schaun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "KON", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Selbst kleinster B\u00e4che Band, wie Silber eingestickt", "tokens": ["Selbst", "kleins\u00b7ter", "B\u00e4\u00b7che", "Band", ",", "wie", "Sil\u00b7ber", "ein\u00b7ge\u00b7stickt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NN", "$,", "PWAV", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dem Sammetdunkelrot, hab deutlich ich erblickt.", "tokens": ["Dem", "Sam\u00b7met\u00b7dun\u00b7kel\u00b7rot", ",", "hab", "deut\u00b7lich", "ich", "er\u00b7blickt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und heute. Eingebannt bin ich in kleinem Raum,", "tokens": ["Und", "heu\u00b7te", ".", "Ein\u00b7ge\u00b7bannt", "bin", "ich", "in", "klei\u00b7nem", "Raum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "VVPP", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nahe Dorfgeh\u00f6lz, seh ich als Schleier kaum.", "tokens": ["Das", "na\u00b7he", "Dorf\u00b7ge\u00b7h\u00f6lz", ",", "seh", "ich", "als", "Schlei\u00b7er", "kaum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "KOUS", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es f\u00e4llt ein schneller Schnee, breitflockig, dicht gedr\u00e4ngt,", "tokens": ["Es", "f\u00e4llt", "ein", "schnel\u00b7ler", "Schnee", ",", "breit\u00b7flo\u00b7ckig", ",", "dicht", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ADJD", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hat in leeres Grau mich dr\u00fcckend eingeengt.", "tokens": ["Und", "hat", "in", "lee\u00b7res", "Grau", "mich", "dr\u00fc\u00b7ckend", "ein\u00b7ge\u00b7engt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo ist der See, der Wald, der blaue H\u00f6henkamm,", "tokens": ["Wo", "ist", "der", "See", ",", "der", "Wald", ",", "der", "blau\u00b7e", "H\u00f6\u00b7hen\u00b7kamm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Darauf der Silberkahn des halben Mondes schwamm?", "tokens": ["Da\u00b7rauf", "der", "Sil\u00b7ber\u00b7kahn", "des", "hal\u00b7ben", "Mon\u00b7des", "schwamm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie bin ich pl\u00f6tzlich arm. Ein K\u00f6nig im Exil,", "tokens": ["Wie", "bin", "ich", "pl\u00f6tz\u00b7lich", "arm", ".", "Ein", "K\u00f6\u00b7nig", "im", "E\u00b7xil", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "ADJD", "$.", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem \u00fcber Nacht vom Haupt die goldene Krone fiel.", "tokens": ["Dem", "\u00fc\u00b7ber", "Nacht", "vom", "Haupt", "die", "gol\u00b7de\u00b7ne", "Kro\u00b7ne", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Er legt von sich den Prunk, die Pracht, die Macht, den Tand,", "tokens": ["Er", "legt", "von", "sich", "den", "Prunk", ",", "die", "Pracht", ",", "die", "Macht", ",", "den", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und in sich selbst entdeckt er tief ein neues Land,", "tokens": ["Und", "in", "sich", "selbst", "ent\u00b7deckt", "er", "tief", "ein", "neu\u00b7es", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "ADV", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das nie er noch geschaut, das, unver\u00e4u\u00dferlich,", "tokens": ["Das", "nie", "er", "noch", "ge\u00b7schaut", ",", "das", ",", "un\u00b7ver\u00b7\u00e4u\u00b7\u00dfer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "ADV", "VVPP", "$,", "PDS", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein reiches K\u00f6nigreich: staunend entdeckt er \u2013 sich.", "tokens": ["Ein", "rei\u00b7ches", "K\u00f6\u00b7nig\u00b7reich", ":", "stau\u00b7nend", "ent\u00b7deckt", "er", "\u2013", "sich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "VVFIN", "PPER", "$(", "PRF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Mein Auge ward beraubt, mein Herz ward reich beschenkt,", "tokens": ["Mein", "Au\u00b7ge", "ward", "be\u00b7raubt", ",", "mein", "Herz", "ward", "reich", "be\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das in sich selber sich mit stiller Kraft versenkt.", "tokens": ["Das", "in", "sich", "sel\u00b7ber", "sich", "mit", "stil\u00b7ler", "Kraft", "ver\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PRF", "ADV", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dort lag der See gewellt, ein blauer Schimmerplan,", "tokens": ["Dort", "lag", "der", "See", "ge\u00b7wellt", ",", "ein", "blau\u00b7er", "Schim\u00b7mer\u00b7plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+