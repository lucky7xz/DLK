.", "tokens": ["Und", "hat", "in", "lee\u00b7res", "Grau", "mich", "dr\u00fc\u00b7ckend", "ein\u00b7ge\u00b7engt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo ist der See, der Wald, der blaue H\u00f6henkamm,", "tokens": ["Wo", "ist", "der", "See", ",", "der", "Wald", ",", "der", "blau\u00b7e", "H\u00f6\u00b7hen\u00b7kamm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Darauf der Silberkahn des halben Mondes schwamm?", "tokens": ["Da\u00b7rauf", "der", "Sil\u00b7ber\u00b7kahn", "des", "hal\u00b7ben", "Mon\u00b7des", "schwamm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie bin ich pl\u00f6tzlich arm. Ein K\u00f6nig im Exil,", "tokens": ["Wie", "bin", "ich", "pl\u00f6tz\u00b7lich", "arm", ".", "Ein", "K\u00f6\u00b7nig", "im", "E\u00b7xil", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "ADJD", "$.", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem \u00fcber Nacht vom Haupt die goldene Krone fiel.", "tokens": ["Dem", "\u00fc\u00b7ber", "Nacht", "vom", "Haupt", "die", "gol\u00b7de\u00b7ne", "Kro\u00b7ne", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "AP