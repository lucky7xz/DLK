bic.hexa"}, "line.104": {"text": "Da ich dich wiederum vertraulich k\u00fcssen mag.", "tokens": ["Da", "ich", "dich", "wie\u00b7de\u00b7rum", "ver\u00b7trau\u00b7lich", "k\u00fcs\u00b7sen", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}