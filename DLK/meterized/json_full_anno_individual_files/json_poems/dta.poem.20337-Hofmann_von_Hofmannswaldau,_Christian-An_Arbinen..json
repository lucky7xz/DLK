{"dta.poem.20337": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "An Arbinen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Arbine/ meine hand geht itzund tieff im leide/", "tokens": ["Ar\u00b7bi\u00b7ne", "/", "mei\u00b7ne", "hand", "geht", "it\u00b7zund", "tieff", "im", "lei\u00b7de", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "APPRART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie hat gleich wie mein hertz die trauer angelegt/", "tokens": ["Sie", "hat", "gleich", "wie", "mein", "hertz", "die", "trau\u00b7er", "an\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOKOM", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie wei\u00df von keinem schmuck/ und leidet keine seide;", "tokens": ["Sie", "wei\u00df", "von", "kei\u00b7nem", "schmuck", "/", "und", "lei\u00b7det", "kei\u00b7ne", "sei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$(", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem sich nichts als angst in ihren adern regt.", "tokens": ["In\u00b7dem", "sich", "nichts", "als", "angst", "in", "ih\u00b7ren", "a\u00b7dern", "reg\u00b7t."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PRF", "PIS", "KOKOM", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du wirst auch ihre schrifft nicht gar zu wohl erkennen/", "tokens": ["Du", "wirst", "auch", "ih\u00b7re", "schrifft", "nicht", "gar", "zu", "wohl", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "VVFIN", "PTKNEG", "ADV", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wem hertz und finger bebt/ der schreibt nicht allzu gut.", "tokens": ["Wem", "hertz", "und", "fin\u00b7ger", "bebt", "/", "der", "schreibt", "nicht", "all\u00b7zu", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VVFIN", "$(", "ART", "VVFIN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es scheint/ ich soll nicht mehr in freuden-flammen brennen/", "tokens": ["Es", "scheint", "/", "ich", "soll", "nicht", "mehr", "in", "freu\u00b7den\u00b7flam\u00b7men", "bren\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VMFIN", "PTKNEG", "ADV", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ach! da\u00df der himmel mir so grosses unrecht thut.", "tokens": ["Ach", "!", "da\u00df", "der", "him\u00b7mel", "mir", "so", "gros\u00b7ses", "un\u00b7recht", "thut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "ART", "NN", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wo ist die edle zeit/ wo seyn die s\u00fcssen stunden?", "tokens": ["Wo", "ist", "die", "ed\u00b7le", "zeit", "/", "wo", "seyn", "die", "s\u00fcs\u00b7sen", "stun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$(", "PWAV", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Genie\u00df ich dann nicht mehr der heissen liebe pfand?", "tokens": ["Ge\u00b7nie\u00df", "ich", "dann", "nicht", "mehr", "der", "heis\u00b7sen", "lie\u00b7be", "pfand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ach die vertraulichkeit ist allbereit verschwunden:", "tokens": ["Ach", "die", "ver\u00b7trau\u00b7lich\u00b7keit", "ist", "all\u00b7be\u00b7reit", "ver\u00b7schwun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vor schmertzen f\u00e4llet mir die feder aus der hand.", "tokens": ["Vor", "schmert\u00b7zen", "f\u00e4l\u00b7let", "mir", "die", "fe\u00b7der", "aus", "der", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich z\u00fcrn itzt auff mich selbst. Verwirrung der gedancken", "tokens": ["Ich", "z\u00fcrn", "itzt", "auff", "mich", "selbst", ".", "Ver\u00b7wir\u00b7rung", "der", "ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ADV", "$.", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ist meine morgen-kost und auch mein abend-brod.", "tokens": ["Ist", "mei\u00b7ne", "mor\u00b7gen\u00b7kost", "und", "auch", "mein", "a\u00b7ben\u00b7dbrod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es scheint/ da\u00df \u00fcberall die freundschaffts-pfeiler wancken/", "tokens": ["Es", "scheint", "/", "da\u00df", "\u00fc\u00b7be\u00b7rall", "die", "freund\u00b7schaffts\u00b7pfei\u00b7ler", "wan\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und meine freundin wird zur schmidin meiner noth.", "tokens": ["Und", "mei\u00b7ne", "freun\u00b7din", "wird", "zur", "schmi\u00b7din", "mei\u00b7ner", "noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Arbine liebstu mich? Du sagsts: Ich mu\u00df es glauben.", "tokens": ["Ar\u00b7bi\u00b7ne", "liebs\u00b7tu", "mich", "?", "Du", "sagsts", ":", "Ich", "mu\u00df", "es", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Durch nasse zeugen will der schwur versiegelt seyn.", "tokens": ["Durch", "nas\u00b7se", "zeu\u00b7gen", "will", "der", "schwur", "ver\u00b7sie\u00b7gelt", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "VMFIN", "ART", "VVFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du liebst und wilst mich doch der liebe frucht berauben/", "tokens": ["Du", "liebst", "und", "wilst", "mich", "doch", "der", "lie\u00b7be", "frucht", "be\u00b7rau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und f\u00fchrst aus \u00f6den mich in d\u00fcrre felder ein.", "tokens": ["Und", "f\u00fchrst", "aus", "\u00f6\u00b7den", "mich", "in", "d\u00fcr\u00b7re", "fel\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du wirst hink\u00fcnfftig mir die augen noch verbinden/", "tokens": ["Du", "wirst", "hin\u00b7k\u00fcnff\u00b7tig", "mir", "die", "au\u00b7gen", "noch", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PPER", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es paart sich grausamkeit und liebe nicht zu wohl/", "tokens": ["Es", "paart", "sich", "grau\u00b7sam\u00b7keit", "und", "lie\u00b7be", "nicht", "zu", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "VVFIN", "PTKNEG", "PTKA", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich kan mich warlich nicht in deinen willen finden/", "tokens": ["Ich", "kan", "mich", "war\u00b7lich", "nicht", "in", "dei\u00b7nen", "wil\u00b7len", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Du lehrst mich denn zuvor/ wie ich dich lieben soll.", "tokens": ["Du", "lehrst", "mich", "denn", "zu\u00b7vor", "/", "wie", "ich", "dich", "lie\u00b7ben", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$(", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Mit was hat doch dein freund die h\u00e4rtigkeit verschuldet/", "tokens": ["Mit", "was", "hat", "doch", "dein", "freund", "die", "h\u00e4r\u00b7tig\u00b7keit", "ver\u00b7schul\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df ihn dein herber schlu\u00df mit solchen jammer tr\u00e4nckt?", "tokens": ["Da\u00df", "ihn", "dein", "her\u00b7ber", "schlu\u00df", "mit", "sol\u00b7chen", "jam\u00b7mer", "tr\u00e4nckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und mit vergessenheit und unlust ihn besoldet?", "tokens": ["Und", "mit", "ver\u00b7ges\u00b7sen\u00b7heit", "und", "un\u00b7lust", "ihn", "be\u00b7sol\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ach w\u00fcrd\u2019 ich doch zugleich itzt in ein grab gesenckt!", "tokens": ["Ach", "w\u00fcrd'", "ich", "doch", "zu\u00b7gleich", "itzt", "in", "ein", "grab", "ge\u00b7senckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So l\u00e4g ich in der ruh befreyt von allen plagen/", "tokens": ["So", "l\u00e4g", "ich", "in", "der", "ruh", "be\u00b7freyt", "von", "al\u00b7len", "pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "APPR", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es fielen mich nicht mehr die ungl\u00fccks-wellen an:", "tokens": ["Es", "fie\u00b7len", "mich", "nicht", "mehr", "die", "un\u00b7gl\u00fccks\u00b7wel\u00b7len", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wei\u00df/ du w\u00fcrdest doch nach meinem tode sagen:", "tokens": ["Ich", "wei\u00df", "/", "du", "w\u00fcr\u00b7dest", "doch", "nach", "mei\u00b7nem", "to\u00b7de", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hier ruht ein werther freund/ dem ich zuviel gethan.", "tokens": ["Hier", "ruht", "ein", "wert\u00b7her", "freund", "/", "dem", "ich", "zu\u00b7viel", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "PRELS", "PPER", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach! k\u00f6nt ich seinen leib dem bleichen schnee entf\u00fchren/", "tokens": ["Ach", "!", "k\u00f6nt", "ich", "sei\u00b7nen", "leib", "dem", "blei\u00b7chen", "schnee", "ent\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Er w\u00fcrde nicht wie er durch liebes durst verzehrt:", "tokens": ["Er", "w\u00fcr\u00b7de", "nicht", "wie", "er", "durch", "lie\u00b7bes", "durst", "ver\u00b7zehrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PWAV", "PPER", "APPR", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Er solte nichts als milch erhitzter k\u00fcsse sp\u00fcren;", "tokens": ["Er", "sol\u00b7te", "nichts", "als", "milch", "er\u00b7hitz\u00b7ter", "k\u00fcs\u00b7se", "sp\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "KOKOM", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Denn seine g\u00f6ldne treu ist solcher s\u00e4ffte werth.", "tokens": ["Denn", "sei\u00b7ne", "g\u00f6ld\u00b7ne", "treu", "ist", "sol\u00b7cher", "s\u00e4ff\u00b7te", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJD", "VAFIN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Verzeihe freundin doch der freyen art zu schreiben:", "tokens": ["Ver\u00b7zei\u00b7he", "freun\u00b7din", "doch", "der", "frey\u00b7en", "art", "zu", "schrei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Du schaust allhier die frucht verwirrter ungedult/", "tokens": ["Du", "schaust", "all\u00b7hier", "die", "frucht", "ver\u00b7wirr\u00b7ter", "un\u00b7ge\u00b7dult", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Kan witz und feder nicht in ihren angeln bleiben.", "tokens": ["Kan", "witz", "und", "fe\u00b7der", "nicht", "in", "ih\u00b7ren", "an\u00b7geln", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So tadle doch nicht mich/ es bleibet deine schuld/", "tokens": ["So", "tad\u00b7le", "doch", "nicht", "mich", "/", "es", "blei\u00b7bet", "dei\u00b7ne", "schuld", "/"], "token_info": ["word", "word", "w