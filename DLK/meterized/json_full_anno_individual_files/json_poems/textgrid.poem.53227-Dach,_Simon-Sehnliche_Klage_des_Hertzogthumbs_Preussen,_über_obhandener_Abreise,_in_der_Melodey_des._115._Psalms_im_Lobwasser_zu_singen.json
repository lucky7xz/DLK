{"textgrid.poem.53227": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Sehnliche Klage des Hertzogthumbs Preussen, \u00fcber obhandener Abreise, in der Melodey des. 115. Psalms im Lobwasser zu singen", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo Thr\u00e4nen, Held, Dir zu Gem\u00fchte gehn,", "tokens": ["Wo", "Thr\u00e4\u00b7nen", ",", "Held", ",", "Dir", "zu", "Ge\u00b7m\u00fch\u00b7te", "gehn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "$,", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So sieh' allhie betr\u00fcbt mich vor Dir stehn,", "tokens": ["So", "sieh'", "all\u00b7hie", "be\u00b7tr\u00fcbt", "mich", "vor", "Dir", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "VVFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ja auff der Erden liegen:", "tokens": ["Ja", "auff", "der", "Er\u00b7den", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die H\u00e4nd' heb ich empor zu GOTT und Dir,", "tokens": ["Die", "H\u00e4nd'", "heb", "ich", "em\u00b7por", "zu", "GoTT", "und", "Dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "APPR", "NE", "KON", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bezeuge Dich und deines Hauses Zier,", "tokens": ["Be\u00b7zeu\u00b7ge", "Dich", "und", "dei\u00b7nes", "Hau\u00b7ses", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df ich nicht Trost mag kriegen.", "tokens": ["Da\u00df", "ich", "nicht", "Trost", "mag", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Abzug setzt mich in zu schweres Leidt,", "tokens": ["Dein", "Ab\u00b7zug", "setzt", "mich", "in", "zu", "schwe\u00b7res", "Leidt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich suche nur zu lieben Traurigkeit,", "tokens": ["Ich", "su\u00b7che", "nur", "zu", "lie\u00b7ben", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alle Lust zu meiden,", "tokens": ["Und", "al\u00b7le", "Lust", "zu", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Dir erseufftzt Jung, Alt und Arm und Reich,", "tokens": ["Nach", "Dir", "er\u00b7seufftzt", "Jung", ",", "Alt", "und", "Arm", "und", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "NN", "$,", "ADJD", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht Du allein, ich nebenst dir zugleich", "tokens": ["Nicht", "Du", "al\u00b7lein", ",", "ich", "ne\u00b7benst", "dir", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "PPER", "ADV", "$,", "PPER", "APPR", "PPER", "ADV"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mu\u00df jetzund von mir scheiden.", "tokens": ["Mu\u00df", "je\u00b7tzund", "von", "mir", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo zeuchst Du hin? von dar ich Dich empfieng", "tokens": ["Wo", "zeuchst", "Du", "hin", "?", "von", "dar", "ich", "Dich", "emp\u00b7fi\u00b7eng"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "PTKVZ", "PPER", "PPER", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Noch fast halb todt, und wo dein Leben hieng", "tokens": ["Noch", "fast", "halb", "todt", ",", "und", "wo", "dein", "Le\u00b7ben", "hieng"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "ADJD", "$,", "KON", "PWAV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An einem Seiden Faden.", "tokens": ["An", "ei\u00b7nem", "Sei\u00b7den", "Fa\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zwar, Herr, es ist dein Erb- und Vater-Land,", "tokens": ["Zwar", ",", "Herr", ",", "es", "ist", "dein", "Er\u00b7b", "und", "Va\u00b7ter\u00b7Land", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Das suchet recht jhm Fried und gutten Stand", "tokens": ["Das", "su\u00b7chet", "recht", "jhm", "Fried", "und", "gut\u00b7ten", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "PPER", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Im Anblick deiner Gnaden.", "tokens": ["Im", "An\u00b7blick", "dei\u00b7ner", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Soll aber ich daneben immerdar", "tokens": ["Soll", "a\u00b7ber", "ich", "da\u00b7ne\u00b7ben", "im\u00b7mer\u00b7dar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "PAV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Furchten stehn, Du m\u00f6chtest in Gefahr", "tokens": ["In", "Furch\u00b7ten", "stehn", ",", "Du", "m\u00f6ch\u00b7test", "in", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "$,", "PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Todes-N\u00f6then schweben?", "tokens": ["Und", "To\u00b7des\u00b7N\u00f6\u00b7then", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hie kan ich Dich f\u00fcr meinen Augen sehn,", "tokens": ["Hie", "kan", "ich", "Dich", "f\u00fcr", "mei\u00b7nen", "Au\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nur Botschafft mu\u00df, was dort mit Dir geschehn", "tokens": ["Nur", "Bot\u00b7schafft", "mu\u00df", ",", "was", "dort", "mit", "Dir", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VMFIN", "$,", "PRELS", "ADV", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mir zu verstehen geben.", "tokens": ["Mir", "zu", "ver\u00b7ste\u00b7hen", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "O da\u00df ich jetzt der Himmel k\u00f6ndte seyn!", "tokens": ["O", "da\u00df", "ich", "jetzt", "der", "Him\u00b7mel", "k\u00f6nd\u00b7te", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "ART", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich wolte stets mit tausent \u00c4ugelein", "tokens": ["Ich", "wol\u00b7te", "stets", "mit", "tau\u00b7sent", "\u00c4u\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von oben auff Dich schawen:", "tokens": ["Von", "o\u00b7ben", "auff", "Dich", "scha\u00b7wen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich w\u00fcrde Dich bewachen, O mein Liecht,", "tokens": ["Ich", "w\u00fcr\u00b7de", "Dich", "be\u00b7wa\u00b7chen", ",", "O", "mein", "Liecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zu Tag' und Nacht, und d\u00f6rffte sorglich nicht", "tokens": ["Zu", "Tag'", "und", "Nacht", ",", "und", "d\u00f6rff\u00b7te", "sorg\u00b7lich", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KON", "VVFIN", "ADV", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nur falscher Zeitung trawen.", "tokens": ["Nur", "fal\u00b7scher", "Zei\u00b7tung", "tra\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was aber n\u00fctzt mein sehnliches Geschrey?", "tokens": ["Was", "a\u00b7ber", "n\u00fctzt", "mein", "sehn\u00b7li\u00b7ches", "Ge\u00b7schrey", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du eilest fort, und l\u00e4ssest mich dabey", "tokens": ["Du", "ei\u00b7lest", "fort", ",", "und", "l\u00e4s\u00b7sest", "mich", "da\u00b7bey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "PRF", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Wiederkunfft nicht wissen,", "tokens": ["Die", "Wie\u00b7der\u00b7kunfft", "nicht", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und kanst vielleicht, wie sehr ich bin betr\u00fcbt,", "tokens": ["Und", "kanst", "viel\u00b7leicht", ",", "wie", "sehr", "ich", "bin", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "$,", "PWAV", "ADV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In deine Marck und Brandenburg verliebt", "tokens": ["In", "dei\u00b7ne", "Marck", "und", "Bran\u00b7den\u00b7burg", "ver\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NE", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mich au\u00df dem Hertzen schliessen.", "tokens": ["Mich", "au\u00df", "dem", "Hert\u00b7zen", "schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "Warumb ich Dich bey deiner Hoheit Pracht,", "tokens": ["Wa\u00b7rumb", "ich", "Dich", "bey", "dei\u00b7ner", "Ho\u00b7heit", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die alles die\u00df, was Welt ist, schamrot macht,", "tokens": ["Die", "al\u00b7les", "die\u00df", ",", "was", "Welt", "ist", ",", "scham\u00b7rot", "macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "PDS", "$,", "PRELS", "NN", "VAFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wil unterth\u00e4nigst bitten,", "tokens": ["Wil", "un\u00b7ter\u00b7th\u00e4\u00b7nigst", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Komm wieder, komm zu Deiner trewen Magd,", "tokens": ["Komm", "wie\u00b7der", ",", "komm", "zu", "Dei\u00b7ner", "tre\u00b7wen", "Magd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die gute Nacht Dir jetzt mit Thr\u00e4nen sagt,", "tokens": ["Die", "gu\u00b7te", "Nacht", "Dir", "jetzt", "mit", "Thr\u00e4\u00b7nen", "sagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Furcht und Angst bestritten.", "tokens": ["Die", "Furcht", "und", "Angst", "be\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der H\u00f6chste Gott und seiner Gnaden Schein", "tokens": ["Der", "H\u00f6chs\u00b7te", "Gott", "und", "sei\u00b7ner", "Gna\u00b7den", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Woll \u00fcberall indessen umb Dich seyn,", "tokens": ["Woll", "\u00fc\u00b7be\u00b7rall", "in\u00b7des\u00b7sen", "umb", "Dich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir H\u00fclff und Schutz gewehren.", "tokens": ["Dir", "H\u00fclff", "und", "Schutz", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir lege sich des Wetters Sturm und Wust,", "tokens": ["Dir", "le\u00b7ge", "sich", "des", "Wet\u00b7ters", "Sturm", "und", "Wust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dir m\u00fcsse Frost und Schnee in beste Lust", "tokens": ["Dir", "m\u00fcs\u00b7se", "Frost", "und", "Schnee", "in", "bes\u00b7te", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des VorJahrs sich verkehren.", "tokens": ["Des", "Vor", "Jahrs", "sich", "ver\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Vertausch mit Ruh dort alle Krieges-Macht,", "tokens": ["Ver\u00b7tausch", "mit", "Ruh", "dort", "al\u00b7le", "Krie\u00b7ge\u00b7sMacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nimm aber Dich daneben wol in acht.", "tokens": ["Nimm", "a\u00b7ber", "Dich", "da\u00b7ne\u00b7ben", "wol", "in", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "PAV", "ADV", "APPR", "CARD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich nehme, Held, f\u00fcr allen", "tokens": ["Ich", "neh\u00b7me", ",", "Held", ",", "f\u00fcr", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$,", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu zeugen Dich, mein bestes Wolergehn,", "tokens": ["Zu", "zeu\u00b7gen", "Dich", ",", "mein", "bes\u00b7tes", "Wo\u00b7ler\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich kan allein durch Deinen Stand nur stehn,", "tokens": ["Ich", "kan", "al\u00b7lein", "durch", "Dei\u00b7nen", "Stand", "nur", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Durch Deinen Fall auch fallen.", "tokens": ["Durch", "Dei\u00b7nen", "Fall", "auch", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wo Thr\u00e4nen, Held, Dir zu Gem\u00fchte gehn,", "tokens": ["Wo", "Thr\u00e4\u00b7nen", ",", "Held", ",", "Dir", "zu", "Ge\u00b7m\u00fch\u00b7te", "gehn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "$,", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So sieh' allhie betr\u00fcbt mich vor Dir stehn,", "tokens": ["So", "sieh'", "all\u00b7hie", "be\u00b7tr\u00fcbt", "mich", "vor", "Dir", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "VVFIN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"te