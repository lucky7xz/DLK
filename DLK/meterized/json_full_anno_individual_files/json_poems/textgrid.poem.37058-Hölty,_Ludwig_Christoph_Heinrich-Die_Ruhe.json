{"textgrid.poem.37058": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Ruhe", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tochter Edens, o Ruh, die du die Finsterni\u00df", "tokens": ["Toch\u00b7ter", "E\u00b7dens", ",", "o", "Ruh", ",", "die", "du", "die", "Fins\u00b7ter\u00b7ni\u00df"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NE", "$,", "FM", "NN", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Stiller Hayne bewohnst, unter der D\u00e4mmerung", "tokens": ["Stil\u00b7ler", "Hay\u00b7ne", "be\u00b7wohnst", ",", "un\u00b7ter", "der", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Mondversilberter Pappeln,", "tokens": ["Mond\u00b7ver\u00b7sil\u00b7ber\u00b7ter", "Pap\u00b7peln", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mit verschlungenen Armen, weilst;", "tokens": ["Mit", "ver\u00b7schlun\u00b7ge\u00b7nen", "Ar\u00b7men", ",", "weilst", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Mit dem Sch\u00e4fer im Hayn fl\u00f6test, der Sch\u00e4ferin,", "tokens": ["Mit", "dem", "Sch\u00e4\u00b7fer", "im", "Hayn", "fl\u00f6\u00b7test", ",", "der", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Unter Blumen der Au, l\u00e4chelnd entgegenkommst,", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "der", "Au", ",", "l\u00e4\u00b7chelnd", "ent\u00b7ge\u00b7gen\u00b7kommst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und dem Schellengeklingel", "tokens": ["Und", "dem", "Schel\u00b7len\u00b7ge\u00b7klin\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihrer tanzenden Sch\u00e4fchen lauschst.", "tokens": ["Ih\u00b7rer", "tan\u00b7zen\u00b7den", "Sch\u00e4f\u00b7chen", "lauschst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Wie der J\u00fcngling die Braut liebet, so lieb ich dich,", "tokens": ["Wie", "der", "J\u00fcng\u00b7ling", "die", "Braut", "lie\u00b7bet", ",", "so", "lieb", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ADV", "ADJD", "PPER", "PRF", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Allgef\u00e4llige Ruh! sp\u00e4hte dir immer nach,", "tokens": ["All\u00b7ge\u00b7f\u00e4l\u00b7li\u00b7ge", "Ruh", "!", "sp\u00e4h\u00b7te", "dir", "im\u00b7mer", "nach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Bald auf blumigen Wiesen,", "tokens": ["Bald", "auf", "blu\u00b7mi\u00b7gen", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Bald im Busche der Nachtigall.", "tokens": ["Bald", "im", "Bu\u00b7sche", "der", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Wie der Pilger den Quell suchet, so sucht ich dich,", "tokens": ["Wie", "der", "Pil\u00b7ger", "den", "Quell", "su\u00b7chet", ",", "so", "sucht", "ich", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ach, und seufzete: Ruh, bist du, wie Morgentraum,", "tokens": ["Ach", ",", "und", "seuf\u00b7ze\u00b7te", ":", "Ruh", ",", "bist", "du", ",", "wie", "Mor\u00b7gen\u00b7traum", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "VVFIN", "$.", "NN", "$,", "VAFIN", "PPER", "$,", "PWAV", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Mit den Jahren der Kindheit,", "tokens": ["Mit", "den", "Jah\u00b7ren", "der", "Kind\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dann auf ewig von mir entflohn?", "tokens": ["Dann", "auf", "e\u00b7wig", "von", "mir", "ent\u00b7flohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Endlich fl\u00fcsterst du mir, Herzenerfreuerin,", "tokens": ["End\u00b7lich", "fl\u00fcs\u00b7terst", "du", "mir", ",", "Her\u00b7ze\u00b7ner\u00b7freu\u00b7e\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mit dem Wehen des Strauchs, Wehen des Uferschilfs,", "tokens": ["Mit", "dem", "We\u00b7hen", "des", "Strauchs", ",", "We\u00b7hen", "des", "U\u00b7fer\u00b7schilfs", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Mit dem Zittern des Laubes,", "tokens": ["Mit", "dem", "Zit\u00b7tern", "des", "Lau\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Deinen Himmel in meine Brust!", "tokens": ["Dei\u00b7nen", "Him\u00b7mel", "in", "mei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Ueberirdisch Gef\u00fchl s\u00e4uselt mich an, und bebt", "tokens": ["Ue\u00b7be\u00b7rir\u00b7disch", "Ge\u00b7f\u00fchl", "s\u00e4u\u00b7selt", "mich", "an", ",", "und", "bebt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Durch mein innerstes Mark! Ri\u00df sich der Himmel auf,", "tokens": ["Durch", "mein", "in\u00b7ners\u00b7tes", "Mark", "!", "Ri\u00df", "sich", "der", "Him\u00b7mel", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "NN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kam die Pilgerin Edens,", "tokens": ["Kam", "die", "Pil\u00b7ge\u00b7rin", "E\u00b7dens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wonne Gottes, herab zu mir?", "tokens": ["Won\u00b7ne", "Got\u00b7tes", ",", "her\u00b7ab", "zu", "mir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Jeder Lispel des Baums, jedes Ger\u00e4usch des Bachs,", "tokens": ["Je\u00b7der", "Lis\u00b7pel", "des", "Baums", ",", "je\u00b7des", "Ge\u00b7r\u00e4usch", "des", "Bachs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Jedes l\u00e4ndliche Lied, welches dem Dorf entschallt,", "tokens": ["Je\u00b7des", "l\u00e4nd\u00b7li\u00b7che", "Lied", ",", "wel\u00b7ches", "dem", "Dorf", "ent\u00b7schallt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Ist mir Zauber der Sph\u00e4ren,", "tokens": ["Ist", "mir", "Zau\u00b7ber", "der", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und Gefl\u00fcster der Seraphim.", "tokens": ["Und", "Ge\u00b7fl\u00fcs\u00b7ter", "der", "Se\u00b7ra\u00b7phim", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.8": {"line.1": {"text": "Hingego\u00dfen auf Thau, blick ich den Abendstern,", "tokens": ["Hin\u00b7ge\u00b7go\u00b7\u00dfen", "auf", "Thau", ",", "blick", "ich", "den", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Den Gespielen der Ruh, blick ich den Mond hinan,", "tokens": ["Den", "Ge\u00b7spie\u00b7len", "der", "Ruh", ",", "blick", "ich", "den", "Mond", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Der so freundlich, so freundlich", "tokens": ["Der", "so", "freund\u00b7lich", ",", "so", "freund\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Durch die nickenden Wipfel schaut.", "tokens": ["Durch", "die", "ni\u00b7cken\u00b7den", "Wip\u00b7fel", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Ruh, o l\u00e4chle mir stets, wie du mir l\u00e4cheltest,", "tokens": ["Ruh", ",", "o", "l\u00e4ch\u00b7le", "mir", "stets", ",", "wie", "du", "mir", "l\u00e4\u00b7chel\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Als mein wallendes Haar, mit der entknospeten", "tokens": ["Als", "mein", "wal\u00b7len\u00b7des", "Haar", ",", "mit", "der", "ent\u00b7knos\u00b7pe\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Fr\u00fchlingsblume bekr\u00e4nzet,", "tokens": ["Fr\u00fch\u00b7lings\u00b7blu\u00b7me", "be\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Abendwinden zum Spiele flog.", "tokens": ["Ab\u00b7end\u00b7win\u00b7den", "zum", "Spie\u00b7le", "flog", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Keiner St\u00e4dterin Reiz, weder ein blaues Aug,", "tokens": ["Kei\u00b7ner", "St\u00e4d\u00b7te\u00b7rin", "Reiz", ",", "we\u00b7der", "ein", "blau\u00b7es", "Aug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Noch ein ku\u00dflicher Mund, soll mich aus deinem Arm,", "tokens": ["Noch", "ein", "ku\u00df\u00b7li\u00b7cher", "Mund", ",", "soll", "mich", "aus", "dei\u00b7nem", "Arm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Zu den Hallen des Tanzes", "tokens": ["Zu", "den", "Hal\u00b7len", "des", "Tan\u00b7zes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Locken, oder des Opernspiels.", "tokens": ["Lo\u00b7cken", ",", "o\u00b7der", "des", "O\u00b7pern\u00b7spiels", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Weile, weile bey mir, unter dem H\u00fcttendach,", "tokens": ["Wei\u00b7le", ",", "wei\u00b7le", "bey", "mir", ",", "un\u00b7ter", "dem", "H\u00fct\u00b7ten\u00b7dach", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Allgef\u00e4llige Ruh, bis du mich an der Hand", "tokens": ["All\u00b7ge\u00b7f\u00e4l\u00b7li\u00b7ge", "Ruh", ",", "bis", "du", "mich", "an", "der", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Eines Engels den Lauben", "tokens": ["Ei\u00b7nes", "En\u00b7gels", "den", "Lau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der Verkl\u00e4rten entgegenf\u00fchrst.", "tokens": ["Der", "Ver\u00b7kl\u00e4r\u0