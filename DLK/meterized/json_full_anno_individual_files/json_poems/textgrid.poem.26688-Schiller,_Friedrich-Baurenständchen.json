{"textgrid.poem.26688": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Baurenst\u00e4ndchen", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mensch! Ich bitte, guck heraus!", "tokens": ["Mensch", "!", "Ich", "bit\u00b7te", ",", "guck", "he\u00b7raus", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "$,", "VVIMP", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Klecken nicht zwo Stunden,", "tokens": ["Kle\u00b7cken", "nicht", "zwo", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Steh ich so vor deinem Haus,", "tokens": ["Steh", "ich", "so", "vor", "dei\u00b7nem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehe mit den Hunden.", "tokens": ["Ste\u00b7he", "mit", "den", "Hun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "'s regnet, was vom Himmel mag,", "tokens": ["'s", "reg\u00b7net", ",", "was", "vom", "Him\u00b7mel", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPRART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "'s gwittert wie zum J\u00fcngsten Tag,", "tokens": ["'s", "gwit\u00b7tert", "wie", "zum", "J\u00fcng\u00b7sten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Pudelna\u00df die Hosen!", "tokens": ["Pu\u00b7del\u00b7na\u00df", "die", "Ho\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Platschna\u00df Rock und Mantel, ei!", "tokens": ["Platsc\u00b7hna\u00df", "Rock", "und", "Man\u00b7tel", ",", "ei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "$,", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Rock und Mantel nagelneu,", "tokens": ["Rock", "und", "Man\u00b7tel", "na\u00b7gel\u00b7neu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Alles dieser Losen.", "tokens": ["Al\u00b7les", "die\u00b7ser", "Lo\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Drau\u00dfen, drau\u00dfen Saus und Braus!", "tokens": ["Drau\u00b7\u00dfen", ",", "drau\u00b7\u00dfen", "Saus", "und", "Braus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Mensch! ich bitte, guck heraus.", "tokens": ["Mensch", "!", "ich", "bit\u00b7te", ",", "guck", "he\u00b7raus", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADV", "$,", "VVIMP", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ei zum Henker! guck heraus!", "tokens": ["Ei", "zum", "Hen\u00b7ker", "!", "guck", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "VVIMP", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00f6scht mir die Laterne \u2013", "tokens": ["L\u00f6scht", "mir", "die", "La\u00b7ter\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Weit am Himmel Nacht und Graus!", "tokens": ["Weit", "am", "Him\u00b7mel", "Nacht", "und", "Graus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weder Mond noch Sterne.", "tokens": ["We\u00b7der", "Mond", "noch", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sto\u00df ich schier an Stein und Stock,", "tokens": ["Sto\u00df", "ich", "schier", "an", "Stein", "und", "Stock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Rei\u00dfe Wams und \u00dcberrock,", "tokens": ["Rei\u00b7\u00dfe", "Wams", "und", "\u00dc\u00b7ber\u00b7rock", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach, da\u00df Gott erbarme!", "tokens": ["Ach", ",", "da\u00df", "Gott", "er\u00b7bar\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Hecken, Stauden ringsumher,", "tokens": ["He\u00b7cken", ",", "Stau\u00b7den", "rings\u00b7um\u00b7her", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Gr\u00e4ben, H\u00fcgel kreuz und quer,", "tokens": ["Gr\u00e4\u00b7ben", ",", "H\u00fc\u00b7gel", "kreuz", "und", "quer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Breche Bein und Arme.", "tokens": ["Bre\u00b7che", "Bein", "und", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Drau\u00dfen, drau\u00dfen Nacht und Graus!", "tokens": ["Drau\u00b7\u00dfen", ",", "drau\u00b7\u00dfen", "Nacht", "und", "Graus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Ei zum Henker, guck heraus!", "tokens": ["Ei", "zum", "Hen\u00b7ker", ",", "guck", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "VVIMP", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ei zum Teufel! guck heraus!", "tokens": ["Ei", "zum", "Teu\u00b7fel", "!", "guck", "he\u00b7raus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "VVIMP", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re mein Gesuche!", "tokens": ["H\u00f6\u00b7re", "mein", "Ge\u00b7su\u00b7che", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Beten, Singen geht mir aus,", "tokens": ["Be\u00b7ten", ",", "Sin\u00b7gen", "geht", "mir", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Willst du, da\u00df ich fluche?", "tokens": ["Willst", "du", ",", "da\u00df", "ich", "flu\u00b7che", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Mu\u00df ich doch ein Hans Dampf sein,", "tokens": ["Mu\u00df", "ich", "doch", "ein", "Hans", "Dampf", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NE", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Fr\u00f6r ich nicht zu Stein und Bein,", "tokens": ["Fr\u00f6r", "ich", "nicht", "zu", "Stein", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn ich l\u00e4nger bliebe?", "tokens": ["Wenn", "ich", "l\u00e4n\u00b7ger", "blie\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Liebe, das verdank ich dir,", "tokens": ["Lie\u00b7be", ",", "das", "ver\u00b7dank", "ich", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Winterbeulen machst du mir,", "tokens": ["Win\u00b7ter\u00b7beu\u00b7len", "machst", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Du vertrackte Liebe!", "tokens": ["Du", "ver\u00b7track\u00b7te", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Drau\u00dfen, drau\u00dfen Kalt und Graus!", "tokens": ["Drau\u00b7\u00dfen", ",", "drau\u00b7\u00dfen", "Kalt", "und", "Graus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Ei, zum Teufel, guck heraus.", "tokens": ["Ei", ",", "zum", "Teu\u00b7fel", ",", "guck", "he\u00b7raus", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "NN", "$,", "VVIMP", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Donner alle! Was ist das,", "tokens": ["Don\u00b7ner", "al\u00b7le", "!", "Was", "ist", "das", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "$.", "PWS", "VAFIN", "PDS", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Das vom Fenster regnet?", "tokens": ["Das", "vom", "Fens\u00b7ter", "reg\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Garstge Hexe, kotigna\u00df,", "tokens": ["Garst\u00b7ge", "He\u00b7xe", ",", "ko\u00b7ti\u00b7gna\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast mich eingesegnet.", "tokens": ["Hast", "mich", "ein\u00b7ge\u00b7seg\u00b7net", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Regen, Hunger, Frost und Wind", "tokens": ["Re\u00b7gen", ",", "Hun\u00b7ger", ",", "Frost", "und", "Wind"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leid ich f\u00fcr das Teufelskind,", "tokens": ["Leid", "ich", "f\u00fcr", "das", "Teu\u00b7fels\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Werde noch gehudelt!", "tokens": ["Wer\u00b7de", "noch", "ge\u00b7hu\u00b7delt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wetter auch! Ich packe mich!", "tokens": ["Wet\u00b7ter", "auch", "!", "Ich", "pa\u00b7cke", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "B\u00f6ser D\u00e4mon, tummle dich,", "tokens": ["B\u00f6\u00b7ser", "D\u00e4\u00b7mon", ",", "tumm\u00b7le", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Habe satt gedudelt!", "tokens": ["Ha\u00b7be", "satt", "ge\u00b7du\u00b7delt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Drau\u00dfen, drau\u00dfen Saus und Braus!", "tokens": ["Drau\u00b7\u00d