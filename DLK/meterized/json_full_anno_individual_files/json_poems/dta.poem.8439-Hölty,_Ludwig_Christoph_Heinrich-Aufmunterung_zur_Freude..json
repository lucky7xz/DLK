{"dta.poem.8439": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aufmunterung zur Freude.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "So lang' uns Lenz und Jugend bl\u00fchn?", "tokens": ["So", "lang'", "uns", "Lenz", "und", "Ju\u00b7gend", "bl\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wollt' in seinen Bl\u00fctentagen", "tokens": ["Wer", "wollt'", "in", "sei\u00b7nen", "Bl\u00fc\u00b7ten\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Stirn' in d\u00fcstre Falten ziehn?", "tokens": ["Die", "Stirn'", "in", "d\u00fcst\u00b7re", "Fal\u00b7ten", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Freude winkt auf allen Wegen,", "tokens": ["Die", "Freu\u00b7de", "winkt", "auf", "al\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die durch dies Pilgerleben gehn;", "tokens": ["Die", "durch", "dies", "Pil\u00b7ger\u00b7le\u00b7ben", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie bringt uns selbst den Kranz entgegen,", "tokens": ["Sie", "bringt", "uns", "selbst", "den", "Kranz", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann wir am Scheidewege stehn.", "tokens": ["Wann", "wir", "am", "Schei\u00b7de\u00b7we\u00b7ge", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch rinnt und rauscht die Wiesenquelle;", "tokens": ["Noch", "rinnt", "und", "rauscht", "die", "Wie\u00b7sen\u00b7quel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch ist die Laube k\u00fchl und gr\u00fcn;", "tokens": ["Noch", "ist", "die", "Lau\u00b7be", "k\u00fchl", "und", "gr\u00fcn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch scheint der liebe Mond so helle,", "tokens": ["Noch", "scheint", "der", "lie\u00b7be", "Mond", "so", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie er durch Adams B\u00e4ume schien!", "tokens": ["Wie", "er", "durch", "A\u00b7dams", "B\u00e4u\u00b7me", "schien", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch macht der Saft der Purpurtraube", "tokens": ["Noch", "macht", "der", "Saft", "der", "Pur\u00b7pur\u00b7trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Menscheu krankes Herz gesund;", "tokens": ["Des", "Men\u00b7scheu", "kran\u00b7kes", "Herz", "ge\u00b7sund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch schmecket in der Abendlaube", "tokens": ["Noch", "schme\u00b7cket", "in", "der", "A\u00b7bend\u00b7lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kuss auf einen rothen Mund!", "tokens": ["Der", "Kuss", "auf", "ei\u00b7nen", "ro\u00b7then", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Noch t\u00f6nt der Busch voll Nachtigallen", "tokens": ["Noch", "t\u00f6nt", "der", "Busch", "voll", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem J\u00fcngling hohe Wonne zu;", "tokens": ["Dem", "J\u00fcng\u00b7ling", "ho\u00b7he", "Won\u00b7ne", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch str\u00f6mt, wenn ihre Lieder schallen,", "tokens": ["Noch", "str\u00f6mt", ",", "wenn", "ih\u00b7re", "Lie\u00b7der", "schal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Selbst in zerrissne Seelen Ruh!", "tokens": ["Selbst", "in", "zer\u00b7riss\u00b7ne", "See\u00b7len", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O wundersch\u00f6n ist Gottes Erde,", "tokens": ["O", "wun\u00b7der\u00b7sch\u00f6n", "ist", "Got\u00b7tes", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VAFIN", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wehrt darauf vergn\u00fcgt zu sein!", "tokens": ["Und", "wehrt", "da\u00b7rauf", "ver\u00b7gn\u00fcgt", "zu", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum will ich, bis ich Asche werde,", "tokens": ["Drum", "will", "ich", ",", "bis", "ich", "A\u00b7sche", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich dieser sch\u00f6nen Erde freun!", "tokens": ["Mich", "die\u00b7ser", "sch\u00f6\u00b7nen", "Er\u00b7de", "freun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}