{"textgrid.poem.35241": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wann der Ph\u00f6nix ist bejahret und nimmt an den Kr\u00e4ften ab,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann der Ph\u00f6nix ist bejahret und nimmt an den Kr\u00e4ften ab,", "tokens": ["Wann", "der", "Ph\u00f6\u00b7nix", "ist", "be\u00b7jah\u00b7ret", "und", "nimmt", "an", "den", "Kr\u00e4f\u00b7ten", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP", "KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Bauet er von Zimmetrinden f\u00fcr sich selbst ein Flammengrab.", "tokens": ["Bau\u00b7et", "er", "von", "Zim\u00b7me\u00b7trin\u00b7den", "f\u00fcr", "sich", "selbst", "ein", "Flam\u00b7men\u00b7grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "PRF", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Auf des h\u00f6chsten Berges Spitzen", "tokens": ["Auf", "des", "h\u00f6chs\u00b7ten", "Ber\u00b7ges", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll er im Gew\u00fcrze sitzen,", "tokens": ["Soll", "er", "im", "Ge\u00b7w\u00fcr\u00b7ze", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit dem schwarzen Trauerkleid angethan, und doch erfreuet,", "tokens": ["Mit", "dem", "schwar\u00b7zen", "Trau\u00b7er\u00b7kleid", "an\u00b7ge\u00b7than", ",", "und", "doch", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "KON", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Da\u00df der holden Sonne Glanz ihn durch ihren Brand erneuet,", "tokens": ["Da\u00df", "der", "hol\u00b7den", "Son\u00b7ne", "Glanz", "ihn", "durch", "ih\u00b7ren", "Brand", "er\u00b7neu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Weil er ihre Flammen liebt,", "tokens": ["Weil", "er", "ih\u00b7re", "Flam\u00b7men", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die ihm todt das Leben giebt.", "tokens": ["Die", "ihm", "todt", "das", "Le\u00b7ben", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Also soll ein jeder Christ seine S\u00fcnden legen ab,", "tokens": ["Al\u00b7so", "soll", "ein", "je\u00b7der", "Christ", "sei\u00b7ne", "S\u00fcn\u00b7den", "le\u00b7gen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und des alten Adams Fleisch gleichsam tragen in das Grab;", "tokens": ["Und", "des", "al\u00b7ten", "A\u00b7dams", "Fleisch", "gleich\u00b7sam", "tra\u00b7gen", "in", "das", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "In der Tr\u00fcbsal Dornenspitzen", "tokens": ["In", "der", "Tr\u00fcb\u00b7sal", "Dor\u00b7nen\u00b7spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll er rein und reuig sitzen,", "tokens": ["Soll", "er", "rein", "und", "reu\u00b7ig", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "So wird ihn der Gnadengeist und die Himmelsglut erfreuen,", "tokens": ["So", "wird", "ihn", "der", "Gna\u00b7den\u00b7geist", "und", "die", "Him\u00b7mels\u00b7glut", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Da\u00df er sich mit Seel' und Sinn, wie der Ph\u00f6nix, wird erneuen,", "tokens": ["Da\u00df", "er", "sich", "mit", "Seel'", "und", "Sinn", ",", "wie", "der", "Ph\u00f6\u00b7nix", ",", "wird", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "KON", "NN", "$,", "PWAV", "ART", "NN", "$,", "VAFIN", "VVINF", "$,"], "meter": "--+-+-+--+-+-+