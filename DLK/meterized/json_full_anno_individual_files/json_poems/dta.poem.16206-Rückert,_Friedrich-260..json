{"dta.poem.16206": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "260.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein B\u00fccherkatalog fiel heut in meine Hand,", "tokens": ["Ein", "B\u00fc\u00b7cher\u00b7ka\u00b7ta\u00b7log", "fiel", "heut", "in", "mei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwer wie ein Riese wog der starke dicke Band.", "tokens": ["Schwer", "wie", "ein", "Rie\u00b7se", "wog", "der", "star\u00b7ke", "di\u00b7cke", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O weh, Literatur wie breit bist du geworden,", "tokens": ["O", "weh", ",", "Li\u00b7te\u00b7ra\u00b7tur", "wie", "breit", "bist", "du", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NN", "KOKOM", "ADJD", "VAFIN", "PPER", "VAPP", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wenn das die Titel nur sind deiner B\u00fccherhorden.", "tokens": ["Wenn", "das", "die", "Ti\u00b7tel", "nur", "sind", "dei\u00b7ner", "B\u00fc\u00b7cher\u00b7hor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wer kann die Titel blo\u00df, wer erst die B\u00fccher lesen?", "tokens": ["Wer", "kann", "die", "Ti\u00b7tel", "blo\u00df", ",", "wer", "erst", "die", "B\u00fc\u00b7cher", "le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADV", "$,", "PWS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Zeiten Gl\u00fcck war gro\u00df, als du noch klein gewesen.", "tokens": ["Der", "Zei\u00b7ten", "Gl\u00fcck", "war", "gro\u00df", ",", "als", "du", "noch", "klein", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch wir, die W\u00e4chterschaar bei dem geschwollnen Drachen,", "tokens": ["Doch", "wir", ",", "die", "W\u00e4ch\u00b7ter\u00b7schaar", "bei", "dem", "ge\u00b7schwoll\u00b7nen", "Dra\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Arbeiten immerdar ihn dicker noch zu machen.", "tokens": ["Ar\u00b7bei\u00b7ten", "im\u00b7mer\u00b7dar", "ihn", "di\u00b7cker", "noch", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}