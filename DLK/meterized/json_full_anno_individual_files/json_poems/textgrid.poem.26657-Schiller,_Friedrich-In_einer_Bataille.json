{"textgrid.poem.26657": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "In einer Bataille", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwer und dumpfig,", "tokens": ["Schwer", "und", "dump\u00b7fig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Eine Wetterwolke,", "tokens": ["Ei\u00b7ne", "Wet\u00b7ter\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Durch die gr\u00fcne Ebne schwankt der Marsch.", "tokens": ["Durch", "die", "gr\u00fc\u00b7ne", "Eb\u00b7ne", "schwankt", "der", "Marsch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zum wilden eisernen W\u00fcrfelspiel", "tokens": ["Zum", "wil\u00b7den", "ei\u00b7ser\u00b7nen", "W\u00fcr\u00b7fel\u00b7spiel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Streckt sich unabsehlich das Gefilde.", "tokens": ["Streckt", "sich", "un\u00b7ab\u00b7seh\u00b7lich", "das", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Blicke kriechen niederw\u00e4rts,", "tokens": ["Bli\u00b7cke", "krie\u00b7chen", "nie\u00b7der\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "An die Rippen pocht das M\u00e4nnerherz,", "tokens": ["An", "die", "Rip\u00b7pen", "pocht", "das", "M\u00e4n\u00b7ner\u00b7herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Vor\u00fcber an hohlen Totengesichtern", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "an", "hoh\u00b7len", "To\u00b7ten\u00b7ge\u00b7sich\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Niederjagt die Front der Major:", "tokens": ["Nie\u00b7der\u00b7jagt", "die", "Front", "der", "Ma\u00b7jor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NE", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "\u00bbhalt!\u00ab", "tokens": ["\u00bb", "halt", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Und Regimenter fesselt das starre Kommando.", "tokens": ["Und", "Re\u00b7gi\u00b7men\u00b7ter", "fes\u00b7selt", "das", "star\u00b7re", "Kom\u00b7man\u00b7do", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Lautlos steht die Front.", "tokens": ["Laut\u00b7los", "steht", "die", "Front", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Pr\u00e4chtig im gl\u00fchenden Morgenrot", "tokens": ["Pr\u00e4ch\u00b7tig", "im", "gl\u00fc\u00b7hen\u00b7den", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Was blitzt dorther vom Geb\u00fcrge?", "tokens": ["Was", "blitzt", "dor\u00b7ther", "vom", "Ge\u00b7b\u00fcr\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Seht ihr des Feindes Fahnen wehn?", "tokens": ["Seht", "ihr", "des", "Fein\u00b7des", "Fah\u00b7nen", "wehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sehn des Feindes Fahnen wehn,", "tokens": ["Wir", "sehn", "des", "Fein\u00b7des", "Fah\u00b7nen", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott mit euch, Weib und Kinder!", "tokens": ["Gott", "mit", "euch", ",", "Weib", "und", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Lustig! h\u00f6rt ihr den Gesang?", "tokens": ["Lus\u00b7tig", "!", "h\u00f6rt", "ihr", "den", "Ge\u00b7sang", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Trommelwirbel, Pfeifenklang", "tokens": ["Trom\u00b7mel\u00b7wir\u00b7bel", ",", "Pfei\u00b7fen\u00b7klang"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schmettert durch die Glieder \u2013", "tokens": ["Schmet\u00b7tert", "durch", "die", "Glie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wie braust es fort im sch\u00f6nen wilden Takt!", "tokens": ["Wie", "braust", "es", "fort", "im", "sch\u00f6\u00b7nen", "wil\u00b7den", "Takt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "APPRART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und braust durch Mark und Bein.", "tokens": ["Und", "braust", "durch", "Mark", "und", "Bein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gott befohlen, Br\u00fcder!", "tokens": ["Gott", "be\u00b7foh\u00b7len", ",", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In einer andern Welt wieder!", "tokens": ["In", "ei\u00b7ner", "an\u00b7dern", "Welt", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Schon fleugt es fort wie Wetterleucht,", "tokens": ["Schon", "fleugt", "es", "fort", "wie", "Wet\u00b7ter\u00b7leucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dumpf br\u00fcllt der Donner schon dort.", "tokens": ["Dumpf", "br\u00fcllt", "der", "Don\u00b7ner", "schon", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die Wimper zuckt, hier kracht er laut,", "tokens": ["Die", "Wim\u00b7per", "zuckt", ",", "hier", "kracht", "er", "laut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Losung braust von Heer zu Heer,", "tokens": ["Die", "Lo\u00b7sung", "braust", "von", "Heer", "zu", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df brausen in Gottes Namen fort,", "tokens": ["La\u00df", "brau\u00b7sen", "in", "Got\u00b7tes", "Na\u00b7men", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "APPR", "NN", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Freier schon atmet die Brust.", "tokens": ["Frei\u00b7er", "schon", "at\u00b7met", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.7": {"line.1": {"text": "Der Tod ist los \u2013 schon wogt sich der Kampf;", "tokens": ["Der", "Tod", "ist", "los", "\u2013", "schon", "wogt", "sich", "der", "Kampf", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Eisern im wolkigten Pulverdampf,", "tokens": ["Ei\u00b7sern", "im", "wol\u00b7kig\u00b7ten", "Pul\u00b7ver\u00b7dampf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eisern fallen die W\u00fcrfel.", "tokens": ["Ei\u00b7sern", "fal\u00b7len", "die", "W\u00fcr\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.8": {"line.1": {"text": "Nah umarmen die Heere sich.", "tokens": ["Nah", "um\u00b7ar\u00b7men", "die", "Hee\u00b7re", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Fertig! heults von P'loton zu P'loton;", "tokens": ["Fer\u00b7tig", "!", "heults", "von", "P'\u00b7lo\u00b7ton", "zu", "P'\u00b7lo\u00b7ton", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADV", "APPR", "NE", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Auf die Knie geworfen", "tokens": ["Auf", "die", "Knie", "ge\u00b7wor\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Feurn die vordern, viele stehen nicht mehr auf,", "tokens": ["Feurn", "die", "vor\u00b7dern", ",", "vie\u00b7le", "ste\u00b7hen", "nicht", "mehr", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "$,", "PIS", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "L\u00fccken rei\u00dft die streifende Kart\u00e4tsche,", "tokens": ["L\u00fc\u00b7cken", "rei\u00dft", "die", "strei\u00b7fen\u00b7de", "Kar\u00b7t\u00e4t\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Auf Vormanns Rumpfe springt der Hintermann,", "tokens": ["Auf", "Vor\u00b7manns", "Rump\u00b7fe", "springt", "der", "Hin\u00b7ter\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verw\u00fcstung rechts und links und um und um,", "tokens": ["Ver\u00b7w\u00fcs\u00b7tung", "rechts", "und", "links", "und", "um", "und", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADV", "KON", "APPR", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bataillone niederw\u00e4lzt der Tod.", "tokens": ["Ba\u00b7tail\u00b7lo\u00b7ne", "nie\u00b7der\u00b7w\u00e4lzt", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Die Sonne l\u00f6scht aus \u2013 hei\u00df brennt die Schlacht,", "tokens": ["Die", "Son\u00b7ne", "l\u00f6scht", "aus", "\u2013", "hei\u00df", "brennt", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schwarz br\u00fctet auf dem Heer die Nacht.", "tokens": ["Schwarz", "br\u00fc\u00b7tet", "auf", "dem", "Heer", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", 