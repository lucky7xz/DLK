{"dta.poem.16275": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer alt geworden, mag sich an der Jugend Spr\u00fcngen", "tokens": ["Wer", "alt", "ge\u00b7wor\u00b7den", ",", "mag", "sich", "an", "der", "Ju\u00b7gend", "Spr\u00fcn\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "VAPP", "$,", "VMFIN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erg\u00f6tzen, doch sie nachzuthun sich nicht verj\u00fcngen.", "tokens": ["Er\u00b7g\u00f6t\u00b7zen", ",", "doch", "sie", "nach\u00b7zu\u00b7thun", "sich", "nicht", "ver\u00b7j\u00fcn\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "VVIZU", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So mag sich diese Zeit auch der Betrachtung freun", "tokens": ["So", "mag", "sich", "die\u00b7se", "Zeit", "auch", "der", "Be\u00b7trach\u00b7tung", "freun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "PDAT", "NN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kindlicher Sagenwelt, nicht aber sie erneun.", "tokens": ["Kind\u00b7li\u00b7cher", "Sa\u00b7gen\u00b7welt", ",", "nicht", "a\u00b7ber", "sie", "er\u00b7neun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKNEG", "ADV", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.