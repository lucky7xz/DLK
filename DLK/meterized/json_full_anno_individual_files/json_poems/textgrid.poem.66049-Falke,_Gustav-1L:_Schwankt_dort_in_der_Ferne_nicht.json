{"textgrid.poem.66049": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schwankt dort in der Ferne nicht", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwankt dort in der Ferne nicht", "tokens": ["Schwankt", "dort", "in", "der", "Fer\u00b7ne", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leis im Wind ein roter Mohn \u2013", "tokens": ["Leis", "im", "Wind", "ein", "ro\u00b7ter", "Mohn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder t\u00e4uscht mich mein Gesicht?", "tokens": ["O\u00b7der", "t\u00e4uscht", "mich", "mein", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber n\u00e4her kommt es schon,", "tokens": ["A\u00b7ber", "n\u00e4\u00b7her", "kommt", "es", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es ist kein roter Mohn.", "tokens": ["Und", "es", "ist", "kein", "ro\u00b7ter", "Mohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bauernrosenriesige Bl\u00fcte", "tokens": ["Bau\u00b7ern\u00b7ro\u00b7sen\u00b7rie\u00b7si\u00b7ge", "Bl\u00fc\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Naht es, und der Herr beh\u00fcte", "tokens": ["Naht", "es", ",", "und", "der", "Herr", "be\u00b7h\u00fc\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gn\u00e4dig uns vor allem B\u00f6sen!", "tokens": ["Gn\u00e4\u00b7dig", "uns", "vor", "al\u00b7lem", "B\u00f6\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "PIS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn sich erst die Blumen l\u00f6sen,", "tokens": ["Wenn", "sich", "erst", "die", "Blu\u00b7men", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einzeln durch die Welt zu rennen,", "tokens": ["Ein\u00b7zeln", "durch", "die", "Welt", "zu", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df der Teufel mit im Bund sein.", "tokens": ["Mu\u00df", "der", "Teu\u00b7fel", "mit", "im", "Bund", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "APPRART", "NN", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wie konnt ichs nur verkennen.", "tokens": ["Doch", "wie", "konnt", "ichs", "nur", "ver\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was k\u00f6nnt auch so rot und rund sein?", "tokens": ["Was", "k\u00f6nnt", "auch", "so", "rot", "und", "rund", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Meine liebe, h\u00fcbsche, runde,", "tokens": ["Mei\u00b7ne", "lie\u00b7be", ",", "h\u00fcb\u00b7sche", ",", "run\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgenfrische, kerngesunde", "tokens": ["Mor\u00b7gen\u00b7fri\u00b7sche", ",", "kern\u00b7ge\u00b7sun\u00b7de"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kleine ist es, mit dem strammen", "tokens": ["Klei\u00b7ne", "ist", "es", ",", "mit", "dem", "stram\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "VAFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schritt, mein tapfrer F\u00fcselier.", "tokens": ["Schritt", ",", "mein", "tapf\u00b7rer", "F\u00fc\u00b7se\u00b7lier", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie die drallen Wangen flammen!", "tokens": ["Wie", "die", "dral\u00b7len", "Wan\u00b7gen", "flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber r\u00f6ter flammt die Zier", "tokens": ["A\u00b7ber", "r\u00f6\u00b7ter", "flammt", "die", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber ihrem Sommerhut", "tokens": ["\u00dc\u00b7ber", "ih\u00b7rem", "Som\u00b7mer\u00b7hut"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem zarten Federlila.", "tokens": ["Mit", "dem", "zar\u00b7ten", "Fe\u00b7der\u00b7li\u00b7la", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "W\u00e4r ein Stier ich zu Sevilla,", "tokens": ["W\u00e4r", "ein", "Stier", "ich", "zu", "Se\u00b7vil\u00b7la", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "APPR", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ich geriet in gro\u00dfe Wut", "tokens": ["Ich", "ge\u00b7riet", "in", "gro\u00b7\u00dfe", "Wut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor dem Sonnenschirm, dem roten,", "tokens": ["Vor", "dem", "Son\u00b7nen\u00b7schirm", ",", "dem", "ro\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den sie schultert, wie der Krieger", "tokens": ["Den", "sie", "schul\u00b7tert", ",", "wie", "der", "Krie\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sein gef\u00e4hrlich Schie\u00dfgewehr:", "tokens": ["Sein", "ge\u00b7f\u00e4hr\u00b7lich", "Schie\u00df\u00b7ge\u00b7wehr", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wollt ihr was, so kommt nur her.", "tokens": ["Wollt", "ihr", "was", ",", "so", "kommt", "nur", "her", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "$,", "ADV", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jede Wut ist hier verboten,", "tokens": ["Je\u00b7de", "Wut", "ist", "hier", "ver\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich w\u00e4r der Unterlieger,", "tokens": ["Denn", "ich", "w\u00e4r", "der", "Un\u00b7ter\u00b7lie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ich wild, wie zu Sevilla,", "tokens": ["Wenn", "ich", "wild", ",", "wie", "zu", "Se\u00b7vil\u00b7la", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "PWAV", "APPR", "NE", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auf den Bandiller der Stier,", "tokens": ["Auf", "den", "Ban\u00b7dil\u00b7ler", "der", "Stier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "St\u00fcrzte auf den F\u00fcselier", "tokens": ["St\u00fcrz\u00b7te", "auf", "den", "F\u00fc\u00b7se\u00b7lier"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit der Feder zart und lila", "tokens": ["Mit", "der", "Fe\u00b7der", "zart", "und", "li\u00b7la"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "NE"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Auf dem neuen Sommerhut,", "tokens": ["Auf", "dem", "neu\u00b7en", "Som\u00b7mer\u00b7hut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und dem Schirm, so rot wie Blut.", "tokens": ["Und", "dem", "Schirm", ",", "so", "rot", "wie", "Blut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ich hab doch sonst wohl Mut.", "tokens": ["Und", "ich", "hab", "doch", "sonst", "wohl", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber hier geht er verloren.", "tokens": ["A\u00b7ber", "hier", "geht", "er", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Kalte, scharfe Blicke bohren", "tokens": ["Kal\u00b7te", ",", "schar\u00b7fe", "Bli\u00b7cke", "boh\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tapfer drohend sich in meine,", "tokens": ["Tap\u00b7fer", "dro\u00b7hend", "sich", "in", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PRF", "APPR", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es schwenkt dabei die Kleine,", "tokens": ["Und", "es", "schwenkt", "da\u00b7bei", "die", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PAV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Diese liebe, h\u00fcbsche, runde,", "tokens": ["Die\u00b7se", "lie\u00b7be", ",", "h\u00fcb\u00b7sche", ",", "run\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJA", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Morgenfrische, kerngesunde", "tokens": ["Mor\u00b7gen\u00b7fri\u00b7sche", ",", "kern\u00b7ge\u00b7sun\u00b7de"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit der unbeschirmten Rechten;", "tokens": ["Mit", "der", "un\u00b7be\u00b7schirm\u00b7ten", "Rech\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Alles ist so klar zum Fechten,", "tokens": ["Al\u00b7les", "ist", "so", "klar", "zum", "Fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df ich klug zur Seite weiche.", "tokens": ["Da\u00df", "ich", "klug", "zur", "Sei\u00b7te", "wei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und der bauernrosengleiche", "tokens": ["Und", "der", "bau\u00b7ern\u00b7ro\u00b7sen\u00b7glei\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schirm entwandelt, schrumpft zusammen", "tokens": ["Schirm", "ent\u00b7wan\u00b7delt", ",", "schrumpft", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "VVFIN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verzwergt sich, bis sein Flammen", "tokens": ["Und", "ver\u00b7zwergt", "sich", ",", "bis", "sein", "Flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder aus der Ferne scheint,", "tokens": ["Wie\u00b7der", "aus", "der", "Fer\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Da\u00df man einen Mohn vermeint", "tokens": ["Da\u00df", "man", "ei\u00b7nen", "Mohn", "ver\u00b7meint"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf bewegtem Halm zu sehn,", "tokens": ["Auf", "be\u00b7weg\u00b7tem", "Halm", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Einen Mohn im Sommerwehn.", "tokens": ["Ei\u00b7nen", "Mohn", "im", "Som\u00b7mer\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schwankt dort in der Ferne nicht", "tokens": ["Schwankt", "dort", "in", "der", "Fer\u00b7ne", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Leis im Wind ein roter Mohn \u2013", "tokens": ["Leis", "im", "Wind", "ein", "ro\u00b7ter", "Mohn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder t\u00e4uscht mich mein Gesicht?", "tokens": ["O\u00b7der", "t\u00e4uscht", "mich", "mein", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber n\u00e4her kommt es schon,", "tokens": ["A\u00b7ber", "n\u00e4\u00b7her", "kommt", "es", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und es ist kein roter Mohn.", "tokens": ["Und", "es", "ist", "kein", "ro\u00b7ter", "Mohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Bauernrosenriesige Bl\u00fcte", "tokens": ["Bau\u00b7ern\u00b7ro\u00b7sen\u00b7rie\u00b7si\u00b7ge", "Bl\u00fc\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Naht es, und der Herr beh\u00fcte", "tokens":