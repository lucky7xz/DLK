{"dta.poem.20594": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Bettler .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1803", "urn": "urn:nbn:de:kobv:b4-200905192133", "language": ["de:0.99"], "booktitle": "[Hebel, Johann Peter]: Allemannische Gedichte. Karlsruhe, 1803."}, "poem": {"stanza.1": {"line.1": {"text": "Vergeltsder Gott, und dankder Gott", "tokens": ["Ver\u00b7gelts\u00b7der", "Gott", ",", "und", "dank\u00b7der", "Gott"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "PAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "du zarten Engel wii\u00df und roth,", "tokens": ["du", "zar\u00b7ten", "En\u00b7gel", "wii\u00df", "und", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und geb der Gott e brave Ma!", "tokens": ["und", "geb", "der", "Gott", "e", "bra\u00b7ve", "Ma", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NE", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was luegsch mi so biwegli a?", "tokens": ["Was", "lu\u00b7egsch", "mi", "so", "bi\u00b7weg\u00b7li", "a", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Hesch \u00f6bben au e Schatz im Zelt,", "tokens": ["Hesch", "\u00f6b\u00b7ben", "au", "e", "Schatz", "im", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mit Schwerdt und Ro\u00df im wite Feld?", "tokens": ["mit", "Schwerdt", "und", "Ro\u00df", "im", "wi\u00b7te", "Feld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Biwahr di Gott vor Weh und Leid,", "tokens": ["Bi\u00b7wahr", "di", "Gott", "vor", "Weh", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "und geb dim Schatz e sicher Gleit,", "tokens": ["und", "geb", "dim", "Schatz", "e", "si\u00b7cher", "Gleit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "und bring der bald e gsunde Ma!", "tokens": ["und", "bring", "der", "bald", "e", "gsun\u00b7de", "Ma", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADV", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u2019s goht ziemli scharf vor Mantua,", "tokens": ["'s", "goht", "ziem\u00b7li", "scharf", "vor", "Man\u00b7tua", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "VVFIN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "\u2019s cha sy, i ch\u00f6nnt der Meldig ge.", "tokens": ["'s", "cha", "sy", ",", "i", "ch\u00f6nnt", "der", "Mel\u00b7dig", "ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "NE", "$,", "FM", "FM", "ART", "NE", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Was luegsch mi a, und wirsch wie Schnee,", "tokens": ["Was", "lu\u00b7egsch", "mi", "a", ",", "und", "wirsch", "wie", "Schnee", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "FM", "FM", "$,", "KON", "ADJD", "KOKOM", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.13": {"text": "und seisch nit: \u201eHenk di Bettelgwand", "tokens": ["und", "seisch", "nit", ":", "\u201e", "Henk", "di", "Bet\u00b7tel\u00b7gwand"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "PTKNEG", "$.", "$(", "NE", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "di falsche graue Bart an d\u2019 Wand?\u201c", "tokens": ["di", "fal\u00b7sche", "grau\u00b7e", "Bart", "an", "d'", "Wand", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "APPR", "NE", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Jez bschau mi recht, und chennsch mi no?", "tokens": ["Jez", "bschau", "mi", "recht", ",", "und", "chennsch", "mi", "no", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADJD", "$,", "KON", "ADJD", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Geb Gott, i seig Gottwilche do!", "tokens": ["Geb", "Gott", ",", "i", "seig", "Gott\u00b7wil\u00b7che", "do", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}