{"textgrid.poem.34945": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verstummt sind Pauken, Posaunen und Zinken.", "tokens": ["Ver\u00b7stummt", "sind", "Pau\u00b7ken", ",", "Po\u00b7sau\u00b7nen", "und", "Zin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An Salomos Lager Wache halten", "tokens": ["An", "Sa\u00b7lo\u00b7mos", "La\u00b7ger", "Wa\u00b7che", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN", "VVINF"], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Die schwertgeg\u00fcrteten Engelgestalten,", "tokens": ["Die", "schwert\u00b7ge\u00b7g\u00fcr\u00b7te\u00b7ten", "En\u00b7gel\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sechstausend zur Rechten, sechstausend zur Linken.", "tokens": ["Sech\u00b7stau\u00b7send", "zur", "Rech\u00b7ten", ",", "sech\u00b7stau\u00b7send", "zur", "Lin\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Sie sch\u00fctzen den K\u00f6nig vor tr\u00e4umendem Leide,", "tokens": ["Sie", "sch\u00fct\u00b7zen", "den", "K\u00f6\u00b7nig", "vor", "tr\u00e4u\u00b7men\u00b7dem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und zieht er finster die Brauen zusammen,", "tokens": ["Und", "zieht", "er", "fins\u00b7ter", "die", "Brau\u00b7en", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da fahren sogleich die st\u00e4hlernen Flammen,", "tokens": ["Da", "fah\u00b7ren", "sog\u00b7leich", "die", "st\u00e4h\u00b7ler\u00b7nen", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zw\u00f6lftausend Schwerter, hervor aus der Scheide.", "tokens": ["Zw\u00f6lf\u00b7tau\u00b7send", "Schwer\u00b7ter", ",", "her\u00b7vor", "aus", "der", "Schei\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Doch wieder zur\u00fcck in die Scheide fallen", "tokens": ["Doch", "wie\u00b7der", "zu\u00b7r\u00fcck", "in", "die", "Schei\u00b7de", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "APPR", "ART", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Schwerter der Engel. Das n\u00e4chtliche Grauen", "tokens": ["Die", "Schwer\u00b7ter", "der", "En\u00b7gel", ".", "Das", "n\u00e4cht\u00b7li\u00b7che", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Verschwindet, es gl\u00e4tten sich wieder die Brauen", "tokens": ["Ver\u00b7schwin\u00b7det", ",", "es", "gl\u00e4t\u00b7ten", "sich", "wie\u00b7der", "die", "Brau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "PRF", "ADV", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Des Schl\u00e4fers, und seine Lippen lallen:", "tokens": ["Des", "Schl\u00e4\u00b7fers", ",", "und", "sei\u00b7ne", "Lip\u00b7pen", "lal\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbo Sulamith! Das Reich ist mein Erbe,", "tokens": ["\u00bb", "o", "Su\u00b7la\u00b7mith", "!", "Das", "Reich", "ist", "mein", "Er\u00b7be", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NE", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Lande sind mir untert\u00e4nig,", "tokens": ["Die", "Lan\u00b7de", "sind", "mir", "un\u00b7ter\u00b7t\u00e4\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bin \u00fcber Juda und Israel K\u00f6nig \u2013", "tokens": ["Bin", "\u00fc\u00b7ber", "Ju\u00b7da", "und", "Is\u00b7rael", "K\u00f6\u00b7nig", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "KON", "NE", "NE", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch liebst du mich nicht, so welk ich und ster