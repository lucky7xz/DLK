{"textgrid.poem.48612": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "23. An Herren Hartman Grahman, F\u00fcrstl. Holstein. Abgesanten nach Moskaw und Persien u.s.w. Leibarzt", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bester meiner besten Freunde,", "tokens": ["Bes\u00b7ter", "mei\u00b7ner", "bes\u00b7ten", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die mir noch mein Gl\u00fccke g\u00f6nnt,", "tokens": ["die", "mir", "noch", "mein", "Gl\u00fc\u00b7cke", "g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "das mir doch, als seinem Feinde", "tokens": ["das", "mir", "doch", ",", "als", "sei\u00b7nem", "Fein\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "stets in dem zuwider sinnt,", "tokens": ["stets", "in", "dem", "zu\u00b7wi\u00b7der", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df es mich nach euch, ihr Lieben,", "tokens": ["da\u00df", "es", "mich", "nach", "euch", ",", "ihr", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "nur nicht