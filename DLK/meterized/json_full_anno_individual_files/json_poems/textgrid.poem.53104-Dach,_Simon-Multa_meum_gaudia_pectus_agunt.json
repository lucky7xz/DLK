ter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "K\u00fcnfftig weyden meinen Geist,", "tokens": ["K\u00fcnff\u00b7tig", "wey\u00b7den", "mei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der sich mir entreisst.", "tokens": ["Der", "sich", "mir", "en\u00b7treisst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPER", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Liebste, la\u00df vns leben!", "tokens": ["Liebs\u00b7te", ",", "la\u00df", "vns", "le\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sey mein Trost in Noht!", "tokens": ["Sey", "mein", "Trost", "in", "Noht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ich wil dir mich geben", "tokens": ["Ich", "wil", "dir", "mich", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auch bi\u00df in den Todt;", "tokens": ["Auch", "bi\u00df", "in", "den", "Todt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Fleuch das rechte Lieben", "tokens": ["Fleuch", "das", "rech\u00b7te", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "L\u00e4nger auffzuschieben,", "tokens": ["L\u00e4n\u00b7ger", "auff\u00b7zu\u00b7schie\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Fort! hab' ich doch Recht dazu,", "tokens": ["Fort", "!", "hab'", "ich", "doch", "Recht", "da\u00b7zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "ADV", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was ich mit dir thue!", "tokens": ["Was", "ich", "mit", "dir", "thue", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}