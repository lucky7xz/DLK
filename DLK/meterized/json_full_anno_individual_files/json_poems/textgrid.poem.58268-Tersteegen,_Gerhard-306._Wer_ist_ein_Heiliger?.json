{"textgrid.poem.58268": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "306. Wer ist ein Heiliger?", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Grund der Heiligkeit setz nicht in dies noch das,", "tokens": ["Den", "Grund", "der", "Hei\u00b7lig\u00b7keit", "setz", "nicht", "in", "dies", "noch", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKNEG", "APPR", "PDS", "ADV", "PDS", "$,"], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Verzeih dich selbst nur ganz und Gott dich gr\u00fcndlich la\u00df!", "tokens": ["Ver\u00b7zeih", "dich", "selbst", "nur", "ganz", "und", "Gott", "dich", "gr\u00fcnd\u00b7lich", "la\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADV", "KON", "NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer lauterlich Gott meint in seinem Tun allein,", "tokens": ["Wer", "lau\u00b7ter\u00b7lich", "Gott", "meint", "in", "sei\u00b7nem", "Tun", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ist ein Heiliger und los von aller Pein.", "tokens": ["Der", "ist", "ein", "Hei\u00b7li\u00b7ger", "und", "los", "von", "al\u00b7ler", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-