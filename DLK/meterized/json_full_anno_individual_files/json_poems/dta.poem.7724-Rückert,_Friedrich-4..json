{"dta.poem.7724": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die gr\u00f6sten F\u00fcrsten all, die auf des Ruhmes Bahnen", "tokens": ["Die", "gr\u00f6s\u00b7ten", "F\u00fcrs\u00b7ten", "all", ",", "die", "auf", "des", "Ruh\u00b7mes", "Bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei Hindu's wandelten und bei den Muselmanen,", "tokens": ["Bei", "Hin\u00b7du's", "wan\u00b7del\u00b7ten", "und", "bei", "den", "Mu\u00b7sel\u00b7ma\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie hatten einen Brauch, mit abgelegten Zeichen", "tokens": ["Sie", "hat\u00b7ten", "ei\u00b7nen", "Brauch", ",", "mit", "ab\u00b7ge\u00b7leg\u00b7ten", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Standes unter'm Volk vermummt umherzuschleichen,", "tokens": ["Des", "Stan\u00b7des", "un\u00b7ter'm", "Volk", "ver\u00b7mummt", "um\u00b7her\u00b7zu\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Um zu erfahren, was sie sonst nicht leicht erfuhren,", "tokens": ["Um", "zu", "er\u00b7fah\u00b7ren", ",", "was", "sie", "sonst", "nicht", "leicht", "er\u00b7fuh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was man von ihnen denk' in H\u00fctten und auf Fluren.", "tokens": ["Was", "man", "von", "ih\u00b7nen", "denk'", "in", "H\u00fct\u00b7ten", "und", "auf", "Flu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPR", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch hielten sie dabei streng ein Gesetz, den Leuten", "tokens": ["Doch", "hiel\u00b7ten", "sie", "da\u00b7bei", "streng", "ein", "Ge\u00b7setz", ",", "den", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PAV", "VVFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie das Verborgene verborgen anzudeuten,", "tokens": ["Nie", "das", "Ver\u00b7bor\u00b7ge\u00b7ne", "ver\u00b7bor\u00b7gen", "an\u00b7zu\u00b7deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "VVIZU", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Noch minder, in des Zorns und Ungest\u00fcms Entwallen,", "tokens": ["Noch", "min\u00b7der", ",", "in", "des", "Zorns", "und", "Un\u00b7ge\u00b7st\u00fcms", "Ent\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Roll', in der sie aufgetreten, zu entfallen;", "tokens": ["Der", "Roll'", ",", "in", "der", "sie", "auf\u00b7ge\u00b7tre\u00b7ten", ",", "zu", "ent\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Still, was Ersprie\u00dfliches sie h\u00f6rten, zu ermessen,", "tokens": ["Still", ",", "was", "Er\u00b7sprie\u00df\u00b7li\u00b7ches", "sie", "h\u00f6r\u00b7ten", ",", "zu", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Und was Verdrie\u00dfliches, als F\u00fcrsten zu vergessen.", "tokens": ["Und", "was", "Ver\u00b7drie\u00df\u00b7li\u00b7ches", ",", "als", "F\u00fcrs\u00b7ten", "zu", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJA", "$,", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}