{"dta.poem.12590": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Erster Theil.  \n Von den Tisch-Liedern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, ich bitte, h\u00fcte dich,", "tokens": ["Freund", ",", "ich", "bit\u00b7te", ",", "h\u00fc\u00b7te", "dich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADV", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Scorpionen schleichen sich", "tokens": ["Scor\u00b7pi\u00b7o\u00b7nen", "schlei\u00b7chen", "sich"], "token_info": ["word", "word", "word"], "pos": ["NE", "VVFIN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter jeden Stein.", "tokens": ["Un\u00b7ter", "je\u00b7den", "Stein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Pflegt Betr\u00fcgerey und List", "tokens": ["Pflegt", "Be\u00b7tr\u00fc\u00b7ge\u00b7rey", "und", "List"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Oft versteckt zu seyn.", "tokens": ["Oft", "ver\u00b7steckt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "PTKZU", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wer seinen treuen Freund", "tokens": ["Wer", "sei\u00b7nen", "treu\u00b7en", "Freund"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auch treu und redlich meynt,", "tokens": ["Auch", "treu", "und", "red\u00b7lich", "meynt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist, wie mich d\u00fcnket, wehrt,", "tokens": ["Ist", ",", "wie", "mich", "d\u00fcn\u00b7ket", ",", "wehrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df Gott und Welt ihn ehrt.", "tokens": ["Da\u00df", "Gott", "und", "Welt", "ihn", "ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}