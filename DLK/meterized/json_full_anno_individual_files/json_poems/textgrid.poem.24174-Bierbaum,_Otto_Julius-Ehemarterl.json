{"textgrid.poem.24174": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ehemarterl", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier fiel ich, steh, Wandrer, und bet ein Gebet,", "tokens": ["Hier", "fiel", "ich", ",", "steh", ",", "Wand\u00b7rer", ",", "und", "bet", "ein", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "$,", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In die H\u00e4nde meiner Frau, der Anna Margreth;", "tokens": ["In", "die", "H\u00e4n\u00b7de", "mei\u00b7ner", "Frau", ",", "der", "An\u00b7na", "Mar\u00b7greth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,", "ART", "NE", "NE", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Es war am f\u00fcnfundzwanzigsten Mai,", "tokens": ["Es", "war", "am", "f\u00fcn\u00b7fund\u00b7zwan\u00b7zigs\u00b7ten", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Als ich ging an diesem \u2020\u2020\u2020 Baume vorbei,", "tokens": ["Als", "ich", "ging", "an", "die\u00b7sem", "\u2020\u2020\u2020", "Bau\u00b7me", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hinter dem sie ganz von ungef\u00e4hr stand;", "tokens": ["Hin\u00b7ter", "dem", "sie", "ganz", "von", "un\u00b7ge\u00b7f\u00e4hr", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "APPR", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ich sagte Guten Abend und gab ihr die Hand.", "tokens": ["Ich", "sag\u00b7te", "Gu\u00b7ten", "A\u00b7bend", "und", "gab", "ihr", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Damals war ich ein Junggesell,", "tokens": ["Da\u00b7mals", "war", "ich", "ein", "Jung\u00b7ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und deshalb verliebte ich mich sehr schnell;", "tokens": ["Und", "des\u00b7halb", "ver\u00b7lieb\u00b7te", "ich", "mich", "sehr", "schnell", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "-++-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Sie behauptete von sich selber das Gleiche", "tokens": ["Sie", "be\u00b7haup\u00b7te\u00b7te", "von", "sich", "sel\u00b7ber", "das", "Glei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "ART", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und verlangte, da\u00df ich die Hand ihr reiche", "tokens": ["Und", "ver\u00b7lang\u00b7te", ",", "da\u00df", "ich", "die", "Hand", "ihr", "rei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", 