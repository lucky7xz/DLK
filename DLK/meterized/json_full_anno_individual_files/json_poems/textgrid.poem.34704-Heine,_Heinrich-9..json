{"textgrid.poem.34704": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich der Mann, der den Hafen erreicht hat,", "tokens": ["Gl\u00fcck\u00b7lich", "der", "Mann", ",", "der", "den", "Ha\u00b7fen", "er\u00b7reicht", "hat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Und hinter sich lie\u00df das Meer und die St\u00fcrme,", "tokens": ["Und", "hin\u00b7ter", "sich", "lie\u00df", "das", "Meer", "und", "die", "St\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PRF", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und jetzo warm und ruhig sitzt", "tokens": ["Und", "jet\u00b7zo", "warm", "und", "ru\u00b7hig", "sitzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im guten Ratskeller zu Bremen.", "tokens": ["Im", "gu\u00b7ten", "Rats\u00b7kel\u00b7ler", "zu", "Bre\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie doch die Welt so traulich und lieblich", "tokens": ["Wie", "doch", "die", "Welt", "so", "trau\u00b7lich", "und", "lieb\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im R\u00f6merglas sich widerspiegelt,", "tokens": ["Im", "R\u00f6\u00b7mer\u00b7glas", "sich", "wi\u00b7der\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie der wogende Mikrokosmus", "tokens": ["Und", "wie", "der", "wo\u00b7gen\u00b7de", "Mik\u00b7ro\u00b7kos\u00b7mus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sonnig hinabflie\u00dft ins durstige Herz!", "tokens": ["Son\u00b7nig", "hin\u00b7ab\u00b7flie\u00dft", "ins", "durs\u00b7ti\u00b7ge", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Alles erblick ich im Glas,", "tokens": ["Al\u00b7les", "er\u00b7blick", "ich", "im", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Alte und neue V\u00f6lkergeschichte,", "tokens": ["Al\u00b7te", "und", "neu\u00b7e", "V\u00f6l\u00b7ker\u00b7ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "T\u00fcrken und Griechen, Hegel und Gans,", "tokens": ["T\u00fcr\u00b7ken", "und", "Grie\u00b7chen", ",", "He\u00b7gel", "und", "Gans", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Zitronenw\u00e4lder und Wachtparaden,", "tokens": ["Zit\u00b7ro\u00b7nen\u00b7w\u00e4l\u00b7der", "und", "Wacht\u00b7pa\u00b7ra\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Berlin und Schilda und Tunis und Hamburg,", "tokens": ["Ber\u00b7lin", "und", "Schil\u00b7da", "und", "Tu\u00b7nis", "und", "Ham\u00b7burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "KON", "NE", "$,"], "meter": "++-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Vor allem aber das Bild der Geliebten,", "tokens": ["Vor", "al\u00b7lem", "a\u00b7ber", "das", "Bild", "der", "Ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Das Engelk\u00f6pfchen auf Rheinweingoldgrund.", "tokens": ["Das", "En\u00b7gel\u00b7k\u00f6pf\u00b7chen", "auf", "Rhein\u00b7wein\u00b7gold\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Oh, wie sch\u00f6n! wie sch\u00f6n bist du, Geliebte!", "tokens": ["Oh", ",", "wie", "sch\u00f6n", "!", "wie", "sch\u00f6n", "bist", "du", ",", "Ge\u00b7lieb\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Du bist wie eine Rose!", "tokens": ["Du", "bist", "wie", "ei\u00b7ne", "Ro\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht wie die Rose von Schiras,", "tokens": ["Nicht", "wie", "die", "Ro\u00b7se", "von", "Schi\u00b7ras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die hafisbesungene Nachtigallbraut;", "tokens": ["Die", "ha\u00b7fis\u00b7be\u00b7sun\u00b7ge\u00b7ne", "Nach\u00b7ti\u00b7gall\u00b7braut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nicht wie die Rose von Saron,", "tokens": ["Nicht", "wie", "die", "Ro\u00b7se", "von", "Sa\u00b7ron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOKOM", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die heiligrote, prophetengefeierte; \u2013", "tokens": ["Die", "hei\u00b7li\u00b7gro\u00b7te", ",", "pro\u00b7phe\u00b7ten\u00b7ge\u00b7fei\u00b7er\u00b7te", ";", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$.", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Du bist wie die Ros' im Ratskeller zu Bremen!", "tokens": ["Du", "bist", "wie", "die", "Ros'", "im", "Rats\u00b7kel\u00b7ler", "zu", "Bre\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "ART", "NN", "APPRART", "NN", "APPR", "NE", "$."], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Das ist die Rose der Rosen,", "tokens": ["Das", "ist", "die", "Ro\u00b7se", "der", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Je \u00e4lter sie wird, je lieblicher bl\u00fcht sie,", "tokens": ["Je", "\u00e4l\u00b7ter", "sie", "wird", ",", "je", "lieb\u00b7li\u00b7cher", "bl\u00fcht", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "ADV", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und ihr himmlischer Duft, er hat mich beseligt,", "tokens": ["Und", "ihr", "himm\u00b7li\u00b7scher", "Duft", ",", "er", "hat", "mich", "be\u00b7se\u00b7ligt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Er hat mich begeistert, er hat mich berauscht,", "tokens": ["Er", "hat", "mich", "be\u00b7geis\u00b7tert", ",", "er", "hat", "mich", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+-++--+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Und hielt mich nicht fest, am Schopfe fest,", "tokens": ["Und", "hielt", "mich", "nicht", "fest", ",", "am", "Schop\u00b7fe", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Der Ratskellermeister von Bremen,", "tokens": ["Der", "Rats\u00b7kel\u00b7ler\u00b7meis\u00b7ter", "von", "Bre\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "Ich w\u00e4re gepurzelt!", "tokens": ["Ich", "w\u00e4\u00b7re", "ge\u00b7pur\u00b7zelt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Der brave Mann! wir sa\u00dfen beisammen", "tokens": ["Der", "bra\u00b7ve", "Mann", "!", "wir", "sa\u00b7\u00dfen", "bei\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und tranken wie Br\u00fcder,", "tokens": ["Und", "tran\u00b7ken", "wie", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wir sprachen von hohen, heimlichen Dingen,", "tokens": ["Wir", "spra\u00b7chen", "von", "ho\u00b7hen", ",", "heim\u00b7li\u00b7chen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir seufzten und sanken uns in die Arme,", "tokens": ["Wir", "seufz\u00b7ten", "und", "san\u00b7ken", "uns", "in", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und er hat mich bekehrt zum Glauben der Liebe \u2013", "tokens": ["Und", "er", "hat", "mich", "be\u00b7kehrt", "zum", "Glau\u00b7ben", "der", "Lie\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ich trank auf das Wohl meiner bittersten Feinde,", "tokens": ["Ich", "trank", "auf", "das", "Wohl", "mei\u00b7ner", "bit\u00b7ters\u00b7ten", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und allen schlechten Poeten vergab ich,", "tokens": ["Und", "al\u00b7len", "schlech\u00b7ten", "Po\u00b7et\u00b7en", "ver\u00b7gab", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie einst mir selber vergeben soll werden \u2013", "tokens": ["Wie", "einst", "mir", "sel\u00b7ber", "ver\u00b7ge\u00b7ben", "soll", "wer\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "VVINF", "VMFIN", "VAINF", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Ich weinte vor Andacht, und endlich", "tokens": ["Ich", "wein\u00b7te", "vor", "An\u00b7dacht", ",", "und", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KON", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Erschlossen sich mir die Pforten des Heils,", "tokens": ["Er\u00b7schlos\u00b7sen", "sich", "mir", "die", "Pfor\u00b7ten", "des", "Heils", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wo die zw\u00f6lf Apostel, die heil'gen St\u00fcckf\u00e4sser,", "tokens": ["Wo", "die", "zw\u00f6lf", "A\u00b7pos\u00b7tel", ",", "die", "heil'\u00b7gen", "St\u00fcck\u00b7f\u00e4s\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "CARD", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Schweigend pred'gen, und doch so verst\u00e4ndlich", "tokens": ["Schwei\u00b7gend", "pre\u00b7d'\u00b7gen", ",", "und", "doch", "so", "ver\u00b7st\u00e4nd\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KON", "ADV", "ADV", "ADJD"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.13": {"text": "F\u00fcr alle V\u00f6lker.", "tokens": ["F\u00fcr", "al\u00b7le", "V\u00f6l\u00b7ker", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Das sind M\u00e4nner!", "tokens": ["Das", "sind", "M\u00e4n\u00b7ner", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Unscheinbar von au\u00dfen, in h\u00f6lzernen R\u00f6cklein,", "tokens": ["Un\u00b7schein\u00b7bar", "von", "au\u00b7\u00dfen", ",", "in", "h\u00f6l\u00b7zer\u00b7nen", "R\u00f6ck\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sind sie von innen sch\u00f6ner und leuchtender", "tokens": ["Sind", "sie", "von", "in\u00b7nen", "sch\u00f6\u00b7ner", "und", "leuch\u00b7ten\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Denn all die stolzen Leviten des Tempels", "tokens": ["Denn", "all", "die", "stol\u00b7zen", "Le\u00b7vi\u00b7ten", "des", "Tem\u00b7pels"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und des Herodes Trabanten und H\u00f6flinge,", "tokens": ["Und", "des", "He\u00b7ro\u00b7des", "Tra\u00b7ban\u00b7ten", "und", "H\u00f6f\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Die goldgeschm\u00fcckten, die purpurgekleideten \u2013", "tokens": ["Die", "gold\u00b7ge\u00b7schm\u00fcck\u00b7ten", ",", "die", "pur\u00b7pur\u00b7ge\u00b7klei\u00b7de\u00b7ten", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "$("], "meter": "-+-+--++-+--", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Hab ich doch immer gesagt,", "tokens": ["Hab", "ich", "doch", "im\u00b7mer", "ge\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Nicht unter ganz gemeinen Leuten,", "tokens": ["Nicht", "un\u00b7ter", "ganz", "ge\u00b7mei\u00b7nen", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nein, in der