{"dta.poem.16029": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "83.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den k\u00f6rperlosen Geist mit sch\u00f6nem K\u00f6rperschein", "tokens": ["Den", "k\u00f6r\u00b7per\u00b7lo\u00b7sen", "Geist", "mit", "sch\u00f6\u00b7nem", "K\u00f6r\u00b7per\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bekleiden, ist von Kunst die eine Seit' allein.", "tokens": ["Be\u00b7klei\u00b7den", ",", "ist", "von", "Kunst", "die", "ei\u00b7ne", "Seit'", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "APPR", "NN", "ART", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die andre Seite ist, den Leib zu Geist verkl\u00e4ren,", "tokens": ["Die", "and\u00b7re", "Sei\u00b7te", "ist", ",", "den", "Leib", "zu", "Geist", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Bild das sinnliche zum Sinnbild umgeb\u00e4ren.", "tokens": ["Das", "Bild", "das", "sinn\u00b7li\u00b7che", "zum", "Sinn\u00b7bild", "um\u00b7ge\u00b7b\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Beim halben Dichter l\u00e4\u00dft sich eins vom andern scheiden;", "tokens": ["Beim", "hal\u00b7ben", "Dich\u00b7ter", "l\u00e4\u00dft", "sich", "eins", "vom", "an\u00b7dern", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF", "PIS", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ganzer ist, wer ungetrennt vereint die beiden.", "tokens": ["Ein", "gan\u00b7zer", "ist", ",", "wer", "un\u00b7ge\u00b7trennt", "ver\u00b7eint", "die", "bei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "$,", "PWS", "ADJD", "VVFIN", "ART", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}