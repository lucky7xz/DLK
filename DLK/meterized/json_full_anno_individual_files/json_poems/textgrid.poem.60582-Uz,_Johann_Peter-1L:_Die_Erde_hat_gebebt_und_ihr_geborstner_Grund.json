unct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ihr Musen, die ihr einst, im Fr\u00fchling meiner Zeit,", "tokens": ["Ihr", "Mu\u00b7sen", ",", "die", "ihr", "einst", ",", "im", "Fr\u00fch\u00b7ling", "mei\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich mit Ambrosia gen\u00e4hret,", "tokens": ["Mich", "mit", "A\u00b7mbro\u00b7sia", "ge\u00b7n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ihr, in eurem Hayn voll heilger Dunkelheit,", "tokens": ["Als", "ihr", ",", "in", "eu\u00b7rem", "Hayn", "voll", "heil\u00b7ger", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die deutsche Leyer mich gelehret!", "tokens": ["Die", "deut\u00b7sche", "Le\u00b7yer", "mich", "ge\u00b7leh\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Zufrieden dank ich euch, da\u00df immer gleiche Lust", "tokens": ["Zu\u00b7frie\u00b7den", "dank", "ich", "euch", ",", "da\u00df", "im\u00b7mer", "glei\u00b7che", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "PPER", "$,", "KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In meiner Seelen helle scheinet,", "tokens": ["In", "mei\u00b7ner", "See\u00b7len", "hel\u00b7le", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und euer stiller Freund nicht, an der Thorheit Brust,", "tokens": ["Und", "eu\u00b7er", "stil\u00b7ler", "Freund", "nicht", ",", "an", "der", "Thor\u00b7heit", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKNEG", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach Fantasien lacht und weinet.", "tokens": ["Nach", "Fan\u00b7ta\u00b7si\u00b7en", "lacht", "und", "wei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.16": {"line.1": {"text": "O la\u00dft, zu aller Zeit, mein Antlitz heiter seyn,", "tokens": ["O", "la\u00dft", ",", "zu", "al\u00b7ler", "Zeit", ",", "mein", "Ant\u00b7litz", "hei\u00b7ter", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,", "PPOSAT", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht blo\u00df in sonnenvollen Tagen,", "tokens": ["Nicht", "blo\u00df", "in", "son\u00b7nen\u00b7vol\u00b7len", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wann mich die Freude sucht, und Saitenspiel und Wein", "tokens": ["Wann", "mich", "die", "Freu\u00b7de", "sucht", ",", "und", "Sai\u00b7ten\u00b7spiel", "und", "Wein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Wolken vor mir her verjagen:", "tokens": ["Die", "Wol\u00b7ken", "vor", "mir", "her", "ver\u00b7ja\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "APZR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.17": {"line.1": {"text": "Nicht blo\u00df im dunkeln Busch und wo die Nachtigall", "tokens": ["Nicht", "blo\u00df", "im", "dun\u00b7keln", "Busch", "und", "wo", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPRART", "ADJA", "NN", "KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald singend \u00fcber mir verweilet,", "tokens": ["Bald", "sin\u00b7gend", "\u00fc\u00b7ber", "mir", "ver\u00b7wei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald an der Quelle seufzt, die reiner, als Crystall,", "tokens": ["Bald", "an", "der", "Quel\u00b7le", "seufzt", ",", "die", "rei\u00b7ner", ",", "als", "Crys\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geschw\u00e4tzig \u00fcber K