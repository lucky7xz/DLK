{"textgrid.poem.60582": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Erde hat gebebt und ihr geborstner Grund", "genre": "verse", "period": "N.A.", "pub_year": 1758, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde hat gebebt und ihr geborstner Grund", "tokens": ["Die", "Er\u00b7de", "hat", "ge\u00b7bebt", "und", "ihr", "ge\u00b7borst\u00b7ner", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die K\u00f6niginn am Meer verschlungen,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7ginn", "am", "Meer", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schw\u00e4rzre Tr\u00fcbsal noch droht unsrem armen Rund", "tokens": ["Und", "schw\u00e4rz\u00b7re", "Tr\u00fcb\u00b7sal", "noch", "droht", "uns\u00b7rem", "ar\u00b7men", "Rund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von schw\u00e4rmender Propheten Zungen:", "tokens": ["Von", "schw\u00e4r\u00b7men\u00b7der", "Pro\u00b7phe\u00b7ten", "Zun\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie aus bemoostem Schutt der Uhu, wann die Nacht", "tokens": ["Wie", "aus", "be\u00b7moos\u00b7tem", "Schutt", "der", "U\u00b7hu", ",", "wann", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In furchtbarn Schatten ihn verstecket,", "tokens": ["In", "furcht\u00b7barn", "Schat\u00b7ten", "ihn", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf stille D\u00e4cher fliegt, selbst melancholisch wacht,", "tokens": ["Auf", "stil\u00b7le", "D\u00e4\u00b7cher", "fliegt", ",", "selbst", "me\u00b7lan\u00b7cho\u00b7lisch", "wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und heulend m\u00fcde St\u00e4dte wecket.", "tokens": ["Und", "heu\u00b7lend", "m\u00fc\u00b7de", "St\u00e4d\u00b7te", "we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf Schwanenfedern horcht die Wollust und erschrickt;", "tokens": ["Auf", "Schwa\u00b7nen\u00b7fe\u00b7dern", "horcht", "die", "Wol\u00b7lust", "und", "er\u00b7schrickt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Schauer bebt durch ihre Glieder.", "tokens": ["Ein", "Schau\u00b7er", "bebt", "durch", "ih\u00b7re", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der sorgenvolle Geiz, auch schlafend unerquickt,", "tokens": ["Der", "sor\u00b7gen\u00b7vol\u00b7le", "Geiz", ",", "auch", "schla\u00b7fend", "un\u00b7er\u00b7quickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bebt heut und wuchert morgen wieder.", "tokens": ["Bebt", "heut", "und", "wu\u00b7chert", "mor\u00b7gen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Propheten wimmeln stets in tr\u00fcber Zeit hervor:", "tokens": ["Pro\u00b7phe\u00b7ten", "wim\u00b7meln", "stets", "in", "tr\u00fc\u00b7ber", "Zeit", "her\u00b7vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der leichte P\u00f6bel glaubt, er zittert,", "tokens": ["Der", "leich\u00b7te", "P\u00f6\u00b7bel", "glaubt", ",", "er", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie d\u00fcrres Laub im Herbst, und wie das schwache Rohr", "tokens": ["Wie", "d\u00fcr\u00b7res", "Laub", "im", "Herbst", ",", "und", "wie", "das", "schwa\u00b7che", "Rohr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "$,", "KON", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Fl\u00fcgel eines Wests ersch\u00fcttert.", "tokens": ["Der", "Fl\u00fc\u00b7gel", "ei\u00b7nes", "Wests", "er\u00b7sch\u00fct\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr Musen, die ihr einst, im Fr\u00fchling meiner Zeit,", "tokens": ["Ihr", "Mu\u00b7sen", ",", "die", "ihr", "einst", ",", "im", "Fr\u00fch\u00b7ling", "mei\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich mit Ambrosia gen\u00e4hret,", "tokens": ["Mich", "mit", "A\u00b7mbro\u00b7sia", "ge\u00b7n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ihr, in eurem Hayn voll heilger Dunkelheit,", "tokens": ["Als", "ihr", ",", "in", "eu\u00b7rem", "Hayn", "voll", "heil\u00b7ger", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die deutsche Leyer mich gelehret!", "tokens": ["Die", "deut\u00b7sche", "Le\u00b7yer", "mich", "ge\u00b7leh\u00b7ret", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zufrieden dank ich euch, da\u00df immer gleiche Lust", "tokens": ["Zu\u00b7frie\u00b7den", "dank", "ich", "euch", ",", "da\u00df", "im\u00b7mer", "glei\u00b7che", "Lust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "PPER", "$,", "KOUS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In meiner Seelen helle scheinet,", "tokens": ["In", "mei\u00b7ner", "See\u00b7len", "hel\u00b7le", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und euer stiller Freund nicht, an der Thorheit Brust,", "tokens": ["Und", "eu\u00b7er", "stil\u00b7ler", "Freund", "nicht", ",", "an", "der", "Thor\u00b7heit", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "PTKNEG", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach Fantasien lacht und weinet.", "tokens": ["Nach", "Fan\u00b7ta\u00b7si\u00b7en", "lacht", "und", "wei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "O la\u00dft, zu aller Zeit, mein Antlitz heiter seyn,", "tokens": ["O", "la\u00dft", ",", "zu", "al\u00b7ler", "Zeit", ",", "mein", "Ant\u00b7litz", "hei\u00b7ter", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "APPR", "PIAT", "NN", "$,", "PPOSAT", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht blo\u00df in sonnenvollen Tagen,", "tokens": ["Nicht", "blo\u00df", "in", "son\u00b7nen\u00b7vol\u00b7len", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wann mich die Freude sucht, und Saitenspiel und Wein", "tokens": ["Wann", "mich", "die", "Freu\u00b7de", "sucht", ",", "und", "Sai\u00b7ten\u00b7spiel", "und", "Wein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Wolken vor mir her verjagen:", "tokens": ["Die", "Wol\u00b7ken", "vor", "mir", "her", "ver\u00b7ja\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "APZR", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Nicht blo\u00df im dunkeln Busch und wo die Nachtigall", "tokens": ["Nicht", "blo\u00df", "im", "dun\u00b7keln", "Busch", "und", "wo", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPRART", "ADJA", "NN", "KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald singend \u00fcber mir verweilet,", "tokens": ["Bald", "sin\u00b7gend", "\u00fc\u00b7ber", "mir", "ver\u00b7wei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald an der Quelle seufzt, die reiner, als Crystall,", "tokens": ["Bald", "an", "der", "Quel\u00b7le", "seufzt", ",", "die", "rei\u00b7ner", ",", "als", "Crys\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geschw\u00e4tzig \u00fcber Kiesel eilet.", "tokens": ["Ge\u00b7schw\u00e4t\u00b7zig", "\u00fc\u00b7ber", "Kie\u00b7sel", "ei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Es m\u00fcss' auf meiner Stirn, wann schon die Erde bebt,", "tokens": ["Es", "m\u00fcss'", "auf", "mei\u00b7ner", "Stirn", ",", "wann", "schon", "die", "Er\u00b7de", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der g\u00f6ttliche Gedanke schimmern,", "tokens": ["Der", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ke", "schim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Tugend gl\u00fccklich ist und meine Seele lebt,", "tokens": ["Da\u00df", "Tu\u00b7gend", "gl\u00fcck\u00b7lich", "ist", "und", "mei\u00b7ne", "See\u00b7le", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "VAFIN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch unter ganzer Welten Tr\u00fcmmern!", "tokens": ["Auch", "un\u00b7ter", "gan\u00b7zer", "Wel\u00b7ten", "Tr\u00fcm\u00b7mern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "N