{"textgrid.poem.47353": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Quelle in der W\u00fcste", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich eine Quelle w\u00fc\u00dfte,", "tokens": ["Wenn", "ich", "ei\u00b7ne", "Quel\u00b7le", "w\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Die von lautrem Weine fl\u00f6sse,", "tokens": ["Die", "von", "laut\u00b7rem", "Wei\u00b7ne", "fl\u00f6s\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu ihr z\u00f6g' ich in die W\u00fcste,", "tokens": ["Zu", "ihr", "z\u00f6g'", "ich", "in", "die", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich ungest\u00f6rt gen\u00f6sse.", "tokens": ["Da\u00df", "ich", "un\u00b7ge\u00b7st\u00f6rt", "ge\u00b7n\u00f6s\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Eine H\u00fctte wollt' ich baun,", "tokens": ["Ei\u00b7ne", "H\u00fct\u00b7te", "wollt'", "ich", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So da\u00df \u00fcber ihre Schwelle", "tokens": ["So", "da\u00df", "\