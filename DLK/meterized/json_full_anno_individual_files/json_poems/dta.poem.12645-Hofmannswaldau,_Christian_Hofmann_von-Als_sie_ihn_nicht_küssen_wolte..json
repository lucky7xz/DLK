{"dta.poem.12645": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihn nicht k\u00fcssen wolte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der garten meiner lust f\u00fchlt itzt ein ungewitter,", "tokens": ["Der", "gar\u00b7ten", "mei\u00b7ner", "lust", "f\u00fchlt", "itzt", "ein", "un\u00b7ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein sturm des ungl\u00fccks weht auf seine felder hin,", "tokens": ["Ein", "sturm", "des", "un\u00b7gl\u00fccks", "weht", "auf", "sei\u00b7ne", "fel\u00b7der", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die vor gehoffte frucht schmeckt coloquinten-bitter,", "tokens": ["Die", "vor", "ge\u00b7hoff\u00b7te", "frucht", "schmeckt", "co\u00b7lo\u00b7quin\u00b7ten\u00b7bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich f\u00fchle den verlust und hoffe doch gewinn.", "tokens": ["Ich", "f\u00fch\u00b7le", "den", "ver\u00b7lust", "und", "hof\u00b7fe", "doch", "ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die rosen-knospen sind in ihrer bl\u00fct ersticket,", "tokens": ["Die", "ro\u00b7sen\u00b7knos\u00b7pen", "sind", "in", "ih\u00b7rer", "bl\u00fct", "er\u00b7sti\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kein zefyr nimmt sich mehr, wie vor, derselben an,", "tokens": ["Kein", "ze\u00b7fyr", "nimmt", "sich", "mehr", ",", "wie", "vor", ",", "der\u00b7sel\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "ADV", "$,", "PWAV", "PTKVZ", "$,", "PDAT", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Die sonne, die vorher geneigt auf sie geblicket,", "tokens": ["Die", "son\u00b7ne", ",", "die", "vor\u00b7her", "ge\u00b7neigt", "auf", "sie", "ge\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weist, da\u00df sie ihre gunst auch nun verh\u00fcllen kan.", "tokens": ["Weist", ",", "da\u00df", "sie", "ih\u00b7re", "gunst", "auch", "nun", "ver\u00b7h\u00fcl\u00b7len", "kan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verdr\u00fc\u00dfliches geschick! und unbelebtes leben!", "tokens": ["Ver\u00b7dr\u00fc\u00df\u00b7li\u00b7ches", "ge\u00b7schick", "!", "und", "un\u00b7be\u00b7leb\u00b7tes", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KON", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da man die seele fast nicht in dem leibe f\u00fchlt,", "tokens": ["Da", "man", "die", "see\u00b7le", "fast", "nicht", "in", "dem", "lei\u00b7be", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "VVFIN", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da man der sehnsucht bleibt die gantze zeit ergeben,", "tokens": ["Da", "man", "der", "sehn\u00b7sucht", "bleibt", "die", "gant\u00b7ze", "zeit", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und doch durch selbige nicht eine frucht erziehlt.", "tokens": ["Und", "doch", "durch", "sel\u00b7bi\u00b7ge", "nicht", "ei\u00b7ne", "frucht", "er\u00b7ziehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Chlorinde macht mir itzt den liebes-himmel tr\u00fcbe,", "tokens": ["Chlo\u00b7rin\u00b7de", "macht", "mir", "itzt", "den", "lie\u00b7bes\u00b7him\u00b7mel", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Doch will ihr regen nicht auf meine lippen ziehn,", "tokens": ["Doch", "will", "ihr", "re\u00b7gen", "nicht", "auf", "mei\u00b7ne", "lip\u00b7pen", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "ADJA", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie saget zwar, da\u00df sie mein wesen annoch liebe,", "tokens": ["Sie", "sa\u00b7get", "zwar", ",", "da\u00df", "sie", "mein", "we\u00b7sen", "an\u00b7noch", "lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPOSAT", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch aber soll ich mich um keinen ku\u00df bem\u00fchn.", "tokens": ["Doch", "a\u00b7ber", "soll", "ich", "mich", "um", "kei\u00b7nen", "ku\u00df", "be\u00b7m\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie reimt sich aber das? zwar lieben, doch nicht k\u00fcssen?", "tokens": ["Wie", "reimt", "sich", "a\u00b7ber", "das", "?", "zwar", "lie\u00b7ben", ",", "doch", "nicht", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "PDS", "$.", "ADV", "VVINF", "$,", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wo soll ein leben seyn, wo doch die seele fehlt?", "tokens": ["Wo", "soll", "ein", "le\u00b7ben", "seyn", ",", "wo", "doch", "die", "see\u00b7le", "fehlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "VVFIN", "VAINF", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich wei\u00df nicht, wes ich mich soll in der angst entschliessen,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wes", "ich", "mich", "soll", "in", "der", "angst", "ent\u00b7schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "PPER", "VMFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weil mich das ungemach zu seinem zweck erwehlt.", "tokens": ["Weil", "mich", "das", "un\u00b7ge\u00b7mach", "zu", "sei\u00b7nem", "zweck", "er\u00b7wehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "O da\u00df ich mir doch lie\u00df das garn der liebe legen,", "tokens": ["O", "da\u00df", "ich", "mir", "doch", "lie\u00df", "das", "garn", "der", "lie\u00b7be", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "O da\u00df ich so geschwind darein gegangen bin,", "tokens": ["O", "da\u00df", "ich", "so", "ge\u00b7schwind", "da\u00b7rein", "ge\u00b7gan\u00b7gen", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "ADJD", "PAV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich h\u00e4tte dieses erst bedeucklich solln erwegen,", "tokens": ["Ich", "h\u00e4t\u00b7te", "die\u00b7ses", "erst", "be\u00b7deuck\u00b7lich", "solln", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDS", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So fiel auf einmahl nicht der freuden trost-gewinn.", "tokens": ["So", "fiel", "auf", "ein\u00b7mahl", "nicht", "der", "freu\u00b7den", "trost\u00b7ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Was aber ist zu thun, der fehler ist geschehen,", "tokens": ["Was", "a\u00b7ber", "ist", "zu", "thun", ",", "der", "feh\u00b7ler", "ist", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PTKZU", "VVINF", "$,", "PRELS", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wer kost den liebes-safft und taumelt darnach nicht?", "tokens": ["Wer", "kost", "den", "lie\u00b7bes\u00b7\u00b7safft", "und", "tau\u00b7melt", "dar\u00b7nach", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "KON", "VVFIN", "PAV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So gehts, wenn wir auf was mit vollen blicken sehen,", "tokens": ["So", "gehts", ",", "wenn", "wir", "auf", "was", "mit", "vol\u00b7len", "bli\u00b7cken", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "APPR", "PRELS", "APPR", "ADJA", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Nicht aber, ob die lust auch das vergn\u00fcgen bricht.", "tokens": ["Nicht", "a\u00b7ber", ",", "ob", "die", "lust", "auch", "das", "ver\u00b7gn\u00fc\u00b7gen", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "ART", "NN", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die liebe leget ja zu kohlen weisse kreide,", "tokens": ["Die", "lie\u00b7be", "le\u00b7get", "ja", "zu", "koh\u00b7len", "weis\u00b7se", "krei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "PTKZU", "VVINF", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Zum besten diamant den schlechtsten kieselstein,", "tokens": ["Zum", "bes\u00b7ten", "di\u00b7a\u00b7mant", "den", "schlechts\u00b7ten", "kie\u00b7sel\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Zum allergr\u00f6hsten garn die allerz\u00e4rtsie seide,", "tokens": ["Zum", "al\u00b7ler\u00b7gr\u00f6hs\u00b7ten", "garn", "die", "al\u00b7ler\u00b7z\u00e4r\u00b7tsie", "sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Das ist: vor freude noth, vor licht den trauerschein.", "tokens": ["Das", "ist", ":", "vor", "freu\u00b7de", "noth", ",", "vor", "licht", "den", "trau\u00b7er\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$.", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ein ku\u00df ist mir versagt, wie wird es mit dem hertzen,", "tokens": ["Ein", "ku\u00df", "ist", "mir", "ver\u00b7sagt", ",", "wie", "wird", "es", "mit", "dem", "hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,", "PWAV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Wie wird es mit der gunst und ihrem geiste stehn?", "tokens": ["Wie", "wird", "es", "mit", "der", "gunst", "und", "ih\u00b7rem", "geis\u00b7te", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "ART", "NN", "KON", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mich deucht ich sehe schon von weitem gr\u00f6\u00dfre schmertzen,", "tokens": ["Mich", "deucht", "ich", "se\u00b7he", "schon", "von", "wei\u00b7tem", "gr\u00f6\u00df\u00b7re", "schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und die mir allbereit noch mehr zu hertzen gehn.", "tokens": ["Und", "die", "mir", "all\u00b7be\u00b7reit", "noch", "mehr", "zu", "hert\u00b7zen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "ADV", "ADV", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Erbarmt euch meiner doch ihr sternen und du gl\u00fccke?", "tokens": ["Er\u00b7barmt", "euch", "mei\u00b7ner", "doch", "ihr", "ster\u00b7nen", "und", "du", "gl\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADV", "PPOSAT", "ADJA", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "(die sterne red ich an, weils einer sonne gilt,)", "tokens": ["(", "die", "ster\u00b7ne", "red", "ich", "an", ",", "weils", "ei\u00b7ner", "son\u00b7ne", "gilt", ",", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und helfft, da\u00df mich ja nicht ein ungemach bestricke,", "tokens": ["Und", "helfft", ",", "da\u00df", "mich", "ja", "nicht", "ein", "un\u00b7ge\u00b7mach", "be\u00b7stri\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Jhr seyd ja, die ihr sonst der menschen sehnsucht stillt.", "tokens": ["Ihr", "seyd", "ja", ",", "die", "ihr", "sonst", "der", "men\u00b7schen", "sehn\u00b7sucht", "stillt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PRELS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich aber will getrost auf be\u00dfre zeiten hoffen,", "tokens": ["Ich", "a\u00b7ber", "will", "ge\u00b7trost", "auf", "be\u00df\u00b7re", "zei\u00b7ten", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die zeit ver\u00e4ndert auch der menschen harten sinn:", "tokens": ["Die", "zeit", "ver\u00b7\u00e4n\u00b7dert", "auch", "der", "men\u00b7schen", "har\u00b7ten", "sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Und hat mich itzund gleich ein harter sturm betroffen,", "tokens": ["Und", "hat", "mich", "it\u00b7zund", "gleich", "ein", "har\u00b7ter", "sturm", "be\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "So hoff ich doch davor was grosses zum gewinn.", "tokens": ["So", "hoff", "ich", "doch", "da\u00b7vor", "was", "gros\u00b7ses", "zum", "ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "PWS", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Die treue mu\u00df doch stets noch ihren zweck erhalten,", "tokens": ["Die", "treu\u00b7e", "mu\u00df", "doch", "stets", "noch", "ih\u00b7ren", "zweck", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ist gleich der anfang schwer, wird doch das ende gut.", "tokens": ["Ist", "gleich", "der", "an\u00b7fang", "schwer", ",", "wird", "doch", "das", "en\u00b7de", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$,", "VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Ich will die liebe nur indessen lassen walten,", "tokens": ["Ich", "will", "die", "lie\u00b7be", "nur", "in\u00b7des\u00b7sen", "las\u00b7sen", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "VVFIN", "ADV", "ADV", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Wer wei\u00df? was heute noch ihr arm vor wunder thut.", "tokens": ["Wer", "wei\u00df", "?", "was", "heu\u00b7te", "noch", "ihr", "arm", "vor", "wun\u00b7der", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "PWS", "ADV", "ADV", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}