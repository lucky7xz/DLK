{"textgrid.poem.51043": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Abendsonne lag am Bergeshang,", "tokens": ["Die", "A\u00b7bend\u00b7son\u00b7ne", "lag", "am", "Ber\u00b7ges\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich stieg hinan, und auf den goldnen Wegen", "tokens": ["Ich", "stieg", "hi\u00b7nan", ",", "und", "auf", "den", "gold\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kam weinend mir ein zartes Kind entgegen,", "tokens": ["Kam", "wei\u00b7nend", "mir", "ein", "zar\u00b7tes", "Kind", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das, mein nicht achtend, schreiend abw\u00e4rts sprang.", "tokens": ["Das", ",", "mein", "nicht", "ach\u00b7tend", ",", "schrei\u00b7end", "ab\u00b7w\u00e4rts", "sprang", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PPOSAT", "PTKNEG", "ADJD", "$,", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ums Haupt war duftig ihm ein Schein gelegen", "tokens": ["Ums", "Haupt", "war", "duf\u00b7tig", "ihm", "ein", "Schein", "ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "VAFIN", "ADJD", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Abendgold, das durch die L\u00f6cklein drang.", "tokens": ["Von", "A\u00b7bend\u00b7gold", ",", "das", "durch", "die", "L\u00f6c\u00b7klein", "drang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sah ihm nach, bis ich den Gramgesang", "tokens": ["Ich", "sah", "ihm", "nach", ",", "bis", "ich", "den", "Gram\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Kleinen nur noch h\u00f6rte aus den H\u00e4gen.", "tokens": ["Des", "Klei\u00b7nen", "nur", "noch", "h\u00f6r\u00b7te", "aus", "den", "H\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zuletzt verstummte er; denn freundlich Kosen", "tokens": ["Zu\u00b7letzt", "ver\u00b7stumm\u00b7te", "er", ";", "denn", "freund\u00b7lich", "Ko\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt ich den Schreihals liebevoll empfangen;", "tokens": ["H\u00f6rt", "ich", "den", "Schrei\u00b7hals", "lie\u00b7be\u00b7voll", "emp\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann t\u00f6nt' empor der Jubelruf des Losen.", "tokens": ["Dann", "t\u00f6nt'", "em\u00b7por", "der", "Ju\u00b7bel\u00b7ruf", "des", "Lo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich aber bin vollends hinaufgegangen,", "tokens": ["Ich", "a\u00b7ber", "bin", "vol\u00b7lends", "hin\u00b7auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo oben bleichten just die letzten Rosen,", "tokens": ["Wo", "o\u00b7ben", "bleich\u00b7ten", "just", "die", "letz\u00b7ten", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fern, wild und weh der Falken Stimmen klangen.", "tokens": ["Fern", ",", "wild", "und", "weh", "der", "Fal\u00b7ken", "Stim\u00b7men", "klan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Abendsonne lag am Bergeshang,", "tokens": ["Die", "A\u00b7bend\u00b7son\u00b7ne", "lag", "am", "Ber\u00b7ges\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich stieg hinan, und auf den goldnen Wegen", "tokens": ["Ich", "