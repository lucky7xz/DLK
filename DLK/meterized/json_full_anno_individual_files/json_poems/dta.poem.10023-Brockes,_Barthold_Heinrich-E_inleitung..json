{"dta.poem.10023": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "E inleitung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn iemand irgendswo in einer H\u00f6hle,", "tokens": ["Wenn", "ie\u00b7mand", "ir\u00b7gends\u00b7wo", "in", "ei\u00b7ner", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allwo desselben Sinn und Seele", "tokens": ["All\u00b7wo", "des\u00b7sel\u00b7ben", "Sinn", "und", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von aller Creatur und allem Vorwurff leer,", "tokens": ["Von", "al\u00b7ler", "Crea\u00b7tur", "und", "al\u00b7lem", "Vor\u00b7wurff", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "PIS", "NN", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In steter D\u00e4mmerung erzogen w\u00e4r;", "tokens": ["In", "ste\u00b7ter", "D\u00e4m\u00b7me\u00b7rung", "er\u00b7zo\u00b7gen", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und tr\u00e4t\u2019 auf einmahl in die Welt,", "tokens": ["Und", "tr\u00e4t'", "auf", "ein\u00b7mahl", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zumahl zur holden Fr\u00fchlings-Zeit,", "tokens": ["Zu\u00b7mahl", "zur", "hol\u00b7den", "Fr\u00fch\u00b7lings\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und s\u00e4he dann der Sonnen Herrlichkeit,", "tokens": ["Und", "s\u00e4\u00b7he", "dann", "der", "Son\u00b7nen", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und s\u00e4h\u2019 ein gr\u00fcn bebl\u00fchmtes Feld,", "tokens": ["Und", "s\u00e4h'", "ein", "gr\u00fcn", "be\u00b7bl\u00fchm\u00b7tes", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und s\u00e4he dick beb\u00fcschte H\u00fcgel,", "tokens": ["Und", "s\u00e4\u00b7he", "dick", "be\u00b7b\u00fcschte", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und s\u00e4he reiner B\u00e4che Spiegel,", "tokens": ["Und", "s\u00e4\u00b7he", "rei\u00b7ner", "B\u00e4\u00b7che", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Durch einen Schatten-reichen Wald,", "tokens": ["Durch", "ei\u00b7nen", "Schat\u00b7ten\u00b7rei\u00b7chen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Mit seiner sich drin spiegelnden Gestalt,", "tokens": ["Mit", "sei\u00b7ner", "sich", "drin", "spie\u00b7geln\u00b7den", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PRF", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Umkr\u00e4ntzt mit glatten Binsen, fliessen,", "tokens": ["Um\u00b7kr\u00e4ntzt", "mit", "glat\u00b7ten", "Bin\u00b7sen", ",", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und s\u00e4he Fl\u00fcsse sich ergiessen,", "tokens": ["Und", "s\u00e4\u00b7he", "Fl\u00fcs\u00b7se", "sich", "er\u00b7gies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Auch ihrer B\u00fcrger schuppicht Heer;", "tokens": ["Auch", "ih\u00b7rer", "B\u00fcr\u00b7ger", "schup\u00b7picht", "Heer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und s\u00e4h\u2019 ein un\u00fcmschr\u00e4ncktes Meer,", "tokens": ["Und", "s\u00e4h'", "ein", "un\u00b7\u00fcm\u00b7schr\u00e4nck\u00b7tes", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und s\u00e4he bunte G\u00e4rten prangen,", "tokens": ["Und", "s\u00e4\u00b7he", "bun\u00b7te", "G\u00e4r\u00b7ten", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Auch, wann die Sonn\u2019 erst untergangen,", "tokens": ["Auch", ",", "wann", "die", "Sonn'", "erst", "un\u00b7ter\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Der Abend-R\u00f6the g\u00fcldne Pracht;", "tokens": ["Der", "A\u00b7ben\u00b7dR\u00f6\u00b7the", "g\u00fcld\u00b7ne", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und s\u00e4h\u2019 in einer heitern Nacht", "tokens": ["Und", "s\u00e4h'", "in", "ei\u00b7ner", "hei\u00b7tern", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Den Wunder-sch\u00f6nen Sternen-Himmel;", "tokens": ["Den", "Wun\u00b7der\u00b7sch\u00f6\u00b7nen", "Ster\u00b7nen\u00b7Him\u00b7mel", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Zusammt dem Silber-reinen Glantz", "tokens": ["Zu\u00b7sammt", "dem", "Sil\u00b7ber\u00b7rei\u00b7nen", "Glantz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der Schatten-Sonne, wann sie gantz;", "tokens": ["Der", "Schat\u00b7ten\u00b7Son\u00b7ne", ",", "wann", "sie", "gantz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Der Singe-V\u00f6gel, und den Schall", "tokens": ["Der", "Sin\u00b7ge\u00b7V\u00f6\u00b7gel", ",", "und", "den", "Schall"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Der angenehmen Nachtigall,", "tokens": ["Der", "an\u00b7ge\u00b7neh\u00b7men", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "In Lust- und Schatten-reichen B\u00fcschen,", "tokens": ["In", "Lust", "und", "Schat\u00b7ten\u00b7rei\u00b7chen", "B\u00fc\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Sich mit dem sanften Rauschen mischen,", "tokens": ["Sich", "mit", "dem", "sanf\u00b7ten", "Rau\u00b7schen", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Und h\u00f6rt\u2019, auf rauh- und glatten Kieseln,", "tokens": ["Und", "h\u00f6rt'", ",", "auf", "rauh", "und", "glat\u00b7ten", "Kie\u00b7seln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Geschwinde B\u00e4che murmelnd rieseln;", "tokens": ["Ge\u00b7schwin\u00b7de", "B\u00e4\u00b7che", "mur\u00b7melnd", "rie\u00b7seln", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.32": {"text": "Und schmeckte vielerley Gerichte,", "tokens": ["Und", "schmeck\u00b7te", "vie\u00b7ler\u00b7ley", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Die Wasser, Lufft und Erde geben;", "tokens": ["Die", "Was\u00b7ser", ",", "Lufft", "und", "Er\u00b7de", "ge\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Und schmeckte, voller Geist und Krafft,", "tokens": ["Und", "schmeck\u00b7te", ",", "vol\u00b7ler", "Geist", "und", "Krafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Den s\u00e4urlich-s\u00fcssen Tranck und Safft", "tokens": ["Den", "s\u00e4ur\u00b7lich\u00b7s\u00fcs\u00b7sen", "Tranck", "und", "Safft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Der lieblichen Tockayer-Reben;", "tokens": ["Der", "lieb\u00b7li\u00b7chen", "To\u00b7ckayer\u00b7\u00b7Re\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.37": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.38": {"text": "In Feldern, W\u00e4ldern und im Garten;", "tokens": ["In", "Fel\u00b7dern", ",", "W\u00e4l\u00b7dern", "und", "im", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Und r\u00f6ch\u2019 auf Bergen und im Thal", "tokens": ["Und", "r\u00f6ch'", "auf", "Ber\u00b7gen", "und", "im", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Gesunde Kr\u00e4uter ohne Zahl;", "tokens": ["Ge\u00b7sun\u00b7de", "Kr\u00e4u\u00b7ter", "oh\u00b7ne", "Zahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Und r\u00f6che balsamirte D\u00fcffte;", "tokens": ["Und", "r\u00f6\u00b7che", "bal\u00b7sa\u00b7mir\u00b7te", "D\u00fcff\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.43": {"text": "Und f\u00fchlte Wunder-s\u00fcsse Triebe", "tokens": ["Und", "f\u00fchl\u00b7te", "Wun\u00b7der\u00b7s\u00fcs\u00b7se", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Von einer zugela\u00dfnen Liebe;", "tokens": ["Von", "ei\u00b7ner", "zu\u00b7ge\u00b7la\u00df\u00b7nen", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Und f\u00fchlte mit vergn\u00fcgter Brust,", "tokens": ["Und", "f\u00fchl\u00b7te", "mit", "ver\u00b7gn\u00fcg\u00b7ter", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Des s\u00fcssen Schlaffes sanfte Lust;", "tokens": ["Des", "s\u00fcs\u00b7sen", "Schlaf\u00b7fes", "sanf\u00b7te", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und f\u00fchlte, wann der Schlaff vorbey,", "tokens": ["Und", "f\u00fchl\u00b7te", ",", "wann", "der", "Schlaff", "vor\u00b7bey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Da\u00df er dadurch gest\u00e4rcket sey,", "tokens": ["Da\u00df", "er", "da\u00b7durch", "ge\u00b7st\u00e4r\u00b7cket", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Um alles, was so Wunder-sch\u00f6n,", "tokens": ["Um", "al\u00b7les", ",", "was", "so", "Wun\u00b7der\u00b7sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Aufs neue wiederum zu sehn.", "tokens": ["Aufs", "neu\u00b7e", "wie\u00b7de\u00b7rum", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf welche sonderbare Weise", "tokens": ["Auf", "wel\u00b7che", "son\u00b7der\u00b7ba\u00b7re", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00fcrd\u2019 er sich nicht darob ergetzen!", "tokens": ["W\u00fcrd'", "er", "sich", "nicht", "da\u00b7rob", "er\u00b7get\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PTKNEG", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00fcrd\u2019 er sich nicht halb selig sch\u00e4tzen?", "tokens": ["W\u00fcrd'", "er", "sich", "nicht", "halb", "se\u00b7lig", "sch\u00e4t\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "PTKNEG", "ADJD", "ADJD", "VVINF", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Er bliebe gantz gewi\u00df dabey,", "tokens": ["Er", "blie\u00b7be", "gantz", "ge\u00b7wi\u00df", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df er, aufs mindst\u2019 im Paradeise,", "tokens": ["Da\u00df", "er", ",", "aufs", "mind\u00b7st'", "im", "Pa\u00b7ra\u00b7dei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "ADJA", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wo nicht schon gar im Himmel sey.", "tokens": ["Wo", "nicht", "schon", "gar", "im", "Him\u00b7mel", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ADV", "ADV", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und wir, die alle diese Gaben", "tokens": ["Und", "wir", ",", "die", "al\u00b7le", "die\u00b7se", "Ga\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PRELS", "PIS", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unstreitig \u00fcm und an uns haben,", "tokens": ["Un\u00b7strei\u00b7tig", "\u00fcm", "und", "an", "uns", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "APPR", "PPER", "VAFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Empfindens minder als ein Stein;", "tokens": ["Emp\u00b7fin\u00b7dens", "min\u00b7der", "als", "ein", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ja machen uns, an deren Stelle,", "tokens": ["Ja", "ma\u00b7chen", "uns", ",", "an", "de\u00b7ren", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "$,", "APPR", "PRELAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Das Paradeis fast selbst zur H\u00f6lle.", "tokens": ["Das", "Pa\u00b7ra\u00b7deis", "fast", "selbst", "zur", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was mag daran wol Ursach seyn?", "tokens": ["Was", "mag", "da\u00b7ran", "wol", "Ur\u00b7sach", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PAV", "ADV", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}