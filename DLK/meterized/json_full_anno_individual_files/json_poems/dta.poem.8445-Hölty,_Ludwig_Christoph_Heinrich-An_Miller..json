{"dta.poem.8445": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Miller.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Fasst der Donnergedanke mich;", "tokens": ["Fasst", "der", "Don\u00b7ner\u00b7ge\u00b7dan\u00b7ke", "mich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dann bew\u00f6lkt sich mein Blick, starret zur Erd' hinab,", "tokens": ["Dann", "be\u00b7w\u00f6lkt", "sich", "mein", "Blick", ",", "star\u00b7ret", "zur", "Erd'", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Schaut nur Bilder der Traurigkeit.", "tokens": ["Schaut", "nur", "Bil\u00b7der", "der", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Ernst, mit finsterer Stirn, wandelt die Stunde her,", "tokens": ["Ernst", ",", "mit", "fins\u00b7te\u00b7rer", "Stirn", ",", "wan\u00b7delt", "die", "Stun\u00b7de", "her", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Die mich fernet von meinem Freund,", "tokens": ["Die", "mich", "fer\u00b7net", "von", "mei\u00b7nem", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wandelt ernster, und schnell fliegt der gezuckte Dolch", "tokens": ["Wan\u00b7delt", "erns\u00b7ter", ",", "und", "schnell", "fliegt", "der", "ge\u00b7zuck\u00b7te", "Dolch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.7": {"text": "In mein blutendes Herz hinab.", "tokens": ["In", "mein", "blu\u00b7ten\u00b7des", "Herz", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Eh dem Baume das Laub r\u00f6thlich und gelb entweht,", "tokens": ["Eh", "dem", "Bau\u00b7me", "das", "Laub", "r\u00f6th\u00b7lich", "und", "gelb", "ent\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Kommt der finstere Scheidetag,", "tokens": ["Kommt", "der", "fins\u00b7te\u00b7re", "Schei\u00b7de\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "St\u00fcrmt die Freunde hinweg, zucket und st\u00fcrzt den Dolch", "tokens": ["St\u00fcrmt", "die", "Freun\u00b7de", "hin\u00b7weg", ",", "zu\u00b7cket", "und", "st\u00fcrzt", "den", "Dolch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "In mein blutendes Herz hinab.", "tokens": ["In", "mein", "blu\u00b7ten\u00b7des", "Herz", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Wann nun wieder den Baum schattendes Gr\u00fcn umrauscht,", "tokens": ["Wann", "nun", "wie\u00b7der", "den", "Baum", "schat\u00b7ten\u00b7des", "Gr\u00fcn", "um\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Irr' ich einsam von Strauch zu Strauch:", "tokens": ["Irr'", "ich", "ein\u00b7sam", "von", "Strauch", "zu", "Strauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.14": {"text": "Vor des Einsamen Blick schliessen sich Blumen zu,", "tokens": ["Vor", "des", "Ein\u00b7sa\u00b7men", "Blick", "schlies\u00b7sen", "sich", "Blu\u00b7men", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PRF", "NN", "PTKVZ", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.15": {"text": "Und die rieselnde Quelle weint,", "tokens": ["Und", "die", "rie\u00b7seln\u00b7de", "Quel\u00b7le", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Und vom Nachtigallbusch t\u00f6net mir Seufzerlaut.", "tokens": ["Und", "vom", "Nach\u00b7ti\u00b7gall\u00b7busch", "t\u00f6\u00b7net", "mir", "Seuf\u00b7zer\u00b7laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Ach die Seelen der Abende,", "tokens": ["Ach", "die", "See\u00b7len", "der", "A\u00b7ben\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.18": {"text": "Die uns Freunden entflohn, sammeln sich dann um mich,", "tokens": ["Die", "uns", "Freun\u00b7den", "ent\u00b7flohn", ",", "sam\u00b7meln", "sich", "dann", "um", "mich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "PTKVZ", "$,", "VVFIN", "PRF", "ADV", "APPR", "PPER", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Sch\u00f6n und l\u00e4chelnd wie Serafim,", "tokens": ["Sch\u00f6n", "und", "l\u00e4\u00b7chelnd", "wie", "Se\u00b7ra\u00b7fim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KOKOM", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.20": {"text": "Und die Bilder der Ruh, welche die Fr\u00fchlingsnacht", "tokens": ["Und", "die", "Bil\u00b7der", "der", "Ruh", ",", "wel\u00b7che", "die", "Fr\u00fch\u00b7lings\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Auf uns Gl\u00fcckliche niedergoss,", "tokens": ["Auf", "uns", "Gl\u00fcck\u00b7li\u00b7che", "nie\u00b7der\u00b7goss", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "Deines trauten Gespr\u00e4chs werd' ich und Freundesblicks", "tokens": ["Dei\u00b7nes", "trau\u00b7ten", "Ge\u00b7spr\u00e4chs", "werd'", "ich", "und", "Freun\u00b7des\u00b7blicks"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "KON", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Dann begehren; und ach umsonst!", "tokens": ["Dann", "be\u00b7geh\u00b7ren", ";", "und", "ach", "um\u00b7sonst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$.", "KON", "ADV", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.24": {"text": "Deines Tugendgesangs, welcher mich himmelan", "tokens": ["Dei\u00b7nes", "Tu\u00b7gend\u00b7ge\u00b7sangs", ",", "wel\u00b7cher", "mich", "him\u00b7me\u00b7lan"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.25": {"text": "Oft gefl\u00fcgelt; und ach umsonst!", "tokens": ["Oft", "ge\u00b7fl\u00fc\u00b7gelt", ";", "und", "ach", "um\u00b7sonst", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$.", "KON", "ADV", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "In den Lauben des Mais, funkelt der Abendstern", "tokens": ["In", "den", "Lau\u00b7ben", "des", "Mais", ",", "fun\u00b7kelt", "der", "A\u00b7bends\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.27": {"text": "Durch die Bl\u00fcten, der oft belauscht", "tokens": ["Durch", "die", "Bl\u00fc\u00b7ten", ",", "der", "oft", "be\u00b7lauscht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVPP"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.28": {"text": "Unsrer Herzen Erguss, werd' ich dich sp\u00e4hn, den Arm", "tokens": ["Uns\u00b7rer", "Her\u00b7zen", "Er\u00b7guss", ",", "werd'", "ich", "dich", "sp\u00e4hn", ",", "den", "Arm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "$,", "VAFIN", "PPER", "PRF", "VVINF", "$,", "ART", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.29": {"text": "Nach dir strecken; und ach umsonst!", "tokens": ["Nach", "dir", "stre\u00b7cken", ";", "und", "ach", "um\u00b7sonst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$.", "KON", "ADV", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.30": {"text": "Nicht der flammende Wunsch, nicht der bethr\u00e4nte Blick", "tokens": ["Nicht", "der", "flam\u00b7men\u00b7de", "Wunsch", ",", "nicht", "der", "be\u00b7thr\u00e4n\u00b7te", "Blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$,", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Bringt dich wieder in meinen Arm;", "tokens": ["Bringt", "dich", "wie\u00b7der", "in", "mei\u00b7nen", "Arm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und mein Klagegesang ruft der Vergangenheit,", "tokens": ["Und", "mein", "Kla\u00b7ge\u00b7ge\u00b7sang", "ruft", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Bis mich h\u00fcllet die Rasengruft.", "tokens": ["Bis", "mich", "h\u00fcl\u00b7let", "die", "Ra\u00b7sen\u00b7gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.34": {"text": "Und die h\u00fcllet mich bald! Lispelt das Rebengr\u00fcn,", "tokens": ["Und", "die", "h\u00fcl\u00b7let", "mich", "bald", "!", "Lis\u00b7pelt", "das", "Re\u00b7ben\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "ADV", "$.", "NE", "ART", "NN", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Wo du horchest der Nachtigall,", "tokens": ["Wo", "du", "hor\u00b7chest", "der", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.36": {"text": "Zittert eine Gestalt, d\u00e4mmernd in mildem Glanz,", "tokens": ["Zit\u00b7tert", "ei\u00b7ne", "Ge\u00b7stalt", ",", "d\u00e4m\u00b7mernd", "in", "mil\u00b7dem", "Glanz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Leises Fluges vor dir vorbei,", "tokens": ["Lei\u00b7ses", "Flu\u00b7ges", "vor", "dir", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.38": {"text": "Winkt und l\u00e4chelt dir zu; Miller, es ist dein Freund!", "tokens": ["Winkt", "und", "l\u00e4\u00b7chelt", "dir", "zu", ";", "Mil\u00b7ler", ",", "es", "ist", "dein", "Freund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "PTKVZ", "$.", "NE", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Durch die Blumen des Gartenbeets", "tokens": ["Durch", "die", "Blu\u00b7men", "des", "Gar\u00b7ten\u00b7beets"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.40": {"text": "Weht der Schatten dahin: Ahndung durchbebt dein Herz,", "tokens": ["Weht", "der", "Schat\u00b7ten", "da\u00b7hin", ":", "Ahn\u00b7dung", "durch\u00b7bebt", "dein", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PAV", "$.", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.41": {"text": "Und du schauerst vom Rasen auf,", "tokens": ["Und", "du", "schau\u00b7erst", "vom", "Ra\u00b7sen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.42": {"text": "Wandelst n\u00e4her, und brichst, freudiger Wehmut voll,", "tokens": ["Wan\u00b7delst", "n\u00e4\u00b7her", ",", "und", "brichst", ",", "freu\u00b7di\u00b7ger", "Weh\u00b7mut", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "VVFIN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.43": {"text": "Dir die Blume, die, wankend noch", "tokens": ["Dir", "die", "Blu\u00b7me", ",", "die", ",", "wan\u00b7kend", "noch"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "PRELS", "$,", "ADJD", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.44": {"text": "Von des fliehenden Freunds Schimmergewand', im Thau", "tokens": ["Von", "des", "flie\u00b7hen\u00b7den", "Freunds", "Schim\u00b7mer\u00b7ge\u00b7wand'", ",", "im", "Thau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,", "APPRART", "NN"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.45": {"text": "Seiner rinnenden Z\u00e4hre gl\u00e4nzt.", "tokens": ["Sei\u00b7ner", "rin\u00b7nen\u00b7den", "Z\u00e4h\u00b7re", "gl\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}