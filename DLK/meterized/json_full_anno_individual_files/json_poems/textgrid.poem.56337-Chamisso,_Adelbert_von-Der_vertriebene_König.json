{"textgrid.poem.56337": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der vertriebene K\u00f6nig", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die alle freien Stimmen ihr verd\u00e4chtigt,", "tokens": ["Die", "al\u00b7le", "frei\u00b7en", "Stim\u00b7men", "ihr", "ver\u00b7d\u00e4ch\u00b7tigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So ihr, dasjenige euch vorzusagen,", "tokens": ["So", "ihr", ",", "das\u00b7je\u00b7ni\u00b7ge", "euch", "vor\u00b7zu\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was nur ihr h\u00f6ren wollt, nicht selbst erm\u00e4chtigt;", "tokens": ["Was", "nur", "ihr", "h\u00f6\u00b7ren", "wollt", ",", "nicht", "selbst", "er\u00b7m\u00e4ch\u00b7tigt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVINF", "VMFIN", "$,", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vernehmt die Stimme denn uralter Sagen;", "tokens": ["Ver\u00b7nehmt", "die", "Stim\u00b7me", "denn", "ur\u00b7al\u00b7ter", "Sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hie bin ich, schlicht die Worte des Verstandes", "tokens": ["Hie", "bin", "ich", ",", "schlicht", "die", "Wor\u00b7te", "des", "Ver\u00b7stan\u00b7des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus eurer V\u00e4ter Zeit euch vorzutragen.", "tokens": ["Aus", "eu\u00b7rer", "V\u00e4\u00b7ter", "Zeit", "euch", "vor\u00b7zu\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es war einmal ein K\u00f6nig Griechenlandes,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "K\u00f6\u00b7nig", "Grie\u00b7chen\u00b7lan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem segnend der Allm\u00e4chtige verliehen", "tokens": ["Dem", "seg\u00b7nend", "der", "All\u00b7m\u00e4ch\u00b7ti\u00b7ge", "ver\u00b7lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Macht, Weisheit und die Liebe seines Landes.", "tokens": ["Macht", ",", "Weis\u00b7heit", "und", "die", "Lie\u00b7be", "sei\u00b7nes", "Lan\u00b7des", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Er lie\u00df von Weisen seinen Sohn erziehen;", "tokens": ["Er", "lie\u00df", "von", "Wei\u00b7sen", "sei\u00b7nen", "Sohn", "er\u00b7zie\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die kamen denn und sprachen: \u00bbNimm ihn hin", "tokens": ["Die", "ka\u00b7men", "denn", "und", "spra\u00b7chen", ":", "\u00bb", "Nimm", "ihn", "hin"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "KON", "VVFIN", "$.", "$(", "VVIMP", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und pr\u00fcf ihn, unser Werk ist wohl gediehen.\u00ab", "tokens": ["Und", "pr\u00fcf", "ihn", ",", "un\u00b7ser", "Werk", "ist", "wohl", "ge\u00b7die\u00b7hen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und da\u00df er pr\u00fcfe seines Sohnes Sinn,", "tokens": ["Und", "da\u00df", "er", "pr\u00fc\u00b7fe", "sei\u00b7nes", "Soh\u00b7nes", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Hie\u00df vieles Gold aus seines Schatzes Hallen", "tokens": ["Hie\u00df", "vie\u00b7les", "Gold", "aus", "sei\u00b7nes", "Schat\u00b7zes", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Er holen und es legen vor ihn hin.", "tokens": ["Er", "ho\u00b7len", "und", "es", "le\u00b7gen", "vor", "ihn", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und vor den Rittern und Baronen allen,", "tokens": ["Und", "vor", "den", "Rit\u00b7tern", "und", "Ba\u00b7ro\u00b7nen", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Das Gold ihm schenkend, sprach er zu dem Sohne:", "tokens": ["Das", "Gold", "ihm", "schen\u00b7kend", ",", "sprach", "er", "zu", "dem", "Soh\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "\u00bbverwende dies nach deinem Wohlgefallen\u00ab,", "tokens": ["\u00bb", "ver\u00b7wen\u00b7de", "dies", "nach", "dei\u00b7nem", "Wohl\u00b7ge\u00b7fal\u00b7len", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PDS", "APPR", "PPOSAT", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und er befahl, die andern sollten, ohne", "tokens": ["Und", "er", "be\u00b7fahl", ",", "die", "an\u00b7dern", "soll\u00b7ten", ",", "oh\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "PRELS", "PIS", "VMFIN", "$,", "KOUI"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ihm Rat zu geben, scharf auf ihn nur sehen,", "tokens": ["Ihm", "Rat", "zu", "ge\u00b7ben", ",", "scharf", "auf", "ihn", "nur", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Und dann Bericht erstatten vor dem Throne.", "tokens": ["Und", "dann", "Be\u00b7richt", "er\u00b7stat\u00b7ten", "vor", "dem", "Thro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Da sah der K\u00f6nigssohn vor\u00fcbergehen", "tokens": ["Da", "sah", "der", "K\u00f6\u00b7nigs\u00b7sohn", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Die Karavanen aus den fernsten Orten,", "tokens": ["Die", "Ka\u00b7ra\u00b7va\u00b7nen", "aus", "den", "ferns\u00b7ten", "Or\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Und hie\u00df die Reisenden ihm Rede stehen.", "tokens": ["Und", "hie\u00df", "die", "Rei\u00b7sen\u00b7den", "ihm", "Re\u00b7de", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Gewandt und k\u00fchn, mit wohlerwognen Worten", "tokens": ["Ge\u00b7wandt", "und", "k\u00fchn", ",", "mit", "woh\u00b7ler\u00b7wog\u00b7nen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Sprach einer: \u00bbHerr, ich bin ein Handelsmann", "tokens": ["Sprach", "ei\u00b7ner", ":", "\u00bb", "Herr", ",", "ich", "bin", "ein", "Han\u00b7dels\u00b7mann"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "$.", "$(", "NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und mir geh\u00f6ren die Kamele dorten.", "tokens": ["Und", "mir", "ge\u00b7h\u00f6\u00b7ren", "die", "Ka\u00b7me\u00b7le", "dor\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Durch eigene Betriebsamkeit gewann", "tokens": ["Durch", "ei\u00b7ge\u00b7ne", "Be\u00b7trieb\u00b7sam\u00b7keit", "ge\u00b7wann"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Ich Sch\u00e4tze, die ich keinem sonst verdanke,", "tokens": ["Ich", "Sch\u00e4t\u00b7ze", ",", "die", "ich", "kei\u00b7nem", "sonst", "ver\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Da mir das Land und mancher danken kann.\u00ab", "tokens": ["Da", "mir", "das", "Land", "und", "man\u00b7cher", "dan\u00b7ken", "kann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "PIS", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Ein zweiter sprach, verloren in Gedanken, \u2013", "tokens": ["Ein", "zwei\u00b7ter", "sprach", ",", "ver\u00b7lo\u00b7ren", "in", "Ge\u00b7dan\u00b7ken", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "VVPP", "APPR", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Er w\u00e4re lieber unbefragt geblieben, \u2013", "tokens": ["Er", "w\u00e4\u00b7re", "lie\u00b7ber", "un\u00b7be\u00b7fragt", "ge\u00b7blie\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Indem zur Erde seine Blicke sanken:", "tokens": ["In\u00b7dem", "zur", "Er\u00b7de", "sei\u00b7ne", "Bli\u00b7cke", "san\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "\u00bbich bin der K\u00f6nig Syriens, den vertrieben", "tokens": ["\u00bb", "ich", "bin", "der", "K\u00f6\u00b7nig", "Sy\u00b7ri\u00b7ens", ",", "den", "ver\u00b7trie\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "NE", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Die aufgeregten V\u00f6lker; mein Verhalten", "tokens": ["Die", "auf\u00b7ge\u00b7reg\u00b7ten", "V\u00f6l\u00b7ker", ";", "mein", "Ver\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "War so, da\u00df sie die Schuld mir zugeschrieben.\u00ab", "tokens": ["War", "so", ",", "da\u00df", "sie", "die", "Schuld", "mir", "zu\u00b7ge\u00b7schrie\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Und alles Gold, wor\u00fcber er zu schalten,", "tokens": ["Und", "al\u00b7les", "Gold", ",", "wo\u00b7r\u00fc\u00b7ber", "er", "zu", "schal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PWAV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Gab diesem alsobald das K\u00f6nigskind,", "tokens": ["Gab", "die\u00b7sem", "al\u00b7so\u00b7bald", "das", "K\u00f6\u00b7nigs\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Darob entr\u00fcstet die Barone schalten.", "tokens": ["Da\u00b7rob", "ent\u00b7r\u00fcs\u00b7tet", "die", "Ba\u00b7ro\u00b7ne", "schal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Sie klagten vor dem Throne: \u00bbHerr, es sind", "tokens": ["Sie", "klag\u00b7ten", "vor", "dem", "Thro\u00b7ne", ":", "\u00bb", "Herr", ",", "es", "sind"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Nicht deines Sohnes Taten lobenswert;", "tokens": ["Nicht", "dei\u00b7nes", "Soh\u00b7nes", "Ta\u00b7ten", "lo\u00b7bens\u00b7wert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "Er schlug der Weisheit Lehren in den Wind,", "tokens": ["Er", "schlug", "der", "Weis\u00b7heit", "Leh\u00b7ren", "in", "den", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Er lie\u00df den Wohlverdienten unbeehrt,", "tokens": ["Er", "lie\u00df", "den", "Wohl\u00b7ver\u00b7dien\u00b7ten", "un\u00b7beehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.44": {"text": "Indem er unbesonnen seine Gabe", "tokens": ["In\u00b7dem", "er", "un\u00b7be\u00b7son\u00b7nen", "sei\u00b7ne", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Dem andern Unbesonnenen beschert.\u00ab", "tokens": ["Dem", "an\u00b7dern", "Un\u00b7be\u00b7son\u00b7ne\u00b7nen", "be\u00b7schert", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Es wurde vorgefodert nun der Knabe,", "tokens": ["Es", "wur\u00b7de", "vor\u00b7ge\u00b7fo\u00b7dert", "nun", "der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Da\u00df Rechenschaft er g\u00e4be, wie verwendet", "tokens": ["Da\u00df", "Re\u00b7chen\u00b7schaft", "er", "g\u00e4\u00b7be", ",", "wie", "ver\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$,", "PWAV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Das seiner Hand vertraute Gut er habe.", "tokens": ["Das", "sei\u00b7ner", "Hand", "ver\u00b7trau\u00b7te", "