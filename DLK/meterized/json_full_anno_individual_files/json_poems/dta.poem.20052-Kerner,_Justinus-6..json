{"dta.poem.20052": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.71", "sv:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm', Br\u00e4ut'gam! kommt, ihr G\u00e4ste!", "tokens": ["Komm'", ",", "Br\u00e4ut'\u00b7gam", "!", "kommt", ",", "ihr", "G\u00e4s\u00b7te", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schon steht im Hochzeitkleid", "tokens": ["Schon", "steht", "im", "Hoch\u00b7zeit\u00b7kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die bleiche Braut bereit,", "tokens": ["Die", "blei\u00b7che", "Braut", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erwartend euch zum Feste.", "tokens": ["Er\u00b7war\u00b7tend", "euch", "zum", "Fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Herbey! herbey! zum Tanz", "tokens": ["Her\u00b7bey", "!", "her\u00b7bey", "!", "zum", "Tanz"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["PTKVZ", "$.", "PTKVZ", "$.", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die bleiche Braut zu f\u00fchren, \u2014", "tokens": ["Die", "blei\u00b7che", "Braut", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seht! ihre Haare zieren", "tokens": ["Seht", "!", "ih\u00b7re", "Haa\u00b7re", "zie\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So Ros' als Lilienkranz.", "tokens": ["So", "Ros'", "als", "Li\u00b7li\u00b7en\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KOUS", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "So Mond und Sterne kr\u00e4nzen", "tokens": ["So", "Mond", "und", "Ster\u00b7ne", "kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lichtvoll das dunkle Thal,", "tokens": ["Licht\u00b7voll", "das", "dunk\u00b7le", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Lampen im Hochzeitsaal,", "tokens": ["Lam\u00b7pen", "im", "Hoch\u00b7zeit\u00b7saal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Die Leichensteine gl\u00e4nzen.", "tokens": ["Die", "Lei\u00b7chen\u00b7stei\u00b7ne", "gl\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und weil nach Tanz und Lauf", "tokens": ["Und", "weil", "nach", "Tanz", "und", "Lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Ruh wir n\u00f6thig h\u00e4tten, \u2014", "tokens": ["Der", "Ruh", "wir", "n\u00f6\u00b7thig", "h\u00e4t\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VAFIN", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schlo\u00df ich zu Schlummerst\u00e4tten", "tokens": ["Schlo\u00df", "ich", "zu", "Schlum\u00b7mer\u00b7st\u00e4t\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die stillen Gr\u00e4ber auf.", "tokens": ["Die", "stil\u00b7len", "Gr\u00e4\u00b7ber", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Seht! eure Betzte kr\u00e4nzet", "tokens": ["Seht", "!", "eu\u00b7re", "Betz\u00b7te", "kr\u00e4n\u00b7zet"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Rosen stolze Art,", "tokens": ["Der", "Ro\u00b7sen", "stol\u00b7ze", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch eine Lilie zart", "tokens": ["Doch", "ei\u00b7ne", "Li\u00b7lie", "zart"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Am Bett' der Braut ergl\u00e4nzet.", "tokens": ["Am", "Bett'", "der", "Braut", "er\u00b7gl\u00e4n\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Hochzeit ist bereit,", "tokens": ["Die", "Hoch\u00b7zeit", "ist", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Komm', Br\u00e4ut'gam! kommt, ihr G\u00e4ste", "tokens": ["Komm'", ",", "Br\u00e4ut'\u00b7gam", "!", "kommt", ",", "ihr", "G\u00e4s\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "NE", "$.", "VVFIN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es \u00f6ffnen sich zum Feste", "tokens": ["Es", "\u00f6ff\u00b7nen", "sich", "zum", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die schwarzen Thore weit!", "tokens": ["Die", "schwar\u00b7zen", "Tho\u00b7re", "weit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}