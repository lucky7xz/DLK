{"dta.poem.7372": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, einen Feind hat der Cors', den Hund,", "tokens": ["Ja", ",", "ei\u00b7nen", "Feind", "hat", "der", "Cor\u00b7s'", ",", "den", "Hund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Luigi, den hagern Podesta,", "tokens": ["Lui\u00b7gi", ",", "den", "ha\u00b7gern", "Po\u00b7des\u00b7ta", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der den Ohm, so stark und gesund,", "tokens": ["Der", "den", "Ohm", ",", "so", "stark", "und", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Lie\u00df henken, den k\u00fchnen di Vesta.", "tokens": ["Lie\u00df", "hen\u00b7ken", ",", "den", "k\u00fch\u00b7nen", "di", "Ves\u00b7ta", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$,", "ART", "ADJA", "NE", "NE", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Er und der rothe Franzose Jocliffe,", "tokens": ["Er", "und", "der", "ro\u00b7the", "Fran\u00b7zo\u00b7se", "Jo\u00b7clif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die Beiden machten ihn hangen,", "tokens": ["Die", "Bei\u00b7den", "mach\u00b7ten", "ihn", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Aber der ging zu dem Schmugglerschiff,", "tokens": ["A\u00b7ber", "der", "ging", "zu", "dem", "Schmug\u00b7gler\u00b7schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Und liegt seit Monden gefangen.", "tokens": ["Und", "liegt", "seit", "Mon\u00b7den", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Steht im Walde Geronimo,", "tokens": ["Steht", "im", "Wal\u00b7de", "Ge\u00b7ro\u00b7ni\u00b7mo", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Und klirrend zieht aus der Scheide", "tokens": ["Und", "klir\u00b7rend", "zieht", "aus", "der", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er das Messer, so und so", "tokens": ["Er", "das", "Mes\u00b7ser", ",", "so", "und", "so"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "An der Sohle wetzt er die Schneide;", "tokens": ["An", "der", "Soh\u00b7le", "wetzt", "er", "die", "Schnei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Gleitet dann in die D\u00e4mmerung,", "tokens": ["Glei\u00b7tet", "dann", "in", "die", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Dem Feinde auf Tod und Leben", "tokens": ["Dem", "Fein\u00b7de", "auf", "Tod", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Mit des Thieres Verst\u00fcmmelung", "tokens": ["Mit", "des", "Thie\u00b7res", "Ver\u00b7st\u00fcm\u00b7me\u00b7lung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.8": {"text": "Ein corsisch Cartel zu geben.", "tokens": ["Ein", "cor\u00b7sisch", "Car\u00b7tel", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Schau! wie Zweig an Zweige er streicht,", "tokens": ["Schau", "!", "wie", "Zweig", "an", "Zwei\u00b7ge", "er", "streicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAV", "NN", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u2014 Kaum fl\u00fcsternd die Bl\u00e4tter schwanken, \u2014", "tokens": ["Kaum", "fl\u00fcs\u00b7ternd", "die", "Bl\u00e4t\u00b7ter", "schwan\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "ART", "NN", "VVINF", "$,", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Gleich der glei\u00dfenden Boa leicht", "tokens": ["Gleich", "der", "glei\u00b7\u00dfen\u00b7den", "Boa", "leicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Hinquillt durch Gelaub und Ranken;", "tokens": ["Hin\u00b7quillt", "durch", "Ge\u00b7laub", "und", "Ran\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dr\u00fcber tr\u00e4ufelt das Mondenlicht,", "tokens": ["Dr\u00fc\u00b7ber", "tr\u00e4u\u00b7felt", "das", "Mon\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wie heimlicher Thr\u00e4ne Klage", "tokens": ["Wie", "heim\u00b7li\u00b7cher", "Thr\u00e4\u00b7ne", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Durch eine dunkele Wimper bricht.", "tokens": ["Durch", "ei\u00b7ne", "dun\u00b7ke\u00b7le", "Wim\u00b7per", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Nun kniet der Corse am Haage.", "tokens": ["Nun", "kniet", "der", "Cor\u00b7se", "am", "Haa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Dort der Anger, \u2014 und dort am Hang", "tokens": ["Dort", "der", "An\u00b7ger", ",", "und", "dort", "am", "Hang"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "$(", "KON", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die einsam weidende Stute,", "tokens": ["Die", "ein\u00b7sam", "wei\u00b7den\u00b7de", "Stu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Langsam schnaubt sie den Rain entlang;", "tokens": ["Lang\u00b7sam", "schnaubt", "sie", "den", "Rain", "ent\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "APPO", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Aus andalusischem Blute,", "tokens": ["Aus", "an\u00b7da\u00b7lu\u00b7si\u00b7schem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hoch, schneeschimmernd, zum Grund gebeugt", "tokens": ["Hoch", ",", "schnee\u00b7schim\u00b7mernd", ",", "zum", "Grund", "ge\u00b7beugt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "APPRART", "NN", "VVPP"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Den m\u00e4hnumflutheten Nacken,", "tokens": ["Den", "m\u00e4h\u00b7num\u00b7flut\u00b7he\u00b7ten", "Na\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Nah sie, n\u00e4her dem Hagen steigt.", "tokens": ["Nah", "sie", ",", "n\u00e4\u00b7her", "dem", "Ha\u00b7gen", "steigt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Nun wird der Corse sie packen!", "tokens": ["Nun", "wird", "der", "Cor\u00b7se", "sie", "pa\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Schon erfa\u00dft er der Schneide Griff,", "tokens": ["Schon", "er\u00b7fa\u00dft", "er", "der", "Schnei\u00b7de", "Griff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Er reckt sich \u00fcber dem Kraute,", "tokens": ["Er", "reckt", "sich", "\u00fc\u00b7ber", "dem", "Krau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da \u2014 ein Geknister und \u2014 still! ein Pfiff,", "tokens": ["Da", "ein", "Ge\u00b7knis\u00b7ter", "und", "still", "!", "ein", "Pfiff", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "$(", "ART", "NN", "KON", "$(", "PTKVZ", "$.", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und wieder \u2014 summende Laute!", "tokens": ["Und", "wie\u00b7der", "sum\u00b7men\u00b7de", "Lau\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und es schreitet dem Hage zu,", "tokens": ["Und", "es", "schrei\u00b7tet", "dem", "Ha\u00b7ge", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Grad wo Geronimo knieet,", "tokens": ["Grad", "wo", "Ge\u00b7ro\u00b7ni\u00b7mo", "kni\u00b7e\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PWAV", "NE", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Nieder gleitet der Cors' im Nu,", "tokens": ["Nie\u00b7der", "glei\u00b7tet", "der", "Cor\u00b7s'", "im", "Nu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ha, wie er keuchet und gl\u00fchet!", "tokens": ["Ha", ",", "wie", "er", "keuc\u00b7het", "und", "gl\u00fc\u00b7het", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Dicht an ihm, \u2014 der Mantel streift,", "tokens": ["Dicht", "an", "ihm", ",", "der", "Man\u00b7tel", "streift", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "$(", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Ferse k\u00f6nnt' er ihm fassen, \u2014", "tokens": ["Die", "Fer\u00b7se", "k\u00f6nnt'", "er", "ihm", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Steht der hagre Podest' und pfeift;", "tokens": ["Steht", "der", "hag\u00b7re", "Po\u00b7dest'", "und", "pfeift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "\u201esorella! ruft er gelassen,", "tokens": ["\u201e", "so\u00b7rel\u00b7la", "!", "ruft", "er", "ge\u00b7las\u00b7sen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und \u201eSorella, mein kluges Thier!\u201c", "tokens": ["Und", "\u201e", "So\u00b7rel\u00b7la", ",", "mein", "klu\u00b7ges", "Thier", "!", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "NE", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der Lauscher h\u00f6ret es stampfen,", "tokens": ["Der", "Lau\u00b7scher", "h\u00f6\u00b7ret", "es", "stamp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ueber ihm, mit hellem Gewieh'r,", "tokens": ["Ue\u00b7ber", "ihm", ",", "mit", "hel\u00b7lem", "Ge\u00b7wieh'r", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Zwei schnaubende N\u00fcstern dampfen.", "tokens": ["Zwei", "schnau\u00b7ben\u00b7de", "N\u00fcs\u00b7tern", "damp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Freundlich klatscht Luigi den Bug,", "tokens": ["Freund\u00b7lich", "klatscht", "Lui\u00b7gi", "den", "Bug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Liebkosend streicht er die M\u00e4hnen,", "tokens": ["Lieb\u00b7ko\u00b7send", "streicht", "er", "die", "M\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Hat nicht z\u00e4rtlicher Worte genug,", "tokens": ["Hat", "nicht", "z\u00e4rt\u00b7li\u00b7cher", "Wor\u00b7te", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJA", "NN", "ADV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Er spricht wie zu seiner Sch\u00f6nen.", "tokens": ["Er", "spricht", "wie", "zu", "sei\u00b7ner", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Einen Blitz aus gl\u00fchendem Aug',", "tokens": ["Ei\u00b7nen", "Blitz", "aus", "gl\u00fc\u00b7hen\u00b7dem", "Aug'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und r\u00fcckw\u00e4rts taumelt die Stute.", "tokens": ["Und", "r\u00fcck\u00b7w\u00e4rts", "tau\u00b7melt", "die", "Stu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u201eei, Sorella, was fehlt dir auch?", "tokens": ["\u201e", "ei", ",", "So\u00b7rel\u00b7la", ",", "was", "fehlt", "dir", "auch", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NE", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Mein T\u00f6chterchen, meine Gute.\u201c", "tokens": ["Mein", "T\u00f6ch\u00b7ter\u00b7chen", ",", "mei\u00b7ne", "Gu\u00b7te", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Candiszucker langt er hervor;", "tokens": ["Can\u00b7dis\u00b7zu\u00b7cker", "langt", "er", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ha, wie ihre N\u00fcstern blasen!", "tokens": ["Ha", ",", "wie", "ih\u00b7re", "N\u00fcs\u00b7tern", "bla\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie sie naschet, gespitzt das Ohr,", "tokens": ["Wie", "sie", "na\u00b7schet", ",", "ge\u00b7spitzt", "das", "Ohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVPP", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und immer glotzet zum Rasen!", "tokens": ["Und", "im\u00b7mer", "glot\u00b7zet", "zum", "Ra\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Einen Blick d