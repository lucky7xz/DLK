{"dta.poem.9086": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ueber die Welt hin ziehen die Wolken.", "tokens": ["Ue\u00b7ber", "die", "Welt", "hin", "zie\u00b7hen", "die", "Wol\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Gr\u00fcn durch die W\u00e4lder", "tokens": ["Gr\u00fcn", "durch", "die", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "fliesst ihr Licht.", "tokens": ["fliesst", "ihr", "Licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Herz, vergiss!", "tokens": ["Herz", ",", "ver\u00b7giss", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "In stiller Sonne", "tokens": ["In", "stil\u00b7ler", "Son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "webt linderndster Zauber,", "tokens": ["webt", "lin\u00b7dernds\u00b7ter", "Zau\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "unter wehenden Blumen bl\u00fcht tausend Trost.", "tokens": ["un\u00b7ter", "we\u00b7hen\u00b7den", "Blu\u00b7men", "bl\u00fcht", "tau\u00b7send", "Trost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "CARD", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": " vergiss! Vergiss!", "tokens": ["ver\u00b7giss", "!", "Ver\u00b7giss", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Aus fernem Grund pfeift, horch, ein Vogel. . . .", "tokens": ["Aus", "fer\u00b7nem", "Grund", "pfeift", ",", "horch", ",", "ein", "Vo\u00b7gel", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "VVIMP", "$,", "ART", "NE", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er singt sein Lied.", "tokens": ["Er", "singt", "sein", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Das Lied vom Gl\u00fcck!", "tokens": ["Das", "Lied", "vom", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Vom Gl\u00fcck.", "tokens": ["Vom", "Gl\u00fcck", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}