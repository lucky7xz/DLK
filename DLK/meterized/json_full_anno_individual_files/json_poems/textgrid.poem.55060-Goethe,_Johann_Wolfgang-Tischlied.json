{"textgrid.poem.55060": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Tischlied", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich ergreift, ich wei\u00df nicht wie,", "tokens": ["Mich", "er\u00b7greift", ",", "ich", "wei\u00df", "nicht", "wie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Himmlisches Behagen.", "tokens": ["Himm\u00b7li\u00b7sches", "Be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Will mich's etwa gar hinauf", "tokens": ["Will", "mich's", "et\u00b7wa", "gar", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "ADV"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.4": {"text": "Zu den Sternen tragen?", "tokens": ["Zu", "den", "Ster\u00b7nen", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Doch ich bleibe lieber hier,", "tokens": ["Doch", "ich", "blei\u00b7be", "lie\u00b7ber", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann ich redlich sagen,", "tokens": ["Kann", "ich", "red\u00b7lich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Beim Gesang und Glase Wein", "tokens": ["Beim", "Ge\u00b7sang", "und", "Gla\u00b7se", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf den Tisch zu schlagen.", "tokens": ["Auf", "den", "Tisch", "zu", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wundert euch, ihr Freunde, nicht,", "tokens": ["Wun\u00b7dert", "euch", ",", "ihr", "Freun\u00b7de", ",", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ich mich geb\u00e4rde;", "tokens": ["Wie", "ich", "mich", "ge\u00b7b\u00e4r\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wirklich ist es allerliebst", "tokens": ["Wirk\u00b7lich", "ist", "es", "al\u00b7ler\u00b7liebst"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der lieben Erde:", "tokens": ["Auf", "der", "lie\u00b7ben", "Er\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Darum schw\u00f6r ich feierlich", "tokens": ["Da\u00b7rum", "schw\u00f6r", "ich", "fei\u00b7er\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ohn alle F\u00e4hrde,", "tokens": ["Und", "ohn", "al\u00b7le", "F\u00e4hr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df ich mich nicht freventlich", "tokens": ["Da\u00df", "ich", "mich", "nicht", "fre\u00b7vent\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wegbegeben werde.", "tokens": ["Weg\u00b7be\u00b7ge\u00b7ben", "wer\u00b7de", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Da wir aber allzumal", "tokens": ["Da", "wir", "a\u00b7ber", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So beisammen weilen,", "tokens": ["So", "bei\u00b7sam\u00b7men", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "D\u00e4cht ich, kl\u00e4nge der Pokal", "tokens": ["D\u00e4cht", "ich", ",", "kl\u00e4n\u00b7ge", "der", "Po\u00b7kal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu des Dichters Zeilen.", "tokens": ["Zu", "des", "Dich\u00b7ters", "Zei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Gute Freunde ziehen fort,", "tokens": ["Gu\u00b7te", "Freun\u00b7de", "zie\u00b7hen", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wohl ein Hundert Meilen,", "tokens": ["Wohl", "ein", "Hun\u00b7dert", "Mei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "CARD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Darum soll man hier am Ort", "tokens": ["Da\u00b7rum", "soll", "man", "hier", "am", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PIS", "ADV", "APPRART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Anzusto\u00dfen eilen.", "tokens": ["An\u00b7zu\u00b7sto\u00b7\u00dfen", "ei\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Lebe hoch, wer Leben schafft!", "tokens": ["Le\u00b7be", "hoch", ",", "wer", "Le\u00b7ben", "schafft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ist meine Lehre.", "tokens": ["Das", "ist", "mei\u00b7ne", "Leh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unser K\u00f6nig denn voran,", "tokens": ["Un\u00b7ser", "K\u00f6\u00b7nig", "denn", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm geb\u00fchrt die Ehre.", "tokens": ["Ihm", "ge\u00b7b\u00fchrt", "die", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Gegen inn' und \u00e4u\u00dfern Feind", "tokens": ["Ge\u00b7gen", "inn'", "und", "\u00e4u\u00b7\u00dfern", "Feind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "KON", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Setzt er sich zur Wehre;", "tokens": ["Setzt", "er", "sich", "zur", "Weh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ans Erhalten denkt er zwar,", "tokens": ["Ans", "Er\u00b7hal\u00b7ten", "denkt", "er", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mehr noch, wie er mehre.", "tokens": ["Mehr", "noch", ",", "wie", "er", "meh\u00b7re", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nun begr\u00fc\u00df ich sie sogleich,", "tokens": ["Nun", "be\u00b7gr\u00fc\u00df", "ich", "sie", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie, die einzig Eine.", "tokens": ["Sie", ",", "die", "ein\u00b7zig", "Ei\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADJD", "ART", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Jeder denke ritterlich", "tokens": ["Je\u00b7der", "den\u00b7ke", "rit\u00b7ter\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich dabei die Seine.", "tokens": ["Sich", "da\u00b7bei", "die", "Sei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "PAV", "ART", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Merket auch ein sch\u00f6nes Kind,", "tokens": ["Mer\u00b7ket", "auch", "ein", "sch\u00f6\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wen ich eben meine,", "tokens": ["Wen", "ich", "e\u00b7ben", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PPOSAT", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nun, so nicke sie mir zu:", "tokens": ["Nun", ",", "so", "ni\u00b7cke", "sie", "mir", "zu", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Leb auch so der Meine!", "tokens": ["Leb", "auch", "so", "der", "Mei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Freunden gilt das dritte Glas,", "tokens": ["Freun\u00b7den", "gilt", "das", "drit\u00b7te", "Glas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zweien oder dreien,", "tokens": ["Zwei\u00b7en", "o\u00b7der", "drei\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "KON", "CARD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die mit uns am guten Tag", "tokens": ["Die", "mit", "uns", "am", "gu\u00b7ten", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich im stillen freuen", "tokens": ["Sich", "im", "stil\u00b7len", "freu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und der Nebel tr\u00fcbe Nacht", "tokens": ["Und", "der", "Ne\u00b7bel", "tr\u00fc\u00b7be", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Leis und leicht zerstreuen;", "tokens": ["Leis", "und", "leicht", "zer\u00b7streu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Diesen sei ein Hoch gebracht,", "tokens": ["Die\u00b7sen", "sei", "ein", "Hoch", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alten oder neuen.", "tokens": ["Al\u00b7ten", "o\u00b7der", "neu\u00b7en", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Breiter wallet nun der Strom,", "tokens": ["Brei\u00b7ter", "wal\u00b7let", "nun", "der", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit vermehrten Wellen.", "tokens": ["Mit", "ver\u00b7mehr\u00b7ten", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Leben jetzt im hohen Ton", "tokens": ["Le\u00b7ben", "jetzt", "im", "ho\u00b7hen", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Redliche Gesellen!", "tokens": ["Red\u00b7li\u00b7che", "Ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Die sich mit gedr\u00e4ngter Kraft", "tokens": ["Die", "sich", "mit", "ge\u00b7dr\u00e4ng\u00b7ter", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Brav zusammenstellen", "tokens": ["Brav", "zu\u00b7sam\u00b7men\u00b7stel\u00b7len"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "In des Gl\u00fcckes Sonnenschein", "tokens": ["In", "des", "Gl\u00fc\u00b7ckes", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und in schlimmen F\u00e4llen.", "tokens": ["Und", "in", "schlim\u00b7men", "F\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Wie wir nun zusammen sind,", "tokens": ["Wie", "wir", "nun", "zu\u00b7sam\u00b7men", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind zusammen viele.", "tokens": ["Sind", "zu\u00b7sam\u00b7men", "vie\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wohl gelingen denn, wie uns,", "tokens": ["Wohl", "ge\u00b7lin\u00b7gen", "denn", ",", "wie", "uns", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "$,", "PWAV", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Andern ihre Spiele!", "tokens": ["An\u00b7dern", "ih\u00b7re", "Spie\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Von der Quelle bis ans Meer", "tokens": ["Von", "der", "Quel\u00b7le", "bis", "ans", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mahlet manche M\u00fchle,", "tokens": ["Mah\u00b7let", "man\u00b7che", "M\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und das Wohl der ganzen Welt", "tokens": ["Und", "das", "Wohl", "der", "gan\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist's, worauf ich ziele.", "tokens": ["Ist's", ",", "wo\u00b7rauf", "ich", "zie\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Mich ergreift, ich wei\u00df nicht wie,", "tokens": ["Mich", "er\u00b7greift", ",", "ich", "wei\u00df", "nicht", "wie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "KOKOM", "$,"], "meter": "+-+-+-+