{"dta.poem.3957": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "XvII.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wei\u00dfheit will ihr Violet auf meines Bru-", "tokens": ["Die", "Wei\u00df\u00b7heit", "will", "ihr", "Vi\u00b7o\u00b7let", "auf", "mei\u00b7nes", "Bru"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ders Scheitel setzen,", "tokens": ["ders", "Schei\u00b7tel", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und so mu\u00df sich bey seinem Gl\u00fcck ein alter and-", "tokens": ["Und", "so", "mu\u00df", "sich", "bey", "sei\u00b7nem", "Gl\u00fcck", "ein", "al\u00b7ter", "an\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "rer Ich erg\u00f6tzen.", "tokens": ["rer", "Ich", "er\u00b7g\u00f6t\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}