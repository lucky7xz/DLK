{"dta.poem.6408": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geschmack.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst uns nur eine Frucht betrachten und bedencken,", "tokens": ["Lasst", "uns", "nur", "ei\u00b7ne", "Frucht", "be\u00b7trach\u00b7ten", "und", "be\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die, da sie aus dem Zweige steigt,", "tokens": ["Die", ",", "da", "sie", "aus", "dem", "Zwei\u00b7ge", "steigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts, als nur einen Safft, der irdisch ist, uns zeigt,", "tokens": ["Nichts", ",", "als", "nur", "ei\u00b7nen", "Safft", ",", "der", "ir\u00b7disch", "ist", ",", "uns", "zeigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von dem die Theilchen sich verdicken und verschr\u00e4ncken.", "tokens": ["Von", "dem", "die", "Theil\u00b7chen", "sich", "ver\u00b7di\u00b7cken", "und", "ver\u00b7schr\u00e4n\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zuerst hat sie vom Baum die H\u00e4rtigkeit der Ninde,", "tokens": ["Zu\u00b7erst", "hat", "sie", "vom", "Baum", "die", "H\u00e4r\u00b7tig\u00b7keit", "der", "Nin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Worinnen ich nichts feuchtes finde.", "tokens": ["Wo\u00b7rin\u00b7nen", "ich", "nichts", "feuch\u00b7tes", "fin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nach einer kurtzen Zeit vergr\u00f6ssert, f\u00e4rbt sie sich;", "tokens": ["Nach", "ei\u00b7ner", "kurt\u00b7zen", "Zeit", "ver\u00b7gr\u00f6s\u00b7sert", ",", "f\u00e4rbt", "sie", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die kleinen C\u00f6rperlein,", "tokens": ["Die", "klei\u00b7nen", "C\u00f6r\u00b7per\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die allgemach versammlet seyn,", "tokens": ["Die", "all\u00b7ge\u00b7mach", "ver\u00b7samm\u00b7let", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sind z\u00e4rtlicher sodann, sind ordentlich", "tokens": ["Sind", "z\u00e4rt\u00b7li\u00b7cher", "so\u00b7dann", ",", "sind", "or\u00b7dent\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "$,", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Verbunden und gef\u00fcgt. Doch ist es noch nicht Zeit,", "tokens": ["Ver\u00b7bun\u00b7den", "und", "ge\u00b7f\u00fcgt", ".", "Doch", "ist", "es", "noch", "nicht", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$.", "KON", "VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dieselbige zu essen, zu verzehren.", "tokens": ["Die\u00b7sel\u00b7bi\u00b7ge", "zu", "es\u00b7sen", ",", "zu", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Sie wird dir sonst, durch herbe Widrigkeit,", "tokens": ["Sie", "wird", "dir", "sonst", ",", "durch", "her\u00b7be", "Wid\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "So Zung\u2019 als Gaum verletzen und versehren.", "tokens": ["So", "Zung'", "als", "Gaum", "ver\u00b7let\u00b7zen", "und", "ver\u00b7seh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Was sie sodann an S\u00e4fften bey sich f\u00fchrt,", "tokens": ["Was", "sie", "so\u00b7dann", "an", "S\u00e4ff\u00b7ten", "bey", "sich", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ist gar zu scharff, ist gar zu sauer.", "tokens": ["Ist", "gar", "zu", "scharff", ",", "ist", "gar", "zu", "sau\u00b7er", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKA", "ADJD", "$,", "VAFIN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Allein, nach einer kurtzen Dauer,", "tokens": ["Al\u00b7lein", ",", "nach", "ei\u00b7ner", "kurt\u00b7zen", "Dau\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wenn sie gereifft, dann koste man sie bald,", "tokens": ["Wenn", "sie", "ge\u00b7reifft", ",", "dann", "kos\u00b7te", "man", "sie", "bald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PIS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "So ist die Anmuth mannichfalt.", "tokens": ["So", "ist", "die", "An\u00b7muth", "man\u00b7nich\u00b7falt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wo man sie aber nicht sodann", "tokens": ["Wo", "man", "sie", "a\u00b7ber", "nicht", "so\u00b7dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Geniesst; verfaulet sie, verschrumpffet, steckt sich an,", "tokens": ["Ge\u00b7niesst", ";", "ver\u00b7fau\u00b7let", "sie", ",", "ver\u00b7schrumpf\u00b7fet", ",", "steckt", "sich", "an", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und anders ist sie nichts, zu solcher Zeit,", "tokens": ["Und", "an\u00b7ders", "ist", "sie", "nichts", ",", "zu", "sol\u00b7cher", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PIS", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Als Eckel und als Bitterkeit.", "tokens": ["Als", "E\u00b7ckel", "und", "als", "Bit\u00b7ter\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}