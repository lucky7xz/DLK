{"dta.poem.21244": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "XxIV.  \n  An die Clio.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Nu trit/ O Clio/ ab/ hier ist dein End vnd Ziel;", "tokens": ["Nu", "trit", "/", "O", "Clio", "/", "ab", "/", "hier", "ist", "dein", "End", "vnd", "Ziel", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "NE", "NE", "$(", "PTKVZ", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ob vntern Sternen du der ", "tokens": ["Ob", "vn\u00b7tern", "Ster\u00b7nen", "du", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPER", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die du dir vorgesatzt/ bleibst du doch vnten liegen;", "tokens": ["Die", "du", "dir", "vor\u00b7ge\u00b7satzt", "/", "bleibst", "du", "doch", "vn\u00b7ten", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVPP", "$(", "VVFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Ruhm der macht dich stum/ der scham bezwingt dein Spiel.", "tokens": ["Der", "Ruhm", "der", "macht", "dich", "stum", "/", "der", "scham", "be\u00b7zwingt", "dein", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "PPER", "ADV", "$(", "ART", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Vnd wo der ", "tokens": ["Vnd", "wo", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Sein ", "tokens": ["Sein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Kanst du die Andacht nicht/ die in dir brennt/ bezeigen/", "tokens": ["Kanst", "du", "die", "An\u00b7dacht", "nicht", "/", "die", "in", "dir", "brennt", "/", "be\u00b7zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "$(", "ART", "APPR", "PPER", "VVFIN", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denckst du gleich noch so tieff vnd sinckst auff beyde Knie.", "tokens": ["Denckst", "du", "gleich", "noch", "so", "tieff", "vnd", "sinckst", "auff", "bey\u00b7de", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df es ja gestehn/ di\u00df Werck trifft hie nicht ein/", "tokens": ["Ich", "mu\u00df", "es", "ja", "ge\u00b7stehn", "/", "di\u00df", "Werck", "trifft", "hie", "nicht", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "$(", "PDS", "NN", "VVFIN", "ADV", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wo der ", "tokens": ["Doch", "wo", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da\u00df bey mir Marspiter/ der wilde Gast/ gelegen/", "tokens": ["Da\u00df", "bey", "mir", "Mar\u00b7spi\u00b7ter", "/", "der", "wil\u00b7de", "Gast", "/", "ge\u00b7le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVPP", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Als du es auff gesatzt wirst du entschuldigt sein.", "tokens": ["Als", "du", "es", "auff", "ge\u00b7satzt", "wirst", "du", "ent\u00b7schul\u00b7digt", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "VVPP", "VAFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}