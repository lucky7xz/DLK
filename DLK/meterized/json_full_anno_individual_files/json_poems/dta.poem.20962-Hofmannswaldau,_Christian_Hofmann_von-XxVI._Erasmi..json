{"dta.poem.20962": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxVI.  \n Erasmi.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Leser stelle dich zu diesem Grabesteine/", "tokens": ["Mein", "Le\u00b7ser", "stel\u00b7le", "dich", "zu", "die\u00b7sem", "Gra\u00b7be\u00b7stei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erasmus lieget hier/ hier sind die wehrten Beine;", "tokens": ["Er\u00b7as\u00b7mus", "lie\u00b7get", "hier", "/", "hier", "sind", "die", "wehr\u00b7ten", "Bei\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$(", "ADV", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schwere da\u00df kein Staub in diesem Grabe liegt/", "tokens": ["Ich", "schwe\u00b7re", "da\u00df", "kein", "Staub", "in", "die\u00b7sem", "Gra\u00b7be", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PIAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So nicht der Barbarie mit Macht hat obgesiegt.", "tokens": ["So", "nicht", "der", "Bar\u00b7ba\u00b7rie", "mit", "Macht", "hat", "ob\u00b7ge\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}