{"dta.poem.20925": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Zuchtheimine an Tugenand.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Brieflein deiner Magd f\u00e4llt hier zu dei-", "tokens": ["Ein", "Brie\u00b7flein", "dei\u00b7ner", "Magd", "f\u00e4llt", "hier", "zu", "dei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "nen F\u00fcssen/", "tokens": ["nen", "F\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und w\u00fcnschet: Tugenand sey alles Seegens", "tokens": ["Und", "w\u00fcn\u00b7schet", ":", "Tu\u00b7gen\u00b7and", "sey", "al\u00b7les", "See\u00b7gens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "voll/", "tokens": ["voll", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Weil du mir es geschafft/ so hab ich schreiben m\u00fcssen/", "tokens": ["Weil", "du", "mir", "es", "ge\u00b7schafft", "/", "so", "hab", "ich", "schrei\u00b7ben", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVPP", "$(", "ADV", "VAFIN", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sonst wei\u00df ich/ das ich nicht mit F\u00fcrsten reden soll.", "tokens": ["Sonst", "wei\u00df", "ich", "/", "das", "ich", "nicht", "mit", "F\u00fcrs\u00b7ten", "re\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PRELS", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich bin wie dir bewust von gar geringen Stande/", "tokens": ["Ich", "bin", "wie", "dir", "be\u00b7wust", "von", "gar", "ge\u00b7rin\u00b7gen", "Stan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "PPER", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wei\u00df nicht was ein Brief so grosser Hand begehrt/", "tokens": ["Und", "wei\u00df", "nicht", "was", "ein", "Brief", "so", "gros\u00b7ser", "Hand", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PWS", "ART", "NN", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man ehrt als einen Gott dich in den Deutschen Lande/", "tokens": ["Man", "ehrt", "als", "ei\u00b7nen", "Gott", "dich", "in", "den", "Deut\u00b7schen", "Lan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "KOKOM", "ART", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich aber bin gewi\u00df nicht deiner Knechte werth.", "tokens": ["Ich", "a\u00b7ber", "bin", "ge\u00b7wi\u00df", "nicht", "dei\u00b7ner", "Knech\u00b7te", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich schreibe wie gesagt/ doch mit verw\u00f6rrten Sinnen/", "tokens": ["Ich", "schrei\u00b7be", "wie", "ge\u00b7sagt", "/", "doch", "mit", "ver\u00b7w\u00f6rr\u00b7ten", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VVPP", "$(", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich bin nicht Adlers Art/ mich blend\u2019 der Sonnen Licht/", "tokens": ["Ich", "bin", "nicht", "Ad\u00b7lers", "Art", "/", "mich", "blend'", "der", "Son\u00b7nen", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJA", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich wei\u00df nicht wie mir ist/ und wa\u00df ich soll beginnen/", "tokens": ["Ich", "wei\u00df", "nicht", "wie", "mir", "ist", "/", "und", "wa\u00df", "ich", "soll", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VAFIN", "$(", "KON", "VVFIN", "PPER", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Vor Strahlen deiner Gunst kenn\u2019 ich mich selber nicht.", "tokens": ["Vor", "Strah\u00b7len", "dei\u00b7ner", "Gunst", "kenn'", "ich", "mich", "sel\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein grosser Hertzog soll ein arme Magd erkiesen/", "tokens": ["Ein", "gros\u00b7ser", "Hert\u00b7zog", "soll", "ein", "ar\u00b7me", "Magd", "er\u00b7kie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die keinen andern Schmuck/ als Tugend in sich f\u00fchrt/", "tokens": ["Die", "kei\u00b7nen", "an\u00b7dern", "Schmuck", "/", "als", "Tu\u00b7gend", "in", "sich", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$(", "KOUS", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von Stande schlecht ber\u00fchmt/ von Sch\u00f6nheit unge-", "tokens": ["Von", "Stan\u00b7de", "schlecht", "be\u00b7r\u00fchmt", "/", "von", "Sch\u00f6n\u00b7heit", "un\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "ADJD", "$(", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "priesen/", "tokens": ["prie\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Von Wei\u00dfheit unbekandt/ von Reichthum ungeziehrt.", "tokens": ["Von", "Wei\u00df\u00b7heit", "un\u00b7be\u00b7kandt", "/", "von", "Reicht\u00b7hum", "un\u00b7ge\u00b7ziehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$(", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Scham/ Furcht und auch verdacht l\u00e4st mich nicht", "tokens": ["Scham", "/", "Furcht", "und", "auch", "ver\u00b7dacht", "l\u00e4st", "mich", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "NN", "KON", "ADV", "VVPP", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.21": {"text": "Worte finden/", "tokens": ["Wor\u00b7te", "fin\u00b7den", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Da\u00df ich wie sichs geb\u00fchrt recht Antwort schreiben kan;", "tokens": ["Da\u00df", "ich", "wie", "sichs", "ge\u00b7b\u00fchrt", "recht", "Ant\u00b7wort", "schrei\u00b7ben", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PWAV", "PIS", "VVFIN", "ADJD", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wem Angst und Bl\u00f6digkeit die schwachen Finger bin-", "tokens": ["Wem", "Angst", "und", "Bl\u00f6\u00b7dig\u00b7keit", "die", "schwa\u00b7chen", "Fin\u00b7ger", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "den/", "tokens": ["den", "/"], "token_info": ["word", "punct"], "pos": ["ART", "$("], "meter": "-", "measure": "single.down"}, "line.25": {"text": "Der greift die Feder nur mit grossen Zittern an.", "tokens": ["Der", "greift", "die", "Fe\u00b7der", "nur", "mit", "gros\u00b7sen", "Zit\u00b7tern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es tr\u00e4get mir itzund dein wohlgeziehrtes Schreiben/", "tokens": ["Es", "tr\u00e4\u00b7get", "mir", "it\u00b7zund", "dein", "wohl\u00b7ge\u00b7ziehr\u00b7tes", "Schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Bluhmen hoher Gunst in Ruhmes Schalen f\u00fcr/", "tokens": ["Die", "Bluh\u00b7men", "ho\u00b7her", "Gunst", "in", "Ruh\u00b7mes", "Scha\u00b7len", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "La\u00df dich zu deiner Magd doch nicht die Liebe treiben/", "tokens": ["La\u00df", "dich", "zu", "dei\u00b7ner", "Magd", "doch", "nicht", "die", "Lie\u00b7be", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Denn was du hast ger\u00fchmt/ das findst du nicht allhier.", "tokens": ["Denn", "was", "du", "hast", "ge\u00b7r\u00fchmt", "/", "das", "findst", "du", "nicht", "all\u00b7hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "VVPP", "$(", "PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es soll der Purpur sich mit Purpur nur verm\u00e4hlen/", "tokens": ["Es", "soll", "der", "Pur\u00b7pur", "sich", "mit", "Pur\u00b7pur", "nur", "ver\u00b7m\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PRF", "APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Den besten Ring besch\u00e4mt ein falscher Diamant/", "tokens": ["Den", "bes\u00b7ten", "Ring", "be\u00b7sch\u00e4mt", "ein", "fal\u00b7scher", "Di\u00b7a\u00b7mant", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ich wei\u00df dein Bitten ist ein h\u00f6ffliches Befehlen/", "tokens": ["Ich", "wei\u00df", "dein", "Bit\u00b7ten", "ist", "ein", "h\u00f6ff\u00b7li\u00b7ches", "Be\u00b7feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch glaub/ ich bin zuschlecht vor eines F\u00fcrsten Handt.", "tokens": ["Doch", "glaub", "/", "ich", "bin", "zu\u00b7schlecht", "vor", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "Handt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das Hau\u00df von Sonnenreich so schwer von Cronen", "tokens": ["Das", "Hau\u00df", "von", "Son\u00b7nen\u00b7reich", "so", "schwer", "von", "Cro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "worden/", "tokens": ["wor\u00b7den", "/"], "token_info": ["word", "punct"], "pos": ["VAPP", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.36": {"text": "Und dem der Purpur Rock fast angebohren ist/", "tokens": ["Und", "dem", "der", "Pur\u00b7pur", "Rock", "fast", "an\u00b7ge\u00b7boh\u00b7ren", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "NN", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Vergist sich endlich selbst/ und seinen hohen Orden/", "tokens": ["Ver\u00b7gist", "sich", "end\u00b7lich", "selbst", "/", "und", "sei\u00b7nen", "ho\u00b7hen", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "In dem mich Tugenand vor andern ausserkiest.", "tokens": ["In", "dem", "mich", "Tu\u00b7gen\u00b7and", "vor", "an\u00b7dern", "aus\u00b7ser\u00b7kiest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Ich wei\u00df nicht was ich wohl soll f\u00fcr Gedancken f\u00fchren/", "tokens": ["Ich", "wei\u00df", "nicht", "was", "ich", "wohl", "soll", "f\u00fcr", "Ge\u00b7dan\u00b7cken", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "ADV", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und ob dein Feuer nicht zu meinem Schimpffe brennt;", "tokens": ["Und", "ob", "dein", "Feu\u00b7er", "nicht", "zu", "mei\u00b7nem", "Schimpf\u00b7fe", "brennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Oft pflegt der Ehrenkrantz die Bluhmen zuverliehren/", "tokens": ["Oft", "pflegt", "der", "Eh\u00b7ren\u00b7krantz", "die", "Bluh\u00b7men", "zu\u00b7ver\u00b7lieh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wenn eine schlehte Magd die grossen Herren kennt.", "tokens": ["Wenn", "ei\u00b7ne", "schleh\u00b7te", "Magd", "die", "gros\u00b7sen", "Her\u00b7ren", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ein Tritt in unser Hau\u00df von solchen hohen F\u00fcssen/", "tokens": ["Ein", "Tritt", "in", "un\u00b7ser", "Hau\u00df", "von", "sol\u00b7chen", "ho\u00b7hen", "F\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ist ein Genaden Werck/ begleitet mit Verdacht/", "tokens": ["Ist", "ein", "Ge\u00b7na\u00b7den", "Werck", "/", "be\u00b7glei\u00b7tet", "mit", "Ver\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$(", "VVPP", "APPR", "NN", "$("], "meter": "-+-+