{"textgrid.poem.60226": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Tod, wie bitter bist du dem,", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Tod, wie bitter bist du dem,", "tokens": ["O", "Tod", ",", "wie", "bit\u00b7ter", "bist", "du", "dem", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "ART", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der reich war, der sich hier ein ewigs Leben dachte,", "tokens": ["Der", "reich", "war", ",", "der", "sich", "hier", "ein", "e\u00b7wigs", "Le\u00b7ben", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PRELS", "PRF", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und alle Stunden angenehm", "tokens": ["Und", "al\u00b7le", "Stun\u00b7den", "an\u00b7ge\u00b7nehm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch neuerfundne Freuden machte!", "tokens": ["Durch", "neu\u00b7er\u00b7fund\u00b7ne", "Freu\u00b7den", "mach\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du kommst: sein Flittergold und seine Federb\u00fcsche,", "tokens": ["Du", "kommst", ":", "sein", "Flit\u00b7ter\u00b7gold", "und", "sei\u00b7ne", "Fe\u00b7der\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die ihm das Gl\u00fcck, die ihm der Ruhm verschwendrisch gab,", "tokens": ["Die", "ihm", "das", "Gl\u00fcck", ",", "die", "ihm", "der", "Ruhm", "ver\u00b7schwend\u00b7risch", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das alles reissest du mit starkem Arm ihm ab.", "tokens": ["Das", "al\u00b7les", "reis\u00b7sest", "du", "mit", "star\u00b7kem", "Arm", "ihm", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und l\u00e4g am Sterbebett auf einem Marmortische", "tokens": ["Und", "l\u00e4g", "am", "Ster\u00b7be\u00b7bett", "auf", "ei\u00b7nem", "Mar\u00b7mor\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Der Zepter \u00fcber eine Welt,", "tokens": ["Der", "Zep\u00b7ter", "\u00fc\u00b7ber", "ei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und w\u00e4re bey die Arzeneyen,", "tokens": ["Und", "w\u00e4\u00b7re", "bey", "die", "Ar\u00b7ze\u00b7ne\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Das Diadem von theurem Stein gestellt:", "tokens": ["Das", "Di\u00b7a\u00b7dem", "von", "theu\u00b7rem", "Stein", "ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch w\u00fcrde nichts den Menschen mehr erfreuen,", "tokens": ["Doch", "w\u00fcr\u00b7de", "nichts", "den", "Men\u00b7schen", "mehr", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der itzt in deine H\u00e4nde f\u00e4llt.", "tokens": ["Der", "itzt", "in", "dei\u00b7ne", "H\u00e4n\u00b7de", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Doch, Menschenfeind, der alles so verg\u00e4llt,", "tokens": ["Doch", ",", "Men\u00b7schen\u00b7feind", ",", "der", "al\u00b7les", "so", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PRELS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Dich zu besch\u00e4men, will ich die Geschichte sagen,", "tokens": ["Dich", "zu", "be\u00b7sch\u00e4\u00b7men", ",", "will", "ich", "die", "Ge\u00b7schich\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Die nicht vor hundert Jahren sich,", "tokens": ["Die", "nicht", "vor", "hun\u00b7dert", "Jah\u00b7ren", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "CARD", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Nein, erst vor kurzer Zeit, wahrhaftig zugetragen.", "tokens": ["Nein", ",", "erst", "vor", "kur\u00b7zer", "Zeit", ",", "wahr\u00b7haf\u00b7tig", "zu\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "ADJA", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Rosaria war sch\u00f6n, reich, vornehm, jugendlich", "tokens": ["Ro\u00b7sa\u00b7ria", "war", "sch\u00f6n", ",", "reich", ",", "vor\u00b7nehm", ",", "ju\u00b7gend\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und erst verm\u00e4hlt seit hundert Tagen", "tokens": ["Und", "erst", "ver\u00b7m\u00e4hlt", "seit", "hun\u00b7dert", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An einen Mann, der sie geliebt,", "tokens": ["An", "ei\u00b7nen", "Mann", ",", "der", "sie", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr, als sein Herz, das ihrem Staube,", "tokens": ["Mehr", ",", "als", "sein", "Herz", ",", "das", "ih\u00b7rem", "Stau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun traurige Besuche giebt!", "tokens": ["Nun", "trau\u00b7ri\u00b7ge", "Be\u00b7su\u00b7che", "giebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Krank ward sie und ihr Reiz ward pl\u00f6tzlich der Gewalt", "tokens": ["Krank", "ward", "sie", "und", "ihr", "Reiz", "ward", "pl\u00f6tz\u00b7lich", "der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Fiebers und der Nichtigkeit zum Raube.", "tokens": ["Des", "Fie\u00b7bers", "und", "der", "Nich\u00b7tig\u00b7keit", "zum", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie lag. Nicht eine Spur der bl\u00fchenden Gestalt", "tokens": ["Sie", "lag", ".", "Nicht", "ei\u00b7ne", "Spur", "der", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7stalt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Blieb \u00fcbrig auf den blassen Wangen:", "tokens": ["Blieb", "\u00fcb\u00b7rig", "auf", "den", "blas\u00b7sen", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So h\u00e4ngt an B\u00e4umen, die im jungen Fr\u00fchling prangen,", "tokens": ["So", "h\u00e4ngt", "an", "B\u00e4u\u00b7men", ",", "die", "im", "jun\u00b7gen", "Fr\u00fch\u00b7ling", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein von dem Frost get\u00f6dtet Blat,", "tokens": ["Ein", "von", "dem", "Frost", "ge\u00b7t\u00f6d\u00b7tet", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Das ausser seiner Form nichts mehr behalten hat!", "tokens": ["Das", "aus\u00b7ser", "sei\u00b7ner", "Form", "nichts", "mehr", "be\u00b7hal\u00b7ten", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "PIS", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Rosaria mit mattem Blicke", "tokens": ["Ro\u00b7sa\u00b7ria", "mit", "mat\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah in das ofne Grab, sah in die Welt zur\u00fccke:", "tokens": ["Sah", "in", "das", "of\u00b7ne", "Grab", ",", "sah", "in", "die", "Welt", "zu\u00b7r\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O alle Freuden waren ihr entflohn!", "tokens": ["O", "al\u00b7le", "Freu\u00b7den", "wa\u00b7ren", "ihr", "ent\u00b7flohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nichts mehr f\u00fcr sie! Ihr Auge wandte", "tokens": ["Nichts", "mehr", "f\u00fcr", "sie", "!", "Ihr", "Au\u00b7ge", "wand\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "PPER", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich heilig zur Religion,", "tokens": ["Sich", "hei\u00b7lig", "zur", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die himmlisch l\u00e4chelnd sich zu ihrer Seele wandte", "tokens": ["Die", "himm\u00b7lisch", "l\u00e4\u00b7chelnd", "sich", "zu", "ih\u00b7rer", "See\u00b7le", "wand\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit G\u00fctern, die vorher ihr Herz nie recht erkannte;", "tokens": ["Mit", "G\u00fc\u00b7tern", ",", "die", "vor\u00b7her", "ihr", "Herz", "nie", "recht", "er\u00b7kann\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und k\u00e4mpfen mu\u00dfte sie noch schwer.", "tokens": ["Und", "k\u00e4mp\u00b7fen", "mu\u00df\u00b7te", "sie", "noch", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie suchte lange Trost und lange blieb sie leer,", "tokens": ["Sie", "such\u00b7te", "lan\u00b7ge", "Trost", "und", "lan\u00b7ge", "blieb", "sie", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Fiel in Entz\u00fcckung, lag tief schlummernd viele Stunden,", "tokens": ["Fiel", "in", "Ent\u00b7z\u00fc\u00b7ckung", ",", "lag", "tief", "schlum\u00b7mernd", "vie\u00b7le", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "ADJD", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schlug sanft die Augen auf, ward Tag im Angesicht", "tokens": ["Schlug", "sanft", "die", "Au\u00b7gen", "auf", ",", "ward", "Tag", "im", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,", "VAFIN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und rief mit Freudigkeit: ich habe nun gefunden!", "tokens": ["Und", "rief", "mit", "Freu\u00b7dig\u00b7keit", ":", "ich", "ha\u00b7be", "nun", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was fand sie denn, o Tod? \u2013 Die Ruhe, welche nicht", "tokens": ["Was", "fand", "sie", "denn", ",", "o", "Tod", "?", "\u2013", "Die", "Ru\u00b7he", ",", "wel\u00b7che", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$.", "$(", "ART", "NN", "$,", "PRELS", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der ganzen Welt Versprechung kann gew\u00e4hren", "tokens": ["Der", "gan\u00b7zen", "Welt", "Ver\u00b7spre\u00b7chung", "kann", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Dem Sterbenden, der alles eitel nennt,", "tokens": ["Dem", "Ster\u00b7ben\u00b7den", ",", "der", "al\u00b7les", "ei\u00b7tel", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Nicht mehr der Erde G\u00f6tzen kennt", "tokens": ["Nicht", "mehr", "der", "Er\u00b7de", "G\u00f6t\u00b7zen", "kennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und Ruhe sucht und Ruhe liegt begehren.", "tokens": ["Und", "Ru\u00b7he", "sucht", "und", "Ru\u00b7he", "liegt", "be\u00b7geh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "KON", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Dann fr\u00e4gt der st\u00e4rkste Geist, der k\u00fchnste Atheist:", "tokens": ["Dann", "fr\u00e4gt", "der", "st\u00e4rks\u00b7te", "Geist", ",", "der", "k\u00fchns\u00b7te", "A\u00b7theist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "Ob Reichthum, Lust und Ehre folgen werden?", "tokens": ["Ob", "Reicht\u00b7hum", ",", "Lust", "und", "Eh\u00b7re", "fol\u00b7gen", "wer\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und alle sagen: nein, und alles bleibt auf Erden,", "tokens": ["Und", "al\u00b7le", "sa\u00b7gen", ":", "nein", ",", "und", "al\u00b7les", "bleibt", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "$.", "PTKANT", "$,", "KON", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und allen spottet fromm der Christ.", "tokens": ["Und", "al\u00b7len", "spot\u00b7tet", "fromm", "der", "Christ", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Dir aber, der du ihm nicht rauh, nicht schrecklich bist,", "tokens": ["Dir", "a\u00b7ber", ",", "der", "du", "ihm", "nicht", "rauh", ",", "nicht", "schreck\u00b7lich", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "PPER", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Reicht er die Hand, wie auf dem Meere", "tokens": ["Reicht", "er", "die", "Hand", ",", "wie", "auf", "dem", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Ein Schwimmender sie reicht dem, der sein Retter w\u00e4re.", "tokens": ["Ein", "Schwim\u00b7men\u00b7der", "sie", "reicht", "dem", ",", "der", "sein", "Ret\u00b7ter", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ART", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O Tod, wie bitter bist du dem,", "tokens": ["O", "Tod", ",", "wie", "bit\u00b7ter", "bist", "du", "dem", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "ART", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der reich war, der sich hier ein ewigs Leben dachte,", "tokens": ["Der", "reich", "war", ",", "der", "sich", "hier", "ein", "e\u00b7wigs", "Le\u00b7ben", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "PRELS", "PRF", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und alle Stunden angenehm", "tokens": ["Und", "al\u00b7le", "Stun\u00b7den", "an\u00b7ge\u00b7nehm"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch neuerfundne Freuden machte!", "tokens": ["Durch", "neu\u00b7er\u00b7fund\u00b7ne", "Freu\u00b7den", "mach\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du kommst: sein Flittergold und seine Federb\u00fcsche,", "tokens": ["Du", "kommst", ":", "sein", "Flit\u00b7ter\u00b7gold", "und", "sei\u00b7ne", "Fe\u00b7der\u00b7b\u00fc\u00b7sche", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die ihm das Gl\u00fcck, die ihm der Ruhm verschwendrisch gab,", "tokens": ["Die", "ihm", "das", "Gl\u00fcck", ",", "die", "ihm", "der", "Ruhm", "ver\u00b7schwend\u00b7risch", "gab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das alles reissest du mit starkem Arm ihm ab.", "tokens": ["Das", "al\u00b7les", "reis\u00b7sest", "du", "mit", "star\u00b7kem", "Arm", "ihm", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und l\u00e4g am Sterbebett auf einem Marmortische", "tokens": ["Und", "l\u00e4g", "am", "Ster\u00b7be\u00b7bett", "auf", "ei\u00b7nem", "Mar\u00b7mor\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Der Zepter \u00fcber eine Welt,", "tokens": ["Der", "Zep\u00b7ter", "\u00fc\u00b7ber", "ei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und w\u00e4re bey die Arzeneyen,", "tokens": ["Und", "w\u00e4\u00b7re", "bey", "die", "Ar\u00b7ze\u00b7ne\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Das Diadem von theurem Stein gestellt:", "tokens": ["Das", "Di\u00b7a\u00b7dem", "von", "theu\u00b7rem", "Stein", "ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch w\u00fcrde nichts den Menschen mehr erfreuen,", "tokens": ["Doch", "w\u00fcr\u00b7de", "nichts", "den", "Men\u00b7schen", "mehr", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der itzt in deine H\u00e4nde f\u00e4llt.", "tokens": ["Der", "itzt", "in", "dei\u00b7ne", "H\u00e4n\u00b7de", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Doch, Menschenfeind, der alles so verg\u00e4llt,", "tokens": ["Doch", ",", "Men\u00b7schen\u00b7feind", ",", "der", "al\u00b7les", "so", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PRELS", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Dich zu besch\u00e4men, will ich die Geschichte sagen,", "tokens": ["Dich", "zu", "be\u00b7sch\u00e4\u00b7men", ",", "will", "ich", "die", "Ge\u00b7schich\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.16": {"text": "Die nicht vor hundert Jahren sich,", "tokens": ["Die", "nicht", "vor", "hun\u00b7dert", "Jah\u00b7ren", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "CARD", "NN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Nein, erst vor kurzer Zeit, wahrhaftig zugetragen.", "tokens": ["Nein", ",", "erst", "vor", "kur\u00b7zer", "Zeit", ",", "wahr\u00b7haf\u00b7tig", "zu\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "ADJA", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Rosaria war sch\u00f6n, reich, vornehm, jugendlich", "tokens": ["Ro\u00b7sa\u00b7ria", "war", "sch\u00f6n", ",", "reich", ",", "vor\u00b7nehm", ",", "ju\u00b7gend\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und erst verm\u00e4hlt seit hundert Tagen", "tokens": ["Und", "erst", "ver\u00b7m\u00e4hlt", "seit", "hun\u00b7dert", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVPP", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "An einen Mann, der sie geliebt,", "tokens": ["An", "ei\u00b7nen", "Mann", ",", "der", "sie", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mehr, als sein Herz, das ihrem Staube,", "tokens": ["Mehr", ",", "als", "sein", "Herz", ",", "das", "ih\u00b7rem", "Stau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nun traurige Besuche giebt!", "tokens": ["Nun", "trau\u00b7ri\u00b7ge", "Be\u00b7su\u00b7che", "giebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Krank ward sie und ihr Reiz ward pl\u00f6tzlich der Gewalt", "tokens": ["Krank", "ward", "sie", "und", "ihr", "Reiz", "ward", "pl\u00f6tz\u00b7lich", "der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Des Fiebers und der Nichtigkeit zum Raube.", "tokens": ["Des", "Fie\u00b7bers", "und", "der", "Nich\u00b7tig\u00b7keit", "zum", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sie lag. Nicht eine Spur der bl\u00fchenden Gestalt", "tokens": ["Sie", "lag", ".", "Nicht", "ei\u00b7ne", "Spur", "der", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7stalt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Blieb \u00fcbrig auf den blassen Wangen:", "tokens": ["Blieb", "\u00fcb\u00b7rig", "auf", "den", "blas\u00b7sen", "Wan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So h\u00e4ngt an B\u00e4umen, die im jungen Fr\u00fchling prangen,", "tokens": ["So", "h\u00e4ngt", "an", "B\u00e4u\u00b7men", ",", "die", "im", "jun\u00b7gen", "Fr\u00fch\u00b7ling", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein von dem Frost get\u00f6dtet Blat,", "tokens": ["Ein", "von", "dem", "Frost", "ge\u00b7t\u00f6d\u00b7tet", "Blat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Das ausser seiner Form nichts mehr behalten hat!", "tokens": ["Das", "aus\u00b7ser", "sei\u00b7ner", "Form", "nichts", "mehr", "be\u00b7hal\u00b7ten", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "NN", "PIS", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Rosaria mit mattem Blicke", "tokens": ["Ro\u00b7sa\u00b7ria", "mit", "mat\u00b7tem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah in das ofne Grab, sah in die Welt zur\u00fccke:", "tokens": ["Sah", "in", "das", "of\u00b7ne", "Grab", ",", "sah", "in", "die", "Welt", "zu\u00b7r\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "O alle Freuden waren ihr entflohn!", "tokens": ["O", "al\u00b7le", "Freu\u00b7den", "wa\u00b7ren", "ihr", "ent\u00b7flohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nichts mehr f\u00fcr sie! Ihr Auge wandte", "tokens": ["Nichts", "mehr", "f\u00fcr", "sie", "!", "Ihr", "Au\u00b7ge", "wand\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ADV", "APPR", "PPER", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sich heilig zur Religion,", "tokens": ["Sich", "hei\u00b7lig", "zur", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die himmlisch l\u00e4chelnd sich zu ihrer Seele wandte", "tokens": ["Die", "himm\u00b7lisch", "l\u00e4\u00b7chelnd", "sich", "zu", "ih\u00b7rer", "See\u00b7le", "wand\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit G\u00fctern, die vorher ihr Herz nie recht erkannte;", "tokens": ["Mit", "G\u00fc\u00b7tern", ",", "die", "vor\u00b7her", "ihr", "Herz", "nie", "recht", "er\u00b7kann\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und k\u00e4mpfen mu\u00dfte sie noch schwer.", "tokens": ["Und", "k\u00e4mp\u00b7fen", "mu\u00df\u00b7te", "sie", "noch", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie suchte lange Trost und lange blieb sie leer,", "tokens": ["Sie", "such\u00b7te", "lan\u00b7ge", "Trost", "und", "lan\u00b7ge", "blieb", "sie", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Fiel in Entz\u00fcckung, lag tief schlummernd viele Stunden,", "tokens": ["Fiel", "in", "Ent\u00b7z\u00fc\u00b7ckung", ",", "lag", "tief", "schlum\u00b7mernd", "vie\u00b7le", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "VVFIN", "ADJD", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schlug sanft die Augen auf, ward Tag im Angesicht", "tokens": ["Schlug", "sanft", "die", "Au\u00b7gen", "auf", ",", "ward", "Tag", "im", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,", "VAFIN", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und rief mit Freudigkeit: ich habe nun gefunden!", "tokens": ["Und", "rief", "mit", "Freu\u00b7dig\u00b7keit", ":", "ich", "ha\u00b7be", "nun", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$.", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was fand sie denn, o Tod? \u2013 Die Ruhe, welche nicht", "tokens": ["Was", "fand", "sie", "denn", ",", "o", "Tod", "?", "\u2013", "Die", "Ru\u00b7he", ",", "wel\u00b7che", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$.", "$(", "ART", "NN", "$,", "PRELS", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der ganzen Welt Versprechung kann gew\u00e4hren", "tokens": ["Der", "gan\u00b7zen", "Welt", "Ver\u00b7spre\u00b7chung", "kann", "ge\u00b7w\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Dem Sterbenden, der alles eitel nennt,", "tokens": ["Dem", "Ster\u00b7ben\u00b7den", ",", "der", "al\u00b7les", "ei\u00b7tel", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Nicht mehr der Erde G\u00f6tzen kennt", "tokens": ["Nicht", "mehr", "der", "Er\u00b7de", "G\u00f6t\u00b7zen", "kennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": [