ON", "PIS", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Dir aber, der du ihm nicht rauh, nicht schrecklich bist,", "tokens": ["Dir", "a\u00b7ber", ",", "der", "du", "ihm", "nicht", "rauh", ",", "nicht", "schreck\u00b7lich", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "PPER", "PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Reicht er die Hand, wie auf dem Meere", "tokens": ["Reicht", "er", "die", "Hand", ",", "wie", "auf", "dem", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Ein Schwimmender sie reicht dem, der sein Retter w\u00e4re.", "tokens": ["Ein", "Schwim\u00b7men\u00b7der", "sie", "reicht", "dem", ",", "der", "sein", "Ret\u00b7ter", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "ART", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}