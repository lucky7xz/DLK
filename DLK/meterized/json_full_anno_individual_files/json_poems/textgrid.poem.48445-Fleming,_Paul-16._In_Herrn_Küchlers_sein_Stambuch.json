{"textgrid.poem.48445": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "16. In Herrn K\u00fcchlers sein Stambuch", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja freilich ist es so: es will nicht nur gelacht", "tokens": ["Ja", "frei\u00b7lich", "ist", "es", "so", ":", "es", "will", "nicht", "nur", "ge\u00b7lacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VAFIN", "PPER", "ADV", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zur Freundschaftstiftung sein; kein Mund-zu-Munde-kehren,", "tokens": ["zur", "Freund\u00b7schafts\u00b7tif\u00b7tung", "sein", ";", "kein", "Mun\u00b7dzu\u00b7Mun\u00b7de\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAINF", "$.", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "kein' H\u00e4ndek\u00fcplerei, kein Dienen vorn' und hinden", "tokens": ["kein'", "H\u00e4n\u00b7de\u00b7k\u00fcp\u00b7le\u00b7rei", ",", "kein", "Die\u00b7nen", "vorn'", "und", "hin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "PTKVZ", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "befreundet uns mit uns. Wenn nicht das Herze wacht", "tokens": ["be\u00b7freun\u00b7det", "uns", "mit", "uns", ".", "Wenn", "nicht", "das", "Her\u00b7ze", "wacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$.", "KOUS", "PTKNEG", "PDS", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und saget Ja darzu, so hilft kein schl\u00e4frigs Ehren.", "tokens": ["und", "sa\u00b7get", "Ja", "dar\u00b7zu", ",", "so", "hilft", "kein", "schl\u00e4f\u00b7rigs", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKANT", "PAV", "$,", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Herz' ist doch allein' im Herzen nur zu finden.", "tokens": ["Ein", "Her\u00b7z'", "ist", "doch", "al\u00b7lein'", "im", "Her\u00b7zen", "nur", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Ja freilich ist es so: es will nicht nur gelacht", "tokens": ["Ja", "frei\u00b7lich", "ist", "es", "so", ":", "es", "will", "nicht", "nur", "ge\u00b7lacht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VAFIN", "PPER", "ADV", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zur Freundschaftstiftung sein; kein Mund-zu-Munde-kehren,", "tokens": ["zur", "Freund\u00b7schafts\u00b7tif\u00b7tung", "sein", ";", "kein", "Mun\u00b7dzu\u00b7Mun\u00b7de\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAINF", "$.", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "kein' H\u00e4ndek\u00fcplerei, kein Dienen vorn' und hinden", "tokens": ["kein'", "H\u00e4n\u00b7de\u00b7k\u00fcp\u00b7le\u00b7rei", ",", "k