{"dta.poem.3728": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Abz\u00e4hlen bei dem Spiel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eins, zwei, drei,               ", "tokens": ["Eins", ",", "zwei", ",", "drei", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "CARD", "$,", "CARD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "In der Dechanei,", "tokens": ["In", "der", "De\u00b7cha\u00b7nei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Steht ein Teller auf dem Tisch,", "tokens": ["Steht", "ein", "Tel\u00b7ler", "auf", "dem", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6mmt die Katz und holt die Fisch,", "tokens": ["K\u00f6mmt", "die", "Katz", "und", "holt", "die", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6mmt der J\u00e4ger mit der Gabel,", "tokens": ["K\u00f6mmt", "der", "J\u00e4\u00b7ger", "mit", "der", "Ga\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sticht die Katze in den Nabel,", "tokens": ["Sticht", "die", "Kat\u00b7ze", "in", "den", "Na\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schreit die Katz: Miaun miaun,", "tokens": ["Schreit", "die", "Katz", ":", "Mi\u00b7aun", "mi\u00b7aun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "FM", "FM", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wills gewi\u00df nicht wieder taun.", "tokens": ["Wills", "ge\u00b7wi\u00df", "nicht", "wie\u00b7der", "taun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}