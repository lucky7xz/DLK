{"dta.poem.3652": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wiegenlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Jesu liebes Herrlein mein,               ", "tokens": ["O", "Je\u00b7su", "lie\u00b7bes", "Herr\u00b7lein", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hilf mir wiegen mein Kindelein,", "tokens": ["Hilf", "mir", "wie\u00b7gen", "mein", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Im Himmelreich, und in der lieben Christenheit,", "tokens": ["Im", "Him\u00b7mel\u00b7reich", ",", "und", "in", "der", "lie\u00b7ben", "Chris\u00b7ten\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Eya! Eya! Schlaf du liebes Kindelein,", "tokens": ["Ey\u00b7a", "!", "Ey\u00b7a", "!", "Schlaf", "du", "lie\u00b7bes", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "Der heilig Christ will bei dir seyn,", "tokens": ["Der", "hei\u00b7lig", "Christ", "will", "bei", "dir", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VMFIN", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seinen lieben Engelein, in Ewigkeit.", "tokens": ["Mit", "sei\u00b7nen", "lie\u00b7ben", "En\u00b7ge\u00b7lein", ",", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O du liebes Jesulein,", "tokens": ["O", "du", "lie\u00b7bes", "Je\u00b7su\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Du Tr\u00f6ster mein, erfreu dich fein,", "tokens": ["Du", "Tr\u00f6s\u00b7ter", "mein", ",", "er\u00b7freu", "dich", "fein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "$,", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und mach uns arme W\u00fcrmelein", "tokens": ["Und", "mach", "uns", "ar\u00b7me", "W\u00fcr\u00b7mel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Zu Dienern dein!", "tokens": ["Zu", "Die\u00b7nern", "dein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "O Jesu, Gottes S\u00f6hnelein,", "tokens": ["O", "Je\u00b7su", ",", "Got\u00b7tes", "S\u00f6h\u00b7nel\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Marien Kindelein,", "tokens": ["Und", "Ma\u00b7ri\u00b7en", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "La\u00df dir mein Kind befohlen seyn,", "tokens": ["La\u00df", "dir", "mein", "Kind", "be\u00b7foh\u00b7len", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Himmelreich, und in seim kleinen Wiegelein,", "tokens": ["Im", "Him\u00b7mel\u00b7reich", ",", "und", "in", "seim", "klei\u00b7nen", "Wie\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eya! Eya! Schlaf mein liebes Kindelein,", "tokens": ["Ey\u00b7a", "!", "Ey\u00b7a", "!", "Schlaf", "mein", "lie\u00b7bes", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Dein Christ bringt dir gut Aepfelein,", "tokens": ["Dein", "Christ", "bringt", "dir", "gut", "A\u00b7e\u00b7pfel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Baut dir ein sch\u00f6nes H\u00e4uselein im Himmelreich.", "tokens": ["Baut", "dir", "ein", "sch\u00f6\u00b7nes", "H\u00e4u\u00b7sel\u00b7ein", "im", "Him\u00b7mel\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du trautes Jesulein,", "tokens": ["Du", "trau\u00b7tes", "Je\u00b7su\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Gottes L\u00e4mmelein, erbarm dich mein,", "tokens": ["Got\u00b7tes", "L\u00e4m\u00b7mel\u00b7ein", ",", "er\u00b7barm", "dich", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "PPER", "PPOSAT", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Und fa\u00df mich auf dein R\u00fcckelein", "tokens": ["Und", "fa\u00df", "mich", "auf", "dein", "R\u00fc\u00b7ckel\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und trag mich fein.", "tokens": ["Und", "trag", "mich", "fein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "O Jesu, liebes Br\u00fcderlein,", "tokens": ["O", "Je\u00b7su", ",", "lie\u00b7bes", "Br\u00fc\u00b7derl\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wollst Emanuelchen seyn,", "tokens": ["Du", "wollst", "E\u00b7ma\u00b7nu\u00b7el\u00b7chen", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unser ewigs Priesterlein,", "tokens": ["Und", "un\u00b7ser", "e\u00b7wigs", "Pries\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Himmelreich, und in der lieben Christenheit.", "tokens": ["Im", "Him\u00b7mel\u00b7reich", ",", "und", "in", "der", "lie\u00b7ben", "Chris\u00b7ten\u00b7heit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eya! Eya! schweig du trautes Kindelein,", "tokens": ["Ey\u00b7a", "!", "Ey\u00b7a", "!", "schweig", "du", "trau\u00b7tes", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Es bei\u00dft dich sonst ein Eselein,", "tokens": ["Es", "bei\u00dft", "dich", "sonst", "ein", "E\u00b7sel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und st\u00f6\u00dft dich Josephs Oechselein, zu Bethlehem:", "tokens": ["Und", "st\u00f6\u00dft", "dich", "Jo\u00b7se\u00b7phs", "O\u00b7ech\u00b7sel\u00b7ein", ",", "zu", "Beth\u00b7le\u00b7hem", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "NE", "NE", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "O du s\u00fc\u00dfes Jesulein,", "tokens": ["O", "du", "s\u00fc\u00b7\u00dfes", "Je\u00b7su\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Erhalt uns rein, im Glauben dein,", "tokens": ["Er\u00b7halt", "uns", "rein", ",", "im", "Glau\u00b7ben", "dein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "APPRART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bitt f\u00fcr uns arme S\u00fcnderlein,", "tokens": ["Bitt", "f\u00fcr", "uns", "ar\u00b7me", "S\u00fcn\u00b7der\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Den Vater dein.", "tokens": ["Den", "Va\u00b7ter", "dein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Jesus das zarte Kindelein,", "tokens": ["Je\u00b7sus", "das", "zar\u00b7te", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag in ein'm harten Krippelein,", "tokens": ["Lag", "in", "ein'm", "har\u00b7ten", "Krip\u00b7pe\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewindelt in ein T\u00fcchelein,", "tokens": ["Ge\u00b7win\u00b7delt", "in", "ein", "T\u00fc\u00b7chel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Bethlehem, im finstern Stall, beim Oechselein.", "tokens": ["Zu", "Beth\u00b7le\u00b7hem", ",", "im", "fins\u00b7tern", "Stall", ",", "beim", "O\u00b7ech\u00b7sel\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPRART", "ADJA", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Eya! Eya! Joseph kocht ein M\u00fcselein,", "tokens": ["Ey\u00b7a", "!", "Ey\u00b7a", "!", "Jo\u00b7se\u00b7ph", "kocht", "ein", "M\u00fc\u00b7se\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NE", "$.", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Maria streichts ihrm S\u00f6hnlein ein,", "tokens": ["Ma\u00b7ria", "streichts", "ihrm", "S\u00f6hn\u00b7lein", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das K\u00fc\u00dflein w\u00e4rmt ein Engelein;", "tokens": ["Das", "K\u00fc\u00df\u00b7lein", "w\u00e4rmt", "ein", "En\u00b7ge\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nun singet fein, o du liebes Jesulein,", "tokens": ["Nun", "sin\u00b7get", "fein", ",", "o", "du", "lie\u00b7bes", "Je\u00b7su\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "FM", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Die Unschuld dein, la\u00df unser seyn,", "tokens": ["Die", "Un\u00b7schuld", "dein", ",", "la\u00df", "un\u00b7ser", "seyn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "$,", "VVIMP", "PPOSAT", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und mach uns arme Leute fein", "tokens": ["Und", "mach", "uns", "ar\u00b7me", "Leu\u00b7te", "fein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Heilig und rein.", "tokens": ["Hei\u00b7lig", "und", "rein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}