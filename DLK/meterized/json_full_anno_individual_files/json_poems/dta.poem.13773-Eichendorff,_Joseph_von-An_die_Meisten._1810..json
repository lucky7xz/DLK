{"dta.poem.13773": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An die Meisten.  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ist denn alles ganz vergebens?", "tokens": ["Ist", "denn", "al\u00b7les", "ganz", "ver\u00b7ge\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freiheit, Ruhm und treue Sitte,", "tokens": ["Frei\u00b7heit", ",", "Ruhm", "und", "treu\u00b7e", "Sit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ritterbild des alten Lebens,", "tokens": ["Rit\u00b7ter\u00b7bild", "des", "al\u00b7ten", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog im Lied durch eure Mitte", "tokens": ["Zog", "im", "Lied", "durch", "eu\u00b7re", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hohnverlacht als Don Quixote;", "tokens": ["Hohn\u00b7ver\u00b7lacht", "als", "Don", "Qui\u00b7xo\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NE", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Euch deckt Schlaf mit plumper Pfote,", "tokens": ["Euch", "deckt", "Schlaf", "mit", "plum\u00b7per", "Pfo\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Ehre ist euch Zote.", "tokens": ["Und", "die", "Eh\u00b7re", "ist", "euch", "Zo\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}