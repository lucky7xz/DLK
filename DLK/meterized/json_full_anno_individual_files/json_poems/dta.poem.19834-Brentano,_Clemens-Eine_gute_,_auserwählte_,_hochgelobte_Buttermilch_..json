{"dta.poem.19834": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Eine gute ,  auserw\u00e4hlte ,  hochgelobte  \n Buttermilch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eins Bauren Sohn h\u00e4tt' sich vermessen,               ", "tokens": ["Eins", "Bau\u00b7ren", "Sohn", "h\u00e4tt'", "sich", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er wollt ein gute Buttermilch essen,", "tokens": ["Er", "wollt", "ein", "gu\u00b7te", "But\u00b7ter\u00b7milch", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein auserw\u00e4hlte Milch, ein hochgelobte Milch,", "tokens": ["Ein", "au\u00b7ser\u00b7w\u00e4hl\u00b7te", "Milch", ",", "ein", "hoch\u00b7ge\u00b7lob\u00b7te", "Milch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein abgefeimte Milch, des Milry Milch, ein gute But-", "tokens": ["Ein", "ab\u00b7ge\u00b7feim\u00b7te", "Milch", ",", "des", "Mil\u00b7ry", "Milch", ",", "ein", "gu\u00b7te", "But"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NE", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "termilch.", "tokens": ["ter\u00b7milch", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Man trug ihm her ein saures Kraut,", "tokens": ["Man", "trug", "ihm", "her", "ein", "sau\u00b7res", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Buttermilch traf ihn bas in die Haut,", "tokens": ["Die", "But\u00b7ter\u00b7milch", "traf", "ihn", "bas", "in", "die", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein auserw\u00e4hlte Milch, ein hochgelobte Milch,", "tokens": ["Ein", "au\u00b7ser\u00b7w\u00e4hl\u00b7te", "Milch", ",", "ein", "hoch\u00b7ge\u00b7lob\u00b7te", "Milch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein abgefeimte Milch, des Milry Milch, ein gute But-", "tokens": ["Ein", "ab\u00b7ge\u00b7feim\u00b7te", "Milch", ",", "des", "Mil\u00b7ry", "Milch", ",", "ein", "gu\u00b7te", "But"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NE", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "termilch.", "tokens": ["ter\u00b7milch", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Man trug ihm her ein Schweinebraten,", "tokens": ["Man", "trug", "ihm", "her", "ein", "Schwei\u00b7ne\u00b7bra\u00b7ten", ","], "token_