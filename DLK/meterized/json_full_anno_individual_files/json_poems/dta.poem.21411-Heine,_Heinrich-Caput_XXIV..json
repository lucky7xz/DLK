{"dta.poem.21411": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ich die enge Sahltrepp\u2019 hinauf", "tokens": ["Wie", "ich", "die", "en\u00b7ge", "Sahl\u00b7trepp'", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Gekommen, ich kann es nicht sagen;", "tokens": ["Ge\u00b7kom\u00b7men", ",", "ich", "kann", "es", "nicht", "sa\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es haben unsichtbare Geister mich", "tokens": ["Es", "ha\u00b7ben", "un\u00b7sicht\u00b7ba\u00b7re", "Geis\u00b7ter", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "PPER"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Vielleicht hinaufgetragen.", "tokens": ["Viel\u00b7leicht", "hin\u00b7auf\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}