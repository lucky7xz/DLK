{"textgrid.poem.65806": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "80", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, wie verzag' ich, wenn ich, von dir dichtend,", "tokens": ["O", ",", "wie", "ver\u00b7zag'", "ich", ",", "wenn", "ich", ",", "von", "dir", "dich\u00b7tend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "$,", "APPR", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wei\u00df, wie ein bess'rer Geist dich hoch erhob,", "tokens": ["Wei\u00df", ",", "wie", "ein", "bess'\u00b7rer", "Geist", "dich", "hoch", "er\u00b7hob", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf deinen Ruhm all seine Kr\u00e4fte richtend,", "tokens": ["Auf", "dei\u00b7nen", "Ruhm", "all", "sei\u00b7ne", "Kr\u00e4f\u00b7te", "rich\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich verstummen mu\u00df mit meinem Lob.", "tokens": ["Da\u00df", "ich", "ver\u00b7stum\u00b7men", "mu\u00df", "mit", "mei\u00b7nem", "Lob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch, weil dein Wert, weit wie der Ozean,", "tokens": ["Doch", ",", "weil", "dein", "Wert", ",", "weit", "wie", "der", "O\u00b7ze\u00b7an", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPOSAT", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die \u00e4rmsten Segel tr\u00e4gt wie reichste Schiffe,", "tokens": ["Die", "\u00e4rms\u00b7ten", "Se\u00b7gel", "tr\u00e4gt", "wie", "reichs\u00b7te", "Schif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wagt sich mein kecker, weit gering'rer Kahn", "tokens": ["Wagt", "sich", "mein", "ke\u00b7cker", ",", "weit", "ge\u00b7ring'\u00b7rer", "Kahn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "$,", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Mutwillig in die Fluten deiner Tiefe.", "tokens": ["Mut\u00b7wil\u00b7lig", "in", "die", "Flu\u00b7ten", "dei\u00b7ner", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Mir macht zur Fahrt dein kleinster Beistand Bahn,", "tokens": ["Mir", "macht", "zur", "Fahrt", "dein", "kleins\u00b7ter", "Bei\u00b7stand", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wenn er auf deiner vollsten Woge ruht:", "tokens": ["Wenn", "er", "auf", "dei\u00b7ner", "volls\u00b7ten", "Wo\u00b7ge", "ruht", ":"], 