{"dta.poem.4216": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ruhe-Platz im Grase.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da ich in dem bebl\u00fchmten Grase", "tokens": ["Da", "ich", "in", "dem", "be\u00b7bl\u00fchm\u00b7ten", "Gra\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "J\u00fcngst ausgestrecket lag und lase;", "tokens": ["J\u00fcngst", "aus\u00b7ge\u00b7stre\u00b7cket", "lag", "und", "la\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Empfand ich, nicht nur im ", "tokens": ["Emp\u00b7fand", "ich", ",", "nicht", "nur", "im"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PTKNEG", "ADV", "APPRART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df solches angenehm und k\u00fchl;", "tokens": ["Da\u00df", "sol\u00b7ches", "an\u00b7ge\u00b7nehm", "und", "k\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein lieblich-s\u00e4urlicher ", "tokens": ["Ein", "lieb\u00b7lich\u00b7s\u00e4ur\u00b7li\u00b7cher"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "und Brust", "tokens": ["und", "Brust"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Annoch mit einer neuen Lust,", "tokens": ["An\u00b7noch", "mit", "ei\u00b7ner", "neu\u00b7en", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die mich, indem ich sie erwog,", "tokens": ["Die", "mich", ",", "in\u00b7dem", "ich", "sie", "er\u00b7wog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und ernstlich in Betrachtung zog,", "tokens": ["Und", "ernst\u00b7lich", "in", "Be\u00b7trach\u00b7tung", "zog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mich zu vergn\u00fcgen trieb. Noch mehr,", "tokens": ["Mich", "zu", "ver\u00b7gn\u00fc\u00b7gen", "trieb", ".", "Noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVFIN", "$.", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Es flisterten dabey gelinde,", "tokens": ["Es", "flis\u00b7ter\u00b7ten", "da\u00b7bey", "ge\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Jm nahen Schilfe, sanfte Winde,", "tokens": ["Jm", "na\u00b7hen", "Schil\u00b7fe", ",", "sanf\u00b7te", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und die\u00df vergn\u00fcgte mein ", "tokens": ["Und", "die\u00df", "ver\u00b7gn\u00fcg\u00b7te", "mein"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Ein kleines gurgelnd V\u00f6gelein", "tokens": ["Ein", "klei\u00b7nes", "gur\u00b7gelnd", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Stimmt\u2019 hin und wieder noch mit ein.", "tokens": ["Stimmt'", "hin", "und", "wie\u00b7der", "noch", "mit", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADV", "ADV", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Um nun bey meinem sanften Liegen", "tokens": ["Um", "nun", "bey", "mei\u00b7nem", "sanf\u00b7ten", "Lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Auch den ", "tokens": ["Auch", "den"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Pfl\u00fcckt ich, mit fr\u00f6hlichem Gem\u00fchte,", "tokens": ["Pfl\u00fcckt", "ich", ",", "mit", "fr\u00f6h\u00b7li\u00b7chem", "Ge\u00b7m\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Von einem Nessel-Strauch die Bl\u00fchte,", "tokens": ["Von", "ei\u00b7nem", "Nes\u00b7sel\u00b7Strauch", "die", "Bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Mit Vorsicht-vollen Fingern, ab,", "tokens": ["Mit", "Vor\u00b7sicht\u00b7vol\u00b7len", "Fin\u00b7gern", ",", "ab", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die mir denn meine Lust vermehrte,", "tokens": ["Die", "mir", "denn", "mei\u00b7ne", "Lust", "ver\u00b7mehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und mir, was ich von ihr begehrte,", "tokens": ["Und", "mir", ",", "was", "ich", "von", "ihr", "be\u00b7gehr\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PWS", "PPER", "APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Den Saft, so s\u00fc\u00df als Honig, gab.", "tokens": ["Den", "Saft", ",", "so", "s\u00fc\u00df", "als", "Ho\u00b7nig", ",", "gab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KOKOM", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Vier Sinnen hatt\u2019 ich nun vergn\u00fcget,", "tokens": ["Vier", "Sin\u00b7nen", "hatt'", "ich", "nun", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Der f\u00fcnfte ward dazu gef\u00fcget,", "tokens": ["Der", "f\u00fcnf\u00b7te", "ward", "da\u00b7zu", "ge\u00b7f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Dem hier am wenigsten gebricht.", "tokens": ["Dem", "hier", "am", "we\u00b7nigs\u00b7ten", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPRART", "PIS", "VVFIN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.27": {"text": "Denn alles, was ich hier erblickte,", "tokens": ["Denn", "al\u00b7les", ",", "was", "ich", "hier", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Schien, als ob sichs ausdr\u00fccklich schm\u00fcckte", "tokens": ["Schien", ",", "als", "ob", "sichs", "aus\u00b7dr\u00fcck\u00b7lich", "schm\u00fcck\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOKOM", "KOUS", "PIS", "ADJD", "VVFIN"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.29": {"text": "Zur Seelen-Anmuht, durchs ", "tokens": ["Zur", "See\u00b7len\u00b7Anm\u00b7uht", ",", "durchs"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPRART", "NN", "$,", "APPRART"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Welch ein smaragdenes Gepr\u00e4nge,", "tokens": ["Welch", "ein", "sma\u00b7rag\u00b7de\u00b7nes", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Welch\u2019 ungez\u00e4hlter Bluhmen Menge,", "tokens": ["Welch'", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter", "Bluh\u00b7men", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Welch ein unz\u00e4hlbar Kr\u00e4uter-Heer,", "tokens": ["Welch", "ein", "un\u00b7z\u00e4hl\u00b7bar", "Kr\u00e4u\u00b7ter\u00b7Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Worinn bald Licht, bald Schatten spielen,", "tokens": ["Wo\u00b7rinn", "bald", "Licht", ",", "bald", "Schat\u00b7ten", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "$,", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wodurch der Sonnen Strahlen fielen,", "tokens": ["Wo\u00b7durch", "der", "Son\u00b7nen", "Strah\u00b7len", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vergn\u00fcgten mich je mehr und mehr!", "tokens": ["Ver\u00b7gn\u00fcg\u00b7ten", "mich", "je", "mehr", "und", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Welch\u2019 eine Farb- und Formen-Schaar", "tokens": ["Welch'", "ei\u00b7ne", "Fa\u00b7rb", "und", "For\u00b7men\u00b7Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Ward ich an diesem Ort gewahr!", "tokens": ["Ward", "ich", "an", "die\u00b7sem", "Ort", "ge\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So da\u00df, wenn ich es \u00fcberlege,", "tokens": ["So", "da\u00df", ",", "wenn", "ich", "es", "\u00fc\u00b7berl\u00b7e\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und das empfindliche Vergn\u00fcgen,", "tokens": ["Und", "das", "emp\u00b7find\u00b7li\u00b7che", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wenn wir im frischen Grase liegen,", "tokens": ["Wenn", "wir", "im", "fri\u00b7schen", "Gra\u00b7se", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "So, wie wir schuldig sind, erwege;", "tokens": ["So", ",", "wie", "wir", "schul\u00b7dig", "sind", ",", "er\u00b7we\u00b7ge", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ich finde, da\u00df sich unsre Seele", "tokens": ["Ich", "fin\u00b7de", ",", "da\u00df", "sich", "uns\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Mit der bebl\u00fchmten Erden Pracht,", "tokens": ["Mit", "der", "be\u00b7bl\u00fchm\u00b7ten", "Er\u00b7den", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Durch einen jeden Sinn, verm\u00e4hle.", "tokens": ["Durch", "ei\u00b7nen", "je\u00b7den", "Sinn", ",", "ver\u00b7m\u00e4h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ich preise denn des Sch\u00f6pfers Macht,", "tokens": ["Ich", "prei\u00b7se", "denn", "des", "Sch\u00f6p\u00b7fers", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und dank\u2019 Jhm, da\u00df ich diese Gabe,", "tokens": ["Und", "dank'", "Jhm", ",", "da\u00df", "ich", "die\u00b7se", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Gedanken, und f\u00fcnf Sinnen, habe.", "tokens": ["Ge\u00b7dan\u00b7ken", ",", "und", "f\u00fcnf", "Sin\u00b7nen", ",", "ha\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KON", "CARD", "NN", "$,", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}