{"dta.poem.10685": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet  \n An der Liebsten Vatterlandt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Dv aller sch\u00f6nster Ort der Fl\u00fc\u00df vnd kalten Bronnen/", "tokens": ["Dv", "al\u00b7ler", "sch\u00f6ns\u00b7ter", "Ort", "der", "Fl\u00fc\u00df", "vnd", "kal\u00b7ten", "Bron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "ART", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dahin sich alle Zier vnd Lust hat eingestalt/", "tokens": ["Da\u00b7hin", "sich", "al\u00b7le", "Zier", "vnd", "Lust", "hat", "ein\u00b7ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PIAT", "NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dahin sich alles Gut begeben mannigfalt/", "tokens": ["Da\u00b7hin", "sich", "al\u00b7les", "Gut", "be\u00b7ge\u00b7ben", "man\u00b7nig\u00b7falt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PIS", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So jemals worden ist beschienen von der Sonnen/", "tokens": ["So", "je\u00b7mals", "wor\u00b7den", "ist", "be\u00b7schie\u00b7nen", "von", "der", "Son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAPP", "VAFIN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du aller sch\u00f6nste Statt/ du Hau\u00df der Frewd vnd Wonnen/", "tokens": ["Du", "al\u00b7ler", "sch\u00f6ns\u00b7te", "Statt", "/", "du", "Hau\u00df", "der", "Frewd", "vnd", "Won\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "$(", "PPER", "NN", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Printze\u00dfin aller St\u00e4tt an Reichthumb vnd Gewalt/", "tokens": ["Print\u00b7ze\u00b7\u00dfin", "al\u00b7ler", "St\u00e4tt", "an", "Reicht\u00b7humb", "vnd", "Ge\u00b7walt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Doch mehr/ weil du erzeygt meins Lebens vffenthalt/", "tokens": ["Doch", "mehr", "/", "weil", "du", "er\u00b7zeygt", "meins", "Le\u00b7bens", "vf\u00b7fent\u00b7halt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der keine Sch\u00e4tze nicht verglichen werden k\u00f6nnen/", "tokens": ["Der", "kei\u00b7ne", "Sch\u00e4t\u00b7ze", "nicht", "ver\u00b7gli\u00b7chen", "wer\u00b7den", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VVPP", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Verzeihe mir du Statt darinnen ich geboren/", "tokens": ["Ver\u00b7zei\u00b7he", "mir", "du", "Statt", "da\u00b7rin\u00b7nen", "ich", "ge\u00b7bo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hier hab ich mir zu sein ins k\u00fcnfftig au\u00df erkoren/", "tokens": ["Hier", "hab", "ich", "mir", "zu", "sein", "ins", "k\u00fcnff\u00b7tig", "au\u00df", "er\u00b7ko\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PTKZU", "VAINF", "APPRART", "ADJD", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hieher hab ich allein mein Hertz vnd sinn gewandt.", "tokens": ["Hie\u00b7her", "hab", "ich", "al\u00b7lein", "mein", "Hertz", "vnd", "sinn", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vnd ob es mir gleich schwer/ da\u00df ich dich werde meiden/", "tokens": ["Vnd", "ob", "es", "mir", "gleich", "schwer", "/", "da\u00df", "ich", "dich", "wer\u00b7de", "mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "ADV", "ADJD", "$(", "KOUS", "PPER", "PPER", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Will dennoch ich von dir/ als ihr/ viel lieber scheiden.", "tokens": ["Will", "den\u00b7noch", "ich", "von", "dir", "/", "als", "ihr", "/", "viel", "lie\u00b7ber", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "APPR", "PPER", "$(", "KOUS", "PPER", "$(", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dann wo mein Leben ist/ da ist mein Vatterland.", "tokens": ["Dann", "wo", "mein", "Le\u00b7ben", "ist", "/", "da", "ist", "mein", "Vat\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PPOSAT", "NN", "VAFIN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}