{"textgrid.poem.44405": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Rangstreit", "genre": "verse", "period": "N.A.", "pub_year": 1825, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich arme, kleine Rose,", "tokens": ["Ich", "ar\u00b7me", ",", "klei\u00b7ne", "Ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich steh an ihrem Fenster", "tokens": ["Ich", "steh", "an", "ih\u00b7rem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und soll ihr Fenster schm\u00fccken,", "tokens": ["Und", "soll", "ihr", "Fens\u00b7ter", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch ach, die Augen aller", "tokens": ["Doch", "ach", ",", "die", "Au\u00b7gen", "al\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ART", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sehn nur nach meiner Herrin,", "tokens": ["Sehn", "nur", "nach", "mei\u00b7ner", "Her\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "Und keines sieht nach mir hin!", "tokens": ["Und", "kei\u00b7nes", "sieht", "nach", "mir", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Bin ich denn nicht die Rose,", "tokens": ["Bin", "ich", "denn", "nicht", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die K\u00f6nigin der Blumen,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "der", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Warum denn schaut ihr mich nicht", "tokens": ["Wa\u00b7rum", "denn", "schaut", "ihr", "mich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PRF", "PTKNEG"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und schaut nur nach der Herrin?", "tokens": ["Und", "schaut", "nur", "nach", "der", "Her\u00b7rin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Strahl ich nicht r\u00f6tlich schimmernd,", "tokens": ["Strahl", "ich", "nicht", "r\u00f6t\u00b7lich", "schim\u00b7mernd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Von Purpur \u00fcbergossen? \u2013", "tokens": ["Von", "Pur\u00b7pur", "\u00fc\u00b7ber\u00b7gos\u00b7sen", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zwar ", "tokens": ["Zwar"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "F\u00e4rbt Morgenrot, wie meine,", "tokens": ["F\u00e4rbt", "Mor\u00b7gen\u00b7rot", ",", "wie", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PWAV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und gern, wie gerne! tauscht ich!", "tokens": ["Und", "gern", ",", "wie", "ger\u00b7ne", "!", "tauscht", "ich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWAV", "ADV", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Seht meine schlanken Stengel \u2013", "tokens": ["Seht", "mei\u00b7ne", "schlan\u00b7ken", "Sten\u00b7gel", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwar schlank ist sie wohl selber,", "tokens": ["Zwar", "schlank", "ist", "sie", "wohl", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wer sie darf umfassen,", "tokens": ["Und", "wer", "sie", "darf", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gern mi\u00dft er meine Dornen.", "tokens": ["Gern", "mi\u00dft", "er", "mei\u00b7ne", "Dor\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch was gleicht meinen Knospen,", "tokens": ["Doch", "was", "gleicht", "mei\u00b7nen", "Knos\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Westwind lieblich spielend?", "tokens": ["Im", "West\u00b7wind", "lieb\u00b7lich", "spie\u00b7lend", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und doch \u2013 als sie am Fenster", "tokens": ["Und", "doch", "\u2013", "als", "sie", "am", "Fens\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$(", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich, niederschauend, beugte,", "tokens": ["Sich", ",", "nie\u00b7der\u00b7schau\u00b7end", ",", "beug\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PRF", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gewahrt ich Zwillingskn\u00f6spchen,", "tokens": ["Ge\u00b7wahrt", "ich", "Zwil\u00b7lings\u00b7kn\u00f6spc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gleich meinen rund und h\u00e4rtlich,", "tokens": ["Gleich", "mei\u00b7nen", "rund", "und", "h\u00e4rt\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gleich meinen r\u00f6tlich strahlend,", "tokens": ["Gleich", "mei\u00b7nen", "r\u00f6t\u00b7lich", "strah\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gleich meinen halb geschlossen,", "tokens": ["Gleich", "mei\u00b7nen", "halb", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gleich meinen leise wogend", "tokens": ["Gleich", "mei\u00b7nen", "lei\u00b7se", "wo\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und strebend nach Enth\u00fcllung.", "tokens": ["Und", "stre\u00b7bend", "nach", "Ent\u00b7h\u00fcl\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch seht im Bl\u00e4tterdunkel", "tokens": ["Doch", "seht", "im", "Bl\u00e4t\u00b7ter\u00b7dun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den vollen Kelch der Rose", "tokens": ["Den", "vol\u00b7len", "Kelch", "der", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit kleinem Laub ums\u00e4umet,", "tokens": ["Mit", "klei\u00b7nem", "Laub", "um\u00b7s\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom Rande, voll und schwellend,", "tokens": ["Vom", "Ran\u00b7de", ",", "voll", "und", "schwel\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nach Innen sanft sich w\u00f6lbend,", "tokens": ["Nach", "In\u00b7nen", "sanft", "sich", "w\u00f6l\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In holder Scham err\u00f6tend,", "tokens": ["In", "hol\u00b7der", "Scham", "er\u00b7r\u00f6\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Labyrinth von Bl\u00e4ttern,", "tokens": ["Ein", "La\u00b7by\u00b7rinth", "von", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die selber sich beschattend,", "tokens": ["Die", "sel\u00b7ber", "sich", "be\u00b7schat\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gleich einer Grotte Dunkel,", "tokens": ["Gleich", "ei\u00b7ner", "Grot\u00b7te", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Sich tief und immer tiefer", "tokens": ["Sich", "tief", "und", "im\u00b7mer", "tie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "In D\u00e4mmernacht verlieren.", "tokens": ["In", "D\u00e4m\u00b7mer\u00b7nacht", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wann saht ihr an der Herrin", "tokens": ["Wann", "saht", "ihr", "an", "der", "Her\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.2": {"text": "Wohl einen Reiz gleich diesem?", "tokens": ["Wohl", "ei\u00b7nen", "Reiz", "gleich", "die\u00b7sem", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "PDAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darin mag sie mir gleichen,", "tokens": ["Da\u00b7rin", "mag", "sie", "mir", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann will ich erst ihr weichen,", "tokens": ["Dann", "will", "ich", "erst", "ihr", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann reich ich ihr die Krone", "tokens": ["Dann", "reich", "ich", "ihr", "die", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und nenne sie die Rose,", "tokens": ["Und", "nen\u00b7ne", "sie", "die", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich sie, die Rose selber.", "tokens": ["Ich", "sie", ",", "die", "Ro\u00b7se", "sel\u00b7ber", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "$,", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich arme, kleine Rose,", "tokens": ["Ich", "ar\u00b7me", ",", "klei\u00b7ne", "Ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word"