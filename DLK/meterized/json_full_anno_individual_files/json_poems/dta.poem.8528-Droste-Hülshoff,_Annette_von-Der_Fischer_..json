{"dta.poem.8528": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fischer .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wehe dem kleinen Fischerssohn,", "tokens": ["We\u00b7he", "dem", "klei\u00b7nen", "Fi\u00b7schers\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "De\u00df Vater fischen gegangen;", "tokens": ["De\u00df", "Va\u00b7ter", "fi\u00b7schen", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "An den Strand l\u00e4uft er t\u00e4glich hinaus,", "tokens": ["An", "den", "Strand", "l\u00e4uft", "er", "t\u00e4g\u00b7lich", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Am Morgen, am Abend nicht minder;", "tokens": ["Am", "Mor\u00b7gen", ",", "am", "A\u00b7bend", "nicht", "min\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "\u201ekehre, Vater, o kehre zur\u00fcck,", "tokens": ["\u201e", "keh\u00b7re", ",", "Va\u00b7ter", ",", "o", "keh\u00b7re", "zu\u00b7r\u00fcck", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "FM", "VVFIN", "PTKVZ", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und bringe die guten Fische!", "tokens": ["Und", "brin\u00b7ge", "die", "gu\u00b7ten", "Fi\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Kleider, reiche, Sandalen auch", "tokens": ["Klei\u00b7der", ",", "rei\u00b7che", ",", "San\u00b7da\u00b7len", "auch"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Und rede freundliche Worte;", "tokens": ["Und", "re\u00b7de", "freund\u00b7li\u00b7che", "Wor\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.9": {"text": "Denn die Mutter in Gr\u00e4men ist stumm,", "tokens": ["Denn", "die", "Mut\u00b7ter", "in", "Gr\u00e4\u00b7men", "ist", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VAFIN", "ADJD", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.10": {"text": "Und der Gl\u00e4ub\u2019ger nahm die Gewande!\u201c", "tokens": ["Und", "der", "Gl\u00e4ub'\u00b7ger", "nahm", "die", "Ge\u00b7wan\u00b7de", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}