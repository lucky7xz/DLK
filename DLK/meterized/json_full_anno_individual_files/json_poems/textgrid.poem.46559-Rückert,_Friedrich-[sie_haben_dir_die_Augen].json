c.tri"}, "line.2": {"text": "Das Haus, das zu schwache,", "tokens": ["Das", "Haus", ",", "das", "zu", "schwa\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df sie noch einmal s\u00e4umte", "tokens": ["Da\u00df", "sie", "noch", "ein\u00b7mal", "s\u00e4um\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im sch\u00f6nsten Gemache;", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "Ge\u00b7ma\u00b7che", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Daraus noch einmal blickte", "tokens": ["Da\u00b7raus", "noch", "ein\u00b7mal", "blick\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins irdische Leben,", "tokens": ["Ins", "ir\u00b7di\u00b7sche", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Eh' sie den Flug beschickte", "tokens": ["Eh'", "sie", "den", "Flug", "be\u00b7schick\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um h\u00f6her zu schweben.", "tokens": ["Um", "h\u00f6\u00b7her", "zu", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Und ist's nicht drin die deine,", "tokens":