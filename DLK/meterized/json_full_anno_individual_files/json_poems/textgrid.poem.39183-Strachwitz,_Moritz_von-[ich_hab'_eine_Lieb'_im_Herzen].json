{"textgrid.poem.39183": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[ich hab' eine Lieb' im Herzen]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' eine Lieb' im Herzen, die sing' ich nimmer aus,", "tokens": ["Ich", "hab'", "ei\u00b7ne", "Lieb'", "im", "Her\u00b7zen", ",", "die", "sing'", "ich", "nim\u00b7mer", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich hab' einen Wein im Humpen, nie geht sein Schimmer aus;", "tokens": ["Ich", "hab'", "ei\u00b7nen", "Wein", "im", "Hum\u00b7pen", ",", "nie", "geht", "sein", "Schim\u00b7mer", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hab' immer das Lieb' am Busen und k\u00fcss' es immerfort,", "tokens": ["Hab'", "im\u00b7mer", "das", "Lieb'", "am", "Bu\u00b7sen", "und", "k\u00fcss'", "es", "im\u00b7mer\u00b7fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPRART", "NN", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Hab' immer den Kelch am Munde und trink' ihn immer aus;", "tokens": ["Hab'", "im\u00b7mer", "den", "Kelch", "am", "Mun\u00b7de", "und", "trink'", "ihn", "im\u00b7mer", "aus", ";"], "token_info": 