": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und st\u00e4nd' ich am Himmelstore, ich schl\u00fcg' es trunken ein,", "tokens": ["Und", "st\u00e4nd'", "ich", "am", "Him\u00b7mels\u00b7to\u00b7re", ",", "ich", "schl\u00fcg'", "es", "trun\u00b7ken", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Und schl\u00f6sse mich auch Sankt Peter von Gottes Zimmer aus.", "tokens": ["Und", "schl\u00f6s\u00b7se", "mich", "auch", "Sankt", "Pe\u00b7ter", "von", "Got\u00b7tes", "Zim\u00b7mer", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVFIN", "NE", "APPR", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich hab' eine Lieb' im Herzen, die sing' ich nimmer aus,", "tokens": ["Ich", "hab'", "ei\u00b7ne", "Lieb'", "im", "Her\u00b7zen", ",", "die", "sing'", "ich", "nim\u00b7mer", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich hab' einen Wein im Humpen, nie geht sein Schimmer aus;", "tokens": ["Ich", "hab'", "ei\u00b7nen", "Wein", "im", "Hum\u00b7pen", ",", "nie", "geht", "sein", "Schim\u00b7mer", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART