{"textgrid.poem.50943": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: O ein Gl\u00f6cklein klingelt mir fr\u00fch und sp\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O ein Gl\u00f6cklein klingelt mir fr\u00fch und sp\u00e4t", "tokens": ["O", "ein", "Gl\u00f6c\u00b7klein", "klin\u00b7gelt", "mir", "fr\u00fch", "und", "sp\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Silbernen Schalles in die Seele hinein,", "tokens": ["Sil\u00b7ber\u00b7nen", "Schal\u00b7les", "in", "die", "See\u00b7le", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Zart wie ein Luftlied, welches von Osten weht,", "tokens": ["Zart", "wie", "ein", "Luft\u00b7lied", ",", "wel\u00b7ches", "von", "Os\u00b7ten", "weht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Unerm\u00fcdlich plaudernd, melodisch rein!", "tokens": ["Un\u00b7er\u00b7m\u00fcd\u00b7lich", "plau\u00b7dernd", ",", "me\u00b7lo\u00b7disch", "rein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber wandl' ich es um zum Becherlein,", "tokens": ["A\u00b7ber", "wandl'", "ich", "es", "um", "zum", "Be\u00b7cher\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Kehr ich es um und h\u00e4ng es an meinen Mund,", "tokens": ["Kehr", "ich", "es", "um", "und", "h\u00e4ng", "es", "an", "mei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Trinke daraus den allers\u00fc\u00dfesten Wein:", "tokens": ["Trin\u00b7ke", "da\u00b7raus", "den", "al\u00b7ler\u00b7s\u00fc\u00b7\u00dfes\u00b7ten", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Schweigt das Glockenbecherchen zur Stund,", "tokens": ["Schweigt", "das", "Glo\u00b7cken\u00b7be\u00b7cher\u00b7chen", "zur", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+---+", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00e4lt sich stille, solang ich trinken mag,", "tokens": ["H\u00e4lt", "sich", "stil\u00b7le", ",", "so\u00b7lang", "ich", "trin\u00b7ken", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "An meinen durstigen Lippen verhallt sein Rand,", "tokens": ["An", "mei\u00b7nen", "durs\u00b7ti\u00b7gen", "Lip\u00b7pen", "ver\u00b7hallt", "sein", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "T\u00f6net jedoch wieder mit hellem Schlag,", "tokens": ["T\u00f6\u00b7net", "je\u00b7doch", "wie\u00b7der", "mit", "hel\u00b7lem", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Kaum ich es der innigen Haft entband.", "tokens": ["Kaum", "ich", "es", "der", "in\u00b7ni\u00b7gen", "Haft", "ent\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Glas und Gl\u00f6cklein ist, mein Engelchen!", "tokens": ["Glas", "und", "Gl\u00f6c\u00b7klein", "ist", ",", "mein", "En\u00b7gel\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-++-++--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Mir dein M\u00fcndchen ohne Rast und Ruh,", "tokens": ["Mir", "dein", "M\u00fcnd\u00b7chen", "oh\u00b7ne", "Rast", "und", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und das Z\u00fcnglein drin das Schwengelchen,", "tokens": ["Und", "das", "Z\u00fcn\u00b7glein", "drin", "das", "Schwen\u00b7gel\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "l