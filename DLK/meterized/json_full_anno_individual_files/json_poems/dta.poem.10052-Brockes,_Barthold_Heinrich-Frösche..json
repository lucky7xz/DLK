{"dta.poem.10052": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00f6sche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein so gro\u00df als muntrer Geist", "tokens": ["Ein", "so", "gro\u00df", "als", "mun\u00b7trer", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sahe j\u00fcngst auf meinem Garten, wie wir eben ab-", "tokens": ["Sa\u00b7he", "j\u00fcngst", "auf", "mei\u00b7nem", "Gar\u00b7ten", ",", "wie", "wir", "e\u00b7ben", "ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "gespeis't,", "tokens": ["ge\u00b7speis't", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Und die Gegend \u00fcbersahn,", "tokens": ["Und", "die", "Ge\u00b7gend", "\u00fc\u00b7bers\u00b7ahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen kleinen Lust-Altan.", "tokens": ["Ei\u00b7nen", "klei\u00b7nen", "Lust\u00b7Al\u00b7tan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier (rieff er), von dieser Cantzel, predigt Brocks der Fr\u00f6-", "tokens": ["Hier", "(", "rieff", "er", ")", ",", "von", "die\u00b7ser", "Cant\u00b7zel", ",", "pre\u00b7digt", "Brocks", "der", "Fr\u00f6"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "VVFIN", "PPER", "$(", "$,", "APPR", "PDAT", "NN", "$,", "VVFIN", "NN", "ART", "TRUNC"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "sche Chor", "tokens": ["sche", "Chor"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Etwas vor.", "tokens": ["Et\u00b7was", "vor", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Ich verstand, worauf er zielte, dennoch schwieg ich still,", "tokens": ["Ich", "ver\u00b7stand", ",", "wo\u00b7rauf", "er", "ziel\u00b7te", ",", "den\u00b7noch", "schwieg", "ich", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "und lachte,", "tokens": ["und", "lach\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Ob ich gleich in meinem Sinn die\u00df bey seinem Schertz ge-", "tokens": ["Ob", "ich", "gleich", "in", "mei\u00b7nem", "Sinn", "die\u00df", "bey", "sei\u00b7nem", "Schertz", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PDS", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "dachte:", "tokens": ["dach\u00b7te", ":"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Ach ich predige nicht ihnen, sondern es ist \u00fcmge-", "tokens": ["Ach", "ich", "pre\u00b7di\u00b7ge", "nicht", "ih\u00b7nen", ",", "son\u00b7dern", "es", "ist", "\u00fcm\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "PTKNEG", "PPER", "$,", "KON", "PPER", "VAFIN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "kehrt.", "tokens": ["kehrt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Ich bin \u00f6ffters von den Fr\u00f6schen selbst erbauet und", "tokens": ["Ich", "bin", "\u00f6ff\u00b7ters", "von", "den", "Fr\u00f6\u00b7schen", "selbst", "er\u00b7bau\u00b7et", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "KON"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "belehrt.", "tokens": ["be\u00b7lehrt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}}}}