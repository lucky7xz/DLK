{"dta.poem.13387": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Da unter", "tokens": ["Da", "un\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "nach abgewichnen 100 Jahren", "tokens": ["nach", "ab\u00b7ge\u00b7wich\u00b7nen", "100", "Jah\u00b7ren"], "token_info": ["word", "word", "number", "word"], "pos": ["APPR", "ADJA", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "die St\u00e4nde wiederum zuerst beysammen\nwaren,", "tokens": ["die", "St\u00e4n\u00b7de", "wie\u00b7de\u00b7rum", "zu\u00b7erst", "bey\u00b7sam\u00b7men", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das Gotha seinen Landtag nennt;", "tokens": ["das", "Go\u00b7tha", "sei\u00b7nen", "Land\u00b7tag", "nennt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So w\u00fcnscht", "tokens": ["So", "w\u00fcnscht"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "viel", "tokens": ["viel"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}}}}