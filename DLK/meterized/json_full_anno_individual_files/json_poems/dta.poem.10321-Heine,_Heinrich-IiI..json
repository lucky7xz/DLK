{"dta.poem.10321": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Im n\u00e4cht'gen Traum hab' ich mich selbst geschaut,", "tokens": ["Im", "n\u00e4cht'\u00b7gen", "Traum", "hab'", "ich", "mich", "selbst", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im schwarzen Gallafrack und seidner Weste,", "tokens": ["Im", "schwar\u00b7zen", "Gal\u00b7la\u00b7frack", "und", "seid\u00b7ner", "Wes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Manschetten an der Hand, als ging's zum Feste,", "tokens": ["Man\u00b7schet\u00b7ten", "an", "der", "Hand", ",", "als", "ging's", "zum", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und vor mir stand mein Liebchen, s\u00fc\u00df und traut.", "tokens": ["Und", "vor", "mir", "stand", "mein", "Lieb\u00b7chen", ",", "s\u00fc\u00df", "und", "traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich beugte mich und sagte: \u201eSind Sie Braut?", "tokens": ["Ich", "beug\u00b7te", "mich", "und", "sag\u00b7te", ":", "\u201e", "Sind", "Sie", "Braut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "$.", "$(", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ei! ei! so gratulir' ich, meine Beste!\u201c", "tokens": ["Ei", "!", "ei", "!", "so", "gra\u00b7tu\u00b7lir'", "ich", ",", "mei\u00b7ne", "Bes\u00b7te", "!", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "NE", "$.", "ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch fast die Kehle mir zusammenpreste", "tokens": ["Doch", "fast", "die", "Keh\u00b7le", "mir", "zu\u00b7sam\u00b7men\u00b7pres\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der langgezog'ne, vornehm kalte Laut.", "tokens": ["Der", "lang\u00b7ge\u00b7zo\u00b7g'\u00b7ne", ",", "vor\u00b7nehm", "kal\u00b7te", "Laut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Und bitt're Thr\u00e4nen pl\u00f6tzlich sich ergossen", "tokens": ["Und", "bitt'\u00b7re", "Thr\u00e4\u00b7nen", "pl\u00f6tz\u00b7lich", "sich", "er\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJD", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Aus Liebchens Augen, und in Thr\u00e4nenwogen", "tokens": ["Aus", "Lieb\u00b7chens", "Au\u00b7gen", ",", "und", "in", "Thr\u00e4\u00b7nen\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ist mir das holde Bildni\u00df fast zerflossen.", "tokens": ["Ist", "mir", "das", "hol\u00b7de", "Bild\u00b7ni\u00df", "fast", "zer\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "O s\u00fc\u00dfe Augen, fromme Liebessterne,", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Au\u00b7gen", ",", "from\u00b7me", "Lie\u00b7bess\u00b7ter\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Obschon ihr mir im Wachen oft gelogen,", "tokens": ["Ob\u00b7schon", "ihr", "mir", "im", "Wa\u00b7chen", "oft", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und auch im Traum, glaub' ich euch dennoch gerne!", "tokens": ["Und", "auch", "im", "Traum", ",", "glaub'", "ich", "euch", "den\u00b7noch", "ger\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}