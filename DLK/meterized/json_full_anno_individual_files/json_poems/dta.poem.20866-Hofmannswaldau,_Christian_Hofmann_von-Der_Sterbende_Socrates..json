{"dta.poem.20866": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df unsre Seele voll Gel\u00fccke", "tokens": ["Da\u00df", "uns\u00b7re", "See\u00b7le", "voll", "Ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bald des C\u00f6rpers Krafft verfleucht", "tokens": ["So", "bald", "des", "C\u00f6r\u00b7pers", "Krafft", "ver\u00b7fleucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ohn alles S\u00e4umnis von ihm zeucht/", "tokens": ["Ohn", "al\u00b7les", "S\u00e4um\u00b7nis", "von", "ihm", "zeucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem kurtzen Augenblicke/", "tokens": ["In", "ei\u00b7nem", "kurt\u00b7zen", "Au\u00b7gen\u00b7bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und nach dem bleichen Tode lebt", "tokens": ["Und", "nach", "dem", "blei\u00b7chen", "To\u00b7de", "lebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jmmittelst reicher Himmels-Sch\u00e4tze/", "tokens": ["Jm\u00b7mit\u00b7telst", "rei\u00b7cher", "Him\u00b7mels\u00b7Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da uns die Gottheit \u00fcberhebt", "tokens": ["Da", "uns", "die", "Got\u00b7theit", "\u00fc\u00b7ber\u00b7hebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des Todes m\u00f6rdrischen Ges\u00e4tze.", "tokens": ["Des", "To\u00b7des", "m\u00f6rd\u00b7ri\u00b7schen", "Ge\u00b7s\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}