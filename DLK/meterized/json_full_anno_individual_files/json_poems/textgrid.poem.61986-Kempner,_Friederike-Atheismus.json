{"textgrid.poem.61986": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Atheismus", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es gleitet das Schiff durch pechschwarze Klippen,", "tokens": ["Es", "glei\u00b7tet", "das", "Schiff", "durch", "pech\u00b7schwar\u00b7ze", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Schon g\u00e4hnt es der bannende Abgrund an, \u2013", "tokens": ["Schon", "g\u00e4hnt", "es", "der", "ban\u00b7nen\u00b7de", "Ab\u00b7grund", "an", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "O wollte die Mannschaft den Himmel erblicken, \u2013", "tokens": ["O", "woll\u00b7te", "die", "Mann\u00b7schaft", "den", "Him\u00b7mel", "er\u00b7bli\u00b7cken", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Der Himmel allein sie erretten kann.", "tokens": ["Der", "Him\u00b7mel", "al\u00b7lein", "sie", "er\u00b7ret\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nichts and'res kann retten \u2013 sonst h\u00fcllen die Sterne", "tokens": ["Nichts", "an\u00b7d'\u00b7res", "kann", "ret\u00b7ten", "\u2013", "sonst", "h\u00fcl\u00b7len", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "PIS", "VMFIN", "VVINF", "$(", "ADV", "VVFIN", "ART", "NN"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Euch weinend das Haupt und strahlen euch nicht \u2013", "tokens": ["Euch", "wei\u00b7nend", "das", "Haupt", "und", "strah\u00b7len", "euch", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "ART", "NN", "KON", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und Wetterwolken bedecken am Tage", "tokens": ["Und", "Wet\u00b7ter\u00b7wol\u00b7ken", "be\u00b7de\u00b7cken", "am", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der heitern Sonne weitreichendes Licht. \u2013", "tokens": ["Der", "hei\u00b7tern", "Son\u00b7ne", "weit\u00b7rei\u00b7chen\u00b7des", "Licht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Auch au\u00dfer dem Meere, im eigenen Herzen", "tokens": ["Auch", "au\u00b7\u00dfer", "dem", "Mee\u00b7re", ",", "im", "ei\u00b7ge\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Beginne der Kampf um das Ja und das Nein \u2013", "tokens": ["Be\u00b7gin\u00b7ne", "der", "Kampf", "um", "das", "Ja", "und", "das", "Nein", "\u2013"], "token_info": ["word", "word", "word"