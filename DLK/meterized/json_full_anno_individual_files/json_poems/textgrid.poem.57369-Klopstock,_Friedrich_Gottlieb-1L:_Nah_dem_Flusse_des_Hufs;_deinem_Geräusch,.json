{"textgrid.poem.57369": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nah dem Flusse des Hufs; deinem Ger\u00e4usch,", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nah dem Flusse des Hufs; deinem Ger\u00e4usch,", "tokens": ["Nah", "dem", "Flus\u00b7se", "des", "Hufs", ";", "dei\u00b7nem", "Ge\u00b7r\u00e4usch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Saal'; am kastalischen Arm; dann wieder", "tokens": ["Saal'", ";", "am", "kas\u00b7ta\u00b7li\u00b7schen", "Arm", ";", "dann", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "APPRART", "ADJA", "NN", "$.", "ADV", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "An dir selber; an der Pleisse", "tokens": ["An", "dir", "sel\u00b7ber", ";", "an", "der", "Pleis\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah ich, h\u00f6rt' ich, genoss,", "tokens": ["Sah", "ich", ",", "h\u00f6rt'", "ich", ",", "ge\u00b7noss", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Froher J\u00fcngling, den Lenz; J\u00fcngling, wie ich", "tokens": ["Fro\u00b7her", "J\u00fcng\u00b7ling", ",", "den", "Lenz", ";", "J\u00fcng\u00b7ling", ",", "wie", "ich"], "token_info": ["word", "word", "punct", "word", "