{"textgrid.poem.42599": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "In ein Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit in ihrem Fluge streift nicht blo\u00df", "tokens": ["Die", "Zeit", "in", "ih\u00b7rem", "Flu\u00b7ge", "streift", "nicht", "blo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des Feldes Blumen und des Waldes Schmuck,", "tokens": ["Des", "Fel\u00b7des", "Blu\u00b7men", "und", "des", "Wal\u00b7des", "Schmuck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Glanz der Jugend und die frische Kraft:", "tokens": ["Den", "Glanz", "der", "Ju\u00b7gend", "und", "die", "fri\u00b7sche", "Kraft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ihr schlimmster Raub trifft die Gedankenwelt.", "tokens": ["Ihr", "schlimms\u00b7ter", "Raub", "trifft", "die", "Ge\u00b7dan\u00b7ken\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was sch\u00f6n und edel, reich und g\u00f6ttlich war", "tokens": ["Was", "sch\u00f6n", "und", "e\u00b7del", ",", "reich", "und", "g\u00f6tt\u00b7lich", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADJD", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und jeder Arbeit, jeden Opfers wert,", "tokens": ["Und", "je\u00b7der", "Ar\u00b7beit", ",", "je\u00b7den", "Op\u00b7fers", "wert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das zeigt sie uns so farblos, hohl und klein,", "tokens": ["Das", "zeigt", "sie", "uns", "so", "farb\u00b7los", ",", "hohl", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So nichtig, da\u00df wir selbst vernichtet sind.", "tokens": ["So", "nich\u00b7tig", ",", "da\u00df", "wir", "selbst", "ver\u00b7nich\u00b7tet", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und dennoch wohl uns, wenn die Asche treu", "tokens": ["Und", "den\u00b7noch", "wohl", "uns", ",", "wenn", "die", "A\u00b7sche", "treu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PPER", "$,", "KOUS", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Den Funken hegt, wenn das get\u00e4uschte Herz", "tokens": ["Den", "Fun\u00b7ken", "hegt", ",", "wenn", "das", "ge\u00b7t\u00e4uschte", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-