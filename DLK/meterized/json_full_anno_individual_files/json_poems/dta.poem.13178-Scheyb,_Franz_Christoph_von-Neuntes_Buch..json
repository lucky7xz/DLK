{"dta.poem.13178": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach ihnen sahen wir noch eine Staats-Matron,", "tokens": ["Nach", "ih\u00b7nen", "sa\u00b7hen", "wir", "noch", "ei\u00b7ne", "Staats\u00b7Ma\u00b7tron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir schien es, da\u00df dem Blick, dem Ansehn, der Person", "tokens": ["Mir", "schien", "es", ",", "da\u00df", "dem", "Blick", ",", "dem", "An\u00b7sehn", ",", "der", "Per\u00b7son"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihren Mienen nach Sie dem Mit-Herrscher gleiche;", "tokens": ["Und", "ih\u00b7ren", "Mie\u00b7nen", "nach", "Sie", "dem", "Mit\u00b7Herr\u00b7scher", "glei\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "ART", "NN", "ADJA", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sonst keiner Freundlichkeit noch muntrer Hoheit weiche.", "tokens": ["Sonst", "kei\u00b7ner", "Freund\u00b7lich\u00b7keit", "noch", "mun\u00b7trer", "Ho\u00b7heit", "wei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "ADJA", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "An ihrer Seite kahm als F\u00fchrer und Gespan", "tokens": ["An", "ih\u00b7rer", "Sei\u00b7te", "kahm", "als", "F\u00fch\u00b7rer", "und", "Ge\u00b7span"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "470Ein Heldenm\u00fcthiger, lebhafter Krieges-Mann;", "tokens": ["Hel\u00b7den\u00b7m\u00fct\u00b7hi\u00b7ger", ",", "leb\u00b7haf\u00b7ter", "Krie\u00b7ge\u00b7sMann", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Von dem Thalia sprach: \u201eSieh! dieses ist der Krieger,", "tokens": ["Von", "dem", "Tha\u00b7lia", "sprach", ":", "\u201e", "Sieh", "!", "die\u00b7ses", "ist", "der", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "$(", "NE", "$.", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "\u201eund unter Kriegenden der Lorber-werthste Sieger.", "tokens": ["\u201e", "und", "un\u00b7ter", "Krie\u00b7gen\u00b7den", "der", "Lor\u00b7ber\u00b7wert\u00b7hs\u00b7te", "Sie\u00b7ger", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "\u201edie\u00df ist der Prinz, von dem die Weisheit uns erz\u00e4hlt,", "tokens": ["\u201e", "die\u00df", "ist", "der", "Prinz", ",", "von", "dem", "die", "Weis\u00b7heit", "uns", "er\u00b7z\u00e4hlt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewie sie desselben Zug sich immer beygesellt;", "tokens": ["\u201e", "wie", "sie", "des\u00b7sel\u00b7ben", "Zug", "sich", "im\u00b7mer", "bey\u00b7ge\u00b7sellt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "PDAT", "NN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "475\u201dWie sie sein Helden-Herz zum Fechten angeleitet,", "tokens": ["\"", "Wie", "sie", "sein", "Hel\u00b7den\u00b7Herz", "zum", "Fech\u00b7ten", "an\u00b7ge\u00b7lei\u00b7tet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eund ihm die wahre Bahn zum Siegen angedeutet.", "tokens": ["\u201e", "und", "ihm", "die", "wah\u00b7re", "Bahn", "zum", "Sie\u00b7gen", "an\u00b7ge\u00b7deu\u00b7tet", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eer ist derjenige, dem weder die Gefahr,", "tokens": ["\u201e", "er", "ist", "der\u00b7je\u00b7ni\u00b7ge", ",", "dem", "we\u00b7der", "die", "Ge\u00b7fahr", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PDS", "$,", "PRELS", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201enoch die Gewalt des Feinds Angst oder Furcht gebar.", "tokens": ["\u201e", "noch", "die", "Ge\u00b7walt", "des", "Feinds", "Angst", "o\u00b7der", "Furcht", "ge\u00b7bar", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "ART", "NN", "NN", "KON", "NN", "ADJD", "$."], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "\u201eer ist der, dessen Geist und Muth nichts konnte schrecken;", "tokens": ["\u201e", "er", "ist", "der", ",", "des\u00b7sen", "Geist", "und", "Muth", "nichts", "konn\u00b7te", "schre\u00b7cken", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "$,", "PRELAT", "NN", "KON", "NN", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "480\u201dMan sah sein Herz dadurch zum K\u00e4mpfen sich erwecken.", "tokens": ["\"", "Man", "sah", "sein", "Herz", "da\u00b7durch", "zum", "K\u00e4mp\u00b7fen", "sich", "er\u00b7we\u00b7cken", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "PPOSAT", "NN", "PAV", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201erath, Wachsamkeit und Macht war seines Heers Geleit;", "tokens": ["\u201e", "rath", ",", "Wach\u00b7sam\u00b7keit", "und", "Macht", "war", "sei\u00b7nes", "Heers", "Ge\u00b7leit", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "NN", "KON", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201edie Weisheit stund f\u00fcr ihn auf jeden Fall bereit:", "tokens": ["\u201e", "die", "Weis\u00b7heit", "stund", "f\u00fcr", "ihn", "auf", "je\u00b7den", "Fall", "be\u00b7reit", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "APPR", "PPER", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "\u201esie wies ihm, wie man sich zum K\u00e4mpfen sollte wagen;", "tokens": ["\u201e", "sie", "wies", "ihm", ",", "wie", "man", "sich", "zum", "K\u00e4mp\u00b7fen", "soll\u00b7te", "wa\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "PWAV", "PIS", "PRF", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "\u201ewie man der Feinde Stolz und Hochmuth k\u00f6nnte schlagen.", "tokens": ["\u201e", "wie", "man", "der", "Fein\u00b7de", "Stolz", "und", "Hoch\u00b7muth", "k\u00f6nn\u00b7te", "schla\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "ART", "NN", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "485\u201dDer Feldzug, welchen er im Feindes Land gemacht,", "tokens": ["\"", "Der", "Feld\u00b7zug", ",", "wel\u00b7chen", "er", "im", "Fein\u00b7des", "Land", "ge\u00b7macht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PWAT", "PPER", "APPRART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "\u201eda er dem st\u00e4rcksten Flu\u00df die Fe\u00dfeln angebracht,", "tokens": ["\u201e", "da", "er", "dem", "st\u00e4rcks\u00b7ten", "Flu\u00df", "die", "Fe\u00b7\u00df\u00b7eln", "an\u00b7ge\u00b7bracht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "\u201eso da\u00df er den Befehl des Prinzens angenommen,", "tokens": ["\u201e", "so", "da\u00df", "er", "den", "Be\u00b7fehl", "des", "Prin\u00b7zens", "an\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "\u201eund nur zu seinem Dienst bebr\u00fccket fort geschwommen;", "tokens": ["\u201e", "und", "nur", "zu", "sei\u00b7nem", "Dienst", "be\u00b7br\u00fc\u00b7cket", "fort", "ge\u00b7schwom\u00b7men", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "\u201eist mehr, dann ein Triumpf des ersten Heldens werth,", "tokens": ["\u201e", "ist", "mehr", ",", "dann", "ein", "Tri\u00b7umpf", "des", "ers\u00b7ten", "Hel\u00b7dens", "werth", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "$,", "ADV", "ART", "NN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "----+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "490\u201dDen je das Alterthum mit Siegs-Geb\u00e4uden ehrt.", "tokens": ["\"", "Den", "je", "das", "Al\u00b7ter\u00b7thum", "mit", "Siegs\u00b7Ge\u00b7b\u00e4u\u00b7den", "ehrt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "\u201ees w\u00e4r ihm dazumahl der gr\u00f6\u00dfte Sieg gelungen;", "tokens": ["\u201e", "es", "w\u00e4r", "ihm", "da\u00b7zu\u00b7mahl", "der", "gr\u00f6\u00df\u00b7te", "Sieg", "ge\u00b7lun\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "\u201eer h\u00e4tt dadurch den Feind so wie den Strohm bezwungen;", "tokens": ["\u201e", "er", "h\u00e4tt", "da\u00b7durch", "den", "Feind", "so", "wie", "den", "Strohm", "be\u00b7zwun\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PAV", "ART", "NN", "ADV", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "\u201eallein es hatte sich ein andrer Feind emp\u00f6rt,", "tokens": ["\u201e", "al\u00b7lein", "es", "hat\u00b7te", "sich", "ein", "an\u00b7drer", "Feind", "em\u00b7p\u00f6rt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "VAFIN", "PRF", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "\u201eder dieses Uebergangs Erfolg und Ziel gest\u00f6rt.", "tokens": ["\u201e", "der", "die\u00b7ses", "Ue\u00b7ber\u00b7gangs", "Er\u00b7folg", "und", "Ziel", "ge\u00b7st\u00f6rt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PDAT", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "495\u201dDer Feind ists, welcher still den argen Rath ergriffe,", "tokens": ["\"", "Der", "Feind", "ists", ",", "wel\u00b7cher", "still", "den", "ar\u00b7gen", "Rath", "er\u00b7grif\u00b7fe", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "$,", "PRELS", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "\u201eund durch verdeckte List den Stahl zum Kriegen schliffe,", "tokens": ["\u201e", "und", "durch", "ver\u00b7deck\u00b7te", "List", "den", "Stahl", "zum", "Krie\u00b7gen", "schlif\u00b7fe", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "ADJA", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "\u201ewomit er unversehns in unsre Grenzen zog,", "tokens": ["\u201e", "wo\u00b7mit", "er", "un\u00b7ver\u00b7sehns", "in", "uns\u00b7re", "Gren\u00b7zen", "zog", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "\u201eund aus dem Vaterland das Marck der V\u00f6lcker sog.", "tokens": ["\u201e", "und", "aus", "dem", "Va\u00b7ter\u00b7land", "das", "Marck", "der", "V\u00f6l\u00b7cker", "so\u00b7g."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["$(", "KON", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "XY"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Das grosse Unternehmen \u00fcber\nden Rhein zu gehen/ war den 1. Ju-\nlij 1744. an mehr als an einem Ort\n\nzugleich und ohne Verlust in das\nWerck gesezet.", "tokens": ["Das", "gros\u00b7se", "Un\u00b7ter\u00b7neh\u00b7men", "\u00fc\u00b7ber", "den", "Rhein", "zu", "ge\u00b7hen", "/", "war", "den", "1.", "Ju", "lij", "17\u00b744", ".", "an", "mehr", "als", "an", "ei\u00b7nem", "Ort", "zu\u00b7gleich", "und", "oh\u00b7ne", "Ver\u00b7lust", "in", "das", "Werck", "ge\u00b7se\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "ordinal", "word", "word", "number", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NE", "PTKZU", "VVINF", "$(", "VAFIN", "ART", "ADJA", "TRUNC", "XY", "CARD", "$.", "APPR", "PIAT", "KOKOM", "APPR", "ART", "NN", "ADV", "KON", "APPR", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+-+-+-+-+-+-+-+-+--+--+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.36": {"text": "\u201ewo weder Gegenwehr, noch Wall; wo keine Waffen,", "tokens": ["\u201e", "wo", "we\u00b7der", "Ge\u00b7gen\u00b7wehr", ",", "noch", "Wall", ";", "wo", "kei\u00b7ne", "Waf\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "KON", "NN", "$,", "ADV", "NE", "$.", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "500\u201dDa kann man sich zwar Beut, doch keinen Sieg verschaffen.", "tokens": ["\"", "Da", "kann", "man", "sich", "zwar", "Beut", ",", "doch", "kei\u00b7nen", "Sieg", "ver\u00b7schaf\u00b7fen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PIS", "PRF", "ADV", "NN", "$,", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}