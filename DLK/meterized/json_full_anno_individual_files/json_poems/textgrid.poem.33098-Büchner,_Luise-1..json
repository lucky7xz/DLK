{"textgrid.poem.33098": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kam die Liebe in mein Herz gezogen,", "tokens": ["Kam", "die", "Lie\u00b7be", "in", "mein", "Herz", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kam nicht wie ein heitrer Sommertag,", "tokens": ["Kam", "nicht", "wie", "ein", "hei\u00b7trer", "Som\u00b7mer\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kam nicht wie das junge Gr\u00fcn im Walde,", "tokens": ["Kam", "nicht", "wie", "das", "jun\u00b7ge", "Gr\u00fcn", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "KOKOM", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie die duft'ge Blume auf der Halde,", "tokens": ["Wie", "die", "duft'\u00b7ge", "Blu\u00b7me", "auf", "der", "Hal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Kam wie Noth und bitt'res Ungemach.", "tokens": ["Kam", "wie", "Noth", "und", "bitt'\u00b7res", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl ist wie ein Sommertag sie kommen,", "tokens": ["Wohl", "ist", "wie", "ein", "Som\u00b7mer\u00b7tag", "sie", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KOKOM", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aber ganz von Staub und Gluth erf\u00fcllt;", "tokens": ["A\u00b7ber", "ganz", "von", "Staub", "und", "Gluth", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie das Gr\u00fcn vom n\u00e4cht'gen Frost verheeret,", "tokens": ["Wie", "das", "Gr\u00fcn", "vom", "n\u00e4cht'\u00b7gen", "Frost", "ver\u00b7hee\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wie die Blume, die der Wurm verzehret,", "tokens": ["Wie", "die", "Blu\u00b7me", ",", "die", "der", "Wurm", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Eh' die Knospe sich noch ganz enth\u00fcllt!", "tokens": ["Eh'", "die", "Knos\u00b7pe", "sich", "noch", "ganz", "ent\u00b7h\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Anders, anders ahnte sie die Seele,", "tokens": ["An\u00b7ders", ",", "an\u00b7ders", "ahn\