{"dta.poem.14293": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachkl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie sind wir doch im Wandern", "tokens": ["Wie", "sind", "wir", "doch", "im", "Wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seitdem so weit zerstreut!", "tokens": ["Seit\u00b7dem", "so", "weit", "zer\u00b7streut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fr\u00e4gt einer nach dem andern,", "tokens": ["Fr\u00e4gt", "ei\u00b7ner", "nach", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch niemand giebt Bescheid.", "tokens": ["Doch", "nie\u00b7mand", "giebt", "Be\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}