{"textgrid.poem.45983": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[kein Tierlein ist auf Erden]", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Tierlein ist auf Erden", "tokens": ["Kein", "Tier\u00b7lein", "ist", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dir, lieber Gott zu klein,", "tokens": ["Dir", ",", "lie\u00b7ber", "Gott", "zu", "klein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du lie\u00dft sie alle werden,", "tokens": ["Du", "lie\u00dft", "sie", "al\u00b7le", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alle sind sie dein.", "tokens": ["Und", "al\u00b7le", "sind", "sie", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu dir, zu dir", "tokens": ["Zu", "dir", ",", "zu", "dir"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ruft Mensch und Tier", "tokens": ["Ruft", "Mensch", "und", "Tier"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der Vogel dir singt", "tokens": ["Der", "Vo\u00b7gel", "dir", "singt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Das Fischlein dir springt", "tokens": ["Das", "Fisc\u00b7hlein", "dir", "springt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Die Biene dir brummt", "tokens": ["Die", "Bie\u00b7ne", "dir", "brummt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Der K\u00e4fer dir summt", "tokens": ["Der", "K\u00e4\u00b7fer", "dir", "summt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Auch pfeifet dir das M\u00e4uslein klein,", "tokens": ["Auch", "pfei\u00b7fet", "dir", "das", "M\u00e4us\u00b7lein", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Herr Gott, du sollst gelobet sein.", "tokens": ["Herr", "Gott", ",", "du", "sollst", "ge\u00b7lo\u00b7bet", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das V\u00f6glein in den L\u00fcften", "tokens": ["Das", "V\u00f6\u00b7glein", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Singt dir aus voller Brust,", "tokens": ["Singt", "dir", "aus", "vol\u00b7ler", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Schlange in den Kl\u00fcften", "tokens": ["Die", "Schlan\u00b7ge", "in", "den", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zischt dir in Lebenslust.", "tokens": ["Zischt", "dir", "in", "Le\u00b7bens\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Zu dir, zu dir", "tokens": ["Zu", "dir", ",", "zu", "dir"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ruft Mensch und Tier usw.", "tokens": ["Ruft", "Mensch", "und", "Tier", "us\u00b7w."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "NN", "KON", "NN", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Fischlein, die da schwimmen", "tokens": ["Die", "Fisc\u00b7hlein", ",", "die", "da", "schwim\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind, Herr, vor dir nicht stumm,", "tokens": ["Sind", ",", "Herr", ",", "vor", "dir", "nicht", "stumm", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "APPR", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du h\u00f6rest ihre Stimmen,", "tokens": ["Du", "h\u00f6\u00b7rest", "ih\u00b7re", "Stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ohn' dich kommt keines um.", "tokens": ["Ohn'", "dich", "kommt", "kei\u00b7nes", "um", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu dir, zu dir usw.", "tokens": ["Zu", "dir", ",", "zu", "dir", "us\u00b7w."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Vor dir tanzt in der Sonne", "tokens": ["Vor", "dir", "tanzt", "in", "der", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der kleinen M\u00fccken Schwarm,", "tokens": ["Der", "klei\u00b7nen", "M\u00fc\u00b7cken", "Schwarm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zum Dank f\u00fcr Lebenswonne", "tokens": ["Zum", "Dank", "f\u00fcr", "Le\u00b7bens\u00b7won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist keins zu klein und arm.", "tokens": ["Ist", "keins", "zu", "klein", "und", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "PTKA", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu dir, zu dir usw.", "tokens": ["Zu", "dir", ",", "zu", "dir", "us\u00b7w."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Sonn', Mond gehn auf und unter", "tokens": ["Sonn'", ",", "Mond", "gehn", "auf", "und", "un\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "VVFIN", "PTKVZ", "KON", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In deinem Gnadenreich,", "tokens": ["In", "dei\u00b7nem", "Gna\u00b7den\u00b7reich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und alle deine Wunder", "tokens": ["Und", "al\u00b7le", "dei\u00b7ne", "Wun\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind sich an Gr\u00f6\u00dfe gleich.", "tokens": ["Sind", "sich", "an", "Gr\u00f6\u00b7\u00dfe", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu dir, zu dir usw.", "tokens": ["Zu", "dir", ",", "zu", "dir", "us\u00b7w."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Zu dir mu\u00df jedes ringen,", "tokens": ["Zu", "dir", "mu\u00df", "je\u00b7des", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn es in N\u00f6ten schwebt,", "tokens": ["Wenn", "es", "in", "N\u00f6\u00b7ten", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur du kannst H\u00fclfe bringen,", "tokens": ["Nur", "du", "kannst", "H\u00fcl\u00b7fe", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch den das Ganze lebt.", "tokens": ["Durch", "den", "das", "Gan\u00b7ze", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Zu dir, zu dir usw.", "tokens": ["Zu", "dir", ",", "zu", "dir", "us\u00b7w."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "In starker Hand die Erde", "tokens": ["In", "star\u00b7ker", "Hand", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tr\u00e4gst du mit Mann und Maus,", "tokens": ["Tr\u00e4gst", "du", "mit", "Mann", "und", "Maus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ruft dein Odem, Werde,", "tokens": ["Es", "ruft", "dein", "O\u00b7dem", ",", "Wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und bl\u00e4st das Lichtlein aus.", "tokens": ["Und", "bl\u00e4st", "das", "Licht\u00b7lein", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu dir, zu dir usw.", "tokens": ["Zu", "dir", ",", "zu", "dir", "us\u00b7w."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Kein Sperling f\u00e4llt vom Dache", "tokens": ["Kein", "Sper\u00b7ling", "f\u00e4llt", "vom", "Da\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ohn' dich, vom Haupt kein Haar,", "tokens": ["Ohn'", "dich", ",", "vom", "Haupt", "kein", "Haar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O teurer Vater wache", "tokens": ["O", "teu\u00b7rer", "Va\u00b7ter", "wa\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bei uns in der Gefahr.", "tokens": ["Bei", "uns", "in", "der", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu dir, zu dir usw.", "tokens": ["Zu", "dir", ",", "zu", "dir", "us\u00b7w."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "NE"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Beh\u00fct' uns vor der Falle", "tokens": ["Be\u00b7h\u00fct'", "uns", "vor", "der", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und vor dem s\u00fc\u00dfen Gift", "tokens": ["Und", "vor", "dem", "s\u00fc\u00b7\u00dfen", "Gift"], "token_info": ["word", "word", "word", 