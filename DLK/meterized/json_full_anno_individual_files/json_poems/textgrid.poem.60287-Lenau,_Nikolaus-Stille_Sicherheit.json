{"textgrid.poem.60287": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Stille Sicherheit", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch, wie still es wird im dunkeln Hain,", "tokens": ["Horch", ",", "wie", "still", "es", "wird", "im", "dun\u00b7keln", "Hain", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00e4dchen, wir sind sicher und allein.", "tokens": ["M\u00e4d\u00b7chen", ",", "wir", "sind", "si\u00b7cher", "und", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Still vers\u00e4uselt hier am Wiesenhang", "tokens": ["Still", "ver\u00b7s\u00e4u\u00b7selt", "hier", "am", "Wie\u00b7sen\u00b7hang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schon der Abendglocke m\u00fcder Klang.", "tokens": ["Schon", "der", "A\u00b7bend\u00b7glo\u00b7cke", "m\u00fc\u00b7der", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Auf den Blumen, die sich dir verneigt,", "tokens": ["Auf", "den", "Blu\u00b7men", ",", "die", "sich", "dir", "ver\u00b7neigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PRF", "PPER", "VVPP", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Schlief das letzte L\u00fcftchen ein und schweigt,", "tokens": ["Schlief", "das", "letz\u00b7te", "L\u00fcft\u00b7chen", "ein", "und", "schweigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sagen darf ich dir, wir sind allein,", "tokens": ["Sa\u00b7gen", "darf", "ich", "dir", ",", "wir", "sind", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,", "PPER", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df mein Herz ist ewig, ewig dein!", "tokens": ["Da\u00df", "mein", "Herz", "ist", "e\u00b7wig", ",", "e\u00b7wig", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "ADJD", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Horch, wie still es wird im dunkeln Hain,", "tokens": ["Horch", ",", "wie", "still", "es", "wird", "im", "dun\u00b7keln", "Hain", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "M\u00e4dchen, wir sind sicher und allein.", "tokens": ["M\u00e4d\u00b7chen", ",", "wir", "sind", "si\u00b7cher", "und", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1