{"dta.poem.1698": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Umwechslung aller Dinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sommer endet sich/ die warmen Tag entfliehen/", "tokens": ["Der", "Som\u00b7mer", "en\u00b7det", "sich", "/", "die", "war\u00b7men", "Tag", "ent\u00b7flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Fr\u00fccht und Blumen sieht man fallen und verbl\u00fchen;", "tokens": ["Die", "Fr\u00fccht", "und", "Blu\u00b7men", "sieht", "man", "fal\u00b7len", "und", "ver\u00b7bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "PIS", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ier aber findet sich ein neuer Sommer ein;", "tokens": ["ier", "a\u00b7ber", "fin\u00b7det", "sich", "ein", "neu\u00b7er", "Som\u00b7mer", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "er neu-gebohrne Sohn kan dessen Zeuge seyn.", "tokens": ["er", "neu\u00b7ge\u00b7bohr\u00b7ne", "Sohn", "kan", "des\u00b7sen", "Zeu\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "PDS", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "ein treuer Wunsch bekleibt: der Sommer wird gerathen/", "tokens": ["ein", "treu\u00b7er", "Wunsch", "be\u00b7kleibt", ":", "der", "Som\u00b7mer", "wird", "ge\u00b7ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "er lange Tag blickt vor durch seiner Kindheit Schaten/", "tokens": ["er", "lan\u00b7ge", "Tag", "blickt", "vor", "durch", "sei\u00b7ner", "Kind\u00b7heit", "Scha\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "VVFIN", "APPR", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "urch Himmels-Glantz bestrahlt/ gepflegt durch gutte Zucht/", "tokens": ["urch", "Him\u00b7mels\u00b7Glantz", "be\u00b7strahlt", "/", "ge\u00b7pflegt", "durch", "gut\u00b7te", "Zucht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "VVPP", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "w\u00e4chst/ ger\u00e4th und reifft die zarte Sommer-Frucht/", "tokens": ["w\u00e4chst", "/", "ge\u00b7r\u00e4\u00b7th", "und", "reifft", "die", "zar\u00b7te", "Som\u00b7mer\u00b7Frucht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJD", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.9": {"text": "ie wird des Sommers Art mit wohlgediegnen Zweigen/", "tokens": ["ie", "wird", "des", "Som\u00b7mers", "Art", "mit", "wohl\u00b7ge\u00b7dieg\u00b7nen", "Zwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "it Bl\u00fctte voll Geruch/ mit Frucht voll Safftes zeigen/", "tokens": ["it", "Bl\u00fct\u00b7te", "voll", "Ge\u00b7ruch", "/", "mit", "Frucht", "voll", "Saff\u00b7tes", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "NN", "$(", "APPR", "NN", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "r Eltern Sommer-Lust wird seyn di\u00df Sommer-Kind:", "tokens": ["r", "El\u00b7tern", "Som\u00b7mer\u00b7Lust", "wird", "seyn", "di\u00df", "Som\u00b7mer\u00b7Kind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VAFIN", "PPOSAT", "PDS", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "enn sich des Haubtes Schnee/ der Jahre Winter find.", "tokens": ["enn", "sich", "des", "Haub\u00b7tes", "Schnee", "/", "der", "Jah\u00b7re", "Win\u00b7ter", "find", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wird sie zuweilen gleich der Sonnen Hitze dr\u00fccken/", "tokens": ["Wird", "sie", "zu\u00b7wei\u00b7len", "gleich", "der", "Son\u00b7nen", "Hit\u00b7ze", "dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mu\u00df sie zu seiner Zeit die Hand des G\u00e4rtners pfl\u00fccken/", "tokens": ["Mu\u00df", "sie", "zu", "sei\u00b7ner", "Zeit", "die", "Hand", "des", "G\u00e4rt\u00b7ners", "pfl\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So wird sie doch dadurch nur an den Ort gebracht", "tokens": ["So", "wird", "sie", "doch", "da\u00b7durch", "nur", "an", "den", "Ort", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "ADV", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wo sie kein Reiff verlezt/ und steter Sommer lacht.", "tokens": ["Wo", "sie", "kein", "Reiff", "ver\u00b7lezt", "/", "und", "ste\u00b7ter", "Som\u00b7mer", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "NN", "VVPP", "$(", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}