{"dta.poem.21325": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie f\u00fchlt sich frey und schwingt sich empor", "tokens": ["Sie", "f\u00fchlt", "sich", "frey", "und", "schwingt", "sich", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "VVFIN", "PRF", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu den h\u00f6chsten Himmelsr\u00e4umen.", "tokens": ["Zu", "den", "h\u00f6chs\u00b7ten", "Him\u00b7mels\u00b7r\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "O deutsche Seele, wie stolz ist dein Flug", "tokens": ["O", "deut\u00b7sche", "See\u00b7le", ",", "wie", "stolz", "ist", "dein", "Flug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In deinen n\u00e4chtlichen Tr\u00e4umen!", "tokens": ["In", "dei\u00b7nen", "n\u00e4cht\u00b7li\u00b7chen", "Tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die G\u00f6tter erbleichen wenn du nah\u2019st!", "tokens": ["Die", "G\u00f6t\u00b7ter", "er\u00b7blei\u00b7chen", "wenn", "du", "nah'st", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du hast auf deinen Wegen", "tokens": ["Du", "hast", "auf", "dei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gar manches Sternlein ausgeputzt", "tokens": ["Gar", "man\u00b7ches", "Stern\u00b7lein", "aus\u00b7ge\u00b7putzt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinen Fl\u00fcgelschl\u00e4gen!", "tokens": ["Mit", "dei\u00b7nen", "Fl\u00fc\u00b7gel\u00b7schl\u00e4\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Franzosen und Russen geh\u00f6rt das Land,", "tokens": ["Fran\u00b7zo\u00b7sen", "und", "Rus\u00b7sen", "ge\u00b7h\u00f6rt", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das Meer geh\u00f6rt den Britten,", "tokens": ["Das", "Meer", "ge\u00b7h\u00f6rt", "den", "Brit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir aber besitzen im Luftreich\u2019 des Traums", "tokens": ["Wir", "a\u00b7ber", "be\u00b7sit\u00b7zen", "im", "Luftreich'", "des", "Traums"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Herrschaft unbestritten.", "tokens": ["Die", "Herr\u00b7schaft", "un\u00b7be\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hier \u00fcben wir die Hegemonie,", "tokens": ["Hier", "\u00fc\u00b7ben", "wir", "die", "He\u00b7ge\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hier sind wir unzerst\u00fcckelt;", "tokens": ["Hier", "sind", "wir", "un\u00b7zer\u00b7st\u00fc\u00b7ckelt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die andern V\u00f6lker haben sich", "tokens": ["Die", "an\u00b7dern", "V\u00f6l\u00b7ker", "ha\u00b7ben", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf platter Erde entwickelt. \u2013 \u2013", "tokens": ["Auf", "plat\u00b7ter", "Er\u00b7de", "ent\u00b7wi\u00b7ckelt", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$.", "$(", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}