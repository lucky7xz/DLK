{"textgrid.poem.34356": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie die Lebensflamme brennt!", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie die Lebensflamme brennt!", "tokens": ["Wie", "die", "Le\u00b7bens\u00b7flam\u00b7me", "brennt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott du hast sie angez\u00fcndet,", "tokens": ["Gott", "du", "hast", "sie", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach und deine Liebe g\u00f6nnt", "tokens": ["Ach", "und", "dei\u00b7ne", "Lie\u00b7be", "g\u00f6nnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir das Gl\u00fcck, das sie empfindet.", "tokens": ["Mir", "das", "Gl\u00fcck", ",", "das", "sie", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber brenn' ich ewig nur?", "tokens": ["A\u00b7ber", "brenn'", "ich", "e\u00b7wig", "nur", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott du siehst den Wunsch der Seele!", "tokens": ["Gott", "du", "siehst", "den", "Wunsch", "der", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brenn' ich ewig, ewig nur,", "tokens": ["Brenn'", "ich", "e\u00b7wig", ",", "e\u00b7wig", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$,", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich andre w\u00e4rm', mich qu\u00e4le?", "tokens": ["Da\u00df", "ich", "and\u00b7re", "w\u00e4rm'", ",", "mich", "qu\u00e4\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach wo brennt sie, himmlisch sch\u00f6n,", "tokens": ["Ach", "wo", "brennt", "sie", ",", "himm\u00b7lisch", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VVFIN", "PPER", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir wird in meinem Leben", "tokens": ["Die", "mir", "wird", "in", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was das Gl\u00fcck sey, zu verstehn,", "tokens": ["Was", "das", "Gl\u00fcck", "sey", ",", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was du seyst zu kosten geben!", "tokens": ["Was", "du", "seyst", "zu", "kos\u00b7ten", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Bi\u00df dahin ist all mein Thun", "tokens": ["Bi\u00df", "da\u00b7hin", "ist", "all", "mein", "Thun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PAV", "VAFIN", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein Geweb von Peinigungen,", "tokens": ["Ein", "Ge\u00b7web", "von", "Pei\u00b7ni\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "All mein Gl\u00fck ein taubes Ruhn,", "tokens": ["All", "mein", "Gl\u00fck", "ein", "tau\u00b7bes", "Ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Lust, mein Dank erzwungen.", "tokens": ["Mei\u00b7ne", "Lust", ",", "mein", "Dank", "er\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du erkennst mein Innerstes,", "tokens": ["Du", "er\u00b7kennst", "mein", "In\u00b7ners\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dieses Herzens heftig Schlagen,", "tokens": ["Die\u00b7ses", "Her\u00b7zens", "hef\u00b7tig", "Schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich erstike seine Klagen,", "tokens": ["Ich", "ers\u00b7ti\u00b7ke", "sei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Aber Gott, du kennest es.", "tokens": ["A\u00b7ber", "Gott", ",", "du", "ken\u00b7nest", "es", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Es ist wahr, ich schmekte schon", "tokens": ["Es", "ist", "wahr", ",", "ich", "schmek\u00b7te", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Augenblike voll Entz\u00fccken,", "tokens": ["Au\u00b7gen\u00b7bli\u00b7ke", "voll", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Gott! \u2013 in Augenbliken", "tokens": ["A\u00b7ber", "Gott", "!", "\u2013", "in", "Au\u00b7gen\u00b7bli\u00b7ken"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "NN", "$.", "$(", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht denn da dein ganzer Lohn?", "tokens": ["Steht", "denn", "da", "dein", "gan\u00b7zer", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Funken waren das von Freuden,", "tokens": ["Fun\u00b7ken", "wa\u00b7ren", "das", "von", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "V\u00f6gel die verk\u00fcndten Land,", "tokens": ["V\u00f6\u00b7gel", "die", "ver\u00b7k\u00fcnd\u00b7ten", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Seele ihrer Leiden", "tokens": ["Wenn", "die", "See\u00b7le", "ih\u00b7rer", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6h und Tief nicht mehr verstand.", "tokens": ["H\u00f6h", "und", "Tief", "nicht", "mehr", "ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Aber g\u00e4b es keine Flammen", "tokens": ["A\u00b7ber", "g\u00e4b", "es", "kei\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und betr\u00f6g uns denn dein Wort,", "tokens": ["Und", "be\u00b7tr\u00f6g", "uns", "denn", "dein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sucht' uns gleich der klugen Ammen", "tokens": ["Sucht'", "uns", "gleich", "der", "klu\u00b7gen", "Am\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einzuschl\u00e4fern fort und fort?", "tokens": ["Ein\u00b7zu\u00b7schl\u00e4\u00b7fern", "fort", "und", "fort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nein ich schreye \u2013 Vater! Retter!", "tokens": ["Nein", "ich", "schre\u00b7ye", "\u2013", "Va\u00b7ter", "!", "Ret\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$(", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Herz will ausgef\u00fcllt,", "tokens": ["Die\u00b7ses", "Herz", "will", "aus\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ges\u00e4ttigt seyn; zerschmetter", "tokens": ["Will", "ge\u00b7s\u00e4t\u00b7tigt", "seyn", ";", "zer\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VMFIN", "VVPP", "VAINF", "$.", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber sonst dein Ebenbild!", "tokens": ["Lie\u00b7ber", "sonst", "dein", "E\u00b7ben\u00b7bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Soll ich ewig harren, streben,", "tokens": ["Soll", "ich", "e\u00b7wig", "har\u00b7ren", ",", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoffen und vertraun in Wind?", "tokens": ["Hof\u00b7fen", "und", "ver\u00b7traun", "in", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein ich la\u00df dich nicht, mein Leben,", "tokens": ["Nein", "ich", "la\u00df", "dich", "nicht", ",", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVIMP", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du beseeligst denn dein Kind!", "tokens": ["Du", "be\u00b7see\u00b7ligst", "denn", "dein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie die Lebensflamme brennt!", "tokens": ["Wie", "die", "Le\u00b7bens\u00b7flam\u00b7me", "brennt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott du hast sie angez\u00fcndet,", "tokens": ["Gott", "du", "hast", "sie", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach und deine Liebe g\u00f6nnt", "tokens": ["Ach", "und", "dei\u00b7ne", "Lie\u00b7be", "g\u00f6nnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir das Gl\u00fcck, das sie empfindet.", "tokens": ["Mir", "das", "Gl\u00fcck", ",", "das", "sie", "emp\u00b7fin\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Aber brenn' ich ewig nur?", "tokens": ["A\u00b7ber", "brenn'", "ich", "e\u00b7wig", "nur", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott du siehst den Wunsch der Seele!", "tokens": ["Gott", "du", "siehst", "den", "Wunsch", "der", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Brenn' ich ewig, ewig nur,", "tokens": ["Brenn'", "ich", "e\u00b7wig", ",", "e\u00b7wig", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "$,", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich andre w\u00e4rm', mich qu\u00e4le?", "tokens": ["Da\u00df", "ich", "and\u00b7re", "w\u00e4rm'", ",", "mich", "qu\u00e4\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Ach wo brennt sie, himmlisch sch\u00f6n,", "tokens": ["Ach", "wo", "brennt", "sie", ",", "himm\u00b7lisch", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PWAV", "VVFIN", "PPER", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir wird in meinem Leben", "tokens": ["Die", "mir", "wird", "in", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was das Gl\u00fcck sey, zu verstehn,", "tokens": ["Was", "das", "Gl\u00fcck", "sey", ",", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was du seyst zu kosten geben!", "tokens": ["Was", "du", "seyst", "zu", "kos\u00b7ten", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "PTKZU", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Bi\u00df dahin ist all mein Thun", "tokens": ["Bi\u00df", "da\u00b7hin", "ist", "all", "mein", "Thun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PAV", "VAFIN", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein Geweb von Peinigungen,", "tokens": ["Ein", "Ge\u00b7web", "von", "Pei\u00b7ni\u00b7gun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "All mein Gl\u00fck ein taubes Ruhn,", "tokens": ["All", "mein", "Gl\u00fck", "ein", "tau\u00b7bes", "Ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Lust, mein Dank erzwungen.", "tokens": ["Mei\u00b7ne", "Lust", ",", "mein", "Dank", "er\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Du erkennst mein Innerstes,", "tokens": ["Du", "er\u00b7kennst", "mein", "In\u00b7ners\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dieses Herzens heftig Schlagen,", "tokens": ["Die\u00b7ses", "Her\u00b7zens", "hef\u00b7tig", "Schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich erstike seine Klagen,", "tokens": ["Ich", "ers\u00b7ti\u00b7ke", "sei\u00b7ne", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Aber Gott, du kennest es.", "tokens": ["A\u00b7ber", "Gott", ",", "du", "ken\u00b7nest", "es", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Es ist wahr, ich schmekte schon", "tokens": ["Es", "ist", "wahr", ",", "ich", "schmek\u00b7te", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Augenblike voll Entz\u00fccken,", "tokens": ["Au\u00b7gen\u00b7bli\u00b7ke", "voll", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber Gott! \u2013 in Augenbliken", "tokens": ["A\u00b7ber", "Gott", "!", "\u2013", "in", "Au\u00b7gen\u00b7bli\u00b7ken"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "NN", "$.", "$(", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht denn da dein ganzer Lohn?", "tokens": ["Steht", "denn", "da", "dein", "gan\u00b7zer", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Funken waren das von Freuden,", "tokens": ["Fun\u00b7ken", "wa\u00b7ren", "das", "von", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "V\u00f6gel die verk\u00fcndten Land,", "tokens": ["V\u00f6\u00b7gel", "die", "ver\u00b7k\u00fcnd\u00b7ten", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Seele ihrer Leiden", "tokens": ["Wenn", "die", "See\u00b7le", "ih\u00b7rer", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6h und Tief nicht mehr verstand.", "tokens": ["H\u00f6h", "und", "Tief", "nicht", "mehr", "ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Aber g\u00e4b es keine Flammen", "tokens": ["A\u00b7ber", "g\u00e4b", "es", "kei\u00b7ne", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und betr\u00f6g uns denn dein Wort,", "tokens": ["Und", "be\u00b7tr\u00f6g", "uns", "denn", "dein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sucht' uns gleich der klugen Ammen", "tokens": ["Sucht'", "uns", "gleich", "der", "klu\u00b7gen", "Am\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einzuschl\u00e4fern fort und fort?", "tokens": ["Ein\u00b7zu\u00b7schl\u00e4\u00b7fern", "fort", "und", "fort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Nein ich schreye \u2013 Vater! Retter!", "tokens": ["Nein", "ich", "schre\u00b7ye", "\u2013", "Va\u00b7ter", "!", "Ret\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "$(", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Herz will ausgef\u00fcllt,", "tokens": ["Die\u00b7ses", "Herz", "will", "aus\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ges\u00e4ttigt seyn; zerschmetter", "tokens": ["Will", "ge\u00b7s\u00e4t\u00b7tigt", "seyn", ";", "zer\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VMFIN", "VVPP", "VAINF", "$.", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber sonst dein Ebenbild!", "tokens": ["Lie\u00b7ber", "sonst", "dein", "E\u00b7ben\u00b7bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Soll ich ewig harren, streben,", "tokens": ["Soll", "ich", "e\u00b7wig", "har\u00b7ren", ",", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoffen und vertraun in Wind?", "tokens": ["Hof\u00b7fen", "und", "ver\u00b7traun", "in", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein ich la\u00df dich nicht, mein Leben,", "tokens": ["Nein", "ich", "la\u00df", "dich", "nicht", ",", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVIMP", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du beseeligst denn dein Kind!", "tokens": ["Du", "be\u00b7see\u00b7ligst", "denn", "dein", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}