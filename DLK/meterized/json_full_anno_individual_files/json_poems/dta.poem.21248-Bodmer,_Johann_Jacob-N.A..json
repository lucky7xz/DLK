{"dta.poem.21248": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198486", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Maler scheinet euch vergeblich liebzukosen,", "tokens": ["Ihr", "Ma\u00b7ler", "schei\u00b7net", "euch", "ver\u00b7geb\u00b7lich", "lieb\u00b7zu\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihr euch an der Pracht der gr\u00f6sten Dichter wag\u2019t,", "tokens": ["Wenn", "ihr", "euch", "an", "der", "Pracht", "der", "gr\u00f6s\u00b7ten", "Dich\u00b7ter", "wag't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dennoch wenig Teutsch in teutschen Zetteln sagt.", "tokens": ["Und", "den\u00b7noch", "we\u00b7nig", "Teutsch", "in", "teut\u00b7schen", "Zet\u00b7teln", "sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn eure Schreibart hat Neapler und Franzosen.", "tokens": ["Denn", "eu\u00b7re", "Schrei\u00b7bart", "hat", "Neap\u00b7ler", "und", "Fran\u00b7zo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NE", "KON", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Soll euer Werck gemalt, und nicht geschmieret seyn;", "tokens": ["Soll", "eu\u00b7er", "Werck", "ge\u00b7malt", ",", "und", "nicht", "ge\u00b7schmie\u00b7ret", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVPP", "$,", "KON", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So tunckt den Pinsel nicht mehr in den Nacht-Stul ein.", "tokens": ["So", "tunckt", "den", "Pin\u00b7sel", "nicht", "mehr", "in", "den", "Nacht\u00b7Stul", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Horn.", "tokens": ["Horn", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}}}}