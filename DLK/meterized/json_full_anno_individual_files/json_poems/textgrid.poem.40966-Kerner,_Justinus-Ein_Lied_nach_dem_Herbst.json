{"textgrid.poem.40966": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Ein Lied nach dem Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O weh! ihr Rebenh\u00fcgel!", "tokens": ["O", "weh", "!", "ihr", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie steht ihr trauernd nun!", "tokens": ["Wie", "steht", "ihr", "trau\u00b7ernd", "nun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sturm schwingt seine Fl\u00fcgel", "tokens": ["Der", "Sturm", "schwingt", "sei\u00b7ne", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und die Ges\u00e4nge ruhn.", "tokens": ["Und", "die", "Ge\u00b7s\u00e4n\u00b7ge", "ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es zog mit eurem Weine", "tokens": ["Es", "zog", "mit", "eu\u00b7rem", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus euch der Jubel aus;", "tokens": ["Aus", "euch", "der", "Ju\u00b7bel", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df er mit ihm erscheine", "tokens": ["Da\u00df", "er", "mit", "ihm", "er\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Neu in des Trinkers Haus.", "tokens": ["Neu", "in", "des", "Trin\u00b7kers", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "La\u00dft euer Herz erwarmen,", "tokens": ["La\u00dft", "eu\u00b7er", "Herz", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr nun schl\u00fcrft den Wein,", "tokens": ["Die", "ihr", "nun", "schl\u00fcrft", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trinkt ihn auch zu dem Armen,", "tokens": ["Trinkt", "ihn", "auch", "zu", "dem", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der ihn geschenket ein!", "tokens": ["Der", "ihn", "ge\u00b7schen\u00b7ket", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der immer tr\u00e4gt und haut,", "tokens": ["Der", "im\u00b7mer", "tr\u00e4gt", "und", "haut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den harten Stein bebaut.", "tokens": ["Den", "har\u00b7ten", "Stein", "be\u00b7baut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie in des Berges Tiefen", "tokens": ["Wie", "in", "des", "Ber\u00b7ges", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rastlos der Bergmann schafft,", "tokens": ["Rast\u00b7los", "der", "Berg\u00b7mann", "schafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Sch\u00e4tze, die da schliefen,", "tokens": ["Die", "Sch\u00e4t\u00b7ze", ",", "die", "da", "schlie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erhebt mit reger Kraft,", "tokens": ["Er\u00b7hebt", "mit", "re\u00b7ger", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "An H\u00e4nden tr\u00e4gt nur Narben,", "tokens": ["An", "H\u00e4n\u00b7den", "tr\u00e4gt", "nur", "Nar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Herr den Edelstein:", "tokens": ["Der", "Herr", "den", "E\u00b7del\u00b7stein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mu\u00df auch der oben darben,", "tokens": ["Mu\u00df", "auch", "der", "o\u00b7ben", "dar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADV", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Trinkt Wasser, ihr den Wein.", "tokens": ["Trinkt", "Was\u00b7ser", ",", "ihr", "den", "Wein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wie der unten nimmer,", "tokens": ["Und", "wie", "der", "un\u00b7ten", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stirbt auch die Hoffnung, ruht,", "tokens": ["Stirbt", "auch", "die", "Hoff\u00b7nung", ",", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So w\u00e4chst beim letzten Schimmer", "tokens": ["So", "w\u00e4chst", "beim", "letz\u00b7ten", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem oben noch der Mut.", "tokens": ["Dem", "o\u00b7ben", "noch", "der", "Mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schl\u00e4gt schwerer Hagel nieder,", "tokens": ["Schl\u00e4gt", "schwe\u00b7rer", "Ha\u00b7gel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was er durchs Jahr erschafft,", "tokens": ["Was", "er", "durchs", "Jahr", "er\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er geht neuhoffend wieder", "tokens": ["Er", "geht", "neu\u00b7ho\u00b7ffend", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ans Werk mit gleicher Kraft.", "tokens": ["Ans", "Werk", "mit", "glei\u00b7cher", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wie in seinem Grabe", "tokens": ["Und", "wie", "in", "sei\u00b7nem", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der unten immer weilt,", "tokens": ["Der", "un\u00b7ten", "im\u00b7mer", "weilt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Greis wie einst als Knabe", "tokens": ["Als", "Greis", "wie", "einst", "als", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KOKOM", "ADV", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu seinen Steinen eilt,", "tokens": ["Zu", "sei\u00b7nen", "Stei\u00b7nen", "eilt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So bleibt bei seinen Reben", "tokens": ["So", "bleibt", "bei", "sei\u00b7nen", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Als Knabe und als Greis", "tokens": ["Als", "Kna\u00b7be", "und", "als", "Greis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "KOUS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der oben \u2013 treu ergeben", "tokens": ["Der", "o\u00b7ben", "\u2013", "treu", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "$(", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Armut und dem Flei\u00df.", "tokens": ["Der", "Ar\u00b7mut", "und", "dem", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er schafft vom ersten Scheine", "tokens": ["Er", "schafft", "vom", "ers\u00b7ten", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sonne bis zur Nacht,", "tokens": ["Der", "Son\u00b7ne", "bis", "zur", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trinkt dann im Schlaf vom Weine,", "tokens": ["Trinkt", "dann", "im", "Schlaf", "vom", "Wei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Den ihm sein Berg gebracht \u2013", "tokens": ["Den", "ihm", "sein", "Berg", "ge\u00b7bracht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und l\u00e4\u00dft, erwacht zur Wahrheit,", "tokens": ["Und", "l\u00e4\u00dft", ",", "er\u00b7wacht", "zur", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den lang ersehnten Wein", "tokens": ["Den", "lang", "er\u00b7sehn\u00b7ten", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In seiner Gottesklarheit", "tokens": ["In", "sei\u00b7ner", "Got\u00b7tes\u00b7klar\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem reichen Trinker sein.", "tokens": ["Dem", "rei\u00b7chen", "Trin\u00b7ker", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er aber, mit der Flasche", "tokens": ["Er", "a\u00b7ber", ",", "mit", "der", "Fla\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll Wasser, geht in Ruh',", "tokens": ["Voll", "Was\u00b7ser", ",", "geht", "in", "Ruh'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Brot in seiner Tasche,", "tokens": ["Ein", "Brot", "in", "sei\u00b7ner", "Ta\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und deckt die Reben zu.", "tokens": ["Und", "deckt", "die", "Re\u00b7ben", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Einst deckt auch ihn, den Armen,", "tokens": ["Einst", "deckt", "auch", "ihn", ",", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der lang geschafft, gewacht,", "tokens": ["Der", "lang", "ge\u00b7schafft", ",", "ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Engel voll Erbarmen", "tokens": ["Ein", "En\u00b7gel", "voll", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und fl\u00fcstert: Gute Nacht!", "tokens": ["Und", "fl\u00fcs\u00b7tert", ":", "Gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O weh! ihr Rebenh\u00fcgel!", "tokens": ["O", "weh", "!", "ihr", "Re\u00b7ben\u00b7h\u00fc\u00b7gel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie steht ihr trauernd nun!", "tokens": ["Wie", "steht", "ihr", "trau\u00b7ernd", "nun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sturm sch