{"dta.poem.7141": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verstimmet einst ein Instrument,", "tokens": ["Ver\u00b7stim\u00b7met", "einst", "ein", "Inst\u00b7ru\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versucht sodenn, ob ihr die T\u00f6ne,", "tokens": ["Ver\u00b7sucht", "so\u00b7denn", ",", "ob", "ihr", "die", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die sonst so lieblich, rein und sch\u00f6ne,", "tokens": ["Die", "sonst", "so", "lieb\u00b7lich", ",", "rein", "und", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus selben zwingen k\u00f6nnt?", "tokens": ["Aus", "sel\u00b7ben", "zwin\u00b7gen", "k\u00f6nnt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jhr werdet nichts aus ihm, als Dissonantzen bringen,", "tokens": ["Ihr", "wer\u00b7det", "nichts", "aus", "ihm", ",", "als", "Dis\u00b7so\u00b7nant\u00b7zen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "PPER", "$,", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die wunderlich und elend klingen.", "tokens": ["Die", "wun\u00b7der\u00b7lich", "und", "e\u00b7lend", "klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So, wenn der Leib verstimmt, die Nerven nachgelassen,", "tokens": ["So", ",", "wenn", "der", "Leib", "ver\u00b7stimmt", ",", "die", "Ner\u00b7ven", "nach\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie k\u00f6nnen sie sodann den Ton behalten,", "tokens": ["Wie", "k\u00f6n\u00b7nen", "sie", "so\u00b7dann", "den", "Ton", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie kann denn das Gehirn was anders fassen,", "tokens": ["Wie", "kann", "denn", "das", "Ge\u00b7hirn", "was", "an\u00b7ders", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "PWS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Als ungeformt\u2019 und seltsame Gestalten,", "tokens": ["Als", "un\u00b7ge\u00b7formt'", "und", "selt\u00b7sa\u00b7me", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die sonder Harmonie, und nicht zusammen hangen.", "tokens": ["Die", "son\u00b7der", "Har\u00b7mo\u00b7nie", ",", "und", "nicht", "zu\u00b7sam\u00b7men", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}