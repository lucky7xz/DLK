{"dta.poem.11474": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das absterben tit. Herrn Johann  \n Christoph Lucas  Ratisbonensis, J. U. Licen-  \n tiati,  im nahmen Jhro  Exc.  des Herrn  \n Geh. Rath Strycks s\u00e4m\u0303tl.  \n tisch-compagnie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Solft du, wohl-edler Freund! schon in dem sarge liegen?", "tokens": ["Solft", "du", ",", "wohl\u00b7ed\u00b7ler", "Freund", "!", "schon", "in", "dem", "sar\u00b7ge", "lie\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "ADJA", "NN", "$.", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Must du so unverhofft ein raub des todes seyn?", "tokens": ["Must", "du", "so", "un\u00b7ver\u00b7hofft", "ein", "raub", "des", "to\u00b7des", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "ART", "NN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kan witz und jugend nicht die sterblichkeit besiegen?", "tokens": ["Kan", "witz", "und", "ju\u00b7gend", "nicht", "die", "sterb\u00b7lich\u00b7keit", "be\u00b7sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und st\u00fcrmt der rauhe nord auch in die lorbern ein?", "tokens": ["Und", "st\u00fcrmt", "der", "rau\u00b7he", "nord", "auch", "in", "die", "lor\u00b7bern", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir wundern uns zwar nicht, da\u00df in den sommer-tagen,", "tokens": ["Wir", "wun\u00b7dern", "uns", "zwar", "nicht", ",", "da\u00df", "in", "den", "som\u00b7mer\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Indem der sonne gluth die halbe welt erhitzt,", "tokens": ["In\u00b7dem", "der", "son\u00b7ne", "gluth", "die", "hal\u00b7be", "welt", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die harten donner-keil in b\u00e4um und h\u00e4user schlagen,", "tokens": ["Die", "har\u00b7ten", "don\u00b7ner\u00b7keil", "in", "b\u00e4um", "und", "h\u00e4u\u00b7ser", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wenn ein eichen-stamm nicht allzusicher sitzt;", "tokens": ["Und", "wenn", "ein", "ei\u00b7chen\u00b7stamm", "nicht", "all\u00b7zu\u00b7si\u00b7cher", "sitzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da\u00df aber itzt dein haupt des todes blitz empfunden,", "tokens": ["Da\u00df", "a\u00b7ber", "itzt", "dein", "haupt", "des", "to\u00b7des", "blitz", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nachdem ihn Themis n\u00e4chst der lorbern werth gesch\u00e4tzt:", "tokens": ["Nach\u00b7dem", "ihn", "The\u00b7mis", "n\u00e4chst", "der", "lor\u00b7bern", "werth", "ge\u00b7sch\u00e4tzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "VVFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das sind uns allerdings gantz ungemeine wunden,", "tokens": ["Das", "sind", "uns", "al\u00b7ler\u00b7dings", "gantz", "un\u00b7ge\u00b7mei\u00b7ne", "wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vor denen witz und muth sich in verwundrung setzt.", "tokens": ["Vor", "de\u00b7nen", "witz", "und", "muth", "sich", "in", "ver\u00b7wund\u00b7rung", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "KON", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Soll kein entbrannter schlag die frischen lorbern treffen?", "tokens": ["Soll", "kein", "ent\u00b7brann\u00b7ter", "schlag", "die", "fri\u00b7schen", "lor\u00b7bern", "tref\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ach! warum sch\u00fctzen sie Astr\u00e4cus priester nicht?", "tokens": ["Ach", "!", "wa\u00b7rum", "sch\u00fct\u00b7zen", "sie", "A\u00b7str\u00e4\u00b7cus", "pries\u00b7ter", "nicht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "NE", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es scheine: Der strenge tod will unsre klugheit \u00e4ffen,", "tokens": ["Es", "schei\u00b7ne", ":", "Der", "stren\u00b7ge", "tod", "will", "uns\u00b7re", "klug\u00b7heit", "\u00e4f\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Und k\u00fcmmert sich nicht viel, was ihre regel spricht.", "tokens": ["Und", "k\u00fcm\u00b7mert", "sich", "nicht", "viel", ",", "was", "ih\u00b7re", "re\u00b7gel", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wir gr\u00fcnden uns umsonst auf alte s\u00e4tz und b\u00fccher.", "tokens": ["Wir", "gr\u00fcn\u00b7den", "uns", "um\u00b7sonst", "auf", "al\u00b7te", "s\u00e4tz", "und", "b\u00fc\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Er h\u00f6rt nicht, was der mensch hier vor gerichte hegt;", "tokens": ["Er", "h\u00f6rt", "nicht", ",", "was", "der", "mensch", "hier", "vor", "ge\u00b7rich\u00b7te", "hegt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ART", "NN", "ADV", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die lorbern bleiben schon vor keinem donner sicher,", "tokens": ["Die", "lor\u00b7bern", "blei\u00b7ben", "schon", "vor", "kei\u00b7nem", "don\u00b7ner", "si\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn sie gleich Ph\u00f6bus selbst auf seiner scheitel tr\u00e4gt.", "tokens": ["Wenn", "sie", "gleich", "Ph\u00f6\u00b7bus", "selbst", "auf", "sei\u00b7ner", "schei\u00b7tel", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du bist, erblaster Freund! ein allzuklar exempel,", "tokens": ["Du", "bist", ",", "er\u00b7blas\u00b7ter", "Freund", "!", "ein", "all\u00b7zu\u00b7klar", "ex\u00b7em\u00b7pel", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJA", "NN", "$.", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.22": {"text": "Das uns nicht ohne schmertz der wahrheit \u00fcberf\u00fchrt;", "tokens": ["Das", "uns", "nicht", "oh\u00b7ne", "schmertz", "der", "wahr\u00b7heit", "\u00fc\u00b7berf\u00b7\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dich sch\u00fctzt kein lorber-krantz, kein hoher ehren-tempel,", "tokens": ["Dich", "sch\u00fctzt", "kein", "lor\u00b7ber\u00b7krantz", ",", "kein", "ho\u00b7her", "eh\u00b7ren\u00b7tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Kein buch, kein g\u00f6ldner ring, mit dem dich Themis ziert.", "tokens": ["Kein", "buch", ",", "kein", "g\u00f6ld\u00b7ner", "ring", ",", "mit", "dem", "dich", "The\u00b7mis", "ziert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "ADJA", "VVFIN", "$,", "APPR", "PRELS", "PPER", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wir schauen deinen leib nur in die grufft versencken:", "tokens": ["Wir", "schau\u00b7en", "dei\u00b7nen", "leib", "nur", "in", "die", "grufft", "ver\u00b7sen\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dein allzufr\u00fcher tod pre\u00dft tausend thr\u00e4nen aus;", "tokens": ["Dein", "all\u00b7zu\u00b7fr\u00fc\u00b7her", "tod", "pre\u00dft", "tau\u00b7send", "thr\u00e4\u00b7nen", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Doch stirbt die tugend nicht: Dein r\u00fchmlich angedencken", "tokens": ["Doch", "stirbt", "die", "tu\u00b7gend", "nicht", ":", "Dein", "r\u00fchm\u00b7lich", "an\u00b7ge\u00b7den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$.", "PPOSAT", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Schwingt, als ein adler, sich bi\u00df an das sonnen-haus.", "tokens": ["Schwingt", ",", "als", "ein", "ad\u00b7ler", ",", "sich", "bi\u00df", "an", "das", "son\u00b7nen\u00b7haus", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "$,", "PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer so, wie du, gelebt, kan ohnedem nicht sterben:", "tokens": ["Wer", "so", ",", "wie", "du", ",", "ge\u00b7lebt", ",", "kan", "oh\u00b7ne\u00b7dem", "nicht", "ster\u00b7ben", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$,", "PWAV", "PPER", "$,", "VVPP", "$,", "VMFIN", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wer dich gekennet hat, kennt auch dein christenthum;", "tokens": ["Wer", "dich", "ge\u00b7ken\u00b7net", "hat", ",", "kennt", "auch", "dein", "chris\u00b7ten\u00b7thum", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Und wenn die lorbern gleich verdorren und verderben,", "tokens": ["Und", "wenn", "die", "lor\u00b7bern", "gleich", "ver\u00b7dor\u00b7ren", "und", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So bl\u00fcht und tauret doch dein wahrer ehren-ruhm.", "tokens": ["So", "bl\u00fcht", "und", "tau\u00b7ret", "doch", "dein", "wah\u00b7rer", "eh\u00b7ren\u00b7ruhm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Themis will dir selbst ein sch\u00f6nes denckmahl stifften,", "tokens": ["Die", "The\u00b7mis", "will", "dir", "selbst", "ein", "sch\u00f6\u00b7nes", "denck\u00b7mahl", "stiff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sie stellt uns deinen flei\u00df zu einem muster vor:", "tokens": ["Sie", "stellt", "uns", "dei\u00b7nen", "flei\u00df", "zu", "ei\u00b7nem", "mus\u00b7ter", "vor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Du sch\u00e4rfftest deinen witz in lesung kluger schrifften,", "tokens": ["Du", "sch\u00e4rff\u00b7test", "dei\u00b7nen", "witz", "in", "le\u00b7sung", "klu\u00b7ger", "schriff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn mancher seine zeit durch m\u00fc\u00dfiggang verlohr.", "tokens": ["Wenn", "man\u00b7cher", "sei\u00b7ne", "zeit", "durch", "m\u00fc\u00b7\u00dfig\u00b7gang", "ver\u00b7lohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "APPR", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und darum kontest du mit ehren lorbern tragen:", "tokens": ["Und", "da\u00b7rum", "kon\u00b7test", "du", "mit", "eh\u00b7ren", "lor\u00b7bern", "tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "APPR", "VVINF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Fridriciana cr\u00f6nt die idioten nicht;", "tokens": ["Frid\u00b7ri\u00b7ci\u00b7a\u00b7na", "cr\u00f6nt", "die", "i\u00b7dio\u00b7ten", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.39": {"text": "Man mu\u00df die barbarey erst aus dem felde schlagen,", "tokens": ["Man", "mu\u00df", "die", "bar\u00b7ba\u00b7rey", "erst", "aus", "dem", "fel\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Eh\u2019 man die cron erlangt, so die gelahrheit flicht.", "tokens": ["Eh'", "man", "die", "cron", "er\u00b7langt", ",", "so", "die", "ge\u00b7lahr\u00