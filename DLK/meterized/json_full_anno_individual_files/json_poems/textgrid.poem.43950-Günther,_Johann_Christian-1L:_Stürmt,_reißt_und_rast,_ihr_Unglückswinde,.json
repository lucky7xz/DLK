{"textgrid.poem.43950": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: St\u00fcrmt, rei\u00dft und rast, ihr Ungl\u00fcckswinde,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "St\u00fcrmt, rei\u00dft und rast, ihr Ungl\u00fcckswinde,", "tokens": ["St\u00fcrmt", ",", "rei\u00dft", "und", "rast", ",", "ihr", "Un\u00b7gl\u00fccks\u00b7win\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt eure ganze Tyranney!", "tokens": ["Zeigt", "eu\u00b7re", "gan\u00b7ze", "Ty\u00b7ran\u00b7ney", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdreht, zerschlizt so Zweig als Rinde", "tokens": ["Ver\u00b7dreht", ",", "zer\u00b7schlizt", "so", "Zweig", "als", "Rin\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und brecht den Hofnungsbaum entzwey!", "tokens": ["Und", "brecht", "den", "Hof\u00b7nungs\u00b7baum", "ent\u00b7zwey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies Hagelwetter", "tokens": ["Dies", "Ha\u00b7gel\u00b7wet\u00b7ter"], "token_info": ["word", "word"], "pos": ["PDS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Trift Stamm und Bl\u00e4tter,", "tokens": ["Trift", "Stamm", "und", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die Wurzel bleibt,", "tokens": ["Die", "Wur\u00b7zel", "bleibt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Bis Sturm und Regen", "tokens": ["Bis", "Sturm", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ihr W\u00fcten legen,", "tokens": ["Ihr", "W\u00fc\u00b7ten", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Da sie von neuem gr\u00fcnt und \u00c4ste treibt.", "tokens": ["Da", "sie", "von", "neu\u00b7em", "gr\u00fcnt", "und", "\u00c4s\u00b7te", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Herz giebt keinen Diamanten,", "tokens": ["Mein", "Herz", "giebt", "kei\u00b7nen", "Di\u00b7a\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Geist den Eichen wenig nach;", "tokens": ["Mein", "Geist", "den", "Ei\u00b7chen", "we\u00b7nig", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Erd und Himmel mich verbannten,", "tokens": ["Wenn", "Erd", "und", "Him\u00b7mel", "mich", "ver\u00b7bann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So troz ich doch mein Ungemach.", "tokens": ["So", "troz", "ich", "doch", "mein", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlagt, bittre Feinde,", "tokens": ["Schlagt", ",", "bitt\u00b7re", "Fein\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Weicht, falschen Freunde!", "tokens": ["Weicht", ",", "fal\u00b7schen", "Freun\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mein Heldenmuth", "tokens": ["Mein", "Hel\u00b7den\u00b7muth"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ist nicht zu d\u00e4mpfen,", "tokens": ["Ist", "nicht", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Drum will ich k\u00e4mpfen", "tokens": ["Drum", "will", "ich", "k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und sehn, was die Gedult vor Wunder thut.", "tokens": ["Und", "sehn", ",", "was", "die", "Ge\u00b7dult", "vor", "Wun\u00b7der", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Liebe schenckt aus g\u00f6ldnen Schaalen", "tokens": ["Die", "Lie\u00b7be", "schenckt", "aus", "g\u00f6ld\u00b7nen", "Schaa\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir einen Wein zur Tapferkeit,", "tokens": ["Mir", "ei\u00b7nen", "Wein", "zur", "Tap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie spricht, mir guten Sold zu zahlen,", "tokens": ["Sie", "spricht", ",", "mir", "gu\u00b7ten", "Sold", "zu", "zah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schickt mich in den Ungl\u00fccksstreit.", "tokens": ["Und", "schickt", "mich", "in", "den", "Un\u00b7gl\u00fccks\u00b7streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier will ich kriegen,", "tokens": ["Hier", "will", "ich", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Hier will ich siegen;", "tokens": ["Hier", "will", "ich", "sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ein gr\u00fcnes Feld", "tokens": ["Ein", "gr\u00fc\u00b7nes", "Feld"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Dient meinem Schilde", "tokens": ["Dient", "mei\u00b7nem", "Schil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Zum Wappenbilde,", "tokens": ["Zum", "Wap\u00b7pen\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Bey dem ein Palmenbaum zwey Ancker h\u00e4lt.", "tokens": ["Bey", "dem", "ein", "Pal\u00b7men\u00b7baum", "zwey", "An\u00b7cker", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "CARD", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Best\u00e4ndig lieb ich dich, mein Kind,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "lieb", "ich", "dich", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis dermahleinst die Dichter schreiben,", "tokens": ["Bis", "der\u00b7mah\u00b7le\u00b7inst", "die", "Dich\u00b7ter", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df du und ich nicht sterblich sind.", "tokens": ["Da\u00df", "du", "und", "ich", "nicht", "sterb\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wort Best\u00e4ndig", "tokens": ["Das", "Wort", "Be\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Macht alles b\u00e4ndig,", "tokens": ["Macht", "al\u00b7les", "b\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Was Elend heist;", "tokens": ["Was", "E\u00b7lend", "heist", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Das st\u00e4rckste Fieber", "tokens": ["Das", "st\u00e4rcks\u00b7te", "Fie\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Geht bald vor\u00fcber,", "tokens": ["Geht", "bald", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Wenn man nur mit Gedult den Frost verbei\u00dft.", "tokens": ["Wenn", "man", "nur", "mit", "Ge\u00b7dult", "den", "Frost", "ver\u00b7bei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.5": {"line.1": {"text": "Nur zweifle nicht an meiner Treue,", "tokens": ["Nur", "zweif\u00b7le", "nicht", "an", "mei\u00b7ner", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die als ein ewig helles Licht,", "tokens": ["Die", "als", "ein", "e\u00b7wig", "hel\u00b7les", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich des Lebens mich verzeihe,", "tokens": ["Wenn", "ich", "des", "Le\u00b7bens", "mich", "ver\u00b7zei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Finstern\u00fc\u00df der Gr\u00e4ber bricht.", "tokens": ["Die", "Fins\u00b7ter\u00b7n\u00fc\u00df", "der", "Gr\u00e4\u00b7ber", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein hartes Gl\u00fccke,", "tokens": ["Kein", "har\u00b7tes", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ja kein Geschicke", "tokens": ["Ja", "kein", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Trennt mich von dir;", "tokens": ["Trennt", "mich", "von", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Du stirbst die Meine,", "tokens": ["Du", "stirbst", "die", "Mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPOSAT", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ich bin der Deine,", "tokens": ["Ich", "bin", "der", "Dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PPOSAT", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Drum wirf den Argwohn weg und glaube mir!", "tokens": ["Drum", "wirf", "den", "Arg\u00b7wohn", "weg", "und", "glau\u00b7be", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "St\u00fcrmt, rei\u00dft und rast, ihr Ungl\u00fcckswinde,", "tokens": ["St\u00fcrmt", ",", "rei\u00dft", "und", "rast", ",", "ihr", "Un\u00b7gl\u00fccks\u00b7win\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zeigt eure ganze Tyranney!", "tokens": ["Zeigt", "eu\u00b7re", "gan\u00b7ze", "Ty\u00b7ran\u00b7ney", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verdreht, zerschlizt so Zweig als Rinde", "tokens": ["Ver\u00b7dreht", ",", "zer\u00b7schlizt", "so", "Zweig", "als", "Rin\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "NN", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und brecht den Hofnungsbaum entzwey!", "tokens": ["Und", "brecht", "den", "Hof\u00b7nungs\u00b7baum", "ent\u00b7zwey", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies Hagelwetter", "tokens": ["Dies", "Ha\u00b7gel\u00b7wet\u00b7ter"], "token_info": ["word", "word"], "pos": ["PDS", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Trift Stamm und Bl\u00e4tter,", "tokens": ["Trift", "Stamm", "und", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die Wurzel bleibt,", "tokens": ["Die", "Wur\u00b7zel", "bleibt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Bis Sturm und Regen", "tokens": ["Bis", "Sturm", "und", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ihr W\u00fcten legen,", "tokens": ["Ihr", "W\u00fc\u00b7ten", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Da sie von neuem gr\u00fcnt und \u00c4ste treibt.", "tokens": ["Da", "sie", "von", "neu\u00b7em", "gr\u00fcnt", "und", "\u00c4s\u00b7te", "treibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Mein Herz giebt keinen Diamanten,", "tokens": ["Mein", "Herz", "giebt", "kei\u00b7nen", "Di\u00b7a\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Geist den Eichen wenig nach;", "tokens": ["Mein", "Geist", "den", "Ei\u00b7chen", "we\u00b7nig", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn Erd und Himmel mich verbannten,", "tokens": ["Wenn", "Erd", "und", "Him\u00b7mel", "mich", "ver\u00b7bann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So troz ich doch mein Ungemach.", "tokens": ["So", "troz", "ich", "doch", "mein", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlagt, bittre Feinde,", "tokens": ["Schlagt", ",", "bitt\u00b7re", "Fein\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Weicht, falschen Freunde!", "tokens": ["Weicht", ",", "fal\u00b7schen", "Freun\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Mein Heldenmuth", "tokens": ["Mein", "Hel\u00b7den\u00b7muth"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Ist nicht zu d\u00e4mpfen,", "tokens": ["Ist", "nicht", "zu", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Drum will ich k\u00e4mpfen", "tokens": ["Drum", "will", "ich", "k\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und sehn, was die Gedult vor Wunder thut.", "tokens": ["Und", "sehn", ",", "was", "die", "Ge\u00b7dult", "vor", "Wun\u00b7der", "thut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die Liebe schenckt aus g\u00f6ldnen Schaalen", "tokens": ["Die", "Lie\u00b7be", "schenckt", "aus", "g\u00f6ld\u00b7nen", "Schaa\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir einen Wein zur Tapferkeit,", "tokens": ["Mir", "ei\u00b7nen", "Wein", "zur", "Tap\u00b7fer\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie spricht, mir guten Sold zu zahlen,", "tokens": ["Sie", "spricht", ",", "mir", "gu\u00b7ten", "Sold", "zu", "zah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schickt mich in den Ungl\u00fccksstreit.", "tokens": ["Und", "schickt", "mich", "in", "den", "Un\u00b7gl\u00fccks\u00b7streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hier will ich kriegen,", "tokens": ["Hier", "will", "ich", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Hier will ich siegen;", "tokens": ["Hier", "will", "ich", "sie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Ein gr\u00fcnes Feld", "tokens": ["Ein", "gr\u00fc\u00b7nes", "Feld"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Dient meinem Schilde", "tokens": ["Dient", "mei\u00b7nem", "Schil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Zum Wappenbilde,", "tokens": ["Zum", "Wap\u00b7pen\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Bey dem ein Palmenbaum zwey Ancker h\u00e4lt.", "tokens": ["Bey", "dem", "ein", "Pal\u00b7men\u00b7baum", "zwey", "An\u00b7cker", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "CARD", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Best\u00e4ndig lieb ich dich, mein Kind,", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "lieb", "ich", "dich", ",", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PPER", "PRF", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis dermahleinst die Dichter schreiben,", "tokens": ["Bis", "der\u00b7mah\u00b7le\u00b7inst", "die", "Dich\u00b7ter", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df du und ich nicht sterblich sind.", "tokens": ["Da\u00df", "du", "und", "ich", "nicht", "sterb\u00b7lich", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wort Best\u00e4ndig", "tokens": ["Das", "Wort", "Be\u00b7st\u00e4n\u00b7dig"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Macht alles b\u00e4ndig,", "tokens": ["Macht", "al\u00b7les", "b\u00e4n\u00b7dig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Was Elend heist;", "tokens": ["Was", "E\u00b7lend", "heist", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Das st\u00e4rckste Fieber", "tokens": ["Das", "st\u00e4rcks\u00b7te", "Fie\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Geht bald vor\u00fcber,", "tokens": ["Geht", "bald", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Wenn man nur mit Gedult den Frost verbei\u00dft.", "tokens": ["Wenn", "man", "nur", "mit", "Ge\u00b7dult", "den", "Frost", "ver\u00b7bei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "---+-+-+-+", "measure": "zehnsilber"}}, "stanza.10": {"line.1": {"text": "Nur zweifle nicht an meiner Treue,", "tokens": ["Nur", "zweif\u00b7le", "nicht", "an", "mei\u00b7ner", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die als ein ewig helles Licht,", "tokens": ["Die", "als", "ein", "e\u00b7wig", "hel\u00b7les", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich des Lebens mich verzeihe,", "tokens": ["Wenn", "ich", "des", "Le\u00b7bens", "mich", "ver\u00b7zei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Finstern\u00fc\u00df der Gr\u00e4ber bricht.", "tokens": ["Die", "Fins\u00b7ter\u00b7n\u00fc\u00df", "der", "Gr\u00e4\u00b7ber", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein hartes Gl\u00fccke,", "tokens": ["Kein", "har\u00b7tes", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ja kein Geschicke", "tokens": ["Ja", "kein", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Trennt mich von dir;", "tokens": ["Trennt", "mich", "von", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Du stirbst die Meine,", "tokens": ["Du", "stirbst", "die", "Mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PPOSAT", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ich bin der Deine,", "tokens": ["Ich", "bin", "der", "Dei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PPOSAT", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Drum wirf den Argwohn weg und glaube mir!", "tokens": ["Drum", "wirf", "den", "Arg\u00b7wohn", "weg", "und", "glau\u00b7be", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}