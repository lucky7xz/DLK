{"dta.poem.23298": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "Da\u00df Vierdte.  \n Der Gerechtfertigte S\u00fcnder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ich war de\u00df Teufels Selav/ un\u0303 gieng in seinen Bande\u0303:", "tokens": ["Ich", "war", "de\u00df", "Teu\u00b7fels", "Se\u00b7lav", "/", "u\u00f1", "gieng", "in", "sei\u00b7nen", "Ban\u00b7d\u1ebd", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "$(", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich war mit S\u00fcnden-Wust verstellt un\u0303 bluttig roth:", "tokens": ["Ich", "war", "mit", "S\u00fcn\u00b7den\u00b7Wust", "ver\u00b7stellt", "u\u00f1", "blut\u00b7tig", "roth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In Wollust weltzt\u2019 ich mich wie eine Sau im Koth:", "tokens": ["In", "Wol\u00b7lust", "weltzt'", "ich", "mich", "wie", "ei\u00b7ne", "Sau", "im", "Koth", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich stank f\u00fcr Eitelkeit die h\u00e4uffig war vorhanden:", "tokens": ["Ich", "stank", "f\u00fcr", "Ei\u00b7tel\u00b7keit", "die", "h\u00e4uf\u00b7fig", "war", "vor\u00b7han\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJD", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich war dem Abgrund nah/ un\u0303 sieng schon anzustran-", "tokens": ["Ich", "war", "dem", "Ab\u00b7grund", "nah", "/", "u\u00f1", "sieng", "schon", "an\u00b7zu\u00b7stran"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$(", "KON", "VVFIN", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "den:", "tokens": ["den", ":"], "token_info": ["word", "punct"], "pos": ["ART", "$."], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Ich lebte wie ein Vieh/ und fragte nicht nach GOtt/", "tokens": ["Ich", "leb\u00b7te", "wie", "ein", "Vieh", "/", "und", "frag\u00b7te", "nicht", "nach", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$(", "KON", "VVFIN", "PTKNEG", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich war ein Schatten Mensch/ un\u0303 noch lebendig Todt.", "tokens": ["Ich", "war", "ein", "Schat\u00b7ten", "Mensch", "/", "u\u00f1", "noch", "le\u00b7ben\u00b7dig", "Todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$(", "KON", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Nu bin ich widerumb in Christo auferstanden/", "tokens": ["Nu", "bin", "ich", "wi\u00b7de\u00b7rumb", "in", "Chris\u00b7to", "auf\u00b7er\u00b7stan\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und lebendig gemacht: die Ketten sind entzwey/", "tokens": ["Und", "le\u00b7ben\u00b7dig", "ge\u00b7macht", ":", "die", "Ket\u00b7ten", "sind", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$.", "ART", "NN", "VAFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Teufel ist verjagt/ und ich bin lo\u00df und frey.", "tokens": ["Der", "Teu\u00b7fel", "ist", "ver\u00b7jagt", "/", "und", "ich", "bin", "lo\u00df", "und", "frey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "KON", "PPER", "VAFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich suche GOtt allein mit eifrigem Gem\u00fctte/", "tokens": ["Ich", "su\u00b7che", "Gott", "al\u00b7lein", "mit", "eif\u00b7ri\u00b7gem", "Ge\u00b7m\u00fct\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und gebe mich Jhm auf. Was Er mir jmmer thut", "tokens": ["Und", "ge\u00b7be", "mich", "Jhm", "auf", ".", "Was", "Er", "mir", "jm\u00b7mer", "thut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "PWS", "PPER", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In Zeit und Ewigkeit/ da\u00df sprech\u2019 ich alles gut.", "tokens": ["In", "Zeit", "und", "E\u00b7wig\u00b7keit", "/", "da\u00df", "sprech'", "ich", "al\u00b7les", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$(", "KOUS", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ach da\u00df Er mich doch nur f\u00fcr mehrerm fall beh\u00fctte!", "tokens": ["Ach", "da\u00df", "Er", "mich", "doch", "nur", "f\u00fcr", "meh\u00b7rerm", "fall", "be\u00b7h\u00fct\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPER", "PRF", "ADV", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}