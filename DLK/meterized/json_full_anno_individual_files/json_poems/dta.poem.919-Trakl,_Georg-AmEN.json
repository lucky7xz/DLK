{"dta.poem.919": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "AmEN", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Verwestes gleitend durch die morsche Stube;", "tokens": ["Ver\u00b7wes\u00b7tes", "glei\u00b7tend", "durch", "die", "mor\u00b7sche", "Stu\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schatten an gelben Tapeten; in dunklen Spiegeln w\u00f6lbt", "tokens": ["Schat\u00b7ten", "an", "gel\u00b7ben", "Ta\u00b7pe\u00b7ten", ";", "in", "dunk\u00b7len", "Spie\u00b7geln", "w\u00f6lbt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$.", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+---+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Sich unserer H\u00e4nde elfenbeinerne Traurigkeit.", "tokens": ["Sich", "un\u00b7se\u00b7rer", "H\u00e4n\u00b7de", "el\u00b7fen\u00b7bei\u00b7ner\u00b7ne", "Trau\u00b7rig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Braune Perlen rinnen durch die erstorbenen Finger.", "tokens": ["Brau\u00b7ne", "Per\u00b7len", "rin\u00b7nen", "durch", "die", "ers\u00b7tor\u00b7be\u00b7nen", "Fin\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "In der Stille", "tokens": ["In", "der", "Stil\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Tun sich eines Engels blaue Mohnaugen auf.", "tokens": ["Tun", "sich", "ei\u00b7nes", "En\u00b7gels", "blau\u00b7e", "Moh\u00b7nau\u00b7gen", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Blau ist auch der Abend;", "tokens": ["Blau", "ist", "auch", "der", "A\u00b7bend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die Stunde unseres Absterbens, Azraels Schatten,", "tokens": ["Die", "Stun\u00b7de", "un\u00b7se\u00b7res", "A\u00b7bster\u00b7bens", ",", "Az\u00b7ra\u00b7els", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "NE", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der ein braunes G\u00e4rtchen verdunkelt.", "tokens": ["Der", "ein", "brau\u00b7nes", "G\u00e4rt\u00b7chen", "ver\u00b7dun\u00b7kelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}