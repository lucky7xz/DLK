{"textgrid.poem.65399": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcbergegossen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast den Becher mir zu voll gegossen,", "tokens": ["Du", "hast", "den", "Be\u00b7cher", "mir", "zu", "voll", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "PTKA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auf die Hand ist dir der Wein geflossen.", "tokens": ["Und", "auf", "die", "Hand", "ist", "dir", "der", "Wein", "ge\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Trink' ab, trink' ab mit deinen rothen Lippen!", "tokens": ["Trink'", "ab", ",", "trink'", "ab", "mit", "dei\u00b7nen", "ro\u00b7then", "Lip\u00b7pen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich will von deiner Hand die Tropfen nippen.", "tokens": ["Ich", "will", "von", "dei\u00b7ner", "Hand", "die", "Trop\u00b7fen", "nip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und um des Bechers Rand such' ich die Stelle,", "tokens": ["Und", "um", "des", "Be\u0