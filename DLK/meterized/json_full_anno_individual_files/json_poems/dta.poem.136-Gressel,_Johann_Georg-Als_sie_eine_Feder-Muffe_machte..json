{"dta.poem.136": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie eine Feder-Muffe machte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Schaut doch die Persis an! die mit den Federn spielet/", "tokens": ["Schaut", "doch", "die", "Per\u00b7sis", "an", "!", "die", "mit", "den", "Fe\u00b7dern", "spie\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$.", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eine Art bald hier/ die ander dorthin setzt/", "tokens": ["Und", "ei\u00b7ne", "Art", "bald", "hier", "/", "die", "an\u00b7der", "dor\u00b7thin", "setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ADV", "$(", "ART", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bis da\u00df der bunte Schein ihr sch\u00f6nes Aug\u2019 erg\u00f6tzt/", "tokens": ["Bis", "da\u00df", "der", "bun\u00b7te", "Schein", "ihr", "sch\u00f6\u00b7nes", "Aug'", "er\u00b7g\u00f6tzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sie ein feines Sammt von zarten Federn f\u00fchlet.", "tokens": ["Und", "sie", "ein", "fei\u00b7nes", "Sammt", "von", "zar\u00b7ten", "Fe\u00b7dern", "f\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie spricht: Es w\u00e4rmt die Hand/ wenn ", "tokens": ["Sie", "spricht", ":", "Es", "w\u00e4rmt", "die", "Hand", "/", "wenn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$(", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und durch des Himmels-Schnee der Finger Schnee ben\u00e4tzt.", "tokens": ["Und", "durch", "des", "Him\u00b7mels\u00b7Schnee", "der", "Fin\u00b7ger", "Schnee", "be\u00b7n\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich brauchs/ wenn ", "tokens": ["Ich", "brauchs", "/", "wenn"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$(", "KOUS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Und wenn vor starren Frost kein Pflug im Acker w\u00fchlet.", "tokens": ["Und", "wenn", "vor", "star\u00b7ren", "Frost", "kein", "Pflug", "im", "A\u00b7cker", "w\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Alleine! sagt mir doch was ists vor sanfft Gefl\u00fcgel?", "tokens": ["Al\u00b7lei\u00b7ne", "!", "sagt", "mir", "doch", "was", "ists", "vor", "sanfft", "Ge\u00b7fl\u00fc\u00b7gel", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "PPER", "ADV", "PWS", "VAFIN", "APPR", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ists n\u00f6htig/ mu\u00df sie auch die Arbeit selber thun?", "tokens": ["Ists", "n\u00f6h\u00b7tig", "/", "mu\u00df", "sie", "auch", "die", "Ar\u00b7beit", "sel\u00b7ber", "thun", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$(", "VMFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sind ", "tokens": ["Sind"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Nein! ", "tokens": ["Nein", "!"], "token_info": ["word", "punct"], "pos": ["PTKANT", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Die Hand hats halb zur Lust/ und halb zum Pracht gethan/", "tokens": ["Die", "Hand", "hats", "halb", "zur", "Lust", "/", "und", "halb", "zum", "Pracht", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$(", "KON", "ADJD", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Felle sind zu rauch/ sie will in Federn ruhn.", "tokens": ["Die", "Fel\u00b7le", "sind", "zu", "rauch", "/", "sie", "will", "in", "Fe\u00b7dern", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$(", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}