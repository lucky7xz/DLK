{"dta.poem.3410": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Elegie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Meine Ida, der Lenz, der Lenz beginnet. Ich", "tokens": ["Mei\u00b7ne", "I\u00b7da", ",", "der", "Lenz", ",", "der", "Lenz", "be\u00b7gin\u00b7net", ".", "Ich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NE", "$,", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$.", "PPER"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "hab' ihm", "tokens": ["hab'", "ihm"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Eben entgegen gegr\u00fcsst, hab' ihn den H\u00fc-", "tokens": ["E\u00b7ben", "ent\u00b7ge\u00b7gen", "ge\u00b7gr\u00fcs\u00b7st", ",", "hab'", "ihn", "den", "H\u00fc"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "VVPP", "$,", "VAFIN", "PPER", "ART", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "gel herab", "tokens": ["gel", "her\u00b7ab"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Schweben gesehen. Er sah so freundlich. Ein buh-", "tokens": ["Schwe\u00b7ben", "ge\u00b7se\u00b7hen", ".", "Er", "sah", "so", "freund\u00b7lich", ".", "Ein", "buh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$.", "PPER", "VVFIN", "ADV", "ADJD", "$.", "ART", "TRUNC"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "lendes L\u00fcftchen", "tokens": ["len\u00b7des", "L\u00fcft\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "Haucht' um den rosigen Hals sein hyazin-", "tokens": ["Haucht'", "um", "den", "ro\u00b7si\u00b7gen", "Hals", "sein", "hya\u00b7zin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PPOSAT", "TRUNC"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "thenes Haar.", "tokens": ["the\u00b7nes", "Haar", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.9": {"text": "Gr\u00e4schen und Bl\u00fcmchen entsprossen des Schreiten-", "tokens": ["Gr\u00e4sc\u00b7hen", "und", "Bl\u00fcm\u00b7chen", "ent\u00b7spros\u00b7sen", "des", "Schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "TRUNC"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "den schwebendem Tritte,", "tokens": ["den", "schwe\u00b7ben\u00b7dem", "Trit\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Schnee und Reif zerschmolz seinem erlauen-", "tokens": ["Schnee", "und", "Reif", "zer\u00b7schmolz", "sei\u00b7nem", "er\u00b7lau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJD", "PPOSAT", "TRUNC"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.12": {"text": "den Hauch.", "tokens": ["den", "Hauch", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Silberstiebende B\u00e4chlein durchrieseln schon schwat-", "tokens": ["Sil\u00b7ber\u00b7stie\u00b7ben\u00b7de", "B\u00e4ch\u00b7lein", "durc\u00b7hrie\u00b7seln", "schon", "schwat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "zend die Fluren,", "tokens": ["zend", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.15": {"text": "Blitzen im Sonnenstrahl, spiegeln die Sonne", "tokens": ["Blit\u00b7zen", "im", "Son\u00b7nen\u00b7strahl", ",", "spie\u00b7geln", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+--+-++--+-", "measure": "iambic.penta.invert"}, "line.16": {"text": "zur\u00fcck.", "tokens": ["zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Zwiefach Leben durchstr\u00f6mt mir das Blut. Gleich", "tokens": ["Zwie\u00b7fach", "Le\u00b7ben", "durch\u00b7str\u00f6mt", "mir", "das", "Blut", ".", "Gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$.", "ADV"], "meter": "+-+--+--++", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "der Lerch' in den L\u00fcften,", "tokens": ["der", "Ler\u00b7ch'", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Gleich dem Schwan im Teich, f\u00fchl' ich mich", "tokens": ["Gleich", "dem", "Schwan", "im", "Teich", ",", "f\u00fchl'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,", "VVFIN", "PPER", "PRF"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.20": {"text": "freudig und frisch;", "tokens": ["freu\u00b7dig", "und", "frisch", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Denn der Lenz beginnt. Wer wollte des kehrenden", "tokens": ["Denn", "der", "Lenz", "be\u00b7ginnt", ".", "Wer", "woll\u00b7te", "des", "keh\u00b7ren\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "$.", "PWS", "VMFIN", "ART", "NN"], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Lenzes", "tokens": ["Len\u00b7zes"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Sich nicht erfreuen, verj\u00fcngt mit der ver-", "tokens": ["Sich", "nicht", "er\u00b7freu\u00b7en", ",", "ver\u00b7j\u00fcngt", "mit", "der", "ver"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "PTKNEG", "VVINF", "$,", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "j\u00fcngten Natur,", "tokens": ["j\u00fcng\u00b7ten", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Nicht die offenen Fluren durchtanzen im luftigen", "tokens": ["Nicht", "die", "of\u00b7fe\u00b7nen", "Flu\u00b7ren", "durch\u00b7tan\u00b7zen", "im", "luf\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVIZU", "APPRART", "NN"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "Sprunge,", "tokens": ["Sprun\u00b7ge", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Nicht lautjauchzend dem Lenz Freudebe-", "tokens": ["Nicht", "lau\u00b7tjauch\u00b7zend", "dem", "Lenz", "Freu\u00b7de\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "TRUNC"], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}, "line.28": {"text": "gr\u00fcssungen schrein?", "tokens": ["gr\u00fcs\u00b7sun\u00b7gen", "schrein", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Ida, der Lenz beginnt. Komm, Tochter sanfterer", "tokens": ["I\u00b7da", ",", "der", "Lenz", "be\u00b7ginnt", ".", "Komm", ",", "Toch\u00b7ter", "sanf\u00b7te\u00b7rer"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "$.", "VVFIN", "$,", "NN", "ADJD"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.30": {"text": "Freuden,", "tokens": ["Freu\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Komm mit mir aufs Feld. Siehe, der Frost", "tokens": ["Komm", "mit", "mir", "aufs", "Feld", ".", "Sie\u00b7he", ",", "der", "Frost"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "NN", "$.", "VVIMP", "$,", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.32": {"text": "ist dahin,", "tokens": ["ist", "da\u00b7hin", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.33": {"text": "Und der Schnee ist zerschmolzen. Es r\u00f6theln die", "tokens": ["Und", "der", "Schnee", "ist", "zer\u00b7schmol\u00b7zen", ".", "Es", "r\u00f6\u00b7theln", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF", "$.", "PPER", "VVFIN", "ART"], "meter": "--+--+--+--", "measure": "anapaest.tri.plus"}, "line.34": {"text": "\u00c4ste der Haseln,", "tokens": ["\u00c4s\u00b7te", "der", "Ha\u00b7seln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.35": {"text": "Und das braune Gebirg lichtet das d\u00fcstere", "tokens": ["Und", "das", "brau\u00b7ne", "Ge\u00b7birg", "lich\u00b7tet", "das", "d\u00fcs\u00b7te\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PDS", "VVFIN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.36": {"text": "Moos.", "tokens": ["Moos", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.37": {"text": "Neues Leben durchwimmelt die Fluren; den S\u00fcm-", "tokens": ["Neu\u00b7es", "Le\u00b7ben", "durch\u00b7wim\u00b7melt", "die", "Flu\u00b7ren", ";", "den", "S\u00fcm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$.", "ART", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.38": {"text": "pfen entgurgeln", "tokens": ["pfen", "ent\u00b7gur\u00b7geln"], "token_info": ["word", "word"], "pos": ["VVFIN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.39": {"text": "Fr\u00f6sche; der Erd' entw\u00fchlt schwirrend", "tokens": ["Fr\u00f6\u00b7sche", ";", "der", "Erd'", "ent\u00b7w\u00fchlt", "schwir\u00b7rend"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "ADJD"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.40": {"text": "Gew\u00fcrm; in der Luft", "tokens": ["Ge\u00b7w\u00fcrm", ";", "in", "der", "Luft"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.41": {"text": "Schallt es vom Jubelgesang der triumphirenden", "tokens": ["Schallt", "es", "vom", "Ju\u00b7bel\u00b7ge\u00b7sang", "der", "tri\u00b7um\u00b7phi\u00b7ren\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "ADJA"], "meter": "+--+--+-+-+--", "measure": "dactylic.di.plus"}, "line.42": {"text": "Lerche.", "tokens": ["Ler\u00b7che", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.43": {"text": "Mit des werdenden Tags spriessendem blas-", "tokens": ["Mit", "des", "wer\u00b7den\u00b7den", "Tags", "spries\u00b7sen\u00b7dem", "blas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "TRUNC"], "meter": "--+--++--+", "measure": "anapaest.di.plus"}, "line.44": {"text": "sestem Strahl", "tokens": ["ses\u00b7tem", "Strahl"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.45": {"text": "Schwingt sich die S\u00e4ngerin himmelempor, und singt,", "tokens": ["Schwingt", "sich", "die", "S\u00e4n\u00b7ge\u00b7rin", "him\u00b7mel\u00b7em\u00b7por", ",", "und", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.46": {"text": "bis die D\u00e4mmrung", "tokens": ["bis", "die", "D\u00e4mm\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.47": {"text": "Berg' und Thale verh\u00fcllt, fr\u00f6hlich das fr\u00f6h-", "tokens": ["Ber\u00b7g'", "und", "Tha\u00b7le", "ver\u00b7h\u00fcllt", ",", "fr\u00f6h\u00b7lich", "das", "fr\u00f6h"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVPP", "$,", "ADJD", "ART", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.48": {"text": "liche Lied.", "tokens": ["li\u00b7che", "Lied", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.49": {"text": "Komm denn, mildes M\u00e4dchen, du Freundin edler", "tokens": ["Komm", "denn", ",", "mil\u00b7des", "M\u00e4d\u00b7chen", ",", "du", "Freun\u00b7din", "ed\u00b7ler"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,", "PPER", "NN", "ADJA"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.50": {"text": "Gef\u00fchle,", "tokens": ["Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.51": {"text": "Komm mit mir aufs Feld. Jubelnd, froh-", "tokens": ["Komm", "mit", "mir", "aufs", "Feld", ".", "Ju\u00b7belnd", ",", "froh"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPRART", "NN", "$.", "VVPP", "$,", "TRUNC"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.52": {"text": "lockend empfahn", "tokens": ["lo\u00b7ckend", "em\u00b7pfahn"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.53": {"text": "Wird dich der Fr\u00fchling, wird \u201eBraut\u201c dich gr\u00fcssen,", "tokens": ["Wird", "dich", "der", "Fr\u00fch\u00b7ling", ",", "wird", "\u201e", "Braut", "\u201c", "dich", "gr\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "VAFIN", "$(", "NN", "$(", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.54": {"text": "und seinem Gel\u00fcste", "tokens": ["und", "sei\u00b7nem", "Ge\u00b7l\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.55": {"text": "Duftender um dich zu wehn, seinem gefie-", "tokens": ["Duf\u00b7ten\u00b7der", "um", "dich", "zu", "wehn", ",", "sei\u00b7nem", "ge\u00b7fie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PPER", "PTKZU", "VVINF", "$,", "PPOSAT", "TRUNC"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.56": {"text": "derten Chor", "tokens": ["der\u00b7ten", "Chor"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.57": {"text": "Schmelzender um dich zu fl\u00f6ten gebieten \u2014 Und", "tokens": ["Schmel\u00b7zen\u00b7der", "um", "dich", "zu", "fl\u00f6\u00b7ten", "ge\u00b7bie\u00b7ten", "Und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "PPER", "PTKZU", "VVINF", "VVINF", "$(", "KON"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.58": {"text": "ach, Geliebte,", "tokens": ["ach", ",", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["XY", "$,", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.59": {"text": "Eine Ahnung der Wonn' schauert mir sanft", "tokens": ["Ei\u00b7ne", "Ah\u00b7nung", "der", "Wonn'", "schau\u00b7ert", "mir", "sanft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.60": {"text": "durch das Herz \u2014", "tokens": ["durch", "das", "Herz"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.61": {"text": "Ach vielleicht, dass auch dich des Schmeichlers", "tokens": ["Ach", "viel\u00b7leicht", ",", "dass", "auch", "dich", "des", "Sc