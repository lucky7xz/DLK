{"textgrid.poem.48955": {"metadata": {"author": {"name": "Kretschmann, Karl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Danket dem Herrn, denn er ist gut und freundlich!", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Danket dem Herrn, denn er ist gut und freundlich!", "tokens": ["Dan\u00b7ket", "dem", "Herrn", ",", "denn", "er", "ist", "gut", "und", "freund\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ewiglich w\u00e4hret seiner Gnade Dauer:", "tokens": ["E\u00b7wig\u00b7lich", "w\u00e4h\u00b7ret", "sei\u00b7ner", "Gna\u00b7de", "Dau\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "G\u00f6ttliche Nahrung gab er uns; er gab uns", "tokens": ["G\u00f6tt\u00b7li\u00b7che", "Nah\u00b7rung", "gab", "er", "uns", ";", "er", "gab", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "PPER"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Speise des Lebens!", "tokens": ["Spei\u00b7se", "des", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Seegen und S\u00e4ttigung ward uns die F\u00fclle.", "tokens": ["See\u00b7gen", "und", "S\u00e4t\u00b7ti\u00b7gung", "ward", "uns", "die", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Br\u00fcder und Schwestern, theure Blutsverwandte,", "tokens": ["Br\u00fc\u00b7der", "und", "Schwes\u00b7tern", ",", "theu\u00b7re", "Bluts\u00b7ver\u00b7wand\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "La\u00dft uns vom Becher seiner Liebe froh sein:", "tokens": ["La\u00dft", "uns", "vom", "Be\u00b7cher", "sei\u00b7ner", "Lie\u00b7be", "froh", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Aber auch dankbar.", "tokens": ["A\u00b7ber", "auch", "dank\u00b7bar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00dft uns des Tempels heiliges Gew\u00f6lbe", "tokens": ["La\u00dft", "uns", "des", "Tem\u00b7pels", "hei\u00b7li\u00b7ges", "Ge\u00b7w\u00f6l\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jubelnd mit Hymnen unsers Danks erf\u00fcllen!", "tokens": ["Ju\u00b7belnd", "mit", "Hym\u00b7nen", "un\u00b7sers", "Danks", "er\u00b7f\u00fcl\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Unsichtbar schwebt hier Gottes Wohlgefallen:", "tokens": ["Un\u00b7sicht\u00b7bar", "schwebt", "hier", "Got\u00b7tes", "Wohl\u00b7ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Aber uns f\u00fchlbar.", "tokens": ["A\u00b7ber", "uns", "f\u00fchl\u00b7bar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "La\u00dft uns erz\u00e4hlen unter s\u00fc\u00dfen Thr\u00e4nen,", "tokens": ["La\u00dft", "uns", "er\u00b7z\u00e4h\u00b7len", "un\u00b7ter", "s\u00fc\u00b7\u00dfen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Obschon der Freude Stammeln unsre Zungen,", "tokens": ["Ob\u00b7schon", "der", "Freu\u00b7de", "Stam\u00b7meln", "uns\u00b7re", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Obschon das Leben dieses Gl\u00fccks all unsre", "tokens": ["Ob\u00b7schon", "das", "Le\u00b7ben", "die\u00b7ses", "Gl\u00fccks", "all", "uns\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PDAT", "NN", "PIAT", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herzen durchzittert;", "tokens": ["Her\u00b7zen", "durch\u00b7zit\u00b7tert", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "La\u00dft uns erz\u00e4hlen von der Liebe Wundern,", "tokens": ["La\u00dft", "uns", "er\u00b7z\u00e4h\u00b7len", "von", "der", "Lie\u00b7be", "Wun\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von der Erbarmung g\u00f6ttlichem Geheimni\u00df;", "tokens": ["Von", "der", "Er\u00b7bar\u00b7mung", "g\u00f6tt\u00b7li\u00b7chem", "Ge\u00b7heim\u00b7ni\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welches Ged\u00e4chtni\u00df Jesus, der Vollbringer,", "tokens": ["Wel\u00b7ches", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "Je\u00b7sus", ",", "der", "Voll\u00b7brin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "NN", "NE", "$,", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Nach sich gelassen.", "tokens": ["Nach", "sich", "ge\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "H\u00f6rtet ihr nicht die heil'gen himmelvollen", "tokens": ["H\u00f6r\u00b7tet", "ihr", "nicht", "die", "heil'\u00b7gen", "him\u00b7mel\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Worte der Stiftung? Saht ihr nicht des Priesters", "tokens": ["Wor\u00b7te", "der", "Stif\u00b7tung", "?", "Saht", "ihr", "nicht", "des", "Pries\u00b7ters"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Segnung und Weihe? \u2013 H\u00f6rt, o h\u00f6rt und merkt, die", "tokens": ["Seg\u00b7nung", "und", "Wei\u00b7he", "?", "\u2013", "H\u00f6rt", ",", "o", "h\u00f6rt", "und", "merkt", ",", "die"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "KON", "NN", "$.", "$(", "VVIMP", "$,", "FM", "VVFIN", "KON", "VVFIN", "$,", "PRELS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Worte des Lebens!", "tokens": ["Wor\u00b7te", "des", "Le\u00b7bens", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbnahm er das Brot, und brachs, und gab es allen; \u2013 \u2013", "tokens": ["\u00bb", "nahm", "er", "das", "Brot", ",", "und", "brachs", ",", "und", "gab", "es", "al\u00b7len", ";", "\u2013", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "PPER", "PIAT", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nahm er den Kelch, und lie\u00df sie alle trinken; \u2013 \u2013\u00ab", "tokens": ["Nahm", "er", "den", "Kelch", ",", "und", "lie\u00df", "sie", "al\u00b7le", "trin\u00b7ken", ";", "\u2013", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "PIS", "VVINF", "$.", "$(", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wunder voll Wonne! Denn er gab sich selber,", "tokens": ["Wun\u00b7der", "voll", "Won\u00b7ne", "!", "Denn", "er", "gab", "sich", "sel\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$.", "KON", "PPER", "VVFIN", "PRF", "ADV", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Da\u00df er uns rette.", "tokens": ["Da\u00df", "er", "uns", "ret\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "G\u00f6ttlicher Freund! o hilf dies Heil uns tragen!", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", "Freund", "!", "o", "hilf", "dies", "Heil", "uns", "tra\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "FM", "FM", "PDS", "NN", "PPER", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hilf uns thun den Willen dieses Bundes:", "tokens": ["Hilf", "uns", "thun", "den", "Wil\u00b7len", "die\u00b7ses", "Bun\u00b7des", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbaber das thut, so oft ihrs thut, zu meiner", "tokens": ["\u00bb", "a\u00b7ber", "das", "thut", ",", "so", "oft", "ihrs", "thut", ",", "zu", "mei\u00b7ner"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "KON", "PDS", "VVFIN", "$,", "ADV", "ADV", "NN", "VVFIN", "$,", "APPR", "PPOSAT"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Liebe Ged\u00e4chtni\u00df.\u00ab", "tokens": ["Lie\u00b7be", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Liebe, ja Liebe bis zu Tod und Grabe!", "tokens": ["Lie\u00b7be", ",", "ja", "Lie\u00b7be", "bis", "zu", "Tod", "und", "Gra\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "NN", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Liebe des Himmels f\u00fcr den Staub der Erde!", "tokens": ["Lie\u00b7be", "des", "Him\u00b7mels", "f\u00fcr", "den", "Staub", "der", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Liebe der Ewigkeit und Allmacht, zu dem", "tokens": ["Lie\u00b7be", "der", "E\u00b7wig\u00b7keit", "und", "All\u00b7macht", ",", "zu", "dem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,", "APPR", "ART"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Sterblichen Menschen!", "tokens": ["Sterb\u00b7li\u00b7chen", "Men\u00b7schen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.10": {"line.1": {"text": "Br\u00fcder und Schwestern, wie er uns geliebet,", "tokens": ["Br\u00fc\u00b7der", "und", "Schwes\u00b7tern", ",", "wie", "er", "uns", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "La\u00dft uns ihn lieben! Weder Welt noch Himmel", "tokens": ["La\u00dft", "uns", "ihn", "lie\u00b7ben", "!", "We\u00b7der", "Welt", "noch", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "VVINF", "$.", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat ihn geschieden. Weder Welt noch Himmel", "tokens": ["Hat", "ihn", "ge\u00b7schie\u00b7den", ".", "We\u00b7der", "Welt", "noch", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "KON", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll uns auch scheiden!", "tokens": ["Soll", "uns", "auch", "schei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Also gen\u00e4hrt durch dieses Brot der Weihe,", "tokens": ["Al\u00b7so", "ge\u00b7n\u00e4hrt", "durch", "die\u00b7ses", "Brot", "der", "Wei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Also gest\u00e4rkt durch diesen Becher, la\u00dft uns", "tokens": ["Al\u00b7so", "ge\u00b7st\u00e4rkt", "durch", "die\u00b7sen", "Be\u00b7cher", ",", "la\u00dft", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "PDAT", "NN", "$,", "VVIMP", "PPER"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Herzen an Herzen, Hand in Hand, nun liebreich,", "tokens": ["Her\u00b7zen", "an", "Her\u00b7zen", ",", "Hand", "in", "Hand", ",", "nun", "lieb\u00b7reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Kl\u00fcglich und muthig,", "tokens": ["Kl\u00fcg\u00b7lich", "und", "mut\u00b7hig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Wallen die Reise durch des Lebens Dornen;", "tokens": ["Wal\u00b7len", "die", "Rei\u00b7se", "durch", "des", "Le\u00b7bens", "Dor\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Treulich dem Pfade, den er vorgewandelt.", "tokens": ["Treu\u00b7lich", "dem", "Pfa\u00b7de", ",", "den", "er", "vor\u00b7ge\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "F\u00fcrchtet die Dornen nicht! denn seine Liebe", "tokens": ["F\u00fcrch\u00b7tet", "die", "Dor\u00b7nen", "nicht", "!", "denn", "sei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$.", "KON", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Schm\u00fcckt sie mit Trauben.", "tokens": ["Schm\u00fcckt", "sie", "mit", "Trau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Jesu, so kommen wir, all deine G\u00e4ste;", "tokens": ["Je\u00b7su", ",", "so", "kom\u00b7men", "wir", ",", "all", "dei\u00b7ne", "G\u00e4s\u00b7te", ";"], "token_info": ["word", "punc