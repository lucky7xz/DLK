{"textgrid.poem.60830": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Baukunst", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles verleiht beinahe dem Maler die sch\u00f6ne Natur schon,", "tokens": ["Al\u00b7les", "ver\u00b7leiht", "bei\u00b7na\u00b7he", "dem", "Ma\u00b7ler", "die", "sch\u00f6\u00b7ne", "Na\u00b7tur", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Baukunst aber erheischt feineren geistigen Sinn:", "tokens": ["Bau\u00b7kunst", "a\u00b7ber", "er\u00b7heischt", "fei\u00b7ne\u00b7ren", "geis\u00b7ti\u00b7gen", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Pomp, Zieraten und dorische S\u00e4ulen und gotische Schn\u00f6rkel,", "tokens": ["Pomp", ",", "Zie\u00b7ra\u00b7ten", "und", "do\u00b7ri\u00b7sche", "S\u00e4u\u00b7len", "und", "go\u00b7ti\u00b7sche", "Schn\u00f6r\u00b7kel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+---+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Spielzeug sind sie, wofern fehlt der geheime Begriff;", "tokens": ["Spiel\u00b7zeug", "sind", "sie", ",", "wo\u00b7fern", "fehlt", "der", "ge\u00b7hei\u00b7me", "Be\u00b7griff", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Aber ein wirkliches Bauwerk ist ein versteinerter Rhythmus,", "tokens": ["A\u00b7ber", "ein", "wirk\u00b7li\u00b7ches", "Bau\u00b7werk", "ist", "ein", "ver\u00b7stei\u00b7ner\u00b7ter", "Rhyth\u00b7mus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-+-+-+", "measure": "dactylic.tri.plus"}, "line.6": {"text": "Deshalb selten, wie auch selten ein gutes Gedicht.", "tokens": ["Des\u00b7halb", "sel\u00b7ten", ",", "wie", "auch", "sel\u00b7ten", "ein", "gu\u00b7tes", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "$,", "PWAV", "ADV", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Alles verleiht beinahe dem Maler die sch\u00f6ne Natur schon,", "tokens": ["Al\u00b7les", "ver\u00b7leiht", "bei\u00b7na\u00b7he", "dem", "Ma\u00b7ler", "die", "sch\u00f6\u00b7ne", "Na\u00b7tur", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Baukunst aber erheischt feineren geistigen Sinn:", "tokens": ["Bau\u00b7kunst", "a\u00b7ber", "er\u00b7heischt", "fei\u00b7ne\u00b7ren", "geis\u00b7ti\u00b7gen", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Pomp, Zieraten und dorische S\u00e4ulen und gotische Schn\u00f6rkel,", "tokens": ["Pomp", ",", "Zie\u00b7ra\u00b7ten", "und", "do\u00b7ri\u00b7