{"textgrid.poem.48005": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Der Heiland", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Prunkschlo\u00df nicht, in goldner K\u00f6nigshalle:", "tokens": ["Im", "Prunk\u00b7schlo\u00df", "nicht", ",", "in", "gold\u00b7ner", "K\u00f6\u00b7nigs\u00b7hal\u00b7le", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in enger Krippe und im niedern Stalle", "tokens": ["in", "en\u00b7ger", "Krip\u00b7pe", "und", "im", "nie\u00b7dern", "Stal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ist einst der Strom des ewigen Lichts entsprungen,", "tokens": ["ist", "einst", "der", "Strom", "des", "e\u00b7wi\u00b7gen", "Lichts", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "der Lebensch\u00f6re Vollakkord erklungen.", "tokens": ["der", "Le\u00b7ben\u00b7sch\u00f6\u00b7re", "Vol\u00b7lak\u00b7kord", "er\u00b7klun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht im Gewand von Goldstoff oder Seide:", "tokens": ["Nicht", "im", "Ge\u00b7wand", "von", "Gold\u00b7stoff", "o\u00b7der", "Sei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit nackten F\u00fc\u00dfen und im h\u00e4renen Kleide", "tokens": ["mit", "nack\u00b7ten", "F\u00fc\u00b7\u00dfen", "und", "im", "h\u00e4\u00b7re\u00b7nen", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ging einst der Christ in seiner Freunde Schar", "tokens": ["ging", "einst", "der", "Christ", "in", "sei\u00b7ner", "Freun\u00b7de", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "hinauf zum Haus, das seines Vaters war. \u2013", "tokens": ["hin\u00b7auf", "zum", "Haus", ",", "das", "sei\u00b7nes", "Va\u00b7ters", "war", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und als am Kreuz, verblutend, wegbestaubt", "tokens": ["Und", "als", "am", "Kreuz", ",", "ver\u00b7blu\u00b7tend", ",", "weg\u00b7be\u00b7staubt"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "KOUS", "APPRART", "NN", "$,", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "er sterbend neigt' das schmerzgekr\u00f6nte Haupt,", "tokens": ["er", "ster\u00b7bend", "neigt'", "das", "schmerz\u00b7ge\u00b7kr\u00f6n\u00b7te", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "da weinten um des k\u00fcnftigen Heils Verk\u00fcnder", "tokens": ["da", "wein\u00b7ten", "um", "des", "k\u00fcnf\u00b7ti\u00b7gen", "Heils", "Ver\u00b7k\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "die Armen nur, die Z\u00f6llner und die S\u00fcnder . . .", "tokens": ["die", "Ar\u00b7men", "nur", ",", "die", "Z\u00f6ll\u00b7ner", "und", "die", "S\u00fcn\u00b7der", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "KON", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch nicht am Kreuze kann der Geist verbluten,", "tokens": ["Doch", "nicht", "am", "Kreu\u00b7ze", "kann", "der", "Geist", "ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und was aus Gott entsprang, mu\u00df rastlos fluten.", "tokens": ["und", "was", "aus", "Gott", "ent\u00b7sprang", ",", "mu\u00df", "rast\u00b7los", "flu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "VVFIN", "$,", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und heut, nachdem Jahrtausende verflossen,", "tokens": ["Und", "heut", ",", "nach\u00b7dem", "Jahr\u00b7tau\u00b7sen\u00b7de", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "durchbebt die Welt ein heimlich Gl\u00fchn und Sprossen:", "tokens": ["durch\u00b7bebt", "die", "Welt", "ein", "heim\u00b7lich", "Gl\u00fchn", "und", "Spros\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "im Volke wandelt, segnend, unerkannt", "tokens": ["im", "Vol\u00b7ke", "wan\u00b7delt", ",", "seg\u00b7nend", ",", "un\u00b7er\u00b7kannt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "der Heiland schon im d\u00fcrftigen Gewand.", "tokens": ["der", "Hei\u00b7land", "schon", "im", "d\u00fcrf\u00b7ti\u00b7gen", "Ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wieder schaun des nahen Heils Verk\u00fcnder", "tokens": ["Und", "wie\u00b7der", "schaun", "des", "na\u00b7hen", "Heils", "Ver\u00b7k\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Enterbte nur, die Siechen und die S\u00fcnder,", "tokens": ["En\u00b7terb\u00b7te", "nur", ",", "die", "Sie\u00b7chen", "und", "die", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "indes der Fromme hohnvoll fragend geht,", "tokens": ["in\u00b7des", "der", "From\u00b7me", "hohn\u00b7voll", "fra\u00b7gend", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbwas Gutes kommen kann aus Nazareth \u2013?\u00ab", "tokens": ["\u00bb", "was", "Gu\u00b7tes", "kom\u00b7men", "kann", "aus", "Na\u00b7za\u00b7reth", "\u2013", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "NN", "VVINF", "VMFIN", "APPR", "NE", "$(", "$.", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Das Kind, dem einst der Engel Loblied scholl,", "tokens": ["Das", "Kind", ",", "dem", "einst", "der", "En\u00b7gel", "Lob\u00b7lied", "scholl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der Friedensk\u00f6nig, der da kommen soll,", "tokens": ["der", "Frie\u00b7dens\u00b7k\u00f6\u00b7nig", ",", "der", "da", "kom\u00b7men", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "aus dessen Mund ert\u00f6nt das zweite \u00bbWerde\u00ab,", "tokens": ["aus", "des\u00b7sen", "Mund", "er\u00b7t\u00f6nt", "das", "zwei\u00b7te", "\u00bb", "Wer\u00b7de", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVFIN", "ART", "ADJA", "$(", "VAFIN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ihr ahnt ihn nicht, ihr M\u00e4chtigen dieser Erde.", "tokens": ["ihr", "ahnt", "ihn", "nicht", ",", "ihr", "M\u00e4ch\u00b7ti\u00b7gen", "die\u00b7ser", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPOSAT", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "In seinem hagern Antlitz lest ihr nur", "tokens": ["In", "sei\u00b7nem", "ha\u00b7gern", "Ant\u00b7litz", "lest", "ihr", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "die tiefe Sehnsucht aller Kreatur:", "tokens": ["die", "tie\u00b7fe", "Sehn\u00b7sucht", "al\u00b7ler", "Kre\u00b7a\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "den Trieb nach Gl\u00fcck, den hei\u00dfen Durst nach Licht \u2013", "tokens": ["den", "Trieb", "nach", "Gl\u00fcck", ",", "den", "hei\u00b7\u00dfen", "Durst", "nach", "Licht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "die Gottesglorie aber seht ihr nicht.", "tokens": ["die", "Got\u00b7tes\u00b7glo\u00b7rie", "a\u00b7ber", "seht", "ihr", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Armen f\u00fcrchtet ihr, der Sklaven Heer,", "tokens": ["Der", "Ar\u00b7men", "f\u00fcrch\u00b7tet", "ihr", ",", "der", "Skla\u00b7ven", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das ihn umdr\u00e4ngt mit zitterndem Begehr,", "tokens": ["das", "ihn", "um\u00b7dr\u00e4ngt", "mit", "zit\u00b7tern\u00b7dem", "Be\u00b7gehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und vor dem Schrei, der aus der Tiefe hallt,", "tokens": ["und", "vor", "dem", "Schrei", ",", "der", "aus", "der", "Tie\u00b7fe", "hallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "verschlie\u00dft ihr eure Ohren mit Gewalt,", "tokens": ["ver\u00b7schlie\u00dft", "ihr", "eu\u00b7re", "Oh\u00b7ren", "mit", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und fl\u00fcchtet euch in eurer Schl\u00f6sser Schutz", "tokens": ["und", "fl\u00fcch\u00b7tet", "euch", "in", "eu\u00b7rer", "Schl\u00f6s\u00b7ser", "Schutz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und ruft die S\u00f6ldner auf zu Wehr und Trutz", "tokens": ["und", "ruft", "die", "S\u00f6ld\u00b7ner", "auf", "zu", "Wehr", "und", "Trutz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und schickt vom Schlo\u00dfhof schon mit Spie\u00df und Stangen", "tokens": ["und", "schickt", "vom", "Schlo\u00df\u00b7hof", "schon", "mit", "Spie\u00df", "und", "Stan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die H\u00e4scher aus, den Fremdling einzufangen \u2013", "tokens": ["die", "H\u00e4\u00b7scher", "aus", ",", "den", "Fremd\u00b7ling", "ein\u00b7zu\u00b7fan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und la\u00dft beim ersten blassen Morgenschimmern", "tokens": ["Und", "la\u00dft", "beim", "ers\u00b7ten", "blas\u00b7sen", "Mor\u00b7gen\u00b7schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "durch eure Knechte schon den Kreuzstamm zimmern.", "tokens": ["durch", "eu\u00b7re", "Knech\u00b7te", "schon", "den", "Kreuz\u00b7stamm", "zim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich aber sag euch, da\u00df, noch eh die Hallen", "tokens": ["Ich", "a\u00b7ber", "sag", "euch", ",", "da\u00df", ",", "noch", "eh", "die", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "KOUS", "$,", "ADV", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "im Fr\u00fchrot gl\u00fchn, in Staub die Balken fallen,", "tokens": ["im", "Fr\u00fch\u00b7rot", "gl\u00fchn", ",", "in", "Staub", "die", "Bal\u00b7ken", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und da\u00df die N\u00e4gel rosten, eh zur Qual", "tokens": ["und", "da\u00df", "die", "N\u00e4\u00b7gel", "ros\u00b7ten", ",", "eh", "zur", "Qual"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "des Menschensohns erh\u00f6ht der Marterpfahl, \u2013", "tokens": ["des", "Men\u00b7schen\u00b7sohns", "er\u00b7h\u00f6ht", "der", "Mar\u00b7ter\u00b7pfahl", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "ich aber sag euch, was die Bibel lehrt:", "tokens": ["ich", "a\u00b7ber", "sag", "euch", ",", "was", "die", "Bi\u00b7bel", "lehrt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "wenn der von Gott Gesandte wiederkehrt,", "tokens": ["wenn", "der", "von", "Gott", "Ge\u00b7sand\u00b7te", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NN", "NN", "VVFIN", "$,"]