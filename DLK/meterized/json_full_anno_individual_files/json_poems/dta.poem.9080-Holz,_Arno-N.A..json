{"dta.poem.9080": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus weissen Wolken", "tokens": ["Aus", "weis\u00b7sen", "Wol\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "baut sich ein Schloss.", "tokens": ["baut", "sich", "ein", "Schloss", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Spiegelnde Seeen, selige Wiesen,", "tokens": ["Spie\u00b7geln\u00b7de", "Se\u00b7e\u00b7en", ",", "se\u00b7li\u00b7ge", "Wie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "singende Brunnen aus tiefstem Smaragd!", "tokens": ["sin\u00b7gen\u00b7de", "Brun\u00b7nen", "aus", "tiefs\u00b7tem", "Sma\u00b7ragd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "In seinen schimmernden Hallen", "tokens": ["In", "sei\u00b7nen", "schim\u00b7mern\u00b7den", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "wohnen", "tokens": ["woh\u00b7nen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "die alten G\u00f6tter.", "tokens": ["die", "al\u00b7ten", "G\u00f6t\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Noch immer,", "tokens": ["Noch", "im\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "abends,", "tokens": ["a\u00b7bends", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "wenn die Sonne purpurn sinkt,", "tokens": ["wenn", "die", "Son\u00b7ne", "pur\u00b7purn", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "gl\u00fchn seine G\u00e4rten,", "tokens": ["gl\u00fchn", "sei\u00b7ne", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "vor ihren Wundern bebt mein Herz", "tokens": ["vor", "ih\u00b7ren", "Wun\u00b7dern", "bebt", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und lange . . . steh ich.", "tokens": ["und", "lan\u00b7ge", ".", ".", ".", "steh", "ich", "."], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "$.", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Sehns\u00fcchtig!", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.6": {"line.1": {"text": "Dann naht die Nacht,", "tokens": ["Dann", "naht", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "die Luft verlischt,", "tokens": ["die", "Luft", "ver\u00b7lischt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "wie zitterndes Silber blinkt das Meer,", "tokens": ["wie", "zit\u00b7tern\u00b7des", "Sil\u00b7ber", "blinkt", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und \u00fcber die ganze Welt hin", "tokens": ["und", "\u00fc\u00b7ber", "die", "gan\u00b7ze", "Welt", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "weht ein Duft", "tokens": ["weht", "ein", "Duft"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "wie von Rosen.", "tokens": ["wie", "von", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}