{"dta.poem.2703": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost-Zeilen an die Fr. Wittib  \n  Bey Beerdigung Hn. G. M. den  19.  \n Decembr. 1677.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn jetzt zwey Palmen stehn mit Lieb-vereinten Zweig&#275;", "tokens": ["Wenn", "jetzt", "zwey", "Pal\u00b7men", "stehn", "mit", "Lie\u00b7bver\u00b7ein\u00b7ten", "Zweig", "&#275;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["KOUS", "ADV", "CARD", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Bl\u00fct und Liebligkeit auff allen Bl\u00e4ttern lacht;", "tokens": ["Und", "Bl\u00fct", "und", "Lieb\u00b7lig\u00b7keit", "auff", "al\u00b7len", "Bl\u00e4t\u00b7tern", "lacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und drauff des Wetters-Straal wird einen niederbeu-", "tokens": ["Und", "drauff", "des", "Wet\u00b7ter\u00b7sStraal", "wird", "ei\u00b7nen", "nie\u00b7der\u00b7beu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "ART", "NN", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihm das gr\u00fcne Haar versengt und d\u00fcnne macht:", "tokens": ["Und", "ihm", "das", "gr\u00fc\u00b7ne", "Haar", "ver\u00b7sengt", "und", "d\u00fcn\u00b7ne", "macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So f\u00e4ngt die andre an aus Beyleid zuerblassen/", "tokens": ["So", "f\u00e4ngt", "die", "and\u00b7re", "an", "aus", "Bey\u00b7leid", "zu\u00b7er\u00b7blas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "APPR", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr laubicht Gipffel h\u00e4ngt/ die Zweige schlaffen ein/", "tokens": ["Ihr", "lau\u00b7bicht", "Gipf\u00b7fel", "h\u00e4ngt", "/", "die", "Zwei\u00b7ge", "schlaf\u00b7fen", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja sie wird so ein Bild des Traurens blicken lassen", "tokens": ["Ja", "sie", "wird", "so", "ein", "Bild", "des", "Trau\u00b7rens", "bli\u00b7cken", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PPER", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "VVINF", "VVINF"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Da\u00df man bekennen mu\u00df/ da\u00df sie verm\u00e4hlte seyn.", "tokens": ["Da\u00df", "man", "be\u00b7ken\u00b7nen", "mu\u00df", "/", "da\u00df", "sie", "ver\u00b7m\u00e4hl\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$(", "KOUS", "PPER", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So auch die Anemon\u2019 in ihrem Purpur-Kleide/", "tokens": ["So", "auch", "die", "A\u00b7ne\u00b7mon'", "in", "ih\u00b7rem", "Pur\u00b7pur\u00b7Klei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Alsbald ihr Aug und Licht die g\u00f6ldne Sonne sinckt/", "tokens": ["Als\u00b7bald", "ihr", "Aug", "und", "Licht", "die", "g\u00f6ld\u00b7ne", "Son\u00b7ne", "sinckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wirfft allen Zierath weg/ und gehet wie im Leide/", "tokens": ["Wirfft", "al\u00b7len", "Zie\u00b7rath", "weg", "/", "und", "ge\u00b7het", "wie", "im", "Lei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKVZ", "$(", "KON", "VVFIN", "KOKOM", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn sie den Thau der Nacht als Thr\u00e4nen in sich trinckt.", "tokens": ["Wenn", "sie", "den", "Thau", "der", "Nacht", "als", "Thr\u00e4\u00b7nen", "in", "sich", "trinckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "KOUS", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nicht anders/ ", "tokens": ["Nicht", "an\u00b7ders", "/"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Nachdem der Liebste mu\u00df den Weg des Fleisches gehn/", "tokens": ["Nach\u00b7dem", "der", "Liebs\u00b7te", "mu\u00df", "den", "Weg", "des", "Flei\u00b7sches", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jhr Auge schwimmt in Fluth/ ihr Hertz in heissem Blute", "tokens": ["Ihr", "Au\u00b7ge", "schwimmt", "in", "Fluth", "/", "ihr", "Hertz", "in", "heis\u00b7sem", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$(", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und man sieht umb sie rumb viel nasse Zeugen stehn.", "tokens": ["Und", "man", "sieht", "umb", "sie", "rumb", "viel", "nas\u00b7se", "Zeu\u00b7gen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "PPER", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Die Turtel-Taube kan den Gatten nicht so klagen", "tokens": ["Die", "Tur\u00b7tel\u00b7Tau\u00b7be", "kan", "den", "Gat\u00b7ten", "nicht", "so", "kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als sie itzt ihren Schatz und ander Hertz betraurt.", "tokens": ["Als", "sie", "itzt", "ih\u00b7ren", "Schatz", "und", "an\u00b7der", "Hertz", "be\u00b7traurt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "KON", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Jhr Sonnen-Schein ist hin/ sie sieht bey Winter-Tagen", "tokens": ["Ihr", "Son\u00b7nen\u00b7Schein", "ist", "hin", "/", "sie", "sieht", "bey", "Win\u00b7ter\u00b7Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$(", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sich nur mit Einsamkeit und banger Furcht ummaurt.", "tokens": ["Sich", "nur", "mit", "Ein\u00b7sam\u00b7keit", "und", "ban\u00b7ger", "Furcht", "um\u00b7maurt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gleicht sie nicht dem Corall/ der in gesaltznen Thr\u00e4nen", "tokens": ["Gleicht", "sie", "nicht", "dem", "Co\u00b7rall", "/", "der", "in", "ge\u00b7saltz\u00b7nen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$(", "ART", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Des Meeres wilder Fluth sein eintzig Wachsthum hat?", "tokens": ["Des", "Mee\u00b7res", "wil\u00b7der", "Fluth", "sein", "eint\u00b7zig", "Wach\u00b7sthum", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPOSAT", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Mach nicht das innre Weh/ und wiederholte Sehnen", "tokens": ["Mach", "nicht", "das", "inn\u00b7re", "Weh", "/", "und", "wie\u00b7der\u00b7hol\u00b7te", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$(", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die Leibes-Kr\u00e4fft schwach/ die Lebens-Geister mat?", "tokens": ["Die", "Lei\u00b7bes\u00b7Kr\u00e4fft", "schwach", "/", "die", "Le\u00b7bens\u00b7Geis\u00b7ter", "mat", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Gemeine Schmertzen sind noch endlich zuertragen/", "tokens": ["Ge\u00b7mei\u00b7ne", "Schmert\u00b7zen", "sind", "noch", "end\u00b7lich", "zu\u00b7er\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Alleine wenn der Tod bi\u00df an das Hertze greifft/", "tokens": ["Al\u00b7lei\u00b7ne", "wenn", "der", "Tod", "bi\u00df", "an", "das", "Hert\u00b7ze", "greifft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "APPR", "PDS", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wird der erschrockne Mund nicht Angst und Wehmuth sagen/", "tokens": ["Wird", "der", "er\u00b7schrock\u00b7ne", "Mund", "nicht", "Angst", "und", "Weh\u00b7muth", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKNEG", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Indem der Plagen-Heer in seine Glieder streifft.", "tokens": ["In\u00b7dem", "der", "Pla\u00b7gen\u00b7Heer", "in", "sei\u00b7ne", "Glie\u00b7der", "streifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.30": {"text": "Der best und liebste Freund auff dieser Welt ist hin:", "tokens": ["Der", "best", "und", "liebs\u00b7te", "Freund", "auff", "die\u00b7ser", "Welt", "ist", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "ADJA", "NN", "APPR", "PDAT", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Zuflucht/ die sie nahm zu seinen Wolfahrts-B\u00e4umen/", "tokens": ["Die", "Zu\u00b7flucht", "/", "die", "sie", "nahm", "zu", "sei\u00b7nen", "Wolfahrts\u00b7B\u00e4u\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.32": {"text": "Erstirbt/ und Thr\u00e4nen sind ihr \u00fcbriger Gewin.", "tokens": ["Er\u00b7stirbt", "/", "und", "Thr\u00e4\u00b7nen", "sind", "ihr", "\u00fcb\u00b7ri\u00b7ger", "Ge\u00b7win", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KON", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Zwey Perlen/ wenn sie sich in zweyen Muscheln trennen", "tokens": ["Zwey", "Per\u00b7len", "/", "wenn", "sie", "sich", "in", "zwe\u00b7yen", "Mu\u00b7scheln", "tren\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "$(", "KOUS", "PPER", "PRF", "APPR", "VVFIN", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Sieht auch der tolle Schaum der See mitleidig an:", "tokens": ["Sieht", "auch", "der", "tol\u00b7le", "Schaum", "der", "See", "mit\u00b7lei\u00b7dig", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Zwey Hertzen/ die zugleich in einer Flamme brennen/", "tokens": ["Zwey", "Hert\u00b7zen", "/", "die", "zu\u00b7gleich", "in", "ei\u00b7ner", "Flam\u00b7me", "bren\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "ART", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sehn durch den Tod zertheilt/ so sonst nichts theilen kan/", "tokens": ["Sehn", "durch", "den", "Tod", "zer\u00b7theilt", "/", "so", "sonst", "nichts", "thei\u00b7len", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVPP", "$(", "ADV", "ADV", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ist ein erb\u00e4rmlich Werck; der Sterbligkeit ihr Spiegel", "tokens": ["Ist", "ein", "er\u00b7b\u00e4rm\u00b7lich", "Werck", ";", "der", "Ster\u00b7blig\u00b7keit", "ihr", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJD", "NN", "$.", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Den sie hat auff den Platz der weiten Welt gesetzt.", "tokens": ["Den", "sie", "hat", "auff", "den", "Platz", "der", "wei\u00b7ten", "Welt", "ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Das eiserne Gebot bricht Eh-Schlu\u00df/ Brieff und Siegel", "tokens": ["Das", "ei\u00b7ser\u00b7ne", "Ge\u00b7bot", "bricht", "Eh\u00b7Schlu\u00df", "/", "Brieff", "und", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$(", "NN", "KON", "NN"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.40": {"text": "Und was der Mensch f\u00fcr fest und unverbr\u00fcchlich sch\u00e4tzt.", "tokens": ["Und", "was", "der", "Mensch", "f\u00fcr", "fest", "und", "un\u00b7ver\u00b7br\u00fcch\u00b7lich", "sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wiewol ", "tokens": ["Wie\u00b7wol"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+-", "measure": "trochaic.single"}, "line.42": {"text": "Zu ihrem Christenthum erbaulich dienen kan/", "tokens": ["Zu", "ih\u00b7rem", "Chris\u00b7ten\u00b7thum", "er\u00b7bau\u00b7lich", "die\u00b7nen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "So lange wir im Fleisch/ so m\u00fcssen wir nur leiden/", "tokens": ["So", "lan\u00b7ge", "wir", "im", "Fleisch", "/", "so", "m\u00fcs\u00b7sen", "wir", "nur", "lei\u00b7den", "/"], "token_in