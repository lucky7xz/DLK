{"dta.poem.21650": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "So leb\u2019 denn wohl, du St\u00e4tte \u2014", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "So leb\u2019 denn wohl, du St\u00e4tte, die dem M\u00fcden,", "tokens": ["So", "leb'", "denn", "wohl", ",", "du", "St\u00e4t\u00b7te", ",", "die", "dem", "M\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$,", "PPER", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Schmerzgesch\u00fcttelten noch Labung bot,", "tokens": ["Dem", "Schmerz\u00b7ge\u00b7sch\u00fct\u00b7tel\u00b7ten", "noch", "La\u00b7bung", "bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Traums\u00fc\u00dfer Ruhe frohbewegten Frieden", "tokens": ["Traum\u00b7s\u00fc\u00b7\u00dfer", "Ru\u00b7he", "froh\u00b7be\u00b7weg\u00b7ten", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nach qualdurchst\u00fcrmter, herzensbanger Noth!", "tokens": ["Nach", "qual\u00b7durchs\u00b7t\u00fcrm\u00b7ter", ",", "her\u00b7zens\u00b7ban\u00b7ger", "Noth", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Hirsch nach Wasser streif\u2019 ich auf der Erde,", "tokens": ["Ein", "Hirsch", "nach", "Was\u00b7ser", "streif'", "ich", "auf", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der durstgequ\u00e4lt um k\u00fchle Letzung schreit,", "tokens": ["Der", "durst\u00b7ge\u00b7qu\u00e4lt", "um", "k\u00fch\u00b7le", "Let\u00b7zung", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verh\u00f6hnt, versto\u00dfen von der Bruderheerde,", "tokens": ["Ver\u00b7h\u00f6hnt", ",", "ver\u00b7sto\u00b7\u00dfen", "von", "der", "Bru\u00b7der\u00b7heer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Gehetzt durch grauenvolle Einsamkeit.", "tokens": ["Ge\u00b7hetzt", "durch", "grau\u00b7en\u00b7vol\u00b7le", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wo war ein Ort, da Balsam sich ergossen", "tokens": ["Wo", "war", "ein", "Ort", ",", "da", "Bal\u00b7sam", "sich", "er\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "KOUS", "NE", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "In meiner Wunden tausendf\u00e4lt\u2019gen Brand?", "tokens": ["In", "mei\u00b7ner", "Wun\u00b7den", "tau\u00b7send\u00b7f\u00e4lt'\u00b7gen", "Brand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Wann schlug die Stunde je, da ich erschlossen", "tokens": ["Wann", "schlug", "die", "Stun\u00b7de", "je", ",", "da", "ich", "er\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Das stets umsonst gesuchte Eden fand?", "tokens": ["Das", "stets", "um\u00b7sonst", "ge\u00b7such\u00b7te", "E\u00b7den", "fand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der Wahrheit treu seit meinem ersten F\u00fchlen", "tokens": ["Der", "Wahr\u00b7heit", "treu", "seit", "mei\u00b7nem", "ers\u00b7ten", "F\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Brach ich zusammen, oft ein irrend Kind,", "tokens": ["Brach", "ich", "zu\u00b7sam\u00b7men", ",", "oft", "ein", "ir\u00b7rend", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Was konnte mir die Flammenstirne k\u00fchlen,", "tokens": ["Was", "konn\u00b7te", "mir", "die", "Flam\u00b7mens\u00b7tir\u00b7ne", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Dem tausend Formen tausend Sphinxe sind?", "tokens": ["Dem", "tau\u00b7send", "For\u00b7men", "tau\u00b7send", "Sphin\u00b7xe", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "CARD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Zum Fremdling dieser Erde ward erschaffen,", "tokens": ["Zum", "Fremd\u00b7ling", "die\u00b7ser", "Er\u00b7de", "ward", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wen tiefstes Geistessehnen ganz erf\u00fcllt,", "tokens": ["Wen", "tiefs\u00b7tes", "Geis\u00b7tes\u00b7seh\u00b7nen", "ganz", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Von Ort zu Ort mu\u00df er sich einsam raffen,", "tokens": ["Von", "Ort", "zu", "Ort", "mu\u00df", "er", "sich", "ein\u00b7sam", "raf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Vom Trauerflor der Schweigsamkeit verh\u00fcllt.", "tokens": ["Vom", "Trau\u00b7er\u00b7flor", "der", "Schweig\u00b7sam\u00b7keit", "ver\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Vor G\u00f6tzenbildern sieht er niedersinken", "tokens": ["Vor", "G\u00f6t\u00b7zen\u00b7bil\u00b7dern", "sieht", "er", "nie\u00b7der\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Der Lichtgebor\u2019nen wahnbeth\u00f6rte Schaar,", "tokens": ["Der", "Licht\u00b7ge\u00b7bor'\u00b7nen", "wahn\u00b7be\u00b7th\u00f6r\u00b7te", "Schaar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Die L\u00fcge sieht er durch die Menge hinken", "tokens": ["Die", "L\u00fc\u00b7ge", "sieht", "er", "durch", "die", "Men\u00b7ge", "hin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "In schillerndem, vielfaltigem Talar.", "tokens": ["In", "schil\u00b7lern\u00b7dem", ",", "viel\u00b7fal\u00b7ti\u00b7gem", "Ta\u00b7lar", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Er will ein Retter, will ein Heiland werden", "tokens": ["Er", "will", "ein", "Ret\u00b7ter", ",", "will", "ein", "Hei\u00b7land", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "VMFIN", "ART", "NN", "VAINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Und weist empor den Pfad aus Nacht zum Licht,", "tokens": ["Und", "weist", "em\u00b7por", "den", "Pfad", "aus", "Nacht", "zum", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "ART", "NN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Er tr\u00e4gt des Kreuzes heilige Beschwerden", "tokens": ["Er", "tr\u00e4gt", "des", "Kreu\u00b7zes", "hei\u00b7li\u00b7ge", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Und k\u00e4mpft voran, bis Schwert und Leben bricht.", "tokens": ["Und", "k\u00e4mpft", "vo\u00b7ran", ",", "bis", "Schwert", "und", "Le\u00b7ben", "bricht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "So leb\u2019 denn wohl! Wenn mit dem Fl\u00f6tenklange", "tokens": ["So", "leb'", "denn", "wohl", "!", "Wenn", "mit", "dem", "Fl\u00f6\u00b7ten\u00b7klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$.", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Des Fl\u00fcgels weichste T\u00f6ne sich verm\u00e4hlt,", "tokens": ["Des", "Fl\u00fc\u00b7gels", "weichs\u00b7te", "T\u00f6\u00b7ne", "sich", "ver\u00b7m\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Wenn leise, wehmuthvolle Lieder bange", "tokens": ["Wenn", "lei\u00b7se", ",", "weh\u00b7muth\u00b7vol\u00b7le", "Lie\u00b7der", "ban\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADJD", "$,", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Und doch so s\u00fc\u00df mich tr\u00e4umerisch beseelt,", "tokens": ["Und", "doch", "so", "s\u00fc\u00df", "mich", "tr\u00e4u\u00b7me\u00b7risch", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Da hat ein selt\u2019ner Gott sich eingefunden", "tokens": ["Da", "hat", "ein", "selt'\u00b7ner", "Gott", "sich", "ein\u00b7ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und gnadenvoll sich \u00fcber mich geneigt,", "tokens": ["Und", "gna\u00b7den\u00b7voll", "sich", "\u00fc\u00b7ber", "mich", "ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Da hab\u2019 auch ich das traute Gl\u00fcck empfunden,", "tokens": ["Da", "hab'", "auch", "ich", "das", "trau\u00b7te", "Gl\u00fcck", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "PDS", "VVFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Das allzuschnell sonst meinem Blick entweicht.", "tokens": ["Das", "all\u00b7zu\u00b7schnell", "sonst", "mei\u00b7nem", "Blick", "ent\u00b7weicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Ihr kennt es nicht, die ihr parfum-umf\u00e4chelt", "tokens": ["Ihr", "kennt", "es", "nicht", ",", "die", "ihr", "pa\u00b7rfum\u00b7um\u00b7f\u00e4\u00b7chelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "So glatt wie hohl ein wenig \u201eLeben\u201c spielt,", "tokens": ["So", "glatt", "wie", "hohl", "ein", "we\u00b7nig", "\u201e", "Le\u00b7ben", "\u201c", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "ART", "PIAT", "$(", "NN", "$(", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Nur wem im Wettersturm die Sonne l\u00e4chelt,", "tokens": ["Nur", "wem", "im", "Wet\u00b7ter\u00b7sturm", "die", "Son\u00b7ne", "l\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Der K\u00e4mpfer nur hat einzig es gef\u00fchlt!", "tokens": ["Der", "K\u00e4mp\u00b7fer", "nur", "hat", "ein\u00b7zig", "es", "ge\u00b7f\u00fchlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}