{"dta.poem.5440": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eigentliche Ehre des Sch\u00f6pfers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unm\u00f6glich kann ich mich entlegen,", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "kann", "ich", "mich", "ent\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu unsers grossen Sch\u00f6pfers Ehr\u2019,", "tokens": ["Zu", "un\u00b7sers", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von seiner Ehre noch was mehr,", "tokens": ["Von", "sei\u00b7ner", "Eh\u00b7re", "noch", "was", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PWS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als bi\u00df dahero, zu erwegen.", "tokens": ["Als", "bi\u00df", "da\u00b7he\u00b7ro", ",", "zu", "er\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PAV", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu GOttes Ehr\u2019 ist jedermann,", "tokens": ["Zu", "Got\u00b7tes", "Ehr'", "ist", "je\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VAFIN", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie uns Vernunft und Schrift berichtet,", "tokens": ["Wie", "uns", "Ver\u00b7nunft", "und", "Schrift", "be\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So viel man immer wei\u00df und kann,", "tokens": ["So", "viel", "man", "im\u00b7mer", "wei\u00df", "und", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "ADV", "VVFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach aller M\u00f6glichkeit verpflichtet.", "tokens": ["Nach", "al\u00b7ler", "M\u00f6g\u00b7lich\u00b7keit", "ver\u00b7pflich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Allein, bedencket man es recht,", "tokens": ["Al\u00b7lein", ",", "be\u00b7den\u00b7cket", "man", "es", "recht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PIS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So scheint das menschliche Geschlecht,", "tokens": ["So", "scheint", "das", "menschli\u00b7che", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Durch ein ", "tokens": ["Durch", "ein"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "--", "measure": "unknown.measure.zero"}, "line.4": {"text": "Und, blos aus Eigennutz getrieben,", "tokens": ["Und", ",", "blos", "aus", "Ei\u00b7gen\u00b7nutz", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Statt Ehre, die nur GOtt geb\u00fchret,", "tokens": ["Statt", "Eh\u00b7re", ",", "die", "nur", "Gott", "ge\u00b7b\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur in der That sich selbst zu lieben.", "tokens": ["Nur", "in", "der", "That", "sich", "selbst", "zu", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Man glaubt, da\u00df man den Sch\u00f6pfer ehrt,", "tokens": ["Man", "glaubt", ",", "da\u00df", "man", "den", "Sch\u00f6p\u00b7fer", "ehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man ihm danckt, da\u00df er uns n\u00e4hrt,", "tokens": ["Wenn", "man", "ihm", "danckt", ",", "da\u00df", "er", "uns", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn man oft eine Predigt h\u00f6rt,", "tokens": ["Wenn", "man", "oft", "ei\u00b7ne", "Pre\u00b7digt", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn wir, aus br\u00fcnstigem Verlangen,", "tokens": ["Wenn", "wir", ",", "aus", "br\u00fcns\u00b7ti\u00b7gem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Seeligkeit dort zu empfangen,", "tokens": ["Die", "See\u00b7lig\u00b7keit", "dort", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und etwan, hier auf dieser Welt,", "tokens": ["Und", "et\u00b7wan", ",", "hier", "auf", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Beqvemlichkeiten, Ehr\u2019 und Geld,", "tokens": ["Be\u00b7qvem\u00b7lich\u00b7kei\u00b7ten", ",", "Ehr'", "und", "Geld", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit \u00f6fters wiederhohltem Beten,", "tokens": ["Mit", "\u00f6f\u00b7ters", "wie\u00b7der\u00b7hohl\u00b7tem", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vor ihn, zur Fr\u00fch-und Mittags-Zeit,", "tokens": ["Vor", "ihn", ",", "zur", "Fr\u00fch\u00b7\u00b7und", "Mit\u00b7tags\u00b7Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bald mit, bald sonder Andacht, treten.", "tokens": ["Bald", "mit", ",", "bald", "son\u00b7der", "An\u00b7dacht", ",", "tre\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}