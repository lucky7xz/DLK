{"dta.poem.1236": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Rost von Rosen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Roselinde gab Silvandern eine Rose voller Scham/", "tokens": ["Ro\u00b7se\u00b7lin\u00b7de", "gab", "Sil\u00b7van\u00b7dern", "ei\u00b7ne", "Ro\u00b7se", "vol\u00b7ler", "Scham", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df der zarten Rosen Farbe selbst auff ihre Wangen kam:", "tokens": ["Da\u00df", "der", "zar\u00b7ten", "Ro\u00b7sen", "Far\u00b7be", "selbst", "auff", "ih\u00b7re", "Wan\u00b7gen", "kam", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Er mit Seufftzen sprach dargegen: Ach/ k\u00f6nt ich das Gl\u00fcck er-", "tokens": ["Er", "mit", "Seufft\u00b7zen", "sprach", "dar\u00b7ge\u00b7gen", ":", "Ach", "/", "k\u00f6nt", "ich", "das", "Gl\u00fcck", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "VVFIN", "PAV", "$.", "ITJ", "$(", "VMFIN", "PPER", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "heben/", "tokens": ["he\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Da\u00df die Rose/ die mir Rosen giebet/ mir sich wolte geben!", "tokens": ["Da\u00df", "die", "Ro\u00b7se", "/", "die", "mir", "Ro\u00b7sen", "gie\u00b7bet", "/", "mir", "sich", "wol\u00b7te", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$(", "PRELS", "PPER", "NN", "VVFIN", "$(", "PPER", "PRF", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}