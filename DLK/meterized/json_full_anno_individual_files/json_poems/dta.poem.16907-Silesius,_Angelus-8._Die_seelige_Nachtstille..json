{"dta.poem.16907": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "8.  Die seelige Nachtstille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Merk/ in der stillen Nacht wird GOtt ein Kind gebohrn/", "tokens": ["Merk", "/", "in", "der", "stil\u00b7len", "Nacht", "wird", "Gott", "ein", "Kind", "ge\u00b7bohrn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "ART", "ADJA", "NN", "VAFIN", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und widerumh ersetzt was ", "tokens": ["Und", "wi\u00b7de\u00b7rumh", "er\u00b7setzt", "was"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "PWS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ist deine Seele still und dem Gesch\u00f6pffe Nacht/", "tokens": ["Ist", "dei\u00b7ne", "See\u00b7le", "still", "und", "dem", "Ge\u00b7sch\u00f6pf\u00b7fe", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "KON", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird GOtt in dir Mensch/ und alles wiederbracht.", "tokens": ["So", "wird", "Gott", "in", "dir", "Mensch", "/", "und", "al\u00b7les", "wie\u00b7der\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "APPR", "PPER", "NN", "$(", "KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}