{"dta.poem.15834": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, lange ma\u00dfest du die Welt mit Winkelma\u00dfen,", "tokens": ["Freund", ",", "lan\u00b7ge", "ma\u00b7\u00dfest", "du", "die", "Welt", "mit", "Win\u00b7kel\u00b7ma\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und pfl\u00fcgtest als dein Feld die leuchtenden Milchstra\u00dfen.", "tokens": ["Und", "pfl\u00fcg\u00b7test", "als", "dein", "Feld", "die", "leuch\u00b7ten\u00b7den", "Milch\u00b7stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Im Dunkel h\u00fctest du nun gerne deinen Winkel,", "tokens": ["Im", "Dun\u00b7kel", "h\u00fc\u00b7test", "du", "nun", "ger\u00b7ne", "dei\u00b7nen", "Win\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den D\u00fcnkel wirfst du weg, und streust im Garten Dinkel.", "tokens": ["Den", "D\u00fcn\u00b7kel", "wirfst", "du", "weg", ",", "und", "streust", "im", "Gar\u00b7ten", "Din\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Welt Oekonomie hast du als Astronom", "tokens": ["Der", "Welt", "O\u00b7e\u00b7ko\u00b7no\u00b7mie", "hast", "du", "als", "Ast\u00b7ro\u00b7nom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "KOUS", "NN"], "meter": "-++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Betrieben, nun bestell dein Haus als Oekonom.", "tokens": ["Be\u00b7trie\u00b7ben", ",", "nun", "be\u00b7stell", "dein", "Haus", "als", "O\u00b7e\u00b7ko\u00b7nom", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "PPOSAT", "NN", "KOKOM", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}