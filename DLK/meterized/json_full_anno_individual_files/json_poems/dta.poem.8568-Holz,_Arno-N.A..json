{"dta.poem.8568": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf einem Schreibtisch,", "tokens": ["Auf", "ei\u00b7nem", "Schreib\u00b7tisch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "neben einem gr\u00fcnverhangenen Fenster, durch das die Sonne scheint,", "tokens": ["ne\u00b7ben", "ei\u00b7nem", "gr\u00fcn\u00b7ver\u00b7han\u00b7ge\u00b7nen", "Fens\u00b7ter", ",", "durch", "das", "die", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "zwischen zwei B\u00fcsten aus Bisquitporzellan, rechts \u201edie Kunst\u201c,", "tokens": ["zwi\u00b7schen", "zwei", "B\u00fcs\u00b7ten", "aus", "Bis\u00b7quit\u00b7por\u00b7zel\u00b7lan", ",", "rechts", "\u201e", "die", "Kunst", "\u201c", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "CARD", "NN", "APPR", "NE", "$,", "ADV", "$(", "ART", "NN", "$(", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "links \u201edie Wissenschaft\u201c,", "tokens": ["links", "\u201e", "die", "Wis\u00b7sen\u00b7schaft", "\u201c", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "$(", "ART", "NN", "$(", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "liegen in einem marmorirten Pappdeckel, den ich selbst geklebt habe,", "tokens": ["lie\u00b7gen", "in", "ei\u00b7nem", "mar\u00b7mo\u00b7rir\u00b7ten", "Papp\u00b7de\u00b7ckel", ",", "den", "ich", "selbst", "ge\u00b7klebt", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+--+-+-+-+----+-+--", "measure": "iambic.septa.invert"}, "line.6": {"text": "meine ersten Gedichte.", "tokens": ["mei\u00b7ne", "ers\u00b7ten", "Ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Vor ihnen,", "tokens": ["Vor", "ih\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "in seinem Lutherstuhl,", "tokens": ["in", "sei\u00b7nem", "Lu\u00b7ther\u00b7stuhl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "im rotpl\u00fcschnen Schlafrock, unrasiert, die Finger in seiner riesigen Fliege,", "tokens": ["im", "rot\u00b7pl\u00fcschnen", "Schla\u00b7frock", ",", "un\u00b7ra\u00b7siert", ",", "die", "Fin\u00b7ger", "in", "sei\u00b7ner", "rie\u00b7si\u00b7gen", "Flie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADJD", "$,", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "mein lieber, alter, v\u00e4terlicher Freund, Herr Fiebig.", "tokens": ["mein", "lie\u00b7ber", ",", "al\u00b7ter", ",", "v\u00e4\u00b7ter\u00b7li\u00b7cher", "Freund", ",", "Herr", "Fie\u00b7big", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "$,", "ADJA", "$,", "ADJA", "NN", "$,", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "\u201eklinginsherz!\u201c", "tokens": ["\u201e", "klin\u00b7gins\u00b7herz", "!", "\u201c"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Mein erstes Werk \u2014 mein erster Kritiker.", "tokens": ["Mein", "ers\u00b7tes", "Werk", "mein", "ers\u00b7ter", "Kri\u00b7ti\u00b7ker", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich sitze da.", "tokens": ["Ich", "sit\u00b7ze", "da", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ueber der kleinen Schreibzeugvenus aus Cuivre poli", "tokens": ["Ue\u00b7ber", "der", "klei\u00b7nen", "Schreib\u00b7zeug\u00b7ve\u00b7nus", "aus", "Cuiv\u00b7re", "po\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NE", "NE"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "die drei Alabastergrazien als Briefbeschwerer,", "tokens": ["die", "drei", "A\u00b7la\u00b7bas\u00b7ter\u00b7gra\u00b7zi\u00b7en", "als", "Brief\u00b7be\u00b7schwe\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "KOUS", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "dahinter in goldbedruckten Prachtb\u00e4nden, deren Titel mich immer so anziehn.", "tokens": ["da\u00b7hin\u00b7ter", "in", "gold\u00b7be\u00b7druck\u00b7ten", "Pracht\u00b7b\u00e4n\u00b7den", ",", "de\u00b7ren", "Ti\u00b7tel", "mich", "im\u00b7mer", "so", "an\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "$,", "PRELAT", "NN", "PPER", "ADV", "ADV", "VVIZU", "$."], "meter": "-+--+-+-+--+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "\u201edie Wunder der Zeugung\u201c, \u201eLiebe und Ehe\u201c, \u201eDer Mensch und sein Geschlecht\u201c,", "tokens": ["\u201e", "die", "Wun\u00b7der", "der", "Zeu\u00b7gung", "\u201c", ",", "\u201e", "Lie\u00b7be", "und", "E\u00b7he", "\u201c", ",", "\u201e", "Der", "Mensch", "und", "sein", "Ge\u00b7schlecht", "\u201c", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ART", "NN", "$(", "$,", "$(", "NN", "KON", "NN", "$(", "$,", "$(", "ART", "NN", "KON", "PPOSAT", "NN", "$(", "$,"], "meter": "-+--+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "und dr\u00fcben \u2014 zwischen den beiden Schweizerlandschaften \u2014", "tokens": ["und", "dr\u00fc\u00b7ben", "zwi\u00b7schen", "den", "bei\u00b7den", "Schwei\u00b7zer\u00b7land\u00b7schaf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "APPR", "ART", "PIAT", "NN", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "nahezu lebensgross,", "tokens": ["na\u00b7he\u00b7zu", "le\u00b7bens\u00b7gross", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-----", "measure": "dactylic.init"}, "line.7": {"text": "die badende Oeldrucknymphe:", "tokens": ["die", "ba\u00b7den\u00b7de", "O\u00b7el\u00b7druck\u00b7nym\u00b7phe", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "eine blendende Brust, ein sinkendes Tuch, ein err\u00f6tendes L\u00e4cheln,", "tokens": ["ei\u00b7ne", "blen\u00b7den\u00b7de", "Brust", ",", "ein", "sin\u00b7ken\u00b7des", "Tuch", ",", "ein", "er\u00b7r\u00f6\u00b7ten\u00b7des", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Schenkel, wie aus einem Schl\u00e4chterladen!", "tokens": ["Schen\u00b7kel", ",", "wie", "aus", "ei\u00b7nem", "Schl\u00e4ch\u00b7ter\u00b7la\u00b7den", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Meine bedr\u00e4ngten Augen irren angstvoll weiter.", "tokens": ["Mei\u00b7ne", "be\u00b7dr\u00e4ng\u00b7ten", "Au\u00b7gen", "ir\u00b7ren", "angst\u00b7voll", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.8": {"line.1": {"text": "Was wird er sagen?", "tokens": ["Was", "wird", "er", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Sein Daumen, nass gemacht, dreht schon die letzte Seite!", "tokens": ["Sein", "Dau\u00b7men", ",", "nass", "ge\u00b7macht", ",", "dreht", "schon", "die", "letz\u00b7te", "Sei\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "VVPP", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ein leeres Papageienbauer, ein B\u00fccherspind, Ariadne auf Naxos,", "tokens": ["Ein", "lee\u00b7res", "Pa\u00b7pa\u00b7gei\u00b7en\u00b7bau\u00b7er", ",", "ein", "B\u00fc\u00b7cher\u00b7spind", ",", "A\u00b7riad\u00b7ne", "auf", "Na\u00b7xos", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "einem ovalen Alfenidsch\u00e4lchen, gleich obenauf, die Visitenkarte des Hausherrn:", "tokens": ["ei\u00b7nem", "o\u00b7va\u00b7len", "Al\u00b7fe\u00b7nid\u00b7sch\u00e4l\u00b7chen", ",", "gleich", "o\u00b7ben\u00b7auf", ",", "die", "Vi\u00b7si\u00b7ten\u00b7kar\u00b7te", "des", "Haus\u00b7herrn", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-++--+-+-+--+--+-", "measure": "iambic.octa.plus.invert"}, "line.3": {"text": "\u201eredakteur des Herzbl\u00e4ttchen, Zeitschrift f\u00fcr Neuverm\u00e4hlte!\u201c", "tokens": ["\u201e", "re\u00b7dak\u00b7teur", "des", "Herz\u00b7bl\u00e4tt\u00b7chen", ",", "Zeit\u00b7schrift", "f\u00fcr", "Neu\u00b7ver\u00b7m\u00e4hl\u00b7te", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$,", "NN", "APPR", "NN", "$.", "$("], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "Weiter! Oben die Decke! Auch dort!", "tokens": ["Wei\u00b7ter", "!", "O\u00b7ben", "die", "De\u00b7cke", "!", "Auch", "dort", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "ART", "NN", "$.", "ADV", "ADV", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.12": {"line.1": {"text": "Zwischen Veilchen, Rosen und Vergissmeinnicht,", "tokens": ["Zwi\u00b7schen", "Veil\u00b7chen", ",", "Ro\u00b7sen", "und", "Ver\u00b7giss\u00b7mein\u00b7nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "auf einem Tintfass.", "tokens": ["auf", "ei\u00b7nem", "Tint\u00b7fass", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+---", "measure": "dactylic.init"}, "line.3": {"text": "ein dicker, fleischfarbner Amor,", "tokens": ["ein", "di\u00b7cker", ",", "fleischfarb\u00b7ner", "A\u00b7mor", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der, umspielt von Schmetterlingen, mit einer Pfauenfeder in ein Buch schreibt:", "tokens": ["der", ",", "um\u00b7spielt", "von", "Schmet\u00b7ter\u00b7lin\u00b7gen", ",", "mit", "ei\u00b7ner", "Pfau\u00b7en\u00b7fe\u00b7der", "in", "ein", "Buch", "schreibt", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVFIN", "APPR", "NN", "$,", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "\u201eohne Liebe gleicht das Leben einer Rose ohne Duft!\u201c", "tokens": ["\u201e", "oh\u00b7ne", "Lie\u00b7be", "gleicht", "das", "Le\u00b7ben", "ei\u00b7ner", "Ro\u00b7se", "oh\u00b7ne", "Duft", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.13": {"line.1": {"text": "Und ich f\u00fchls:", "tokens": ["Und", "ich", "f\u00fchls", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "ich bin \u00fcber und \u00fcber rot geworden!", "tokens": ["ich", "bin", "\u00fc\u00b7ber", "und", "\u00fc\u00b7ber", "rot", "ge\u00b7wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "KON", "APPR", "ADJD", "VAPP", "$."], "meter": "+++--+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}