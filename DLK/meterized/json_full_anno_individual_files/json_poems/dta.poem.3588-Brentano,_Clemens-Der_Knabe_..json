{"dta.poem.3588": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da droben aufm H\u00fcgel,               ", "tokens": ["Da", "dro\u00b7ben", "aufm", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo die Nachtigall singt,", "tokens": ["Wo", "die", "Nach\u00b7ti\u00b7gall", "singt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da tanzt der Einsiedel,", "tokens": ["Da", "tanzt", "der", "Ein\u00b7sie\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df die Kutt in die H\u00f6b springt.", "tokens": ["Da\u00df", "die", "Kutt", "in", "die", "H\u00f6b", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ey la\u00dft ihn nur tanzen,", "tokens": ["Ey", "la\u00dft", "ihn", "nur", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Ey la\u00df ihn nur seyn;", "tokens": ["Ey", "la\u00df", "ihn", "nur", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "ADV", "VAINF", "$."], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "Zu Nacht mu\u00df er beten,", "tokens": ["Zu", "Nacht", "mu\u00df", "er", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und schlafen allein.", "tokens": ["Und", "schla\u00b7fen", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Da droben aufm H\u00fcgel,", "tokens": ["Da", "dro\u00b7ben", "aufm", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo's F\u00fcchslein drauf lauft;", "tokens": ["Wo's", "F\u00fcchs\u00b7lein", "drauf", "lauft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PAV", "VVFIN", "$."], "meter": "+---+", "measure": "dactylic.init"}, "line.3": {"text": "Da sitzt der Einsiedel,", "tokens": ["Da", "sitzt", "der", "Ein\u00b7sie\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hat d' Kutte verkauft.", "tokens": ["Hat", "d'", "Kut\u00b7te", "ver\u00b7kauft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Da droben aufm H\u00fcgel,", "tokens": ["Da", "dro\u00b7ben", "aufm", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wo die Nachtigall singt,", "tokens": ["Wo", "die", "Nach\u00b7ti\u00b7gall", "singt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da ist es mein Sch\u00e4tzel,", "tokens": ["Da", "ist", "es", "mein", "Sch\u00e4t\u00b7zel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mein allerliebst Kind.", "tokens": ["Mein", "al\u00b7ler\u00b7liebst", "Kind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Der Einsiedel auf dem Zitterbaum,", "tokens": ["Der", "Ein\u00b7sie\u00b7del", "auf", "dem", "Zit\u00b7ter\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Der schaute wo der Tag her kam.", "tokens": ["Der", "schau\u00b7te", "wo", "der", "Tag", "her", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PWAV", "ART", "NN", "APZR", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Tag, der kommt vom Morgenstern,", "tokens": ["Der", "Tag", ",", "der", "kommt", "vom", "Mor\u00b7gens\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Bei meinem Liebchen bin ich gern.", "tokens": ["Bei", "mei\u00b7nem", "Lieb\u00b7chen", "bin", "ich", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}