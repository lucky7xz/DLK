{"textgrid.poem.39092": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sechsunddrei\u00dfig Vaterl\u00e4nder", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sechsunddrei\u00dfig Vaterl\u00e4nder", "tokens": ["Sech\u00b7sund\u00b7drei\u00b7\u00dfig", "Va\u00b7ter\u00b7l\u00e4n\u00b7der"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hatte sonst der gute Deutsche,", "tokens": ["hat\u00b7te", "sonst", "der", "gu\u00b7te", "Deut\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sechsunddrei\u00dfig bunte B\u00e4nder", "tokens": ["sech\u00b7sund\u00b7drei\u00b7\u00dfig", "bun\u00b7te", "B\u00e4n\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "flicht man jetzt zu einer Peitsche:", "tokens": ["flicht", "man", "jetzt", "zu", "ei\u00b7ner", "Peit\u00b7sche", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einer Peitsche, leicht und zierlich,", "tokens": ["Ei\u00b7ner", "Peit\u00b7sche", ",", "leicht", "und", "zier\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "um die Ohren uns zu wippen,", "tokens": ["um", "die", "Oh\u00b7ren", "uns", "zu", "wip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wenn der Mund zu unmanierlich,", "tokens": ["wenn", "der", "Mund", "zu", "un\u00b7ma\u00b7nier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wenn zu plauderhaft die Lippen.", "tokens": ["wenn", "zu", "plau\u00b7der\u00b7haft", "die", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKA", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, ich schw\u00f6r's bei diesem Blute,", "tokens": ["Ja", ",", "ich", "schw\u00f6r's", "bei", "die\u00b7sem", "Blu\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "halten wird man, was versprochen,", "tokens": ["hal\u00b7ten", "wird", "man", ",", "was", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVINF", "VAFIN", "PIS", "$,", "PWS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wird uns mit derselben Knute,", "tokens": ["wird", "uns", "mit", "der\u00b7sel\u00b7ben", "Knu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "einheitselig, unterjochen!", "tokens": ["ein\u00b7heit\u00b7se\u00b7lig", ",", "un\u00b7ter\u00b7jo\u00b7chen", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Einen Kaiser, ohne Zweifel", "tokens": ["Ei\u00b7nen", "Kai\u00b7ser", ",", "oh\u00b7ne", "Zwei\u00b7fel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "werden ebenfalls wir kriegen:", "tokens": ["wer\u00b7den", "e\u00b7ben\u00b7falls", "wir", "krie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nikolaus, den gro\u00dfen Teufel \u2013", "tokens": ["Ni\u00b7ko\u00b7laus", ",", "den", "gro\u00b7\u00dfen", "Teu\u00b7fel", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dann wird die Freiheit siegen!", "tokens": ["Und", "dann", "wird", "die", "Frei\u00b7heit", "sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sechsunddrei\u00dfig Vaterl\u00e4nder", "tokens": ["Sech\u00b7sund\u00b7drei\u00b7\u00dfig", "Va\u00b7ter\u00b7l\u00e4n\u00b7der"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "hatte sonst der gute Deutsche,", "tokens": ["hat\u00b7te", "sonst", "der", "gu\u00b7te", "Deut\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "sechsunddrei\u00dfig bunte B\u00e4nder", "tokens": ["sech\u00b7sund\u00b7drei\u00b7\u00dfig", "bun\u00b7te", "B\u00e4n\u00b7der"], "token_info": ["word", "word", "word"