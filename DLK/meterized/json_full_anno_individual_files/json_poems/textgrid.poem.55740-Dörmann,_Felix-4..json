{"textgrid.poem.55740": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schl\u00e4ft in Deinem Auge", "tokens": ["Es", "schl\u00e4ft", "in", "Dei\u00b7nem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Liebeswahnsinn-Atom,", "tokens": ["Ein", "Lie\u00b7bes\u00b7wahn\u00b7sinn\u00b7A\u00b7tom", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch scheut und f\u00fcrchtet die Seele", "tokens": ["Noch", "scheut", "und", "f\u00fcrch\u00b7tet", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den tosenden Flammenstrom.", "tokens": ["Den", "to\u00b7sen\u00b7den", "Flam\u00b7mens\u00b7trom", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich aber will ihn entfesseln,", "tokens": ["Ich", "a\u00b7ber", "will", "ihn", "ent\u00b7fes\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und sei es mit frevelnder Faust,", "tokens": ["Und", "sei", "es", "mit", "fre\u00b7veln\u00b7der", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Ich will, da\u00df sein Gluthauch bet\u00f6rend", "tokens": ["Ich", "will", ",", "da\u00df", "sein", "Glut\u00b7hauch", "be\u00b7t\u00f6\u00b7rend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPOSAT", "NN", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Zwei selige Menschen umsaust.", "tokens": ["Zwei", "se\u00b7li\u00b7ge", "Men\u00b7schen", "um\u00b7saust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVFIN", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Auch Deine Seele d\u00fcrstet", "tokens": ["Auch", "Dei\u00b7ne", "See\u00b7le", "d\u00fcrs\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nach einem Liebesmeer,", "tokens": ["Nach", "ei\u00b7nem", "Lie\u00b7bes\u00b7meer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Es brennen und gl\u00fchn Deine H\u00e4nde, \u2013", "tokens": ["Es", "bren\u00b7nen", "und", "gl\u00fchn", "Dei\u00b7ne", "H\u00e4n\u00b7de", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "PPOSAT", "NN", "$,", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Dein Herz gl\u00fcht tausendmal mehr.", "tokens": ["Dein", "Herz", "gl\u00fcht", "tau\u00b7send\u00b7mal", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.13": {"text": "Du schweigst und Du zitterst, Lucia,", "tokens": ["Du", "schweigst", "und", "Du", "zit\u00b7terst", ",", "Lu\u00b7cia", ","], "token_inf