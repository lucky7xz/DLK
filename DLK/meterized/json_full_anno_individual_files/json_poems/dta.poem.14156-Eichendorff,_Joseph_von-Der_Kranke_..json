{"dta.poem.14156": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Kranke .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft mich, Gespenster,", "tokens": ["La\u00dft", "mich", ",", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Lied, riegl' auf die Gruft!", "tokens": ["Lied", ",", "riegl'", "auf", "die", "Gruft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Oeffnet die Fenster,", "tokens": ["Oeff\u00b7net", "die", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Luft, frische freie Luft!", "tokens": ["Luft", ",", "fri\u00b7sche", "frei\u00b7e", "Luft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}