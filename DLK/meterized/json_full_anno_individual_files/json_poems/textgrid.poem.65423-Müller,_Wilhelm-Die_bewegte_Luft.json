{"textgrid.poem.65423": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die bewegte Luft", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist's ein Wunder, da\u00df die Luft nie bei uns kann ruhig werden?", "tokens": ["Ist's", "ein", "Wun\u00b7der", ",", "da\u00df", "die", "Luft", "nie", "bei", "uns", "kann", "ru\u00b7hig", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "KOUS", "ART", "NN", "ADV", "APPR", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da\u00df die Winde nimmermehr auf sich schwingen von der Erden?", "tokens": ["Da\u00df", "die", "Win\u00b7de", "nim\u00b7mer\u00b7mehr", "auf", "sich", "schwin\u00b7gen", "von", "der", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PRF", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Wi\u00dft, so lange diese Locken ihnen sind verg\u00f6nnt zum Spiel,", "tokens": ["Wi\u00dft", ",", "so", "lan\u00b7ge", "die\u00b7se", "Lo\u00b7cken", "ih\u00b7nen", "sind", "ver\u00b7g\u00f6nnt", "zum", "Spiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ADV", "PDAT", "NN", "PPER", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Finden sie bei Nacht und Tage nimmer ihres Wehens Ziel.", "tokens": ["Fin\u00b7den", "sie", "bei", "Nacht", "und", "Ta\u00b7ge", "nim\u00b7mer", "