{"textgrid.poem.64222": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Herr Guzzo vom Gauchen aus Bayerland", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem Bergland der Bavaren,", "tokens": ["Aus", "dem", "Berg\u00b7land", "der", "Ba\u00b7va\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Loisach leuchtend rinnt,", "tokens": ["Wo", "die", "Loi\u00b7sach", "leuch\u00b7tend", "rinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weit nach Ostnordost verfahren,", "tokens": ["Weit", "nach", "Ost\u00b7nor\u00b7dost", "ver\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat mich zu den Pelzbarbaren", "tokens": ["Hat", "mich", "zu", "den", "Pelz\u00b7bar\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ungelind ein Wetterwind.", "tokens": ["Un\u00b7ge\u00b7lind", "ein", "Wet\u00b7ter\u00b7wind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ist viel davon zu melden!", "tokens": ["Was", "ist", "viel", "da\u00b7von", "zu", "mel\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gro\u00dfe Herren fallen weich:", "tokens": ["Gro\u00b7\u00dfe", "Her\u00b7ren", "fal\u00b7len", "weich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch wir schimmerlosen Helden,", "tokens": ["Doch", "wir", "schim\u00b7mer\u00b7lo\u00b7sen", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wir verderben's mit Frau S\u00e4lden", "tokens": ["Wir", "ver\u00b7der\u00b7ben's", "mit", "Frau", "S\u00e4l\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Leicht bei jedem lust'gen Streich. \u2013", "tokens": ["Leicht", "bei", "je\u00b7dem", "lust'\u00b7gen", "Streich", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "PIAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf mein Schlo\u00df im Loisachgrunde", "tokens": ["Auf", "mein", "Schlo\u00df", "im", "Loi\u00b7sach\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schickt ein wackrer Trinkgesell", "tokens": ["Schickt", "ein", "wack\u00b7rer", "Trink\u00b7ge\u00b7sell"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir geheim vertraute Kunde,", "tokens": ["Mir", "ge\u00b7heim", "ver\u00b7trau\u00b7te", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie und wo \u2013 zu welcher Stunde \u2013 ...", "tokens": ["Wie", "und", "wo", "\u2013", "zu", "wel\u00b7cher", "Stun\u00b7de", "\u2013", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "KON", "PWAV", "$(", "APPR", "PWAT", "NN", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Rechter Zeit war ich zur Stell'. \u2013", "tokens": ["Rech\u00b7ter", "Zeit", "war", "ich", "zur", "Stell'", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Was braucht allen R\u00fcdesheimer", "tokens": ["Was", "braucht", "al\u00b7len", "R\u00fc\u00b7des\u00b7hei\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Salzburgs Bischof ganz allein!", "tokens": ["Salz\u00b7burgs", "Bi\u00b7schof", "ganz", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alter Litaneienreimer,", "tokens": ["Al\u00b7ter", "Li\u00b7ta\u00b7nei\u00b7en\u00b7rei\u00b7mer", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dacht' ich, diese zwanzig Eimer", "tokens": ["Dacht'", "ich", ",", "die\u00b7se", "zwan\u00b7zig", "Ei\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PDAT", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Bring' ich in die Gauchburg ein.", "tokens": ["Bring'", "ich", "in", "die", "Gauch\u00b7burg", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Tief im Tann bei Traunstein lagen", "tokens": ["Tief", "im", "Tann", "bei", "Traun\u00b7stein", "la\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wir mit achtzehn Lanzen still:", "tokens": ["Wir", "mit", "acht\u00b7zehn", "Lan\u00b7zen", "still", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Langsam rumpeln an die Wagen: \u2013", "tokens": ["Lang\u00b7sam", "rum\u00b7peln", "an", "die", "Wa\u00b7gen", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wir drauf los: doch wie ich schlagen", "tokens": ["Wir", "drauf", "los", ":", "doch", "wie", "ich", "schla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PAV", "PTKVZ", "$.", "ADV", "KOKOM", "PPER", "VVINF"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Just vom Gaul den F\u00fchrer will, \u2013", "tokens": ["Just", "vom", "Gaul", "den", "F\u00fch\u00b7rer", "will", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "VMFIN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Merk' ich's an dem Scharlachb\u00e4fflein:", "tokens": ["Merk'", "ich's", "an", "dem", "Schar\u00b7lach\u00b7b\u00e4f\u00b7flein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Bischof Bumpo selbst war das", "tokens": ["Bi\u00b7schof", "Bum\u00b7po", "selbst", "war", "das"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "VAFIN", "ART"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Schau', selbst f\u00fchrt den Wein das! Pf\u00e4fflein! \u2013", "tokens": ["Schau'", ",", "selbst", "f\u00fchrt", "den", "Wein", "das", "!", "Pf\u00e4f\u00b7flein", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "$.", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun, da half nichts! ein klein Trefflein", "tokens": ["Nun", ",", "da", "half", "nichts", "!", "ein", "klein", "Tref\u00b7flein"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PIS", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit der Faust: \u2013 weich war das Gras! \u2013 \u2013", "tokens": ["Mit", "der", "Faust", ":", "\u2013", "weich", "war", "das", "Gras", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "ADJD", "VAFIN", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Kaum vertrunken und verschlafen", "tokens": ["Kaum", "ver\u00b7trun\u00b7ken", "und", "ver\u00b7schla\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "War der Wein \u2013 Gott segne ihn! \u2013", "tokens": ["War", "der", "Wein", "\u2013", "Gott", "seg\u00b7ne", "ihn", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "$(", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als beim Marquartsteiner Grafen", "tokens": ["Als", "beim", "Mar\u00b7quart\u00b7stei\u00b7ner", "Gra\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbwegraub! Friedbruch! Zeter! W\u00e2fen!\u00ab", "tokens": ["\u00bb", "we\u00b7graub", "!", "Fried\u00b7bruch", "!", "Ze\u00b7ter", "!", "W\u00e2\u00b7fen", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "NN", "$.", "NN", "$.", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle Durst'gen Salzburgs schrien.", "tokens": ["Al\u00b7le", "Dur\u00b7st'\u00b7gen", "Salz\u00b7burgs", "schri\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NE", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "K\u00f6nig Rudolf lie\u00df mir sagen:", "tokens": ["K\u00f6\u00b7nig", "Ru\u00b7dolf", "lie\u00df", "mir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbguzegauch, das war zu stark!", "tokens": ["\u00bb", "gu\u00b7ze\u00b7gauch", ",", "das", "war", "zu", "stark", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PDS", "VAFIN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4tt'st du nicht so fest geschlagen", "tokens": ["H\u00e4tt'st", "du", "nicht", "so", "fest", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst im Marchfeld, g\u00e4lt's den Kragen! \u2013", "tokens": ["Einst", "im", "March\u00b7feld", ",", "g\u00e4lt's", "den", "Kra\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zieh' dich flugs gen Preu\u00dfenmark!\u00ab", "tokens": ["Zieh'", "dich", "flugs", "gen", "Preu\u00b7\u00dfen\u00b7mark", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "NE", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Anfangs wollt' mich's schwer verdreu\u00dfen.", "tokens": ["An\u00b7fangs", "wollt'", "mich's", "schwer", "ver\u00b7dreu\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Bischofsburzelbaum", "tokens": ["Um", "den", "Bi\u00b7schofs\u00b7bur\u00b7zel\u00b7baum"], "token_info": ["word", "word", "word"], "pos": ["KOUI", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich bis Heidenland! bis Preu\u00dfen!", "tokens": ["Gleich", "bis", "Hei\u00b7den\u00b7land", "!", "bis", "Preu\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$.", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ob dort auch Tropfen fleu\u00dfen,", "tokens": ["Und", "ob", "dort", "auch", "Trop\u00b7fen", "fleu\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Die ein Mann mag trinken? \u2013 \u2013 kaum!", "tokens": ["Die", "ein", "Mann", "mag", "trin\u00b7ken", "?", "\u2013", "\u2013", "kaum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "VMFIN", "VVINF", "$.", "$(", "$(", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun, so schlimm ist's nicht geworden.", "tokens": ["Nun", ",", "so", "schlimm", "ist's", "nicht", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJD", "VAFIN", "PTKNEG", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwar das Land: \u2013 \u2013 ein arg flach Moor!", "tokens": ["Zwar", "das", "Land", ":", "\u2013", "\u2013", "ein", "arg", "flach", "Moor", "!"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "$(", "$(", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch mir taugt der tapfre Orden:", "tokens": ["Doch", "mir", "taugt", "der", "tapf\u00b7re", "Or\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich im Kampf tut's uns der Norden,", "tokens": ["Gleich", "im", "Kampf", "tut's", "uns", "der", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tut's im Trunk uns noch zuvor! \u2013 \u2013", "tokens": ["Tut's", "im", "Trunk", "uns", "noch", "zu\u00b7vor", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "ADV", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Aber freilich, ganz vor'm Ende", "tokens": ["A\u00b7ber", "frei\u00b7lich", ",", "ganz", "vor'm", "En\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' ich einmal schauen noch", "tokens": ["M\u00f6cht'", "ich", "ein\u00b7mal", "schau\u00b7en", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fchn im Abendgoldgeblende", "tokens": ["Gl\u00fchn", "im", "A\u00b7bend\u00b7gold\u00b7ge\u00b7blen\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure stolzen Schroffenw\u00e4nde,", "tokens": ["Eu\u00b7re", "stol\u00b7zen", "Schrof\u00b7fen\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Torstein und Karwendeljoch! \u2013", "tokens": ["Tor\u00b7stein", "und", "Kar\u00b7wen\u00b7del\u00b7joch", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$