", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "NN", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Die ein Mann mag trinken? \u2013 \u2013 kaum!", "tokens": ["Die", "ein", "Mann", "mag", "trin\u00b7ken", "?", "\u2013", "\u2013", "kaum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["ART", "ART", "NN", "VMFIN", "VVINF", "$.", "$(", "$(", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Nun, so schlimm ist's nicht geworden.", "tokens": ["Nun", ",", "so", "schlimm", "ist's", "nicht", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADJD", "VAFIN", "PTKNEG", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwar das Land: \u2013 \u2013 ein arg flach Moor!", "tokens": ["Zwar", "das", "Land", ":", "\u2013", "\u2013", "ein", "arg", "flach", "Moor", "!"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "$(", "$(", "ART", "ADJD", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch mir taugt der tapfre Orden:", "tokens": ["Doch", "mir", "taugt", "der", "tapf\u00b7re", "Or\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleich im Kampf tut's uns der Norden,", "tokens": ["Gleich", "im", "Kampf", "tut's", "uns", "der", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tut's im Trunk uns noch zuvor! \u2013 \u2013", "tokens": ["Tut's", "im", "Trunk", "uns", "noch", "zu\u00b7vor", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "ADV", "ADV", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Aber freilich, ganz vor'm Ende", "tokens": ["A\u00b7ber", "frei\u00b7lich", ",", "ganz", "vor'm", "En\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6cht' ich einmal schauen noch", "tokens": ["M\u00f6cht'", "ich", "ein\u00b7mal", "schau\u00b7en", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fchn im Abendgoldgeblende", "tokens": ["Gl\u00fchn", "im", "A\u00b7bend\u00b7gold\u00b7ge\u00b7blen\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure stolzen Schroffenw\u00e4nde,", "tokens": ["Eu\u00b7re", "stol\u00b7zen", "Schrof\u00b7fen\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Torstein und Karwendeljoch! \u2013", "tokens": ["Tor\u00b7stein", "und", "Kar\u00b7wen\u00b7del\u00b7joch", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$.", "$("], "meter": "++-+--+", "measure": "iambic.tetra.chol"}}}}}