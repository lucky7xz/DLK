{"textgrid.poem.64337": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "58. An die Sonne", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sonne, dir jauchzet, bei deinem Erwachen, der Erdkreis entgegen,", "tokens": ["Son\u00b7ne", ",", "dir", "jauch\u00b7zet", ",", "bei", "dei\u00b7nem", "Er\u00b7wa\u00b7chen", ",", "der", "Erd\u00b7kreis", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Dir das Wogenger\u00e4usch des erdumg\u00fcrtenden Meeres!", "tokens": ["Dir", "das", "Wo\u00b7gen\u00b7ge\u00b7r\u00e4usch", "des", "er\u00b7dum\u00b7g\u00fcr\u00b7ten\u00b7den", "Mee\u00b7res", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Fliehend rollet der Wagen der Nacht, in nichtige Wolken", "tokens": ["Flie\u00b7hend", "rol\u00b7let", "der", "Wa\u00b7gen", "der", "Nacht", ",", "in", "nich\u00b7ti\u00b7ge", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Eingeh\u00fcllet, und schwindet hinab in die schauernde Tiefe!", "tokens": ["Ein\u00b7ge\u00b7h\u00fcl\u00b7let", ",", "und", "schwin\u00b7det", "hin\u00b7ab", "in", "die", "schau\u00b7ern\u00b7de", "Tie\u00b7fe", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.5": {"text": "Segnend strahlst du herauf, und br\u00e4utlich kr\u00e4nzet die Erde", "tokens": ["Seg\u00b7nend", "strahlst", "du", "her\u00b7auf", ",", "und", "br\u00e4ut\u00b7lich", "kr\u00e4n\u00b7zet", "die", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Dir die flammenden Schl\u00e4fen mit tauendem Purpurgew\u00f6lke!", "tokens": ["Dir", "die", "flam\u00b7men\u00b7den", "Schl\u00e4\u00b7fen", "mit", "tau\u00b7en\u00b7dem", "Pur\u00b7pur\u00b7ge\u00b7w\u00f6l\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Alles freuet sich dein! In schimmernde Feiergewande", "tokens": ["Al\u00b7les", "freu\u00b7et", "sich", "dein", "!", "In", "schim\u00b7mern\u00b7de", "Fei\u00b7er\u00b7ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PRF", "PPOSAT", "$.", "APPR", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Kleidest du den Himmel, die Erd' und die Fluten des Meeres!", "tokens": ["Klei\u00b7dest", "du", "den", "Him\u00b7mel", ",", "die", "Erd'", "und", "die", "Flu\u00b7ten", "des", "Mee\u00b7res", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.9": {"text": "Siehe, du leitest am rosigen G\u00e4ngelbande den jungen", "tokens": ["Sie\u00b7he", ",", "du", "lei\u00b7test", "am", "ro\u00b7si\u00b7gen", "G\u00e4n\u00b7gel\u00b7ban\u00b7de", "den", "jun\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Freundlichen Tag; er h\u00fcllt sich in deine Safrangewande:", "tokens": ["Freund\u00b7li\u00b7chen", "Tag", ";", "er", "h\u00fcllt", "sich", "in", "dei\u00b7ne", "Saf\u00b7ran\u00b7ge\u00b7wan\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Aber, wie wachsen so schnell die Kr\u00e4fte des himmlischen J\u00fcnglings!", "tokens": ["A\u00b7ber", ",", "wie", "wach\u00b7sen", "so", "schnell", "die", "Kr\u00e4f\u00b7te", "des", "himm\u00b7li\u00b7schen", "J\u00fcng\u00b7lings", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "VVFIN", "ADV", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Feuriger blickt er, und greift nach deinem strahlenden K\u00f6cher,", "tokens": ["Feu\u00b7ri\u00b7ger", "blickt", "er", ",", "und", "greift", "nach", "dei\u00b7nem", "strah\u00b7len\u00b7den", "K\u00f6\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.13": {"text": "Und schon schnellt er vom goldenen Bogen flammende Pfeile!", "tokens": ["Und", "schon", "schnellt", "er", "vom", "gol\u00b7de\u00b7nen", "Bo\u00b7gen", "flam\u00b7men\u00b7de", "Pfei\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Z\u00fcrne, Himmlischer, nicht! und soll dein Bogen ert\u00f6nen,", "tokens": ["Z\u00fcr\u00b7ne", ",", "Himm\u00b7li\u00b7scher", ",", "nicht", "!", "und", "soll", "dein", "Bo\u00b7gen", "er\u00b7t\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PTKNEG", "$.", "KON", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.15": {"text": "O, so richte dein furchtbar Gescho\u00df auf des Oceans Fluten,", "tokens": ["O", ",", "so", "rich\u00b7te", "dein", "furcht\u00b7bar", "Ge\u00b7scho\u00df", "auf", "des", "O\u00b7ceans", "Flu\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPOSAT", "ADJD", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Auf der schneeichten Alpen herunterschmelzende Gipfel,", "tokens": ["Auf", "der", "schne\u00b7eich\u00b7ten", "Al\u00b7pen", "her\u00b7un\u00b7ter\u00b7schmel\u00b7zen\u00b7de", "Gip\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.17": {"text": "Und auf sandige W\u00fcsten, die L\u00f6wen und Tiger durchirren!", "tokens": ["Und", "auf", "san\u00b7di\u00b7ge", "W\u00fcs\u00b7ten", ",", "die", "L\u00f6\u00b7wen", "und", "Ti\u00b7ger", "durch\u00b7ir\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Z\u00fcrne, Himmlischer, nicht! Dir flehn der V\u00f6gel Ges\u00e4nge;", "tokens": ["Z\u00fcr\u00b7ne", ",", "Himm\u00b7li\u00b7scher", ",", "nicht", "!", "Dir", "flehn", "der", "V\u00f6\u00b7gel", "Ge\u00b7s\u00e4n\u00b7ge", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PTKNEG", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.19": {"text": "Dir der s\u00e4uselnde Wald, und dir die duftende Blume!", "tokens": ["Dir", "der", "s\u00e4u\u00b7seln\u00b7de", "Wald", ",", "und", "dir", "die", "duf\u00b7ten\u00b7de", "Blu\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "KON", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Wollest nicht des wehenden Zephyrs Fl\u00fcgel versengen!", "tokens": ["Wol\u00b7lest", "nicht", "des", "we\u00b7hen\u00b7den", "Ze\u00b7phyrs", "Fl\u00fc\u00b7gel", "ver\u00b7sen\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.21": {"text": "Wollest nicht austrinken das Labsal k\u00fchlender Quellen!", "tokens": ["Wol\u00b7lest", "nicht", "aus\u00b7trin\u00b7ken", "das", "Lab\u00b7sal", "k\u00fch\u00b7len\u00b7der", "Quel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVIZU", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Wollest vom zarten Gr\u00e4schen den kr\u00fcmmenden Tropfen nicht nehmen!", "tokens": ["Wol\u00b7lest", "vom", "zar\u00b7ten", "Gr\u00e4sc\u00b7hen", "den", "kr\u00fcm\u00b7men\u00b7den", "Trop\u00b7fen", "nicht", "neh\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Sonne, l\u00e4chle der Erd', und geu\u00df aus strahlender Urne", "tokens": ["Son\u00b7ne", ",", "l\u00e4ch\u00b7le", "der", "Erd'", ",", "und", "geu\u00df", "aus", "strah\u00b7len\u00b7der", "Ur\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Leben auf die Natur! Du hast die F\u00fclle des Lebens;", "tokens": ["Le\u00b7ben", "auf", "die", "Na\u00b7tur", "!", "Du", "hast", "die", "F\u00fcl\u00b7le", "des", "Le\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Sch\u00f6pfest, n\u00e4her dem Himmel, aus himmlischen Quellen und d\u00fcrstest", "tokens": ["Sch\u00f6p\u00b7fest", ",", "n\u00e4\u00b7her", "dem", "Him\u00b7mel", ",", "aus", "himm\u00b7li\u00b7schen", "Quel\u00b7len", "und", "d\u00fcrs\u00b7test"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "ART", "NN", "$,", "APPR", "ADJA", "NN", "KON", "VVFIN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Selber nimmer! Als Gott aus seiner g\u00fcrtenden Allmacht,", "tokens": ["Sel\u00b7ber", "nim\u00b7mer", "!", "Als", "Gott", "aus", "sei\u00b7ner", "g\u00fcr\u00b7ten\u00b7den", "All\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "KOUS", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Wie aus g\u00fcrtendem Schlauch ein S\u00e4mann, Sonnen dahinwarf,", "tokens": ["Wie", "aus", "g\u00fcr\u00b7ten\u00b7dem", "Schlauch", "ein", "S\u00e4\u00b7mann", ",", "Son\u00b7nen", "da\u00b7hin\u00b7warf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "ART", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Millionen auf ", "tokens": ["Mil\u00b7lion\u00b7en", "auf"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Rief er, Sonnen, euch zu: verbreitet Leben und W\u00e4rme", "tokens": ["Rief", "er", ",", "Son\u00b7nen", ",", "euch", "zu", ":", "ver\u00b7brei\u00b7tet", "Le\u00b7ben", "und", "W\u00e4r\u00b7me"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PPER", "PTKVZ", "$.", "VVPP", "NN", "KON", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Auf die d\u00fcrftigen Erden! Erbarmt euch der D\u00fcrstenden, da\u00df ich", "tokens": ["Auf", "die", "d\u00fcrf\u00b7ti\u00b7gen", "Er\u00b7den", "!", "Er\u00b7barmt", "euch", "der", "D\u00fcrs\u00b7ten\u00b7den", ",", "da\u00df", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER"], "meter": "+-+--+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Mich am gro\u00dfen Abend des Himmels euer erbarme!", "tokens": ["Mich", "am", "gro\u00b7\u00dfen", "A\u00b7bend", "des", "Him\u00b7mels", "eu\u00b7er", "er\u00b7bar\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "ART", "NN", "PPOSAT", "ADJA", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Also rief er. Gedenke de\u00df, o Strahlende! Fr\u00fcher", "tokens": ["Al\u00b7so", "rief", "er", ".", "Ge\u00b7den\u00b7ke", "de\u00df", ",", "o", "Strah\u00b7len\u00b7de", "!", "Fr\u00fc\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "ART", "$,", "FM", "NN", "$.", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.11": {"text": "Oder sp\u00e4ter kommt der gro\u00dfe Abend des Himmels,", "tokens": ["O\u00b7der", "sp\u00e4\u00b7ter", "kommt", "der", "gro\u00b7\u00dfe", "A\u00b7bend", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Da ihr alle, zahlloses Heer von m\u00e4chtigen Sonnen,", "tokens": ["Da", "ihr", "al\u00b7le", ",", "zahl\u00b7lo\u00b7ses", "Heer", "von", "m\u00e4ch\u00b7ti\u00b7gen", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "$,", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "l