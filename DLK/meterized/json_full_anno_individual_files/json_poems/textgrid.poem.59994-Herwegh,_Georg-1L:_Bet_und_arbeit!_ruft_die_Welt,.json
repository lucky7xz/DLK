{"textgrid.poem.59994": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bet und arbeit! ruft die Welt,", "genre": "verse", "period": "N.A.", "pub_year": 1863, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bet und arbeit! ruft die Welt,", "tokens": ["Bet", "und", "ar\u00b7beit", "!", "ruft", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bete kurz! denn Zeit ist Geld.", "tokens": ["Be\u00b7te", "kurz", "!", "denn", "Zeit", "ist", "Geld", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "KON", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An die T\u00fcre pocht die Not \u2013", "tokens": ["An", "die", "T\u00fc\u00b7re", "pocht", "die", "Not", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bete kurz! denn Zeit ist Brot.", "tokens": ["Be\u00b7te", "kurz", "!", "denn", "Zeit", "ist", "Brot", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "KON", "NN", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und du ackerst, und du s\u00e4st,", "tokens": ["Und", "du", "ac\u00b7kerst", ",", "und", "du", "s\u00e4st", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und du nietest, und du n\u00e4hst,", "tokens": ["Und", "du", "nie\u00b7test", ",", "und", "du", "n\u00e4hst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du h\u00e4mmerst, und du spinnst \u2013", "tokens": ["Und", "du", "h\u00e4m\u00b7merst", ",", "und", "du", "spinnst", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "KON", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sag, o Volk, was du gewinnst!", "tokens": ["Sag", ",", "o", "Volk", ",", "was", "du", "ge\u00b7winnst", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PWS", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wirkst am Webstuhl Tag und Nacht,", "tokens": ["Wirkst", "am", "Web\u00b7stuhl", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00fcrfst im Erz- und Kohlenschacht,", "tokens": ["Sch\u00fcrfst", "im", "Er\u00b7z", "und", "Koh\u00b7len\u00b7schacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "TRUNC", "KON", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "F\u00fcllst des \u00dcberflusses Horn,", "tokens": ["F\u00fcllst", "des", "\u00dc\u00b7berf\u00b7lus\u00b7ses", "Horn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcllst es hoch mit Wein und Korn \u2013", "tokens": ["F\u00fcllst", "es", "hoch", "mit", "Wein", "und", "Korn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wo ist ", "tokens": ["Doch", "wo", "ist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Doch wo ist ", "tokens": ["Doch", "wo", "ist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch wo ist ", "tokens": ["Doch", "wo", "ist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Doch wo ist ", "tokens": ["Doch", "wo", "ist"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Alles ist dein Werk! o sprich,", "tokens": ["Al\u00b7les", "ist", "dein", "Werk", "!", "o", "sprich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPOSAT", "NN", "$.", "FM", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles, aber nichts f\u00fcr dich!", "tokens": ["Al\u00b7les", ",", "a\u00b7ber", "nichts", "f\u00fcr", "dich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "PIS", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von allem nur allein,", "tokens": ["Und", "von", "al\u00b7lem", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIS", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die du schmiedst, die Kette, dein?", "tokens": ["Die", "du", "schmiedst", ",", "die", "Ket\u00b7te", ",", "dein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kette, die den Leib umstrickt,", "tokens": ["Ket\u00b7te", ",", "die", "den", "Leib", "um\u00b7strickt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die dem Geist die Fl\u00fcgel knickt,", "tokens": ["Die", "dem", "Geist", "die", "Fl\u00fc\u00b7gel", "knickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die am Fu\u00df des Kindes schon", "tokens": ["Die", "am", "Fu\u00df", "des", "Kin\u00b7des", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Klirrt \u2013 o Volk, das ist dein Lohn.", "tokens": ["Klirrt", "\u2013", "o", "Volk", ",", "das", "ist", "dein", "Lohn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "FM", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Was ihr hebt ans Sonnenlicht,", "tokens": ["Was", "ihr", "hebt", "ans", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00e4tze sind es f\u00fcr den Wicht;", "tokens": ["Sch\u00e4t\u00b7ze", "sind", "es", "f\u00fcr", "den", "Wicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ihr webt, es ist der Fluch", "tokens": ["Was", "ihr", "webt", ",", "es", "ist", "der", "Fluch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr euch selbst \u2013 ins bunte Tuch.", "tokens": ["F\u00fcr", "euch", "selbst", "\u2013", "ins", "bun\u00b7te", "Tuch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$(", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Was ihr baut, kein sch\u00fctzend Dach", "tokens": ["Was", "ihr", "baut", ",", "kein", "sch\u00fct\u00b7zend", "Dach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PIAT", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat's f\u00fcr euch und kein Gemach;", "tokens": ["Hat's", "f\u00fcr", "euch", "und", "kein", "Ge\u00b7mach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ihr kleidet und beschuht,", "tokens": ["Was", "ihr", "klei\u00b7det", "und", "be\u00b7schuht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tritt auf euch voll \u00dcbermut.", "tokens": ["Tritt", "auf", "euch", "voll", "\u00dc\u00b7ber\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ADJD", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Menschenbienen, die Natur,", "tokens": ["Men\u00b7schen\u00b7bie\u00b7nen", ",", "die", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gab sie euch den Honig nur?", "tokens": ["Gab", "sie", "euch", "den", "Ho\u00b7nig", "nur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht die Drohnen um euch her!", "tokens": ["Seht", "die", "Droh\u00b7nen", "um", "euch", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Habt ihr keinen Stachel mehr?", "tokens": ["Habt", "ihr", "kei\u00b7nen", "Sta\u00b7chel", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Mann der Arbeit, aufgewacht!", "tokens": ["Mann", "der", "Ar\u00b7beit", ",", "auf\u00b7ge\u00b7wacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erkenne deine Macht!", "tokens": ["Und", "er\u00b7ken\u00b7ne", "dei\u00b7ne", "Macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle R\u00e4der stehen still,", "tokens": ["Al\u00b7le", "R\u00e4\u00b7der", "ste\u00b7hen", "still", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn dein starker Arm es will.", "tokens": ["Wenn", "dein", "star\u00b7ker", "Arm", "es", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Deiner Dr\u00e4nger Schar erbla\u00dft,", "tokens": ["Dei\u00b7ner", "Dr\u00e4n\u00b7ger", "Schar", "er\u00b7bla\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn du, m\u00fcde deiner Last,", "tokens": ["Wenn", "du", ",", "m\u00fc\u00b7de", "dei\u00b7ner", "Last", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In die Ecke lehnst den Pflug,", "tokens": ["In", "die", "E\u00b7cke", "lehnst", "den", "Pflug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du rufst: Es ist genug!", "tokens": ["Wenn", "du", "rufst", ":", "Es", "ist", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Brecht das Doppeljoch entzwei!", "tokens": ["Brecht", "das", "Dop\u00b7pel\u00b7joch", "ent\u00b7zwei", "!"], "to