{"textgrid.poem.58716": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Habe ich ges\u00fcndigt, was tue ich dir damit, o du Menschenh\u00fcter? Hiob 7, 20.", "tokens": ["Ha\u00b7be", "ich", "ge\u00b7s\u00fcn\u00b7digt", ",", "was", "tue", "ich", "dir", "da\u00b7mit", ",", "o", "du", "Men\u00b7schen\u00b7h\u00fc\u00b7ter", "?", "Hiob", "7", ",", "20."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "number", "punct", "ordinal"], "pos": ["NN", "PPER", "VVPP", "$,", "PWS", "VVFIN", "PPER", "PPER", "PAV", "$,", "FM", "PPER", "NN", "$.", "NE", "CARD", "$,", "ADJA"], "meter": "+-+-+-++--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Wende dich zu mir und sei mir gn\u00e4dig! Psalm 119, 132", "tokens": ["Wen\u00b7de", "dich", "zu", "mir", "und", "sei", "mir", "gn\u00e4\u00b7dig", "!", "Psalm", "119", ",", "132"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "number", "punct", "number"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "KON", "VAFIN", "PPER", "ADJD", "$.", "NN", "CARD", "$,", "CARD"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Geh doch nicht weg, mein Br\u00e4utigam,", "tokens": ["Geh", "doch", "nicht", "weg", ",", "mein", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "PTKVZ", "$,", "PPOSAT", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hoff' in dich allein, mein Lamm;", "tokens": ["Ich", "hoff'", "in", "dich", "al\u00b7lein", ",", "mein", "Lamm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schau meinen Glauben, meine Schmerzen,", "tokens": ["Schau", "mei\u00b7nen", "Glau\u00b7ben", ",", "mei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df deine Liebe sein gewogen meinem Herzen!", "tokens": ["La\u00df", "dei\u00b7ne", "Lie\u00b7be", "sein", "ge\u00b7wo\u00b7gen", "mei\u00b7nem", "Her\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPOSAT", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Flieh nicht, du siehst ja meine Pein,", "tokens": ["Flieh", "nicht", ",", "du", "siehst", "ja", "mei\u00b7ne", "Pein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es kann kein Feu'r noch Qual mir so empfindlich sein,", "tokens": ["Es", "kann", "kein", "Feu'r", "noch", "Qual", "mir", "so", "emp\u00b7find\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ADV", "NN", "PPER", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Herr, straf mich nur nach deiner Strenge,", "tokens": ["Herr", ",", "straf", "mich", "nur", "nach", "dei\u00b7ner", "Stren\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es setzen deine Schl\u00e4g' mich so nicht ins Gedr\u00e4nge!", "tokens": ["Es", "set\u00b7zen", "dei\u00b7ne", "Schl\u00e4g'", "mich", "so", "nicht", "ins", "Ge\u00b7dr\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Nein, Liebe, nein, ich kann's nicht sehn,", "tokens": ["Nein", ",", "Lie\u00b7be", ",", "nein", ",", "ich", "kann's", "nicht", "sehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PTKANT", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du entfernest dich, \u2013 o bittres Von-mir-gehen! \u2013", "tokens": ["Da\u00df", "du", "ent\u00b7fer\u00b7nest", "dich", ",", "\u2013", "o", "bitt\u00b7res", "Von\u00b7mir\u00b7ge\u00b7hen", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "$,", "$(", "FM", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich will mein Leben lieber missen;", "tokens": ["Ich", "will", "mein", "Le\u00b7ben", "lie\u00b7ber", "mis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es tut mir leid, kehr um, werd du mir nicht entrissen!", "tokens": ["Es", "tut", "mir", "leid", ",", "kehr", "um",