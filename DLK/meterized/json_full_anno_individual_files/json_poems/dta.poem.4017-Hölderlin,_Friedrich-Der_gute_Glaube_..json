{"dta.poem.4017": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der gute Glaube .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6nes Leben! Du liegst krank und das Herz", "tokens": ["Sch\u00f6\u00b7nes", "Le\u00b7ben", "!", "Du", "liegst", "krank", "und", "das", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ADJD", "KON", "ART", "NN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "ist mir", "tokens": ["ist", "mir"], "token_info": ["word", "word"], "pos": ["VAFIN", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "M\u00fcd vom Weinen und schon d\u00e4mmert die Furcht", "tokens": ["M\u00fcd", "vom", "Wei\u00b7nen", "und", "schon", "d\u00e4m\u00b7mert", "die", "Furcht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "KON", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "in mir;", "tokens": ["in", "mir", ";"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Doch, doch kann ich nicht glauben,", "tokens": ["Doch", ",", "doch", "kann", "ich", "nicht", "glau\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df Du sterbest, so lang Du liebst.", "tokens": ["Da\u00df", "Du", "ster\u00b7best", ",", "so", "lang", "Du", "liebst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}