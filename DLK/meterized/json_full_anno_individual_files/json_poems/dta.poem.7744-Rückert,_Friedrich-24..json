{"dta.poem.7744": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So sprach der kluge Narr zu einer sch\u00f6nen Frau,", "tokens": ["So", "sprach", "der", "klu\u00b7ge", "Narr", "zu", "ei\u00b7ner", "sch\u00f6\u00b7nen", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die im geschm\u00fcckten Kleid am Fenster stand zur Schau:", "tokens": ["Die", "im", "ge\u00b7schm\u00fcck\u00b7ten", "Kleid", "am", "Fens\u00b7ter", "stand", "zur", "Schau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du f\u00fcr deinen Mann hast angethan den Putz,", "tokens": ["Wenn", "du", "f\u00fcr", "dei\u00b7nen", "Mann", "hast", "an\u00b7ge\u00b7than", "den", "Putz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So geh vom Fenster weg! wozu bist du hier nutz?", "tokens": ["So", "geh", "vom", "Fens\u00b7ter", "weg", "!", "wo\u00b7zu", "bist", "du", "hier", "nutz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$.", "PWAV", "VAFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als da\u00df wir von der Gass' aufkehren unsre Blicke,", "tokens": ["Als", "da\u00df", "wir", "von", "der", "Gass'", "auf\u00b7keh\u00b7ren", "uns\u00b7re", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns sto\u00dfen an den Stein und brechen das Genicke!", "tokens": ["Uns", "sto\u00b7\u00dfen", "an", "den", "Stein", "und", "bre\u00b7chen", "das", "Ge\u00b7ni\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}