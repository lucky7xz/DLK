{"textgrid.poem.33861": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vater unser", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blitze lauern hinter Wolken,", "tokens": ["Blit\u00b7ze", "lau\u00b7ern", "hin\u00b7ter", "Wol\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Eichen w\u00fchlt der Sturm;", "tokens": ["In", "den", "Ei\u00b7chen", "w\u00fchlt", "der", "Sturm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dicker Wald; ein Nothgel\u00e4ute", "tokens": ["Di\u00b7cker", "Wald", ";", "ein", "Noth\u00b7ge\u00b7l\u00e4u\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hallt schon dumpf von manchem Thurm.", "tokens": ["Hallt", "schon", "dumpf", "von", "man\u00b7chem", "Thurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ruhig unter'm breiten Baume,", "tokens": ["Ru\u00b7hig", "un\u00b7ter'm", "brei\u00b7ten", "Bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Pfeife in dem Mund,", "tokens": ["Sei\u00b7ne", "Pfei\u00b7fe", "in", "dem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegt der alte R\u00e4uberhauptmann;", "tokens": ["Liegt", "der", "al\u00b7te", "R\u00e4u\u00b7ber\u00b7haupt\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm zu F\u00fc\u00dfen schl\u00e4ft sein Hund.", "tokens": ["Ihm", "zu", "F\u00fc\u00b7\u00dfen", "schl\u00e4ft", "sein", "Hund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ein J\u00fcngling, bleich, wie Keiner,", "tokens": ["Und", "ein", "J\u00fcng\u00b7ling", ",", "bleich", ",", "wie", "Kei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "$,", "PWAV", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streckt sich ihm zur Seite hin.", "tokens": ["Streckt", "sich", "ihm", "zur", "Sei\u00b7te", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbschleif dein Messer!\u00ab spricht der Alte,", "tokens": ["\u00bb", "schleif", "dein", "Mes\u00b7ser", "!", "\u00ab", "spricht", "der", "Al\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "PPOSAT", "NN", "$.", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Er gehorcht mit schwerem Sinn.", "tokens": ["Er", "ge\u00b7horcht", "mit", "schwe\u00b7rem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Roth und zischend zwischen Beide", "tokens": ["Roth", "und", "zi\u00b7schend", "zwi\u00b7schen", "Bei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPR", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Springt ein Blitz, doch trifft er nicht.", "tokens": ["Springt", "ein", "Blitz", ",", "doch", "trifft", "er", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbvater unser!\u00ab ruft der J\u00fcngling,", "tokens": ["\u00bb", "va\u00b7ter", "un\u00b7ser", "!", "\u00ab", "ruft", "der", "J\u00fcng\u00b7ling", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PPOSAT", "$.", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch der Alte flucht und spricht:", "tokens": ["Doch", "der", "Al\u00b7te", "flucht", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbvater unser lass' ich gelten,", "tokens": ["\u00bb", "va\u00b7ter", "un\u00b7ser", "lass'", "ich", "gel\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PPOSAT", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn man auf dem Richtstuhl sitzt,", "tokens": ["Wenn", "man", "auf", "dem", "Richt\u00b7stuhl", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Scheere in den Haaren", "tokens": ["Wenn", "die", "Schee\u00b7re", "in", "den", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Beil im Nacken blitzt.", "tokens": ["Und", "das", "Beil", "im", "Na\u00b7cken", "blitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jetzt verbiet' ich dir das Beten,", "tokens": ["Jetzt", "ver\u00b7biet'", "ich", "dir", "das", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn zum Herrn erkorst du mich,", "tokens": ["Denn", "zum", "Herrn", "er\u00b7korst", "du", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich stell' den Mord noch heute", "tokens": ["Und", "ich", "stell'", "den", "Mord", "noch", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADV", "ADV"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Dunkel zwischen Gott und dich!", "tokens": ["Dun\u00b7kel", "zwi\u00b7schen", "Gott", "und", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja, ich schw\u00f6r's, du sollst den Ersten,", "tokens": ["Ja", ",", "ich", "schw\u00f6r's", ",", "du", "sollst", "den", "Ers\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den du hier erblicken wirst,", "tokens": ["Den", "du", "hier", "er\u00b7bli\u00b7cken", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6dten, da\u00df du nicht noch einmal", "tokens": ["T\u00f6d\u00b7ten", ",", "da\u00df", "du", "nicht", "noch", "ein\u00b7mal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dich von mir zu Gott verirrst.", "tokens": ["Dich", "von", "mir", "zu", "Gott", "ver\u00b7irrst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Du erschrickst? Ich will's nicht schelten,", "tokens": ["Du", "er\u00b7schrickst", "?", "Ich", "will's", "nicht", "schel\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir auch schien das einst gar viel,", "tokens": ["Mir", "auch", "schien", "das", "einst", "gar", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PDS", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auch du erlebst die Zeiten,", "tokens": ["Und", "auch", "du", "er\u00b7lebst", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wo du's treibst, wie ich, als Spiel.", "tokens": ["Wo", "du's", "treibst", ",", "wie", "ich", ",", "als", "Spiel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "PWAV", "PPER", "$,", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Mir ist solch ein Muth gekommen,", "tokens": ["Mir", "ist", "solch", "ein", "Muth", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seit ich, weil er zornig sprach", "tokens": ["Seit", "ich", ",", "weil", "er", "zor\u00b7nig", "sprach"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vom Gericht und andern Dingen,", "tokens": ["Vom", "Ge\u00b7richt", "und", "an\u00b7dern", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Vater niederstach.\u00ab", "tokens": ["Mei\u00b7nen", "Va\u00b7ter", "nie\u00b7der\u00b7stach", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Angstgesch\u00fcttelt ruft der J\u00fcngling:", "tokens": ["Angst\u00b7ge\u00b7sch\u00fct\u00b7telt", "ruft", "der", "J\u00fcng\u00b7ling", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbnimmer, nimmer that'st du das!\u00ab", "tokens": ["\u00bb", "nim\u00b7mer", ",", "nim\u00b7mer", "that'st", "du", "das", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$,", "ADV", "VVFIN", "PPER", "PDS", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kr\u00e4ftig schmauchend spricht der Alte:", "tokens": ["Kr\u00e4f\u00b7tig", "schmau\u00b7chend", "spricht", "der", "Al\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbei, ich that's, und ist's denn was?\u00ab", "tokens": ["\u00bb", "ei", ",", "ich", "that's", ",", "und", "ist's", "denn", "was", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VVFIN", "$,", "KON", "NE", "KON", "PWS", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "\u00bbwohl, da mu\u00df ich's freilich halten,", "tokens": ["\u00bb", "wohl", ",", "da", "mu\u00df", "ich's", "frei\u00b7lich", "hal\u00b7ten", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "ADV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was du schwurst, und thu's mit Lust!\u00ab", "tokens": ["Was", "du", "schwurst", ",", "und", "thu's", 