{"dta.poem.10758": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Ejusdem Epigrammata", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Wer seinen Feind veracht/ kompt leichtlich in verderben/", "tokens": ["Wer", "sei\u00b7nen", "Feind", "ver\u00b7acht", "/", "kompt", "leicht\u00b7lich", "in", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "ADJD", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer seinen Feind verehrt/ wird seiner H\u00e4nde sterben.", "tokens": ["Wer", "sei\u00b7nen", "Feind", "ver\u00b7ehrt", "/", "wird", "sei\u00b7ner", "H\u00e4n\u00b7de", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVPP", "$(", "VAFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der ist eim Esel gleich/ vnd einem dummen ", "tokens": ["Der", "ist", "eim", "E\u00b7sel", "gleich", "/", "vnd", "ei\u00b7nem", "dum\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADV", "$(", "KON", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Welcher den jenigen/ der jhn verachtet/ ehrt.", "tokens": ["Wel\u00b7cher", "den", "je\u00b7ni\u00b7gen", "/", "der", "jhn", "ver\u00b7ach\u00b7tet", "/", "ehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAT", "ART", "PIAT", "$(", "PRELS", "PPER", "VVFIN", "$(", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Hastu schon Sechtzig R\u00e4ht/ die deine sach betrachten/", "tokens": ["Has\u00b7tu", "schon", "Secht\u00b7zig", "R\u00e4ht", "/", "die", "dei\u00b7ne", "sach", "be\u00b7trach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "CARD", "NN", "$(", "ART", "PPOSAT", "NN", "VVINF", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "So solst doch auch den Rath deins Hertzen nicht verachten/", "tokens": ["So", "solst", "doch", "auch", "den", "Rath", "deins", "Hert\u00b7zen", "nicht", "ver\u00b7ach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dasselb am besten f\u00fchlt/ wo dich der Schuch hindruckt/", "tokens": ["Das\u00b7selb", "am", "bes\u00b7ten", "f\u00fchlt", "/", "wo", "dich", "der", "Schuch", "hin\u00b7druckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKA", "ADJD", "VVFIN", "$(", "PWAV", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Manch ander mehr vf sein als deinen nutzen lugt.", "tokens": ["Manch", "an\u00b7der", "mehr", "vf", "sein", "als", "dei\u00b7nen", "nut\u00b7zen", "lugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "ADV", "ADJD", "VAINF", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}