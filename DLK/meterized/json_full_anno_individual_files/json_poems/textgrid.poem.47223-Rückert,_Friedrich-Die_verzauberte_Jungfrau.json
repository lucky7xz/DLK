{"textgrid.poem.47223": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die verzauberte Jungfrau", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Jungfrau, die verzaubert dort", "tokens": ["Die", "Jung\u00b7frau", ",", "die", "ver\u00b7zau\u00b7bert", "dort"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sitzt in der H\u00f6hle Grunde,", "tokens": ["Sitzt", "in", "der", "H\u00f6h\u00b7le", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat auf Erl\u00f6sung fort und fort", "tokens": ["Hat", "auf", "Er\u00b7l\u00f6\u00b7sung", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gewartet bis zur Stunde;", "tokens": ["Ge\u00b7war\u00b7tet", "bis", "zur", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer sich an die Erl\u00f6sung wagt,", "tokens": ["Wer", "sich", "an", "die", "Er\u00b7l\u00f6\u00b7sung", "wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Mu\u00df einen Ku\u00df nur unverzagt", "tokens": ["Mu\u00df", "ei\u00b7nen", "Ku\u00df", "nur", "un\u00b7ver\u00b7zagt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aufdr\u00fccken ihrem Munde.", "tokens": ["Auf\u00b7dr\u00fc\u00b7cken", "ih\u00b7rem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Allein beim K\u00fcssen ziert sie sich,", "tokens": ["Al\u00b7lein", "beim", "K\u00fcs\u00b7sen", "ziert", "sie", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gar nicht hold jungfr\u00e4ulich,", "tokens": ["Und", "gar", "nicht", "hold", "jung\u00b7fr\u00e4u\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verwandelnd umgebiert sie sich", "tokens": ["Ver\u00b7wan\u00b7delnd", "um\u00b7ge\u00b7biert", "sie", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In viel Gestalten greulich,", "tokens": ["In", "viel", "Ge\u00b7stal\u00b7ten", "greu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df nur ein unerschrockner Mann", "tokens": ["Da\u00df", "nur", "ein", "un\u00b7er\u00b7schrock\u00b7ner", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es ansehn und sie k\u00fcssen kann,", "tokens": ["Es", "an\u00b7sehn", "und", "sie", "k\u00fcs\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie sie sich stellt abscheulich.", "tokens": ["Wie", "sie", "sich", "stellt", "ab\u00b7scheu\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da war ein Schneider jung und keck,", "tokens": ["Da", "war", "ein", "Schnei\u00b7der", "jung", "und", "keck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der k\u00fchnste Mann auf Erden,", "tokens": ["Der", "k\u00fchns\u00b7te", "Mann", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem sa\u00df das Herz am rechten Fleck:", "tokens": ["Dem", "sa\u00df", "das", "Herz", "am", "rech\u00b7ten", "Fleck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbmagst du dich nur geb\u00e4rden!", "tokens": ["\u00bb", "magst", "du", "dich", "nur", "ge\u00b7b\u00e4r\u00b7den", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Und was du thust und was du sagst,", "tokens": ["Und", "was", "du", "thust", "und", "was", "du", "sagst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie du dich verwandeln magst,", "tokens": ["Und", "wie", "du", "dich", "ver\u00b7wan\u00b7deln", "magst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du sollst erl\u00f6set werden.\u00ab", "tokens": ["Du", "sollst", "er\u00b7l\u00f6\u00b7set", "wer\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Jungfrau ward von Angesicht", "tokens": ["Die", "Jung\u00b7frau", "ward", "von", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum schrecklichsten der Drachen;", "tokens": ["Zum", "schreck\u00b7lichs\u00b7ten", "der", "Dra\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Der tapfre Schneider zittert nicht", "tokens": ["Der", "tapf\u00b7re", "Schnei\u00b7der", "zit\u00b7tert", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fc\u00dft sie auf den Rachen;", "tokens": ["Und", "k\u00fc\u00dft", "sie", "auf", "den", "Ra\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Jungfrau wird ein grimmer Leu,", "tokens": ["Die", "Jung\u00b7frau", "wird", "ein", "grim\u00b7mer", "Leu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schon will der Schneider, auch nicht scheu,", "tokens": ["Schon", "will", "der", "Schnei\u00b7der", ",", "auch", "nicht", "scheu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NE", "$,", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zum Ku\u00df sich fertig machen.", "tokens": ["Zum", "Ku\u00df", "sich", "fer\u00b7tig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Jungfrau wird zum Krokodil,", "tokens": ["Die", "Jung\u00b7frau", "wird", "zum", "Kro\u00b7ko\u00b7dil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er will zum Kusse schreiten;", "tokens": ["Er", "will", "zum", "Kus\u00b7se", "schrei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie sie sich verwandeln will,", "tokens": ["Und", "wie", "sie", "sich", "ver\u00b7wan\u00b7deln", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wird sie doch erstreiten.", "tokens": ["Er", "wird", "sie", "doch", "er\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zuletzt wird sie ein Ziegenbock,", "tokens": ["Zu\u00b7letzt", "wird", "sie", "ein", "Zie\u00b7gen\u00b7bock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da rennt er \u00fcber Stock und Block:", "tokens": ["Da", "rennt", "er", "\u00fc\u00b7ber", "Stock", "und", "Block", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbdich mag der Teufel reiten!\u00ab", "tokens": ["\u00bb", "dich", "mag", "der", "Teu\u00b7fel", "rei\u00b7ten", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Jungfrau, die verzaubert dort", "tokens": ["Die", "Jung\u00b7frau", ",", "die", "ver\u00b7zau\u00b7bert", "dort"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sitzt in der H\u00f6hle Grunde,", "tokens": ["Sitzt", "in", "der", "H\u00f6h\u00b7le", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat auf Erl\u00f6sung fort und fort", "tokens": ["Hat", "auf", "Er\u00b7l\u00f6\u00b7sung", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN