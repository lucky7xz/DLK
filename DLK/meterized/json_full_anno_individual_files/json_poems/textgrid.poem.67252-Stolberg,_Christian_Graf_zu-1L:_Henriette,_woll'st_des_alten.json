{"textgrid.poem.67252": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Henriette, woll'st des alten", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Henriette, woll'st des alten", "tokens": ["Hen\u00b7riet\u00b7te", ",", "woll'st", "des", "al\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Oheims Liebe nicht verschm\u00e4hn,", "tokens": ["O\u00b7heims", "Lie\u00b7be", "nicht", "ver\u00b7schm\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht in seiner Stirne Falten", "tokens": ["Nicht", "in", "sei\u00b7ner", "Stir\u00b7ne", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur den m\u00fcrrischen, den kalten,", "tokens": ["Nur", "den", "m\u00fcr\u00b7ri\u00b7schen", ",", "den", "kal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Den verlebten Vierz'ger sp\u00e4hn!", "tokens": ["Den", "ver\u00b7leb\u00b7ten", "Vier\u00b7z'\u00b7ger", "sp\u00e4hn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Noch, ich dank's dem guten Gott,", "tokens": ["Noch", ",", "ich", "dank's", "dem", "gu\u00b7ten", "Gott", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch ist mir der Freude Quellchen", "tokens": ["Noch", "ist", "mir", "der", "Freu\u00b7de", "Quell\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nicht versiegt; in seinen Wellchen,", "tokens": ["Nicht", "ver\u00b7siegt", ";", "in", "sei\u00b7nen", "Well\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVPP", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Manchem J\u00fcnglinge zu Spott,", "tokens": ["Man\u00b7chem", "J\u00fcng\u00b7lin\u00b7ge", "zu", "Spott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Strahlt in meines Abends Schimmer", "tokens": ["Strahlt", "in", "mei\u00b7nes", "A\u00b7bends", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Jedes Bl\u00fcmchen, wo's nur immer", "tokens": ["Je\u00b7des", "Bl\u00fcm\u00b7chen", ",", "wo's", "nur", "im\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PWAV", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mir am Wege keimt, so klar,", "tokens": ["Mir", "am", "We\u00b7ge", "keimt", ",", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie's in meiner Jugend Morgen", "tokens": ["Wie's", "in", "mei\u00b7ner", "Ju\u00b7gend", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Strahlte, da mir M\u00fchen, Sorgen,", "tokens": ["Strahl\u00b7te", ",", "da", "mir", "M\u00fc\u00b7hen", ",", "Sor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und ein graues Kummerhaar", "tokens": ["Und", "ein", "grau\u00b7es", "Kum\u00b7mer\u00b7haar"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Unbekannt, wie dir, noch war.", "tokens": ["Un\u00b7be\u00b7kannt", ",", "wie", "dir", ",", "noch", "war", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPER", "$,", "ADV", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sei du gl\u00fccklich, gutes M\u00e4dchen!", "tokens": ["Sei", "du", "gl\u00fcck\u00b7lich", ",", "gu\u00b7tes", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur mit sanftem, seidnen Dr\u00e4htchen", "tokens": ["Nur", "mit", "sanf\u00b7tem", ",", "seid\u00b7nen", "Dr\u00e4ht\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spinne sich dein Lebensf\u00e4dchen;", "tokens": ["Spin\u00b7ne", "sich", "dein", "Le\u00b7bens\u00b7f\u00e4d\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei du gl\u00fccklich, liebe, s\u00fc\u00dfe", "tokens": ["Sei", "du", "gl\u00fcck\u00b7lich", ",", "lie\u00b7be", ",", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VVFIN", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kleine, gl\u00fccklich, und genie\u00dfe", "tokens": ["Klei\u00b7ne", ",", "gl\u00fcck\u00b7lich", ",", "und", "ge\u00b7nie\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "$,", "ADJD", "$,", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jede reine Seligkeit,", "tokens": ["Je\u00b7de", "rei\u00b7ne", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die dir Einfalt, die dir Unschuld beut!", "tokens": ["Die", "dir", "Ein\u00b7falt", ",", "die", "dir", "Un\u00b7schuld", "beut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Immer l\u00e4chl' im Segensblick,", "tokens": ["Im\u00b7mer", "l\u00e4chl'", "im", "Se\u00b7gens\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Strahlend, wolkenlos und blau,", "tokens": ["Strah\u00b7lend", ",", "wol\u00b7ken\u00b7los", "und", "blau", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dir der Himmel; ach, so strahlend,", "tokens": ["Dir", "der", "Him\u00b7mel", ";", "ach", ",", "so", "strah\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$.", "XY", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und so wolkenlos, so blau,", "tokens": ["Und", "so", "wol\u00b7ken\u00b7los", ",", "so", "blau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie dir deiner Mutter Blick", "tokens": ["Wie", "dir", "dei\u00b7ner", "Mut\u00b7ter", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "L\u00e4chelt, jener, den ich auch", "tokens": ["L\u00e4\u00b7chelt", ",", "je\u00b7ner", ",", "den", "ich", "auch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PDS", "$,", "PRELS", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Kenne, wenn im warmen Hauch", "tokens": ["Ken\u00b7ne", ",", "wenn", "im", "war\u00b7men", "Hauch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Schneller R\u00fchrung, fromme, milde", "tokens": ["Schnel\u00b7ler", "R\u00fch\u00b7rung", ",", "from\u00b7me", ",", "mil\u00b7de"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Wehmuth sie zum Engelbilde", "tokens": ["Weh\u00b7muth", "sie", "zum", "En\u00b7gel\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Malt, und durch den hellen Thau", "tokens": ["Malt", ",", "und", "durch", "den", "hel\u00b7len", "Thau"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Sch\u00f6ner blickt des Auges Blau.", "tokens": ["Sch\u00f6\u00b7ner", "blickt", "des", "Au\u00b7ges", "Blau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00e4dchen, dieser Himmelsthau", "tokens": ["M\u00e4d\u00b7chen", ",", "die\u00b7ser", "Him\u00b7mel\u00b7sthau"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tr\u00e4ufl' auch dir! In seinem Glanze", "tokens": ["Tr\u00e4ufl'", "auch", "dir", "!", "In", "sei\u00b7nem", "Glan\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schimmert selbst der Freude Pflanze", "tokens": ["Schim\u00b7mert", "selbst", "der", "Freu\u00b7de", "Pflan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6ner, wenn im Bl\u00fcthenschoo\u00df", "tokens": ["Sch\u00f6\u00b7ner", ",", "wenn", "im", "Bl\u00fc\u00b7then\u00b7schoo\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diese lautre Perle bebt,", "tokens": ["Die\u00b7se", "laut\u00b7re", "Per\u00b7le", "bebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die zur Ahnung uns belebt,", "tokens": ["Die", "zur", "Ah\u00b7nung", "uns", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unser sch\u00f6nstes Erdenloos", "tokens": ["Un\u00b7ser", "sch\u00f6ns\u00b7tes", "Er\u00b7den\u00b7loos"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei die Sehnsucht, sie, die schmachtend,", "tokens": ["Sei", "die", "Sehn\u00b7sucht", ",", "sie", ",", "die", "schmach\u00b7tend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PPER", "$,", "PRELS", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Jenseit Mond und Sonne trachtend,", "tokens": ["Jen\u00b7seit", "Mond", "und", "Son\u00b7ne", "trach\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Uns zu h\u00f6h'rer Wonn' erhebt.", "tokens": ["Uns", "zu", "h\u00f6h'\u00b7rer", "Wonn'", "er\u00b7hebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Henriette, woll'st des alten", "tokens": ["Hen\u00b7riet\u00b7te", ",", "woll'st", "des", "al\u00b7ten"], "token_info": ["word"