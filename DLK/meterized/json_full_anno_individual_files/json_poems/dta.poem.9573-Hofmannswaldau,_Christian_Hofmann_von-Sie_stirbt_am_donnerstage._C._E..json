{"dta.poem.9573": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie stirbt am donnerstage.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verworffner donnerstag! du anfang meiner plagen!", "tokens": ["Ver\u00b7worff\u00b7ner", "don\u00b7ners\u00b7tag", "!", "du", "an\u00b7fang", "mei\u00b7ner", "pla\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat man dein donnern wohl je grausamer gesp\u00fchrt/", "tokens": ["Hat", "man", "dein", "don\u00b7nern", "wohl", "je", "grau\u00b7sa\u00b7mer", "ge\u00b7sp\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als heute/ da dein blitz das hertze mir ger\u00fchrt/", "tokens": ["Als", "heu\u00b7te", "/", "da", "dein", "blitz", "das", "hert\u00b7ze", "mir", "ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$(", "KOUS", "PPOSAT", "NN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und alle meine lust bi\u00df in den grund zerschlagen?", "tokens": ["Und", "al\u00b7le", "mei\u00b7ne", "lust", "bi\u00df", "in", "den", "grund", "zer\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie billig kanstu itzt den strengen nahmen tragen!", "tokens": ["Wie", "bil\u00b7lig", "kans\u00b7tu", "itzt", "den", "stren\u00b7gen", "nah\u00b7men", "tra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "ADV", "ART", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wann hat er jemahls dir mit gr\u00f6ssrem recht geb\u00fchrt?", "tokens": ["Wann", "hat", "er", "je\u00b7mahls", "dir", "mit", "gr\u00f6ss\u00b7rem", "recht", "ge\u00b7b\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du hast Arminden/ ja! mich selber mir entf\u00fchrt/", "tokens": ["Du", "hast", "Ar\u00b7min\u00b7den", "/", "ja", "!", "mich", "sel\u00b7ber", "mir", "ent\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$(", "PTKANT", "$.", "PPER", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und suchst in meiner angst dein s\u00fcsses wohlbehagen.", "tokens": ["Und", "suchst", "in", "mei\u00b7ner", "angst", "dein", "s\u00fcs\u00b7ses", "wohl\u00b7be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer r\u00e4chet mich an dir? fleh\u2019 ich den himmelan?", "tokens": ["Wer", "r\u00e4\u00b7chet", "mich", "an", "dir", "?", "fleh'", "ich", "den", "him\u00b7me\u00b7lan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "PPER", "$.", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Nein! dieser hat mir mehr/ als wie du selbst/ gethan.", "tokens": ["Nein", "!", "die\u00b7ser", "hat", "mir", "mehr", "/", "als", "wie", "du", "selbst", "/", "ge\u00b7than", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "$.", "PDS", "VAFIN", "PPER", "ADV", "$(", "KOUS", "PWAV", "PPER", "ADV", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ist di\u00df die freude/ die der freytag mir versprochen/", "tokens": ["Ist", "di\u00df", "die", "freu\u00b7de", "/", "die", "der", "frey\u00b7tag", "mir", "ver\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$(", "PRELS", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als mir der erste blick von ihrer gunst geschah?", "tokens": ["Als", "mir", "der", "ers\u00b7te", "blick", "von", "ih\u00b7rer", "gunst", "ge\u00b7schah", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie war ich dazumahl doch meinem gl\u00fcck so nah/", "tokens": ["Wie", "war", "ich", "da\u00b7zu\u00b7mahl", "doch", "mei\u00b7nem", "gl\u00fcck", "so", "nah", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das so erschrecklich sich itzt hat an mir gerochen!", "tokens": ["Das", "so", "er\u00b7schreck\u00b7lich", "sich", "itzt", "hat", "an", "mir", "ge\u00b7ro\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "PRF", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}