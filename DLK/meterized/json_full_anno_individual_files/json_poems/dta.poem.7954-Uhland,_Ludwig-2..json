{"dta.poem.7954": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Nach Hohem, W\u00fcrd\u2019gem nur hast du gerungen,", "tokens": ["Nach", "Ho\u00b7hem", ",", "W\u00fcrd'\u00b7gem", "nur", "hast", "du", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Kleinliche verschm\u00e4hend, wie das Wilde;", "tokens": ["Das", "Klein\u00b7li\u00b7che", "ver\u00b7schm\u00e4\u00b7hend", ",", "wie", "das", "Wil\u00b7de", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PWAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So fa\u00dftest du in kr\u00e4ftige Gebilde", "tokens": ["So", "fa\u00df\u00b7test", "du", "in", "kr\u00e4f\u00b7ti\u00b7ge", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das wundervolle Lied der Nibelungen.", "tokens": ["Das", "wun\u00b7der\u00b7vol\u00b7le", "Lied", "der", "Ni\u00b7be\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schon hatte Hagens Gr\u00f6\u00dfe dich durchdrungen,", "tokens": ["Schon", "hat\u00b7te", "Ha\u00b7gens", "Gr\u00f6\u00b7\u00dfe", "dich", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon stand vor dir die R\u00e4cherin Chriemhilde,", "tokens": ["Schon", "stand", "vor", "dir", "die", "R\u00e4\u00b7che\u00b7rin", "Chriem\u00b7hil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor Allem aber r\u00fchrte dich die Milde", "tokens": ["Vor", "Al\u00b7lem", "a\u00b7ber", "r\u00fchr\u00b7te", "dich", "die", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des edeln Sifrids, Giselhers, des jungen.", "tokens": ["Des", "e\u00b7deln", "Sif\u00b7rids", ",", "Gi\u00b7sel\u00b7hers", ",", "des", "jun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NE", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit Fug ward Giselher von dir beklaget,", "tokens": ["Mit", "Fug", "ward", "Gi\u00b7sel\u00b7her", "von", "dir", "be\u00b7kla\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der bl\u00fchend hinsank in des Kampfs Bedr\u00e4ngni\u00df,", "tokens": ["Der", "bl\u00fc\u00b7hend", "hin\u00b7sank", "in", "des", "Kampfs", "Be\u00b7dr\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dich selbst hat nun so fr\u00fcher Tod erjaget.", "tokens": ["Dich", "selbst", "hat", "nun", "so", "fr\u00fc\u00b7her", "Tod", "er\u00b7ja\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADV", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Warst du vielleicht zu innig schon versunken", "tokens": ["Warst", "du", "viel\u00b7leicht", "zu", "in\u00b7nig", "schon", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "PTKA", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In jenes Lied, de\u00df furchtbares Verh\u00e4ngni\u00df", "tokens": ["In", "je\u00b7nes", "Lied", ",", "de\u00df", "furcht\u00b7ba\u00b7res", "Ver\u00b7h\u00e4ng\u00b7ni\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Tode Jedem, nun auch dir, gewunken?", "tokens": ["Zum", "To\u00b7de", "Je\u00b7dem", ",", "nun", "auch", "dir", ",", "ge\u00b7wun\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "$,", "ADV", "ADV", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}