{"textgrid.poem.55211": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Erkl\u00e4rung", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht ein junger Feigenstock", "tokens": ["Es", "steht", "ein", "jun\u00b7ger", "Fei\u00b7gen\u00b7stock"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In einem sch\u00f6nen Garten;", "tokens": ["In", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Gar\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Daneben sitzt ein Ziegenbock,", "tokens": ["Da\u00b7ne\u00b7ben", "sitzt", "ein", "Zie\u00b7gen\u00b7bock", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wollt er seiner warten.", "tokens": ["Als", "wollt", "er", "sei\u00b7ner", "war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPOSAT", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Allein, Quiriten, wie man irrt!", "tokens": ["Al\u00b7lein", ",", "Qui\u00b7ri\u00b7ten", ",", "wie", "man", "irrt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Baum ist schlecht geh\u00fctet;", "tokens": ["Der", "Baum", "ist", "schlecht", "ge\u00b7h\u00fc\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ihm zur andern Seite schwirrt", "tokens": ["Und", "ihm", "zur", "an\u00b7dern", "Sei\u00b7te", "schwirrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein K\u00e4fer ausgebr\u00fctet.", "tokens": ["Ein", "K\u00e4\u00b7fer", "aus\u00b7ge\u00b7br\u00fc\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es fliegt der Held mit Panzerbrust", "tokens": ["Es", "fliegt", "der", "Held", "mit", "Pan\u00b7zer\u00b7brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und naschet in den Zweigen,", "tokens": ["Und", "na\u00b7schet", "in", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und auch der Bock hat gro\u00dfe Lust,", "tokens": ["Und", "auch", "der", "Bock", "hat", "gro\u00b7\u00dfe", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NE", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gem\u00e4chlich aufzusteigen.", "tokens": ["Ge\u00b7m\u00e4ch\u00b7lich", "auf\u00b7zu\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drum seht ihr, Freunde, schon beinah", "tokens": ["Drum", "seht", "ihr", ",", "Freun\u00b7de", ",", "schon", "bei\u00b7na