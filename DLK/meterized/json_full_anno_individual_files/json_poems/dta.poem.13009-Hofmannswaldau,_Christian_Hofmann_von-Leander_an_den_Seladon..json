{"dta.poem.13009": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Leander an den Seladon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich z\u00f6rne darum nicht, da\u00df dieser Musen-sohn,", "tokens": ["Ich", "z\u00f6r\u00b7ne", "da\u00b7rum", "nicht", ",", "da\u00df", "die\u00b7ser", "Mu\u00b7sen\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKNEG", "$,", "KOUS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als ihn des Ph\u00f6bus geist so ungemein entz\u00fccket,", "tokens": ["Als", "ihn", "des", "Ph\u00f6\u00b7bus", "geist", "so", "un\u00b7ge\u00b7mein", "ent\u00b7z\u00fc\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf der geweihten h\u00f6h des edlen Heliron", "tokens": ["Auf", "der", "ge\u00b7weih\u00b7ten", "h\u00f6h", "des", "ed\u00b7len", "He\u00b7li\u00b7ron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mich in der dichter-zunfft mit keinem aug\u2019 erblicket;", "tokens": ["Mich", "in", "der", "dich\u00b7ter\u00b7zunfft", "mit", "kei\u00b7nem", "aug'", "er\u00b7bli\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das aber \u00e4rgert mich, da\u00df er dich \u00fcbersehn,", "tokens": ["Das", "a\u00b7ber", "\u00e4r\u00b7gert", "mich", ",", "da\u00df", "er", "dich", "\u00fc\u00b7ber\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und von Floretten selbst aus neid nichts melden wollen.", "tokens": ["Und", "von", "Flo\u00b7ret\u00b7ten", "selbst", "aus", "neid", "nichts", "mel\u00b7den", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "APPR", "NN", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn ist es gleich, mein Freund! um deinen schlu\u00df geschehn;", "tokens": ["Denn", "ist", "es", "gleich", ",", "mein", "Freund", "!", "um", "dei\u00b7nen", "schlu\u00df", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$.", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Indem ich mit gewalt zum dichter werden sollen;", "tokens": ["In\u00b7dem", "ich", "mit", "ge\u00b7walt", "zum", "dich\u00b7ter", "wer\u00b7den", "sol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPRART", "ADJA", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So l\u00e4st Florette mich doch sicher prophezeyn:", "tokens": ["So", "l\u00e4st", "Flo\u00b7ret\u00b7te", "mich", "doch", "si\u00b7cher", "pro\u00b7phe\u00b7zeyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie werde Seudery, und du ihr bruder seyn.", "tokens": ["Sie", "wer\u00b7de", "Seu\u00b7de\u00b7ry", ",", "und", "du", "ihr", "bru\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$,", "KON", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}