{"dta.poem.1228": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Cartell auff ein Piquet-Spiel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Doris/ dir ist unvergessen/", "tokens": ["Do\u00b7ris", "/", "dir", "ist", "un\u00b7ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was du j\u00fcngster Zeit gethan/", "tokens": ["Was", "du", "j\u00fcngs\u00b7ter", "Zeit", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie dein Mund sich hat vermessen/", "tokens": ["Wie", "dein", "Mund", "sich", "hat", "ver\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich als Feind zu greiffen an:", "tokens": ["Mich", "als", "Feind", "zu", "greif\u00b7fen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie man vor bekandten Ohren", "tokens": ["Wie", "man", "vor", "be\u00b7kand\u00b7ten", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir Capoth und Martsch geschworen/", "tokens": ["Mir", "Ca\u00b7poth", "und", "Martsch", "ge\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie man eyfrig war bedacht", "tokens": ["Wie", "man", "ey\u00b7frig", "war", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ADJD", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir zu lieffern eine Schlacht.", "tokens": ["Mir", "zu", "lief\u00b7fern", "ei\u00b7ne", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weil denn ohn Verlust der Ehren", "tokens": ["Weil", "denn", "ohn", "Ver\u00b7lust", "der", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und nach Cavalieres-Pflicht/", "tokens": ["Und", "nach", "Ca\u00b7va\u00b7lie\u00b7res\u00b7Pflicht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich nicht schweigend kan verh\u00f6ren/", "tokens": ["Ich", "nicht", "schwei\u00b7gend", "kan", "ver\u00b7h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was man mir zu Hohne spricht/", "tokens": ["Was", "man", "mir", "zu", "Hoh\u00b7ne", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil wir/ sonder uns zu schlagen/", "tokens": ["Weil", "wir", "/", "son\u00b7der", "uns", "zu", "schla\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "KON", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmer k\u00f6nnen seyn vertragen/", "tokens": ["Nim\u00b7mer", "k\u00f6n\u00b7nen", "seyn", "ver\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VAINF", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "So sey/ Doris/ nur bereit", "tokens": ["So", "sey", "/", "Do\u00b7ris", "/", "nur", "be\u00b7reit"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "$(", "NE", "$(", "ADV", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Dich zu finden in den Streit.", "tokens": ["Dich", "zu", "fin\u00b7den", "in", "den", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar/ indem ich bin geruffen/", "tokens": ["Zwar", "/", "in\u00b7dem", "ich", "bin", "ge\u00b7ruf\u00b7fen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00fcnde mir das W\u00e4hlen frey/", "tokens": ["St\u00fcn\u00b7de", "mir", "das", "W\u00e4h\u00b7len", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch/ damit du nicht darffst hoffen", "tokens": ["Doch", "/", "da\u00b7mit", "du", "nicht", "darffst", "hof\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$(", "KOUS", "PPER", "PTKNEG", "PAV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich abzuschrecken sey/", "tokens": ["Da\u00df", "ich", "ab\u00b7zu\u00b7schre\u00b7cken", "sey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVIZU", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie dein Mund mir wird beschreiben", "tokens": ["Wie", "dein", "Mund", "mir", "wird", "be\u00b7schrei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ort und Art/ so soll es bleiben/", "tokens": ["Ort", "und", "Art", "/", "so", "soll", "es", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "ADV", "VMFIN", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn dirs wird gelegen seyn/", "tokens": ["Wenn", "dirs", "wird", "ge\u00b7le\u00b7gen", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "VVPP", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "So will ich mich finden ein.", "tokens": ["So", "will", "ich", "mich", "fin\u00b7den", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Wisse/ da\u00df ohn Ehr-erwerben", "tokens": ["Wis\u00b7se", "/", "da\u00df", "ohn", "Ehr\u00b7er\u00b7wer\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich nicht von dem Platze weich/", "tokens": ["Ich", "nicht", "von", "dem", "Plat\u00b7ze", "weich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPR", "ART", "NN", "ADJD", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Liegen/ siegen/ leben/ sterben/", "tokens": ["Lie\u00b7gen", "/", "sie\u00b7gen", "/", "le\u00b7ben", "/", "ster\u00b7ben", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "VVINF", "$(", "VVINF", "$(", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll mir alles gelten gleich/", "tokens": ["Soll", "mir", "al\u00b7les", "gel\u00b7ten", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "VVFIN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch/ Cupiden ausgenommen/", "tokens": ["Auch", "/", "Cu\u00b7pi\u00b7den", "aus\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$(", "NN", "VVPP", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mag/ wer will/ vor Beystand kommen.", "tokens": ["Mag", "/", "wer", "will", "/", "vor", "Beys\u00b7tand", "kom\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "PWS", "VMFIN", "$(", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Solt ich gleich drob b\u00fcssen ein/", "tokens": ["Solt", "ich", "gleich", "drob", "b\u00fcs\u00b7sen", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVFIN", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hertzen wird mein Rummel seyn.", "tokens": ["Hert\u00b7zen", "wird", "mein", "Rum\u00b7mel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}