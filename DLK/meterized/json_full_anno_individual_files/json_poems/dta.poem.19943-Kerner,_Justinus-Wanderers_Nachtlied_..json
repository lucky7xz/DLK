{"dta.poem.19943": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Wanderers Nachtlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch Sturm und Nacht in fremden Land", "tokens": ["Durch", "Sturm", "und", "Nacht", "in", "frem\u00b7den", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Irr' ich in Einsamkeit,", "tokens": ["Irr'", "ich", "in", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sing' ich froh durch Berg und Thal;", "tokens": ["Doch", "sing'", "ich", "froh", "durch", "Berg", "und", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df, mir wird kein Leid.", "tokens": ["Ich", "wei\u00df", ",", "mir", "wird", "kein", "Leid", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sch\u00fctzt der Himmel liebevoll,", "tokens": ["Sie", "sch\u00fctzt", "der", "Him\u00b7mel", "lie\u00b7be\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4r' er auch z\u00fcrnend mir,", "tokens": ["W\u00e4r'", "er", "auch", "z\u00fcr\u00b7nend", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mir wird kein Schmerz, mir wird kein Leid:", "tokens": ["Mir", "wird", "kein", "Schmerz", ",", "mir", "wird", "kein", "Leid", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Alles ja w\u00fcrd' ", "tokens": ["Denn", "Al\u00b7les", "ja", "w\u00fcrd'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "VAFIN"], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}