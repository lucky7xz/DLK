{"dta.poem.16790": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "149.  Du selbst bist alle Dinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.85", "af:0.14"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wie magstu was begehrn? du selber kanst allein/", "tokens": ["Wie", "mags\u00b7tu", "was", "be\u00b7gehrn", "?", "du", "sel\u00b7ber", "kanst", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "VVINF", "$.", "PPER", "ADV", "VMFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Himme! und die Erd\u2019/ und tausend Engel seyn.", "tokens": ["Der", "Him\u00b7me", "!", "und", "die", "Erd'", "/", "und", "tau\u00b7send", "En\u00b7gel", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "KON", "ART", "NN", "$(", "KON", "CARD", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}