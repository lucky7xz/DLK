{"dta.poem.21493": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6rt ihr es nicht? \u2026  \n 1884.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt ihr es nicht? In meinem Ohre bang", "tokens": ["H\u00f6rt", "ihr", "es", "nicht", "?", "In", "mei\u00b7nem", "Oh\u00b7re", "bang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPER", "PTKNEG", "$.", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ewig t\u00f6nt herber dumpfer Trommelklang.", "tokens": ["E\u00b7wig", "t\u00f6nt", "her\u00b7ber", "dum\u00b7pfer", "Trom\u00b7mel\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "In heller Lenznacht in der Nachtigall", "tokens": ["In", "hel\u00b7ler", "Lenz\u00b7nacht", "in", "der", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vertr\u00e4umtes Lied rauscht schwerer Waffenschall.", "tokens": ["Ver\u00b7tr\u00e4um\u00b7tes", "Lied", "rauscht", "schwe\u00b7rer", "Waf\u00b7fen\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Sommer gl\u00fcht in dunkler Rosen Duft \u2014", "tokens": ["Der", "Som\u00b7mer", "gl\u00fcht", "in", "dunk\u00b7ler", "Ro\u00b7sen", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Rossestampfen schallt es durch die Luft.", "tokens": ["Wie", "Ros\u00b7se\u00b7stamp\u00b7fen", "schallt", "es", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn der Wein im gr\u00fcnen Glase quillt, \u2014", "tokens": ["Und", "wenn", "der", "Wein", "im", "gr\u00fc\u00b7nen", "Gla\u00b7se", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rst nicht das Schlachtwort, das so blutig schrillt?", "tokens": ["H\u00f6rst", "nicht", "das", "Schlacht\u00b7wort", ",", "das", "so", "blu\u00b7tig", "schrillt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O Winternacht! Der Sturmwind heulend f\u00e4hrt,", "tokens": ["O", "Win\u00b7ter\u00b7nacht", "!", "Der", "Sturm\u00b7wind", "heu\u00b7lend", "f\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die starrenden Wege leer sein Odem kehrt.", "tokens": ["Die", "star\u00b7ren\u00b7den", "We\u00b7ge", "leer", "sein", "O\u00b7dem", "kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Vergebens gl\u00fcht am Feuerheerd der Rost,", "tokens": ["Ver\u00b7ge\u00b7bens", "gl\u00fcht", "am", "Feu\u00b7er\u00b7heerd", "der", "Rost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "St\u00e4rker als Feuer brennt der kalte Frost.", "tokens": ["St\u00e4r\u00b7ker", "als", "Feu\u00b7er", "brennt", "der", "kal\u00b7te", "Frost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "An Haus und Wand und an des Weg\u2019s Geleis\u2019", "tokens": ["An", "Haus", "und", "Wand", "und", "an", "des", "Weg's", "Ge\u00b7leis'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Fliegt Schnee und knarrt das demantharte Eis.", "tokens": ["Fliegt", "Schnee", "und", "knarrt", "das", "de\u00b7man\u00b7thar\u00b7te", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "O Winternacht! Durch Eis und fliegenden Schnee", "tokens": ["O", "Win\u00b7ter\u00b7nacht", "!", "Durch", "Eis", "und", "flie\u00b7gen\u00b7den", "Schnee"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Lauter als Sturmgeist, schreit ein wildes Weh.", "tokens": ["Lau\u00b7ter", "als", "Sturm\u00b7geist", ",", "schreit", "ein", "wil\u00b7des", "Weh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "NN", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Wie an dem Strand die w\u00fcste Woge hallt,", "tokens": ["Wie", "an", "dem", "Strand", "die", "w\u00fcs\u00b7te", "Wo\u00b7ge", "hallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Nacht hindurch Geschrei und Schlachtruf schallt.", "tokens": ["Die", "Nacht", "hin\u00b7durch", "Ge\u00b7schrei", "und", "Schlach\u00b7truf", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "In dunklen Schaaren dr\u00e4ngt es finster an,", "tokens": ["In", "dunk\u00b7len", "Schaa\u00b7ren", "dr\u00e4ngt", "es", "fins\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Beil und Hammer wogt es dumpf heran.", "tokens": ["Mit", "Beil", "und", "Ham\u00b7mer", "wogt", "es", "dumpf", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Zerlumpte Haufen, wie vom Sturm verwirrt,", "tokens": ["Zer\u00b7lump\u00b7te", "Hau\u00b7fen", ",", "wie", "vom", "Sturm", "ver\u00b7wirrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PWAV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Eisen dr\u00f6hnt, das blanke Messer klirrt.", "tokens": ["Das", "Ei\u00b7sen", "dr\u00f6hnt", ",", "das", "blan\u00b7ke", "Mes\u00b7ser", "klirrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Das Angesicht, bla\u00df wie ein Wintertag,", "tokens": ["Das", "An\u00b7ge\u00b7sicht", ",", "bla\u00df", "wie", "ein", "Win\u00b7ter\u00b7tag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sagt, wie das Elend gar so fressen mag.", "tokens": ["Sagt", ",", "wie", "das", "E\u00b7lend", "gar", "so", "fres\u00b7sen", "mag."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Das Auge tief, die Wange hohl und schmal,", "tokens": ["Das", "Au\u00b7ge", "tief", ",", "die", "Wan\u00b7ge", "hohl", "und", "schmal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Stirn\u2019 und Wang\u2019 der Krankheit brand\u2019ges Mal.", "tokens": ["Auf", "Stirn'", "und", "Wang'", "der", "Krank\u00b7heit", "bran\u00b7d'\u00b7ges", "Mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.14": {"line.1": {"text": "Das Haar gel\u00f6st auf braunen Nacken h\u00e4ngt,", "tokens": ["Das", "Haar", "ge\u00b7l\u00f6st", "auf", "brau\u00b7nen", "Na\u00b7cken", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den nackten, schweren Fu\u00df kein Schuh umzw\u00e4ngt.", "tokens": ["Den", "nack\u00b7ten", ",", "schwe\u00b7ren", "Fu\u00df", "kein", "Schuh", "um\u00b7zw\u00e4ngt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Das Banner dr\u00e4ut, wie Herzblut dunkelroth,", "tokens": ["Das", "Ban\u00b7ner", "dr\u00e4ut", ",", "wie", "Herz\u00b7blut", "dun\u00b7kel\u00b7roth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dort die Fahn\u2019, schwarz wie der W\u00fcrger Tod.", "tokens": ["Und", "dort", "die", "Fahn'", ",", "schwarz", "wie", "der", "W\u00fcr\u00b7ger", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Parol\u2019 die Frag: Was f\u00fcr ein seltsam Wesen?", "tokens": ["Pa\u00b7rol'", "die", "Frag", ":", "Was", "f\u00fcr", "ein", "selt\u00b7sam", "We\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "PWS", "APPR", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Antwort: Vom Elend wollen wir genesen.", "tokens": ["Ant\u00b7wort", ":", "Vom", "E\u00b7lend", "wol\u00b7len", "wir", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPRART", "NN", "VMFIN", "PPER", "VVPP", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}}, "stanza.17": {"line.1": {"text": "Es dr\u00e4ngt heran, es wogt die dunkle Fluth", "tokens": ["Es", "dr\u00e4ngt", "he\u00b7ran", ",", "es", "wogt", "die", "dunk\u00b7le", "Fluth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und in den L\u00fcften schwimmt\u2019s wie schwarzes Blut.", "tokens": ["Und", "in", "den", "L\u00fcf\u00b7ten", "schwimmt's", "wie", "schwar\u00b7zes", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Auf, auf die Herzen, die am Thron ihr sitzt,", "tokens": ["Auf", ",", "auf", "die", "Her\u00b7zen", ",", "die", "am", "Thron", "ihr", "sitzt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Gold und hei\u00dfem Demantglanz umblitzt!", "tokens": ["Von", "Gold", "und", "hei\u00b7\u00dfem", "De\u00b7mant\u00b7glanz", "um\u00b7blitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.19": {"line.1": {"text": "Auf, auf die Herzen, die beim duft\u2019gen Mahl", "tokens": ["Auf", ",", "auf", "die", "Her\u00b7zen", ",", "die", "beim", "duft'\u00b7gen", "Mahl"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$,", "APPR", "ART", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr schwingt den silberstrahlenden Weinpokal.", "tokens": ["Ihr", "schwingt", "den", "sil\u00b7ber\u00b7strah\u00b7len\u00b7den", "Wein\u00b7po\u00b7kal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.20": {"line.1": {"text": "Seht ihr es nicht, das Zeichen, das sich hebt?", "tokens": ["Seht", "ihr", "es", "nicht", ",", "das", "Zei\u00b7chen", ",", "das", "sich", "hebt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$,", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein eherner Kelch vor euren Augen schwebt!", "tokens": ["Ein", "e\u00b7her\u00b7ner", "Kelch", "vor", "eu\u00b7ren", "Au\u00b7gen", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.21": {"line.1": {"text": "Ein eherner Kelch mit Thr\u00e4nen angef\u00fcllt,", "tokens": ["Ein", "e\u00b7her\u00b7ner", "Kelch", "mit", "Thr\u00e4\u00b7nen", "an\u00b7ge\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Dornen und in Stacheln eingeh\u00fcllt.", "tokens": ["In", "Dor\u00b7nen", "und", "in", "Sta\u00b7cheln", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.22": {"line.1": {"text": "H\u00f6rt aus der Tiefe schmerzenbanges Schrein \u2014", "tokens": ["H\u00f6rt", "aus", "der", "Tie\u00b7fe", "schmer\u00b7zen\u00b7ban\u00b7ges", "Schrein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf, auf die Herzen, la\u00dft die Liebe ein!", "tokens": ["Auf", ",", "auf", "die", "Her\u00b7zen", ",", "la\u00dft", "die", "Lie\u00b7be", "ein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.23": {"line.1": {"text": "Rei\u00dft ab das rothe Gold vom Sammtgewand,", "tokens": ["Rei\u00dft", "ab", "das", "ro\u00b7the", "Gold", "vom", "Sammt\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Demantschmuck, das schimmernde Perlenband.", "tokens": ["Den", 