{"dta.poem.19561": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Liebesjahr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Hat sich die Kelter gedreht? Tanzt dort mit dem Laub eine Flocke?", "tokens": ["Hat", "sich", "die", "Kel\u00b7ter", "ge\u00b7dreht", "?", "Tanzt", "dort", "mit", "dem", "Laub", "ei\u00b7ne", "Flo\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ART", "NN", "VVFIN", "$.", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zuckte der Blitz im August? Bl\u00fchten die Kirschen im Mai?", "tokens": ["Zuck\u00b7te", "der", "Blitz", "im", "Au\u00b7gust", "?", "Bl\u00fch\u00b7ten", "die", "Kir\u00b7schen", "im", "Mai", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$.", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Bl\u00fcthen und Aehren und Trauben erblickt' ich in schwellendem", "tokens": ["Bl\u00fc\u00b7then", "und", "A\u00b7eh\u00b7ren", "und", "Trau\u00b7ben", "er\u00b7blickt'", "ich", "in", "schwel\u00b7len\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "PPER", "APPR", "ADJA"], "meter": "+---+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Um das geliebteste Haupt und ich erblicke sie noch.", "tokens": ["Um", "das", "ge\u00b7lieb\u00b7tes\u00b7te", "Haupt", "und", "ich", "er\u00b7bli\u00b7cke", "sie", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}}}}}