{"dta.poem.23572": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Blitzableiter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wilder Geist, wie Wetterwolke", "tokens": ["Wil\u00b7der", "Geist", ",", "wie", "Wet\u00b7ter\u00b7wol\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber uns zusammenzieht:", "tokens": ["Ue\u00b7ber", "uns", "zu\u00b7sam\u00b7men\u00b7zieht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, wie hilft man unserm Volke,", "tokens": ["Ach", ",", "wie", "hilft", "man", "un\u00b7serm", "Vol\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ihm nicht ein Leid geschieht?", "tokens": ["Da\u00df", "ihm", "nicht", "ein", "Leid", "ge\u00b7schieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wettersch\u00e4den zu verh\u00fcten,", "tokens": ["Wet\u00b7ter\u00b7sch\u00e4\u00b7den", "zu", "ver\u00b7h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Giebt es ja ein Mittel jetzt;", "tokens": ["Giebt", "es", "ja", "ein", "Mit\u00b7tel", "jetzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr des wilden Geistes W\u00fcthen", "tokens": ["F\u00fcr", "des", "wil\u00b7den", "Geis\u00b7tes", "W\u00fct\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Giebt's ein Mittel auch zuletzt.", "tokens": ["Giebt's", "ein", "Mit\u00b7tel", "auch", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00e4nget an die Blitzableiter", "tokens": ["H\u00e4n\u00b7get", "an", "die", "Blitz\u00b7ab\u00b7lei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Titel, W\u00fcrden, Orden, Geld,", "tokens": ["Ti\u00b7tel", ",", "W\u00fcr\u00b7den", ",", "Or\u00b7den", ",", "Geld", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Wetter wird gleich heiter,", "tokens": ["Und", "das", "Wet\u00b7ter", "wird", "gleich", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und beruhigt ist die Welt.", "tokens": ["Und", "be\u00b7ru\u00b7higt", "ist", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}