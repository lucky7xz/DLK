{"dta.poem.1829": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Vier Winde sind die unsre Ruhe st\u00f6ren!", "tokens": ["Vier", "Win\u00b7de", "sind", "die", "uns\u00b7re", "Ru\u00b7he", "st\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ART", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald scherzt um uns der Hoffnung leichter West/", "tokens": ["Bald", "scherzt", "um", "uns", "der", "Hoff\u00b7nung", "leich\u00b7ter", "West", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bald sp\u00fcrt man da\u00df ein furchtsam Ostwind bl\u00e4st;", "tokens": ["Bald", "sp\u00fcrt", "man", "da\u00df", "ein", "furcht\u00b7sam", "Ost\u00b7wind", "bl\u00e4st", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOUS", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bald pfleget uns der besten Sinnen Pest/", "tokens": ["Bald", "pfle\u00b7get", "uns", "der", "bes\u00b7ten", "Sin\u00b7nen", "Pest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein fauler S\u00fcd der Freude zu beth\u00f6ren;", "tokens": ["Ein", "fau\u00b7ler", "S\u00fcd", "der", "Freu\u00b7de", "zu", "be\u00b7th\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bald l\u00e4st sich drauff des Traurens Nordwind h\u00f6ren.", "tokens": ["Bald", "l\u00e4st", "sich", "drauff", "des", "Trau\u00b7rens", "Nord\u00b7wind", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PAV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So mancher Sturm kan unsre Ruhe st\u00f6ren.", "tokens": ["So", "man\u00b7cher", "Sturm", "kan", "uns\u00b7re", "Ru\u00b7he", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}