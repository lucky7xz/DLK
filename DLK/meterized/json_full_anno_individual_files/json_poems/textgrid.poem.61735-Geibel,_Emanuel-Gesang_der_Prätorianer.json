{"textgrid.poem.61735": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der Pr\u00e4torianer", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil dem Gewalt'gen, Heil dem Kaiser,", "tokens": ["Heil", "dem", "Ge\u00b7walt'\u00b7gen", ",", "Heil", "dem", "Kai\u00b7ser", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dem Herrn im blut'gen Kriegsgezelt!", "tokens": ["Dem", "Herrn", "im", "blut'\u00b7gen", "Kriegs\u00b7ge\u00b7zelt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er gibt uns Gold und Lorbeerreiser,", "tokens": ["Er", "gibt", "uns", "Gold", "und", "Lor\u00b7beer\u00b7rei\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir geben ihm daf\u00fcr die Welt.", "tokens": ["Wir", "ge\u00b7ben", "ihm", "da\u00b7f\u00fcr", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn scheu vor unsrer Adler Blitzen", "tokens": ["Denn", "scheu", "vor", "uns\u00b7rer", "Ad\u00b7ler", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Boden fliegt der V\u00f6lker Blick;", "tokens": ["Zu", "Bo\u00b7den", "fliegt", "der", "V\u00f6l\u00b7ker", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir tragen auf den Lanzenspitzen", "tokens": ["Wir", "tra\u00b7gen", "auf", "den", "Lan\u00b7zens\u00b7pit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Heil des Reichs, der Welt Geschick.", "tokens": ["Das", "Heil", "des", "Reichs", ",", "der", "Welt", "Ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Als Herrscher ziehn wir durch die Lande,", "tokens": ["Als", "Herr\u00b7scher", "ziehn", "wir", "durch", "die", "Lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat den Willen, wir die Macht;", "tokens": ["Er", "hat", "den", "Wil\u00b7len", ",", "wir", "die", "Macht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hohnlachend jedem Widerstande", "tokens": ["Hohn\u00b7la\u00b7chend", "je\u00b7dem", "Wi\u00b7der\u00b7stan\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4\u00dft er uns los im Feld der Schlacht.", "tokens": ["L\u00e4\u00dft", "er", "uns", "los", "im", "Feld", "der", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob tausend \u00fcber tausend sinken,", "tokens": ["Ob", "tau\u00b7send", "\u00fc\u00b7ber", "tau\u00b7send", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "CARD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was k\u00fcmmert's ihn? Er ", "tokens": ["Was", "k\u00fcm\u00b7mert's", "ihn", "?", "Er"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wir bringen ihm beim Schall der Zinken", "tokens": ["Wir", "brin\u00b7gen", "ihm", "beim", "Schall", "der", "Zin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Aus jedem Sturm den Sieg zur\u00fcck.", "tokens": ["Aus", "je\u00b7dem", "Sturm", "den", "Sieg", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann lobt und kost er seine Meute,", "tokens": ["Dann", "lobt", "und", "kost", "er", "sei\u00b7ne", "Meu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was uns zufiel, teilt er ein;", "tokens": ["Und", "was", "uns", "zu\u00b7fiel", ",", "teilt", "er", "ein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "F\u00fcr ihn der Ruhm; f\u00fcr uns die Beute,", "tokens": ["F\u00fcr", "ihn", "der", "Ruhm", ";", "f\u00fcr", "uns", "die", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$.", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr uns die Weiber und der Wein!", "tokens": ["F\u00fcr", "uns", "die", "Wei\u00b7ber", "und", "der", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da bricht die Lust aus allen Z\u00fcgeln,", "tokens": ["Da", "bricht", "die", "Lust", "aus", "al\u00b7len", "Z\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da flammt die Feuersbrunst ins Tal;", "tokens": ["Da", "flammt", "die", "Feu\u00b7ers\u00b7brunst", "ins", "Tal", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf St\u00e4dteschutt und Leichenh\u00fcgeln", "tokens": ["Auf", "St\u00e4d\u00b7te\u00b7schutt", "und", "Lei\u00b7chen\u00b7h\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Beginnen wir das Bakchanal.", "tokens": ["Be\u00b7gin\u00b7nen", "wir", "das", "Bak\u00b7cha\u00b7nal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So w\u00e4lzt er uns wie Lavafluten", "tokens": ["So", "w\u00e4lzt", "er", "uns", "wie", "La\u00b7va\u00b7flu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Siegesfeld zu Siegesfeld", "tokens": ["Von", "Sie\u00b7ges\u00b7feld", "zu", "Sie\u00b7ges\u00b7feld"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schreibt von Nacht zu Nacht mit Gluten", "tokens": ["Und", "schreibt", "von", "Nacht", "zu", "Nacht", "mit", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Machtgebot ans Himmelszelt.", "tokens": ["Sein", "Macht\u00b7ge\u00b7bot", "ans", "Him\u00b7mels\u00b7zelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er spricht \u2013 wer wagt zu widersprechen!", "tokens": ["Er", "spricht", "\u2013", "wer", "wagt", "zu", "wi\u00b7der\u00b7spre\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wer fragt noch, was beschworen sei!", "tokens": ["Wer", "fragt", "noch", ",", "was", "be\u00b7schwo\u00b7ren", "sei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$,", "PWS", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er will \u2013 und die Vertr\u00e4ge brechen,", "tokens": ["Er", "will", "\u2013", "und", "die", "Ver\u00b7tr\u00e4\u00b7ge", "bre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die moos'gen Tafeln, morsch entzwei.", "tokens": ["Die", "moos'\u00b7gen", "Ta\u00b7feln", ",", "morsch", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mag knirschend ihn der B\u00fcrger hassen:", "tokens": ["Mag", "knir\u00b7schend", "ihn", "der", "B\u00fcr\u00b7ger", "has\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er bangt und schweigt, das ist genug;", "tokens": ["Er", "bangt", "und", "schweigt", ",", "das", "ist", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PDS", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der P\u00f6bel jubelt auf den Gassen", "tokens": ["Der", "P\u00f6\u00b7bel", "ju\u00b7belt", "auf", "den", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stets dem, der ihn in Ketten schlug.", "tokens": ["Stets", "dem", ",", "der", "ihn", "in", "Ket\u00b7ten", "schlug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ist das Recht? Ein Schreck der Zahmen \u2013", "tokens": ["Was", "ist", "das", "Recht", "?", "Ein", "Schreck", "der", "Zah\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was ist die Freiheit? Wahn und Spott \u2013", "tokens": ["Was", "ist", "die", "Frei\u00b7heit", "?", "Wahn", "und", "Spott", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was sind die G\u00f6tter? Hohle Namen \u2013", "tokens": ["Was", "sind", "die", "G\u00f6t\u00b7ter", "?", "Hoh\u00b7le", "Na\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Kaiser ist auf Erden Gott.", "tokens": ["Der", "Kai\u00b7ser", "ist", "auf", "Er\u00b7den", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Triumph! Triumph! Und wenn hienieden", "tokens": ["Tri\u00b7umph", "!", "Tri\u00b7umph", "!", "Und", "wenn", "hien\u00b7ie\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "KON", "KOUS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ke