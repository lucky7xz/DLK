{"dta.poem.20948": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XiI .  \n Gonsalvo.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Witz gab Spanien ein weites K\u00f6nigreich/", "tokens": ["Mein", "Witz", "gab", "Spa\u00b7ni\u00b7en", "ein", "wei\u00b7tes", "K\u00f6\u00b7nig\u00b7reich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es war an Kopff und Faust mir keiner endlich gleich;", "tokens": ["Es", "war", "an", "Kopff", "und", "Faust", "mir", "kei\u00b7ner", "end\u00b7lich", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "PPER", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es hat des Neides Hand selbst umb mein Grab ge-", "tokens": ["Es", "hat", "des", "Nei\u00b7des", "Hand", "selbst", "umb", "mein", "Grab", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "ADV", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "schrieben/ (ben.", "tokens": ["schrie\u00b7ben", "/", "(", "ben", "."], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "$(", "VVPP", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "Gonsalvo hat den Feind aus Hertz' und Land getrie-", "tokens": ["Gon\u00b7sal\u00b7vo", "hat", "den", "Feind", "aus", "Hertz'", "und", "Land", "ge\u00b7trie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}