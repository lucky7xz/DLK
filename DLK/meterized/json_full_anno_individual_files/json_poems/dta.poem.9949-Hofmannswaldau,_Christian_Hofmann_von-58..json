{"dta.poem.9949": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "58.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allen menschen bin ich n\u00fctze/ f\u00fcrsten werd ich vorgesetzt/", "tokens": ["Al\u00b7len", "men\u00b7schen", "bin", "ich", "n\u00fct\u00b7ze", "/", "f\u00fcrs\u00b7ten", "werd", "ich", "vor\u00b7ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVFIN", "$(", "VVFIN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Auff dem land und in den st\u00e4dten iederzeit sehr hoch gesch\u00e4tzt:", "tokens": ["Auff", "dem", "land", "und", "in", "den", "st\u00e4d\u00b7ten", "ie\u00b7der\u00b7zeit", "sehr", "hoch", "ge\u00b7sch\u00e4tzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wenn man kommet in die welt/ wird alsbald nach mir geeilet/", "tokens": ["Wenn", "man", "kom\u00b7met", "in", "die", "welt", "/", "wird", "als\u00b7bald", "nach", "mir", "ge\u00b7ei\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "APPR", "ART", "NN", "$(", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "+-+---+--++--+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Werd\u2019 in alles/ was natur nur erzeuget/ au\u00dfgetheilet.", "tokens": ["Werd'", "in", "al\u00b7les", "/", "was", "na\u00b7tur", "nur", "er\u00b7zeu\u00b7get", "/", "au\u00df\u00b7ge\u00b7thei\u00b7let", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "$(", "PWS", "NN", "ADV", "VVFIN", "$(", "VVPP", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}}}}}