{"textgrid.poem.55298": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter halbverwelkten Maien", "tokens": ["Un\u00b7ter", "halb\u00b7ver\u00b7welk\u00b7ten", "Mai\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schl\u00e4ft der liebe Freund so still;", "tokens": ["Schl\u00e4ft", "der", "lie\u00b7be", "Freund", "so", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Oh! wie soll es ihn erfreuen,", "tokens": ["Oh", "!", "wie", "soll", "es", "ihn", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ich ihm vertrauen will:", "tokens": ["Was", "ich", "ihm", "ver\u00b7trau\u00b7en", "will", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne Wurzeln dieses Reisig,", "tokens": ["Oh\u00b7ne", "Wur\u00b7zeln", "die\u00b7ses", "Rei\u00b7sig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Es verdorrt das junge Blut;", "tokens": ["Es", "ver\u00b7dorrt", "das", "jun\u00b7ge", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA",