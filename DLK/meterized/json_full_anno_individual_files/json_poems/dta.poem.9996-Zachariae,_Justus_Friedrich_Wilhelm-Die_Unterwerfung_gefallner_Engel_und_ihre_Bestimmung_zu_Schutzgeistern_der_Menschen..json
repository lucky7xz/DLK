{"dta.poem.9996": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Unterwerfung gefallner Engel  \n und ihre Bestimmung  \n zu  \n Schutzgeistern der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alsbald gab Orion Befehl, die Posaune zu blasen;", "tokens": ["Als\u00b7bald", "gab", "O\u00b7rion", "Be\u00b7fehl", ",", "die", "Po\u00b7sau\u00b7ne", "zu", "bla\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ein m\u00e4chtiger Cherubim stie\u00df mit harmonischen", "tokens": ["Und", "ein", "m\u00e4ch\u00b7ti\u00b7ger", "Che\u00b7ru\u00b7bim", "stie\u00df", "mit", "har\u00b7mo\u00b7ni\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NE", "VVFIN", "APPR", "ADJA"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "In das \u00e4thersche Metall, die ganze Gegend erschallte", "tokens": ["In", "das", "\u00e4t\u00b7her\u00b7sche", "Me\u00b7tall", ",", "die", "gan\u00b7ze", "Ge\u00b7gend", "er\u00b7schall\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+---+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Von dem Get\u00f6n. Mit fliegenden Schritten begaben", "tokens": ["Von", "dem", "Ge\u00b7t\u00f6n", ".", "Mit", "flie\u00b7gen\u00b7den", "Schrit\u00b7ten", "be\u00b7ga\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$.", "APPR", "ADJA", "NN", "VVINF"], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Unter ihre Standarten und Fahnen. Die gl\u00e4nzenden", "tokens": ["Un\u00b7ter", "ih\u00b7re", "Stand\u00b7ar\u00b7ten", "und", "Fah\u00b7nen", ".", "Die", "gl\u00e4n\u00b7zen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$.", "ART", "ADJA"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Dr\u00e4ngten sich dicht an einander, und mit geh\u00f6rneten", "tokens": ["Dr\u00e4ng\u00b7ten", "sich", "dicht", "an", "ein\u00b7an\u00b7der", ",", "und", "mit", "ge\u00b7h\u00f6r\u00b7ne\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "PRF", "$,", "KON", "APPR", "ADJA"], "meter": "+--+--+--+-+--", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Schlo\u00df sich das s\u00e4mtliche Heer an seinen F\u00fchrer, Orion,", "tokens": ["Schlo\u00df", "sich", "das", "s\u00e4mt\u00b7li\u00b7che", "Heer", "an", "sei\u00b7nen", "F\u00fch\u00b7rer", ",", "O\u00b7rion", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PRF", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,", "NN", "$,"], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.8": {"text": "Neben welchem der hohe Gesandte zum Sprechen be-", "tokens": ["Ne\u00b7ben", "wel\u00b7chem", "der", "ho\u00b7he", "Ge\u00b7sand\u00b7te", "zum", "Spre\u00b7chen", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "APPRART", "NN", "TRUNC"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Ehrerbietige Stille beherrschte die wartenden Schaaren,", "tokens": ["Ehr\u00b7er\u00b7bie\u00b7ti\u00b7ge", "Stil\u00b7le", "be\u00b7herrschte", "die", "war\u00b7ten\u00b7den", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Und mit auf ihn geheftetem Blick, und banger Erwar-", "tokens": ["Und", "mit", "auf", "ihn", "ge\u00b7hef\u00b7te\u00b7tem", "Blick", ",", "und", "ban\u00b7ger", "Er\u00b7wa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "APPR", "PPER", "ADJA", "NN", "$,", "KON", "ADJD", "TRUNC"], "meter": "--+--+--+-+-+--", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Standen sie, seine Worte zu h\u00f6ren; \u2014 voll Anstand", "tokens": ["Stan\u00b7den", "sie", ",", "sei\u00b7ne", "Wor\u00b7te", "zu", "h\u00f6\u00b7ren", ";", "voll", "An\u00b7stand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$.", "$(", "ADJD", "NN"], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}}}}}