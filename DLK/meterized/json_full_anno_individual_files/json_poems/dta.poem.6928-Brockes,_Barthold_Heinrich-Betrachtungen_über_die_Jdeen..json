{"dta.poem.6928": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was die Natur uns zeigt, woran wir uns erfreuen,", "tokens": ["Was", "die", "Na\u00b7tur", "uns", "zeigt", ",", "wo\u00b7ran", "wir", "uns", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVFIN", "$,", "PWAV", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und aller C\u00f6rper Schmuck, der gantzen Erden Pracht", "tokens": ["Und", "al\u00b7ler", "C\u00f6r\u00b7per", "Schmuck", ",", "der", "gant\u00b7zen", "Er\u00b7den", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist sonder Endzweck nur gemacht:", "tokens": ["Ist", "son\u00b7der", "End\u00b7zweck", "nur", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Weil unsre Augen", "tokens": ["Weil", "uns\u00b7re", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Nichts w\u00fcrckliches darinn zu schauen taugen.", "tokens": ["Nichts", "w\u00fcrck\u00b7li\u00b7ches", "da\u00b7rinn", "zu", "schau\u00b7en", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PAV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ja, wenn ein Noth-Gesetz die Himmels-Creise", "tokens": ["Ja", ",", "wenn", "ein", "Noth\u00b7Ge\u00b7setz", "die", "Him\u00b7mels\u00b7Crei\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Beweget und sie f\u00fchrt;", "tokens": ["Be\u00b7we\u00b7get", "und", "sie", "f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "So werden sie auf diese Weise,", "tokens": ["So", "wer\u00b7den", "sie", "auf", "die\u00b7se", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Vom Mechanismo blo\u00df geordnet und regiert.", "tokens": ["Vom", "Me\u00b7cha\u00b7nis\u00b7mo", "blo\u00df", "ge\u00b7ord\u00b7net", "und", "re\u00b7giert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ADV", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}