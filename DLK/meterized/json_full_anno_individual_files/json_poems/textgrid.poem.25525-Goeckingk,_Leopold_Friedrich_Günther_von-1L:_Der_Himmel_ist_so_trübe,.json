, "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gerad' um diese Stunde", "tokens": ["Ge\u00b7rad'", "um", "die\u00b7se", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum Mond' hinauf zu sehn.", "tokens": ["Zum", "Mond'", "hin\u00b7auf", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und du willst nicht erscheinen,", "tokens": ["Und", "du", "willst", "nicht", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Da\u00df unsre Blicke sich", "tokens": ["Da\u00df", "uns\u00b7re", "Bli\u00b7cke", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Auf dir, o Mond, vereinen,", "tokens": ["Auf", "dir", ",", "o", "Mond", ",", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der uns so oft beschlich,", "tokens": ["Der", "uns", "so", "oft", "be\u00b7schlich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wenn Worte das nicht sagten,", "tokens": ["Wenn", "Wor\u00b7te", "das", "nicht", "sag\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PDS", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambi