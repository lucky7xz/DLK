, "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wen wol trifft und geht es an?", "tokens": ["wen", "wol", "trifft", "und", "geht", "es", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seid nicht ihr's, ihr Liebsten beide,", "tokens": ["Seid", "nicht", "ih\u00b7r's", ",", "ihr", "Liebs\u00b7ten", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PTKNEG", "NE", "$,", "PPOSAT", "NN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "denen die und andre Freude", "tokens": ["de\u00b7nen", "die", "und", "and\u00b7re", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRELS", "ART", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "blos zur Lust wird angetan?", "tokens": ["blos", "zur", "Lust", "wird", "an\u00b7ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Ja, ihr seid's! So braucht der Ehren,", "tokens": ["Ja", ",", "ihr", "sei\u00b7d's", "!", "So", "braucht", "der", "Eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die euch Alles hilft vermehren,", "tokens": ["die", "euch", "Al\u00b7les", "hilft", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "was sich freuet weit und breit!", "tokens": ["was", "sich", "freu\u00b7et", "weit", "und", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilt zur Lust, lauft zu den T\u00e4nzen,", "tokens": ["Eilt", "zur", "Lust", ",", "lauft", "zu", "den", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "weil ihr noch seid in den Lenzen,", "tokens": ["weil", "ihr", "noch", "seid", "in", "den", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "euren und des Jahres Zeit.", "tokens": ["eu\u00b7ren", "und", "des", "Jah\u00b7res", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Norden, Osten, S\u00fcd und Westen", "tokens": ["Nor\u00b7den", ",", "Os\u00b7ten", ",", "S\u00fcd", "und", "Wes\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "blasen den berauschten G\u00e4sten", "tokens": ["bla\u00b7sen", "den", "be\u00b7rauschten", "G\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "k\u00fchle, linde L\u00fcfte zu.", "tokens": ["k\u00fch\u00b7le", ",", "lin\u00b7de", "L\u00fcf\u00b7te", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr indessen, weil sie trinken,", "tokens": ["Ihr", "in\u00b7des\u00b7sen", ",", "weil", "sie", "trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "la\u00dft die matten Augen sinken", "tokens": ["la\u00dft", "die", "mat\u00b7ten", "Au\u00b7gen", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "in die angen\u00e4me Ruh'!", "tokens": ["in", "die", "an\u00b7ge\u00b7n\u00e4\u00b7me", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Wol! Wer will euch das verwehren,", "tokens": ["Wol", "!", "Wer", "will", "euch", "das", "ver\u00b7weh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "VMFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was wir Alle doch begehren?", "tokens": ["was", "wir", "Al\u00b7le", "doch", "be\u00b7geh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht das sanfte Rasten ein!", "tokens": ["Geht", "das", "sanf\u00b7te", "Ras\u00b7ten", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ihr schlafet, was ihr wachet,", "tokens": ["Was", "ihr", "schla\u00b7fet", ",", "was", "ihr", "wa\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "was ihr lachet, was ihr machet,", "tokens": ["was", "ihr", "la\u00b7chet", ",", "was", "ihr", "ma\u00b7chet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "werden lauter Scherze sein,", "tokens": ["wer\u00b7den", "lau\u00b7ter", "Scher\u00b7ze", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "solche Scherze, so mit nichten", "tokens": ["sol\u00b7che", "Scher\u00b7ze", ",", "so", "mit", "nich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "ADV", "APPR", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "blo\u00dfe Scherze sind zu richten,", "tokens": ["blo\u00b7\u00dfe", "Scher\u00b7ze", "sind", "zu", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df hernach di\u00df sage frei,", "tokens": ["da\u00df", "her\u00b7nach", "di\u00df", "sa\u00b7ge", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "VVFIN", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "warum ihr euch itzt wollt herzen,", "tokens": ["wa\u00b7rum", "ihr", "euch", "itzt", "wollt", "her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df zugleich in euren Scherzen", "tokens": ["da\u00df", "zu\u00b7gleich", "in", "eu\u00b7ren", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schimpf und Ernst gewesen sei.", "tokens": ["Schimpf", "und", "Ernst", "ge\u00b7we\u00b7sen", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}