{"textgrid.poem.48573": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. Auf Herrn Peter Kuchens und Jungfrau Helenen Ilgens ihre Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und was ist es fast von N\u00f6ten,", "tokens": ["Und", "was", "ist", "es", "fast", "von", "N\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df sich m\u00fchen die Poeten,", "tokens": ["da\u00df", "sich", "m\u00fc\u00b7hen", "die", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Br\u00e4utgam, um dein Hochzeitfest,", "tokens": ["Br\u00e4ut\u00b7gam", ",", "um", "dein", "Hoch\u00b7zeit\u00b7fest", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "da das junge Jahr in Allen", "tokens": ["da", "das", "jun\u00b7ge", "Jahr", "in", "Al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "dir und Deiner zu Gefallen", "tokens": ["dir", "und", "Dei\u00b7ner", "zu", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "KON", "PPOSAT", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "einen Brauttanz h\u00f6ren l\u00e4\u00dft?", "tokens": ["ei\u00b7nen", "Braut\u00b7tanz", "h\u00f6\u00b7ren", "l\u00e4\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der gesunde Mai kommt gangen", "tokens": ["Der", "ge\u00b7sun\u00b7de", "Mai", "kommt", "gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "in den ganz verbl\u00fcmten Wangen", "tokens": ["in", "den", "ganz", "ver\u00b7bl\u00fcm\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und verj\u00fcngt euch seinen Schein.", "tokens": ["und", "ver\u00b7j\u00fcngt", "euch", "sei\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ph\u00f6bus sendet seine Stralen,", "tokens": ["Ph\u00f6\u00b7bus", "sen\u00b7det", "sei\u00b7ne", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "l\u00e4\u00dft den Platz mit Farben malen", "tokens": ["l\u00e4\u00dft", "den", "Platz", "mit", "Far\u00b7ben", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "da der erste Reihn soll sein.", "tokens": ["da", "der", "ers\u00b7te", "Reihn", "soll", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zephyr fleugt mit offnem Munde", "tokens": ["Ze\u00b7phyr", "fleugt", "mit", "off\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und haucht aus dem Blumenschlunde", "tokens": ["und", "haucht", "aus", "dem", "Blu\u00b7men\u00b7schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "mancher Blumen liebe Zier.", "tokens": ["man\u00b7cher", "Blu\u00b7men", "lie\u00b7be", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aklei, Tulpen und Narzissen", "tokens": ["Ak\u00b7lei", ",", "Tul\u00b7pen", "und", "Nar\u00b7zis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "sieht man aus dem Boden sprie\u00dfen,", "tokens": ["sieht", "man", "aus", "dem", "Bo\u00b7den", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "den ihr tretet, f\u00fcr und f\u00fcr.", "tokens": ["den", "ihr", "tre\u00b7tet", ",", "f\u00fcr", "und", "f\u00fcr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Die gelehrten Nachtigallen", "tokens": ["Die", "ge\u00b7lehr\u00b7ten", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schrein euch zu mit lauten Schallen:", "tokens": ["schrein", "euch", "zu", "mit", "lau\u00b7ten", "Schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fcck, Gl\u00fcck, Gl\u00fcck, du trautes Paar!", "tokens": ["Gl\u00fcck", ",", "Gl\u00fcck", ",", "Gl\u00fcck", ",", "du", "trau\u00b7tes", "Paar", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir, dir, dir gilt unser Singen,", "tokens": ["Dir", ",", "dir", ",", "dir", "gilt", "un\u00b7ser", "Sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "dem die T\u00e4ler widerklingen!", "tokens": ["dem", "die", "T\u00e4\u00b7ler", "wi\u00b7der\u00b7klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "ruft des Federp\u00f6fels Schaar.", "tokens": ["ruft", "des", "Fe\u00b7der\u00b7p\u00f6\u00b7fels", "Schaar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf den Feldern, in den Auen", "tokens": ["Auf", "den", "Fel\u00b7dern", ",", "in", "den", "Au\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "habt ihr eure Lust zu schauen;", "tokens": ["habt", "ihr", "eu\u00b7re", "Lust", "zu", "schau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles schicket sich in euch.", "tokens": ["Al\u00b7les", "schi\u00b7cket", "sich", "in", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die verbulten Heerden scherzen,", "tokens": ["Die", "ver\u00b7bul\u00b7ten", "Heer\u00b7den", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "wenn sie euch sehn sehnlich herzen,", "tokens": ["wenn", "sie", "euch", "sehn", "sehn\u00b7lich", "her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und umfangen sich zugleich.", "tokens": ["und", "um\u00b7fan\u00b7gen", "sich", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Da\u00df die ", "tokens": ["Da\u00df", "die"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "da\u00df um Bulerinnen tauschet", "tokens": ["da\u00df", "um", "Bu\u00b7le\u00b7rin\u00b7nen", "tau\u00b7schet"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "manches liebes Wasservolk,", "tokens": ["man\u00b7ches", "lie\u00b7bes", "Was\u00b7ser\u00b7volk", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df die P\u00fcscher sanfter brausen,", "tokens": ["da\u00df", "die", "P\u00fc\u00b7scher", "sanf\u00b7ter", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "da\u00df die L\u00fcfte linder sausen", "tokens": ["da\u00df", "die", "L\u00fcf\u00b7te", "lin\u00b7der", "sau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "und uns tr\u00fcbet keine Wolk':", "tokens": ["und", "uns", "tr\u00fc\u00b7bet", "kei\u00b7ne", "Wol\u00b7k'", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Alles di\u00df und anders Alles,", "tokens": ["Al\u00b7les", "di\u00df", "und", "an\u00b7ders", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "KON", "ADV", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was uns wol tut gleiches Falles,", "tokens": ["was", "uns", "wol", "tut", "glei\u00b7ches", "Fal\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wen wol trifft und geht es an?", "tokens": ["wen", "wol", "trifft", "und", "geht", "es", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seid nicht ihr's, ihr Liebsten beide,", "tokens": ["Seid", "nicht", "ih\u00b7r's", ",", "ihr", "Liebs\u00b7ten", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PTKNEG", "NE", "$,", "PPOSAT", "NN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "denen die und andre Freude", "tokens": ["de\u00b7nen", "die", "und", "and\u00b7re", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRELS", "ART", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "blos zur Lust wird angetan?", "tokens": ["blos", "zur", "Lust", "wird", "an\u00b7ge\u00b7tan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ja, ihr seid's! So braucht der Ehren,", "tokens": ["Ja", ",", "ihr", "sei\u00b7d's", "!", "So", "braucht", "der", "Eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "$.", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die euch Alles hilft vermehren,", "tokens": ["die", "euch", "Al\u00b7les", "hilft", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "was sich freuet weit und breit!", "tokens": ["was", "sich", "freu\u00b7et", "weit", "und", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilt zur Lust, lauft zu den T\u00e4nzen,", "tokens": ["Eilt", "zur", "Lust", ",", "lauft", "zu", "den", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "weil ihr noch seid in den Lenzen,", "tokens": ["weil", "ihr", "noch", "seid", "in", "den", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "euren und des Jahres Zeit.", "tokens": ["eu\u00b7ren", "und", "des", "Jah\u00b7res", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Norden, Osten, S\u00fcd und Westen", "tokens": ["Nor\u00b7den", ",", "Os\u00b7ten", ",", "S\u00fcd", "und", "Wes\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "blasen den berauschten G\u00e4sten", "tokens": ["bla\u00b7sen", "den", "be\u00b7rauschten", "G\u00e4s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "k\u00fchle, linde L\u00fcfte zu.", "tokens": ["k\u00fch\u00b7le", ",", "lin\u00b7de", "L\u00fcf\u00b7te", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr indessen, weil sie trinken,", "tokens": ["Ihr", "in\u00b7des\u00b7sen", ",", "weil", "sie", "trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "la\u00dft die matten Augen sinken", "tokens": ["la\u00dft", "die", "mat\u00b7ten", "Au\u00b7gen", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "in die angen\u00e4me Ruh'!", "tokens": ["in", "die", "an\u00b7ge\u00b7n\u00e4\u00b7me", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wol! Wer will euch das verwehren,", "tokens": ["Wol", "!", "Wer", "will", "euch", "das", "ver\u00b7weh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "VMFIN", "PPER", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was wir Alle doch begehren?", "tokens": ["was", "wir", "Al\u00b7le", "doch", "be\u00b7geh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht das sanfte Rasten ein!", "tokens": ["Geht", "das", "sanf\u00b7te", "Ras\u00b7ten", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ihr schlafet, was ihr wachet,", "tokens": ["Was", "ihr", "schla\u00b7fet", ",", "was", "ihr", "wa\u00b7chet", ","], "token_info": ["word", "wor