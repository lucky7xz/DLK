{"textgrid.poem.33710": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1. Die Seest\u00e4dte", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor allen St\u00e4dten seid ihr Meeresbr\u00e4ute", "tokens": ["Vor", "al\u00b7len", "St\u00e4d\u00b7ten", "seid", "ihr", "Mee\u00b7res\u00b7br\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die herrlichsten; der Tiefe Sch\u00e4tze quellen", "tokens": ["Die", "herr\u00b7lichs\u00b7ten", ";", "der", "Tie\u00b7fe", "Sch\u00e4t\u00b7ze", "quel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu euch empor, des Gl\u00fcckes Segel schwellen", "tokens": ["Zu", "euch", "em\u00b7por", ",", "des", "Gl\u00fc\u00b7ckes", "Se\u00b7gel", "schwel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An euren Strand der fernsten L\u00e4nder Beute.", "tokens": ["An", "eu\u00b7ren", "Strand", "der", "ferns\u00b7ten", "L\u00e4n\u00b7der", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da, Tyrus, Indien dir Weihrauch streute,", "tokens": ["Da", ",", "Ty\u00b7rus", ",", "In\u00b7di\u00b7en", "dir", "Weih\u00b7rauch", "streu\u00b7te", ","], "token_info": ["word", "punc