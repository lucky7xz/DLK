"}, "line.2": {"text": "Wie sein Sch\u00f6pfer, Welten schafft,", "tokens": ["Wie", "sein", "Sch\u00f6p\u00b7fer", ",", "Wel\u00b7ten", "schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in seinem Bildungskreis", "tokens": ["Und", "in", "sei\u00b7nem", "Bil\u00b7dungs\u00b7kreis"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles fromm zu machen wei\u00df.", "tokens": ["Al\u00b7les", "fromm", "zu", "ma\u00b7chen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "T\u00f6dte nicht die junge Braut,", "tokens": ["T\u00f6d\u00b7te", "nicht", "die", "jun\u00b7ge", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n f\u00fcr ihren Mann gebaut,", "tokens": ["Sch\u00f6n", "f\u00fcr", "ih\u00b7ren", "Mann", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, wie Sulamit gestimmt,", "tokens": ["Die", ",", "wie", "Su\u00b7la\u00b7mit", "ge\u00b7stimmt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "NE", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe giebt und Liebe nimmt.", "tokens": ["Lie\u00b7be", "giebt", "und", "Lie\u00b7be", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct