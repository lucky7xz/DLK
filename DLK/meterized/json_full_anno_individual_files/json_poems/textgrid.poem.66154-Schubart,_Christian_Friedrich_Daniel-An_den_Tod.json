{"textgrid.poem.66154": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An den Tod", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tod, du Schrecken der Natur,", "tokens": ["Tod", ",", "du", "Schre\u00b7cken", "der", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer rieselt deine Uhr,", "tokens": ["Im\u00b7mer", "rie\u00b7selt", "dei\u00b7ne", "Uhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die geschwungne Sense blinkt,", "tokens": ["Die", "ge\u00b7schwung\u00b7ne", "Sen\u00b7se", "blinkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gras, und Halm, und Blume sinkt.", "tokens": ["Gras", ",", "und", "Halm", ",", "und", "Blu\u00b7me", "sinkt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00e4he nicht ohn' Unterschied,", "tokens": ["M\u00e4\u00b7he", "nicht", "ohn'", "Un\u00b7ter\u00b7schied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Bl\u00fcmchen, das erst bl\u00fcht,", "tokens": ["Die\u00b7ses", "Bl\u00fcm\u00b7chen", ",", "das", "erst", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses R\u00f6schen erst halbroth;", "tokens": ["Die\u00b7ses", "R\u00f6\u00b7schen", "erst", "hal\u00b7broth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei barmherzig, lieber Tod!", "tokens": ["Sei", "barm\u00b7her\u00b7zig", ",", "lie\u00b7ber", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nimm den holden Knaben nicht,", "tokens": ["Nimm", "den", "hol\u00b7den", "Kna\u00b7ben", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der voll Unschuld im Gesicht", "tokens": ["Der", "voll", "Un\u00b7schuld", "im", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der Brust der Mutter spielt,", "tokens": ["Mit", "der", "Brust", "der", "Mut\u00b7ter", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein erstes Leben f\u00fchlt.", "tokens": ["Und", "sein", "ers\u00b7tes", "Le\u00b7ben", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und den J\u00fcngling schone mir,", "tokens": ["Und", "den", "J\u00fcng\u00b7ling", "scho\u00b7ne", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der am f\u00fchlenden Klavier", "tokens": ["Der", "am", "f\u00fch\u00b7len\u00b7den", "Kla\u00b7vier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Goldne Saiten wiegt und schwingt", "tokens": ["Gold\u00b7ne", "Sai\u00b7ten", "wiegt", "und", "schwingt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein Lied von Liebe singt.", "tokens": ["Und", "ein", "Lied", "von", "Lie\u00b7be", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieh, dort steht ein deutscher Held", "tokens": ["Sieh", ",", "dort", "steht", "ein", "deut\u00b7scher", "Held"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In Columbus' neuer Welt,", "tokens": ["In", "Co\u00b7lum\u00b7bus'", "neu\u00b7er", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der des Wilden Axt nicht scheut;", "tokens": ["Der", "des", "Wil\u00b7den", "Axt", "nicht", "scheut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Tod, ach friste seine Zeit!", "tokens": ["Tod", ",", "ach", "fris\u00b7te", "sei\u00b7ne", "Zeit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schon' den Dichter, dessen Kraft,", "tokens": ["Schon'", "den", "Dich\u00b7ter", ",", "des\u00b7sen", "Kraft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sein Sch\u00f6pfer, Welten schafft,", "tokens": ["Wie", "sein", "Sch\u00f6p\u00b7fer", ",", "Wel\u00b7ten", "schafft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in seinem Bildungskreis", "tokens": ["Und", "in", "sei\u00b7nem", "Bil\u00b7dungs\u00b7kreis"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles fromm zu machen wei\u00df.", "tokens": ["Al\u00b7les", "fromm", "zu", "ma\u00b7chen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "T\u00f6dte nicht die junge Braut,", "tokens": ["T\u00f6d\u00b7te", "nicht", "die", "jun\u00b7ge", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n f\u00fcr ihren Mann gebaut,", "tokens": ["Sch\u00f6n", "f\u00fcr", "ih\u00b7ren", "Mann", "ge\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, wie Sulamit gestimmt,", "tokens": ["Die", ",", "wie", "Su\u00b7la\u00b7mit", "ge\u00b7stimmt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "NE", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe giebt und Liebe nimmt.", "tokens": ["Lie\u00b7be", "giebt", "und", "Lie\u00b7be", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nicht den Frommen in dem Land,", "tokens": ["Nicht", "den", "From\u00b7men", "in", "dem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dessen hochgehobne Hand", "tokens": ["Des\u00b7sen", "hoch\u00b7ge\u00b7hob\u00b7ne", "Hand"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Betend Gottes Himmel st\u00fctzt,", "tokens": ["Be\u00b7tend", "Got\u00b7tes", "Him\u00b7mel", "st\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er Rache niederblitzt.", "tokens": ["Wenn", "er", "Ra\u00b7che", "nie\u00b7der\u00b7blitzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ach, den S\u00fcnder t\u00f6dte nicht!", "tokens": ["Ach", ",", "den", "S\u00fcn\u00b7der", "t\u00f6d\u00b7te", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schreck' ihn nur mit dem Gericht;", "tokens": ["Schreck'", "ihn", "nur", "mit", "dem", "Ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er bang zusammen f\u00e4hrt,", "tokens": ["Da\u00df", "er", "bang", "zu\u00b7sam\u00b7men", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bu\u00dfe weint, und sich bekehrt.", "tokens": ["Bu\u00b7\u00dfe", "weint", ",", "und", "sich", "be\u00b7kehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "In der F\u00fcrsten goldnem Saal,", "tokens": ["In", "der", "F\u00fcrs\u00b7ten", "gold\u00b7nem", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieber Tod, bist du zur Qual;", "tokens": ["Lie\u00b7ber", "Tod", ",", "bist", "du", "zur", "Qual", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schone sie, bis sie vom Wind", "tokens": ["Scho\u00b7ne", "sie", ",", "bis", "sie", "vom", "Wind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eitles Prunks ges\u00e4ttigt sind.", "tokens": ["Eit\u00b7les", "Prunks", "ge\u00b7s\u00e4t\u00b7tigt", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Keinen Reichen t\u00f6dte du!", "tokens": ["Kei\u00b7nen", "Rei\u00b7chen", "t\u00f6d\u00b7te", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Gesunden la\u00df in Ruh'!", "tokens": ["Den", "Ge\u00b7sun\u00b7den", "la\u00df", "in", "Ruh'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Triffst du gute Laune an,", "tokens": ["Triffst", "du", "gu\u00b7te", "Lau\u00b7ne", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So verl\u00e4ngre ihre Bahn!", "tokens": ["So", "ver\u00b7l\u00e4ng\u00b7re", "ih\u00b7re", "Bahn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Aber mu\u00dft du t\u00f6dten, Tod,", "tokens": ["A\u00b7ber", "mu\u00dft", "du", "t\u00f6d\u00b7ten", ",", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach so thu's, wo dir die Noth", "tokens": ["Ach", "so", "thu's", ",", "wo", "dir", "die", "Noth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus zerfre\u00dfnem Auge winkt", "tokens": ["Aus", "zer\u00b7fre\u00df\u00b7nem", "Au\u00b7ge", "winkt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und in Staub des Kerkers sinkt,", "tokens": ["Und", "in", "Staub", "des", "Ker\u00b7kers", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wo mit jedem Morgen \u2013 Tod!", "tokens": ["Wo", "mit", "je\u00b7dem", "Mor\u00b7gen", "\u2013", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "PIAT", "NN", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo mit jedem Abend \u2013 Tod!", "tokens": ["Wo", "mit", "je\u00b7dem", "A\u00b7bend", "\u2013", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "PIAT", "NN", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tod! um Mitternacht erschallt,", "tokens": ["Tod", "!", "um", "Mit\u00b7ter\u00b7nacht", "er\u00b7schallt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Schauerzelle hallt.", "tokens": ["Da\u00df", "die", "Schau\u00b7er\u00b7zel\u00b7le", "hallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Tod, wann kommst du, meine Lust?", "tokens": ["Tod", ",", "wann", "kommst", "du", ",", "mei\u00b7ne", "Lust", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ziehst den Dolch aus meiner Brust?", "tokens": ["Ziehst", "den", "Dolch", "aus", "mei\u00b7ner", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Streifst die Fesseln von der Hand?", "tokens": ["Streifst", "die", "Fes\u00b7seln", "von", "der", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, wann deckst du mich mit Sand?", "tokens": ["Ach", ",", "wann", "deckst", "du", "mich", "mit", "Sand", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Diese Todesstimme ruft", "tokens": ["Die\u00b7se", "To\u00b7dess\u00b7tim\u00b7me", "ruft"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus so mancher Kerkergruft,", "tokens": ["Aus", "so", "man\u00b7cher", "Ker\u00b7ker\u00b7gruft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der Gram verzweiflungsvoll", "tokens": ["Wo", "der", "Gram", "ver\u00b7zwei\u00b7flungs\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Hoffnung schmachten soll.", "tokens": ["Oh\u00b7ne", "Hoff\u00b7nung", "schmach\u00b7ten", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Drum, o Tod! wenn dir's gef\u00e4llt,", "tokens": ["Drum", ",", "o", "Tod", "!", "wenn", "dir's", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "FM", "NN", "$.", "KOUS", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hol' Gefangne aus der Welt:", "tokens": ["Hol'", "Ge\u00b7fang\u00b7ne", "aus", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Komm, vollende ihre Noth;", "tokens": ["Komm", ",", "voll\u00b7en\u00b7de", "ih\u00b7re", "Noth", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sei barmherzig, lieber Tod.", "tokens": ["Sei", "barm\u00b7her\u00b7zig", ",", "lie\u00b7ber", "Tod", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Tod, du Schrecken der Natur,", "tokens": ["Tod", ",", "du", "Schre\u00b7cken", "der", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "I