 "pos": ["VAIMP", "ADJD", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Nimm den holden Knaben nicht,", "tokens": ["Nimm", "den", "hol\u00b7den", "Kna\u00b7ben", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der voll Unschuld im Gesicht", "tokens": ["Der", "voll", "Un\u00b7schuld", "im", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit der Brust der Mutter spielt,", "tokens": ["Mit", "der", "Brust", "der", "Mut\u00b7ter", "spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein erstes Leben f\u00fchlt.", "tokens": ["Und", "sein", "ers\u00b7tes", "Le\u00b7ben", "f\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Und den J\u00fcngling schone mir,", "tokens": ["Und", "den", "J\u00fcng\u00b7ling", "scho\u00b7ne", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der am f\u00fchlenden Klavier", "tokens": ["Der", "am", "f\u00fch\u00b7len\u00b7den", "Kla\u00b7vier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Goldne Saiten wiegt und schwingt", "tokens": ["Gold\u00b7ne", "Sai\u00b7ten", "wiegt", "und", "schwingt"], "tok