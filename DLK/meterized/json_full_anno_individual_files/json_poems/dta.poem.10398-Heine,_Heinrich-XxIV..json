{"dta.poem.10398": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Sie haben dir viel erz\u00e4hlet,", "tokens": ["Sie", "ha\u00b7ben", "dir", "viel", "er\u00b7z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und haben viel geklagt;", "tokens": ["Und", "ha\u00b7ben", "viel", "ge\u00b7klagt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch was meine Seele gequ\u00e4let,", "tokens": ["Doch", "was", "mei\u00b7ne", "See\u00b7le", "ge\u00b7qu\u00e4\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das haben sie nicht gesagt.", "tokens": ["Das", "ha\u00b7ben", "sie", "nicht", "ge\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie machten ein gro\u00dfes Wesen,", "tokens": ["Sie", "mach\u00b7ten", "ein", "gro\u00b7\u00dfes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und sch\u00fcttelten kl\u00e4glich das Haupt;", "tokens": ["Und", "sch\u00fct\u00b7tel\u00b7ten", "kl\u00e4g\u00b7lich", "das", "Haupt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie nannten mich den B\u00f6sen,", "tokens": ["Sie", "nann\u00b7ten", "mich", "den", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und du hast alles geglaubt.", "tokens": ["Und", "du", "hast", "al\u00b7les", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Jedoch das Allerschlimmste,", "tokens": ["Je\u00b7doch", "das", "Al\u00b7ler\u00b7schlimms\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das haben sie nicht gewu\u00dft;", "tokens": ["Das", "ha\u00b7ben", "sie", "nicht", "ge\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Schlimmste und das D\u00fcmmste,", "tokens": ["Das", "Schlimms\u00b7te", "und", "das", "D\u00fcmms\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das trug ich geheim in der Brust.", "tokens": ["Das", "trug", "ich", "ge\u00b7heim", "in", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}