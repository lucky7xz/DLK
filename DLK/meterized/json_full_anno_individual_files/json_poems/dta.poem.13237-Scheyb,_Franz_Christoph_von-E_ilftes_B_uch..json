{"dta.poem.13237": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eversteht ihr, Tugenden! wo euer Beystand n\u00fczt?", "tokens": ["\u201e", "ver\u00b7steht", "ihr", ",", "Tu\u00b7gen\u00b7den", "!", "wo", "eu\u00b7er", "Beys\u00b7tand", "n\u00fczt", "?"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$.", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "110\u201dMacht, da\u00df die Einigkeit das Vaterland besch\u00fczt.", "tokens": ["\"", "Macht", ",", "da\u00df", "die", "Ei\u00b7nig\u00b7keit", "das", "Va\u00b7ter\u00b7land", "be\u00b7sch\u00fczt", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "$,", "KOUS", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "\u201egeht! ruhet nicht, bi\u00df ihr den stillen Trieb entdecket,", "tokens": ["\u201e", "geht", "!", "ru\u00b7het", "nicht", ",", "bi\u00df", "ihr", "den", "stil\u00b7len", "Trieb", "ent\u00b7de\u00b7cket", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eder die Verwirrungen des Vaterlands erwecket.", "tokens": ["\u201e", "der", "die", "Ver\u00b7wir\u00b7run\u00b7gen", "des", "Va\u00b7ter\u00b7lands", "er\u00b7we\u00b7cket", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "\u201eerkl\u00e4rt: es seyen nur zwey Sachen in der Wahl,", "tokens": ["\u201e", "er\u00b7kl\u00e4rt", ":", "es", "se\u00b7yen", "nur", "zwey", "Sa\u00b7chen", "in", "der", "Wahl", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$.", "PPER", "VAFIN", "ADV", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ehier Sieg und Heil und Ruhm; dort Abgrund, Sturz und Fall.", "tokens": ["\u201e", "hier", "Sieg", "und", "Heil", "und", "Ruhm", ";", "dort", "Ab\u00b7grund", ",", "Sturz", "und", "Fall", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "KON", "NN", "KON", "NN", "$.", "ADV", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "115\u201dZeigt, wie man fechten soll, die Kette zu zerreissen;", "tokens": ["\"", "Zeigt", ",", "wie", "man", "fech\u00b7ten", "soll", ",", "die", "Ket\u00b7te", "zu", "zer\u00b7reis\u00b7sen", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201efragt, ob es l\u00f6blich sey, sich selber Knecht zu heissen?", "tokens": ["\u201e", "fragt", ",", "ob", "es", "l\u00f6b\u00b7lich", "sey", ",", "sich", "sel\u00b7ber", "Knecht", "zu", "heis\u00b7sen", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PRF", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201efragt, ob sie lieber sich gefesselt wollen sehn,", "tokens": ["\u201e", "fragt", ",", "ob", "sie", "lie\u00b7ber", "sich", "ge\u00b7fes\u00b7selt", "wol\u00b7len", "sehn", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PRF", "VVPP", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eals hier im Sieg, wann sie den Degen w\u00fcrden drehn?", "tokens": ["\u201e", "als", "hier", "im", "Sieg", ",", "wann", "sie", "den", "De\u00b7gen", "w\u00fcr\u00b7den", "drehn", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "APPRART", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201egeht! trachtet, da\u00df sie sich nach euerm Rath bequemen,", "tokens": ["\u201e", "geht", "!", "trach\u00b7tet", ",", "da\u00df", "sie", "sich", "nach", "eu\u00b7erm", "Rath", "be\u00b7que\u00b7men", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "120\u201dUnd wieder ihren Feind gerechte Waffen nehmen.", "tokens": ["\"", "Und", "wie\u00b7der", "ih\u00b7ren", "Feind", "ge\u00b7rech\u00b7te", "Waf\u00b7fen", "neh\u00b7men", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}