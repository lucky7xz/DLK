{"textgrid.poem.67062": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ende einer Gedichtfolge", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erscholl von jeder Heide, jedem H\u00fcgel", "tokens": ["Er\u00b7scholl", "von", "je\u00b7der", "Hei\u00b7de", ",", "je\u00b7dem", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "APPR", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Schreckengel\u00e4rm gewappneter W\u00fctriche her.", "tokens": ["Das", "Schre\u00b7cken\u00b7ge\u00b7l\u00e4rm", "ge\u00b7wapp\u00b7ne\u00b7ter", "W\u00fct\u00b7ri\u00b7che", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Doch wenig Stunden sann um Mitternacht der Held,", "tokens": ["Doch", "we\u00b7nig", "Stun\u00b7den", "sann", "um", "Mit\u00b7ter\u00b7nacht", "der", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vollbrachte mit st\u00fcrmender Hand, was er sann, am gefl\u00fcgelten Tag,", "tokens": ["Voll\u00b7brach\u00b7te", "mit", "st\u00fcr\u00b7men\u00b7der", "Hand", ",", "was", "er", "sann", ",", "am", "ge\u00b7fl\u00fc\u00b7gel\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.5": {"text": "Und ha! wo war er nun, der Fremdlinge Grimm?", "tokens": ["Und", "ha", "!", "wo", "war", "er", "nun", ",", "der", "Fremd\u00b7lin\u00b7ge", "Grimm", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "PWAV", "VAFIN", "PPER", "ADV", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die Racheblicke, wie so bange rollten sie?", "tokens": ["Die", "Ra\u00b7che\u00b7bli\u00b7cke", ",", "wie", "so", "ban\u00b7ge", "roll\u00b7ten", "sie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Rosse Schnauben hatt in R\u00f6cheln sich gewandelt,", "tokens": ["Der", "Ros\u00b7se", "Schnau\u00b7ben", "hatt", "in", "R\u00f6\u00b7cheln", "sich", "ge\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zerrissen moderten im Blut des Fl\u00fcchtlings", "tokens": ["Zer\u00b7ris\u00b7sen", "mo\u00b7der\u00b7ten", "im", "Blut", "des", "Fl\u00fccht\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Die g\u00fcldenen Paniere, Raben kr\u00e4chzten", "tokens": ["Die", "g\u00fcl\u00b7de\u00b7nen", "Pa\u00b7nie\u00b7re", ",", "Ra\u00b7ben", "kr\u00e4chz\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Im leichenvollen Hinterhalt, und Angstgeheul", "tokens": ["Im", "lei\u00b7chen\u00b7vol\u00b7len", "Hin\u00b7ter\u00b7halt", ",", "und", "Angst\u00b7ge\u00b7heul"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Erscholl von jeder Heide, jedem H\u00fcgel.", "tokens": ["Er\u00b7scholl", "von", "je\u00b7der", "Hei\u00b7de", ",", "je\u00b7dem", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Verschlungen hatte sie der gr\u00f6\u00dfre Strom.", "tokens": ["Ver\u00b7schlun\u00b7gen", "hat\u00b7te", "sie", "der", "gr\u00f6\u00df\u00b7re", "Strom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Tag des Weltgerichts \u2013 auch er! auch er!", "tokens": ["Der", "Tag", "des", "Welt\u00b7ge\u00b7richts", "\u2013", "auch", "er", "!", "auch", "er", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ADV", "PPER", "$.", "ADV", "PPER", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wird zeugen einst im Angesicht der V\u00f6lker.", "tokens": ["Wird", "zeu\u00b7gen", "einst", "im", "An\u00b7ge\u00b7sicht", "der", "V\u00f6l\u00b7ker", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So spricht Jehova: Herrlich sei dein Lohn!", "tokens": ["So", "spricht", "Je\u00b7ho\u00b7va", ":", "Herr\u00b7lich", "sei", "dein", "Lohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie sch\u00e4ndeten zum blutbefleckten Greul", "tokens": ["Sie", "sch\u00e4n\u00b7de\u00b7ten", "zum", "blut\u00b7be\u00b7fleck\u00b7ten", "Greul"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Fahne meines Reichs \u2013 die Lehre meines Mundes", "tokens": ["Die", "Fah\u00b7ne", "mei\u00b7nes", "Reichs", "\u2013", "die", "Leh\u00b7re", "mei\u00b7nes", "Mun\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zur Menschenw\u00fcrgerin, zur Bruderm\u00f6rderin.", "tokens": ["Zur", "Men\u00b7schen\u00b7w\u00fcr\u00b7ge\u00b7rin", ",", "zur", "Bru\u00b7der\u00b7m\u00f6r\u00b7de\u00b7rin", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Henkersf\u00e4usten trieben sie vom Vaterland", "tokens": ["Mit", "Hen\u00b7kers\u00b7f\u00e4us\u00b7ten", "trie\u00b7ben", "sie", "vom", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Kinder meines Luthers, die das Joch des Wahns", "tokens": ["Die", "Kin\u00b7der", "mei\u00b7nes", "Lu\u00b7thers", ",", "die", "das", "Joch", "des", "Wahns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vom Nacken sch\u00fcttelten, in Todesw\u00fcsten hin.", "tokens": ["Vom", "Na\u00b7cken", "sch\u00fct\u00b7tel\u00b7ten", ",", "in", "To\u00b7de\u00b7sw\u00fcs\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da trocknet' ihre Tr\u00e4nen Gustav ab,", "tokens": ["Da", "trock\u00b7net'", "ih\u00b7re", "Tr\u00e4\u00b7nen", "Gus\u00b7tav", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Fromme baute H\u00e4user meinen Irrenden.", "tokens": ["Der", "From\u00b7me", "bau\u00b7te", "H\u00e4u\u00b7ser", "mei\u00b7nen", "Ir\u00b7ren\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.11": {"text": "Dein Lohn sei herrlich! du Gesegneter!", "tokens": ["Dein", "Lohn", "sei", "herr\u00b7lich", "!", "du", "Ge\u00b7se\u00b7gne\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "PPER", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "So spricht Jehova, und die Myriaden", "tokens": ["So", "spricht", "Je\u00b7ho\u00b7va", ",", "und", "die", "My\u00b7ria\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Versammleter erheben ihre H\u00e4upter", "tokens": ["Ver\u00b7samm\u00b7le\u00b7ter", "er\u00b7he\u00b7ben", "ih\u00b7re", "H\u00e4up\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und breiten ihre Arme gegen Gustav aus,", "tokens": ["Und", "brei\u00b7ten", "ih\u00b7re", "Ar\u00b7me", "ge\u00b7gen", "Gus\u00b7tav", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "PPOSAT", "NN", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und jubeln: Amen! herrlich ist sein Lohn.", "tokens": ["Und", "ju\u00b7beln", ":", "A\u00b7men", "!", "herr\u00b7lich", "ist", "sein", "Lohn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "NN", "$.", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Gustav! Gustav! hast du dein Ohr geneigt", "tokens": ["O", "Gus\u00b7tav", "!", "Gus\u00b7tav", "!", "hast", "du", "dein", "Ohr", "ge\u00b7neigt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "NE", "$.", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Zeugen deiner Gr\u00f6\u00dfe \u2013 du Herrlicher!", "tokens": ["Den", "Zeu\u00b7gen", "dei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "\u2013", "du", "Herr\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "PPER", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und z\u00fcrnst du nicht, und l\u00e4chelst du im", "tokens": ["Und", "z\u00fcrnst", "du", "nicht", ",", "und", "l\u00e4\u00b7chelst", "du", "im"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "VVFIN", "PPER", "APPRART"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.4": {"text": "Arme der Helden zu uns herunter?", "tokens": ["Ar\u00b7me", "der", "Hel\u00b7den", "zu", "uns", "her\u00b7un\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Verzeih, du Liebling Gottes! ich liebe dich! \u2013", "tokens": ["Ver\u00b7zeih", ",", "du", "Lieb\u00b7ling", "Got\u00b7tes", "!", "ich", "lie\u00b7be", "dich", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPER", "NN", "NN", "$.", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wann Donner rollen \u00fcber mein trautes Tal,", "tokens": ["Wann", "Don\u00b7ner", "rol\u00b7len", "\u00fc\u00b7ber", "mein", "trau\u00b7tes", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So denk ich dein, und wenn der Obstbaum", "tokens": ["So", "denk", "ich", "dein", ",", "und", "wenn", "der", "Obst\u00b7baum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "$,", "KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Freundlich den Apfel herunterreichet,", "tokens": ["Freund\u00b7lich", "den", "Ap\u00b7fel", "her\u00b7un\u00b7ter\u00b7rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "So nenn ich deinen Namen. Denn ringsum sieht", "tokens": ["So", "nenn", "ich", "dei\u00b7nen", "Na\u00b7men", ".", "Denn", "ring\u00b7sum", "sieht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "KON", "ADV", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Denkmal deiner Taten mein staunend Aug.", "tokens": ["Ein", "Denk\u00b7mal", "dei\u00b7ner", "Ta\u00b7ten", "mein", "stau\u00b7nend", "Aug."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "PPOSAT", "NN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und ha! wie wird dies Auge staunen,", "tokens": ["Und", "ha", "!", "wie", "wird", "dies", "Au\u00b7ge", "stau\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$.", "PWAV", "VAFIN", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchret mich f\u00f6rder hinauf zum Tempel,", "tokens": ["F\u00fch\u00b7ret", "mich", "f\u00f6r\u00b7der", "hin\u00b7auf", "zum", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Zum h\u00f6chsten Tempel seiner Erhabenheit", "tokens": ["Zum", "h\u00f6chs\u00b7ten", "Tem\u00b7pel", "sei\u00b7ner", "Er\u00b7ha\u00b7ben\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit wolkenlosem Mut die Begeisterung \u2013", "tokens": ["Mit", "wol\u00b7ken\u00b7lo\u00b7sem", "Mut", "die", "Be\u00b7geis\u00b7te\u00b7rung", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hinauf, wo es dem T\u00e4ndler schwindelt,", "tokens": ["Hin\u00b7auf", ",", "wo", "es", "dem", "T\u00e4nd\u00b7ler", "schwin\u00b7delt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo der Gebrechliche nie hinanklimmt!", "tokens": ["Wo", "der", "Ge\u00b7brech\u00b7li\u00b7che", "nie", "hi\u00b7nan\u00b7klimmt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+--+--+-++", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Umdonnert, Meereswogen! die einsame", "tokens": ["Um\u00b7don\u00b7nert", ",", "Mee\u00b7res\u00b7wo\u00b7gen", "!", "die", "ein\u00b7sa\u00b7me"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVPP", "$,", "NN", "$.", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gewagte Bahn! euch bebet die Saite nicht!", "tokens": ["Ge\u00b7wag\u00b7te", "Bahn", "!", "euch", "be\u00b7bet", "die", "Sai\u00b7te", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"t