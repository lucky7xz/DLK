{"dta.poem.18885": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Despotie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Dem Vern\u00fcnft\u2019gen sind Lockspeise Schedschaai\u2019s", "tokens": ["Dem", "Ver\u00b7n\u00fcnft'\u00b7gen", "sind", "Lock\u00b7spei\u00b7se", "Sche\u00b7dschaai's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gedichte,", "tokens": ["Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Hundert V\u00f6gel wie ich fliegen begierig darauf.", "tokens": ["Hun\u00b7dert", "V\u00f6\u00b7gel", "wie", "ich", "flie\u00b7gen", "be\u00b7gie\u00b7rig", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "KOKOM", "PPER", "VVINF", "ADJD", "PAV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Geh mein Gedicht und k\u00fcss\u2019 vor dem Herrn die", "tokens": ["Geh", "mein", "Ge\u00b7dicht", "und", "k\u00fcss'", "vor", "dem", "Herrn", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "KON", "ADJD", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Erde und sag\u2019 ihm:", "tokens": ["Er\u00b7de", "und", "sag'", "ihm", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Du, die Tugend der Zeit, Tugendepoche bist du.", "tokens": ["Du", ",", "die", "Tu\u00b7gend", "der", "Zeit", ",", "Tu\u00b7gen\u00b7de\u00b7po\u00b7che", "bist", "du", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ART", "NN", "$,", "NN", "VAFIN", "PPER", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}