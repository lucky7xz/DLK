{"textgrid.poem.42233": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sinnlichkeit gibt mir Abschiedsfest;", "tokens": ["Die", "Sinn\u00b7lich\u00b7keit", "gibt", "mir", "Ab\u00b7schieds\u00b7fest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das sind kuriose Gestalten,", "tokens": ["Das", "sind", "ku\u00b7ri\u00b7o\u00b7se", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Binden gewickelt, in Schienen gepre\u00dft,", "tokens": ["In", "Bin\u00b7den", "ge\u00b7wi\u00b7ckelt", ",", "in", "Schie\u00b7nen", "ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und kaum mehr festzuhalten.", "tokens": ["Und", "kaum", "mehr", "fest\u00b7zu\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die strahlende Nacktheit such ich so bang,", "tokens": ["Die", "strah\u00b7len\u00b7de", "Nackt\u00b7heit", "such", "ich", "so", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es fehlt ihr wohl an Vertrauen.", "tokens": ["Es", "fehlt", "ihr", "wohl", "an", "Ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich hab sie bei gellendem Becherklang", "tokens": ["Ich", "hab", "sie", "bei", "gel\u00b7len\u00b7dem", "Be\u00b7cher\u00b7klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zu h\u00e4ufig zusammen gehauen.", "tokens": ["Zu", "h\u00e4u\u00b7fig", "zu\u00b7sam\u00b7men", "ge\u00b7hau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Und ist erst das Seelenleben entweibt,", "tokens": ["Und", "ist", "erst", "das", "See\u00b7len\u00b7le\u00b7ben", "ent\u00b7weibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann sind s\u00e4mtliche Lampen erloschen.", "tokens": ["Dann", "sind", "s\u00e4mt\u00b7li\u00b7che", "Lam\u00b7pen", "er\u00b7lo\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "F\u00fcr das, was f\u00fcr mich dann noch \u00fcbrigbleibt,", "tokens": ["F\u00fcr", "das", ",", "was", "f\u00fcr", "mich", "dann", "noch", "\u00fcb\u00b7rig\u00b7bleibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "PPER", "ADV", "ADV", "VVFIN", "$,"], "mete