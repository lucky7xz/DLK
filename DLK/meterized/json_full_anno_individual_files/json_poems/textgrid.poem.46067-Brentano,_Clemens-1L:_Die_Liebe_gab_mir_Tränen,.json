{"textgrid.poem.46067": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Liebe gab mir Tr\u00e4nen,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe gab mir Tr\u00e4nen,", "tokens": ["Die", "Lie\u00b7be", "gab", "mir", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die linde hat mir T\u00fcchlein dann ges\u00e4umt,", "tokens": ["Die", "lin\u00b7de", "hat", "mir", "T\u00fcch\u00b7lein", "dann", "ge\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu trocknen viele Tr\u00e4nen,", "tokens": ["Zu", "trock\u00b7nen", "vie\u00b7le", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ich um sie zu weinen noch vers\u00e4umt,", "tokens": ["Die", "ich", "um", "sie", "zu", "wei\u00b7nen", "noch", "ver\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Willkomm' zuk\u00fcnft'ge Tr\u00e4nen,", "tokens": ["Will\u00b7komm'", "zu\u00b7k\u00fcnft'\u00b7ge", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr habt euch solche Huld wohl nicht getr\u00e4umt,", "tokens": ["Ihr", "habt", "euch", "sol\u00b7che", "Huld", "wohl", "nicht", "ge\u00b7tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O Mutter meiner Tr\u00e4nen,", "tokens": ["O", "Mut\u00b7ter", "mei\u00b7ner", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die jetzt noch unter deinem Herzen ruhn,", "tokens": ["Die", "jetzt", "noch", "un\u00b7ter", "dei\u00b7nem", "Her\u00b7zen", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Fromm tust du meinen Tr\u00e4nen,", "tokens": ["Fromm", "tust", "du", "mei\u00b7nen", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wie treue M\u00fctter ihren Kindlein tun,", "tokens": ["Wie", "treu\u00b7e", "M\u00fct\u00b7ter", "ih\u00b7ren", "Kin\u00b7dlein", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Bereitest meinen Tr\u00e4nen", "tokens": ["Be\u00b7rei\u00b7test", "mei\u00b7nen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ein mildes Lager mit der linden Hand", "tokens": ["Ein", "mil\u00b7des", "La\u00b7ger", "mit", "der", "lin\u00b7den", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "O reicher Strom der Tr\u00e4nen!", "tokens": ["O", "rei\u00b7cher", "Strom", "der", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Der so ein liebes lindes Bett hier fand,", "tokens": ["Der", "so", "ein", "lie\u00b7bes", "lin\u00b7des", "Bett", "hier", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "O nehmt mich auf ihr Tr\u00e4nen!", "tokens": ["O", "nehmt", "mich", "auf", "ihr", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Tragt mich hin\u00fcber in das andre Land,", "tokens": ["Tragt", "mich", "hin\u00b7\u00fc\u00b7ber", "in", "das", "and\u00b7re", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Und spiegelt mir ihr Tr\u00e4nen", "tokens": ["Und", "spie\u00b7gelt", "mir", "ihr", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Die Linde, die da an des Saumes Rand", "tokens": ["Die", "Lin\u00b7de", ",", "die", "da", "an", "des", "Sau\u00b7mes", "Rand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Vielleicht mit eignen Tr\u00e4nen", "tokens": ["Viel\u00b7leicht", "mit", "eig\u00b7nen", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Die hebe Gabe Stich vor Stich erfand,", "tokens": ["Die", "he\u00b7be", "Ga\u00b7be", "Stich", "vor", "Stich", "er\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Wo sollt ihr hin, ihr Tr\u00e4nen?", "tokens": ["Wo", "sollt", "ihr", "hin", ",", "ihr", "Tr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Wenn eure Mutter sich von euch gewandt,", "tokens": ["Wenn", "eu\u00b7re", "Mut\u00b7ter", "sich", "von", "euch", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Verrinnen m\u00fc\u00dft ihr Tr\u00e4nen", "tokens": ["Ver\u00b7rin\u00b7nen", "m\u00fc\u00dft", "ihr", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "In einer \u00f6den W\u00fcste gl\u00fchem Sand.", "tokens": ["In", "ei\u00b7ner", "\u00f6\u00b7den", "W\u00fcs\u00b7te", "gl\u00fc\u00b7hem", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Erweichet doch ihr Tr\u00e4nen", "tokens": ["Er\u00b7wei\u00b7chet", "doch", "ihr", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Das Herz, das nie ein ander Herz noch fand,", "tokens": ["Das", "Herz", ",", "das", "nie", "ein", "an\u00b7der", "Herz", "noch", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJD", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Und euch gebar ihr Tr\u00e4nen,", "tokens": ["Und", "euch", "ge\u00b7bar", "ihr", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Und euch die T\u00fcchlein in die Wiege band,", "tokens": ["Und", "euch", "die", "T\u00fcch\u00b7lein", "in", "die", "Wie\u00b7ge", "band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Da\u00df es vor euch, ihr Tr\u00e4nen", "tokens": ["Da\u00df", "es", "vor", "euch", ",", "ihr", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Nicht grausam fliehe, fern und abgewandt;", "tokens": ["Nicht", "grau\u00b7sam", "flie\u00b7he", ",", "fern", "und", "ab\u00b7ge\u00b7wandt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "$,", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Es gab wohl kaum noch Tr\u00e4nen,", "tokens": ["Es", "gab", "wohl", "kaum", "noch", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Die ihre arme Mutter so geliebt,", "tokens": ["Die", "ih\u00b7re", "ar\u00b7me", "Mut\u00b7ter", "so", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und doch, o arme Tr\u00e4nen,", "tokens": ["Und", "doch", ",", "o", "ar\u00b7me", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Die liebe, s\u00fc\u00dfe Mutter so betr\u00fcbt!!?", "tokens": ["Die", "lie\u00b7be", ",", "s\u00fc\u00b7\u00dfe", "Mut\u00b7ter", "so", "be\u00b7tr\u00fcbt", "!!?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$,", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "O arme, sel'ge Tr\u00e4nen!", "tokens": ["O", "ar\u00b7me", ",", "sel'\u00b7ge", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "O liebe, linde, die so gern vergiebt.", "tokens": ["O", "lie\u00b7be", ",", "lin\u00b7de", ",", "die", "so", "gern", "ver\u00b7giebt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ADJA", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Vergieb, vergieb den Tr\u00e4nen!", "tokens": ["Ver\u00b7gieb", ",", "ver\u00b7gieb", "den", "Tr\u00e4\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Sieh stille zu, es sind die letzten bald,", "tokens": ["Sieh", "stil\u00b7le", "zu", ",", "es", "sind", "die", "letz\u00b7ten", "bald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Wenn ich in Reuetr\u00e4nen", "tokens": ["Wenn", "ich", "in", "Reu\u00b7e\u00b7tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Ein T\u00fcchlein sterbend in den H\u00e4nden halt',", "tokens": ["Ein", "T\u00fcch\u00b7lein", "ster\u00b7bend", "in", "den", "H\u00e4n\u00b7den", "halt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Will ich mit treuen Tr\u00e4nen", "tokens": ["Will", "ich", "mit", "treu\u00b7en", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Ans Herz es dr\u00fccken, das schon \u00fcberwallt,", "tokens": ["Ans", "Herz", "es", "dr\u00fc\u00b7cken", ",", "das", "schon", "\u00fc\u00b7berw\u00b7allt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Das \u00fcberwallt in Tr\u00e4nen", "tokens": ["Das", "\u00fc\u00b7berw\u00b7allt", "in", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Und meine Seele tr\u00e4gt vor Richters Thron,", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "tr\u00e4gt", "vor", "Rich\u00b7ters", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Da will ich euch ihr Tr\u00e4nen", "tokens": ["Da", "will", "ich", "euch", "ihr", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Im T\u00fcchlein reichen meines Gottes Sohn,", "tokens": ["Im", "T\u00fcch\u00b7lein", "rei\u00b7chen", "mei\u00b7nes", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Da\u00df er sein Blut, ihr Tr\u00e4nen,", "tokens": ["Da\u00df", "er", "sein", "Blut", ",", "ihr", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Euch mischend, mir die schwere Schuld vergiebt,", "tokens": ["Euch", "mi\u00b7schend", ",", "mir", "die", "schwe\u00b7re", "Schuld", "ver\u00b7giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "$,", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.49": {"text": "Und zu euch spricht ihr Tr\u00e4nen:", "tokens": ["Und", "zu", "euch", "spricht", "ihr", "Tr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Viel ist vergeben euch, die viel geliebt!", "tokens": ["Viel", "ist", "ver\u00b7ge\u00b7ben", "euch", ",", "die", "viel", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "PPER", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Liebe gab mir Tr\u00e4nen,", "tokens": ["Die", "Lie\u00b7be", "gab", "mir", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die linde hat mir T\u00fcchlein dann ges\u00e4umt,", "tokens": ["Die", "lin\u00b7de", "hat", "mir", "T\u00fcch\u00b7lein", "dann", "ge\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu trocknen viele Tr\u00e4nen,", "tokens": ["Zu", "trock\u00b7nen", "vie\u00b7le", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ich um sie zu weinen noch vers\u00e4umt,", "tokens": ["Die", "ich", "um", "sie", "zu", "wei\u00b7nen", "no