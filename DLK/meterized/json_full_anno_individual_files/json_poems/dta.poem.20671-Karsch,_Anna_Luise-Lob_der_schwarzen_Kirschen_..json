{"dta.poem.20671": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Lob  \n der schwarzen Kirschen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Weinstocks Saftgew\u00e4chse ward                 ", "tokens": ["Des", "Wein\u00b7stocks", "Saft\u00b7ge\u00b7w\u00e4ch\u00b7se", "ward"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von tausend Dichtern laut erhoben;", "tokens": ["Von", "tau\u00b7send", "Dich\u00b7tern", "laut", "er\u00b7ho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Warum will denn nach S\u00e4ngerart", "tokens": ["Wa\u00b7rum", "will", "denn", "nach", "S\u00e4n\u00b7ge\u00b7rart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Mensch die Kirsche loben?", "tokens": ["Kein", "Mensch", "die", "Kir\u00b7sche", "lo\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O die karfunkelfarbne Frucht", "tokens": ["O", "die", "kar\u00b7fun\u00b7kel\u00b7farb\u00b7ne", "Frucht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In reifer Sch\u00f6nheit ward vor diesen", "tokens": ["In", "rei\u00b7fer", "Sch\u00f6n\u00b7heit", "ward", "vor", "die\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "APPR", "PDAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Unfehlbar von der Frau versucht,", "tokens": ["Un\u00b7fehl\u00b7bar", "von", "der", "Frau", "ver\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Die Milton hat gepriesen.", "tokens": ["Die", "Mil\u00b7ton", "hat", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kein Apfel reizet so den Gaum", "tokens": ["Kein", "Ap\u00b7fel", "rei\u00b7zet", "so", "den", "Gaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00f6schet so des Durstes Flammen;", "tokens": ["Und", "l\u00f6\u00b7schet", "so", "des", "Durs\u00b7tes", "Flam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er mag gleich vom Chineser-Baum", "tokens": ["Er", "mag", "gleich", "vom", "Chi\u00b7ne\u00b7ser\u00b7Baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "In \u00e4chter Abkunft stammen.", "tokens": ["In", "\u00e4ch\u00b7ter", "Ab\u00b7kunft", "stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der ausgekochte Kirschensaft", "tokens": ["Der", "aus\u00b7ge\u00b7koch\u00b7te", "Kir\u00b7schen\u00b7saft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Giebt aller Sommersuppen beste,", "tokens": ["Giebt", "al\u00b7ler", "Som\u00b7mer\u00b7sup\u00b7pen", "bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verleiht der Leber neue Kraft", "tokens": ["Ver\u00b7leiht", "der", "Le\u00b7ber", "neu\u00b7e", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fchlt der Adern Aeste;", "tokens": ["Und", "k\u00fchlt", "der", "A\u00b7dern", "A\u00b7es\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wem das schreckliche Verboth", "tokens": ["Und", "wem", "das", "schreck\u00b7li\u00b7che", "Ver\u00b7both"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Arztes jeden Wein geraubet,", "tokens": ["Des", "Arz\u00b7tes", "je\u00b7den", "Wein", "ge\u00b7rau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der misch ihn mit der Kirsche roth", "tokens": ["Der", "misch", "ihn", "mit", "der", "Kir\u00b7sche", "roth"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann ist er ihm erlaubet;", "tokens": ["Dann", "ist", "er", "ihm", "er\u00b7lau\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und w\u00e4re seine Lunge wund,", "tokens": ["Und", "w\u00e4\u00b7re", "sei\u00b7ne", "Lun\u00b7ge", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seine ganze Brust durchgraben:", "tokens": ["Und", "sei\u00b7ne", "gan\u00b7ze", "Brust", "durch\u00b7gra\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So darf sich doch sein matter Mund", "tokens": ["So", "darf", "sich", "doch", "sein", "mat\u00b7ter", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit diesem Tranke laben.", "tokens": ["Mit", "die\u00b7sem", "Tran\u00b7ke", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn ich den goldnen Rheinstrandwein", "tokens": ["Wenn", "ich", "den", "gold\u00b7nen", "Rhein\u00b7strand\u00b7wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und silbernen Champagner meide,", "tokens": ["Und", "sil\u00b7ber\u00b7nen", "Cham\u00b7pag\u00b7ner", "mei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann Freunde mischt mir Kirschblut drein", "tokens": ["Dann", "Freun\u00b7de", "mischt", "mir", "Kirschblut", "drein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PPER", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zur Aug- und Zungenweide:", "tokens": ["Zur", "Aug", "und", "Zun\u00b7gen\u00b7wei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dann werd\u2019 ich eben so verf\u00fchrt,", "tokens": ["Dann", "werd'", "ich", "e\u00b7ben", "so", "ver\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Eva, die den Baum betrachtet,", "tokens": ["Als", "E\u00b7va", ",", "die", "den", "Baum", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sch\u00f6n gewachsen und geziert,", "tokens": ["So", "sch\u00f6n", "ge\u00b7wach\u00b7sen", "und", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nach der Frucht geschmachtet.", "tokens": ["Und", "nach", "der", "Frucht", "ge\u00b7schmach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ich trink und rufe dreymal hoch!", "tokens": ["Ich", "trink", "und", "ru\u00b7fe", "drey\u00b7mal", "hoch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Dichter singt im Ernst und Scherze", "tokens": ["Ihr", "Dich\u00b7ter", "singt", "im", "Ernst", "und", "Scher\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu oft die Rose, singet doch", "tokens": ["Zu", "oft", "die", "Ro\u00b7se", ",", "sin\u00b7get", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einmal der Kirschen Schw\u00e4rze!", "tokens": ["Ein\u00b7mal", "der", "Kir\u00b7schen", "Schw\u00e4r\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}