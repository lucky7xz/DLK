{"textgrid.poem.65154": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was dir zumeist am Herzen nagt?", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was dir zumeist am Herzen nagt?", "tokens": ["Was", "dir", "zu\u00b7meist", "am", "Her\u00b7zen", "nagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O pr\u00fcfe dich! du wirst gestehen,", "tokens": ["O", "pr\u00fc\u00b7fe", "dich", "!", "du", "wirst", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Leid nicht ist's, das dir geschehen,", "tokens": ["Das", "Leid", "nicht", "ist's", ",", "das", "dir", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VAFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht die Sorge, die dich plagt.", "tokens": ["Und", "nicht", "die", "Sor\u00b7ge", ",", "die", "dich", "plagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du k\u00f6nntest sie zur Not vergessen,", "tokens": ["Du", "k\u00f6nn\u00b7test", "sie", "zur", "Not", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch nimmermehr das Traumbild dessen", "tokens": ["Doch", "nim\u00b7mer\u00b7mehr", "das", "Traum\u00b7bild", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "PDS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Was dein Geschick dir streng versagt.", "tokens": ["Was", "dein", "Ge\u00b7schick", "dir", "streng", "ver\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur dieses, und nur dies allein,", "tokens": ["Nur", "die\u00b7ses", ",", "und", "nur", "dies", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "$,", "KON", "ADV", "PDS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht immerdar vor deinen Augen,", "tokens": ["Steht", "im\u00b7mer\u00b7dar", "vor", "dei\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es darf dir Kraft und Mut entsaugen,", "tokens": ["Es", "darf", "dir", "Kraft", "und", "Mut", "ent\u00b7sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zerr\u00fctten dir dein innerst Sein;", "tokens": ["Zer\u00b7r\u00fct\u00b7ten", "dir", "dein", "in\u00b7nerst", "Sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"