{"textgrid.poem.66672": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wir auch mu\u00dften finger\u00fcbend tasten,", "tokens": ["Und", "wir", "auch", "mu\u00df\u00b7ten", "fin\u00b7ge\u00b7r\u00fc\u00b7bend", "tas\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VMFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis eins dem andern v\u00f6llig auf der Spur,", "tokens": ["Bis", "eins", "dem", "an\u00b7dern", "v\u00f6l\u00b7lig", "auf", "der", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "ADJA", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis wir uns kontrapunktisch klar erfa\u00dften", "tokens": ["Bis", "wir", "uns", "kon\u00b7tra\u00b7punk\u00b7tisch", "klar", "er\u00b7fa\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf unsrer doppelten Klaviatur.", "tokens": ["Auf", "uns\u00b7rer", "dop\u00b7pel\u00b7ten", "Kla\u00b7vi\u00b7a\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gl\u00fcck ist kein Leierst\u00fcck f\u00fcr Klimperkasten \u2013", "tokens": ["Gl\u00fcck", "ist", "kein", "Lei\u00b7er\u00b7st\u00fcck", "f\u00fcr", "Klim\u00b7per\u00b7kas\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Durch Dissonanz strebt tiefere Natur", "tokens": ["Durch", "Dis\u00b7so\u00b7nanz", "strebt", "tie\u00b7fe\u00b7re", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im Wechselkampf von Suchen und Sichfliehen", "tokens": ["Im", "Wech\u00b7sel\u00b7kampf", "von", "Su\u00b7chen", "und", "Sich\u00b7flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zum Lustverschmelzen seliger Harmonien.", "tokens": ["Zum", "Lust\u00b7ver\u00b7schmel\u00b7zen", "se\u00b7li\u00b7ger", "Har\u00b7mo\u00b7ni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Du bist nicht ich, ich bin nicht du. Es zittert", "tokens": ["Du", "bist", "nicht", "ich", ",", "ich", "bin", "nicht", "du", ".", "Es", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "PPER", "$,", "PPER", "VAFIN", "PTKNEG", "PPER", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch jeden anders der Akkord der Welt.", "tokens": ["Durch", "je\u00b7den", "an\u00b7ders", "der", "Ak\u00b7kord", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Urfugen sind milliardenfach zersplittert,", "tokens": ["Ur\u00b7fu\u00b7gen", "sind", "mil\u00b7li\u00b7ar\u00b7den\u00b7fach", "zer\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "++--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Zu Wellchen ist des Kosmos Klang zerschellt.", "tokens": ["Zu", "Well\u00b7chen", "ist", "des", "Kos\u00b7mos", "Klang", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Liebe sp\u00fcrt im Klanggewirr und wittert,", "tokens": ["Die", "Lie\u00b7be", "sp\u00fcrt", "im", "Klang\u00b7ge\u00b7wirr", "und", "wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo sehnsuchtsvoll sich Ton dem Ton gesellt \u2013", "tokens": ["Wo", "sehn\u00b7suchts\u00b7voll", "sich", "Ton", "dem", "Ton", "ge\u00b7sellt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn sich zwei Menschenleben ganz gefunden,", "tokens": ["Wenn", "sich", "zwei", "Men\u00b7schen\u00b7le\u00b7ben", "ganz", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "CARD", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hat sich geschiedner Schall dem Schall verbunden.", "tokens": ["Hat", "sich", "ge\u00b7schied\u00b7ner", "Schall", "dem", "Schall", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wer kann der Liebe reines Wesen deuten,", "tokens": ["Wer", "kann", "der", "Lie\u00b7be", "rei\u00b7nes", "We\u00b7sen", "deu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Gottes heiligstes Geheimnis ist?", "tokens": ["Das", "Got\u00b7tes", "hei\u00b7ligs\u00b7tes", "Ge\u00b7heim\u00b7nis", "ist", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus dieses Urgrunds Meerestiefen l\u00e4uten", "tokens": ["Aus", "die\u00b7ses", "Ur\u00b7grunds", "Mee\u00b7res\u00b7tie\u00b7fen", "l\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verborgne Glocken, deren Klang ermi\u00dft", "tokens": ["Ver\u00b7borg\u00b7ne", "Glo\u00b7cken", ",", "de\u00b7ren", "Klang", "er\u00b7mi\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Kein Pfiffikus mit wissenschaftsgescheuten", "tokens": ["Kein", "Pfif\u00b7fi\u00b7kus", "mit", "wis\u00b7sen\u00b7schafts\u00b7ge\u00b7scheu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Witzohren und geriebener Forscherlist:", "tokens": ["Wit\u00b7zoh\u00b7ren", "und", "ge\u00b7rie\u00b7be\u00b7ner", "For\u00b7scher\u00b7list", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Mit schwingt ein dunkler Ton aus ewigen Sph\u00e4ren,", "tokens": ["Mit", "schwingt", "ein", "dunk\u00b7ler", "Ton", "aus", "e\u00b7wi\u00b7gen", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Den kann mir kein Akustiker erkl\u00e4ren.", "tokens": ["Den", "kann", "mir", "kein", "A\u00b7kus\u00b7ti\u00b7ker", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+--+----+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Er klingt aus leisem Unterton der Stimme,", "tokens": ["Er", "klingt", "aus", "lei\u00b7sem", "Un\u00b7ter\u00b7ton", "der", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sich die Wange dicht zur Wange schmiegt,", "tokens": ["Wenn", "sich", "die", "Wan\u00b7ge", "dicht", "zur", "Wan\u00b7ge", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Klingt aus dem Schluchzen, wenn von Trotz und Grimme", "tokens": ["Klingt", "aus", "dem", "Schluch\u00b7zen", ",", "wenn", "von", "Trotz", "und", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Die Doppelseele sich Verzeihn ersiegt.", "tokens": ["Die", "Dop\u00b7pel\u00b7see\u00b7le", "sich", "Ver\u00b7zeihn", "er\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Klingt aus dem Zuruf, da\u00df zum Licht ich klimme", "tokens": ["Klingt", "aus", "dem", "Zu\u00b7ruf", ",", "da\u00df", "zum", "Licht", "ich", "klim\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Aus Stunden, drin mich's d\u00fcster \u00fcberfliegt,", "tokens": ["Aus", "Stun\u00b7den", ",", "drin", "mich's", "d\u00fcs\u00b7ter", "\u00fc\u00b7berf\u00b7liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Klingt aus dem Abschiedsgru\u00df beim H\u00e4ndedrucke,", "tokens": ["Klingt", "aus", "dem", "Ab\u00b7schieds\u00b7gru\u00df", "beim", "H\u00e4n\u00b7de\u00b7dru\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.8": {"text": "Wenn ich, wie vor Verlust, zusammenzucke.", "tokens": ["Wenn", "ich", ",", "wie", "vor", "Ver\u00b7lust", ",", "zu\u00b7sam\u00b7men\u00b7zu\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iamb