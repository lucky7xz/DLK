{"dta.poem.11359": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "ArIA.  \n  Wider das ungeduldige murren.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man mu\u00df dem himmel weichen,", "tokens": ["Man", "mu\u00df", "dem", "him\u00b7mel", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und vor der sternen krafft", "tokens": ["Und", "vor", "der", "ster\u00b7nen", "krafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die schlaffen seegel streichen:", "tokens": ["Die", "schlaf\u00b7fen", "see\u00b7gel", "strei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des gl\u00fcckes eigenschafft", "tokens": ["Des", "gl\u00fc\u00b7ckes", "ei\u00b7gen\u00b7schafft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verfolget harte sinnen:", "tokens": ["Ver\u00b7fol\u00b7get", "har\u00b7te", "sin\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier hilfft kein streit, kein krieg,", "tokens": ["Hier", "hilfft", "kein", "streit", ",", "kein", "krieg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Man kan allhier den sieg", "tokens": ["Man", "kan", "all\u00b7hier", "den", "sieg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Nur mit geduld gewinnen.", "tokens": ["Nur", "mit", "ge\u00b7duld", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Denn flucht ihr aufs geschicke", "tokens": ["Denn", "flucht", "ihr", "aufs", "ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So h\u00e4ufft ihr straf und schuld,", "tokens": ["So", "h\u00e4ufft", "ihr", "straf", "und", "schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und euer ungel\u00fccke", "tokens": ["Und", "eu\u00b7er", "un\u00b7ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird gros durch ungeduld.", "tokens": ["Wird", "gros", "durch", "un\u00b7ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die starcke macht von oben", "tokens": ["Die", "star\u00b7cke", "macht", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verlacht der menschen wuth,", "tokens": ["Ver\u00b7lacht", "der", "men\u00b7schen", "wuth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die mit erbostem muth", "tokens": ["Die", "mit", "er\u00b7bos\u00b7tem", "muth"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Auf ihr verh\u00e4ngni\u00df toben.", "tokens": ["Auf", "ihr", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "to\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Klagt ihr die fl\u00fcchtigkeiten", "tokens": ["Klagt", "ihr", "die", "fl\u00fcch\u00b7tig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der guten stunden an:", "tokens": ["Der", "gu\u00b7ten", "stun\u00b7den", "an", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sprecht ihr, der lauff der zeiten,", "tokens": ["Sprecht", "ihr", ",", "der", "lauff", "der", "zei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hab euch gewalt gethan;", "tokens": ["Hab", "euch", "ge\u00b7walt", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo wolt ihr recht erlangen?", "tokens": ["Wo", "wolt", "ihr", "recht", "er\u00b7lan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wer will der richter seyn?", "tokens": ["Wer", "will", "der", "rich\u00b7ter", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wird eure seelen-pein", "tokens": ["Wird", "eu\u00b7re", "see\u00b7len\u00b7pein"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dadurch ein end empfangen?", "tokens": ["Da\u00b7durch", "ein", "end", "emp\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und was? Wir sind ja knechte", "tokens": ["Und", "was", "?", "Wir", "sind", "ja", "knech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "$.", "PPER", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des himmels, der uns schl\u00e4gt:", "tokens": ["Des", "him\u00b7mels", ",", "der", "uns", "schl\u00e4gt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir leiden gar mit rechte,", "tokens": ["Wir", "lei\u00b7den", "gar", "mit", "rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was er uns auferlegt:", "tokens": ["Was", "er", "uns", "auf\u00b7er\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir sind leibeigne selaven:", "tokens": ["Wir", "sind", "leib\u00b7eig\u00b7ne", "se\u00b7la\u00b7ven", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Das murren ziemt uns nicht,", "tokens": ["Das", "mur\u00b7ren", "ziemt", "uns", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn uns das zorn-gericht", "tokens": ["Wenn", "uns", "das", "zorn\u00b7ge\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Des herren will bestrafen.", "tokens": ["Des", "her\u00b7ren", "will", "be\u00b7stra\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Kein mittel ist zu finden,", "tokens": ["Kein", "mit\u00b7tel", "ist", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dadurch ein tapffrer mann", "tokens": ["Da\u00b7durch", "ein", "tapf\u00b7frer", "mann"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Den sauren ungl\u00fccks-winden", "tokens": ["Den", "sau\u00b7ren", "un\u00b7gl\u00fccks\u00b7win\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die stirne bieten kan,", "tokens": ["Die", "stir\u00b7ne", "bie\u00b7ten", "kan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Als wann er willig leidet", "tokens": ["Als", "wann", "er", "wil\u00b7lig", "lei\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PWAV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das, was er leiden mu\u00df,", "tokens": ["Das", ",", "was", "er", "lei\u00b7den", "mu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bi\u00df ihm des himmels schlu\u00df", "tokens": ["Bi\u00df", "ihm", "des", "him\u00b7mels", "schlu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein besser gut bescheidet.", "tokens": ["Ein", "bes\u00b7ser", "gut", "be\u00b7schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}