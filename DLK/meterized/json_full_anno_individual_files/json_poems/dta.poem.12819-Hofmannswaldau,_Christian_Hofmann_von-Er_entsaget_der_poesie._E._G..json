{"dta.poem.12819": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er entsaget der poesie.  \n E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, was ich noch poetisiren soll:", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "ich", "noch", "po\u00b7e\u00b7ti\u00b7si\u00b7ren", "soll", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es schmeckt das schlechte zeug noch immer manchem wohl,", "tokens": ["Es", "schmeckt", "das", "schlech\u00b7te", "zeug", "noch", "im\u00b7mer", "man\u00b7chem", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADV", "PIAT", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ieder tag will fast ein reimgen von mir haben;", "tokens": ["Und", "ie\u00b7der", "tag", "will", "fast", "ein", "reim\u00b7gen", "von", "mir", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "ADV", "ART", "ADJA", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich aber dencke stets mein dichten zu begraben.", "tokens": ["Ich", "a\u00b7ber", "den\u00b7cke", "stets", "mein", "dich\u00b7ten", "zu", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu zeiten k\u00f6mmt mich zwar ein kleiner k\u00fctzel an:", "tokens": ["Zu", "zei\u00b7ten", "k\u00f6mmt", "mich", "zwar", "ein", "klei\u00b7ner", "k\u00fct\u00b7zel", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Allein wie butter nicht beym feuer dauren kan;", "tokens": ["Al\u00b7lein", "wie", "but\u00b7ter", "nicht", "beym", "feu\u00b7er", "dau\u00b7ren", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "PTKNEG", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So mu\u00df der schnelle trieb sich auch gar bald verlieren.", "tokens": ["So", "mu\u00df", "der", "schnel\u00b7le", "trieb", "sich", "auch", "gar", "bald", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "VVFIN", "PRF", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So bald die feder nur will ein paar zeilen schmieren,", "tokens": ["So", "bald", "die", "fe\u00b7der", "nur", "will", "ein", "paar", "zei\u00b7len", "schmie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "VMFIN", "ART", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die zwar zu anfangs gold, am ende steingen seyn,", "tokens": ["Die", "zwar", "zu", "an\u00b7fangs", "gold", ",", "am", "en\u00b7de", "stein\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADV", "ADJD", "$,", "APPRART", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und pr\u00e4gt ich Jupitern und auch die Venus drein;", "tokens": ["Und", "pr\u00e4gt", "ich", "Ju\u00b7pi\u00b7tern", "und", "auch", "die", "Ve\u00b7nus", "drein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So mu\u00df ich doch alsbald des albern zeuges lachen,", "tokens": ["So", "mu\u00df", "ich", "doch", "als\u00b7bald", "des", "al\u00b7bern", "zeu\u00b7ges", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ART", "ADJD", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So bald nur der verstand bey solchen sch\u00f6nen sachen,", "tokens": ["So", "bald", "nur", "der", "ver\u00b7stand", "bey", "sol\u00b7chen", "sch\u00f6\u00b7nen", "sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "VVFIN", "APPR", "PIAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Selbst seine bl\u00f6se zeigt. Denn schmei\u00df ich alles hin,", "tokens": ["Selbst", "sei\u00b7ne", "bl\u00f6\u00b7se", "zeigt", ".", "Denn", "schmei\u00df", "ich", "al\u00b7les", "hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "VVFIN", "$.", "KON", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und schwere tausendmal, da\u00df ich kein dichter bin:", "tokens": ["Und", "schwe\u00b7re", "tau\u00b7send\u00b7mal", ",", "da\u00df", "ich", "kein", "dich\u00b7ter", "bin", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PIAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die feder wird zerstampt: die dinte wird vergossen,", "tokens": ["Die", "fe\u00b7der", "wird", "zer\u00b7stampt", ":", "die", "din\u00b7te", "wird", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Aus der doch auch vorhin zuweilen was geflossen,", "tokens": ["Aus", "der", "doch", "auch", "vor\u00b7hin", "zu\u00b7wei\u00b7len", "was", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADV", "ADV", "ADV", "PWS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das toll in ohren klang. Bald fang ich wieder an", "tokens": ["Das", "toll", "in", "oh\u00b7ren", "klang", ".", "Bald", "fang", "ich", "wie\u00b7der", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "APPR", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und suche mit verdru\u00df, ob ich noch dichten kan,", "tokens": ["Und", "su\u00b7che", "mit", "ver\u00b7dru\u00df", ",", "ob", "ich", "noch", "dich\u00b7ten", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "ADV", "ADJA", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Besonders wenn ich seh\u2019, wie andre geld verdienen,", "tokens": ["Be\u00b7son\u00b7ders", "wenn", "ich", "seh'", ",", "wie", "and\u00b7re", "geld", "ver\u00b7die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bey denen witz und kunst, gleich klee im winter, gr\u00fcnen,", "tokens": ["Bey", "de\u00b7nen", "witz", "und", "kunst", ",", "gleich", "klee", "im", "win\u00b7ter", ",", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "KON", "PTKVZ", "$,", "ADV", "VVFIN", "APPRART", "ADJA", "$,", "ADJA", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.21": {"text": "Da mischt sich denn der geitz in alle zeilen ein:", "tokens": ["Da", "mischt", "sich", "denn", "der", "geitz", "in", "al\u00b7le", "zei\u00b7len", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der will vors henckers danck: ich soll ein dichter seyn:", "tokens": ["Der", "will", "vors", "hen\u00b7ckers", "danck", ":", "ich", "soll", "ein", "dich\u00b7ter", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "PTKVZ", "$.", "PPER", "VMFIN", "ART", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich soll mit Laps und Taps um ziel und wette kriegen:", "tokens": ["Ich", "soll", "mit", "Laps", "und", "Taps", "um", "ziel", "und", "wet\u00b7te", "krie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "APPR", "NN", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich soll, dem Neuburg gleich, bey gro\u00dfen Herren l\u00fcgen:", "tokens": ["Ich", "soll", ",", "dem", "Neu\u00b7burg", "gleich", ",", "bey", "gro\u00b7\u00dfen", "Her\u00b7ren", "l\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ART", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich soll das, was noch nie in unser welt geschehn,", "tokens": ["Ich", "soll", "das", ",", "was", "noch", "nie", "in", "un\u00b7ser", "welt", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "$,", "PRELS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und was nicht werden wird, auch in dem dunckeln sehn:", "tokens": ["Und", "was", "nicht", "wer\u00b7den", "wird", ",", "auch", "in", "dem", "dun\u00b7ckeln", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "VAINF", "VAFIN", "$,", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ich soll der F\u00fcrsten witz, der diener redligkeiten:", "tokens": ["Ich", "soll", "der", "F\u00fcrs\u00b7ten", "witz", ",", "der", "die\u00b7ner", "red\u00b7lig\u00b7kei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ich soll die fr\u00f6mmigkeit, die doch bey unsern zeiten", "tokens": ["Ich", "soll", "die", "fr\u00f6m\u00b7mig\u00b7keit", ",", "die", "doch", "bey", "un\u00b7sern", "zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ein rares wildpret ist; ich soll die kluge kunst,", "tokens": ["Ein", "ra\u00b7res", "wild\u00b7pret", "ist", ";", "ich", "soll", "die", "klu\u00b7ge", "kunst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$.", "PPER", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die bey den meisten nichts, als aufgeblasner dunst,", "tokens": ["Die", "bey", "den", "meis\u00b7ten", "nichts", ",", "als", "auf\u00b7ge\u00b7blas\u00b7ner", "dunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "PIAT", "PIS", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "An allen priestern sehn: Ich soll die keuschheit preisen,", "tokens": ["An", "al\u00b7len", "pries\u00b7tern", "sehn", ":", "Ich", "soll", "die", "keuschheit", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$.", "PPER", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "Mit der die jungfern noch bis zu den sternen reisen,", "tokens": ["Mit", "der", "die", "jung\u00b7fern", "noch", "bis", "zu", "den", "ster\u00b7nen", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "ADV", "ADV", "APPR", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Wo der Lucretien ihr altes bildni\u00df steht:", "tokens": ["Wo", "der", "Lu\u00b7cre\u00b7ti\u00b7en", "ihr", "al\u00b7tes", "bild\u00b7ni\u00df", "steht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Ich soll der frauen treu, die doch so bald vergeht,", "tokens": ["Ich", "soll", "der", "frau\u00b7en", "treu", ",", "die", "doch", "so", "bald", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "$,", "PRELS", "ADV", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So bald das licht verl\u00f6scht, bis an den himmel heben:", "tokens": ["So", "bald", "das", "licht", "ver\u00b7l\u00f6scht", ",", "bis", "an", "den", "him\u00b7mel", "he\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,", "KOUS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ich soll des Hercules und Herculiskens leben", "tokens": ["Ich", "soll", "des", "Her\u00b7cu\u00b7les", "und", "Her\u00b7cu\u00b7lis\u00b7kens", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "An unserm Sterops schaun: Ja was? ich soll noch mehr:", "tokens": ["An", "un\u00b7serm", "Ste\u00b7rops", "schaun", ":", "Ja", "was", "?", "ich", "soll", "noch", "mehr", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "PTKANT", "PWS", "$.", "PPER", "VMFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich soll den ochsen-fu\u00df, und tr\u00e4t\u2019 er noch so sehr,", "tokens": ["Ich", "soll", "den", "och\u00b7sen\u00b7fu\u00df", ",", "und", "tr\u00e4t'", "er", "noch", "so", "sehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Vor eine engel-hand ums geld pa\u00dfiren lassen:", "tokens": ["Vor", "ei\u00b7ne", "en\u00b7gel\u00b7hand", "ums", "geld", "pa\u00b7\u00dfi\u00b7ren", "las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ich soll