{"dta.poem.604": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Nahmens-Tag einer Mutter/  \n vor einen andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was deine G\u00fct und Allmacht sch\u00fctzt/", "tokens": ["Was", "dei\u00b7ne", "G\u00fct", "und", "All\u00b7macht", "sch\u00fctzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das f\u00fchlt nicht Schwachheit Schmertz und Plage", "tokens": ["Das", "f\u00fchlt", "nicht", "Schwach\u00b7heit", "Schmertz", "und", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PTKNEG", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo deiner Gnaden Sonne blitzt", "tokens": ["Wo", "dei\u00b7ner", "Gna\u00b7den", "Son\u00b7ne", "blitzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da sp\u00fchrt man lauter frohe Tage/", "tokens": ["Da", "sp\u00fchrt", "man", "lau\u00b7ter", "fro\u00b7he", "Ta\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bek\u00fcmmerni\u00df/ Noht/ Sorg und Quaal", "tokens": ["Be\u00b7k\u00fcm\u00b7mer\u00b7ni\u00df", "/", "Noht", "/", "Sorg", "und", "Qua\u00b7al"], "token_info": ["word", "punct", "word", "punct", "word", "word", "wo