{"dta.poem.19437": {"metadata": {"author": {"name": "Stolberg-Stolberg, Friedrich Leopold zu", "birth": "N.A.", "death": "N.A."}, "title": "An die Grazien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1779", "urn": "urn:nbn:de:kobv:b4-20589-5", "language": ["de:0.99"], "booktitle": "Stolberg-Stolberg, Christian zu; Stolberg-Stolberg, Friedrich Leopold zu: Gedichte. Leipzig, 1779."}, "poem": {"stanza.1": {"line.1": {"text": "Leicht, wie Hauche des Abendwinds,", "tokens": ["Leicht", ",", "wie", "Hau\u00b7che", "des", "Ab\u00b7end\u00b7winds", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "NN", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Schwebe leicht, mein Gesang! sanft, wie des", "tokens": ["Schwe\u00b7be", "leicht", ",", "mein", "Ge\u00b7sang", "!", "sanft", ",", "wie", "des"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$.", "ADJD", "$,", "PWAV", "ART"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Liebenden", "tokens": ["Lie\u00b7ben\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Ku\u00df von Lippe zu Lippe schwebt!", "tokens": ["Ku\u00df", "von", "Lip\u00b7pe", "zu", "Lip\u00b7pe", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "APPR", "NE", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Wehe D\u00fcfte des Lobs, s\u00fcsser denn Weihrauchs", "tokens": ["We\u00b7he", "D\u00fcf\u00b7te", "des", "Lobs", ",", "s\u00fcs\u00b7ser", "denn", "Weih\u00b7rauchs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN", "$,", "ADJD", "KON", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Duft,", "tokens": ["Duft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Zum Altare der Grazien,", "tokens": ["Zum", "Al\u00b7ta\u00b7re", "der", "Gra\u00b7zi\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Junger Blumen Geruch, welche die Muse mir", "tokens": ["Jun\u00b7ger", "Blu\u00b7men", "Ge\u00b7ruch", ",", "wel\u00b7che", "die", "Mu\u00b7se", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "$,", "PRELS", "ART", "NN", "PPER"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Jm geheimeren Thale las!", "tokens": ["Jm", "ge\u00b7hei\u00b7me\u00b7ren", "Tha\u00b7le", "las", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "L\u00e4chelt immer mir zu, stimmet mein Saiten-", "tokens": ["L\u00e4\u00b7chelt", "im\u00b7mer", "mir", "zu", ",", "stim\u00b7met", "mein", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "PTKVZ", "$,", "VVFIN", "PPOSAT", "TRUNC"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "spiel,", "tokens": ["spiel", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.12": {"text": "Allbelebende G\u00f6ttinnen!", "tokens": ["All\u00b7be\u00b7le\u00b7ben\u00b7de", "G\u00f6t\u00b7tin\u00b7nen", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Lehret meinen Gesang senken vom Himmel sich,", "tokens": ["Leh\u00b7ret", "mei\u00b7nen", "Ge\u00b7sang", "sen\u00b7ken", "vom", "Him\u00b7mel", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "PRF", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "In die Quelle der Sch\u00f6nheit sich", "tokens": ["In", "die", "Quel\u00b7le", "der", "Sch\u00f6n\u00b7heit", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PRF"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Tauchen, gl\u00e4nzender dann steigen dem Himmel", "tokens": ["Tau\u00b7chen", ",", "gl\u00e4n\u00b7zen\u00b7der", "dann", "stei\u00b7gen", "dem", "Him\u00b7mel"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "zu!", "tokens": ["zu", "!"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Ach, die Blume des Liedes welkt", "tokens": ["Ach", ",", "die", "Blu\u00b7me", "des", "Lie\u00b7des", "welkt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.18": {"text": "In dem Kranze des Ruhms, wenn sie ein Sterb-", "tokens": ["In", "dem", "Kran\u00b7ze", "des", "Ruhms", ",", "wenn", "sie", "ein", "Ster\u00b7b"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "TRUNC"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "licher", "tokens": ["li\u00b7cher"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Mit unheiligen H\u00e4nden pfl\u00fcckt!", "tokens": ["Mit", "un\u00b7hei\u00b7li\u00b7gen", "H\u00e4n\u00b7den", "pfl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.21": {"text": "Pfl\u00fccket ihr sie f\u00fcr mich, da\u00df nicht der silberne", "tokens": ["Pfl\u00fc\u00b7cket", "ihr", "sie", "f\u00fcr", "mich", ",", "da\u00df", "nicht", "der", "sil\u00b7ber\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "PPER", "$,", "KOUS", "PTKNEG", "ART", "NN"], "meter": "+--+-+-+-+--", "measure": "iambic.penta.invert"}, "line.22": {"text": "Sonnenstralende Morgenthau", "tokens": ["Son\u00b7nen\u00b7stra\u00b7len\u00b7de", "Mor\u00b7gent\u00b7hau"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.23": {"text": "Jhr enttr\u00e4ufle, sie nicht hangend gekr\u00e4uselte", "tokens": ["Ihr", "ent\u00b7tr\u00e4uf\u00b7le", ",", "sie", "nicht", "han\u00b7gend", "ge\u00b7kr\u00e4u\u00b7sel\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "PTKNEG", "ADJD", "ADJA"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "Bl\u00e4tter senke der Erde zu!", "tokens": ["Bl\u00e4t\u00b7ter", "sen\u00b7ke", "der", "Er\u00b7de", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.25": {"text": "Euch soll k\u00fcnftig ein Hain bl\u00fchender Stauden,", "tokens": ["Euch", "soll", "k\u00fcnf\u00b7tig", "ein", "Hain", "bl\u00fc\u00b7hen\u00b7der", "Stau\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "euch", "tokens": ["euch"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Meine Quelle geweihet sein,", "tokens": ["Mei\u00b7ne", "Quel\u00b7le", "ge\u00b7wei\u00b7het", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VAINF", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.28": {"text": "Euch mein moosiges Dach, und die Bewohner", "tokens": ["Euch", "mein", "moo\u00b7si\u00b7ges", "Dach", ",", "und", "die", "Be\u00b7woh\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "der", "tokens": ["der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.30": {"text": "Stillen H\u00fctte geweihet sein!", "tokens": ["Stil\u00b7len", "H\u00fct\u00b7te", "ge\u00b7wei\u00b7het", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.31": {"text": "Suchet ihr mir, und bald, unter den freundlichen", "tokens": ["Su\u00b7chet", "ihr", "mir", ",", "und", "bald", ",", "un\u00b7ter", "den", "freund\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KON", "ADV", "$,", "APPR", "ART", "ADJA"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.32": {"text": "T\u00f6chtern Deutschlands ein M\u00e4dchen aus,", "tokens": ["T\u00f6ch\u00b7tern", "Deutschlands", "ein", "M\u00e4d\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Blau die Augen, ihr Haar golden, und schlank", "tokens": ["Blau", "die", "Au\u00b7gen", ",", "ihr", "Haar", "gol\u00b7den", ",", "und", "schlank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PPOSAT", "NN", "ADJD", "$,", "KON", "VVFIN"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.34": {"text": "ihr Wuchs,", "tokens": ["ihr", "Wuchs", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.35": {"text": "Sanft die Seele, den Augen gleich,", "tokens": ["Sanft", "die", "See\u00b7le", ",", "den", "Au\u00b7gen", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.36": {"text": "Da\u00df sie Priesterin sei eurem Altare, fr\u00fch,", "tokens": ["Da\u00df", "sie", "Pries\u00b7te\u00b7rin", "sei", "eu\u00b7rem", "Al\u00b7ta\u00b7re", ",", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Wenn ihr r\u00f6thend die Sonne winkt,", "tokens": ["Wenn", "ihr", "r\u00f6\u00b7thend", "die", "Son\u00b7ne", "winkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.38": {"text": "Jhr im leichten Gewand flattert die Morgenluft,", "tokens": ["Ihr", "im", "leich\u00b7ten", "Ge\u00b7wand", "flat\u00b7tert", "die", "Mor\u00b7gen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.39": {"text": "Und im wallenden Schleierflor!", "tokens": ["Und", "im", "wal\u00b7len\u00b7den", "Schlei\u00b7er\u00b7flor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.40": {"text": "Da\u00df sie Priesterin sei eurem Altare, sp\u00e4t,", "tokens": ["Da\u00df", "sie", "Pries\u00b7te\u00b7rin", "sei", "eu\u00b7rem", "Al\u00b7ta\u00b7re", ",", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Wenn ihr winket der Abendstern,", "tokens": ["Wenn", "ihr", "win\u00b7ket", "der", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Und der Nachtigall Lied um den Altar ert\u00f6nt!", "tokens": ["Und", "der", "Nach\u00b7ti\u00b7gall", "Lied", "um", "den", "Al\u00b7tar", "er\u00b7t\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.43": {"text": "Wenn ein Kind ihr am Busen h\u00e4ngt,", "tokens": ["Wenn", "ein", "Kind", "ihr", "am", "Bu\u00b7sen", "h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.44": {"text": "Wird sie weihen das Kind euren Alt\u00e4ren; einst", "tokens": ["Wird", "sie", "wei\u00b7hen", "das", "Kind", "eu\u00b7ren", "Al\u00b7t\u00e4\u00b7ren", ";", "einst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$.", "ADV"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.45": {"text": "Wird die Tochter, die Enkelin", "tokens": ["Wird", "die", "Toch\u00b7ter", ",", "die", "En\u00b7ke\u00b7lin"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.46": {"text": "Euch noch singen mein Lied; dann werd\u2019 ich", "tokens": ["Euch", "noch", "sin\u00b7gen", "mein", "Lied", ";", "dann", "werd'", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "PPER"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.47": {"text": "freudiger", "tokens": ["freu\u00b7di\u00b7ger"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+--", "measure": "dactylic.init"}, "line.48": {"text": "Greis mit zitternden Thr\u00e4nen noch", "tokens": ["Greis", "mit", "zit\u00b7tern\u00b7den", "Thr\u00e4\u00b7nen", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.49": {"text": "Mich am w\u00e4rmenden Stral sonnen, mit zit-", "tokens": ["Mich", "am", "w\u00e4r\u00b7men\u00b7den", "Stral", "son\u00b7nen", ",", "mit", "zit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPRART", "ADJA", "NN", "VVINF", "$,", "APPR", "TRUNC"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.50": {"text": "ternder", "tokens": ["tern\u00b7der"], "token_info": ["word"], "pos": ["NE"], "meter": "--", "measure": "unknown.measure.zero"}, "line.51": {"text": "Hand noch r\u00fchren mein Saitenspiel,", "tokens": ["Hand", "noch", "r\u00fch\u00b7ren", "mein", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.52": {"text": "Bis mit L\u00e4cheln mein Haupt sanft in die Gru-", "tokens": ["Bis", "mit", "L\u00e4\u00b7cheln", "mein", "Haupt", "sanft", "in", "die", "Gru"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "NN", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.53": {"text": "be sinkt!", "tokens": ["be", "sinkt", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}