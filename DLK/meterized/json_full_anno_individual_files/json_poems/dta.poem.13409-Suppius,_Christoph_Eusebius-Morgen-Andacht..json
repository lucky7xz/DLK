{"dta.poem.13409": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Morgen-Andacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Tag! Gottlob! dich seh ich wieder,", "tokens": ["Tag", "!", "Gott\u00b7lob", "!", "dich", "seh", "ich", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der du diese Welt geweckt,", "tokens": ["Der", "du", "die\u00b7se", "Welt", "ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und was seine m\u00fcden Glieder", "tokens": ["Und", "was", "sei\u00b7ne", "m\u00fc\u00b7den", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Schatten ausgestreckt;", "tokens": ["In", "den", "Schat\u00b7ten", "aus\u00b7ge\u00b7streckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein Erscheinen hat gemacht,", "tokens": ["Dein", "Er\u00b7schei\u00b7nen", "hat", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df mein munter Aug erwacht.", "tokens": ["Da\u00df", "mein", "mun\u00b7ter", "Aug", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was die Dunkelheit verborgen,", "tokens": ["Was", "die", "Dun\u00b7kel\u00b7heit", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was in ihr versunken war,", "tokens": ["Was", "in", "ihr", "ver\u00b7sun\u00b7ken", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stellest du, o g\u00fcldner Morgen!", "tokens": ["Stel\u00b7lest", "du", ",", "o", "g\u00fcld\u00b7ner", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleichsam junggebohren dar,", "tokens": ["Gleich\u00b7sam", "jung\u00b7ge\u00b7boh\u00b7ren", "dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich erblick in Luft und Feld", "tokens": ["Ich", "er\u00b7blick", "in", "Luft", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine neuerschaffne Welt.", "tokens": ["Ei\u00b7ne", "neu\u00b7er\u00b7schaff\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Traurig hatt ich mich geleget,", "tokens": ["Trau\u00b7rig", "hatt", "ich", "mich", "ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gestern schlief ich furchtsam ein,", "tokens": ["Ge\u00b7stern", "schlief", "ich", "furcht\u00b7sam", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Weil der Zweifel sich gereget:", "tokens": ["Weil", "der", "Zwei\u00b7fel", "sich", "ge\u00b7re\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirst du auch noch morgen seyn?", "tokens": ["Wirst", "du", "auch", "noch", "mor\u00b7gen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwischen Schlaf und Sterbens-Zeit", "tokens": ["Zwi\u00b7schen", "Schlaf", "und", "Ster\u00b7bens\u00b7Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist fast gar kein Unterscheid.", "tokens": ["Ist", "fast", "gar", "kein", "Un\u00b7ter\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber ich bin auferstanden,", "tokens": ["A\u00b7ber", "ich", "bin", "auf\u00b7er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Tage dauren noch,", "tokens": ["Mei\u00b7ne", "Ta\u00b7ge", "dau\u00b7ren", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch bin ich gesund vorhanden;", "tokens": ["Auch", "bin", "ich", "ge\u00b7sund", "vor\u00b7han\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seele, schwinge dich nun hoch,", "tokens": ["See\u00b7le", ",", "schwin\u00b7ge", "dich", "nun", "hoch", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Stimme dem, der die\u00df gethan,", "tokens": ["Stim\u00b7me", "dem", ",", "der", "die\u00df", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PDS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Morgen Halleluja an!", "tokens": ["Mor\u00b7gen", "Hal\u00b7le\u00b7lu\u00b7ja", "an", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "HeRR der Tage! GOTT der Zeiten!", "tokens": ["HeRR", "der", "Ta\u00b7ge", "!", "GoTT", "der", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$.", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6pfer, der die Welt erschuf!", "tokens": ["Sch\u00f6p\u00b7fer", ",", "der", "die", "Welt", "er\u00b7schuf", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir will ich ein Lob bereiten,", "tokens": ["Dir", "will", "ich", "ein", "Lob", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das erfordert mein Beruf,", "tokens": ["Das", "er\u00b7for\u00b7dert", "mein", "Be\u00b7ruf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil Du, was vern\u00fcnftig denkt,", "tokens": ["Weil", "Du", ",", "was", "ver\u00b7n\u00fcnf\u00b7tig", "denkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Meinem K\u00f6rper eingesenkt.", "tokens": ["Mei\u00b7nem", "K\u00f6r\u00b7per", "ein\u00b7ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00f6chster! sey von mir gepriesen,", "tokens": ["H\u00f6chs\u00b7ter", "!", "sey", "von", "mir", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig sey Dir Lob gebracht!", "tokens": ["E\u00b7wig", "sey", "Dir", "Lob", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00e4chtig hast Du Dich bewiesen", "tokens": ["M\u00e4ch\u00b7tig", "hast", "Du", "Dich", "be\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der abgewichnen Nacht,", "tokens": ["In", "der", "ab\u00b7ge\u00b7wich\u00b7nen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df mich mitten in dem Schlaf", "tokens": ["Da\u00df", "mich", "mit\u00b7ten", "in", "dem", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kein verheerend Uebel traf.", "tokens": ["Kein", "ver\u00b7hee\u00b7rend", "Ue\u00b7bel", "traf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Mit dankbarestem Gem\u00fcthe,", "tokens": ["Mit", "dank\u00b7ba\u00b7res\u00b7tem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Mit inbr\u00fcnstigem Geschrey", "tokens": ["Mit", "in\u00b7br\u00fcns\u00b7ti\u00b7gem", "Ge\u00b7schrey"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "R\u00fchm ich, Vater! deine G\u00fcte,", "tokens": ["R\u00fchm", "ich", ",", "Va\u00b7ter", "!", "dei\u00b7ne", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "NN", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die an jedem Morgen neu:", "tokens": ["Die", "an", "je\u00b7dem", "Mor\u00b7gen", "neu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie erhielt mich jedesmahl", "tokens": ["Sie", "er\u00b7hielt", "mich", "je\u00b7des\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch so vieler N\u00e4chte Zahl.", "tokens": ["Durch", "so", "vie\u00b7ler", "N\u00e4ch\u00b7te", "Zahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O mein GOtt! reich an Erbarmen,", "tokens": ["O", "mein", "Gott", "!", "reich", "an", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welcher mich der Finsterni\u00df", "tokens": ["Wel\u00b7cher", "mich", "der", "Fins\u00b7ter\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser Nacht mit Retter-Armen", "tokens": ["Die\u00b7ser", "Nacht", "mit", "Ret\u00b7ter\u00b7Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohnbesch\u00e4diget entri\u00df,", "tokens": ["Ohn\u00b7be\u00b7sch\u00e4\u00b7di\u00b7get", "ent\u00b7ri\u00df", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mund und Andacht stimmen an:", "tokens": ["Mund", "und", "An\u00b7dacht", "stim\u00b7men", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses hat der HErr gethan!", "tokens": ["Die\u00b7ses", "hat", "der", "Herr", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Vater! der Du meiner pflegest,", "tokens": ["Va\u00b7ter", "!", "der", "Du", "mei\u00b7ner", "pfle\u00b7gest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PRELS", "PPER", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Du mit so starker Wehr", "tokens": ["Der", "Du", "mit", "so", "star\u00b7ker", "Wehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich um meinen Schlaf-Ort legest,", "tokens": ["Dich", "um", "mei\u00b7nen", "Schlaf\u00b7Ort", "le\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Durch der guten Engel Heer,", "tokens": ["Durch", "der", "gu\u00b7ten", "En\u00b7gel", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df so manche Nacht verstrich,", "tokens": ["Da\u00df", "so", "man\u00b7che", "Nacht", "ver\u00b7strich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn verdient ich das um Dich?", "tokens": ["Wenn", "ver\u00b7dient", "ich", "das", "um", "Dich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "PPER", "ART", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "In der Nacht des Mutterleibes,", "tokens": ["In", "der", "Nacht", "des", "Mut\u00b7ter\u00b7lei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh ich noch vollkommen ward,", "tokens": ["Eh", "ich", "noch", "voll\u00b7kom\u00b7men", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r ich, S\u00fcndenfrucht des Weibes,", "tokens": ["W\u00e4r", "ich", ",", "S\u00fcn\u00b7den\u00b7frucht", "des", "Wei\u00b7bes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alsbald wiederum verscharrt,", "tokens": ["Als\u00b7bald", "wie\u00b7de\u00b7rum", "ver\u00b7scharrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh ich noch das Licht gesehn,", "tokens": ["Eh", "ich", "noch", "das", "Licht", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sollt es nach Verdiensten gehn.", "tokens": ["Sollt", "es", "nach", "Ver\u00b7diens\u00b7ten", "gehn", "."], "token_info": ["