{"dta.poem.8799": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xx.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Aber wie die keusche Rose sey er tief und voll!", "tokens": ["A\u00b7ber", "wie", "die", "keu\u00b7sche", "Ro\u00b7se", "sey", "er", "tief", "und", "voll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "La\u00df den Schmerz in deiner Seele wogen auf und ab,", "tokens": ["La\u00df", "den", "Schmerz", "in", "dei\u00b7ner", "See\u00b7le", "wo\u00b7gen", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Da so oft dem Quell des Leidens dein Gesang entquoll!", "tokens": ["Da", "so", "oft", "dem", "Quell", "des", "Lei\u00b7dens", "dein", "Ge\u00b7sang", "ent\u00b7quoll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Peinigt dich ein Liebeskummer, sey getrost, o Herz!", "tokens": ["Pei\u00b7nigt", "dich", "ein", "Lie\u00b7bes\u00b7kum\u00b7mer", ",", "sey", "ge\u00b7trost", ",", "o", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "VAFIN", "VVPP", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Traurig macht verschm\u00e4hte Liebe, doch begl\u00fcckte toll;", "tokens": ["Trau\u00b7rig", "macht", "ver\u00b7schm\u00e4h\u00b7te", "Lie\u00b7be", ",", "doch", "be\u00b7gl\u00fcck\u00b7te", "toll", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADJA", "NN", "$,", "ADV", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "W\u00e4re Daphne nicht entronnen ihres Buhlen Arm,", "tokens": ["W\u00e4\u00b7re", "Daph\u00b7ne", "nicht", "ent\u00b7ron\u00b7nen", "ih\u00b7res", "Buh\u00b7len", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Welchen Kranz um seine Lyra schl\u00e4nge dann Apoll?", "tokens": ["Wel\u00b7chen", "Kranz", "um", "sei\u00b7ne", "Ly\u00b7ra", "schl\u00e4n\u00b7ge", "dann", "A\u00b7poll", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "F\u00fcrchte nicht zu sterben, Guter! denn das Leben tr\u00fcgt:", "tokens": ["F\u00fcrch\u00b7te", "nicht", "zu", "ster\u00b7ben", ",", "Gu\u00b7ter", "!", "denn", "das", "Le\u00b7ben", "tr\u00fcgt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKZU", "VVINF", "$,", "NN", "$.", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.9": {"text": "Gib der Erde gern den lezten, schauderhaften Zoll!", "tokens": ["Gib", "der", "Er\u00b7de", "gern", "den", "lez\u00b7ten", ",", "schau\u00b7der\u00b7haf\u00b7ten", "Zoll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "La\u00df das welke Blatt vom Baume st\u00fcrzen in den Teich,", "tokens": ["La\u00df", "das", "wel\u00b7ke", "Blatt", "vom", "Bau\u00b7me", "st\u00fcr\u00b7zen", "in", "den", "Teich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Weil es noch im Todestaumel sich berauschen soll!", "tokens": ["Weil", "es", "noch", "im", "To\u00b7de\u00b7stau\u00b7mel", "sich", "be\u00b7rau\u00b7schen", "soll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}