{"textgrid.poem.49489": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "An den Grafen von Stralenheim, Me\u00eetre de Camp, und Obrister beym Regiment Royal-Allemand", "genre": "verse", "period": "N.A.", "pub_year": 1747, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieb ich dich, du unbezwungener Krieger,", "tokens": ["Wie", "lieb", "ich", "dich", ",", "du", "un\u00b7be\u00b7zwun\u00b7ge\u00b7ner", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Armuth sch\u00e4zt, und persische Weichlichkeit flieht,", "tokens": ["Der", "Ar\u00b7muth", "sch\u00e4zt", ",", "und", "per\u00b7si\u00b7sche", "Weich\u00b7lich\u00b7keit", "flieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und wie ein Cherub, zwischen flammenden W\u00e4nden,", "tokens": ["Und", "wie", "ein", "Che\u00b7rub", ",", "zwi\u00b7schen", "flam\u00b7men\u00b7den", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zum goldnen Schlo\u00df der Ewigkeit steigt.", "tokens": ["Zum", "gold\u00b7nen", "Schlo\u00df", "der", "E\u00b7wig\u00b7keit", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Dein bloses Haupt dr\u00fcckt nachts im t\u00fcrkschen Gezelte,", "tokens": ["Dein", "blo\u00b7ses", "Haupt", "dr\u00fcckt", "nachts", "im", "t\u00fcrk\u00b7schen", "Ge\u00b7zel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In das der Mond mit hellem Angesicht guckt,", "tokens": ["In", "das", "der", "Mond", "mit", "hel\u00b7lem", "An\u00b7ge\u00b7sicht", "guckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An Polsters statt, den blaugest\u00e4hleten Kuras,", "tokens": ["An", "Pols\u00b7ters", "statt", ",", "den", "blau\u00b7ge\u00b7st\u00e4h\u00b7le\u00b7ten", "Ku\u00b7ras", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und tr\u00e4umt auf harter Erde von Ruh,", "tokens": ["Und", "tr\u00e4umt", "auf", "har\u00b7ter", "Er\u00b7de", "von", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Und tr\u00e4umend wandelstu in marmornen G\u00e4ngen,", "tokens": ["Und", "tr\u00e4u\u00b7mend", "wan\u00b7dels\u00b7tu", "in", "mar\u00b7mor\u00b7nen", "G\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit Philosophen und mit Helden vermischt,", "tokens": ["Mit", "Phi\u00b7lo\u00b7so\u00b7phen", "und", "mit", "Hel\u00b7den", "ver\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die ihre Kr\u00e4ntz um deine Kr\u00e4ntze vertauschten,", "tokens": ["Die", "ih\u00b7re", "Kr\u00e4ntz", "um", "dei\u00b7ne", "Kr\u00e4nt\u00b7ze", "ver\u00b7tauschten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und sehn, du bist nicht kleiner, als sie.", "tokens": ["Und", "sehn", ",", "du", "bist", "nicht", "klei\u00b7ner", ",", "als", "sie", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Bi\u00df um dich her das Prasseln fressender Flammen,", "tokens": ["Bi\u00df", "um", "dich", "her", "das", "Pras\u00b7seln", "fres\u00b7sen\u00b7der", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PRF", "APZR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um Mitternacht den sanften Morpheus verjagt,", "tokens": ["Um", "Mit\u00b7ter\u00b7nacht", "den", "sanf\u00b7ten", "Mor\u00b7pheus", "ver\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dann greift die braune Faust zur schwankenden Lanze,", "tokens": ["Dann", "greift", "die", "brau\u00b7ne", "Faust", "zur", "schwan\u00b7ken\u00b7den", "Lan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die in dem Rasen, neben dir, steckt;", "tokens": ["Die", "in", "dem", "Ra\u00b7sen", ",", "ne\u00b7ben", "dir", ",", "steckt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$,", "APPR", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Denn gehestu, mit lebhaft bl\u00fchenden Wangen,", "tokens": ["Denn", "ge\u00b7hes\u00b7tu", ",", "mit", "leb\u00b7haft", "bl\u00fc\u00b7hen\u00b7den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "(als rolle deine Braut in f\u00fcrstlicher Pracht,", "tokens": ["(", "als", "rol\u00b7le", "dei\u00b7ne", "Braut", "in", "f\u00fcrst\u00b7li\u00b7cher", "Pracht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Auf einem Wagen von gediegenem Silber,", "tokens": ["Auf", "ei\u00b7nem", "Wa\u00b7gen", "von", "ge\u00b7die\u00b7ge\u00b7nem", "Sil\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vom \u00f6stlichen Geb\u00fcrge herab,)", "tokens": ["Vom", "\u00f6st\u00b7li\u00b7chen", "Ge\u00b7b\u00fcr\u00b7ge", "her\u00b7ab", ",", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$,", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "Den blankbepanzerten Phalangen entgegen,", "tokens": ["Den", "blank\u00b7be\u00b7pan\u00b7zer\u00b7ten", "Pha\u00b7lan\u00b7gen", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die, gleich dem ewgen Wall um Rhadamanths Stadt,", "tokens": ["Die", ",", "gleich", "dem", "ew\u00b7gen", "Wall", "um", "Rha\u00b7da\u00b7manths", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Erhaben stehn; doch die du schneller zertheilest,", "tokens": ["Er\u00b7ha\u00b7ben", "stehn", ";", "doch", "die", "du", "schnel\u00b7ler", "zer\u00b7thei\u00b7lest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "ADV", "ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als Luna Creise giftigen Dunsts.", "tokens": ["Als", "Lu\u00b7na", "Crei\u00b7se", "gif\u00b7ti\u00b7gen", "Dunsts", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Wenn nun ein schwartzes Meer lautrauschenden Blutes,", "tokens": ["Wenn", "nun", "ein", "schwart\u00b7zes", "Meer", "laut\u00b7rau\u00b7schen\u00b7den", "Blu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In das der Abendstern den Silberstrahl tunkt,", "tokens": ["In", "das", "der", "A\u00b7bends\u00b7tern", "den", "Sil\u00b7ber\u00b7strahl", "tunkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Mit regen Wirbeln halbverbrannte Gefilde,", "tokens": ["Mit", "re\u00b7gen", "Wir\u00b7beln", "halb\u00b7ver\u00b7brann\u00b7te", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nebst Mann und Ro\u00df und Wagen verschlingt:", "tokens": ["Nebst", "Mann", "und", "Ro\u00df", "und", "Wa\u00b7gen", "ver\u00b7schlingt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Denn sieht die Muse, deren silberner Wurfspie\u00df", "tokens": ["Denn", "sieht", "die", "Mu\u00b7se", ",", "de\u00b7ren", "sil\u00b7ber\u00b7ner", "Wurf\u00b7spie\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-----+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dein feines Ohr mit hohem Sausen erf\u00fcllt,", "tokens": ["Dein", "fei\u00b7nes", "Ohr", "mit", "ho\u00b7hem", "Sau\u00b7sen", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Dort deinen Vater", "tokens": ["Dort", "dei\u00b7nen", "Va\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Aus dem Pallast Elysiens schaun,", "tokens": ["Aus", "dem", "Pal\u00b7last", "E\u00b7ly\u00b7si\u00b7ens", "schaun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVINF", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.9": {"line.1": {"text": "Wie sich dies Wort von seinen Lippen ergieset:", "tokens": ["Wie", "sich", "dies", "Wort", "von", "sei\u00b7nen", "Lip\u00b7pen", "er\u00b7gie\u00b7set", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PDS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u00bbvortreflichsch\u00f6n, Sohn, wer f\u00fcrs Vaterland lebt;", "tokens": ["\u00bb", "vor\u00b7tre\u00b7flich\u00b7sch\u00f6n", ",", "Sohn", ",", "wer", "f\u00fcrs", "Va\u00b7ter\u00b7land", "lebt", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "NN", "$,", "PWS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Unendlichsch\u00f6ner, wer f\u00fcrs Vaterlands Beste,", "tokens": ["Un\u00b7end\u00b7lich\u00b7sch\u00f6\u00b7ner", ",", "wer", "f\u00fcrs", "Va\u00b7ter\u00b7lands", "Bes\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "APPRART", "NN", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie du, ins Todes Rachen sich wagt.", "tokens": ["Wie", "du", ",", "ins", "To\u00b7des", "Ra\u00b7chen", "sich", "wagt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPRART", "NN", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.10": {"line.1": {"text": "Kein K\u00f6nigreich mag den nach W\u00fcrden belohnen;", "tokens": ["Kein", "K\u00f6\u00b7nig\u00b7reich", "mag", "den", "nach", "W\u00fcr\u00b7den", "be\u00b7loh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ART", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nur der Unsterblichkeit demantener Krantz,", "tokens": ["Nur", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "de\u00b7man\u00b7te\u00b7ner", "Krantz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und ein Gesang, den Langens goldene Leyer", "tokens": ["Und", "ein", "Ge\u00b7sang", ",", "den", "Lan\u00b7gens", "gol\u00b7de\u00b7ne", "Le\u00b7yer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf dem Geb\u00fcrg Aoniens singt.\u00ab", "tokens": ["Auf", "dem", "Ge\u00b7b\u00fcrg", "A\u00b7o\u00b7ni\u00b7ens", "singt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Wie lieb ich dich, du unbezwungener Krieger,", "tokens": ["Wie", "lieb", "ich", "dich", ",", "du", "un\u00b7be\u00b7zwun\u00b7ge\u00b7ner", "Krie\u00b7ge