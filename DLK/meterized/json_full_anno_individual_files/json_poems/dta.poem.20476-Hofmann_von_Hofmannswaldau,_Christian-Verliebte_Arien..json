{"dta.poem.20476": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fach/ Amaranthe/ deine ballen", "tokens": ["Fach", "/", "A\u00b7ma\u00b7ran\u00b7the", "/", "dei\u00b7ne", "bal\u00b7len"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$(", "NE", "$(", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit frischen seuffzern wieder auff;", "tokens": ["Mit", "fri\u00b7schen", "seuff\u00b7zern", "wie\u00b7der", "auff", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df nicht die b\u00e4lge niederfallen/", "tokens": ["La\u00df", "nicht", "die", "b\u00e4l\u00b7ge", "nie\u00b7der\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fl\u00f6\u00df thau von deinen lippen drauff:", "tokens": ["Fl\u00f6\u00df", "thau", "von", "dei\u00b7nen", "lip\u00b7pen", "drauff", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "H\u00fcll ihre flammen", "tokens": ["H\u00fcll", "ih\u00b7re", "flam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In scharlach ein/", "tokens": ["In", "schar\u00b7lach", "ein", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zwing sie zusammen/", "tokens": ["Zwing", "sie", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Bi\u00df sie wie stein", "tokens": ["Bi\u00df", "sie", "wie", "stein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Gebildet seyn.", "tokens": ["Ge\u00b7bil\u00b7det", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die br\u00fcste sind der liebe zunder/", "tokens": ["Die", "br\u00fcs\u00b7te", "sind", "der", "lie\u00b7be", "zun\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von den die sch\u00f6nheit nahrung nimmt.", "tokens": ["Von", "den", "die", "sch\u00f6n\u00b7heit", "nah\u00b7rung", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein stamm und abgott aller wunder/", "tokens": ["Ein", "stamm", "und", "ab\u00b7gott", "al\u00b7ler", "wun\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bl\u00fcht und frucht zugleich bek\u00f6mmt.", "tokens": ["So", "bl\u00fcht", "und", "frucht", "zu\u00b7gleich", "be\u00b7k\u00f6mmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des tempels kertzen", "tokens": ["Des", "tem\u00b7pels", "kert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Sind so verdacht/", "tokens": ["Sind", "so", "ver\u00b7dacht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da\u00df kalten hertzen", "tokens": ["Da\u00df", "kal\u00b7ten", "hert\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Durch ihre pracht", "tokens": ["Durch", "ih\u00b7re", "pracht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wird glut beybracht.", "tokens": ["Wird", "glut", "bey\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Der wangen bunte zauber-k\u00fcnste/", "tokens": ["Der", "wan\u00b7gen", "bun\u00b7te", "zau\u00b7ber\u00b7k\u00fcns\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verliehrn die w\u00fcrckung bey der brust;", "tokens": ["Ver\u00b7liehrn", "die", "w\u00fcr\u00b7ckung", "bey", "der", "brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die achseln schimmern/ wie durch d\u00fcnste/", "tokens": ["Die", "ach\u00b7seln", "schim\u00b7mern", "/", "wie", "durch", "d\u00fcns\u00b7te", "/"], "to