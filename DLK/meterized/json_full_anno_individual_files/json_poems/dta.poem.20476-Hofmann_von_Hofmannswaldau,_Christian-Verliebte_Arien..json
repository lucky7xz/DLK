"token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Ihr zucker-\u00e4pffel aller l\u00fcste/", "tokens": ["Ihr", "zu\u00b7cke\u00b7r\u00e4pf\u00b7fel", "al\u00b7ler", "l\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entbl\u00f6st den marmol/ den ihr hegt/", "tokens": ["Ent\u00b7bl\u00f6st", "den", "mar\u00b7mol", "/", "den", "ihr", "hegt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ART", "PPER", "VVFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Weht flor und schleyer weg ihr br\u00fcste/", "tokens": ["Weht", "flor", "und", "schle\u00b7yer", "weg", "ihr", "br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "KON", "ADJD", "ADV", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weist was der strauch vor blumen tr\u00e4gt.", "tokens": ["Weist", "was", "der", "strauch", "vor", "blu\u00b7men", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was stets entdecket/", "tokens": ["Was", "stets", "ent\u00b7de\u00b7cket", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ist nur gemein;", "tokens": ["Ist", "nur", "ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "W