{"dta.poem.2099": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Biber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Daher sie gleichfalls zwey, drey Tag\u2019 an solchen Oertern", "tokens": ["Da\u00b7her", "sie", "gleich\u00b7falls", "zwey", ",", "drey", "Tag'", "an", "sol\u00b7chen", "O\u00b7er\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "CARD", "$,", "CARD", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "stille stehn,", "tokens": ["stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Des Bibers Wiederkunft erwartend, da sie ihn f\u00e4llen", "tokens": ["Des", "Bi\u00b7bers", "Wie\u00b7der\u00b7kunft", "er\u00b7war\u00b7tend", ",", "da", "sie", "ihn", "f\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVPP", "$,", "KOUS", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "und erschlagen,", "tokens": ["und", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Und seinen n\u00fctz- und kostbarn Pelz zur Beute mit nach", "tokens": ["Und", "sei\u00b7nen", "n\u00fctz", "und", "kost\u00b7barn", "Pelz", "zur", "Beu\u00b7te", "mit", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "TRUNC", "KON", "ADJA", "NN", "APPRART", "NN", "APPR", "APPR"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Hause tragen.", "tokens": ["Hau\u00b7se", "tra\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Der Reichthum ist nicht auszudr\u00fccken, den uns ihr Haar", "tokens": ["Der", "Reicht\u00b7hum", "ist", "nicht", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ",", "den", "uns", "ihr", "Haar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "VVINF", "$,", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "und Pelzwerk reicht,", "tokens": ["und", "Pelz\u00b7werk", "reicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "So da\u00df in Canada dem Handel von Pelzwerk fast kein", "tokens": ["So", "da\u00df", "in", "Ca\u00b7na\u00b7da", "dem", "Han\u00b7del", "von", "Pelz\u00b7werk", "fast", "kein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "NE", "ART", "NN", "APPR", "NN", "ADV", "PIAT"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "andrer gleicht.", "tokens": ["an\u00b7drer", "gleicht", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Wer wollte denn, da diese Thier\u2019 uns solchen gro\u00dfen", "tokens": ["Wer", "woll\u00b7te", "denn", ",", "da", "die\u00b7se", "Thier'", "uns", "sol\u00b7chen", "gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "$,", "KOUS", "PDAT", "NN", "PPER", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Vortheil schenken,", "tokens": ["Vor\u00b7theil", "schen\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Wenn man dieselben braucht und n\u00fctzt, nicht auch an", "tokens": ["Wenn", "man", "die\u00b7sel\u00b7ben", "braucht", "und", "n\u00fctzt", ",", "nicht", "auch", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PDAT", "VVFIN", "KON", "VVFIN", "$,", "PTKNEG", "ADV", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "ihren Sch\u00f6pfer denken?", "tokens": ["ih\u00b7ren", "Sch\u00f6p\u00b7fer", "den\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}