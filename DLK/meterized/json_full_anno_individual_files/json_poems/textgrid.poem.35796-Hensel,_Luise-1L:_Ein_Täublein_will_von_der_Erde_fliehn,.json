{"textgrid.poem.35796": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein T\u00e4ublein will von der Erde fliehn,", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein T\u00e4ublein will von der Erde fliehn,", "tokens": ["Ein", "T\u00e4ub\u00b7lein", "will", "von", "der", "Er\u00b7de", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fliegt auf zum Himmelsblau:", "tokens": ["Fliegt", "auf", "zum", "Him\u00b7mels\u00b7blau", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbade, ihr W\u00e4lder und Felder gr\u00fcn!", "tokens": ["\u00bb", "a\u00b7de", ",", "ihr", "W\u00e4l\u00b7der", "und", "Fel\u00b7der", "gr\u00fcn", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PPOSAT", "NN", "KON", "NN", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ade, du bunte Au!\u00ab", "tokens": ["A\u00b7de", ",", "du", "bun\u00b7te", "Au", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Ach T\u00e4ublein, warte ein Weilchen noch,", "tokens": ["Ach", "T\u00e4ub\u00b7lein", ",", "war\u00b7te", "ein", "Weil\u00b7chen", "noch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Magst nochmals um dich sehn;", "tokens": ["Magst", "noch\u00b7mals", "um", "dich", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach T\u00e4ublein, bleib' hienieden doch,", "tokens": ["Ach", "T\u00e4ub\u00b7lein", ",", "bleib'", "hien\u00b7ie\u00b7den", "doch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Erd' ist ja so sch\u00f6n!", "tokens": ["Die", "Erd'", "ist", "ja", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbwozu denn hat mir Fl\u00fcgelein", "tokens": ["\u00bb", "wo\u00b7zu", "denn", "hat", "mir", "Fl\u00fc\u00b7ge\u00b7lein"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ADV", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der liebe Gott verliehn?", "tokens": ["Der", "lie\u00b7be", "Gott", "ver\u00b7liehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann nicht l\u00e4nger auf Erden sein,", "tokens": ["Ich", "kann", "nicht", "l\u00e4n\u00b7ger", "auf", "Er\u00b7den", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "La\u00df mich von hinnen ziehn!\u00ab", "tokens": ["La\u00df", "mich", "von", "hin\u00b7nen", "ziehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das T\u00e4ublein fliegt hoch in die H\u00f6h',", "tokens": ["Das", "T\u00e4ub\u00b7lein", "fliegt", "hoch", "in", "die", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "L\u00e4\u00dft alle Freuden gern;", "tokens": ["L\u00e4\u00dft", "al\u00b7le", "Freu\u00b7den", "gern", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch thun ihm pl\u00f6tzlich die Fl\u00fcglein weh,", "tokens": ["Doch", "thun", "ihm", "pl\u00f6tz\u00b7lich", "die", "Fl\u00fcg\u00b7lein", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Himmel ist noch so fern.", "tokens": ["Der", "Him\u00b7mel", "ist", "noch", "so", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Da steht ein hohes Felsgestein;", "tokens": ["Da", "steht", "ein", "ho\u00b7hes", "Fels\u00b7ge\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das T\u00e4ublein ruhen begehrt.", "tokens": ["Das", "T\u00e4ub\u00b7lein", "ru\u00b7hen", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da sitzt es verlassen, die Aeugelein", "tokens": ["Da", "sitzt", "es", "ver\u00b7las\u00b7sen", ",", "die", "A\u00b7e\u00b7u\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "$,", "ART", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Zum Himmel, zur Erde gekehrt.", "tokens": ["Zum", "Him\u00b7mel", ",", "zur", "Er\u00b7de", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Ein andres T\u00e4ublein fliegt auch auf", "tokens": ["Ein", "and\u00b7res", "T\u00e4ub\u00b7lein", "fliegt", "auch", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit m\u00fcden Fl\u00fcgelein:", "tokens": ["Mit", "m\u00fc\u00b7den", "Fl\u00fc\u00b7ge\u00b7lein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbach weh, wir kommen nicht hinauf,", "tokens": ["\u00bb", "ach", "weh", ",", "wir", "kom\u00b7men", "nicht", "hin\u00b7auf", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was sitzen wir auf dem Stein?", "tokens": ["Was", "sit\u00b7zen", "wir", "auf", "dem", "Stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Was sitzen wir auf dem \u00f6den Stein?", "tokens": ["Was", "sit\u00b7zen", "wir", "auf", "dem", "\u00f6\u00b7den", "Stein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da unten war's lustig und gr\u00fcn.\u00ab \u2013", "tokens": ["Da", "un\u00b7ten", "wa\u00b7r's", "lus\u00b7tig", "und", "gr\u00fcn", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ADJD", "KON", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u00bbdort oben mu\u00df es sch\u00f6ner sein:", "tokens": ["\u00bb", "dort", "o\u00b7ben", "mu\u00df", "es", "sch\u00f6\u00b7ner", "sein", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df uns von hinnen ziehn!", "tokens": ["La\u00df", "uns", "von", "hin\u00b7nen", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Doch hast du ein st\u00e4ubig Federlein", "tokens": ["Doch", "hast", "du", "ein", "st\u00e4u\u00b7big", "Fe\u00b7der\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "ADJD", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In deinem Fl\u00fcgelpaar;", "tokens": ["In", "dei\u00b7nem", "Fl\u00fc\u00b7gel\u00b7paar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Komm her, ich rupf' es dir sanft und fein,", "tokens": ["Komm", "her", ",", "ich", "rupf'", "es", "dir", "sanft", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann bist du silberklar.\u00ab", "tokens": ["Dann", "bist", "du", "sil\u00b7ber\u00b7klar", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbauch du hast wohl ein Federlein,", "tokens": ["\u00bb", "auch", "du", "hast", "wohl", "ein", "Fe\u00b7der\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist nicht silberklar;", "tokens": ["Das", "ist", "nicht", "sil\u00b7ber\u00b7klar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich rupf' dir's aus mit dem Schn\u00e4belein,", "tokens": ["Ich", "rupf'", "dir's", "aus", "mit", "dem", "Schn\u00e4\u00b7be\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann bist du gl\u00e4nzend gar.\u00ab", "tokens": ["Dann", "bist", "du", "gl\u00e4n\u00b7zend", "gar", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie putzen sich die Fl\u00fcgelein", "tokens": ["Sie", "put\u00b7zen", "sich", "die", "Fl\u00fc\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fliegen hoch hinauf,", "tokens": ["Und", "flie\u00b7gen", "hoch", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann nehmen sie die Engelein", "tokens": ["Dann", "neh\u00b7men", "sie", "die", "En\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihre Schaaren auf.", "tokens": ["In", "ih\u00b7re", "Schaa\u00b7ren", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ein T\u00e4ublein will von der Erde fliehn,", "tokens": ["Ein", "T\u00e4ub\u00b7lein", "will", "von", "der", "Er\u00b7de", "fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fliegt auf zum Himmelsblau:", "tokens": ["Fliegt", "auf", "zum", "Him\u00b7mels\u00b7blau", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbade, ihr W\u00e4lder und Felder gr\u00fcn!", "tokens": ["\u00bb", "a\u00b