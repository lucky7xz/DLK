{"textgrid.poem.37765": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist's, das wir in Ahnung f\u00fchlen", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist's, das wir in Ahnung f\u00fchlen", "tokens": ["Was", "ist's", ",", "das", "wir", "in", "Ah\u00b7nung", "f\u00fch\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was erh\u00f6het jede Stirn?", "tokens": ["Und", "was", "er\u00b7h\u00f6\u00b7het", "je\u00b7de", "Stirn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Herzen dunkle Wurzeln w\u00fchlen,", "tokens": ["Im", "Her\u00b7zen", "dunk\u00b7le", "Wur\u00b7zeln", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Knospen brechen auf im Hirn;", "tokens": ["Die", "Knos\u00b7pen", "bre\u00b7chen", "auf", "im", "Hirn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ist in dieser Nacht geschehen,", "tokens": ["Was", "ist", "in", "die\u00b7ser", "Nacht", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das uns so freudig will umwehen?", "tokens": ["Das", "uns", "so", "freu\u00b7dig", "will", "um\u00b7we\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ob wir in s\u00fc\u00dfer Liebe wachten", "tokens": ["Ob", "wir", "in", "s\u00fc\u00b7\u00dfer", "Lie\u00b7be", "wach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor manchem Jahr um diese Zeit?", "tokens": ["Vor", "man\u00b7chem", "Jahr", "um", "die\u00b7se", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "War heut ein Jahres-Tag der Schlachten,", "tokens": ["War", "heut", "ein", "Jah\u00b7res\u00b7Tag", "der", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die unser Vaterland befreit?", "tokens": ["Die", "un\u00b7ser", "Va\u00b7ter\u00b7land", "be\u00b7freit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch der Kalender in dem Herzen", "tokens": ["Doch", "der", "Ka\u00b7len\u00b7der", "in", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wei\u00df nichts von Sieg und s\u00fc\u00dfen Scherzen.", "tokens": ["Wei\u00df", "nichts", "von", "Sieg", "und", "s\u00fc\u00b7\u00dfen", "Scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr Sterne, nennet mir dies Zeichen,", "tokens": ["Ihr", "Ster\u00b7ne", ",", "nen\u00b7net", "mir", "dies", "Zei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PDS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das heute \u00fcber uns regiert?", "tokens": ["Das", "heu\u00b7te", "\u00fc\u00b7ber", "uns", "re\u00b7giert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah: ihr Alle m\u00fcsset weichen,", "tokens": ["Ich", "sah", ":", "ihr", "Al\u00b7le", "m\u00fcs\u00b7set", "wei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun es den Himmelsrand ber\u00fchrt;", "tokens": ["Nun", "es", "den", "Him\u00b7mels\u00b7rand", "be\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Vollmonds bl\u00fchend rothe Wangen", "tokens": ["Des", "Voll\u00b7monds", "bl\u00fc\u00b7hend", "ro\u00b7the", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind uns zum Vorbild aufgegangen.", "tokens": ["Sind", "uns", "zum", "Vor\u00b7bild", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Weil heut der Vollmond uns bescheinet,", "tokens": ["Weil", "heut", "der", "Voll\u00b7mond", "uns", "be\u00b7schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So schenken wir die Gl\u00e4ser voll,", "tokens": ["So", "schen\u00b7ken", "wir", "die", "Gl\u00e4\u00b7ser", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir wissen, was der Himmel meinet,", "tokens": ["Wir", "wis\u00b7sen", ",", "was", "der", "Him\u00b7mel", "mei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Warum er heut uns scheinen soll:", "tokens": ["Wa\u00b7rum", "er", "heut", "uns", "schei\u00b7nen", "soll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sollen sehn, wie er sich f\u00fcllte,", "tokens": ["Wir", "sol\u00b7len", "sehn", ",", "wie", "er", "sich", "f\u00fcll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seit er den Durst im Thaue stillte.", "tokens": ["Seit", "er", "den", "Durst", "im", "Thau\u00b7e", "still\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus vollen Flaschen werden Neigen", "tokens": ["Aus", "vol\u00b7len", "Fla\u00b7schen", "wer\u00b7den", "Nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und leere Menschen werden voll,", "tokens": ["Und", "lee\u00b7re", "Men\u00b7schen", "wer\u00b7den", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es h\u00e4ngt der Himmel voller Geigen:", "tokens": ["Es", "h\u00e4ngt", "der", "Him\u00b7mel", "vol\u00b7ler", "Gei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil heut ein Jeder tanzen soll;", "tokens": ["Weil", "heut", "ein", "Je\u00b7der", "tan\u00b7zen", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Erde dreht sich schon im Kreise,", "tokens": ["Die", "Er\u00b7de", "dreht", "sich", "schon", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Pfropfen springen nach der Weise.", "tokens": ["Die", "Pfrop\u00b7fen", "sprin\u00b7gen", "nach", "der", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Auf Pfropfen steigen wir zum Monde", "tokens": ["Auf", "Pfrop\u00b7fen", "stei\u00b7gen", "wir", "zum", "Mon\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der allen Wein der Erde reift,", "tokens": ["Der", "al\u00b7len", "Wein", "der", "Er\u00b7de", "reift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und machen gern mit ihm die Ronde,", "tokens": ["Und", "ma\u00b7chen", "gern", "mit", "ihm", "die", "Ron\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn queer er durch den Himmel schweift.", "tokens": ["Wenn", "que\u00b7er", "er", "durch", "den", "Him\u00b7mel", "schweift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Heut ist im Mond die gro\u00dfe Fa\u00dfnacht,", "tokens": ["Heut", "ist", "im", "Mond", "die", "gro\u00b7\u00dfe", "Fa\u00df\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und alles Wein da, was hier na\u00df macht.", "tokens": ["Und", "al\u00b7les", "Wein", "da", ",", "was", "hier", "na\u00df", "macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Die gro\u00dfe Noth in den Finanzen", "tokens": ["Die", "gro\u00b7\u00dfe", "Noth", "in", "den", "Fi\u00b7nan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und der Verfassung Schwierigkeit,", "tokens": ["Und", "der", "Ver\u00b7fas\u00b7sung", "Schwie\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00f6st sich, nun wir die Welt im Ganzen", "tokens": ["L\u00f6st", "sich", ",", "nun", "wir", "die", "Welt", "im", "Gan\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "ADV", "PPER", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beschaun, als eine Kleinigkeit;", "tokens": ["Be\u00b7schaun", ",", "als", "ei\u00b7ne", "Klei\u00b7nig\u00b7keit", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kommt Zeit, kommt Rath! im Wein ist Wahrheit", "tokens": ["Kommt", "Zeit", ",", "kommt", "Rath", "!", "im", "Wein", "ist", "Wahr\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "$,", "VVFIN", "NN", "$.", "APPRART", "NN", "VAFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und wer gespart, der zahlet baar heut.", "tokens": ["Und", "wer", "ge\u00b7spart", ",", "der", "zah\u00b7let", "baar", "heut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVPP", "$,", "PRELS", "VVFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein Gl\u00fcck, da\u00df ich kein Gott geworden,", "tokens": ["Ein", "Gl\u00fcck", ",", "da\u00df", "ich", "kein", "Gott", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PIAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn ich vertr\u00e4nk' mein Bischen Welt,", "tokens": ["Denn", "ich", "ver\u00b7tr\u00e4nk'", "mein", "Bi\u00b7schen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den diamantnen Sternen-Orden", "tokens": ["Den", "di\u00b7a\u00b7mant\u00b7nen", "Ster\u00b7nen\u00b7Or\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch das blaue Himmelszelt,", "tokens": ["Und", "auch", "das", "blau\u00b7e", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dies Zelt, das mir so wohlgefallen,", "tokens": ["Dies", "Zelt", ",", "das", "mir", "so", "wohl\u00b7ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Seit unsre Stimmen drin erschallen.", "tokens": ["Seit", "uns\u00b7re", "Stim\u00b7men", "drin", "er\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ja, morgen w\u00fcrd' ich's recht bereuen:", "tokens": ["Ja", ",", "mor\u00b7gen", "w\u00fcrd'", "ich's", "recht", "be\u00b7reu\u00b7en", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn \u00fcber uns der Himmel leer;", "tokens": ["Wenn", "\u00fc\u00b7ber", "uns", "der", "Him\u00b7mel", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich w\u00fcrd' ein neues Zelt mir leihen,", "tokens": ["Ich", "w\u00fcrd'", "ein", "neu\u00b7es", "Zelt", "mir", "lei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn es bei dem Teufel w\u00e4r;", "tokens": ["Und", "wenn", "es", "bei", "dem", "Teu\u00b7fel", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja Freunde, la\u00dft uns das bedenken,", "tokens": ["Ja", "Freun\u00b7de", ",", "la\u00dft", "uns", "das", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$,", "VVIMP", "PPER", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Eh wir vom Glauben was verschenken.", "tokens": ["Eh", "wir", "vom", "Glau\u00b7ben", "was", "ver\u00b7schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Am Himmel ist nichts \u00fcberfl\u00fc\u00dfig,", "tokens": ["Am", "Him\u00b7mel", "ist", "nichts", "\u00fc\u00b7berf\u00b7l\u00fc\u00b7\u00dfig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und auf der Erde nichts zu viel,", "tokens": ["Und", "auf", "der", "Er\u00b7de", "nichts", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PIS", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn wir ihrer \u00fcberdr\u00fc\u00dfig", "tokens": ["Und", "wenn", "wir", "ih\u00b7rer", "\u00fc\u00b7berd\u00b7r\u00fc\u00b7\u00dfig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wenn der Himmel uns zu k\u00fchl,", "tokens": ["Und", "wenn", "der", "Him\u00b7mel", "uns", "zu", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Steigt s\u00fc\u00dfer Schlaf aus edlem Weine", "tokens": ["Steigt", "s\u00fc\u00b7\u00dfer", "Schlaf", "aus", "ed\u00b7lem", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und h\u00fcllet in Tr\u00e4umen die Gemeine.", "tokens": ["Und", "h\u00fcl\u00b7let", "in", "Tr\u00e4u\u00b7men", "die", "Ge\u00b7mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Was ist's, das wir in Ahnung f\u00fchlen", "tokens": ["Was", "ist's", ",", "das", "wir", "in", "Ah\u00b7nung", "f\u00fch\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und was erh\u00f6het jede Stirn?", "tokens": ["Und", "was", "er\u00b7h\u00f6\u00b7het", "je\u00b7de", "Stirn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Herzen dunkle Wurzeln w\u00fchlen,", "tokens": ["Im", "Her\u00b7zen", "dunk\u00b7le", "Wur\u00b7zeln", "w\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Knospen brechen auf im Hirn;", "tokens": ["Die", "Knos\u00b7pen", "bre\u00b7chen", "auf", "im", "Hirn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ist in dieser Nacht geschehen,", "tokens": ["Was", "ist", "in", "die\u00b7ser", "Nacht", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das uns so freudig will umwehen?", "tokens": ["Das", "uns", "so", "freu\u00b7dig", "will", "um\u00b7we\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ob wir in s\u00fc\u00dfer Liebe wachten", "tokens": ["Ob", "wir", "in", "s\u00fc\u00b7\u00dfer", "Lie\u00b7be", "wach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor manchem Jahr um diese Zeit?", "tokens": ["Vor", "man\u00b7chem", "Jahr", "um", "die\u00b7se", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "War heut ein Jahres-Tag der Schlachten,", "tokens": ["War", "heut", "ein", "Jah\u00b7res\u00b7Tag", "der", "Schlach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die unser Vaterland befreit?", "tokens": ["Die", "un\u00b7ser", "Va\u00b7ter\u00b7land", "be\u00b7freit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch der Kalender in dem Herzen", "tokens": ["Doch", "der", "Ka\u00b7len\u00b7der", "in", "dem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wei\u00df nichts von Sieg