exa"}, "line.4": {"text": "Und was von deinem thun der Pleissen-strand erfahren.", "tokens": ["Und", "was", "von", "dei\u00b7nem", "thun", "der", "Pleis\u00b7sen\u00b7strand", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur dieses wei\u00df man nicht, warum dein kluger geist", "tokens": ["Nur", "die\u00b7ses", "wei\u00df", "man", "nicht", ",", "wa\u00b7rum", "dein", "klu\u00b7ger", "geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VVFIN", "PIS", "PTKNEG", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht l\u00e4ngsten mit der post dem doctor nachgereist,", "tokens": ["Nicht", "l\u00e4ngs\u00b7ten", "mit", "der", "post", "dem", "doc\u00b7tor", "nach\u00b7ge\u00b7reist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und dir ein weg beliebt, den Taps und Mops vergessen,", "tokens": ["Und", "dir", "ein", "weg", "be\u00b7liebt", ",", "den", "Taps", "und", "Mops", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADV", "ADJD", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die die gelehrsamkeit nach einem brod-korb messen.", "tokens": ["Die", "die", "ge\u00b7lehr\u00b7sam\u00b7keit", "nach", "ei\u00b7nem", "bro\u00b7dkorb", "mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Jedoch, du woltest nicht ein halber doctor seyn,", "tokens": ["Je\u00b7doch", ",", "du", "wol\u00b7test", "nicht", "ein", "hal\u00b7ber", "doc\u00b7tor", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "PTKNEG", "ART", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die gelahrheit nur bey einem zipffel fassen;", "tokens": ["Und", "die", "ge\u00b7lahr\u00b7heit", "nur", "bey", "ei\u00b7nem", "zipf\u00b7fel", "fas\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die wei\u00dfheit gab dir selbst die klugen lehren ein,", "tokens": ["Die", "wei\u00df\u00b7heit", "gab", "dir", "selbst", "die", "klu\u00b7gen", "leh\u00b7ren", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du soltest dir ihr reich durchaus entlarven lassen.", "tokens": ["Du", "sol\u00b7test", "dir", "ihr", "reich", "durc\u00b7haus", "ent\u00b7lar\u00b7ven", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "ADJD", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum hat vernunfft und kunst dir gar genau entdeckt,", "tok