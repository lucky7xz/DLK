{"dta.poem.11386": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als tit. Herr S. K. Heydenreich zu Jena  \n  Licentiatus medicin\u00e6  wurde, im nahmen  \n der Schlesischen lands-  \n mannschafft.  \n E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr priester der natur, und engel dieser welt!", "tokens": ["Ihr", "pries\u00b7ter", "der", "na\u00b7tur", ",", "und", "en\u00b7gel", "die\u00b7ser", "welt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,", "KON", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihr der seelen schlo\u00df mit fester hand bewachet!", "tokens": ["Die", "ihr", "der", "see\u00b7len", "schlo\u00df", "mit", "fes\u00b7ter", "hand", "be\u00b7wa\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verzeiht, wenn unser blick etwas verwegen f\u00e4llt,", "tokens": ["Ver\u00b7zeiht", ",", "wenn", "un\u00b7ser", "blick", "et\u00b7was", "ver\u00b7we\u00b7gen", "f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sich ins heiligthum der Hygiene machet!", "tokens": ["Und", "sich", "ins", "hei\u00b7lig\u00b7thum", "der", "Hy\u00b7gi\u00b7e\u00b7ne", "ma\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der tag, da ihre huld sich einen diener weiht,", "tokens": ["Der", "tag", ",", "da", "ih\u00b7re", "huld", "sich", "ei\u00b7nen", "die\u00b7ner", "weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Macht unsern w\u00fcnschen ja darzu gelegenheit,", "tokens": ["Macht", "un\u00b7sern", "w\u00fcn\u00b7schen", "ja", "dar\u00b7zu", "ge\u00b7le\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "VVFIN", "ADV", "PAV", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und will den vorhang stets von ihren augen ziehen:", "tokens": ["Und", "will", "den", "vor\u00b7hang", "stets", "von", "ih\u00b7ren", "au\u00b7gen", "zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wer wolte diesen glantz mit eulen-augen fliehen?", "tokens": ["Wer", "wol\u00b7te", "die\u00b7sen", "glantz", "mit", "eu\u00b7len\u00b7au\u00b7gen", "flie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDAT", "NN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In kl\u00fcffte bauet man nicht tempel und altar:", "tokens": ["In", "kl\u00fcff\u00b7te", "bau\u00b7et", "man", "nicht", "tem\u00b7pel", "und", "al\u00b7tar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "PIS", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was n\u00fctzt ein k\u00f6nigs-schmuck bey d\u00fcstren todten-beinen?", "tokens": ["Was", "n\u00fctzt", "ein", "k\u00f6\u00b7nigs\u00b7schmuck", "bey", "d\u00fcst\u00b7ren", "tod\u00b7ten\u00b7bei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sonne stellet sich der menschen augen dar,", "tokens": ["Die", "son\u00b7ne", "stel\u00b7let", "sich", "der", "men\u00b7schen", "au\u00b7gen", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und lichter d\u00fcrffen nicht in finstern kerckern scheinen.", "tokens": ["Und", "lich\u00b7ter", "d\u00fcrf\u00b7fen", "nicht", "in", "fins\u00b7tern", "ker\u00b7ckern", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr seyd genung begl\u00fcckt, da\u00df sich das reich der welt", "tokens": ["Ihr", "seyd", "ge\u00b7nung", "be\u00b7gl\u00fcckt", ",", "da\u00df", "sich", "das", "reich", "der", "welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "PRF", "ART", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit seiner sch\u00e4tze pracht zu euren diensten stellt:", "tokens": ["Mit", "sei\u00b7ner", "sch\u00e4t\u00b7ze", "pracht", "zu", "eu\u00b7ren", "diens\u00b7ten", "stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da Hygienens macht in wasser, lufft und gr\u00fcnden,", "tokens": ["Da", "Hy\u00b7gi\u00b7e\u00b7nens", "macht", "in", "was\u00b7ser", ",", "lufft", "und", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "APPR", "ADJA", "$,", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja in dem himmel selbst kan rare sch\u00e4tze finden.", "tokens": ["Ja", "in", "dem", "him\u00b7mel", "selbst", "kan", "ra\u00b7re", "sch\u00e4t\u00b7ze", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "ADV", "VMFIN", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn der beliebte lentz die wiesen tr\u00e4chtig macht,", "tokens": ["Wenn", "der", "be\u00b7lieb\u00b7te", "lentz", "die", "wie\u00b7sen", "tr\u00e4ch\u00b7tig", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und unsre g\u00e4rten sich mit tausend farben mahlen;", "tokens": ["Und", "uns\u00b7re", "g\u00e4r\u00b7ten", "sich", "mit", "tau\u00b7send", "far\u00b7ben", "mah\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PRF", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df der anmuths-platz, wo Florens purpur lacht,", "tokens": ["So", "mu\u00df", "der", "an\u00b7muths\u00b7platz", ",", "wo", "Flo\u00b7rens", "pur\u00b7pur", "lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "PWAV", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Euch die gesch\u00f6pffte lust mit theurer frucht bezahlen", "tokens": ["Euch", "die", "ge\u00b7sch\u00f6pff\u00b7te", "lust", "mit", "theu\u00b7rer", "frucht", "be\u00b7zah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sehn andre \u00e4usserlich nur schatten, laub und schein;", "tokens": ["Sehn", "and\u00b7re", "\u00e4us\u00b7ser\u00b7lich", "nur", "schat\u00b7ten", ",", "laub", "und", "schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "ADV", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So steiget euer geist ins wasser selbst hinein,", "tokens": ["So", "stei\u00b7get", "eu\u00b7er", "geist", "ins", "was\u00b7ser", "selbst", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und l\u00e4st der augen licht durch alle adern dringen,", "tokens": ["Und", "l\u00e4st", "der", "au\u00b7gen", "licht", "durch", "al\u00b7le", "a\u00b7dern", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die tieff verborgne krafft daraus hervor zu bringen.", "tokens": ["Die", "tieff", "ver\u00b7borg\u00b7ne", "krafft", "da\u00b7raus", "her\u00b7vor", "zu", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PAV", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Kriecht andre, wie ihr wolt, in tieffer kl\u00fcfften schacht!", "tokens": ["Kriecht", "and\u00b7re", ",", "wie", "ihr", "wolt", ",", "in", "tief\u00b7fer", "kl\u00fcff\u00b7ten", "schacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "$,", "PWAV", "PPER", "VMFIN", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchsucht der erden marck: Verfolgt das schnelle gl\u00fccke", "tokens": ["Durch\u00b7sucht", "der", "er\u00b7den", "marck", ":", "Ver\u00b7folgt", "das", "schnel\u00b7le", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch wellen, fluth und see: Holt fremder l\u00e4nder pracht!", "tokens": ["Durch", "wel\u00b7len", ",", "fluth", "und", "see", ":", "Holt", "frem\u00b7der", "l\u00e4n\u00b7der", "pracht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "NN", "KON", "VVFIN", "$.", "NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr bringt doch alle nichts, als fremden koth zur\u00fccke.", "tokens": ["Ihr", "bringt", "doch", "al\u00b7le", "nichts", ",", "als", "frem\u00b7den", "koth", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "PIS", "$,", "KOUS", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wem Hygienens hand den weg nach Colchis weist,", "tokens": ["Wem", "Hy\u00b7gi\u00b7e\u00b7nens", "hand", "den", "weg", "nach", "Col\u00b7chis", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "ART", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der hat weit n\u00fctzlicher, als andrer fu\u00df, gereist:", "tokens": ["Der", "hat", "weit", "n\u00fctz\u00b7li\u00b7cher", ",", "als", "an\u00b7drer", "fu\u00df", ",", "ge\u00b7reist", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADJD", "$,", "KOUS", "PIS", "PTKVZ", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der bringt, wann eure sch\u00e4tz euch tausend sorgen geben,", "tokens": ["Der", "bringt", ",", "wann", "eu\u00b7re", "sch\u00e4tz", "euch", "tau\u00b7send", "sor\u00b7gen", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "PPER", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch sein erworbnes flie\u00df viel sterblichen das leben.", "tokens": ["Durch", "sein", "er\u00b7worb\u00b7nes", "flie\u00df", "viel", "sterb\u00b7li\u00b7chen", "das", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVFIN", "ADV", "VVFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Jhm mu\u00df das gantze heer der welt zu dienste stehn:", "tokens": ["Jhm", "mu\u00df", "das", "gant\u00b7ze", "heer", "der", "welt", "zu", "diens\u00b7te", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "NN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die v\u00f6gel m\u00fcssen hier die fl\u00fcgel niederlegen:", "tokens": ["Die", "v\u00f6\u00b7gel", "m\u00fcs\u00b7sen", "hier", "die", "fl\u00fc\u00b7gel", "nie\u00b7der\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ungez\u00e4hmte wild mu\u00df in gehorsam gehn,", "tokens": ["Das", "un\u00b7ge\u00b7z\u00e4hm\u00b7te", "wild", "mu\u00df", "in", "ge\u00b7hor\u00b7sam", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und seinen krancken-tisch mit medicin verpflegen:", "tokens": ["Und", "sei\u00b7nen", "kran\u00b7cken\u00b7tisch", "mit", "me\u00b7di\u00b7cin", "ver\u00b7pfle\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die wilde fluth der see wirfft perlen-muschel aus:", "tokens": ["Die", "wil\u00b7de", "fluth", "der", "see", "wirfft", "per\u00b7len\u00b7mu\u00b7schel", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die schlang und kr\u00f6te bringt selbst segen in sein haus;", "tokens": ["Die", "schlang", "und", "kr\u00f6\u00b7te", "bringt", "selbst", "se\u00b7gen", "in", "sein", "haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "NN", "VVFIN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn Hygienen mu\u00df der \u00e4rgste schaum auf erden", "tokens": ["Denn", "Hy\u00b7gi\u00b7e\u00b7nen", "mu\u00df", "der", "\u00e4rgs\u00b7te", "schaum", "auf", "er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zu theurer gold-tinctur, und gifft zu balsam werden.", "tokens": ["Zu", "theu\u00b7rer", "gold\u00b7tinc\u00b7tur", ",", "und", "gifft", "zu", "bal\u00b7sam", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "NN", "$,", "KON", "VVFIN", "PTKA", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und so beherrscht ihr witz den bau der grossen welt!", "tokens": ["Und", "so", "be\u00b7herrscht", "ihr", "witz", "den", "bau", "der", "gros\u00b7sen", "welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der mensch, die kleine, w\u00fcnscht sich selber ihr regioren,", "tokens": ["Der", "mensch", ",", "die", "klei\u00b7ne", ",", "w\u00fcnscht", "sich", "sel\u00b7ber", "ihr", "re\u00b7gi\u00b7o\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "PRF", "ADV", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "So bald der m\u00fcde puls aus seiner ordnung f\u00e4llt,", "tokens": ["So", "bald", "der", "m\u00fc\u00b7de", "puls", "aus", "sei\u00b7ner", "ord\u00b7nung", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schmertz und sorgen uns ins krancken-bette f\u00fchren.", "tokens": ["Und", "schmertz", "und", "sor\u00b7gen", "uns", "ins", "kran\u00b7cken\u00b7bet\u00b7te", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die diener, so sie schickt, sind eine engel-post:", "tokens": ["Die", "die\u00b7ner", ",", "so", "sie", "schickt", ",", "sind", "ei\u00b7ne", "en\u00b7gel\u