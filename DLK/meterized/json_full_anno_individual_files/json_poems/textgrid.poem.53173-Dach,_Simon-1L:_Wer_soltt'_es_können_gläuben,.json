{"textgrid.poem.53173": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer soltt' es k\u00f6nnen gl\u00e4uben,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer soltt' es k\u00f6nnen gl\u00e4uben,", "tokens": ["Wer", "soltt'", "es", "k\u00f6n\u00b7nen", "gl\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df alle Fertigkeit", "tokens": ["Da\u00df", "al\u00b7le", "Fer\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im singen oder schreiben", "tokens": ["Im", "sin\u00b7gen", "o\u00b7der", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir abliegt manche Zeit?", "tokens": ["Mir", "ab\u00b7liegt", "man\u00b7che", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Geist geht wie in Ketten,", "tokens": ["Mein", "Geist", "geht", "wie", "in", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd w\u00fcst' ein guter Reim", "tokens": ["Vnd", "w\u00fcst'", "ein", "gu\u00b7ter", "Reim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Leben mir zu retten,", "tokens": ["Das", "Le\u00b7ben", "mir", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So ist er nicht daheim.", "tokens": ["So", "ist", "er", "nicht", "da\u00b7heim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Seht, jetzund z\u00fcrnt jhr wieder", "tokens": ["Seht", ",", "je\u00b7tzund", "z\u00fcrnt", "jhr", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus blossem Eigen-wahn,", "tokens": ["Aus", "blos\u00b7sem", "Ei\u00b7gen\u00b7wahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich fleh euch, meine Lieder,", "tokens": ["Ich", "fleh", "euch", ",", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr kehrt euch nicht daran.", "tokens": ["Ihr", "kehrt", "euch", "nicht", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Adler gleicht im fliegen", "tokens": ["Kein", "Ad\u00b7ler", "gleicht", "im", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bisweilen ewrer Fahrt,", "tokens": ["Bis\u00b7wei\u00b7len", "ew\u00b7rer", "Fahrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bisweilen bleibt jhr liegen", "tokens": ["Bis\u00b7wei\u00b7len", "bleibt", "jhr", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd habet Schnecken-Art.", "tokens": ["Vnd", "ha\u00b7bet", "Schne\u00b7cken\u00b7Art", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich mus aus Scham err\u00f6hten,", "tokens": ["Ich", "mus", "aus", "Scham", "er\u00b7r\u00f6h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun jhr mir wollt entstehn,", "tokens": ["Nun", "jhr", "mir", "wollt", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja th\u00e4t es nicht von n\u00f6hten,", "tokens": ["Ja", "th\u00e4t", "es", "nicht", "von", "n\u00f6h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich liess' euch immer gehn,", "tokens": ["Ich", "liess'", "euch", "im\u00b7mer", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der ich mich dem befinde", "tokens": ["Der", "ich", "mich", "dem", "be\u00b7fin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verstrickt als keinem mehr,", "tokens": ["Ver\u00b7strickt", "als", "kei\u00b7nem", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "PIAT", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Macht Hochzeit seinem Kinde", "tokens": ["Macht", "Hoch\u00b7zeit", "sei\u00b7nem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der thewre Herr Wolder.", "tokens": ["Der", "thew\u00b7re", "Herr", "Wol\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie reichlich ich vor Jahren", "tokens": ["Wie", "reich\u00b7lich", "ich", "vor", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Genossen seiner Hand,", "tokens": ["Ge\u00b7nos\u00b7sen", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat, wie jhr wisst, erfahren", "tokens": ["Hat", ",", "wie", "jhr", "wisst", ",", "er\u00b7fah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Elb- vnd Pregel-Strand,", "tokens": ["Der", "Elb", "vnd", "Pre\u00b7gel\u00b7Strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er ist die Seelen-Pflege", "tokens": ["Er", "ist", "die", "See\u00b7len\u00b7Pfle\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der gantzen Alten Stad,", "tokens": ["Der", "gant\u00b7zen", "Al\u00b7ten", "Stad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo Gott sein Wort-Gehege", "tokens": ["Wo", "Gott", "sein", "Wor\u00b7tGe\u00b7he\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Vnd lieben Schaffstall hat.", "tokens": ["Vnd", "lie\u00b7ben", "Schaff\u00b7stall", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er treiffelt Milch vnd Oele.", "tokens": ["Er", "treif\u00b7felt", "Milch", "vnd", "O\u00b7e\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wenn er die Stimm erhebt,", "tokens": ["Wenn", "er", "die", "Stimm", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "St\u00e4rckt sich die matte Seele,", "tokens": ["St\u00e4rckt", "sich", "die", "mat\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Reich der hellen bebt,", "tokens": ["Das", "Reich", "der", "hel\u00b7len", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Tod mus sich verstecken,", "tokens": ["Der", "Tod", "mus", "sich", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der dreygek\u00f6pffte Hund", "tokens": ["Der", "drey\u00b7ge\u00b7k\u00f6pff\u00b7te", "Hund"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Begiebet sich aus Schrecken", "tokens": ["Be\u00b7gie\u00b7bet", "sich", "aus", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis auff Cocytus Grund.", "tokens": ["Bis", "auff", "Co\u00b7cy\u00b7tus", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Des Br\u00e4utgams zu gedencken,", "tokens": ["Des", "Br\u00e4ut\u00b7gams", "zu", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mus jhm der Jugend fleis", "tokens": ["Mus", "jhm", "der", "Ju\u00b7gend", "fleis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Nicht Rhum vnd Ehre schencken?", "tokens": ["Nicht", "Rhum", "vnd", "Eh\u00b7re", "schen\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er ist der Aertzte Prei\u00df.", "tokens": ["Er", "ist", "der", "A\u00b7ertz\u00b7te", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "W\u00e4r uns sein frommes Leben", "tokens": ["W\u00e4r", "uns", "sein", "from\u00b7mes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vorhin nicht gnug bekant,", "tokens": ["Vor\u00b7hin", "nicht", "gnug", "be\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es k\u00f6ntte dessen geben", "tokens": ["Es", "k\u00f6nt\u00b7te", "des\u00b7sen", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Zeugnis Niederland.", "tokens": ["Ein", "Zeug\u00b7nis", "Nie\u00b7der\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Bey solchen sch\u00f6nen Sachen", "tokens": ["Bey", "sol\u00b7chen", "sch\u00f6\u00b7nen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entsaget jhr mir gar,", "tokens": ["Ent\u00b7sa\u00b7get", "jhr", "mir", "gar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wollt kein Braut-Lied machen,", "tokens": ["Vnd", "wollt", "kein", "Braut\u00b7Lied", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ey, habt ein gutes Jahr!", "tokens": ["Ey", ",", "habt", "ein", "gu\u00b7tes", "Jahr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was ist an euch gelegen?", "tokens": ["Was", "ist", "an", "euch", "ge\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Hochzeit Licht vnd Schein", "tokens": ["Der", "Hoch\u00b7zeit", "Licht", "vnd", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bringt wol in hundert wegen,", "tokens": ["Bringt", "wol", "in", "hun\u00b7dert", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "CARD", "APPR", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df jhr gebrechet, ein.", "tokens": ["Da\u00df", "jhr", "ge\u00b7bre\u00b7chet", ",", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es hat sich herbegeben", "tokens": ["Es", "hat", "sich", "her\u00b7be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Raht vnd das Gericht,", "tokens": ["Der", "Raht", "vnd", "das", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00fcrgerschafft daneben,", "tokens": ["Die", "B\u00fcr\u00b7ger\u00b7schafft", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd keine Zunfft gebricht,", "tokens": ["Vnd", "kei\u00b7ne", "Zunfft", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was irgends weis zu dienen,", "tokens": ["Was", "ir\u00b7gends", "weis", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lahck, Stein-Tamm allerseit,", "tokens": ["Lahck", ",", "Stein\u00b7Tamm", "al\u00b7ler\u00b7seit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die H\u00fcbner sind erschienen", "tokens": ["Die", "H\u00fcb\u00b7ner", "sind", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bey dieser Fr\u00f6ligkeit.", "tokens": ["Bey", "die\u00b7ser", "Fr\u00f6\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die Frewde junger Leute,", "tokens": ["Die", "Frew\u00b7de", "jun\u00b7ger", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Tantz wird auch geh\u00f6rt,", "tokens": ["Der", "Tantz", "wird", "auch", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der durch so manche Beute", "tokens": ["Der", "durch", "so", "man\u00b7che", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebe Reich vermehrt.", "tokens": ["Der", "Lie\u00b7be", "Reich", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Ceres schencket Leben", "tokens": ["Die", "Ce\u00b7res", "schen\u00b7cket", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd Anstand aller Pein,", "tokens": ["Vnd", "An\u00b7stand", "al\u00b7ler", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Safft der edlen Reben", "tokens": ["Der", "Safft", "der", "ed\u00b7len", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wil vnverf\u00e4lschet seyn.", "tokens": ["Wil", "vn\u00b7ver\u00b7f\u00e4l\u00b7schet", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wer soltt' es k\u00f6nnen gl\u00e4uben,", "tokens": ["Wer", "soltt'", "es", "k\u00f6n\u00b7nen", "gl\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df alle Fertigkeit", "tokens": ["Da\u00df", "al\u00b7le", "Fer\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im singen oder schreiben", "tokens": ["Im", "sin\u00b7gen", "o\u00b7der", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir abliegt manche Zeit?", "tokens": ["Mir", "ab\u00b7liegt", "man\u00b7che", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mein Geist geht wie in Ketten,", "tokens": ["Mein", "Geist", "geht", "wie", "in", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd w\u00fcst' ein guter Reim", "tokens": ["Vnd", "w\u00fcst'", "ein", "gu\u00b7ter", "Reim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Leben mir zu retten,", "tokens": ["Das", "Le\u00b7ben", "mir", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So ist er nicht daheim.", "tokens": ["So", "ist", "er", "nicht", "da\u00b7heim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Seht, jetzund z\u00fcrnt jhr wieder", "tokens": ["Seht", ",", "je\u00b7tzund", "z\u00fcrnt", "jhr", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus blossem Eigen-wahn,", "tokens": ["Aus", "blos\u00b7sem", "Ei\u00b7gen\u00b7wahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich fleh euch, meine Lieder,", "tokens": ["Ich", "fleh", "euch", ",", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr kehrt euch nicht daran.", "tokens": ["Ihr", "kehrt", "euch", "nicht", "da\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Adler gleicht im fliegen", "tokens": ["Kein", "Ad\u00b7ler", "gleicht", "im", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bisweilen ewrer Fahrt,", "tokens": ["Bis\u00b7wei\u00b7len", "ew\u00b7rer", "Fahrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bisweilen bleibt jhr liegen", "tokens": ["Bis\u00b7wei\u00b7len", "bleibt", "jhr", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd habet Schnecken-Art.", "tokens": ["Vnd", "ha\u00b7bet", "Schne\u00b7cken\u00b7Art", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich mus aus Scham err\u00f6hten,", "tokens": ["Ich", "mus", "aus", "Scham", "er\u00b7r\u00f6h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun jhr mir wollt entstehn,", "tokens": ["Nun", "jhr", "mir", "wollt", "ent\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja th\u00e4t es nicht von n\u00f6hten,", "tokens": ["Ja", "th\u00e4t", "es", "nicht", "von", "n\u00f6h\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich liess' euch immer gehn,", "tokens": ["Ich", "liess'", "euch", "im\u00b7mer", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der ich mich dem befinde", "tokens": ["Der", "ich", "mich", "dem", "be\u00b7fin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verstrickt als keinem mehr,", "tokens": ["Ver\u00b7strickt", "als", "kei\u00b7nem", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "PIAT", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Macht Hochzeit seinem Kinde", "tokens": ["Macht", "Hoch\u00b7zeit", "sei\u00b7nem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der thewre Herr Wolder.", "tokens": ["Der", "thew\u00b7re", "Herr", "Wol\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Wie reichlich ich vor Jahren", "tokens": ["Wie", "reich\u00b7lich", "ich", "vor", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Genossen seiner Hand,", "tokens": ["Ge\u00b7nos\u00b7sen", "sei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hat, wie jhr wisst, erfahren", "tokens": ["Hat", ",", "wie", "jhr", "wisst", ",", "er\u00b7fah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Elb- vnd Pregel-Strand,", "tokens": ["Der", "Elb", "vnd", "Pre\u00b7gel\u00b7Strand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er ist die Seelen-Pflege", "tokens": ["Er", "ist", "die", "See\u00b7len\u00b7Pfle\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der gantzen Alten Stad,", "tokens": ["Der", "gant\u00b7zen", "Al\u00b7ten", "Stad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo Gott sein Wort-Gehege", "tokens": ["Wo", "Gott", "sein", "Wor\u00b7tGe\u00b7he\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Vnd lieben Schaffstall hat.", "tokens": ["Vnd", "lie\u00b7ben", "Schaff\u00b7stall", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Er treiffelt Milch vnd Oele.", "tokens": ["Er", "treif\u00b7felt", "Milch", "vnd", "O\u00b7e\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wenn er die Stimm erhebt,", "tokens": ["Wenn", "er", "die", "Stimm", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "St\u00e4rckt sich die matte Seele,", "tokens": ["St\u00e4rckt", "sich", "die", "mat\u00b7te", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Reich der hellen bebt,", "tokens": ["Das", "Reich", "der", "hel\u00b7len", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Tod mus sich verstecken,", "tokens": ["Der", "Tod", "mus", "sich", "ver\u00b7ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der dreygek\u00f6pffte Hund", "tokens": ["Der", "drey\u00b7ge\u00b7k\u00f6pff\u00b7te", "Hund"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Begiebet sich aus Schrecken", "tokens": ["Be\u00b7gie\u00b7bet", "sich", "aus", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis auff Cocytus Grund.", "tokens": ["Bis", "auff", "Co\u00b7cy\u00b7tus", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Des Br\u00e4utgams zu gedencken,", "tokens": ["Des", "Br\u00e4ut\u00b7gams", "zu", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mus jhm der Jugend fleis", "tokens": ["Mus", "jhm", "der", "Ju\u00b7gend", "fleis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Nicht Rhum vnd Ehre schencken?", "tokens": ["Nicht", "Rhum", "vnd", "Eh\u00b7re", "schen\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er ist der Aertzte Prei\u00df.", "tokens": ["Er", "ist", "der", "A\u00b7ertz\u00b7te", "Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "W\u00e4r uns sein frommes Leben", "tokens": ["W\u00e4r", "uns", "sein", "from\u00b7mes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vorhin nicht gnug bekant,", "tokens": ["Vor\u00b7hin", "nicht", "gnug", "be\u00b7kant", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es k\u00f6ntte dessen geben", "tokens": ["Es", "k\u00f6nt\u00b7te", "des\u00b7sen", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Zeugnis Niederland.", "tokens": ["Ein", "Zeug\u00b7nis", "Nie\u00b7der\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Bey solchen sch\u00f6nen Sachen", "tokens": ["Bey", "sol\u00b7chen", "sch\u00f6\u00b7nen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entsaget jhr mir gar,", "tokens": ["Ent\u00b7sa\u00b7get", "jhr", "mir", "gar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wollt kein Braut-Lied machen,", "tokens": ["Vnd", "wollt", "kein", "Braut\u00b7Lied", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ey, habt ein gutes Jahr!", "tokens": ["Ey", ",", "habt", "ein", "gu\u00b7tes", "Jahr", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was ist an euch gelegen?", "tokens": ["Was", "ist", "an", "euch", "ge\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Hochzeit Licht vnd Schein", "tokens": ["Der", "Hoch\u00b7zeit", "Licht", "vnd", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Bringt wol in hundert wegen,", "tokens": ["Bringt", "wol", "in", "hun\u00b7dert", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "CARD", "APPR", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df jhr gebrechet, ein.", "tokens": ["Da\u00df", "jhr", "ge\u00b7bre\u00b7chet", ",", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Es hat sich herbegeben", "tokens": ["Es", "hat", "sich", "her\u00b7be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Raht vnd das Gericht,", "tokens": ["Der", "Raht", "vnd", "das", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die B\u00fcrgerschafft daneben,", "tokens": ["Die", "B\u00fcr\u00b7ger\u00b7schafft", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd keine Zunfft gebricht,", "tokens": ["Vnd", "kei\u00b7ne", "Zunfft", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was irgends weis zu dienen,", "tokens": ["Was", "ir\u00b7gends", "weis", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Lahck, Stein-Tamm allerseit,", "tokens": ["Lahck", ",", "Stein\u00b7Tamm", "al\u00b7ler\u00b7seit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die H\u00fcbner sind erschienen", "tokens": ["Die", "H\u00fcb\u00b7ner", "sind", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bey dieser Fr\u00f6ligkeit.", "tokens": ["Bey", "die\u00b7ser", "Fr\u00f6\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Die Frewde junger Leute,", "tokens": ["Die", "Frew\u00b7de", "jun\u00b7ger", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Tantz wird auch geh\u00f6rt,", "tokens": ["Der", "Tantz", "wird", "auch", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der durch so manche Beute", "tokens": ["Der", "durch", "so", "man\u00b7che", "Beu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebe Reich vermehrt.", "tokens": ["Der", "Lie\u00b7be", "Reich", "ver\u00b7mehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Ceres schencket Leben", "tokens": ["Die", "Ce\u00b7res", "schen\u00b7cket", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vnd Anstand aller Pein,", "tokens": ["Vnd", "An\u00b7stand", "al\u00b7ler", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Safft der edlen Reben", "tokens": ["Der", "Safft", "der", "ed\u00b7len", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wil vnverf\u00e4lschet seyn.", "tokens": ["Wil", "vn\u00b7ver\u00b7f\u00e4l\u00b7schet", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}