{"textgrid.poem.34966": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie langsam kriechet sie dahin,", "tokens": ["Wie", "lang\u00b7sam", "krie\u00b7chet", "sie", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zeit, die schauderhafte Schnecke!", "tokens": ["Die", "Zeit", ",", "die", "schau\u00b7der\u00b7haf\u00b7te", "Schne\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich aber, ganz bewegungslos", "tokens": ["Ich", "a\u00b7ber", ",", "ganz", "be\u00b7we\u00b7gungs\u00b7los"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Blieb ich hier auf demselben Flecke.", "tokens": ["Blieb", "ich", "hier", "auf", "dem\u00b7sel\u00b7ben", "Fle\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In meine dunkle Zelle dringt", "tokens": ["In", "mei\u00b7ne", "dunk\u00b7le", "Zel\u00b7le", "dringt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kein Sonnenstrahl, kein Hoffnungsschimmer,", "tokens": ["Kein", "Son\u00b7nen\u00b7strahl", ",", "kein", "Hoff\u00b7nungs\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df, nur mit der Kirchhofsgruft", "tokens": ["Ich", "wei\u00df", ",", "nur", "mit", "der", "Kirch\u00b7hofs\u00b7gruft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vertausch ich dies fatale Zimmer.", "tokens": ["Ver\u00b7tausch", "ich", "dies", "fa\u00b7ta\u00b7le", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Vielleicht bin ich gestorben l\u00e4ngst;", "tokens": ["Viel\u00b7leicht", "bin", "ich", "ge\u00b7stor\u00b7ben", "l\u00e4ngst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es sind vielleicht nur Spukgestalten", "tokens": ["Es", "sind", "viel\u00b7leicht", "nur", "Spuk\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Phantasien, die des Nachts", "tokens": ["Die", "Phan\u00b7ta\u00b7si\u00b7en", ",", "die", "des", "Nachts"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Im Hirn den bunten Umzug halten.", "tokens": ["Im", "Hirn", "den", "bun\u00b7ten", "Um\u00b7zug", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVI