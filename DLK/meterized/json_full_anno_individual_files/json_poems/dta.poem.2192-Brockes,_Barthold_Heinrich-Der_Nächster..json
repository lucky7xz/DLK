{"dta.poem.2192": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der N\u00e4chster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Denken scheinet eine Kraft sich Etwas blo\u00df nur", "tokens": ["Das", "Den\u00b7ken", "schei\u00b7net", "ei\u00b7ne", "Kraft", "sich", "Et\u00b7was", "blo\u00df", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PRF", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "vorzustellen,", "tokens": ["vor\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVIZU", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Das Meynen, eigentlich von dem, was man gedenkt, ein", "tokens": ["Das", "Mey\u00b7nen", ",", "ei\u00b7gent\u00b7lich", "von", "dem", ",", "was", "man", "ge\u00b7denkt", ",", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "ART", "$,", "PRELS", "PIS", "VVFIN", "$,", "ART"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Urthel f\u00e4llen.", "tokens": ["Ur\u00b7thel", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Nun fragt sichs, Stolz und Zank zu d\u00e4mpfen, ob wir da-", "tokens": ["Nun", "fragt", "sichs", ",", "Stolz", "und", "Zank", "zu", "d\u00e4mp\u00b7fen", ",", "ob", "wir", "da"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "NN", "KON", "NN", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "her nicht billig schlie\u00dfen:", "tokens": ["her", "nicht", "bil\u00b7lig", "schlie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df Thiere denken, Menschen meynen, und da\u00df allein die", "tokens": ["Da\u00df", "Thie\u00b7re", "den\u00b7ken", ",", "Men\u00b7schen", "mey\u00b7nen", ",", "und", "da\u00df", "al\u00b7lein", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVINF", "$,", "NN", "VVFIN", "$,", "KON", "KOUS", "ADV", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Engel wissen?", "tokens": ["En\u00b7gel", "wis\u00b7sen", "?"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}