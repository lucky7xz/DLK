{"dta.poem.6820": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom C\u00f6rper, womit sich die Seele bind't, verlangen,", "tokens": ["Vom", "C\u00f6r\u00b7per", ",", "wo\u00b7mit", "sich", "die", "See\u00b7le", "bind't", ",", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er den Druck empfind, den wir durch ihn empfangen,", "tokens": ["Da\u00df", "er", "den", "Druck", "emp\u00b7find", ",", "den", "wir", "durch", "ihn", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist eben, als wenn man von einer Laute wollte,", "tokens": ["Ist", "e\u00b7ben", ",", "als", "wenn", "man", "von", "ei\u00b7ner", "Lau\u00b7te", "woll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOKOM", "KOUS", "PIS", "APPR", "ART", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie die Melodey vernehmen sollte", "tokens": ["Da\u00df", "sie", "die", "Me\u00b7lo\u00b7dey", "ver\u00b7neh\u00b7men", "soll\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von den, von uns erregten Saiten.", "tokens": ["Von", "den", ",", "von", "uns", "er\u00b7reg\u00b7ten", "Sai\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dies heisst dem Instrument das Wissen, das Verm\u00f6gen,", "tokens": ["Dies", "heisst", "dem", "Inst\u00b7ru\u00b7ment", "das", "Wis\u00b7sen", ",", "das", "Ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Anmuth sammt den Lieblichkeiten,", "tokens": ["Die", "An\u00b7muth", "sammt", "den", "Lieb\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die es den Sinnen nur verursacht, beyzulegen.", "tokens": ["Die", "es", "den", "Sin\u00b7nen", "nur", "ver\u00b7ur\u00b7sacht", ",", "bey\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "VVPP", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dies heisst die Meinungen von einem Bilde hegen,", "tokens": ["Dies", "heisst", "die", "Mei\u00b7nun\u00b7gen", "von", "ei\u00b7nem", "Bil\u00b7de", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df es an einer Schilderey,", "tokens": ["Da\u00df", "es", "an", "ei\u00b7ner", "Schil\u00b7de\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Ordnung, und des Tons der Farben kundig sey,", "tokens": ["Der", "Ord\u00b7nung", ",", "und", "des", "Tons", "der", "Far\u00b7ben", "kun\u00b7dig", "sey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und da\u00df, wofern man es von ungefehr beweget,", "tokens": ["Und", "da\u00df", ",", "wo\u00b7fern", "man", "es", "von", "un\u00b7ge\u00b7fehr", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "KOUS", "PIS", "PPER", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es ihm entweder Lust, entweder Schmertz erreget.", "tokens": ["Es", "ihm", "ent\u00b7we\u00b7der", "Lust", ",", "ent\u00b7we\u00b7der", "Schmertz", "er\u00b7re\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "KON", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}