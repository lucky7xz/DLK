{"textgrid.poem.60440": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mira wird mit jedem Tage blasser,", "tokens": ["Mi\u00b7ra", "wird", "mit", "je\u00b7dem", "Ta\u00b7ge", "blas\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In den tiefsten Wald, auf Wildesbahnen", "tokens": ["In", "den", "tiefs\u00b7ten", "Wald", ",", "auf", "Wil\u00b7des\u00b7bah\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Flieht sie, wenn der Marosch laute Wasser", "tokens": ["Flieht", "sie", ",", "wenn", "der", "Ma\u00b7rosch", "lau\u00b7te", "Was\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sie zu schmerzlich jener Nacht gemahnen.", "tokens": ["Sie", "zu", "schmerz\u00b7lich", "je\u00b7ner", "Nacht", "ge\u00b7mah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKA", "ADJD", "PDAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Mischka klagt, doch fern, da\u00df er verdamme", "tokens": ["Mischka", "klagt", ",", "doch", "fern", ",", "da\u00df", "er", "ver\u00b7dam\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ADV", "ADJD", "$,", "KOUS", "PP