ord", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PPER", "$,", "PDS", "VVFIN", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Antlitz eingedr\u00fcckt dem gr\u00fcnen Schilfe.", "tokens": ["Ant\u00b7litz", "ein\u00b7ge\u00b7dr\u00fcckt", "dem", "gr\u00fc\u00b7nen", "Schil\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und der J\u00fcngling, der ein Herz gebrochen,", "tokens": ["Und", "der", "J\u00fcng\u00b7ling", ",", "der", "ein", "Herz", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "L\u00e4\u00dft ein andres schon an seinem pochen.", "tokens": ["L\u00e4\u00dft", "ein", "and\u00b7res", "schon", "an", "sei\u00b7nem", "po\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "ADV", "APPR", "PPOSAT", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Mischka stiehlt sich in den Stall des Grafen", "tokens": ["Mischka", "stiehlt", "sich", "in", "den", "Stall", "des", "Gra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mitternachts \u2013 die m\u00fcden Knechte schlafen \u2013,", "tokens": ["Mit\u00b7ter\u00b7nachts", "\u2013", "die", "m\u00fc\u00b7den", "Knech\u00b7te", "schla\u00b7fen", "\u2013", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV"