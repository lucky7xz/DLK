{"dta.poem.23413": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonne im Zeichen des Krebses.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Europa schien voll Wonne", "tokens": ["Auf", "Eu\u00b7ro\u00b7pa", "schien", "voll", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst der Freiheit lichte Sonne.", "tokens": ["Einst", "der", "Frei\u00b7heit", "lich\u00b7te", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Planet ward manches Land,", "tokens": ["Ein", "Pla\u00b7net", "ward", "man\u00b7ches", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aber, ach! die Sonne schwand.", "tokens": ["A\u00b7ber", ",", "ach", "!", "die", "Son\u00b7ne", "schwand", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als sie schwand, da schwand f\u00fcr immer", "tokens": ["Als", "sie", "schwand", ",", "da", "schwand", "f\u00fcr", "im\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "ADV"], "meter": "--+-+-++", "measure": "anapaest.init"}, "line.2": {"text": "Der Planeten lichter Schimmer,", "tokens": ["Der", "Pla\u00b7ne\u00b7ten", "lich\u00b7ter", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gro\u00dfbritanien allein", "tokens": ["Gro\u00df\u00b7bri\u00b7ta\u00b7ni\u00b7en", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Blieb der Freiheit Widerschein.", "tokens": ["Blieb", "der", "Frei\u00b7heit", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn an ihren spr\u00fchenden Funken", "tokens": ["Denn", "an", "ih\u00b7ren", "spr\u00fc\u00b7hen\u00b7den", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Hatt' es sich so satt getrunken,", "tokens": ["Hatt'", "es", "sich", "so", "satt", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df es jetzt noch hell und hehr", "tokens": ["Da\u00df", "es", "jetzt", "noch", "hell", "und", "hehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Leuchtet \u00fcbers ferne Meer.", "tokens": ["Leuch\u00b7tet", "\u00fc\u00b7bers", "fer\u00b7ne", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Freiheitssonne, kehre wieder!", "tokens": ["Frei\u00b7heits\u00b7son\u00b7ne", ",", "keh\u00b7re", "wie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blick auf alle Lande nieder!", "tokens": ["Blick", "auf", "al\u00b7le", "Lan\u00b7de", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringst du Fr\u00fchling nicht herab,", "tokens": ["Bringst", "du", "Fr\u00fch\u00b7ling", "nicht", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird Europa bald ein Grab.", "tokens": ["Wird", "Eu\u00b7ro\u00b7pa", "bald", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}