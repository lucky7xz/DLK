rcht begleiten?", "tokens": ["Das", "nichts", "als", "Schand", "vnd", "Furcht", "be\u00b7glei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "KOKOM", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man saget: Venus sey entsprossen", "tokens": ["Man", "sa\u00b7get", ":", "Ve\u00b7nus", "sey", "ent\u00b7spros\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$.", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur blos her aus des Meeres Schaum;", "tokens": ["Nur", "blos", "her", "aus", "des", "Mee\u00b7res", "Schaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie recht! der, so jhr hat genossen,", "tokens": ["Wie", "recht", "!", "der", ",", "so", "jhr", "hat", "ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$.", "ART", "$,", "ADV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wei\u00df, da\u00df sie fleucht, wenn man sie kaum", "tokens": ["Wei\u00df", ",", "da\u00df", "sie", "fleucht", ",", "wenn", "man", "sie", "kaum"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Empfunden hat, gleich wie ein Traum,", "tokens": ["Emp\u00b7fun\u00b7den", "hat", ",", "gleich", "wie", "ein", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$