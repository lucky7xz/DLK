{"dta.poem.16045": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Unbedingten, das nicht hier ist bei den Dingen,", "tokens": ["Zum", "Un\u00b7be\u00b7ding\u00b7ten", ",", "das", "nicht", "hier", "ist", "bei", "den", "Din\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PTKNEG", "ADV", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ringt, o bedingter Geist, dein unbedingtes Ringen,", "tokens": ["Ringt", ",", "o", "be\u00b7ding\u00b7ter", "Geist", ",", "dein", "un\u00b7be\u00b7ding\u00b7tes", "Rin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "FM", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn von den Dingen weist dich ein bedingtes fort", "tokens": ["Denn", "von", "den", "Din\u00b7gen", "weist", "dich", "ein", "be\u00b7ding\u00b7tes", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum andern, und zuletzt zum Unbedingten dort.", "tokens": ["Zum", "an\u00b7dern", ",", "und", "zu\u00b7letzt", "zum", "Un\u00b7be\u00b7ding\u00b7ten", "dort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "KON", "ADV", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Im Unbedingten dort, in welchem die Bedingung", "tokens": ["Im", "Un\u00b7be\u00b7ding\u00b7ten", "dort", ",", "in", "wel\u00b7chem", "die", "Be\u00b7din\u00b7gung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alles Bedingten ruht, ist deiner Ruh Erringung;", "tokens": ["Al\u00b7les", "Be\u00b7ding\u00b7ten", "ruht", ",", "ist", "dei\u00b7ner", "Ruh", "Er\u00b7rin\u00b7gung", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Im Unbedingten, das, indem es sich bedingt,", "tokens": ["Im", "Un\u00b7be\u00b7ding\u00b7ten", ",", "das", ",", "in\u00b7dem", "es", "sich", "be\u00b7dingt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PDS", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Dinge und hervor dich selbst, Bedingter, bringt.", "tokens": ["Die", "Din\u00b7ge", "und", "her\u00b7vor", "dich", "selbst", ",", "Be\u00b7ding\u00b7ter", ",", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "KON", "PTKVZ", "PPER", "ADV", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Unbedingte hat sich selbst hervorgebracht,", "tokens": ["Das", "Un\u00b7be\u00b7ding\u00b7te", "hat", "sich", "selbst", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bedingter Geist, in dir, indem du's hast gedacht.", "tokens": ["Be\u00b7ding\u00b7ter", "Geist", ",", "in", "dir", ",", "in\u00b7dem", "du's", "hast", "ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "$,", "KOUS", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}