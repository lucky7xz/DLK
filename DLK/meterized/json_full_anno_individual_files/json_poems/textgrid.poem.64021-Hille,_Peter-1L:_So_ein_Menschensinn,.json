{"textgrid.poem.64021": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: So ein Menschensinn,", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ein Menschensinn,", "tokens": ["So", "ein", "Men\u00b7schen\u00b7sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ihr wi\u00dft ja nicht,", "tokens": ["Ihr", "wi\u00dft", "ja", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie gro\u00df der ist,", "tokens": ["Wie", "gro\u00df", "der", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wie gewaltig und fest!", "tokens": ["Wie", "ge\u00b7wal\u00b7tig", "und", "fest", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Weilend und eilend", "tokens": ["Wei\u00b7lend", "und", "ei\u00b7lend"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ein Proteus.", "tokens": ["Ein", "Pro\u00b7teus", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "\u00dcberallhin, \u00fcberallhin", "tokens": ["\u00dc\u00b7be\u00b7rall\u00b7hin", ",", "\u00fc\u00b7be\u00b7rall\u00b7hin"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Reichen reine Kr\u00e4fte,", "tokens": ["Rei\u00b7chen", "rei\u00b7ne", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Die sich der Triebe begeben.", "tokens": ["Die", "sich", "der", "Trie\u00b7be", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "In zarten Farben", "tokens": ["In", "zar\u00b7ten", "Far\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Atmet der Geist ein seliges Leben,", "tokens": ["At\u00b7met", "der", "Geist", "ein", "se\u00b7li\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Bange vor F\u00fclle.", "tokens": ["Ban\u00b7ge", "vor", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Alles ist von Blumen zu,", "tokens": ["Al\u00b7les", "ist", "von", "Blu\u00b7men", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Wo gibt es ein Ende?", "tokens": ["Wo", "gibt", "es", "ein", "En\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "\u00dcber alles rieselst du hin,", "tokens": ["\u00dc\u00b7ber", "al\u00b7les", "rie\u00b7selst", "du", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "G\u00f6ttlicher Geist,", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", "Geist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Und schaust dein selber", "tokens": ["Und", "schaust", "dein", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Beschwichtigtes Schicksal,", "tokens": ["Be\u00b7schwich\u00b7tig\u00b7tes", "Schick\u00b7sal", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Und freust Dich,", "tokens": ["Und", "freust", "Dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Eines gewaltigen Vaters der Dinge,", "tokens": ["Ei\u00b7nes", "ge\u00b7wal\u00b7ti\u00b7gen", "Va\u00b7ters", "der", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.21": {"text": "Der nirgends wohnt,", "tokens": ["Der", "nir\u00b7gends", "wohnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Um so glutender naht seine Kraft", "tokens": ["Um", "so", "glu\u00b7ten\u00b7der", "naht", "sei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.23": {"text": "Den wachsenden S\u00f6hnen,", "tokens": ["Den", "wach\u00b7sen\u00b7den", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "In ihnen w\u00e4chst er dr\u00e4ngend", "tokens": ["In", "ih\u00b7nen", "w\u00e4chst", "er", "dr\u00e4n\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "\u00dcber die Erden,", "tokens": ["\u00dc\u00b7ber", "die", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Neu sie erschaffend,", "tokens": ["Neu", "sie", "er\u00b7schaf\u00b7fend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Unverlassen,", "tokens": ["Un\u00b7ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Anders gestaltet,", "tokens": ["An\u00b7ders", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Kann er die Welten", "tokens": ["Kann", "er", "die", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Und ihr buntes leuchtendes Leben", "tokens": ["Und", "ihr", "bun\u00b7tes", "leuch\u00b7ten\u00b7des", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Ruhend aus sich tun.", "tokens": ["Ru\u00b7hend", "aus", "sich", "tun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.32": {"text": "Sein Sein schon ist Leben.", "tokens": ["Sein", "Sein", "schon", "ist", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "Farbige Weihe,", "tokens": ["Far\u00b7bi\u00b7ge", "Wei\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.34": {"text": "Ungeheure Angesichter", "tokens": ["Un\u00b7ge\u00b7heu\u00b7re", "An\u00b7ge\u00b7sich\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Her zu mir gestellt", "tokens": ["Her", "zu", "mir", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.36": {"text": "Aus der Unendlichkeit,", "tokens": ["Aus", "der", "Un\u00b7end\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Und starke deutliche H\u00e4nde", "tokens": ["Und", "star\u00b7ke", "deut\u00b7li\u00b7che", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Mit festen br\u00fcchigen Daumenn\u00e4geln,", "tokens": ["Mit", "fes\u00b7ten", "br\u00fc\u00b7chi\u00b7gen", "Dau\u00b7men\u00b7n\u00e4\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Knoten an den Gelenken", "tokens": ["Kno\u00b7ten", "an", "den", "Ge\u00b7len\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.40": {"text": "Und blauen t\u00e4glichen \u00c4rmeln,", "tokens": ["Und", "blau\u00b7en", "t\u00e4g\u00b7li\u00b7chen", "\u00c4r\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.41": {"text": "Oder ziegelroten", "tokens": ["O\u00b7der", "zie\u00b7gel\u00b7ro\u00b7ten"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.42": {"text": "Und breitem, wei\u00dfen, l\u00e4ssigen Aufschlag,", "tokens": ["Und", "brei\u00b7tem", ",", "wei\u00b7\u00dfen", ",", "l\u00e4s\u00b7si\u00b7gen", "Auf\u00b7schlag", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Die kommen mir aus dem klaren,", "tokens": ["Die", "kom\u00b7men", "mir", "aus", "dem", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "Dem Blicke weichenden Himmelsgew\u00f6lbe.", "tokens": ["Dem", "Bli\u00b7cke", "wei\u00b7chen\u00b7den", "Him\u00b7mels\u00b7ge\u00b7w\u00f6l\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.45": {"text": "Ein Wortbauer,", "tokens": ["Ein", "Wort\u00b7bau\u00b7er", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.46": {"text": "Gestalten sinnend,", "tokens": ["Ge\u00b7stal\u00b7ten", "sin\u00b7nend", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.47": {"text": "Gesetze gewinnend", "tokens": ["Ge\u00b7set\u00b7ze", "ge\u00b7win\u00b7nend"], "token_info": ["word", "word"], "pos": ["NN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.48": {"text": "Von h\u00fcben", "tokens": ["Von", "h\u00fc\u00b7ben"], "token_info": ["word", "word"], "pos": ["APPR", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.49": {"text": "Und dr\u00fcben,", "tokens": ["Und", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.50": {"text": "Zuwartend,", "tokens": ["Zu\u00b7war\u00b7tend", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.51": {"text": "Rein mich putzend", "tokens": ["Rein", "mich", "put\u00b7zend"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.52": {"text": "Und liebend, liebend.", "tokens": ["Und", "lie\u00b7bend", ",", "lie\u00b7bend", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.53": {"text": "Die brennende Sehnsucht", "tokens": ["Die", "bren\u00b7nen\u00b7de", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.54": {"text": "Zum weiteren", "tokens": ["Zum", "wei\u00b7te\u00b7ren"], "token_info": ["word", "word"], "pos": ["APPRART", "ADJA"], "meter": "-+-+", "measure": "iambic.di"}, "line.55": {"text": "Leben und Tod", "tokens": ["Le\u00b7ben", "und", "Tod"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.56": {"text": "Und Sterne", "tokens": ["Und", "Ster\u00b7ne"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.57": {"text": "Und Sonnenbahnen", "tokens": ["Und", "Son\u00b7nen\u00b7bah\u00b7nen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.58": {"text": "Aus meinem helleren,", "tokens": ["Aus", "mei\u00b7nem", "hel\u00b7le\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "Tieferen Geiste zu lesen,", "tokens": ["Tie\u00b7fe\u00b7ren", "Geis\u00b7te", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.60": {"text": "Sie wird gestillt nach Gesetzen", "tokens": ["Sie", "wird", "ge\u00b7stillt", "nach", "Ge\u00b7set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.61": {"text": "Zur Zeit.", "tokens": ["Zur", "Zeit", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "So ein Menschensinn,", "tokens": ["So", "ein", "Men\u00b7schen\u00b7sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ihr wi\u00dft ja nicht,", "tokens": ["Ihr", "wi\u00dft", "ja", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie gro\u00df der ist,", "tokens": ["Wie", "gro\u00df", "der", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wie gewaltig und fest!", "tokens": ["Wie", "ge\u00b7wal\u00b7tig", "und", "fest", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Weilend und eilend", "tokens": ["Wei\u00b7lend", "und", "ei\u00b7lend"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ein Proteus.", "tokens": ["Ein", "Pro\u00b7teus", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "\u00dcberallhin, \u00fcberallhin", "tokens": ["\u00dc\u00b7be\u00b7rall\u00b7hin", ",", "\u00fc\u00b7be\u00b7rall\u00b7hin"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Reichen reine Kr\u00e4fte,", "tokens": ["Rei\u00b7chen", "rei\u00b7ne", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Die sich der Triebe begeben.", "tokens": ["Die", "sich", "der", "Trie\u00b7be", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "In zarten Farben", "tokens": ["In", "zar\u00b7ten", "Far\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Atmet der Geist ein seliges Leben,", "tokens": ["At\u00b7met", "der", "Geist", "ein", "se\u00b7li\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Bange vor F\u00fclle.", "tokens": ["Ban\u00b7ge", "vor", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Alles ist von Blumen zu,", "tokens": ["Al\u00b7les", "ist", "von", "Blu\u00b7men", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Wo gibt es ein Ende?", "tokens": ["Wo", "gibt", "es", "ein", "En\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "\u00dcber alles rieselst du hin,", "tokens": ["\u00dc\u00b7ber", "al\u00b7les", "rie\u00b7selst", "du", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.16": {"text": "G\u00f6ttlicher Geist,", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", "Geist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Und schaust dein selber", "tokens": ["Und", "schaust", "dein", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Beschwichtigtes Schicksal,", "tokens": ["Be\u00b7schwich\u00b7tig\u00b7tes", "Schick\u00b7sal", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Und freust Dich,", "tokens": ["Und", "freust", "Dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Eines gewaltigen Vaters der Dinge,", "tokens": ["Ei\u00b7nes", "ge\u00b7wal\u00b7ti\u00b7gen", "Va\u00b7ters", "der", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.21": {"text": "Der nirgends wohnt,", "tokens": ["Der", "nir\u00b7gends", "wohnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Um so glutender naht seine Kraft", "tokens": ["Um", "so", "glu\u00b7ten\u00b7der", "naht", "sei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.23": {"text": "Den wachsenden S\u00f6hnen,", "tokens": ["Den", "wach\u00b7sen\u00b7den", "S\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "In ihnen w\u00e4chst er dr\u00e4ngend", "tokens": ["In", "ih\u00b7nen", "w\u00e4chst", "er", "dr\u00e4n\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "\u00dcber die Erden,", "tokens": ["\u00dc\u00b7ber", "die", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Neu sie erschaffend,", "tokens": ["Neu", "sie", "er\u00b7schaf\u00b7fend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Unverlassen,", "tokens": ["Un\u00b7ver\u00b7las\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Anders gestaltet,", "tokens": ["An\u00b7ders", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.29": {"text": "Kann er die Welten", "tokens": ["Kann", "er", "die", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Und ihr buntes leuchtendes Leben", "tokens": ["Und", "ihr", "bun\u00b7tes", "leuch\u00b7ten\u00b7des", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Ruhend aus sich tun.", "tokens": ["Ru\u00b7hend", "aus", "sich", "tun", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.32": {"text": "Sein Sein schon ist Leben.", "tokens": ["Sein", "Sein", "schon", "ist", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.33": {"text": "Farbige Weihe,", "tokens": ["Far\u00b7bi\u00b7ge", "Wei\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.34": {"text": "Ungeheure Angesichter", "tokens": ["Un\u00b7ge\u00b7heu\u00b7re", "An\u00b7ge\u00b7sich\u00b7ter"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Her zu mir gestellt", "tokens": ["Her", "zu", "mir", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.36": {"text": "Aus der Unendlichkeit,", "tokens": ["Aus", "der", "Un\u00b7end\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Und starke deutliche H\u00e4nde", "tokens": ["Und", "star\u00b7ke", "deut\u00b7li\u00b7che", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Mit festen br\u00fcchigen Daumenn\u00e4geln,", "tokens": ["Mit", "fes\u00b7ten", "br\u00fc\u00b7chi\u00b7gen", "Dau\u00b7men\u00b7n\u00e4\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Knoten an den Gelenken", "tokens": ["Kno\u00b7ten", "an", "den", "Ge\u00b7len\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.40": {"text": "Und blauen t\u00e4glichen \u00c4rmeln,", "tokens": ["Und", "blau\u00b7en", "t\u00e4g\u00b7li\u00b7chen", "\u00c4r\u00b7meln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.41": {"text": "Oder ziegelroten", "tokens": ["O\u00b7der", "zie\u00b7gel\u00b7ro\u00b7ten"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.42": {"text": "Und breitem, wei\u00dfen, l\u00e4ssigen Aufschlag,", "tokens": ["Und", "brei\u00b7tem", ",", "wei\u00b7\u00dfen", ",", "l\u00e4s\