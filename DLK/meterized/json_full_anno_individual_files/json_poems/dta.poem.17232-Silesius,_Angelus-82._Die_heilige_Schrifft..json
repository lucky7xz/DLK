{"dta.poem.17232": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "82.  Die heilige Schrifft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie die Spinne saugt au\u00df einer Rose Gifft:", "tokens": ["Gleich", "wie", "die", "Spin\u00b7ne", "saugt", "au\u00df", "ei\u00b7ner", "Ro\u00b7se", "Gifft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Also wird auch verkehrt vom b\u00f6sen Gottesschrifft!", "tokens": ["Al\u00b7so", "wird", "auch", "ver\u00b7kehrt", "vom", "b\u00f6\u00b7sen", "Got\u00b7tes\u00b7schrifft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}