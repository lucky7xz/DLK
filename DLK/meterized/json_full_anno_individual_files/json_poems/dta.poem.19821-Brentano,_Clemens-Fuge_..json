{"dta.poem.19821": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Fuge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Musikant wollte fr\u00f6hlich seyn,               ", "tokens": ["Ein", "Mu\u00b7si\u00b7kant", "woll\u00b7te", "fr\u00f6h\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es th\u00e4t ihm wohl gelingen,", "tokens": ["Es", "th\u00e4t", "ihm", "wohl", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er sa\u00df bey einem guten Wein,", "tokens": ["Er", "sa\u00df", "bey", "ei\u00b7nem", "gu\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da wollt er lustig singen,", "tokens": ["Da", "wollt", "er", "lus\u00b7tig", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bekannt ist weit und breit der Wein,", "tokens": ["Be\u00b7kannt", "ist", "weit", "und", "breit", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gewachsen hin und her am Rhein,", "tokens": ["Ge\u00b7wach\u00b7sen", "hin", "und", "her", "am", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADV", "APPRART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Macht sittlich modulieren,", "tokens": ["Macht", "sitt\u00b7lich", "mo\u00b7du\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Thut manchen oft verf\u00fchren.", "tokens": ["Thut", "man\u00b7chen", "oft", "ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Davon sezt er ein Liedlein klein,", "tokens": ["Da\u00b7von", "sezt", "er", "ein", "Lied\u00b7lein", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Das thut er wohl betrachten,", "tokens": ["Das", "thut", "er", "wohl", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mischet gute Fugen ein,", "tokens": ["Und", "mi\u00b7schet", "gu\u00b7te", "Fu\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Niemand konnts ihm verachten.", "tokens": ["Nie\u00b7mand", "konnts", "ihm", "ver\u00b7ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Er dacht in dem Gem\u00fcthe sein,", "tokens": ["Er", "dacht", "in", "dem", "Ge\u00b7m\u00fc\u00b7the", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ey w\u00e4ren tausend Kronen mein,", "tokens": ["Ey", "w\u00e4\u00b7ren", "tau\u00b7send", "Kro\u00b7nen", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "CARD", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und alle Jahr ein Fuder Wein,", "tokens": ["Und", "al\u00b7le", "Jahr", "ein", "Fu\u00b7der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das k\u00f6nnten gute Fugen seyn.", "tokens": ["Das", "k\u00f6nn\u00b7ten", "gu\u00b7te", "Fu\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}