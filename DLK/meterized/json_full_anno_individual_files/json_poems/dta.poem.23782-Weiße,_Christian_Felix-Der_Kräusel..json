{"dta.poem.23782": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Kr\u00e4usel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Kr\u00e4usel h\u00fcpfet froh umher,", "tokens": ["Mein", "Kr\u00e4u\u00b7sel", "h\u00fcp\u00b7fet", "froh", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn ich ihn flei\u00dfig treibe,", "tokens": ["Wenn", "ich", "ihn", "flei\u00b7\u00dfig", "trei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch ganz unth\u00e4tig lieget er,", "tokens": ["Doch", "ganz", "un\u00b7th\u00e4\u00b7tig", "lie\u00b7get", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ich in Ruhe bleibe.", "tokens": ["Wenn", "ich", "in", "Ru\u00b7he", "blei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer stets dem Gl\u00fcck im Schoo\u00dfe", "tokens": ["Wer", "stets", "dem", "Gl\u00fcck", "im", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ruht,", "tokens": ["ruht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wird oft zur Tugend tr\u00e4ge:", "tokens": ["Wird", "oft", "zur", "Tu\u00b7gend", "tr\u00e4\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch er wird th\u00e4tig, weise, gut,", "tokens": ["Doch", "er", "wird", "th\u00e4\u00b7tig", ",", "wei\u00b7se", ",", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fchlt er des Ungl\u00fccks Schl\u00e4ge.", "tokens": ["F\u00fchlt", "er", "des", "Un\u00b7gl\u00fccks", "Schl\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}