{"dta.poem.8838": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn deine goldne Locke in tausend Ringe f\u00e4llt!", "tokens": ["Wenn", "dei\u00b7ne", "gold\u00b7ne", "Lo\u00b7cke", "in", "tau\u00b7send", "Rin\u00b7ge", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Begl\u00fcckt, wer einzuathmen der Locken Duft vermag,", "tokens": ["Be\u00b7gl\u00fcckt", ",", "wer", "ein\u00b7zu\u00b7ath\u00b7men", "der", "Lo\u00b7cken", "Duft", "ver\u00b7mag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "VVFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Begl\u00fcckter, wer gefangen in ihre Schlinge f\u00e4llt!", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", ",", "wer", "ge\u00b7fan\u00b7gen", "in", "ih\u00b7re", "Schlin\u00b7ge", "f\u00e4llt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Allm\u00e4chtig ist dein Auge, doch ist es ein Tyrann,", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "ist", "dein", "Au\u00b7ge", ",", "doch", "ist", "es", "ein", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Vor dem der Gro\u00dfe zittert und der Geringe f\u00e4llt.", "tokens": ["Vor", "dem", "der", "Gro\u00b7\u00dfe", "zit\u00b7tert", "und", "der", "Ge\u00b7rin\u00b7ge", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "VVFIN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Du weilst als Stern am Himmel, inde\u00df als Schnuppe", "tokens": ["Du", "weilst", "als", "Stern", "am", "Him\u00b7mel", ",", "in\u00b7de\u00df", "als", "Schnup\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "APPRART", "NN", "$,", "ADV", "KOUS", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Was sonst ich vor das Auge der Seele bringe, f\u00e4llt.", "tokens": ["Was", "sonst", "ich", "vor", "das", "Au\u00b7ge", "der", "See\u00b7le", "brin\u00b7ge", ",", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Du wohnst so hoch und ferne, da\u00df eh' er dich erreicht,", "tokens": ["Du", "wohnst", "so", "hoch", "und", "fer\u00b7ne", ",", "da\u00df", "eh'", "er", "dich", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "KON", "ADJA", "$,", "KOUS", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Dem Falken des Verlangens die matte Schwinge f\u00e4llt!", "tokens": ["Dem", "Fal\u00b7ken", "des", "Ver\u00b7lan\u00b7gens", "die", "mat\u00b7te", "Schwin\u00b7ge", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}