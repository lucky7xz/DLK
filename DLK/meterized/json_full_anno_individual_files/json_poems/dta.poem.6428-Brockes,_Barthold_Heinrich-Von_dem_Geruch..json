{"dta.poem.6428": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00e4ngt je mehr und mehr der Geist an zu verstehn,", "tokens": ["Es", "f\u00e4ngt", "je", "mehr", "und", "mehr", "der", "Geist", "an", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "KON", "ADV", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie weit der Vorw\u00fcrff\u2019 Kr\u00e4fft\u2019 auf unsre Sinne gehn.", "tokens": ["Wie", "weit", "der", "Vor\u00b7w\u00fcrff'", "Kr\u00e4fft'", "auf", "uns\u00b7re", "Sin\u00b7ne", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Vorhang \u00f6ffnet sich allm\u00e4hlig Staffelweise,", "tokens": ["Ein", "Vor\u00b7hang", "\u00f6ff\u00b7net", "sich", "all\u00b7m\u00e4h\u00b7lig", "Staf\u00b7fel\u00b7wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zeigt uns besser noch, was man zu sehn verlangt;", "tokens": ["Und", "zeigt", "uns", "bes\u00b7ser", "noch", ",", "was", "man", "zu", "sehn", "ver\u00b7langt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "$,", "PRELS", "PIS", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man sieht der C\u00f6rper Bau, wie er zusammen hangt.", "tokens": ["Man", "sieht", "der", "C\u00f6r\u00b7per", "Bau", ",", "wie", "er", "zu\u00b7sam\u00b7men", "hangt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn das, was wir gesagt, von dem Geschmack der Speise,", "tokens": ["Denn", "das", ",", "was", "wir", "ge\u00b7sagt", ",", "von", "dem", "Ge\u00b7schmack", "der", "Spei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "PPER", "VVPP", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wird uns in riechenden und Dunst-erf\u00fcllten Sachen,", "tokens": ["Wird", "uns", "in", "rie\u00b7chen\u00b7den", "und", "Dunst\u00b7er\u00b7f\u00fcll\u00b7ten", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Nachfrag\u2019 allbereit um so viel leichter machen.", "tokens": ["Die", "Nach\u00b7frag'", "all\u00b7be\u00b7reit", "um", "so", "viel", "leich\u00b7ter", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}