{"dta.poem.8451": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entz\u00fcckung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Durch den \u00fcberreiften Lindengang!", "tokens": ["Durch", "den", "\u00fc\u00b7berr\u00b7eif\u00b7ten", "Lin\u00b7den\u00b7gang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Horchet, aus den todten Wipfeln schallet", "tokens": ["Hor\u00b7chet", ",", "aus", "den", "tod\u00b7ten", "Wip\u00b7feln", "schal\u00b7let"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ueberirdischer Gesang!", "tokens": ["Ue\u00b7be\u00b7rir\u00b7di\u00b7scher", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Alles muss sich, wo sie wandelt, heitern:", "tokens": ["Al\u00b7les", "muss", "sich", ",", "wo", "sie", "wan\u00b7delt", ",", "hei\u00b7tern", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blumen sprossen, und der West erwacht,", "tokens": ["Blu\u00b7men", "spros\u00b7sen", ",", "und", "der", "West", "er\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KON", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Blumen wanken unter gr\u00fcnen Kr\u00e4utern;", "tokens": ["Blu\u00b7men", "wan\u00b7ken", "un\u00b7ter", "gr\u00fc\u00b7nen", "Kr\u00e4u\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Alles freut sich, alles lacht!", "tokens": ["Al\u00b7les", "freut", "sich", ",", "al\u00b7les", "lacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie verl\u00e4sst, mir einen Gruss zu nicken,", "tokens": ["Sie", "ver\u00b7l\u00e4sst", ",", "mir", "ei\u00b7nen", "Gruss", "zu", "ni\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mich zum Gott zu l\u00e4cheln, ihren Hain!", "tokens": ["Mich", "zum", "Gott", "zu", "l\u00e4\u00b7cheln", ",", "ih\u00b7ren", "Hain", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKZU", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Geusst ein Engel heiliges Entz\u00fccken", "tokens": ["Geusst", "ein", "En\u00b7gel", "hei\u00b7li\u00b7ges", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch mein zitterndes Gebein?", "tokens": ["Durch", "mein", "zit\u00b7tern\u00b7des", "Ge\u00b7bein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}