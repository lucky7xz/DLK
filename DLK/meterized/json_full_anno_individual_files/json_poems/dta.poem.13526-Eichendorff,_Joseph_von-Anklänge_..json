{"dta.poem.13526": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ankl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nnt' ich zu den W\u00e4ldern fl\u00fcchten,", "tokens": ["K\u00f6nnt'", "ich", "zu", "den", "W\u00e4l\u00b7dern", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Gr\u00fcn in frischer Lust", "tokens": ["Mit", "dem", "Gr\u00fcn", "in", "fri\u00b7scher", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich zum Himmelsglanz aufrichten \u2014", "tokens": ["Mich", "zum", "Him\u00b7mels\u00b7glanz", "auf\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stark und frei w\u00e4r' da die Brust!", "tokens": ["Stark", "und", "frei", "w\u00e4r'", "da", "die", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}