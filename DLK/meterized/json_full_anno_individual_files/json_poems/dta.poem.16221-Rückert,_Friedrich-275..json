{"dta.poem.16221": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "275.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott, also hat gesagt ein hoher Glaubenslehrer,", "tokens": ["Gott", ",", "al\u00b7so", "hat", "ge\u00b7sagt", "ein", "ho\u00b7her", "Glau\u00b7bens\u00b7leh\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott selber w\u00e4chst in dir, o glaubiger Verehrer.", "tokens": ["Gott", "sel\u00b7ber", "w\u00e4chst", "in", "dir", ",", "o", "glau\u00b7bi\u00b7ger", "Ver\u00b7eh\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "APPR", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er w\u00e4chst nicht in sich selbst, da ist er stets vollkommen,", "tokens": ["Er", "w\u00e4chst", "nicht", "in", "sich", "selbst", ",", "da", "ist", "er", "stets", "voll\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PRF", "ADV", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der zur Vollkommenheit nur auch in dir soll kommen.", "tokens": ["Der", "zur", "Voll\u00b7kom\u00b7men\u00b7heit", "nur", "auch", "in", "dir", "soll", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "ADV", "APPR", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und w\u00e4chst er nicht in dir, jemehr du ihn begreifst,", "tokens": ["Und", "w\u00e4chst", "er", "nicht", "in", "dir", ",", "je\u00b7mehr", "du", "ihn", "be\u00b7greifst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jemehr in deiner Brust du sein Geheimnis reifst?", "tokens": ["Je\u00b7mehr", "in", "dei\u00b7ner", "Brust", "du", "sein", "Ge\u00b7heim\u00b7nis", "reifst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn dich ein m\u00e4\u00dfiges Verst\u00e4ndni\u00df gestern freute,", "tokens": ["Wenn", "dich", "ein", "m\u00e4\u00b7\u00dfi\u00b7ges", "Ver\u00b7st\u00e4nd\u00b7ni\u00df", "ge\u00b7stern", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So freuet h\u00f6here Verst\u00e4ndigung dich heute.", "tokens": ["So", "freu\u00b7et", "h\u00f6\u00b7he\u00b7re", "Ver\u00b7st\u00e4n\u00b7di\u00b7gung", "dich", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Noch tiefre Einsicht geht dir morgen auf villeicht,", "tokens": ["Noch", "tief\u00b7re", "Ein\u00b7sicht", "geht", "dir", "mor\u00b7gen", "auf", "vil\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und immer w\u00e4chst der Glanz, der nie die Spitz' erreicht.", "tokens": ["Und", "im\u00b7mer", "w\u00e4chst", "der", "Glanz", ",", "der", "nie", "die", "Spitz'", "er\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und sollt' es Gott nicht freun, sowie es dich erfreut,", "tokens": ["Und", "sollt'", "es", "Gott", "nicht", "freun", ",", "so\u00b7wie", "es", "dich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "PTKNEG", "VVINF", "$,", "KON", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In dir sich zu erneun, indem er dich erneut?", "tokens": ["In", "dir", "sich", "zu", "er\u00b7neun", ",", "in\u00b7dem", "er", "dich", "er\u00b7neut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "PTKZU", "VVINF", "$,", "KOUS", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Beschaut ein Lehrer doch in seines Sch\u00fclers Brust", "tokens": ["Be\u00b7schaut", "ein", "Leh\u00b7rer", "doch", "in", "sei\u00b7nes", "Sch\u00fc\u00b7lers", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stets reiner ausgepr\u00e4gt sein eignes Bild mit Lust.", "tokens": ["Stets", "rei\u00b7ner", "aus\u00b7ge\u00b7pr\u00e4gt", "sein", "eig\u00b7nes", "Bild", "mit", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Nicht minder schauet Gott im Spiegel von Kristallen,", "tokens": ["Nicht", "min\u00b7der", "schau\u00b7et", "Gott", "im", "Spie\u00b7gel", "von", "Kris\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "NN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wozu dein Herz er schuf, sich selbst mit Wohlgefallen.", "tokens": ["Wo\u00b7zu", "dein", "Herz", "er", "schuf", ",", "sich", "selbst", "mit", "Wohl\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$,", "PRF", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "O Herz, das zum Behuf des Spiegels Er erschuf,", "tokens": ["O", "Herz", ",", "das", "zum", "Be\u00b7huf", "des", "Spie\u00b7gels", "Er", "er\u00b7schuf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie weit bist du entfernt zu gn\u00fcgen dem Beruf!", "tokens": ["Wie", "weit", "bist", "du", "ent\u00b7fernt", "zu", "gn\u00fc\u00b7gen", "dem", "Be\u00b7ruf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}