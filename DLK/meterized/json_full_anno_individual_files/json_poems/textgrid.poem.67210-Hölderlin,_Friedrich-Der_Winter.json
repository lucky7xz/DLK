{"textgrid.poem.67210": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Winter", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Feld ist kahl, auf ferner H\u00f6he gl\u00e4nzet", "tokens": ["Das", "Feld", "ist", "kahl", ",", "auf", "fer\u00b7ner", "H\u00f6\u00b7he", "gl\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der blaue Himmel nur, und wie die Pfade gehen,", "tokens": ["Der", "blau\u00b7e", "Him\u00b7mel", "nur", ",", "und", "wie", "die", "Pfa\u00b7de", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "KON", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erscheinet die Natur, als Einerlei, das Wehen", "tokens": ["Er\u00b7schei\u00b7net", "die", "Na\u00b7tur", ",", "als", "Ei\u00b7ner\u00b7lei", ",", "das", "We\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist frisch, und die Natur von Helle nur umkr\u00e4nzet.", "tokens": ["Ist", "frisch", ",", "und", "die", "Na\u00b7tur", "von", "Hel\u00b7le", "nur", "um\u00b7kr\u00e4n\u00b7zet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "ART", "NN", "APPR", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alex