{"textgrid.poem.27790": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Krieger Art und Werk bi\u00dfher war rauben, stehlen;", "tokens": ["Der", "Krie\u00b7ger", "Art", "und", "Werk", "bi\u00df\u00b7her", "war", "rau\u00b7ben", ",", "steh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "ADV", "VAFIN", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der St\u00e4ter Art und Werk erkauffen und verh\u00f6len.", "tokens": ["Der", "St\u00e4\u00b7ter", "Art", "und", "Werk", "er\u00b7kauf\u00b7fen", "und", "ver\u00b7h\u00f6\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es ist was starck gesagt. Es ist ja gut gemeinet,", "tokens": ["Es", "ist", "was", "starck", "ge\u00b7sagt", ".", "Es", "ist", "ja", "gut", "ge\u00b7mei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "VVPP", "$.", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wiewols von aussen nicht, als wie es solte, scheinet.", "tokens": ["Wie\u00b7wols", "von", "aus\u00b7sen", "nicht", ",", "als", "wie", "es", "sol\u00b7te", ",", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "PTKNEG", "$,", "KOUS", "PWAV", "PPER", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was neu ist, das ist gut; drum ist ihr Sinn gewesen,", "tokens": ["Was", "neu", "ist", ",", "das", "ist", "gut", ";", "drum", "ist", "ihr", "Sinn", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PDS", "VAFIN", "ADJD", "$.", "PAV", "VAFIN", "PPOSAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df man bei allem soll das Renovatum lesen.", "tokens": ["Da\u00df", "man", "bei", "al\u00b7lem", "soll", "das", "Re\u00b7no\u00b7va\u00b7tum", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Krieger Art und Werk bi\u00dfher war rauben, stehlen;", "tokens": ["Der", "Krie\u00b7ger", "Art", "und", "Werk", "bi\u00df\u00b7her", "war", "rau\u00b7ben", ",", "steh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "ADV", "VAFIN", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der St\u00e4ter Art und Werk erkauffen und verh\u00f6len.", "tokens": ["Der", "St\u00e4\u00b7ter", "Art", "und", "Werk", "er\u00b7kauf\u00b7fen", "und", "ver\u00b7h\u00f6\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text"