{"textgrid.poem.65320": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Verschiedenes Schicksal der S\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich im Walde neulich bin gegangen,", "tokens": ["Als", "ich", "im", "Wal\u00b7de", "neu\u00b7lich", "bin", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "H\u00f6rt' ich ein Nachtigallenm\u00e4nnchen singen:", "tokens": ["H\u00f6rt'", "ich", "ein", "Nach\u00b7ti\u00b7gal\u00b7len\u00b7m\u00e4nn\u00b7chen", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es rief nach seinem Weibchen voll Verlangen.", "tokens": ["Es", "rief", "nach", "sei\u00b7nem", "Weib\u00b7chen", "voll", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und husch, da kam das Weibchen gleich geflogen", "tokens": ["Und", "husch", ",", "da", "kam", "das", "Weib\u00b7chen", "gleich", "ge\u00b7flo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch Busch und Dorn, und schrie, als wollt' es sagen:", "tokens": ["Durch", "Busch", "und", "Dorn", ",", "und", "schrie", ",", "als", "wollt'", "es", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KON", "VVFIN", "$,", "KOUS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier bin ich ja, mein lieber, lieber Vogel!", "tokens": ["Hier", "bin", "ich", "ja", ",", "mein", "lie\u00b7ber", ",", "lie\u00b7ber", "Vo\u00b7gel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PPOSAT", "ADV", "$,", "ADV", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich Armer singe nun vor deinem Hause", "tokens": ["Ich", "Ar\u00b7mer", "sin\u00b7ge", "nun", "vor", "dei\u00b7nem", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon Monde lang der Sehnsucht Klageweise \u2013", "tokens": ["Schon", "Mon\u00b7de", "lang", "der", "Sehn\u00b7sucht", "Kla\u00b7ge\u00b7wei\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJD", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du aber kicherst hinter'm Fensterglase.", "tokens": ["Du", "a\u00b7ber", "ki\u00b7cherst", "hin\u00b7ter'm", "Fens\u00b7ter\u00b7gla\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als ich im Walde neulich bin gegangen,", "tokens": ["Als", "ich", "im", "Wal\u00b7de", "neu\u00b7lich", "bin", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "wor