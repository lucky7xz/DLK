{"dta.poem.17608": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "230.  Das b\u00f6se ist deine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Das gutte kom\u0304t au\u00df Gott/ drum\u0304 ists auch seyn\u2019 allein:", "tokens": ["Das", "gut\u00b7te", "kom\u0304t", "au\u00df", "Gott", "/", "drum\u0304", "ists", "auch", "seyn'", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "NN", "$(", "PAV", "VAFIN", "ADV", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das b\u00f6s\u2019 entfteht au\u00df dir: das la\u00df du deine seyn.", "tokens": ["Das", "b\u00f6s'", "entf\u00b7teht", "au\u00df", "dir", ":", "das", "la\u00df", "du", "dei\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "APPR", "PPER", "$.", "PDS", "VVFIN", "PPER", "PPOSAT", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}