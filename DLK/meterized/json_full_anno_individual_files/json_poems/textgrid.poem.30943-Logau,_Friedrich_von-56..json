{"textgrid.poem.30943": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Gut besucht ich nechst; das Feld war voller Segen;", "tokens": ["Mein", "Gut", "be\u00b7sucht", "ich", "nechst", ";", "das", "Feld", "war", "vol\u00b7ler", "Se\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst war mirs nicht so gut, wie in der Stadt, gelegen:", "tokens": ["Sonst", "war", "mirs", "nicht", "so", "gut", ",", "wie", "in", "der", "Stadt", ",", "ge\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "PTKNEG", "ADV", "ADJD", "$,", "PWAV", "APPR", "ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Tisch, der war ein Bret; mein Bette kunte gehen;", "tokens": ["Mein", "Tisch", ",", "der", "war", "ein", "Bret", ";", "mein", "Bet\u00b7te", "kun\u00b7te", "ge\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "$.", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich hatte fromen Tranck; zur Speise hatt ich stehen", "tokens": ["Ich", "hat\u00b7te", "fro\u00b7men", "Tranck", ";", "zur", "Spei\u00b7se", "hatt", "ich", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$.", "APPRART", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Kind, ein solches Kind, da\u00df, wann es ietzt geboren,", "tokens": ["Ein", "Kind", ",", "ein", "sol\u00b7ches", "Kind", ",", "da\u00df", ",", "wann", "es", "ietzt", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "PIAT", "NN", "$,", "KOUS", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Mutter dr\u00fcber singt; ich hatte mir erkoren", "tokens": ["Die", "Mut\u00b7ter", "dr\u00fc\u00b7ber", "singt", ";", "ich", "hat\u00b7te", "mir", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Platz, worauff der Grund zur Music wird ge\u00fcbet;", "tokens": ["Den", "Platz", ",", "wo\u00b7rauff", "der", "Grund", "zur", "Mu\u00b7sic", "wird", "ge\u00b7\u00fc\u00b7bet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Noch dennoch war mir wol und alles viel geliebet,", "tokens": ["Noch", "den\u00b7noch", "war", "mir", "wol", "und", "al\u00b7les", "viel", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "KON", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weil Ruh mir wolgefiel. Das zancken der Parteyen,", "tokens": ["Weil", "Ruh", "mir", "wol\u00b7ge\u00b7fiel", ".", "Das", "zan\u00b7cken", "der", "Par\u00b7te\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VMFIN", "$.", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Der \u00dcberlauff de\u00df Volcks, de\u00df Hofes Schwelgereyen,", "tokens": ["Der", "\u00dc\u00b7berl\u00b7auff", "de\u00df", "Volcks", ",", "de\u00df", "Ho\u00b7fes", "Schwel\u00b7ge\u00b7re\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "Verleumdung, Neid und Ha\u00df, Trug, Heucheley und H\u00f6hnen,", "tokens": ["Ver\u00b7leum\u00b7dung", ",", "Neid", "und", "Ha\u00df", ",", "Trug", ",", "Heu\u00b7che\u00b7ley", "und", "H\u00f6h\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die au\u00dfgeschm\u00fcckten Wort und f\u00e4lschliches besch\u00f6nen,", "tokens": ["Die", "au\u00df\u00b7ge\u00b7schm\u00fcck\u00b7ten", "Wort", "und", "f\u00e4lschli\u00b7ches", "be\u00b7sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Das hatte hier nicht stat; ich kunte seyn mein eigen", "tokens": ["Das", "hat\u00b7te", "hier"