{"dta.poem.21026": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Xc.  \n Einer andern dessen Beschaffenheit.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Die vor geschencktes Geld entbl\u00f6ste Leib und Brust/", "tokens": ["Die", "vor", "ge\u00b7schenck\u00b7tes", "Geld", "ent\u00b7bl\u00f6s\u00b7te", "Leib", "und", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht der ergrim\u0303te Tod zu des Gew\u00fcrmes Kost.", "tokens": ["Macht", "der", "er\u00b7grim\u0303te", "Tod", "zu", "des", "Ge\u00b7w\u00fcr\u00b7mes", "Kost", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Jhr Buhler last alhier die Thr\u00e4nenstr\u00f6me fliessen/", "tokens": ["Ihr", "Buh\u00b7ler", "last", "al\u00b7hier", "die", "Thr\u00e4\u00b7nen\u00b7str\u00f6\u00b7me", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So kan noch mancher Wurm bey Speise Tranck", "tokens": ["So", "kan", "noch", "man\u00b7cher", "Wurm", "bey", "Spei\u00b7se", "Tranck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PIAT", "NN", "APPR", "NN"