{"dta.poem.17273": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "123.  Von der H. Martha an den   Po-  \n lypragmon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.85", "en:0.14"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der HErr spricht Eins ist noth; und was die Martha", "tokens": ["Der", "Herr", "spricht", "Eins", "ist", "noth", ";", "und", "was", "die", "Mar\u00b7tha"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "VAFIN", "NN", "$.", "KON", "PWS", "ART", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "thut/", "tokens": ["thut", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Das ist auch an sich selbst gar l\u00f6blich/ fein/ und gutt:", "tokens": ["Das", "ist", "auch", "an", "sich", "selbst", "gar", "l\u00f6b\u00b7lich", "/", "fein", "/", "und", "gutt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "APPR", "PRF", "ADV", "ADV", "ADJD", "$(", "ADJD", "$(", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dennoch strafft Er sie. Merks ", "tokens": ["Und", "den\u00b7noch", "strafft", "Er", "sie", ".", "Merks"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "$.", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Da\u00df man mit vielerley sich nicht zerr\u00fctten sol.", "tokens": ["Da\u00df", "man", "mit", "vie\u00b7ler\u00b7ley", "sich