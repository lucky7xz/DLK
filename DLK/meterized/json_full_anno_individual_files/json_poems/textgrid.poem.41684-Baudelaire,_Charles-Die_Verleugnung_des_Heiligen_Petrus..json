{"textgrid.poem.41684": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "Die Verleugnung des Heiligen Petrus.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was macht Gott Vater mit der Flut von L\u00e4sterungen,", "tokens": ["Was", "macht", "Gott", "Va\u00b7ter", "mit", "der", "Flut", "von", "L\u00e4s\u00b7te\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "NN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Tag f\u00fcr Tag sich auf zu seinen Engeln schwingt?", "tokens": ["Die", "Tag", "f\u00fcr", "Tag", "sich", "auf", "zu", "sei\u00b7nen", "En\u00b7geln", "schwingt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PRF", "APPR", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ruht er wie ein Tyrann, den Fleisch und Wein bezwingt,", "tokens": ["Ruht", "er", "wie", "ein", "Ty\u00b7rann", ",", "den", "Fleisch", "und", "Wein", "be\u00b7zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von unsrer Fl\u00fcche Klang in sanften Schlaf gesungen?", "tokens": ["Von", "uns\u00b7rer", "Fl\u00fc\u00b7che", "Klang", "in", "sanf\u00b7ten", "Schlaf", "ge\u00b7sun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Dulder Schluchzen und der Schrei der Opfer schwillt", "tokens": ["Der", "Dul\u00b7der", "Schluch\u00b7zen", "und", "der", "Schrei", "der", "Op\u00b7fer", "schwillt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohl zu berauschender Musik erw\u00fcnschter Qualen,", "tokens": ["Wohl", "zu", "be\u00b7rau\u00b7schen\u00b7der", "Mu\u00b7sik", "er\u00b7w\u00fcnschter", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+--+---+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Denn trotz dem Blut, mit dem sie diese Lust bezahlen,", "tokens": ["Denn", "trotz", "dem", "Blut", ",", "mit", "dem", "sie", "die\u00b7se", "Lust", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist noch der Himmel nicht ges\u00e4ttigt und gestillt.", "tokens": ["Ist", "noch", "der", "Him\u00b7mel", "nicht", "ge\u00b7s\u00e4t\u00b7tigt", "und", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKNEG", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O Jesus! Denke an des \u00d6lbergs bittre Klagen,", "tokens": ["O", "Je\u00b7sus", "!", "Den\u00b7ke", "an", "des", "\u00d6l\u00b7bergs", "bitt\u00b7re", "Kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da, als du kindlich Ihn auf Knien angefleht,", "tokens": ["Da", ",", "als", "du", "kind\u00b7lich", "Ihn", "auf", "Kni\u00b7en", "an\u00b7ge\u00b7fleht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der bei der N\u00e4gel Klang sich lachend weggedreht,", "tokens": ["Der", "bei", "der", "N\u00e4\u00b7gel", "Klang", "sich", "la\u00b7chend", "weg\u00b7ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die niedre Henker in dein zuckend Fleisch geschlagen.", "tokens": ["Die", "nied\u00b7re", "Hen\u00b7ker", "in", "dein", "zu\u00b7ckend", "Fleisch", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Als deine G\u00f6ttlichkeit bespien ward und entweiht", "tokens": ["Als", "dei\u00b7ne", "G\u00f6tt\u00b7lich\u00b7keit", "be\u00b7spi\u00b7en", "ward", "und", "ent\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Vom niedren Kriegsvolk und vom Auswurf roher Buben,", "tokens": ["Vom", "nied\u00b7ren", "Kriegs\u00b7volk", "und", "vom", "Aus\u00b7wurf", "ro\u00b7her", "Bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als du gef\u00fchlt, wie tief die Dornen sich dir gruben", "tokens": ["Als", "du", "ge\u00b7f\u00fchlt", ",", "wie", "tief", "die", "Dor\u00b7nen", "sich", "dir", "gru\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PWAV", "ADJD", "ART", "NN", "PRF", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ins Haupt, in dem gewohnt der ganzen Menschheit Leid,", "tokens": ["Ins", "Haupt", ",", "in", "dem", "ge\u00b7wohnt", "der", "gan\u00b7zen", "Menschheit", "Leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "ADJD", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Als dein gebrochner Leib mit schwerer Last die Arme", "tokens": ["Als", "dein", "ge\u00b7broch\u00b7ner", "Leib", "mit", "schwe\u00b7rer", "Last", "die", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir grauenhaft gedehnt, und als entsetzlich dann", "tokens": ["Dir", "grau\u00b7en\u00b7haft", "ge\u00b7dehnt", ",", "und", "als", "ent\u00b7setz\u00b7lich", "dann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVPP", "$,", "KON", "KOUS", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dir Blut und Schwei\u00df herab von bleicher Stirne rann,", "tokens": ["Dir", "Blut", "und", "Schwei\u00df", "her\u00b7ab", "von", "blei\u00b7cher", "Stir\u00b7ne", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als eine Zielscheib du hingst vor der L\u00e4strer Schwarme,", "tokens": ["Als", "ei\u00b7ne", "Ziel\u00b7scheib", "du", "hingst", "vor", "der", "L\u00e4st\u00b7rer", "Schwar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gedachtest tr\u00e4umend du an jenen lichten Tag,", "tokens": ["Ge\u00b7dach\u00b7test", "tr\u00e4u\u00b7mend", "du", "an", "je\u00b7nen", "lich\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da zur Erf\u00fcllung des Versprechens froh du schrittest,", "tokens": ["Da", "zur", "Er\u00b7f\u00fcl\u00b7lung", "des", "Ver\u00b7spre\u00b7chens", "froh", "du", "schrit\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Da auf der Eselin, der s\u00e4nftlichen, du rittest", "tokens": ["Da", "auf", "der", "E\u00b7se\u00b7lin", ",", "der", "s\u00e4nft\u00b7li\u00b7chen", ",", "du", "rit\u00b7test"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "PRELS", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Weg, der voll Gezweig und reichen Bl\u00fcten lag.", "tokens": ["Den", "Weg", ",", "der", "voll", "Ge\u00b7zweig", "und", "rei\u00b7chen", "Bl\u00fc\u00b7ten", "lag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Da ganz das Herz erf\u00fcllt von Mut und Hoffnungsglanze", "tokens": ["Da", "ganz", "das", "Herz", "er\u00b7f\u00fcllt", "von", "Mut", "und", "Hoff\u00b7nungs\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die H\u00e4ndler du gest\u00e4upt in g\u00f6ttlichem Gericht,", "tokens": ["Die", "H\u00e4nd\u00b7ler", "du", "ge\u00b7st\u00e4upt", "in", "g\u00f6tt\u00b7li\u00b7chem", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da endlich Herr du warst! ... Drang denn die Reue nicht", "tokens": ["Da", "end\u00b7lich", "Herr", "du", "warst", "!", "...", "Drang", "denn", "die", "Reu\u00b7e", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "NN", "PPER", "VAFIN", "$.", "$(", "NN", "KON", "ART", "NN", "PTKNEG"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dir in die Seite ein noch vor dem Stich der Lanze?", "tokens": ["Dir", "in", "die", "Sei\u00b7te", "ein", "noch", "vor", "dem", "Stich", "der", "Lan\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich, wahrlich, fliehe gern dies irdische Geschlecht,", "tokens": ["Ich", ",", "wahr\u00b7lich", ",", "flie\u00b7he", "gern", "dies", "ir\u00b7di\u00b7sche", "Ge\u00b7schlecht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "$,", "VVFIN", "ADV", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Traum und Handlung nicht gleichw\u00e4gt in Schwesterh\u00e4nden,", "tokens": ["Wo", "Traum", "und", "Hand\u00b7lung", "nicht", "gleich\u00b7w\u00e4gt", "in", "Schwes\u00b7ter\u00b7h\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PTKNEG", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "D\u00fcrft ich den Degen ziehn und durch den Degen enden!", "tokens": ["D\u00fcrft", "ich", "den", "De\u00b7gen", "ziehn", "und", "durch", "den", "De\u00b7gen", "en\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "KON", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Petrus verleugnete den Herren \u2013 er tat