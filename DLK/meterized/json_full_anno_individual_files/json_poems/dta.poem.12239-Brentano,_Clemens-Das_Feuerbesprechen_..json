{"dta.poem.12239": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Das Feuerbesprechen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zigeuner sieben von Reitern gebracht,               ", "tokens": ["Zi\u00b7geu\u00b7ner", "sie\u00b7ben", "von", "Rei\u00b7tern", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gerichtet verurtheilt in einer Nacht,", "tokens": ["Ge\u00b7rich\u00b7tet", "ver\u00b7urt\u00b7heilt", "in", "ei\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie klagen um ihre Unschuld laut,", "tokens": ["Sie", "kla\u00b7gen", "um", "ih\u00b7re", "Un\u00b7schuld", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Jud h\u00e4t ihnen den Kelch vertraut.", "tokens": ["Ein", "Jud", "h\u00e4t", "ih\u00b7nen", "den", "Kelch", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Rathsherrn sprechen das Leben leicht ab", "tokens": ["Die", "Raths\u00b7herrn", "spre\u00b7chen", "das", "Le\u00b7ben", "leicht", "ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie brachen dem sechsten schon den Stab,", "tokens": ["Sie", "bra\u00b7chen", "dem", "sechs\u00b7ten", "schon", "den", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der siebent ihr K\u00f6nig sprach da mit Ruh:", "tokens": ["Der", "sie\u00b7bent", "ihr", "K\u00f6\u00b7nig", "sprach", "da", "mit", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "\u201eich h\u00f6r' wohl in L\u00fcften den V\u00f6geln zu!", "tokens": ["\u201e", "ich", "h\u00f6r'", "wohl", "in", "L\u00fcf\u00b7ten", "den", "V\u00f6\u00b7geln", "zu", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "\u201eihr sollt mir nicht sengen ein H\u00e4rlein vom Kleid,", "tokens": ["\u201e", "ihr", "sollt", "mir", "nicht", "sen\u00b7gen", "ein", "H\u00e4r\u00b7lein", "vom", "Kleid", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "\u201ebald kr\u00e4het der rothe Hahn so weit!\u201c", "tokens": ["\u201e", "bald", "kr\u00e4\u00b7het", "der", "ro\u00b7the", "Hahn", "so", "weit", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da bricht die Flamme wohl \u00fcber wohl aus,", "tokens": ["Da", "bricht", "die", "Flam\u00b7me", "wohl", "\u00fc\u00b7ber", "wohl", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+---", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus allen vier Ecken der Stadt so kraus.", "tokens": ["Aus", "al\u00b7len", "vier", "E\u00b7cken", "der", "Stadt", "so", "kraus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "CARD", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Der rothe Hahn auf die Spitze gesteckt,", "tokens": ["Der", "ro\u00b7the", "Hahn", "auf", "die", "Spit\u00b7ze", "ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er kr\u00e4het, wie jener, der Petrum erweckt,", "tokens": ["Er", "kr\u00e4\u00b7het", ",", "wie", "je\u00b7ner", ",", "der", "Pet\u00b7rum", "er\u00b7weckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PDS", "$,", "ART", "NE", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Herren erwachen aus S\u00fcnden Schlaf,", "tokens": ["Die", "Her\u00b7ren", "er\u00b7wa\u00b7chen", "aus", "S\u00fcn\u00b7den", "Schlaf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Gedenke, der Unschuld, der harten Straf.", "tokens": ["Ge\u00b7den\u00b7ke", ",", "der", "Un\u00b7schuld", ",", "der", "har\u00b7ten", "Straf", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Die Herren sie sprechen zum Manne mit Flehn,", "tokens": ["Die", "Her\u00b7ren", "sie", "spre\u00b7chen", "zum", "Man\u00b7ne", "mit", "Flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "APPR", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Er m\u00f6ge besprechen das feurige Wehn,", "tokens": ["Er", "m\u00f6\u00b7ge", "be\u00b7spre\u00b7chen", "das", "feu\u00b7ri\u00b7ge", "Wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er m\u00f6ge halten den feurigen Wind,", "tokens": ["Er", "m\u00f6\u00b7ge", "hal\u00b7ten", "den", "feu\u00b7ri\u00b7gen", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sein Leben sie wollten ihm schenken geschwind.", "tokens": ["Sein", "Le\u00b7ben", "sie", "woll\u00b7ten", "ihm", "schen\u00b7ken", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VMFIN", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Den Todesstab da entreist er gleich,", "tokens": ["Den", "To\u00b7des\u00b7stab", "da", "en\u00b7treist", "er", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Den Herren damit giebt Backenstreich,", "tokens": ["Den", "Her\u00b7ren", "da\u00b7mit", "giebt", "Ba\u00b7cken\u00b7streich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er ruft: \u201eWas gie\u00dfet ihr schuldlos Blut?", "tokens": ["Er", "ruft", ":", "\u201e", "Was", "gie\u00b7\u00dfet", "ihr", "schuld\u00b7los", "Blut", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWS", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "\u201ewie wollet ihr l\u00f6schen die h\u00f6llische Glut?", "tokens": ["\u201e", "wie", "wol\u00b7let", "ihr", "l\u00f6\u00b7schen", "die", "h\u00f6l\u00b7li\u00b7sche", "Glut", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.7": {"line.1": {"text": "\u201edas Kindlein vom Stahle die Funken gern zieht,", "tokens": ["\u201e", "das", "Kin\u00b7dlein", "vom", "Stah\u00b7le", "die", "Fun\u00b7ken", "gern", "zieht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u201eder Fromme im Steine das Feuer wohl sieht,", "tokens": ["\u201e", "der", "From\u00b7me", "im", "Stei\u00b7ne", "das", "Feu\u00b7er", "wohl", "sieht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "\u201ewas spielt ihr mit Dingen, die schneidig und spitz,", "tokens": ["\u201e", "was", "spielt", "ihr", "mit", "Din\u00b7gen", ",", "die", "schnei\u00b7dig", "und", "spitz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u201eder rothe Hahn wohl unter euch sitzt.", "tokens": ["\u201e", "der", "ro\u00b7the", "Hahn", "wohl", "un\u00b7ter", "euch", "sitzt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Jezt spricht er: \u201eWillkommen du feuriger Gast,", "tokens": ["Jezt", "spricht", "er", ":", "\u201e", "Will\u00b7kom\u00b7men", "du", "feu\u00b7ri\u00b7ger", "Gast", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "\u201enichts greife weiter, als was du hast,", "tokens": ["\u201e", "nichts", "grei\u00b7fe", "wei\u00b7ter", ",", "als", "was", "du", "hast", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VVFIN", "PTKVZ", "$,", "KOUS", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u201edas sag ich dir Feuer zu deiner Bu\u00df,", "tokens": ["\u201e", "das", "sag", "ich", "dir", "Feu\u00b7er", "zu", "dei\u00b7ner", "Bu\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "\u201eim Namen Christi, des Blut hier auch flo\u00df.", "tokens": ["\u201e", "im", "Na\u00b7men", "Chris\u00b7ti", ",", "des", "Blut", "hier", "auch", "flo\u00df", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "NE", "$,", "ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "\u201eich sage dir Feuer bey Gottes Kraft,", "tokens": ["\u201e", "ich", "sa\u00b7ge", "dir", "Feu\u00b7er", "bey", "Got\u00b7tes", "Kraft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u201edie alles thut und alles schafft,", "tokens": ["\u201e", "die", "al\u00b7les", "thut", "und", "al\u00b7les", "schafft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIS", "VVFIN", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201edu wollest also stille stehn,", "tokens": ["\u201e", "du", "wol\u00b7lest", "al\u00b7so", "stil\u00b7le", "stehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ewie Christus wollt im Jordan stehn.", "tokens": ["\u201e", "wie", "Chris\u00b7tus", "wollt", "im", "Jor\u00b7dan", "stehn", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "NE", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "\u201eich sag dir Feuer, behalt dein Flamm,", "tokens": ["\u201e", "ich", "sag", "dir", "Feu\u00b7er", ",", "be\u00b7halt", "dein", "Flamm", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201ewie einst Maria die heilge Dam", "tokens": ["\u201e", "wie", "einst", "Ma\u00b7ria", "die", "heil\u00b7ge", "Dam"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ADV", "NE", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201ehielt Jungfrauschaft so keusch so rein,", "tokens": ["\u201e", "hielt", "Jung\u00b7frausc\u00b7haft", "so", "keusch", "so", "rein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eso stelle Flamm deine Reinigung ein.\u201c", "tokens": ["\u201e", "so", "stel\u00b7le", "Flamm", "dei\u00b7ne", "Rei\u00b7ni\u00b7gung", "ein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Da flog der rothe Hahn hinweg,", "tokens": ["Da", "flog", "der", "ro\u00b7the", "Hahn", "hin\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da nahm der Wind den andern Weg,", "tokens": ["Da", "nahm", "der", "Wind", "den", "an\u00b7dern", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Feuer sank in sich zusamm,", "tokens": ["Das", "Feu\u00b7er", "sank", "in", "sich", "zu\u00b7samm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Wundermann ging fort durch die Flamm.", "tokens": ["Der", "Wun\u00b7der\u00b7mann", "ging", "fort", "durch", "die", "Flamm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}