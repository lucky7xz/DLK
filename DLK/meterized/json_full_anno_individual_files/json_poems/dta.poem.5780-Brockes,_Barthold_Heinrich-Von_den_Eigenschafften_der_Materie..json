{"dta.poem.5780": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf gleiche Weis' ersetzet sich auch immer", "tokens": ["Auf", "glei\u00b7che", "Weis'", "er\u00b7set\u00b7zet", "sich", "auch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ADV", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Abgang unsrer Welt durch diese kleine Tr\u00fcmmer.", "tokens": ["Der", "Ab\u00b7gang", "uns\u00b7rer", "Welt", "durch", "die\u00b7se", "klei\u00b7ne", "Tr\u00fcm\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die zarte C\u00f6rperlein, die sich vom C\u00f6rper trennen,", "tokens": ["Die", "zar\u00b7te", "C\u00f6r\u00b7per\u00b7lein", ",", "die", "sich", "vom", "C\u00f6r\u00b7per", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Indem sie in der Lufft theils fliehn, theils mit der Erden", "tokens": ["In\u00b7dem", "sie", "in", "der", "Lufft", "theils", "fliehn", ",", "theils", "mit", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VVINF", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vermischet werden,", "tokens": ["Ver\u00b7mi\u00b7schet", "wer\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Verlieren sich-verirren sich auch nimmer.", "tokens": ["Ver\u00b7lie\u00b7ren", "sich\u00b7ver\u00b7ir\u00b7ren", "sich", "auch", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Indem sie wieder durch geheime G\u00e4nge rennen,", "tokens": ["In\u00b7dem", "sie", "wie\u00b7der", "durch", "ge\u00b7hei\u00b7me", "G\u00e4n\u00b7ge", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sich an jedem Ort,", "tokens": ["Und", "sich", "an", "je\u00b7dem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Bald hier, bald dort,", "tokens": ["Bald", "hier", ",", "bald", "dort", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Verschiedne Oeffnungen und Th\u00fcren machen k\u00f6nnen;", "tokens": ["Ver\u00b7schied\u00b7ne", "Oeff\u00b7nun\u00b7gen", "und", "Th\u00fc\u00b7ren", "ma\u00b7chen", "k\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So zeugen Dinge, die nach unserm Augen-Schein", "tokens": ["So", "zeu\u00b7gen", "Din\u00b7ge", ",", "die", "nach", "un\u00b7serm", "Au\u00b7gen\u00b7Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Verst\u00f6hret seyn,", "tokens": ["Ver\u00b7st\u00f6h\u00b7ret", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Aus ihren C\u00f6rpern, die so klein,", "tokens": ["Aus", "ih\u00b7ren", "C\u00f6r\u00b7pern", ",", "die", "so", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vereinigungen, welche neu", "tokens": ["Ver\u00b7ei\u00b7ni\u00b7gun\u00b7gen", ",", "wel\u00b7che", "neu"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und mancherley.", "tokens": ["Und", "man\u00b7cher\u00b7ley", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "PIS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Selbst die Natur, die wir nicht sehn,", "tokens": ["Selbst", "die", "Na\u00b7tur", ",", "die", "wir", "nicht", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Erh\u00e4lt ihr Werck. Ganz neue Ding\u2019 entstehn.", "tokens": ["Er\u00b7h\u00e4lt", "ihr", "Werck", ".", "Ganz", "neu\u00b7e", "Ding'", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}