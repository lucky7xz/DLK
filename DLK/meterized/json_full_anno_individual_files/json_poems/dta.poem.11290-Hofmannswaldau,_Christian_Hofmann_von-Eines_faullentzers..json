{"dta.poem.11290": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines faullentzers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer fault die faule haut, und kan sich in dem liegen,", "tokens": ["Hjer", "fault", "die", "fau\u00b7le", "haut", ",", "und", "kan", "sich", "in", "dem", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "VVFIN", "$,", "KON", "VMFIN", "PRF", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wie vor auf der welt, nach hertzens-lust vergn\u00fcgen;", "tokens": ["Als", "wie", "vor", "auf", "der", "welt", ",", "nach", "hert\u00b7zens\u00b7lust", "ver\u00b7gn\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "APPR", "ART", "NN", "$,", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch f\u00fcrchtet sie sich sehr bey diesem m\u00fc\u00dfig-gehn,", "tokens": ["Doch", "f\u00fcrch\u00b7tet", "sie", "sich", "sehr", "bey", "die\u00b7sem", "m\u00fc\u00b7\u00dfig\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie ins k\u00fcnfftige wird sollen auferstehn.", "tokens": ["Da\u00df", "sie", "ins", "k\u00fcnff\u00b7ti\u00b7ge", "wird", "sol\u00b7len", "auf\u00b7er\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "VAFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}