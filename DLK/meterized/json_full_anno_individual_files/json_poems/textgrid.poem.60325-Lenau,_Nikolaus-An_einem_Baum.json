{"textgrid.poem.60325": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An einem Baum", "genre": "verse", "period": "N.A.", "pub_year": 1835, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Baum, so morsch und lebensarm,", "tokens": ["Du", "Baum", ",", "so", "morsch", "und", "le\u00b7ben\u00b7sarm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So ausgeh\u00f6hlt, sei mir gegr\u00fc\u00dft;", "tokens": ["So", "aus\u00b7ge\u00b7h\u00f6hlt", ",", "sei", "mir", "ge\u00b7gr\u00fc\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie doch dein froher Bienenschwarm", "tokens": ["Wie", "doch", "dein", "fro\u00b7her", "Bie\u00b7nen\u00b7schwarm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Todeswunde dir vers\u00fc\u00dft!", "tokens": ["Die", "To\u00b7des\u00b7wun\u00b7de", "dir", "ver\u00b7s\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie wandern fort im raschen Zug,", "tokens": ["Sie", "wan\u00b7dern", "fort", "im", "ra\u00b7schen", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie kehren summend wieder heim", "tokens": ["Sie", "keh\u00b7ren", "sum\u00