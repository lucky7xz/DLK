{"dta.poem.12042": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du zitterst vor der Nacht und bebest vor dem Tage,", "tokens": ["Du", "zit\u00b7terst", "vor", "der", "Nacht", "und", "be\u00b7best", "vor", "dem", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Solang dein Gl\u00fcck du hast in einer \u00e4u\u00dfern Lage.", "tokens": ["So\u00b7lang", "dein", "Gl\u00fcck", "du", "hast", "in", "ei\u00b7ner", "\u00e4u\u00b7\u00dfern", "La\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn jede Nacht kann es mit einem Sto\u00df zerr\u00fctten,", "tokens": ["Denn", "je\u00b7de", "Nacht", "kann", "es", "mit", "ei\u00b7nem", "Sto\u00df", "zer\u00b7r\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es jeder neue Tag mit einem Sturz versch\u00fctten.", "tokens": ["Es", "je\u00b7der", "neu\u00b7e", "Tag", "mit", "ei\u00b7nem", "Sturz", "ver\u00b7sch\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nur wenn du's innen hast, kanns nicht von au\u00dfen schwinden;", "tokens": ["Nur", "wenn", "du's", "in\u00b7nen", "hast", ",", "kanns", "nicht", "von", "au\u00b7\u00dfen", "schwin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADV", "VAFIN", "$,", "VMFIN", "PTKNEG", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Gl\u00fcck wird sich als Gl\u00fcck in jeder Lage finden.", "tokens": ["Dein", "Gl\u00fcck", "wird", "sich", "als", "Gl\u00fcck", "in", "je\u00b7der", "La\u00b7ge", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "KOUS", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}