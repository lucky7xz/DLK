{"textgrid.poem.59340": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein zart Geheimnis webt in stillen R\u00e4umen,", "tokens": ["Ein", "zart", "Ge\u00b7heim\u00b7nis", "webt", "in", "stil\u00b7len", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Erde l\u00f6st die diamantnen Schleifen,", "tokens": ["Die", "Er\u00b7de", "l\u00f6st", "die", "di\u00b7a\u00b7mant\u00b7nen", "Schlei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nach des Himmels s\u00fc\u00dfen Strahlen greifen", "tokens": ["Und", "nach", "des", "Him\u00b7mels", "s\u00fc\u00b7\u00dfen", "Strah\u00b7len", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Blumen, die der Mutter Kleid bes\u00e4umen.", "tokens": ["Die", "Blu\u00b7men", ",", "die", "der", "Mut\u00b7ter", "Kleid", "be\u00b7s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da rauscht's lebendig drau\u00dfen in den B\u00e4umen,", "tokens": ["Da", "rauscht's", "le\u00b7ben\u00b7dig", "drau\u00b7\u00dfen", "in", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Osten langen purpurrote Streifen,", "tokens": ["Aus", "Os\u00b7ten", "lan\u00b7gen", "pur\u00b7pur\u00b7ro\u00b7te", "Strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hoch Lerchenlieder durch das Zwielicht schweifen \u2013", "tokens": ["Hoch", "Ler\u00b7chen\u00b7lie\u00b7der", "durch", "das", "Zwie\u00b7licht", "schwei\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du hebst das bl\u00fchnde K\u00f6pfchen hold aus Tr\u00e4umen.", "tokens": ["Du", "hebst", "das", "bl\u00fchn\u00b7de", "K\u00f6pf\u00b7chen", "hold", "aus", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was sind's f\u00fcr Kl\u00e4nge, die ans Fenster flogen?", "tokens": ["Was", "sin\u00b7d's", "f\u00fcr", "Kl\u00e4n\u00b7ge", ",", "die", "ans", "Fens\u00b7ter", "flo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So altbekannt verlocken diese Lieder,", "tokens": ["So", "alt\u00b7be\u00b7kannt", "ver\u00b7lo\u00b7cken", "die\u00b7se", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein S\u00e4nger steht im schwanken D\u00e4mmerschein.", "tokens": ["Ein", "S\u00e4n\u00b7ger", "steht", "im", "schwan\u00b7ken", "D\u00e4m\u00b7mer\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wach auf! Dein Liebster ist fernher gezogen,", "tokens": ["Wach", "auf", "!", "Dein", "Liebs\u00b7ter", "ist", "fern\u00b7her", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Fr\u00fchling ist's auf Tal und Bergen wieder,", "tokens": ["Und", "Fr\u00fch\u00b7ling", "ist's", "auf", "Tal", "und", "Ber\u00b7gen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wach auf, wach auf, nun bist du ewig mein!", "tokens": ["Wach", "auf", ",", "wach", "auf", ",", "nun", "bist", "du", "e\u00b7wig", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein zart Geheimnis webt in stillen R\u00e4umen,", "tokens": ["Ein", "zart", "Ge\u00b7heim\u00b7nis", "webt", "in", "stil\u00b7len", "R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word