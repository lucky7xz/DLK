{"textgrid.poem.38611": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam wandelt dein Freund im Fr\u00fchlingsgarten,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam wandelt dein Freund im Fr\u00fchlingsgarten,", "tokens": ["Ein\u00b7sam", "wan\u00b7delt", "dein", "Freund", "im", "Fr\u00fch\u00b7lings\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Mild vom lieblichen Zauberlicht umflossen,", "tokens": ["Mild", "vom", "lieb\u00b7li\u00b7chen", "Zau\u00b7ber\u00b7licht", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Das durch wankende Bl\u00fcthenzweige zittert,", "tokens": ["Das", "durch", "wan\u00b7ken\u00b7de", "Bl\u00fc\u00b7then\u00b7zwei\u00b7ge", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Adelaide!", "tokens": ["A\u00b7de\u00b7lai\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "In der spiegelnden Fluth, im Schnee der Alpen,", "tokens": ["In", "der", "spie\u00b7geln\u00b7den", "Fluth", ",", "im", "Schnee", "der", "Al\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter":