{"textgrid.poem.40457": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Zavelstein", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleine Burg f\u00fcr wenig Mannen,", "tokens": ["Klei\u00b7ne", "Burg", "f\u00fcr", "we\u00b7nig", "Man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4dtlein ru\u00dfig, eng und schmal,", "tokens": ["St\u00e4dt\u00b7lein", "ru\u00b7\u00dfig", ",", "eng", "und", "schmal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rings des Schwarzwalds Edeltannen,", "tokens": ["Rings", "des", "Schwarz\u00b7walds", "E\u00b7del\u00b7tan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unten tief das Teinachtal \u2013", "tokens": ["Un\u00b7ten", "tief", "das", "Tei\u00b7nach\u00b7tal", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rauhe L\u00fcfte, Wolkenfl\u00fcge,", "tokens": ["Rau\u00b7he", "L\u00fcf\u00b7te", ",", "Wol\u00b7ken\u00b7fl\u00fc\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schneegest\u00f6ber, Sonnenschein:", "tokens": ["Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", ",", "Son\u00b7nen\u00b7schein", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Also wandernd im Aprilis,", "tokens": ["Al\u00b7so", "wan\u00b7dernd", "im", "Ap\u00b7ri\u00b7lis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Schaut' ich einst den Zavelstein.", "tokens": ["Schaut'", "ich", "einst", "den", "Za\u00b7vel\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nie von Ri\u00df und Sprung gen\u00f6tet", "tokens": ["Nie", "von", "Ri\u00df", "und", "Sprung", "ge\u00b7n\u00f6\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ragt sein schlanker R\u00f6merturm", "tokens": ["Ragt", "sein", "schlan\u00b7ker", "R\u00f6\u00b7mer\u00b7turm"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie gegossen und gel\u00f6tet", "tokens": ["Wie", "ge\u00b7gos\u00b7sen", "und", "ge\u00b7l\u00f6\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Quaderfest im Zeitensturm ...", "tokens": ["Qua\u00b7der\u00b7fest", "im", "Zei\u00b7ten\u00b7sturm", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "... Ruhsam stund der Ortsbewohner", "tokens": ["...", "Ruh\u00b7sam", "stund", "der", "Orts\u00b7be\u00b7woh\u00b7ner"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor dem Haus im Sonntagskleid,", "tokens": ["Vor", "dem", "Haus", "im", "Sonn\u00b7tags\u00b7kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auch der Burghof pflag der Ruhe", "tokens": ["Auch", "der", "Burg\u00b7hof", "pflag", "der", "Ru\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Winterschl\u00e4frig, tief verschneit.", "tokens": ["Win\u00b7ter\u00b7schl\u00e4f\u00b7rig", ",", "tief", "ver\u00b7schneit", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber ostw\u00e4rts auf den Halden", "tokens": ["A\u00b7ber", "ost\u00b7w\u00e4rts", "auf", "den", "Hal\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weicht besiegt der Schneelast Druck,", "tokens": ["Weicht", "be\u00b7siegt", "der", "Schnee\u00b7last", "Druck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Kelche hoch entfalten", "tokens": ["Sei\u00b7ne", "Kel\u00b7che", "hoch", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ein wilder Bl\u00fctenschmuck,", "tokens": ["Will", "ein", "wil\u00b7der", "Bl\u00fc\u00b7ten\u00b7schmuck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und im Schmelz der Farbent\u00f6ne", "tokens": ["Und", "im", "Schmelz", "der", "Far\u00b7ben\u00b7t\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dunkelviolett bis wei\u00df", "tokens": ["Dun\u00b7kel\u00b7vi\u00b7o\u00b7lett", "bis", "wei\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dr\u00e4ngt sich fremde Purpursch\u00f6ne", "tokens": ["Dr\u00e4ngt", "sich", "frem\u00b7de", "Pur\u00b7pur\u00b7sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcppigst wuchernd aus dem Eis.", "tokens": ["\u00dcp\u00b7pigst", "wu\u00b7chernd", "aus", "dem", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Seltner Gast auf Schwabens Flur,", "tokens": ["Selt\u00b7ner", "Gast", "auf", "Schwa\u00b7bens", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeugnis ewig jungen Fr\u00fchlings", "tokens": ["Zeug\u00b7nis", "e\u00b7wig", "jun\u00b7gen", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und uralter Weltkultur;", "tokens": ["Und", "ur\u00b7al\u00b7ter", "Welt\u00b7kul\u00b7tur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wo itzt Flocken niederwirbeln", "tokens": ["Wo", "itzt", "Flo\u00b7cken", "nie\u00b7der\u00b7wir\u00b7beln"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf die wohldurchbl\u00fcmte Au',", "tokens": ["Auf", "die", "wohl\u00b7durch\u00b7bl\u00fcm\u00b7te", "Au'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Pflanzte einst ihr Saffrang\u00e4rtlein", "tokens": ["Pflanz\u00b7te", "einst", "ihr", "Saf\u00b7fran\u00b7g\u00e4rt\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Eine kluge R\u00f6merfrau.", "tokens": ["Ei\u00b7ne", "klu\u00b7ge", "R\u00f6\u00b7mer\u00b7frau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Saft den S\u00fcpplein ihrer K\u00fcche,", "tokens": ["Saft", "den", "S\u00fcp\u00b7plein", "ih\u00b7rer", "K\u00fc\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herzarznei f\u00fcr b\u00f6se Sucht,", "tokens": ["Herz\u00b7arz\u00b7nei", "f\u00fcr", "b\u00f6\u00b7se", "Sucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunkeln Locken Wohlger\u00fcche", "tokens": ["Dun\u00b7keln", "Lo\u00b7cken", "Wohl\u00b7ge\u00b7r\u00fc\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog sie aus der edeln Frucht.", "tokens": ["Zog", "sie", "aus", "der", "e\u00b7deln", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und im Anhauch dieser Blume", "tokens": ["Und", "im", "An\u00b7hauch", "die\u00b7ser", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schritt sie, wenn der Fr\u00fchling nah,", "tokens": ["Schritt", "sie", ",", "wenn", "der", "Fr\u00fch\u00b7ling", "nah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Opfernd zu dem Heiligtume", "tokens": ["Op\u00b7fernd", "zu", "dem", "Hei\u00b7lig\u00b7tu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der Diana Abnoba.", "tokens": ["Der", "Di\u00b7a\u00b7na", "Ab\u00b7no\u00b7ba", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Kleine Burg f\u00fcr wenig Mannen,", "tokens": ["Klei\u00b7ne", "Burg", "f\u00fcr", "we\u00b7nig", "Man\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4dtlein ru\u00dfig, eng und schmal,", "tokens": ["St\u00e4dt\u00b7lein", "ru\u00b7\u00dfig", ",", "