{"dta.poem.3316": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Dichtkunst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe Feld und Bach, der Sonne Morgenstrahl,", "tokens": ["Ich", "lie\u00b7be", "Feld", "und", "Bach", ",", "der", "Son\u00b7ne", "Mor\u00b7gen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NE", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein schwarzbeschattet einsam Thal,", "tokens": ["Ein", "schwarz\u00b7be\u00b7schat\u00b7tet", "ein\u00b7sam", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jenen stillen Lorbeer-Wald,", "tokens": ["Und", "je\u00b7nen", "stil\u00b7len", "Lor\u00b7beer\u00b7Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo keuscher Musen Fl\u00f6te schallt.", "tokens": ["Wo", "keu\u00b7scher", "Mu\u00b7sen", "Fl\u00f6\u00b7te", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich mische mich in ihre Ch\u00f6re;", "tokens": ["Ich", "mi\u00b7sche", "mich", "in", "ih\u00b7re", "Ch\u00f6\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie weihten mich zum Priester ein:", "tokens": ["Sie", "weih\u00b7ten", "mich", "zum", "Pries\u00b7ter", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sollten W\u00fcnsche mindrer Ehre", "tokens": ["Und", "soll\u00b7ten", "W\u00fcn\u00b7sche", "mind\u00b7rer", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein ruhig Herz entweihn?", "tokens": ["Mein", "ru\u00b7hig", "Herz", "ent\u00b7weihn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Entzeuch, o Dichtkunst! mir dein gl\u00e4nzend Angesicht,", "tokens": ["Ent\u00b7zeuch", ",", "o", "Dicht\u00b7kunst", "!", "mir", "dein", "gl\u00e4n\u00b7zend", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$.", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O du der Liebe Tochter! nicht:", "tokens": ["O", "du", "der", "Lie\u00b7be", "Toch\u00b7ter", "!", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NN", "$.", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn in der ersten Sch\u00e4fer-Welt,", "tokens": ["Denn", "in", "der", "ers\u00b7ten", "Sch\u00e4\u00b7fer\u00b7Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die uns im Bilde noch gef\u00e4llt,", "tokens": ["Die", "uns", "im", "Bil\u00b7de", "noch", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gebahr dem Gotte frohes Weines", "tokens": ["Ge\u00b7bahr", "dem", "Got\u00b7te", "fro\u00b7hes", "Wei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Liebe dich, ihr \u00e4hnlich Kind,", "tokens": ["Die", "Lie\u00b7be", "dich", ",", "ihr", "\u00e4hn\u00b7lich", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In dunkeln Schatten eines Haynes,", "tokens": ["In", "dun\u00b7keln", "Schat\u00b7ten", "ei\u00b7nes", "Hay\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die dir noch heilig sind.", "tokens": ["Die", "dir", "noch", "hei\u00b7lig", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie sch\u00f6n erzogen dich die Unschuld und Natur", "tokens": ["Wie", "sch\u00f6n", "er\u00b7zo\u00b7gen", "dich", "die", "Un\u00b7schuld", "und", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVPP", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf Triften und bebl\u00fchmter Flur!", "tokens": ["Auf", "Trif\u00b7ten", "und", "be\u00b7bl\u00fchm\u00b7ter", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch nicht um stolzen Schmuck bem\u00fcht,", "tokens": ["Noch", "nicht", "um", "stol\u00b7zen", "Schmuck", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ert\u00f6nte hier dein sanftes Lied.", "tokens": ["Er\u00b7t\u00f6n\u00b7te", "hier", "dein", "sanf\u00b7tes", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es h\u00f6rten die erstaunten Hirten", "tokens": ["Es", "h\u00f6r\u00b7ten", "die", "er\u00b7staun\u00b7ten", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den ungek\u00fcnstelten Gesang,", "tokens": ["Den", "un\u00b7ge\u00b7k\u00fcns\u00b7tel\u00b7ten", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der \u00f6fters um geheime Myrthen", "tokens": ["Der", "\u00f6f\u00b7ters", "um", "ge\u00b7hei\u00b7me", "Myr\u00b7then"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und oft beym Wein erklang.", "tokens": ["Und", "oft", "beym", "Wein", "er\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Weisheit bracht' alsdann dich, junge Sch\u00e4ferin\u0303!", "tokens": ["Die", "Weis\u00b7heit", "bracht'", "als\u00b7dann", "dich", ",", "jun\u00b7ge", "Sch\u00e4\u00b7fe\u00b7ri\u00f1", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum unbewohnten H\u00e4mus hin;", "tokens": ["Zum", "un\u00b7be\u00b7wohn\u00b7ten", "H\u00e4\u00b7mus", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und lehrte dich der Dinge Grund,", "tokens": ["Und", "lehr\u00b7te", "dich", "der", "Din\u00b7ge", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie das Weltgeb\u00e4ud entstund:", "tokens": ["Und", "wie", "das", "Welt\u00b7ge\u00b7b\u00e4ud", "ent\u00b7stund", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Warum der Fr\u00fchling gr\u00fcne H\u00fcgel", "tokens": ["Wa\u00b7rum", "der", "Fr\u00fch\u00b7ling", "gr\u00fc\u00b7ne", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und lauen West und Floren liebt,", "tokens": ["Und", "lau\u00b7en", "West", "und", "Flo\u00b7ren", "liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und was den Winden ihre Fl\u00fcgel,", "tokens": ["Und", "was", "den", "Win\u00b7den", "ih\u00b7re", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Donner Kr\u00e4fte giebt.", "tokens": ["Dem", "Don\u00b7ner", "Kr\u00e4f\u00b7te", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du lerntest, wer mit Recht hoch oder niedrig hei\u00dft!", "tokens": ["Du", "lern\u00b7test", ",", "wer", "mit", "Recht", "hoch", "o\u00b7der", "nied\u00b7rig", "hei\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "APPR", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns adelt nur ein edler Geist,", "tokens": ["Uns", "a\u00b7delt", "nur", "ein", "ed\u00b7ler", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nicht ein schimmernd hoher Stand,", "tokens": ["Und", "nicht", "ein", "schim\u00b7mernd", "ho\u00b7her", "Stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht ein verg\u00fcldetes Gewand;", "tokens": ["Nicht", "ein", "ver\u00b7g\u00fcl\u00b7de\u00b7tes", "Ge\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch da\u00df man gro\u00df genennet werde", "tokens": ["Noch", "da\u00df", "man", "gro\u00df", "ge\u00b7nen\u00b7net", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "ADJD", "VVPP", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Lippen feiger Schmeicheley,", "tokens": ["Von", "Lip\u00b7pen", "fei\u00b7ger", "Schmei\u00b7che\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und einem Winkel weiter Erde", "tokens": ["Und", "ei\u00b7nem", "Win\u00b7kel", "wei\u00b7ter", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bekannt und furchtbar sey.", "tokens": ["Be\u00b7kannt", "und", "furcht\u00b7bar", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Aue schwieg vor dir, als du vom H\u00e4mus kamst,", "tokens": ["Die", "Au\u00b7e", "schwieg", "vor", "dir", ",", "als", "du", "vom", "H\u00e4\u00b7mus", "kamst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und eine k\u00fchnre Leyer nahmst.", "tokens": ["Und", "ei\u00b7ne", "k\u00fchn\u00b7re", "Le\u00b7yer", "nahmst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wallte junger Hirten Blut;", "tokens": ["Es", "wall\u00b7te", "jun\u00b7ger", "Hir\u00b7ten", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie f\u00fchlten ungef\u00fchlte Glut,", "tokens": ["Sie", "f\u00fchl\u00b7ten", "un\u00b7ge\u00b7f\u00fchl\u00b7te", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als nun dein h\u00f6hers Lied ert\u00f6nte,", "tokens": ["Als", "nun", "dein", "h\u00f6\u00b7hers", "Lied", "er\u00b7t\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das, reizend, wann es unterwies,", "tokens": ["Das", ",", "rei\u00b7zend", ",", "wann", "es", "un\u00b7ter\u00b7wies", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "VVPP", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von rauher Wildheit sie entw\u00f6hnte,", "tokens": ["Von", "rau\u00b7her", "Wild\u00b7heit", "sie", "ent\u00b7w\u00f6hn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Menschen werden hie\u00df.", "tokens": ["Und", "Men\u00b7schen", "wer\u00b7den", "hie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du sangst: es rissen sich bemooste Felsen los", "tokens": ["Du", "sangst", ":", "es", "ris\u00b7sen", "sich", "be\u00b7moos\u00b7te", "Fel\u00b7sen", "los"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PRF", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus drohender Gebirge Schoos,", "tokens": ["Aus", "dro\u00b7hen\u00b7der", "Ge\u00b7bir\u00b7ge", "Schoos", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und rollten fort mit eignem Lauf,", "tokens": ["Und", "roll\u00b7ten", "fort", "mit", "eig\u00b7nem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und th\u00fcrmten sich zu Mauern auf.", "tokens": ["Und", "th\u00fcrm\u00b7ten", "sich", "zu", "Mau\u00b7ern", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Tieger unter d\u00fcstern Str\u00e4uchen", "tokens": ["Die", "Tie\u00b7ger", "un\u00b7ter", "d\u00fcs\u00b7tern", "Str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Behorchten dein entz\u00fcckend Spiel;", "tokens": ["Be\u00b7horch\u00b7ten", "dein", "ent\u00b7z\u00fc\u00b7ckend", "Spiel", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und auch die unbelebten Eichen", "tokens": ["Und", "auch", "die", "un\u00b7be\u00b7leb\u