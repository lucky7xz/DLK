, "pos": ["KOUS", "ADJA", "NN", "$(", "ART", "ADJA", "NE", "VVFIN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Gehet so ohn alles Wanken/", "tokens": ["Ge\u00b7het", "so", "ohn", "al\u00b7les", "Wan\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Die trefflichen Arten und Werke/ die er schreibt/", "tokens": ["Die", "treff\u00b7li\u00b7chen", "Ar\u00b7ten", "und", "Wer\u00b7ke", "/", "die", "er", "schreibt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Vberlauffen alle Schranken/", "tokens": ["Vber\u00b7lauf\u00b7fen", "al\u00b7le", "Schran\u00b7ken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die w\u00fcrtliche Faust/ die Fertigkeit der Zungen/", "tokens": ["Die", "w\u00fcrt\u00b7li\u00b7che", "Faust", "/", "die", "Fer\u00b7tig\u00b7keit", "der", "Zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Die wakkern Geberden/ nach Redners-brauch gezwungen/", "tokens": ["Die", "wak\u00b7kern", "Ge\u00b7ber\u00b7den", "/", "nach", "Red\u00b7ner\u00b7sbrauch", "ge\u00b7zwun\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Vnd alles/ was er kl\u0