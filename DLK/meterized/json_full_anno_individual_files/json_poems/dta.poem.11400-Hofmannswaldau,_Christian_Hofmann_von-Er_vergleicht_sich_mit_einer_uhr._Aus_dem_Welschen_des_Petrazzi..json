{"dta.poem.11400": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er vergleicht sich mit einer uhr.  \n Aus dem Welschen des  Petrazzi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es mag Florette lachen!", "tokens": ["Es", "mag", "Flo\u00b7ret\u00b7te", "la\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So will der himmel doch ein uhrwerck aus mir machen,", "tokens": ["So", "will", "der", "him\u00b7mel", "doch", "ein", "uhr\u00b7werck", "aus", "mir", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo der gedancken lauff an statt der r\u00e4der ist.", "tokens": ["Wo", "der", "ge\u00b7dan\u00b7cken", "lauff", "an", "statt", "der", "r\u00e4\u00b7der", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "APPR", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein \u00e4ngstig sehnen bleibt zur unruh auserkiest,", "tokens": ["Mein", "\u00e4ngs\u00b7tig", "seh\u00b7nen", "bleibt", "zur", "un\u00b7ruh", "aus\u00b7er\u00b7kiest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVINF", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weil jenes sich, wie dieses stets, beweget:", "tokens": ["Weil", "je\u00b7nes", "sich", ",", "wie", "die\u00b7ses", "stets", ",", "be\u00b7we\u00b7get", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PDS", "PRF", "$,", "PWAV", "PDS", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein hertze soll das gl\u00f6ckgen seyn,", "tokens": ["Mein", "hert\u00b7ze", "soll", "das", "gl\u00f6ck\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ART", "ADJA", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf dem der liebe strahl und deiner blicke schein", "tokens": ["Auf", "dem", "der", "lie\u00b7be", "strahl", "und", "dei\u00b7ner", "bli\u00b7cke", "schein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die stunden und minuten schl\u00e4get.", "tokens": ["Die", "stun\u00b7den", "und", "mi\u00b7nu\u00b7ten", "schl\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Du aber, s\u00fcsser glantz! der mich gebunden f\u00fchrt,", "tokens": ["Du", "a\u00b7ber", ",", "s\u00fcs\u00b7ser", "glantz", "!", "der", "mich", "ge\u00b7bun\u00b7den", "f\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ADJA", "NN", "$.", "ART", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und meine sonne ziert,", "tokens": ["Und", "mei\u00b7ne", "son\u00b7ne", "ziert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Nach der ich so viel seusstzer sende,", "tokens": ["Nach", "der", "ich", "so", "viel", "seusst\u00b7zer", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Du bist der mittel-punct, um welchen ich mich wende.", "tokens": ["Du", "bist", "der", "mit\u00b7tel\u00b7punct", ",", "um", "wel\u00b7chen", "ich", "mich", "wen\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUI", "PWAT", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}