{"dta.poem.21070": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Geist der Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer bist du, Geist der Liebe,               ", "tokens": ["Wer", "bist", "du", ",", "Geist", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der durch das Weltall webt?", "tokens": ["Der", "durch", "das", "Wel\u00b7tall", "webt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Schooss der Erde schw\u00e4ngert,", "tokens": ["Den", "Scho\u00b7oss", "der", "Er\u00b7de", "schw\u00e4n\u00b7gert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und den Atom belebt,", "tokens": ["Und", "den", "A\u00b7tom", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Elemente bindet,", "tokens": ["Der", "E\u00b7le\u00b7men\u00b7te", "bin\u00b7det", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Der Weltenkugeln ballt,", "tokens": ["Der", "Wel\u00b7ten\u00b7ku\u00b7geln", "ballt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Aus Engelharfen jubelt,", "tokens": ["Aus", "En\u00b7gel\u00b7har\u00b7fen", "ju\u00b7belt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und aus dem S\u00e4ugling lallt?", "tokens": ["Und", "aus", "dem", "S\u00e4ug\u00b7ling", "lallt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer bist du, Kraft der Kr\u00e4fte,", "tokens": ["Wer", "bist", "du", ",", "Kraft", "der", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Greisesaugen hellt?", "tokens": ["Die", "Grei\u00b7se\u00b7sau\u00b7gen", "hellt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der J\u00fcnglingswangen r\u00f6thet,", "tokens": ["Der", "J\u00fcng\u00b7lings\u00b7wan\u00b7gen", "r\u00f6\u00b7thet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und M\u00e4dchenbusen schwellt?", "tokens": ["Und", "M\u00e4d\u00b7chen\u00b7bu\u00b7sen", "schwellt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Liebe beut und fodert,", "tokens": ["Der", "Lie\u00b7be", "beut", "und", "fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um Liebe ringt und wirbt,", "tokens": ["Um", "Lie\u00b7be", "ringt", "und", "wirbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Messiaden dichtet,", "tokens": ["Und", "Mes\u00b7si\u00b7a\u00b7den", "dich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Brutustode stirbt?", "tokens": ["Und", "Bru\u00b7tus\u00b7to\u00b7de", "stirbt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bist du nicht Odem Gottes,", "tokens": ["Bist", "du", "nicht", "O\u00b7dem", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Unstr\u00e4flich, wie sein Licht,", "tokens": ["U\u00b7nstr\u00e4f\u00b7lich", ",", "wie", "sein", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und stark, wie seine Rechte,", "tokens": ["Und", "stark", ",", "wie", "sei\u00b7ne", "Rech\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welten baut und bricht?", "tokens": ["Die", "Wel\u00b7ten", "baut", "und", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bist unsers Kreuzzugs Fahne,", "tokens": ["Bist", "un\u00b7sers", "Kreuz\u00b7zugs", "Fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Entflammst mit heilger Scham", "tokens": ["Ent\u00b7flammst", "mit", "heil\u00b7ger", "Scham"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Feigen und den Matten,", "tokens": ["Den", "Fei\u00b7gen", "und", "den", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein wehend Oriflamm.", "tokens": ["Ein", "we\u00b7hend", "O\u00b7ri\u00b7flamm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nur der ist gut und edel,", "tokens": ["Nur", "der", "ist", "gut", "und", "e\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem du den Bogen spannst.", "tokens": ["Dem", "du", "den", "Bo\u00b7gen", "spannst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur der ist gross und g\u00f6ttlich,", "tokens": ["Nur", "der", "ist", "gross", "und", "g\u00f6tt\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den du zum Mann ermannst.", "tokens": ["Den", "du", "zum", "Mann", "er\u00b7mannst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein Werk ist Pyramide,", "tokens": ["Sein", "Werk", "ist", "Py\u00b7ra\u00b7mi\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sein Wort ist Machtgebot.", "tokens": ["Sein", "Wort", "ist", "Macht\u00b7ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Spott ist ihm die H\u00f6lle,", "tokens": ["Ein", "Spott", "ist", "ihm", "die", "H\u00f6l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Hohn ist ihm der Tod", "tokens": ["Ein", "Hohn", "ist", "ihm", "der", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}