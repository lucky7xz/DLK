{"dta.poem.18082": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Das Braut-Lied.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als Filander mit grossem Lust", "tokens": ["Als", "Fi\u00b7lan\u00b7der", "mit", "gros\u00b7sem", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die lang-begehrte edle Blust", "tokens": ["Die", "lang\u00b7be\u00b7gehr\u00b7te", "ed\u00b7le", "Blust"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seiner standhafften Lieb errungen:", "tokens": ["Sei\u00b7ner", "stand\u00b7haff\u00b7ten", "Lieb", "er\u00b7run\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hat ein Hauff Liebelein gar laut", "tokens": ["Hat", "ein", "Hauff", "Lie\u00b7be\u00b7lein", "gar", "laut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Br\u00e4utigam und seiner Braut", "tokens": ["Dem", "Br\u00e4u\u00b7ti\u00b7gam", "und", "sei\u00b7ner", "Braut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Ehren dieses Lied gesungen.", "tokens": ["Zu", "Eh\u00b7ren", "die\u00b7ses", "Lied", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O da\u00df ihr m\u00f6get allezeit", "tokens": ["O", "da\u00df", "ihr", "m\u00f6\u00b7get", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einig, in keinem andern Streit,", "tokens": ["Ei\u00b7nig", ",", "in", "kei\u00b7nem", "an\u00b7dern", "Streit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dann nur in Liebes-Streite, leben,", "tokens": ["Dann", "nur", "in", "Lie\u00b7bes\u00b7Strei\u00b7te", ",", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darinnen eines jeden Hertz", "tokens": ["Da\u00b7rin\u00b7nen", "ei\u00b7nes", "je\u00b7den", "Hertz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem andern m\u00f6g Wollust und Schertz", "tokens": ["Dem", "an\u00b7dern", "m\u00f6g", "Wol\u00b7lust", "und", "Schertz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "F\u00fcr Schertz und Wollust wieder geben.", "tokens": ["F\u00fcr", "Schertz", "und", "Wol\u00b7lust", "wie\u00b7der", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch K\u00fc\u00df von s\u00fcssem Nectar feucht", "tokens": ["Durch", "K\u00fc\u00df", "von", "s\u00fcs\u00b7sem", "Nec\u00b7tar", "feucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Hertz und Seel von Freuden leicht", "tokens": ["Das", "Hertz", "und", "Seel", "von", "Freu\u00b7den", "leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Solt ihr euch nehmen und mittheilen:", "tokens": ["Solt", "ihr", "euch", "neh\u00b7men", "und", "mit\u00b7thei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jhr solt durch tiefwundende K\u00fc\u00df,", "tokens": ["Ihr", "solt", "durch", "tief\u00b7wun\u00b7den\u00b7de", "K\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Jhr solt durch s\u00fc\u00dfheilende Bi\u00df,", "tokens": ["Ihr", "solt", "durch", "s\u00fc\u00df\u00b7hei\u00b7len\u00b7de", "Bi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Verwunden euch und wieder heilen.", "tokens": ["Ver\u00b7wun\u00b7den", "euch", "und", "wie\u00b7der", "hei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Des einen Mund, soll mit Wollust", "tokens": ["Des", "ei\u00b7nen", "Mund", ",", "soll", "mit", "Wol\u00b7lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "NN", "$,", "VMFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Des andern Hertz auf seiner Brust", "tokens": ["Des", "an\u00b7dern", "Hertz", "auf", "sei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu nehmen, ihm die Brust aufspalten.", "tokens": ["Zu", "neh\u00b7men", ",", "ihm", "die", "Brust", "auf\u00b7spal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des andern Hertz soll mit dem Mund", "tokens": ["Des", "an\u00b7dern", "Hertz", "soll", "mit", "dem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch s\u00fcsse K\u00fc\u00df verwundend wund", "tokens": ["Durch", "s\u00fcs\u00b7se", "K\u00fc\u00df", "ver\u00b7wun\u00b7dend", "wund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der andern Brust sich nicht enthalten.", "tokens": ["Der", "an\u00b7dern", "Brust", "sich", "nicht", "ent\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}