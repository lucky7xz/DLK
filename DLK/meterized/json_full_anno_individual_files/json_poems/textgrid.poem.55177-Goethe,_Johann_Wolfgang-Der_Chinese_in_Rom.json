{"textgrid.poem.55177": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der Chinese in Rom", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Chinesen sah ich in Rom; die gesamten Geb\u00e4ude", "tokens": ["Ei\u00b7nen", "Chi\u00b7ne\u00b7sen", "sah", "ich", "in", "Rom", ";", "die", "ge\u00b7sam\u00b7ten", "Ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NE", "$.", "ART", "ADJA", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Alter und neuerer Zeit schienen ihm l\u00e4stig und schwer.", "tokens": ["Al\u00b7ter", "und", "neu\u00b7e\u00b7rer", "Zeit", "schie\u00b7nen", "ihm", "l\u00e4s\u00b7tig", "und", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u00bbach!\u00ab so seufzt' er, \u00bbdie Armen! ich hoffe, sie sollen begreifen,", "tokens": ["\u00bb", "ach", "!", "\u00ab", "so", "seufzt'", "er", ",", "\u00bb", "die", "Ar\u00b7men", "!", "ich", "hof\u00b7fe", ",", "sie", "sol\u00b7len", "be\u00b7grei\u00b7fen", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$.", "$(", "ADV", "VVFIN", "PPER", "$,", "$(", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Wie erst S\u00e4ulchen von Holz tragen des Daches Gezelt,", "tokens": ["Wie", "erst", "S\u00e4ul\u00b7chen", "von", "Holz", "tra\u00b7gen", "des", "Da\u00b7ches", "Ge\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Da\u00df an Latten und Pappen, Geschnitz und bunter Vergoldung", "tokens": ["Da\u00df", "an", "Lat\u00b7ten", "und", "Pap\u00b7pen", ",", "Ge\u00b7schnitz", "und", "bun\u00b7ter", "Ver\u00b7gol\u00b7dung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "$,", "NN", "KON", "ADJD", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Sich des gebildeten Augs feinerer Sinn nur erfreut.\u00ab", "tokens": ["Sich", "des", "ge\u00b7bil\u00b7de\u00b7ten", "Augs", "fei\u00b7ne\u00b7rer", "Sinn", "nur", "er\u00b7freut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "ADJA", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Siehe, da glaubt ich, im Bilde so manchen Schw\u00e4rmer zu schauen,", "tokens": ["Sie\u00b7he", ",", "da", "glaubt", "ich", ",", "im", "Bil\u00b7de", "so", "man\u00b7chen", "Schw\u00e4r\u00b7mer", "zu", "schau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADV", "VVFIN", "PPER", "$,", "APPRART", "NN", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Der sein luftig Gespinst mit der soliden Natur", "tokens": ["Der", "sein", "luf\u00b7tig", "Ge\u00b7spinst", "mit", "der", "so\u00b7li\u00b7den", "Na\u00b7tur"], "token_info": ["word", "word