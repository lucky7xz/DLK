{"dta.poem.16211": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "265.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gefragt ein Weiser: denkst du nie ans Vaterland?", "tokens": ["Ge\u00b7fragt", "ein", "Wei\u00b7ser", ":", "denkst", "du", "nie", "ans", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch, sprach er, stets! und wies zum Himmel mit der Hand.", "tokens": ["Doch", ",", "sprach", "er", ",", "stets", "!", "und", "wies", "zum", "Him\u00b7mel", "mit", "der", "Hand", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "$,", "ADV", "$.", "KON", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Weisen Vaterland ist all des Himmels Weite,", "tokens": ["Des", "Wei\u00b7sen", "Va\u00b7ter\u00b7land", "ist", "all", "des", "Him\u00b7mels", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PIAT", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und, \u00fcberw\u00f6lbt davon, der Menschheit ganze Breite.", "tokens": ["Und", ",", "\u00fc\u00b7berw\u00b7\u00f6lbt", "da\u00b7von", ",", "der", "Menschheit", "gan\u00b7ze", "Brei\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PAV", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Hat er beim Weiteren das Engere vergessen?", "tokens": ["Hat", "er", "beim", "Wei\u00b7te\u00b7ren", "das", "En\u00b7ge\u00b7re", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergisset er doch auch beim Denken nicht das Essen!", "tokens": ["Ver\u00b7gis\u00b7set", "er", "doch", "auch", "beim", "Den\u00b7ken", "nicht", "das", "Es\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch m\u00e4\u00dfig isset er, und so ermisset er", "tokens": ["Doch", "m\u00e4\u00b7\u00dfig", "is\u00b7set", "er", ",", "und", "so", "er\u00b7mis\u00b7set", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,", "KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Klein Kleines, Gro\u00dfes nicht darob vergisset er.", "tokens": ["Klein", "Klei\u00b7nes", ",", "Gro\u00b7\u00dfes", "nicht", "da\u00b7rob", "ver\u00b7gis\u00b7set", "er", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "PTKNEG", "PAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}}}}