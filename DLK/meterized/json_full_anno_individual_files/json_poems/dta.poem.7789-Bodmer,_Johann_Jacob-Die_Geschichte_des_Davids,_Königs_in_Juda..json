{"dta.poem.7789": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin dir, sagt hierauf Thalmais, hoch verbunden,", "tokens": ["Ich", "bin", "dir", ",", "sagt", "hier\u00b7auf", "Thal\u00b7mais", ",", "hoch", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "VVFIN", "PAV", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da\u00df demer Heimlichkeit du mich gew\u00fcrdigt funden,", "tokens": ["Da\u00df", "de\u00b7mer", "Heim\u00b7lich\u00b7keit", "du", "mich", "ge\u00b7w\u00fcr\u00b7digt", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "PRF", "VVPP", "VVFIN", "$,"], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Doch weil die Beroa, meins Mannes Brudern Frau,", "tokens": ["Doch", "weil", "die", "Be\u00b7roa", ",", "meins", "Man\u00b7nes", "Bru\u00b7dern", "Frau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PPOSAT", "NN", "NN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wei\u00df diese Lieb und schweigt, will ich auch, da\u00df mir trau", "tokens": ["Wei\u00df", "die\u00b7se", "Lieb", "und", "schweigt", ",", "will", "ich", "auch", ",", "da\u00df", "mir", "trau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "KON", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die so der Adriel, der tapfre Held, erwehlet,", "tokens": ["Die", "so", "der", "Ad\u00b7riel", ",", "der", "tapf\u00b7re", "Held", ",", "er\u00b7weh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Hast du schon gleich wie er mich vor der Zeit gequ\u00e4let", "tokens": ["Hast", "du", "schon", "gleich", "wie", "er", "mich", "vor", "der", "Zeit", "ge\u00b7qu\u00e4\u00b7let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "KOKOM", "PPER", "PRF", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In meiner Lieb, so ist es ohne Schuld geschehn.", "tokens": ["In", "mei\u00b7ner", "Lieb", ",", "so", "ist", "es", "oh\u00b7ne", "Schuld", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gott la\u00df dich in dem Stand, in dem ich bin, bald stehn!", "tokens": ["Gott", "la\u00df", "dich", "in", "dem", "Stand", ",", "in", "dem", "ich", "bin", ",", "bald", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das w\u00fcnschet dir mein Hertz! Der H\u00f6chste das erh\u00f6re.", "tokens": ["Das", "w\u00fcn\u00b7schet", "dir", "mein", "Hertz", "!", "Der", "H\u00f6chs\u00b7te", "das", "er\u00b7h\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "ART", "ADJA", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die andre wieder sprach: Wann es zu seiner Ehre", "tokens": ["Die", "and\u00b7re", "wie\u00b7der", "sprach", ":", "Wann", "es", "zu", "sei\u00b7ner", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVFIN", "$.", "PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und meinem Nutzen reicht. Die\u00df h\u00f6rte David an,", "tokens": ["Und", "mei\u00b7nem", "Nut\u00b7zen", "reicht", ".", "Die\u00df", "h\u00f6r\u00b7te", "Da\u00b7vid", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "PDS", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sein Hertze was empfindt, das er nicht melden kan.", "tokens": ["Sein", "Hert\u00b7ze", "was", "emp\u00b7findt", ",", "das", "er", "nicht", "mel\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PIS", "VVPP", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es liebet Adriel die eine von den beyden,", "tokens": ["Es", "lie\u00b7bet", "Ad\u00b7riel", "die", "ei\u00b7ne", "von", "den", "bey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ART", "ART", "APPR", "ART", "PIAT", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Das hat er angeh\u00f6rt, und mu\u00df es auch so leiden,", "tokens": ["Das", "hat", "er", "an\u00b7ge\u00b7h\u00f6rt", ",", "und", "mu\u00df", "es", "auch", "so", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VVPP", "$,", "KON", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und f\u00e4llt ihm Merob ein, da\u00df die k\u00f6nn seyn die Braut,", "tokens": ["Und", "f\u00e4llt", "ihm", "Me\u00b7rob", "ein", ",", "da\u00df", "die", "k\u00f6nn", "seyn", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NE", "PTKVZ", "$,", "KOUS", "ART", "NN", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die ihm Gott hat bestimmt, f\u00fcr Adriel ihm graut.", "tokens": ["Die", "ihm", "Gott", "hat", "be\u00b7stimmt", ",", "f\u00fcr", "Ad\u00b7riel", "ihm", "graut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "VAFIN", "VVPP", "$,", "APPR", "NE", "PPER", "VVFIN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Wann Michal Lieblichkeit ihm dann bescheint die Sinne,", "tokens": ["Wann", "Mic\u00b7hal", "Lieb\u00b7lich\u00b7keit", "ihm", "dann", "be\u00b7scheint", "die", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NN", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Meint er, dem Adriel er sie nicht lassen k\u00f6nne.", "tokens": ["Meint", "er", ",", "dem", "Ad\u00b7riel", "er", "sie", "nicht", "las\u00b7sen", "k\u00f6n\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "PPER", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "Doch wie er sich besinnt, da\u00df er ja werd geliebt,", "tokens": ["Doch", "wie", "er", "sich", "be\u00b7sinnt", ",", "da\u00df", "er", "ja", "werd", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "ADJD", "$,", "KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und auch der Adriel, sein Hertz zur Ruh sich giebt.", "tokens": ["Und", "auch", "der", "Ad\u00b7riel", ",", "sein", "Hertz", "zur", "Ruh", "sich", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PPOSAT", "NN", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Hierauf bedencket er, wie frommer Leute Kinder", "tokens": ["Hier\u00b7auf", "be\u00b7den\u00b7cket", "er", ",", "wie", "from\u00b7mer", "Leu\u00b7te", "Kin\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "PWAV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Oft von der rechten Bahn nachgehn den Weg der S\u00fcnder,", "tokens": ["Oft", "von", "der", "rech\u00b7ten", "Bahn", "nach\u00b7gehn", "den", "Weg", "der", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und da\u00df nicht die Gebuhrt mitbring denselben Geist,", "tokens": ["Und", "da\u00df", "nicht", "die", "Ge\u00b7buhrt", "mit\u00b7bring", "den\u00b7sel\u00b7ben", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "ART", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der in den V\u00e4tern ist, wie Joels Beyspiel weist.", "tokens": ["Der", "in", "den", "V\u00e4\u00b7tern", "ist", ",", "wie", "Joels", "Bey\u00b7spiel", "weist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "$,", "PWAV", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Daher oft dient zur Qual, was an dern ist ein Seegen,", "tokens": ["Da\u00b7her", "oft", "dient", "zur", "Qual", ",", "was", "an", "dern", "ist", "ein", "See\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "APPRART", "NN", "$,", "PRELS", "APPR", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Kein Kind viel besser ist, als viele, die da legen", "tokens": ["Kein", "Kind", "viel", "bes\u00b7ser", "ist", ",", "als", "vie\u00b7le", ",", "die", "da", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "ADJD", "VAFIN", "$,", "KOUS", "PIS", "$,", "PRELS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sich auf die Lasterbahn, das diese t\u00f6den kan,", "tokens": ["Sich", "auf", "die", "Las\u00b7ter\u00b7bahn", ",", "das", "die\u00b7se", "t\u00f6\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "$,", "PRELS", "PDS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Von denen sie zuerst das Leben nehmen an.", "tokens": ["Von", "de\u00b7nen", "sie", "zu\u00b7erst", "das", "Le\u00b7ben", "neh\u00b7men", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Er w\u00fcnscht bey Jsai sich stets so zu bezeigen,", "tokens": ["Er", "w\u00fcnscht", "bey", "Jsai", "sich", "stets", "so", "zu", "be\u00b7zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "PRF", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Da\u00df der nicht mog zum Grab die grauen Haare neigen,", "tokens": ["Da\u00df", "der", "nicht", "mog", "zum", "Grab", "die", "grau\u00b7en", "Haa\u00b7re", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PTKNEG", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Besondern seine Lust im Alter an ihm sehn.", "tokens": ["Be\u00b7son\u00b7dern", "sei\u00b7ne", "Lust", "im", "Al\u00b7ter", "an", "ihm", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die\u00df will er halten stets wie auch bisher geschehn.", "tokens": ["Die\u00df", "will", "er", "hal\u00b7ten", "stets", "wie", "auch", "bis\u00b7her", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "VVFIN", "ADV", "KOKOM", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}