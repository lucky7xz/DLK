{"dta.poem.4348": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Welt im Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein wundersch\u00f6ner Winter-Tag, da es geschneiet und", "tokens": ["Ein", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Win\u00b7ter\u00b7Tag", ",", "da", "es", "ge\u00b7schnei\u00b7et", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVPP", "KON"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "gefroren,", "tokens": ["ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und sich der Wolken grauer Duft zerst\u00fcckt vom Firma-", "tokens": ["Und", "sich", "der", "Wol\u00b7ken", "grau\u00b7er", "Duft", "zer\u00b7st\u00fcckt", "vom", "Fir\u00b7ma"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "ART", "NN", "ADJA", "NN", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ment verlohren,", "tokens": ["ment", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "So da\u00df der Sonnen heller Glanz den weissen Kreis der", "tokens": ["So", "da\u00df", "der", "Son\u00b7nen", "hel\u00b7ler", "Glanz", "den", "weis\u00b7sen", "Kreis", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJA", "NN", "ART", "ADJA", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Welt bestrahlt',", "tokens": ["Welt", "be\u00b7strahlt'", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und dadurch alle sch\u00f6ne Vorw\u00fcrf\u2019 annoch viel hell- und", "tokens": ["Und", "da\u00b7durch", "al\u00b7le", "sch\u00f6\u00b7ne", "Vor\u00b7w\u00fcr\u00b7f'", "an\u00b7noch", "viel", "hell", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "PIAT", "ADJA", "NN", "ADV", "ADV", "TRUNC", "KON"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "sch\u00f6ner mahlt'", "tokens": ["sch\u00f6\u00b7ner", "mahlt'"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Bewegte mich, auf meines Schlosses sehr hoch erhabnen", "tokens": ["Be\u00b7weg\u00b7te", "mich", ",", "auf", "mei\u00b7nes", "Schlos\u00b7ses", "sehr", "hoch", "er\u00b7hab\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Thurm zu steigen,", "tokens": ["Thurm", "zu", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Damit die sch\u00f6ne weisse Welt sich mir noch m\u00f6gte sch\u00f6ner", "tokens": ["Da\u00b7mit", "die", "sch\u00f6\u00b7ne", "weis\u00b7se", "Welt", "sich", "mir", "noch", "m\u00f6g\u00b7te", "sch\u00f6\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "ADJA", "NN", "PRF", "PPER", "ADV", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "zeigen.", "tokens": ["zei\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Ich stieg hinauf, und ward so gleich, als wie von einem", "tokens": ["Ich", "stieg", "hin\u00b7auf", ",", "und", "ward", "so", "gleich", ",", "als", "wie", "von", "ei\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VAFIN", "ADV", "ADV", "$,", "KOUS", "KOKOM", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Blitz ger\u00fchrt,", "tokens": ["Blitz", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Als die von dort entdeckte Weite den Blick durch so viel", "tokens": ["Als", "die", "von", "dort", "ent\u00b7deck\u00b7te", "Wei\u00b7te", "den", "Blick", "durch", "so", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "ADV", "VVFIN", "NN", "ART", "NN", "APPR", "ADV", "ADV"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Sch\u00f6nheit f\u00fchrt.", "tokens": ["Sch\u00f6n\u00b7heit", "f\u00fchrt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Es ward auf einmahl meinen Augen ein\u2019 Erd- und eine", "tokens": ["Es", "ward", "auf", "ein\u00b7mahl", "mei\u00b7nen", "Au\u00b7gen", "ein'", "Erd", "und", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "PPOSAT", "NN", "ART", "TRUNC", "KON", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Wasser-Welt,", "tokens": ["Was\u00b7ser\u00b7Welt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Die einem vollen Monden glich, an Glanz und Weisse,", "tokens": ["Die", "ei\u00b7nem", "vol\u00b7len", "Mon\u00b7den", "glich", ",", "an", "Glanz", "und", "Weis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "vorgestellt.", "tokens": ["vor\u00b7ge\u00b7stellt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.21": {"text": "Es schimmerte der Erden Fl\u00e4che, beschneiter B\u00e4um\u2019 und", "tokens": ["Es", "schim\u00b7mer\u00b7te", "der", "Er\u00b7den", "Fl\u00e4\u00b7che", ",", "be\u00b7schnei\u00b7ter", "B\u00e4um'", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "ADJA", "NN", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "B\u00fcsche Reiser,", "tokens": ["B\u00fc\u00b7sche", "Rei\u00b7ser", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Die Giebel, theils mit rohten Ziegeln bedeckter, theils", "tokens": ["Die", "Gie\u00b7bel", ",", "theils", "mit", "roh\u00b7ten", "Zie\u00b7geln", "be\u00b7deck\u00b7ter", ",", "theils"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "ADJA", "NN", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "bemooster H\u00e4user,", "tokens": ["be\u00b7moos\u00b7ter", "H\u00e4u\u00b7ser", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Die Aecker, deren lange Grenzen gef\u00fcllet, eben nur zu", "tokens": ["Die", "A\u00b7e\u00b7cker", ",", "de\u00b7ren", "lan\u00b7ge", "Gren\u00b7zen", "ge\u00b7f\u00fcl\u00b7let", ",", "e\u00b7ben", "nur", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "ADJA", "NN", "VVPP", "$,", "ADV", "ADV", "APPR"], "meter": "-+----+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "sehn,", "tokens": ["sehn", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "Die liessen ein recht blendend Wei\u00df vom hell- bestrahlten", "tokens": ["Die", "lies\u00b7sen", "ein", "recht", "blen\u00b7dend", "Wei\u00df", "vom", "hell", "be\u00b7strahl\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJD", "ADJD", "NN", "APPRART", "TRUNC", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Schnee mir sehn.", "tokens": ["Schnee", "mir", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Allein ich sah noch mehr als die\u00df; ich sah die blaue Fluht", "tokens": ["Al\u00b7lein", "ich", "sah", "noch", "mehr", "als", "die\u00df", ";", "ich", "sah", "die", "blau\u00b7e", "Fluht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "PIAT", "KOKOM", "PDS", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "von weiten", "tokens": ["von", "wei\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Bey dem erhabnen Schutz des Landes, den Deichen, sanft", "tokens": ["Bey", "dem", "er\u00b7hab\u00b7nen", "Schutz", "des", "Lan\u00b7des", ",", "den", "Dei\u00b7chen", ",", "sanft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN", "$,", "ADJD"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "vor\u00fcber gleiten,", "tokens": ["vor\u00b7\u00fc\u00b7ber", "glei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Mit Schiffen hier und dort bedeckt. Die grosse Fl\u00e4che", "tokens": ["Mit", "Schif\u00b7fen", "hier", "und", "dort", "be\u00b7deckt", ".", "Die", "gros\u00b7se", "Fl\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "KON", "ADV", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "zog den Blick", "tokens": ["zog", "den", "Blick"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Auf eine ferne Reise fort, und lie\u00df ihn nicht so bald", "tokens": ["Auf", "ei\u00b7ne", "fer\u00b7ne", "Rei\u00b7se", "fort", ",", "und", "lie\u00df", "ihn", "nicht", "so", "bald"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "zur\u00fcck.", "tokens": ["zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Ich sucht\u2019 auf einen Gegenstand denselbigen mit M\u00fch\u2019", "tokens": ["Ich", "sucht'", "auf", "ei\u00b7nen", "Ge\u00b7gen\u00b7stand", "den\u00b7sel\u00b7bi\u00b7gen", "mit", "M\u00fch'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "zu wenden;", "tokens": ["zu", "wen\u00b7den", ";"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Allein es konnte seine Reise nicht eh, als in der Luft, sich", "tokens": ["Al\u00b7lein", "es", "konn\u00b7te", "sei\u00b7ne", "Rei\u00b7se", "nicht", "eh", ",", "als", "in", "der", "Luft", ",", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "KOUS", "$,", "KOUS", "APPR", "ART", "NN", "$,", "PRF"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "enden,", "tokens": ["en\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Weil keine Grenzen sonst vorhanden. Ich dachte, was in", "tokens": ["Weil", "kei\u00b7ne", "Gren\u00b7zen", "sonst", "vor\u00b7han\u00b7den", ".", "Ich", "dach\u00b7te", ",", "was", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ADJD", "$.", "PPER", "VVFIN", "$,", "PRELS", "APPR"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "diesen Tiefen", "tokens": ["die\u00b7sen", "Tie\u00b7fen"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Doch wohl f\u00fcr ungez\u00e4hlte Formen beschuppter Wasser-", "tokens": ["Doch", "wohl", "f\u00fcr", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "For\u00b7men", "be\u00b7schupp\u00b7ter", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "B\u00fcrger liefen!", "tokens": ["B\u00fcr\u00b7ger", "lie\u00b7fen", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Was doch wohl f\u00fcr ein Heer voll Wunder in diesem hohlen", "tokens": ["Was", "doch", "wohl", "f\u00fcr", "ein", "Heer", "voll", "Wun\u00b7der", "in", "die\u00b7sem", "hoh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "APPR", "ART", "NN", "ADJD", "NN", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Grunde w\u00fchlt!", "tokens": ["Grun\u00b7de", "w\u00fchlt", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Welch eine Creaturen-Menge in diesem dunklen Schlunde", "tokens": ["Welch", "ei\u00b7ne", "Crea\u00b7tu\u00b7ren\u00b7Men\u00b7ge", "in", "die\u00b7sem", "dunk\u00b7len", "Schlun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "spielt!", "tokens": ["spielt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Bis mich der Glanz der sch\u00f6nen Erde von neuem wieder", "tokens": ["Bis", "mich", "der", "Glanz", "der", "sch\u00f6\u00b7nen", "Er\u00b7de", "von", "neu\u00b7em", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "ART", "ADJA", "NN", "APPR", "ADJA", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "auf sich zog,", "tokens": ["auf", "sich", "zog", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Da ich denn, durch den Winter-Schmuck der Welt ger\u00fchrt,", "tokens": ["Da", "ich", "denn", ",", "durch", "den", "Win\u00b7ter\u00b7Schmuck", "der", "Welt", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "aufs neu erwog:", "tokens": ["aufs", "neu", "er\u00b7wog", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Da so viel\u2019 Gegenw\u00fcrf\u2019 im Winter, durchs Sonnen-Licht,", "tokens": ["Da", "so", "viel'", "Ge\u00b7gen\u00b7w\u00fcr\u00b7f'", "im", "Win\u00b7ter", ",", "durchs", "Son\u00b7nen\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "--+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "und unsre Augen,", "tokens": ["und", "uns\u00b7re", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Dem Geiste, der es \u00fcberlegt, Vergn\u00fcgungen zu bringen", "tokens": ["Dem", "