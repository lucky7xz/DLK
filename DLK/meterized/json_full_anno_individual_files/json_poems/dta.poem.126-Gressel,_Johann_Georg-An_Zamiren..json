{"dta.poem.126": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Zamiren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Zamire du bist sch\u00f6n/ dein Wesen ist galant,", "tokens": ["Za\u00b7mi\u00b7re", "du", "bist", "sch\u00f6n", "/", "dein", "We\u00b7sen", "ist", "ga\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VAFIN", "ADJD", "$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Ein jeder/ der dich sieht/ wird gleich durch dich entbrannt;", "tokens": ["Ein", "je\u00b7der", "/", "der", "dich", "sieht", "/", "wird", "gleich", "durch", "dich", "ent\u00b7brannt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "$(", "PRELS", "PPER", "VVFIN", "$(", "VAFIN", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein artigs Wesen sucht mit unentwandten Blicken", "tokens": ["Dein", "ar\u00b7tigs", "We\u00b7sen", "sucht", "mit", "un\u00b7ent\u00b7wand\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den ungezwungnen Sinn mit Fesseln zu bestricken.", "tokens": ["Den", "un\u00b7ge\u00b7zwung\u00b7nen", "Sinn", "mit", "Fes\u00b7seln", "zu", "be\u00b7stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Augen feyren nicht/ die Mienen sch\u00fcren zu/", "tokens": ["Die", "Au\u00b7gen", "fey\u00b7ren", "nicht", "/", "die", "Mie\u00b7nen", "sch\u00fc\u00b7ren", "zu", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$(", "ART", "NN", "VVINF", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So nunst du l\u00e4chelnd weg den Grund-Stein unser Ruh/", "tokens": ["So", "nunst", "du", "l\u00e4\u00b7chelnd", "weg", "den", "Grun\u00b7dStein", "un\u00b7ser", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADJD", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und l\u00e4\u00dft an dessen statt Brand/ Quaal und Angst zur\u00fccke", "tokens": ["Und", "l\u00e4\u00dft", "an", "des\u00b7sen", "statt", "Brand", "/", "Qua\u00b7al", "und", "Angst", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDS", "APPR", "NN", "$(", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Damit uns hart genug der Sclaven Fessel dr\u00fccke.", "tokens": ["Da\u00b7mit", "uns", "hart", "ge\u00b7nug", "der", "Scla\u00b7ven", "Fes\u00b7sel", "dr\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und sucht man denn bey dir Errettung aus der Quaal/", "tokens": ["Und", "sucht", "man", "denn", "bey", "dir", "Er\u00b7ret\u00b7tung", "aus", "der", "Qua\u00b7al", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPR", "PPER", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So doppelt unser Noht ein ernsthafft Augen-Strahl/", "tokens": ["So", "dop\u00b7pelt", "un\u00b7ser", "Noht", "ein", "ernst\u00b7hafft", "Au\u00b7gen\u00b7Strahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die arme Seele mu\u00df in tausend Angst verschmachten/", "tokens": ["Die", "ar\u00b7me", "See\u00b7le", "mu\u00df", "in", "tau\u00b7send", "Angst", "ver\u00b7schmach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und darf in ihrer Pein auf keine H\u00fclffe trachten.", "tokens": ["Und", "darf", "in", "ih\u00b7rer", "Pein", "auf", "kei\u00b7ne", "H\u00fclf\u00b7fe", "trach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Manch Geist ist schon dahin/ wo ihn das dunckle Grab", "tokens": ["Manch", "Geist", "ist", "schon", "da\u00b7hin", "/", "wo", "ihn", "das", "dunck\u00b7le", "Grab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PAV", "$(", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Flammen seiner Brunst k\u00fchlt in der Erden ab/", "tokens": ["Die", "Flam\u00b7men", "sei\u00b7ner", "Brunst", "k\u00fchlt", "in", "der", "Er\u00b7den", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Viel m\u00fcssen jetzo noch die heissen Flammen nehren/", "tokens": ["Viel", "m\u00fcs\u00b7sen", "jet\u00b7zo", "noch", "die", "heis\u00b7sen", "Flam\u00b7men", "neh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und durch ein heimlich Feur den matten Geist verzehren.", "tokens": ["Und", "durch", "ein", "heim\u00b7lich", "Feur", "den", "mat\u00b7ten", "Geist", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJD", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich f\u00fchle auch den Strahl/ ", "tokens": ["Ich", "f\u00fch\u00b7le", "auch", "den", "Strahl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Der mich in mir verwirrt und meine schlechte Lust", "tokens": ["Der", "mich", "in", "mir", "ver\u00b7wirrt", "und", "mei\u00b7ne", "schlech\u00b7te", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER", "ADJD", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Verst\u00f6hrt: ich bin dein Knecht/ der Liebes-Fesseln tr\u00e4get/", "tokens": ["Ver\u00b7st\u00f6hrt", ":", "ich", "bin", "dein", "Knecht", "/", "der", "Lie\u00b7bes\u00b7Fes\u00b7seln", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die mir dein artger Geist/ mein Engel/ angeleget.", "tokens": ["Die", "mir", "dein", "art\u00b7ger", "Geist", "/", "mein", "En\u00b7gel", "/", "an\u00b7ge\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich senffze in der Last/ ich seh nach Rettung aus/", "tokens": ["Ich", "senff\u00b7ze", "in", "der", "Last", "/", "ich", "seh", "nach", "Ret\u00b7tung", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Allein dein Auge dreut mir einen harten Straus/", "tokens": ["Al\u00b7lein", "dein", "Au\u00b7ge", "dreut", "mir", "ei\u00b7nen", "har\u00b7ten", "Straus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und zeiget/ da\u00df es auch so grausahm ist als sch\u00f6ne/", "tokens": ["Und", "zei\u00b7get", "/", "da\u00df", "es", "auch", "so", "grau\u00b7sahm", "ist", "als", "sch\u00f6\u00b7ne", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "KOKOM", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und da\u00df ich mich umsonst/ dich zu erbitten/ sehne.", "tokens": ["Und", "da\u00df", "ich", "mich", "um\u00b7sonst", "/", "dich", "zu", "er\u00b7bit\u00b7ten", "/", "seh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "$(", "PPER", "PTKZU", "VVINF", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zamire meine Lust/ sey gn\u00e4dig meinem Flehn", "tokens": ["Za\u00b7mi\u00b7re", "mei\u00b7ne", "Lust", "/", "sey", "gn\u00e4\u00b7dig", "mei\u00b7nem", "Flehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$(", "VAFIN", "ADJD", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.26": {"text": "Und la\u00df das Jubel-Fest in Kurtzem vor sich gehn.", "tokens": ["Und", "la\u00df", "das", "Ju\u00b7bel\u00b7Fest", "in", "Kurt\u00b7zem", "vor", "sich", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "APPR", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Soll ich das Purpur-Blut der sch\u00f6nen Lippen k\u00fcssen?", "tokens": ["Soll", "ich", "das", "Pur\u00b7pur\u00b7Blut", "der", "sch\u00f6\u00b7nen", "Lip\u00b7pen", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So wirst du dich gewi\u00df nicht lange weigern m\u00fcssen.", "tokens": ["So", "wirst", "du", "dich", "ge\u00b7wi\u00df", "nicht", "lan\u00b7ge", "wei\u00b7gern", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wo aber nicht? so zieh die strengen Strahlen ein/", "tokens": ["Wo", "a\u00b7ber", "nicht", "?", "so", "zieh", "die", "stren\u00b7gen", "Strah\u00b7len", "ein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKNEG", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und la\u00df der Augen-Blitz nicht so durch dringend seyn/", "tokens": ["Und", "la\u00df", "der", "Au\u00b7gen\u00b7Blitz", "nicht", "so", "durch", "drin\u00b7gend", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKNEG", "ADV", "APPR", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Damit/ wenn ich nicht soll ein g\u00fctig Hertze hoffen/", "tokens": ["Da\u00b7mit", "/", "wenn", "ich", "nicht", "soll", "ein", "g\u00fc\u00b7tig", "Hert\u00b7ze", "hof\u00b7fen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$(", "KOUS", "PPER", "PTKNEG", "VMFIN", "ART", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Mein Geist nicht allzu sehr/ ", "tokens": ["Mein", "Geist", "nicht", "all\u00b7zu", "sehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "PTKA", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}