{"textgrid.poem.35279": {"metadata": {"author": {"name": "Rubiner, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "4. Die Engel", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fchrer, du stehst klein, eine zuckende Bluts\u00e4ule auf der schmalen Trib\u00fcne,", "tokens": ["F\u00fch\u00b7rer", ",", "du", "stehst", "klein", ",", "ei\u00b7ne", "zu\u00b7cken\u00b7de", "Blut\u00b7s\u00e4u\u00b7le", "auf", "der", "schma\u00b7len", "Tri\u00b7b\u00fc\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "ADJD", "$,", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+-+--+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Deine Augen werfen im Horizontflug leuchtende Fl\u00fcgel ins Gr\u00fcne,", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "wer\u00b7fen", "im", "Ho\u00b7ri\u00b7zont\u00b7flug", "leuch\u00b7ten\u00b7de", "Fl\u00fc\u00b7gel", "ins", "Gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+-+--+--+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Deine Ringerarme kreisen weit hinein ins feindliche Menschenfeld.", "tokens": ["Dei\u00b7ne", "Rin\u00b7ge\u00b7rar\u00b7me", "krei\u00b7sen", "weit", "hin\u00b7ein", "ins", "feind\u00b7li\u00b7che", "Men\u00b7schen\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "O Engel, ihr fliegt im leu