{"dta.poem.10307": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Trutz, blanke Hans.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Heut bin ich &#252;ber Rungholt gefahren,             ", "tokens": ["Heut", "bin", "ich", "&#252;", "ber", "Rung\u00b7holt", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$(", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die Stadt ging unter vor f\u00fcnfhundert Jahren.", "tokens": ["Die", "Stadt", "ging", "un\u00b7ter", "vor", "f\u00fcnf\u00b7hun\u00b7dert", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch schlagen die Wellen da wild und emp\u00f6rt,", "tokens": ["Noch", "schla\u00b7gen", "die", "Wel\u00b7len", "da", "wild", "und", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wie damals, als sie die Marschen zerst\u00f6rt.", "tokens": ["Wie", "da\u00b7mals", ",", "als", "sie", "die", "Mar\u00b7schen", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Maschine des Dampfers sch\u00fcttert\u2019 und st\u00f6hnte,", "tokens": ["Die", "Ma\u00b7schi\u00b7ne", "des", "Damp\u00b7fers", "sch\u00fct\u00b7tert'", "und", "st\u00f6hn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-++--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Aus den Wassern rief es unheimlich und h\u00f6hnte:", "tokens": ["Aus", "den", "Was\u00b7sern", "rief", "es", "un\u00b7heim\u00b7lich", "und", "h\u00f6hn\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$."], "meter": "--+-+-+---+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Von der Nordsee, der Mordsee, vom Festland geschieden,", "tokens": ["Von", "der", "Nord\u00b7see", ",", "der", "Mord\u00b7see", ",", "vom", "Fest\u00b7land", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Liegen die friesischen Inseln im Frieden.", "tokens": ["Lie\u00b7gen", "die", "frie\u00b7si\u00b7schen", "In\u00b7seln", "im", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Und Zeugen weltenvernichtender Wut,", "tokens": ["Und", "Zeu\u00b7gen", "wel\u00b7ten\u00b7ver\u00b7nich\u00b7ten\u00b7der", "Wut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PWAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Taucht Hallig auf Hallig aus fliehender Flut.", "tokens": ["Taucht", "Hal\u00b7lig", "auf", "Hal\u00b7lig", "aus", "flie\u00b7hen\u00b7der", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die M\u00f6we zankt schon auf wachsenden Watten,", "tokens": ["Die", "M\u00f6\u00b7we", "zankt", "schon", "auf", "wach\u00b7sen\u00b7den", "Wat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Seehund schon sonnt sich auf sandigen Platten.", "tokens": ["Der", "See\u00b7hund", "schon", "sonnt", "sich", "auf", "san\u00b7di\u00b7gen", "Plat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Im Ocean, mitten, schl\u00e4ft bis zur Stunde,", "tokens": ["Im", "O\u00b7cean", ",", "mit\u00b7ten", ",", "schl\u00e4ft", "bis", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADV", "$,", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Ungeheuer, tief auf dem Grunde.", "tokens": ["Ein", "Un\u00b7ge\u00b7heu\u00b7er", ",", "tief", "auf", "dem", "Grun\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sein Haupt ruht dicht vor Englands Strand,", "tokens": ["Sein", "Haupt", "ruht", "dicht", "vor", "En\u00b7glands", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schwanzflosse spielt nah\u2019 Brasiliens Sand.", "tokens": ["Die", "Schwanz\u00b7flos\u00b7se", "spielt", "nah'", "Bra\u00b7si\u00b7li\u00b7ens", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "NN", "$."], "meter": "-++-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Es zieht, sechs Stunden, den Atem nach innen,", "tokens": ["Es", "zieht", ",", "sechs", "Stun\u00b7den", ",", "den", "A\u00b7tem", "nach", "in\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "CARD", "NN", "$,", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und treibt ihn, sechs Stunden, wieder von hinnen.", "tokens": ["Und", "treibt", "ihn", ",", "sechs", "Stun\u00b7den", ",", "wie\u00b7der", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "CARD", "NN", "$,", "ADV", "APPR", "ADV", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch einmal in jedem Jahrhundert entlassen", "tokens": ["Doch", "ein\u00b7mal", "in", "je\u00b7dem", "Jahr\u00b7hun\u00b7dert", "ent\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Kiemen gewaltige Wassermassen.", "tokens": ["Die", "Kie\u00b7men", "ge\u00b7wal\u00b7ti\u00b7ge", "Was\u00b7ser\u00b7mas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dann holt das Untier tief Atem ein,", "tokens": ["Dann", "holt", "das", "Un\u00b7tier", "tief", "A\u00b7tem", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und peitscht die Welle und schl\u00e4ft wieder ein.", "tokens": ["Und", "peitscht", "die", "Wel\u00b7le", "und", "schl\u00e4ft", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Viel tausend Menschen im Nordland ertrinken,", "tokens": ["Viel", "tau\u00b7send", "Men\u00b7schen", "im", "Nord\u00b7land", "er\u00b7trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Viel reiche L\u00e4nder und St\u00e4dte versinken.", "tokens": ["Viel", "rei\u00b7che", "L\u00e4n\u00b7der", "und", "St\u00e4d\u00b7te", "ver\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Rungholt ist reich und wird immer reicher,", "tokens": ["Rung\u00b7holt", "ist", "reich", "und", "wird", "im\u00b7mer", "rei\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADJD", "KON", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Kein Korn mehr fa\u00dft selbst der gr\u00f6\u00dfeste Speicher.", "tokens": ["Kein", "Korn", "mehr", "fa\u00dft", "selbst", "der", "gr\u00f6\u00b7\u00dfes\u00b7te", "Spei\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie zur Bl\u00fctezeit im alten Rom,", "tokens": ["Wie", "zur", "Bl\u00fc\u00b7te\u00b7zeit", "im", "al\u00b7ten", "Rom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "APPRART", "ADJA", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Staut hier t\u00e4glich der Menschenstrom.", "tokens": ["Staut", "hier", "t\u00e4g\u00b7lich", "der", "Men\u00b7schen\u00b7strom", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Die S\u00e4nften tragen Syrer und Mohren,", "tokens": ["Die", "S\u00e4nf\u00b7ten", "tra\u00b7gen", "Sy\u00b7rer", "und", "Moh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit Goldblech und Flitter in Nasen und Ohren.", "tokens": ["Mit", "Gold\u00b7blech", "und", "Flit\u00b7ter", "in", "Na\u00b7sen", "und", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Zum Feste heut klingen Cymbeln und Zinken,", "tokens": ["Zum", "Fes\u00b7te", "heut", "klin\u00b7gen", "Cym\u00b7beln", "und", "Zin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus den Fenstern mit T\u00fcchern die Frauen winken", "tokens": ["Aus", "den", "Fens\u00b7tern", "mit", "T\u00fc\u00b7chern", "die", "Frau\u00b7en", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "ART", "NN", "VVINF"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Und bl\u00e4ttern Blumen in alle die Pracht \u2014", "tokens": ["Und", "bl\u00e4t\u00b7tern", "Blu\u00b7men", "in", "al\u00b7le", "die", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PIS", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Kirchen schlo\u00df wer aber \u00fcber Nacht?", "tokens": ["Die", "Kir\u00b7chen", "schlo\u00df", "wer", "a\u00b7ber", "\u00fc\u00b7ber", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PWS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Rungholter wollen sich selbst regieren,", "tokens": ["Die", "Rung\u00b7hol\u00b7ter", "wol\u00b7len", "sich", "selbst", "re\u00b7gie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und keine Zeit mehr mit Gott verlieren.", "tokens": ["Und", "kei\u00b7ne", "Zeit", "mehr", "mit", "Gott", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Auf allen M\u00e4rkten, auf allen Gassen", "tokens": ["Auf", "al\u00b7len", "M\u00e4rk\u00b7ten", ",", "auf", "al\u00b7len", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "L\u00e4rmende Leute, betrunkene Massen.", "tokens": ["L\u00e4r\u00b7men\u00b7de", "Leu\u00b7te", ",", "be\u00b7trun\u00b7ke\u00b7ne", "Mas\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Sie ziehn am Abend hinaus auf den Deich:", "tokens": ["Sie", "ziehn", "am", "A\u00b7bend", "hin\u00b7aus", "auf", "den", "Deich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APZR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wir trotzen dir, blanker Hans, Nordseeteich!", "tokens": ["Wir", "trot\u00b7zen", "dir", ",", "blan\u00b7ker", "Hans", ",", "Nord\u00b7see\u00b7teich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADJA", "NE", "$,", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und wie sie drohend die F\u00e4uste ballen,", "tokens": ["Und", "wie", "sie", "dro\u00b7hend", "die", "F\u00e4us\u00b7te", "bal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVPP", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zieht leis aus dem Schlamm der Krake die Krallen.", "tokens": ["Zieht", "leis", "aus", "dem", "Schlamm", "der", "Kra\u00b7ke", "die", "Kral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Die Wasser ebben, die V\u00f6gel ruhen,", "tokens": ["Die", "Was\u00b7ser", "eb\u00b7ben", ",", "die", "V\u00f6\u00b7gel", "ru\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der liebe Gott geht auf leisesten Schuhen.", "tokens": ["Der", "lie\u00b7be", "Gott", "geht", "auf", "lei\u00b7ses\u00b7ten", "Schu\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Mond zieht am Himmel gelassen die Bahn,", "tokens": ["Der", "Mond", "zieht", "am", "Him\u00b7mel", "ge\u00b7las\u00b7sen", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Bel\u00e4chelt der protzigen Rungholter Wahn.", "tokens": ["Be\u00b7l\u00e4\u00b7chelt", "der", "prot\u00b7zi\u00b7gen", "Rung\u00b7hol\u00b7ter", "Wahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+--++--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Von Brasilien gl\u00e4nzt bis zu 