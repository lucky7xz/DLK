{"textgrid.poem.40939": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: I bi ne Geist usem Oberland,", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "I bi ne Geist usem Oberland,", "tokens": ["I", "bi", "ne", "Geist", "u\u00b7sem", "O\u00b7ber\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "und vierzig Johr und achti huus i scho", "tokens": ["und", "vier\u00b7zig", "Johr", "und", "ach\u00b7ti", "huus", "i", "scho"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "KON", "FM", "FM", "FM", "FM"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "do in dem Zwerchsack, und gang nie me drus.", "tokens": ["do", "in", "dem", "Zwerch\u00b7sack", ",", "und", "gang", "nie", "me", "drus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADV", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "I ha ne frische Chnab us 's Vaters Huus", "tokens": ["I", "ha", "ne", "fri\u00b7sche", "Chnab", "us", "'s", "Va\u00b7ters", "Huus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "PPER", "NN", "NE"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "z'bigleite cha in d'Stadt zu siner Lehr", "tokens": ["z'\u00b7biglei\u00b7te", "cha", "in", "d'\u00b7Stadt", "zu", "si\u00b7ner", "Lehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "APPR", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "und mit sim B\u00fcndeli, und wenig drin.", "tokens": ["und", "mit", "sim", "B\u00fcn\u00b7de\u00b7li", ",", "und", "we\u00b7nig", "drin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "NE", "$,", "KON", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbgib achtig uffen\u00ab, het der lieb Gott gseit,", "tokens": ["\u00bb", "gib", "ach\u00b7tig", "uf\u00b7fen", "\u00ab", ",", "het", "der", "lieb", "Gott", "gseit", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ADJD", "VVINF", "$(", "$,", "VAFIN", "ART", "ADJD", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u00bbund mach, da\u00df \u00f6bbis Ordligs usem wird!\u00ab", "tokens": ["\u00bb", "und", "mach", ",", "da\u00df", "\u00f6b\u00b7bis", "Ord\u00b7ligs", "u\u00b7sem", "wird", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVFIN", "$,", "KOUS", "ADJA", "NN", "NE", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "'s isch \u00f6bbis us der worden, alte Chnab,", "tokens": ["'s", "isch", "\u00f6b\u00b7bis", "us", "der", "wor\u00b7den", ",", "al\u00b7te", "Chnab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "FM", "FM", "FM", "ART", "VAPP", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "und sch\u00f6n stoht jez di gr\u00fcene Ehrechranz", "tokens": ["und", "sch\u00f6n", "stoht", "jez", "di", "gr\u00fce\u00b7ne", "Eh\u00b7rec\u00b7hranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "in dine graue Locken, und di Sack \u2013", "tokens": ["in", "di\u00b7ne", "grau\u00b7e", "Lo\u00b7cken", ",", "und", "di", "Sack", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "NE", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "nei lueg, bim Bluest, er chennt di nit,", "tokens": ["nei", "lu\u00b7eg", ",", "bim", "Blu\u00b7est", ",", "er", "chennt", "di", "nit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,", "APPRART", "NN", "$,", "PPER", "VVFIN", "NE", "PTKNEG", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "er chennt di nimme! Aber du chennsch ihn", "tokens": ["er", "chennt", "di", "nim\u00b7me", "!", "A\u00b7ber", "du", "chennsch", "ihn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "VVFIN", "$.", "KON", "PPER", "ADJD", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "in Demut \u2013 alle gute Gabe kommt", "tokens": ["in", "De\u00b7mut", "\u2013", "al\u00b7le", "gu\u00b7te", "Ga\u00b7be", "kommt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$(", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.15": {"text": "von oben her, vom Vater alles Lichts \u2013", "tokens": ["von", "o\u00b7ben", "her", ",", "vom", "Va\u00b7ter", "al\u00b7les", "Lichts", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$,", "APPRART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "und denksch jez wieder an di ersti Stund", "tokens": ["und", "denksch", "jez", "wie\u00b7der", "an", "di", "ers\u00b7ti", "Stund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ADV", "APPR", "NE", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "voll banger Hoffnig in der fremde Stadt:", "tokens": ["voll", "ban\u00b7ger", "Hoff\u00b7nig", "in", "der", "frem\u00b7de", "Stadt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "\u00bbin deine Vaterh\u00e4nde, du, mein Gott,", "tokens": ["\u00bb", "in", "dei\u00b7ne", "Va\u00b7ter\u00b7h\u00e4n\u00b7de", ",", "du", ",", "mein", "Gott", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "$,", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "leg' ich mein Schicksal!\u00ab Guete H\u00e4nde hesch's", "tokens": ["leg'", "ich", "mein", "Schick\u00b7sal", "!", "\u00ab", "Gue\u00b7te", "H\u00e4n\u00b7de", "he\u00b7sch's"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "ADJA", "NN", "NE"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.20": {"text": "vertraut \u2013 's isch n\u00e4ume wie ne fremde Somechern,", "tokens": ["ver\u00b7traut", "\u2013", "'s", "isch", "n\u00e4u\u00b7me", "wie", "ne", "frem\u00b7de", "So\u00b7me\u00b7chern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$(", "PPER", "ADJD", "VVFIN", "KOKOM", "NE", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "me luegt en a, me wei\u00df nit, was isch drin.", "tokens": ["me", "luegt", "en", "a", ",", "me", "wei\u00df", "nit", ",", "was", "isch", "drin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "VVFIN", "VVFIN", "PTKNEG", "$,", "PRELS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Was gschieht? Im Rege und im Morgetau", "tokens": ["Was", "gschieht", "?", "Im", "Re\u00b7ge", "und", "im", "Mor\u00b7ge\u00b7tau"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "APPRART", "NN", "KON", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "wachst usem chline Ch\u00f6rnli \u00f6bbis uff", "tokens": ["wachst", "u\u00b7sem", "chli\u00b7ne", "Ch\u00f6rn\u00b7li", "\u00f6b\u00b7bis", "uff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "und streckt si s\u00f6lli; jez gr\u00fcent Laub an Laub,", "tokens": ["und", "streckt", "si", "s\u00f6l\u00b7li", ";", "jez", "gr\u00fcent", "Laub", "an", "Laub", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "FM", "FM", "$.", "ADV", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "jez tribt's in alle-n-\u00c4ste Bluest an Bluest,", "tokens": ["jez", "tribt's", "in", "al\u00b7le\u00b7n\u00c4s\u00b7te", "Blu\u00b7est", "an", "Blu\u00b7est", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "jez hangt's voll Frucht. So isch us seller Stund \u2013", "tokens": ["jez", "hangt's", "voll", "Frucht", ".", "So", "isch", "us", "sel\u00b7ler", "Stund", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "$.", "ADV", "ADJD", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "de hesch's nit gew\u00fc\u00dft, di Herz het ni dra denkt \u2013", "tokens": ["de", "he\u00b7sch's", "nit", "ge\u00b7w\u00fc\u00dft", ",", "di", "Herz", "het", "ni", "dra", "denkt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKNEG", "VVPP", "$,", "NE", "NN", "VAFIN", "NE", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "meng Freudejohr erspro\u00dft, und Gl\u00fcck und Heil", "tokens": ["meng", "Freu\u00b7de\u00b7johr", "ers\u00b7pro\u00dft", ",", "und", "Gl\u00fcck", "und", "Heil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "VVFIN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "und Ehr und Chinderdank isch jez di Teil.", "tokens": ["und", "Ehr", "und", "Chin\u00b7der\u00b7dank", "isch", "jez", "di", "Teil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADJD", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "\u2013 Nun freue dich in Ruh und Heiterkeit", "tokens": ["\u2013", "Nun", "freu\u00b7e", "dich", "in", "Ruh", "und", "Hei\u00b7ter\u00b7keit"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "des Segens, der dich kr\u00f6nt, und lieblich sei", "tokens": ["des", "Se\u00b7gens", ",", "der", "dich", "kr\u00f6nt", ",", "und", "lieb\u00b7lich", "sei"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "KON", "ADJD", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "dein Abend nach des Tages schw\u00fcler Hitze.", "tokens": ["dein", "A\u00b7bend", "nach", "des", "Ta\u00b7ges", "schw\u00fc\u00b7ler", "Hit\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Genie\u00dfe lang des treuen Lebens Lohn", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "lang", "des", "treu\u00b7en", "Le\u00b7bens", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "zu deiner Gattin, deiner Kinder Trost!", "tokens": ["zu", "dei\u00b7ner", "Gat\u00b7tin", ",", "dei\u00b7ner", "Kin\u00b7der", "Trost", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "I bi ne Geist usem Oberland,", "tokens": ["I", "bi", "ne", "Geist", "u\u00b7sem", "O\u00b7ber\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "und vierzig Johr und achti huus i scho", "tokens": ["und", "vier\u00b7zig", "Johr", "und", "ach\u00b7