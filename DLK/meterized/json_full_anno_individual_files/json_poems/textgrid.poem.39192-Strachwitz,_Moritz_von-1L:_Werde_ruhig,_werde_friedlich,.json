{"textgrid.poem.39192": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Werde ruhig, werde friedlich,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werde ruhig, werde friedlich,", "tokens": ["Wer\u00b7de", "ru\u00b7hig", ",", "wer\u00b7de", "fried\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df den Schlachtgesang vertosen,", "tokens": ["La\u00df", "den", "Schlacht\u00b7ge\u00b7sang", "ver\u00b7to\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe niedlich und gem\u00fctlich,", "tokens": ["Sin\u00b7ge", "nied\u00b7lich", "und", "ge\u00b7m\u00fct\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Statt mit Blute, schreib' mit Rosen,", "tokens": ["Statt", "mit", "Blu\u00b7te", ",", "schreib'", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df die Schwerter in der Scheide,", "tokens": ["La\u00df", "die", "Schwer\u00b7ter", "in", "der", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den Helmsturz la\u00df am Nagel,", "tokens": ["Und", "den", "Helm\u00b7sturz", "la\u00df", "am", "Na\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df die Pferde auf der Weide", "tokens": ["La\u00df", "die", "Pfer\u00b7de", "auf", "der", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und vergi\u00df den Lanzenhagel.", "tokens": ["Und", "ver\u00b7gi\u00df", "den", "Lan\u00b7zen\u00b7ha\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.9": {"text": "La\u00df die Hiebe ungerochen", "tokens": ["La\u00df", "die", "Hie\u00b7be", "un\u00b7ge\u00b7ro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die Herzen unersch\u00fcttert,", "tokens": ["Und", "die", "Her\u00b7zen", "un\u00b7er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "La\u00df die Sch\u00e4del unzerbrochen", "tokens": ["La\u00df", "die", "Sch\u00e4\u00b7del", "un\u00b7zer\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und die Raben ungef\u00fcttert!", "tokens": ["Und", "die", "Ra\u00b7ben", "un\u00b7ge\u00b7f\u00fct\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was Du singst vom Heldentume,", "tokens": ["Was", "Du", "singst", "vom", "Hel\u00b7den\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist gemacht und ist Manier,", "tokens": ["Ist", "ge\u00b7macht", "und", "ist", "Ma\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe von der G\u00e4nseblume,", "tokens": ["Sin\u00b7ge", "von", "der", "G\u00e4n\u00b7se\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Natur, die lieben wir;", "tokens": ["Die", "Na\u00b7tur", ",", "die", "lie\u00b7ben", "wir", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem Kalb, das hingegossen", "tokens": ["Von", "dem", "Kalb", ",", "das", "hin\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PDS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter Blumen wiederk\u00e4ut,", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "wie\u00b7der\u00b7k\u00e4ut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber nicht von wilden Rossen,", "tokens": ["A\u00b7ber", "nicht", "von", "wil\u00b7den", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deren N\u00fcster schnaubt nach Streit.", "tokens": ["De\u00b7ren", "N\u00fcs\u00b7ter", "schnaubt", "nach", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Von Dam\u00f6t, dem Sch\u00e4ferknechte,", "tokens": ["Von", "Da\u00b7m\u00f6t", ",", "dem", "Sch\u00e4\u00b7fer\u00b7knech\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der auf seiner Syrinx pfeift,", "tokens": ["Der", "auf", "sei\u00b7ner", "Sy\u00b7rinx", "pfeift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht vom Helden, dessen Rechte", "tokens": ["Nicht", "vom", "Hel\u00b7den", ",", "des\u00b7sen", "Rech\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nach dem Stern der Ehre greift.", "tokens": ["Nach", "dem", "Stern", "der", "Eh\u00b7re", "greift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Willst Du Dich verliebt gebaren,", "tokens": ["Willst", "Du", "Dich", "ver\u00b7liebt", "ge\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun so liebe nach der Mode,", "tokens": ["Nun", "so", "lie\u00b7be", "nach", "der", "Mo\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4rme nicht von Kampfgefahren", "tokens": ["L\u00e4r\u00b7me", "nicht", "von", "Kampf\u00b7ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von tausendfachem Tode.", "tokens": ["Und", "von", "tau\u00b7send\u00b7fa\u00b7chem", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes L\u00f6ckchen, jedes Gr\u00fcbchen", "tokens": ["Je\u00b7des", "L\u00f6ck\u00b7chen", ",", "je\u00b7des", "Gr\u00fcb\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werde flugs Dir zum Gedichte,", "tokens": ["Wer\u00b7de", "flugs", "Dir", "zum", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Um die Schl\u00e4fe Deinem Liebchen", "tokens": ["Um", "die", "Schl\u00e4\u00b7fe", "Dei\u00b7nem", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Winde Du Vergi\u00dfmeinnichte.", "tokens": ["Win\u00b7de", "Du", "Ver\u00b7gi\u00df\u00b7mein\u00b7nich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Von verschmachtendem Entfernen,", "tokens": ["Von", "ver\u00b7schmach\u00b7ten\u00b7dem", "Ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nicht von trotziger Entf\u00fchrung,", "tokens": ["Nicht", "von", "trot\u00b7zi\u00b7ger", "Ent\u00b7f\u00fch\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sing' von Blumen, sing' von Sternen,", "tokens": ["Sing'", "von", "Blu\u00b7men", ",", "sing'", "von", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und zerschmilz vor lauter R\u00fchrung! \u2013", "tokens": ["Und", "zer\u00b7schmilz", "vor", "lau\u00b7ter", "R\u00fch\u00b7rung", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u2013 Ich bedaure sehr, Ihr Guten,", "tokens": ["\u2013", "Ich", "be\u00b7dau\u00b7re", "sehr", ",", "Ihr", "Gu\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein'ge J\u00e4hrchen m\u00fc\u00dft Ihr harren,", "tokens": ["Ein'\u00b7ge", "J\u00e4hr\u00b7chen", "m\u00fc\u00dft", "Ihr", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis verl\u00f6scht die jungen Gluten", "tokens": ["Bis", "ver\u00b7l\u00f6scht", "die", "jun\u00b7gen", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verkohlt mein toller Sparren.", "tokens": ["Und", "ver\u00b7kohlt", "mein", "tol\u00b7ler", "Spar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Singt indessen meinethalben", "tokens": ["Singt", "in\u00b7des\u00b7sen", "mei\u00b7net\u00b7hal\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eure zarten Leberreimchen,", "tokens": ["Eu\u00b7re", "zar\u00b7ten", "Le\u00b7ber\u00b7reim\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vom Gezwitscher junger Schwalben", "tokens": ["Vom", "Ge\u00b7zwit\u00b7scher", "jun\u00b7ger", "Schwal\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und vom Klagelied der Heimchen.", "tokens": ["Und", "vom", "Kla\u00b7ge\u00b7lied", "der", "Heim\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ja! Besingt im Gras die Veilchen", "tokens": ["Ja", "!", "Be\u00b7singt", "im", "Gras", "die", "Veil\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und am Bach die alten Weiden,", "tokens": ["Und", "am", "Bach", "die", "al\u00b7ten", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Aber g\u00f6nnt mir's noch ein Weilchen,", "tokens": ["A\u00b7ber", "g\u00f6nnt", "mir's", "noch", "ein", "Weil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mich in rauhern Stoff zu kleiden.", "tokens": ["Mich", "in", "rau\u00b7hern", "Stoff", "zu", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was Ihr stets ins Ohr mir fl\u00fcstert,", "tokens": ["Was", "Ihr", "stets", "ins", "Ohr", "mir", "fl\u00fcs\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was Ihr scheltet die Manier,", "tokens": ["Was", "Ihr", "schel\u00b7tet", "die", "Ma\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht, das ist mit mir verschwistert,", "tokens": ["Seht", ",", "das", "ist", "mit", "mir", "ver\u00b7schwis\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PDS", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist erzeugt, erstarkt in mir.", "tokens": ["Ist", "er\u00b7zeugt", ",", "er\u00b7starkt", "in", "mir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich dem frommen Pelikane", "tokens": ["Gleich", "dem", "from\u00b7men", "Pe\u00b7li\u00b7ka\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tr\u00e4nkt' ich's mit dem eignen Blut,", "tokens": ["Tr\u00e4nkt'", "ich's", "mit", "dem", "eig\u00b7nen", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Im verwegnen Dichterwahne,", "tokens": ["Im", "ver\u00b7weg\u00b7nen", "Dich\u00b7ter\u00b7wah\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df es keine niedre Brut;", "tokens": ["Da\u00df", "es", "kei\u00b7ne", "nied\u00b7re", "Brut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df dereinst es matt und machtlos", "tokens": ["Da\u00df", "de\u00b7reinst", "es", "matt", "und", "macht\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nicht im Neste werde liegen,", "tokens": ["Nicht", "im", "Nes\u00b7te", "wer\u00b7de", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wenn die andern k\u00fchn und achtlos", "tokens": ["Wenn", "die", "an\u00b7dern", "k\u00fchn", "und", "acht\u00b7los"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJD", "KON", "XY"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Durchs Gew\u00f6lk zur Sonne fliegen.", "tokens": ["Durchs", "Ge\u00b7w\u00f6lk", "zur", "Son\u00b7ne", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Seht, ich lass' Euch Eure Bl\u00fcmchen,", "tokens": ["Seht", ",", "ich", "lass'", "Euch", "Eu\u00b7re", "Bl\u00fcm\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eure Liebchen und Manierchen,", "tokens": ["Eu\u00b7re", "Lieb\u00b7chen", "und", "Ma\u00b7nier\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Euer wohlerworbnes R\u00fchmchen,", "tokens": ["Eu\u00b7er", "woh\u00b7ler\u00b7worb\u00b7nes", "R\u00fchm\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn Ihr seid sonst gute Tierchen.", "tokens": ["Denn", "Ihr", "seid", "sonst", "gu\u00b7te", "Tier\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft auch mich! Ich mag es leiden,", "tokens": ["La\u00dft", "auch", "mich", "!", "Ich", "mag", "es", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schl\u00e4gt der Fink und girrt der Tauber;", "tokens": ["Schl\u00e4gt", "der", "Fink", "und", "girrt", "der", "Tau\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch aus rost'gen Degenscheiden", "tokens": ["Doch", "aus", "rost'\u00b7gen", "De\u00b7gen\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Klingt f\u00fcr mich ein eigner Zauber.", "tokens": ["Klingt", "f\u00fcr", "mich", "ein", "eig\u00b7ner", "Zau\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Jeder Mann nach seinem Wahne,", "tokens": ["Je\u00b7der", "Mann", "nach", "sei\u00b7nem", "Wah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ihn verfechten nenn' ich Tugend.", "tokens": ["Ihn", "ver\u00b7fech\u00b7ten", "nenn'", "ich", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Jeder Mann zu seiner Fahne,", "tokens": ["Je\u00b7der", "Mann", "zu", "sei\u00b7ner", "Fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Meine Fahne sei die Jugend.", "tokens": ["Mei\u00b7ne", "Fah\u00b7ne", "sei", "die", "Ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Werde ruhig, werde friedlich,", "tokens": ["Wer\u00b7de", "ru\u00b7hig", ",", "wer\u00b7de", "fried\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df den Schlachtgesang vertosen,", "tokens": ["La\u00df", "den", "Schlacht\u00b7ge\u00b7sang", "ver\u00b7to\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe niedlich und gem\u00fctlich,", "tokens": ["Sin\u00b7ge", "nied\u00b7lich", "und", "ge\u00b7m\u00fct\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Statt mit Blute, schreib' mit Rosen,", "tokens": ["Statt", "mit", "Blu\u00b7te", ",", "schreib'", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df die Schwerter in der Scheide,", "tokens": ["La\u00df", "die", "Schwer\u00b7ter", "in", "der", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den Helmsturz la\u00df am Nagel,", "tokens": ["Und", "den", "Helm\u00b7sturz", "la\u00df", "am", "Na\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "La\u00df die Pferde auf der Weide", "tokens": ["La\u00df", "die", "Pfer\u00b7de", "auf", "der", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und vergi\u00df den Lanzenhagel.", "tokens": ["Und", "ver\u00b7gi\u00df", "den", "Lan\u00b7zen\u00b7ha\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "$."], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.9": {"text": "La\u00df die Hiebe ungerochen", "tokens": ["La\u00df", "die", "Hie\u00b7be", "un\u00b7ge\u00b7ro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und die Herzen unersch\u00fcttert,", "tokens": ["Und", "die", "Her\u00b7zen", "un\u00b7er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "La\u00df die Sch\u00e4del unzerbrochen", "tokens": ["La\u00df", "die", "Sch\u00e4\u00b7del", "un\u00b7zer\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und die Raben ungef\u00fcttert!", "tokens": ["Und", "die", "Ra\u00b7ben", "un\u00b7ge\u00b7f\u00fct\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Was Du singst vom Heldentume,", "tokens": ["Was", "Du", "singst", "vom", "Hel\u00b7den\u00b7tu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist gemacht und ist Manier,", "tokens": ["Ist", "ge\u00b7macht", "und", "ist", "Ma\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "KON", "VAFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singe von der G\u00e4nseblume,", "tokens": ["Sin\u00b7ge", "von", "der", "G\u00e4n\u00b7se\u00b7blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Natur, die lieben wir;", "tokens": ["Die", "Na\u00b7tur", ",", "die", "lie\u00b7ben", "wir", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem Kalb, das hingegossen", "tokens": ["Von", "dem", "Kalb", ",", "das", "hin\u00b7ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PDS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter Blumen wiederk\u00e4ut,", "tokens": ["Un\u00b7ter", "Blu\u00b7men", "wie\u00b7der\u00b7k\u00e4ut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber nicht von wilden Rossen,", "tokens": ["A\u00b7ber", "nicht", "von", "wil\u00b7den", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Deren N\u00fcster schnaubt nach Streit.", "tokens": ["De\u00b7ren", "N\u00fcs\u00b7ter", "schnaubt", "nach", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Von Dam\u00f6t, dem Sch\u00e4ferknechte,", "tokens": ["Von", "Da\u00b7m\u00f6t", ",", "dem", "Sch\u00e4\u00b7fer\u00b7knech\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der auf seiner Syrinx pfeift,", "tokens": ["Der", "auf", "sei\u00b7ner", "Sy\u00b7rinx", "pfeift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht vom Helden, dessen Rechte", "tokens": ["Nicht", "vom", "Hel\u00b7den", ",", "des\u00b7sen", "Rech\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nach dem Stern der Ehre greift.", "tokens": ["Nach", "dem", "Stern", "der", "Eh\u00b7re", "greift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Willst Du Dich verliebt gebaren,", "tokens": ["Willst", "Du", "Dich", "ver\u00b7liebt", "ge\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun so liebe nach der Mode,", "tokens": ["Nun", "so", "lie\u00b7be", "nach", "der", "Mo\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4rme nicht von Kampfgefahren", "tokens": ["L\u00e4r\u00b7me", "nicht", "von", "Kampf\u00b7ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von tausendfachem Tode.", "tokens": ["Und", "von", "tau\u00b7send\u00b7fa\u00b7chem", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes L\u00f6ckchen, jedes Gr\u00fcbchen", "tokens": ["Je\u00b7des", "L\u00f6ck\u00b7chen", ",", "je\u00b7des", "Gr\u00fcb\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werde flugs Dir zum Gedichte,", "tokens": ["Wer\u00b7de", "flugs", "Dir", "zum", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Um die Schl\u00e4fe Deinem Liebchen", "tokens": ["Um", "die", "Schl\u00e4\u00b7fe", "Dei\u00b7nem", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Winde Du Vergi\u00dfmeinnichte.", "tokens": ["Win\u00b7de", "Du", "Ver\u00b7gi\u00df\u00b7mein\u00b7nich\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Von verschmachtendem Entfernen,", "tokens": ["Von", "ver\u00b7schmach\u00b7ten\u00b7dem", "Ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nicht von trotziger Entf\u00fchrung,", "tokens": ["Nicht", "von", "trot\u00b7zi\u00b7ger", "Ent\u00b7f\u00fch\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Sing' von Blumen, sing' von Sternen,", "tokens": ["Sing'", "von", "Blu\u00b7men", ",", "sing'", "von", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Und zerschmilz vor lauter R\u00fchrung! \u2013", "tokens": ["Und", "zer\u00b7schmilz", "vor", "lau\u00b7ter", "R\u00fch\u00b7rung", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u2013 Ich bedaure sehr, Ihr Guten,", "tokens": ["\u2013", "Ich", "be\u00b7dau\u00b7re", "sehr", ",", "Ihr", "Gu\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein'ge J\u00e4hrchen m\u00fc\u00dft Ihr harren,", "tokens": ["Ein'\u00b7ge", "J\u00e4hr\u00b7chen", "m\u00fc\u00dft", "Ihr", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis verl\u00f6scht die jungen Gluten", "tokens": ["Bis", "ver\u00b7l\u00f6scht", "die", "jun\u00b7gen", "Glu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verkohlt mein toller Sparren.", "tokens": ["Und", "ver\u00b7kohlt", "mein", "tol\u00b7ler", "Spar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Singt indessen meinethalben", "tokens": ["Singt", "in\u00b7des\u00b7sen", "mei\u00b7net\u00b7hal\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eure zarten Leberreimchen,", "tokens": ["Eu\u00b7re", "zar\u00b7ten", "Le\u00b7ber\u00b7reim\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Vom Gezwitscher junger Schwalben", "tokens": ["Vom", "Ge\u00b7zwit\u00b7scher", "jun\u00b7ger", "Schwal\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und vom Klagelied der Heimchen.", "tokens": ["Und", "vom", "Kla\u00b7ge\u00b7lied", "der", "Heim\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ja! Besingt im Gras die Veilchen", "tokens": ["Ja", "!", "Be\u00b7singt", "im", "Gras", "die", "Veil\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "