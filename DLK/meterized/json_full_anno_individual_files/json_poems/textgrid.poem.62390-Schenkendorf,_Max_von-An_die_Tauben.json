{"textgrid.poem.62390": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "An die Tauben", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fliegt nur aus, geliebte Tauben!", "tokens": ["Fliegt", "nur", "aus", ",", "ge\u00b7lieb\u00b7te", "Tau\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch als Boten send' ich hin;", "tokens": ["Euch", "als", "Bo\u00b7ten", "send'", "ich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sagt ihr, und sie wird euch glauben,", "tokens": ["Sagt", "ihr", ",", "und", "sie", "wird", "euch", "glau\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich krank vor Liebe bin.", "tokens": ["Da\u00df", "ich", "krank", "vor", "Lie\u00b7be", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihr k\u00f6nnt fliegen, ihr k\u00f6nnt eilen,", "tokens": ["Ihr", "k\u00f6nnt", "flie\u00b7gen", ",", "ihr", "k\u00f6nnt", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tauben, froh bergab und an;", "tokens": ["Tau\u00b7ben", ",", "froh", "berg\u00b7ab", "und", "an", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich mu\u00df in der Fremde weilen,", "tokens": ["Ich", "mu\u00df", "in", "der", "Frem\u00b7de", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ewig ein gequ\u00e4lter Mann.", "tokens": ["E\u00b7wig", "ein", "ge\u00b7qu\u00e4l\u00b7ter", "Mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Auch mein Brieflein soll noch gehen", "tokens": ["Auch", "mein", "Brie\u00b7flein", "soll", "noch", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heut zu ihr, mein Liebesgru\u00df,", "tokens": ["Heut", "zu", "ihr", ",", "mein", "Lie\u00b7bes\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll sie suchen auf den H\u00f6hen,", "tokens": ["Soll", "sie", "su\u00b7chen", "auf", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem sch\u00f6nen gr\u00fcnen Flu\u00df.", "tokens": ["An", "dem", "sch\u00f6\u00b7nen", "gr\u00fc\u0