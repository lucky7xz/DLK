{"textgrid.poem.37934": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Prinzenraub", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir wolln ein Liedel heben an,", "tokens": ["Wir", "wolln", "ein", "Lie\u00b7del", "he\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was sich hat angespunnen,", "tokens": ["Was", "sich", "hat", "an\u00b7ge\u00b7spun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie's im Plei\u00dfnerland gar schlecht war bestallt,", "tokens": ["Wie's", "im", "Plei\u00df\u00b7ner\u00b7land", "gar", "schlecht", "war", "be\u00b7stallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADV", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Als den jungen F\u00fcrst'n geschah Gewalt,", "tokens": ["Als", "den", "jun\u00b7gen", "F\u00fcr\u00b7st'n", "ge\u00b7schah", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Durch Kuntzen von Kauffungen.", "tokens": ["Durch", "Kunt\u00b7zen", "von", "Kauf\u00b7fun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Adler hat auf'm Fels gebaut", "tokens": ["Der", "Ad\u00b7ler", "hat", "auf'm", "Fels", "ge\u00b7baut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein sch\u00f6nes Nest mit Jungen,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Nest", "mit", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie er einst geflogen aus,", "tokens": ["Und", "wie", "er", "einst", "ge\u00b7flo\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Holt ein Geyer die Jungen heraus,", "tokens": ["Holt", "ein", "Ge\u00b7yer", "die", "Jun\u00b7gen", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Drauf ward's Nest leer gefunden.", "tokens": ["Drauf", "ward's", "Nest", "leer", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo der Geyer auf'm Dache sitzt,", "tokens": ["Wo", "der", "Ge\u00b7yer", "auf'm", "Da\u00b7che", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedeihen die K\u00fcchlein selten,", "tokens": ["Ge\u00b7dei\u00b7hen", "die", "K\u00fcch\u00b7lein", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es war da ein seltsam Narrenspiel,", "tokens": ["Es", "war", "da", "ein", "selt\u00b7sam", "Nar\u00b7ren\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Welcher F\u00fcrst seinen R\u00e4then traut zu viel,", "tokens": ["Wel\u00b7cher", "F\u00fcrst", "sei\u00b7nen", "R\u00e4\u00b7then", "traut", "zu", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPOSAT", "NN", "VVFIN", "APPR", "PIS", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Mu\u00df oft es selber entgelten.", "tokens": ["Mu\u00df", "oft", "es", "sel\u00b7ber", "ent\u00b7gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Altenburg, du feine Stadt,", "tokens": ["Al\u00b7ten\u00b7burg", ",", "du", "fei\u00b7ne", "Stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich th\u00e4t er mit Untreu meinen,", "tokens": ["Dich", "th\u00e4t", "er", "mit", "Un\u00b7treu", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da in dir war'n all' Hofleut voll,", "tokens": ["Da", "in", "dir", "wa\u00b7r'n", "all'", "Hof\u00b7leut", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kam Kunz mit Leitern und Buben toll,", "tokens": ["Kam", "Kunz", "mit", "Lei\u00b7tern", "und", "Bu\u00b7ben", "toll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und holt die F\u00fcrsten so kleine.", "tokens": ["Und", "holt", "die", "F\u00fcrs\u00b7ten", "so", "klei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADJA", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Was blast dich, Kunz, f\u00fcr Unlust an,", "tokens": ["Was", "blast", "dich", ",", "Kunz", ",", "f\u00fcr", "Un\u00b7lust", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NE", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da du ins Schlo\u00df einsteigest?", "tokens": ["Da", "du", "ins", "Schlo\u00df", "ein\u00b7stei\u00b7gest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und stiehlst die zarten Herren heraus,", "tokens": ["Und", "stiehlst", "die", "zar\u00b7ten", "Her\u00b7ren", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Als der Kurf\u00fcrst eben nit war zu Haus,", "tokens": ["Als", "der", "Kur\u00b7f\u00fcrst", "e\u00b7ben", "nit", "war", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "PTKNEG", "VAFIN", "APPR", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die zarten F\u00fcrsten-Zweige.", "tokens": ["Die", "zar\u00b7ten", "F\u00fcrs\u00b7ten\u00b7Zwei\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es war wohl als ein Wunderding,", "tokens": ["Es", "war", "wohl", "als", "ein", "Wun\u00b7der\u00b7ding", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sich das Land beweget,", "tokens": ["Wie", "sich", "das", "Land", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was da auf'n Stra\u00dfen war'n f\u00fcr Leut',", "tokens": ["Was", "da", "auf'n", "Stra\u00b7\u00dfen", "wa\u00b7r'n", "f\u00fcr", "Leut'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPRART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die den R\u00e4ubern folgten nach in Zeit,", "tokens": ["Die", "den", "R\u00e4u\u00b7bern", "folg\u00b7ten", "nach", "in", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "All's wibbelt, kribbelt, sich beweget.", "tokens": ["All's", "wib\u00b7belt", ",", "krib\u00b7belt", ",", "sich", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "$,", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Im Walde dort ward Kunz ertappt,", "tokens": ["Im", "Wal\u00b7de", "dort", "ward", "Kunz", "er\u00b7tappt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wollt er Beeren naschen;", "tokens": ["Da", "wollt", "er", "Bee\u00b7ren", "na\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4r er in der Hast wacker fortgeritten,", "tokens": ["W\u00e4r", "er", "in", "der", "Hast", "wa\u00b7cker", "fort\u00b7ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df 'n die K\u00f6hler nit gefangen h\u00e4tten,", "tokens": ["Da\u00df", "'n", "die", "K\u00f6h\u00b7ler", "nit", "ge\u00b7fan\u00b7gen", "h\u00e4t\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "PTKNEG", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "H\u00e4tt er sie kunt verpaschen.", "tokens": ["H\u00e4tt", "er", "sie", "kunt", "ver\u00b7pa\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ab'r sie wurden ihm wieder abgejagt,", "tokens": ["Ab'r", "sie", "wur\u00b7den", "ihm", "wie\u00b7der", "ab\u00b7ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und Kunz mit seinen Gesellen", "tokens": ["Und", "Kunz", "mit", "sei\u00b7nen", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf Gr\u00fcnhain, in unsers Herrn Abts Gewalt", "tokens": ["Auf", "Gr\u00fcn\u00b7hain", ",", "in", "un\u00b7sers", "Herrn", "Ab\u00b7ts", "Ge\u00b7walt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "NE", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gebracht, und auf die Zwika gestellt,", "tokens": ["Ge\u00b7bracht", ",", "und", "auf", "die", "Zwi\u00b7ka", "ge\u00b7stellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und muste sich lassen prellen.", "tokens": ["Und", "mus\u00b7te", "sich", "las\u00b7sen", "prel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Daf\u00fcr fiel ab gar mancher Kopf,", "tokens": ["Da\u00b7f\u00fcr", "fiel", "ab", "gar", "man\u00b7cher", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und keiner der Gefangnen", "tokens": ["Und", "kei\u00b7ner", "der", "Ge\u00b7fang\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kam aus der Haft ganzbeinigt davon,", "tokens": ["Kam", "aus", "der", "Haft", "ganz\u00b7bei\u00b7nigt", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVPP", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schwerdt, Rad, Zang'n, Strick, die war'n ihr Lohn,", "tokens": ["Schwerdt", ",", "Rad", ",", "Zang'n", ",", "Strick", ",", "die", "wa\u00b7r'n", "ihr", "Lohn", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NE", "$,", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Man sah die R\u00fcmpfe hangen.", "tokens": ["Man", "sah", "die", "R\u00fcmp\u00b7fe", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "So geht's, wer wider die Obrigkeit", "tokens": ["So", "geht's", ",", "wer", "wi\u00b7der", "die", "Ob\u00b7rig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWS", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich unbesonnen emp\u00f6ret.", "tokens": ["Sich", "un\u00b7be\u00b7son\u00b7nen", "em\u00b7p\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wers nicht meint, schau an Kuntzen,", "tokens": ["Wers", "nicht", "meint", ",", "schau", "an", "Kunt\u00b7zen", ","], "tok