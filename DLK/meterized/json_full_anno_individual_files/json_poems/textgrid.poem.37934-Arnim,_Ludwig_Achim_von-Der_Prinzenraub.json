 "gar", "man\u00b7cher", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und keiner der Gefangnen", "tokens": ["Und", "kei\u00b7ner", "der", "Ge\u00b7fang\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kam aus der Haft ganzbeinigt davon,", "tokens": ["Kam", "aus", "der", "Haft", "ganz\u00b7bei\u00b7nigt", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVPP", "PAV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schwerdt, Rad, Zang'n, Strick, die war'n ihr Lohn,", "tokens": ["Schwerdt", ",", "Rad", ",", "Zang'n", ",", "Strick", ",", "die", "wa\u00b7r'n", "ihr", "Lohn", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "NE", "$,", "NN", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Man sah die R\u00fcmpfe hangen.", "tokens": ["Man", "sah", "die", "R\u00fcmp\u00b7fe", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "So geht's, wer wider die Obrigkeit", "tokens": ["So", "geht's", ",", "wer", "wi\u00b7der", "die", "Ob\u00b7rig\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWS", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich unbesonnen emp\u00f6ret.", "tokens": ["Sich", "un\u00b7be\u00b7son\u00b7nen", "em\u00b7p\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wers nicht meint, schau an Kuntzen,", "tokens": ["Wers", "nicht", "meint", ",", "schau", "an", "Kunt\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "VVFIN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein Kopf thut z' Freiberg noch runterschmunzen,", "tokens": ["Sein", "Kopf", "thut", "z'", "Frei\u00b7berg", "noch", "run\u00b7ter\u00b7schmun\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "NE", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und Jedermann davon lehret.", "tokens": ["Und", "Je\u00b7der\u00b7mann", "da\u00b7von", "leh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.22": {"line.1": {"text": "Gott thu den frommen Christen alles Guts,", "tokens": ["Gott", "thu", "den", "from\u00b7men", "Chris\u00b7ten", "al\u00b7les", "Guts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und la\u00df die jungen Herren,", "tokens": ["Und", "la\u00df", "die", "jun\u00b7gen", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In kein Feindes Hand mehr also komm'n,", "tokens": ["In", "kein", "Fein\u00b7des", "Hand", "mehr", "al\u00b7so", "kom\u00b7m'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Geb auch der Frau Churf\u00fcrstin viel Fromm'n,", "tokens": ["Geb", "auch", "der", "Frau", "Chur\u00b7f\u00fcrs\u00b7tin", "viel", "From\u00b7m'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NE", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df wir uns in Ruhe ern\u00e4hren.", "tokens": ["Da\u00df", "wir", "uns", "in", "Ru\u00b7he", "er\u00b7n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}