{"dta.poem.18454": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Einer frauen wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Doctor/ der schier nacht und tag", "tokens": ["Zum", "Doc\u00b7tor", "/", "der", "schier", "nacht", "und", "tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$(", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Studiert und auf den b\u00fcchern lag/", "tokens": ["Stu\u00b7diert", "und", "auf", "den", "b\u00fc\u00b7chern", "lag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sprach seine frau. Ich w\u00fcnsch allein.", "tokens": ["Sprach", "sei\u00b7ne", "frau", ".", "Ich", "w\u00fcnsch", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein rares buch vor euch zu seyn;", "tokens": ["Ein", "ra\u00b7res", "buch", "vor", "euch", "zu", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So seyd/ sprach er/ denn unter den Calendern/", "tokens": ["So", "seyd", "/", "sprach", "er", "/", "denn", "un\u00b7ter", "den", "Ca\u00b7len\u00b7dern", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$(", "VVFIN", "PPER", "$(", "KON", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die kan und mus man alle jahr verendern.", "tokens": ["Die", "kan", "und", "mus", "man", "al\u00b7le", "jahr", "ver\u00b7en\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "KON", "VMFIN", "PIS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}