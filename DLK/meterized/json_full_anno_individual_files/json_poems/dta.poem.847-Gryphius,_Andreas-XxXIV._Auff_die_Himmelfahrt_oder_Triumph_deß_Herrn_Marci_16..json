{"dta.poem.847": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXIV.  Auff die Himmelfahrt oder Triumph  \n de\u00df Herrn/ Marci 16.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Triumf/ der Tod ist todt/ Triumf! jhr Himmel-scharen ", "tokens": ["Tri\u00b7umf", "/", "der", "Tod", "ist", "todt", "/", "Tri\u00b7umf", "!", "jhr", "Him\u00b7mel\u00b7scha\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$(", "ART", "NN", "VAFIN", "ADJD", "$(", "NE", "$.", "PPOSAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Trinmpf! die H\u00f6lle ligt! Mein K\u00f6nig f\u00e4hret auff!", "tokens": ["Trinmpf", "!", "die", "H\u00f6l\u00b7le", "ligt", "!", "Mein", "K\u00f6\u00b7nig", "f\u00e4h\u00b7ret", "auff", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Vnd f\u00fchrt gebunden schaw/ der Teuffel schwartzen hauff", "tokens": ["Vnd", "f\u00fchrt", "ge\u00b7bun\u00b7den", "schaw", "/", "der", "Teuf\u00b7fel", "schwart\u00b7zen", "hauff"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVPP", "VVFIN", "$(", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er l\u00f6\u00dft die auf/ die fest in S\u00fcnden ketten waren/", "tokens": ["Er", "l\u00f6\u00dft", "die", "auf", "/", "die", "fest", "in", "S\u00fcn\u00b7den", "ket\u00b7ten", "wa\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPR", "$(", "ART", "ADJD", "APPR", "NN", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er bindet was vn\u00df zwang/ vnd den vor so viel jahren", "tokens": ["Er", "bin\u00b7det", "was", "vn\u00df", "zwang", "/", "vnd", "den", "vor", "so", "viel", "jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PWS", "PPER", "VVFIN", "$(", "KON", "ART", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Parady\u00df versperr\u2019t durch Mutter Evens kauff/", "tokens": ["Das", "Pa\u00b7ra\u00b7dy\u00df", "ver\u00b7sperr't", "durch", "Mut\u00b7ter", "E\u00b7vens", "kauff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "NE", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd Adams Bi\u00df holt eyn/ sein Blut/ vnd Lehr/ vnd", "tokens": ["Vnd", "A\u00b7dams", "Bi\u00df", "holt", "eyn", "/", "sein", "Blut", "/", "vnd", "Lehr", "/", "vnd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "NN", "APPR", "VVFIN", "ART", "$(", "PPOSAT", "NN", "$(", "KON", "NN", "$(", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tauff ", "tokens": ["Tauff"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Triumf der HERR f\u00e4hrt auff mit vielmal tausend paaren.", "tokens": ["Tri\u00b7umf", "der", "HeRR", "f\u00e4hrt", "auff", "mit", "viel\u00b7mal", "tau\u00b7send", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "APPR", "APPR", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jtzt sitzt Er vnd regirt an EOttes rechten Hand", "tokens": ["Jtzt", "sitzt", "Er", "vnd", "re\u00b7girt", "an", "E\u00b7ot\u00b7tes", "rech\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vnd tritt/ was sich vorhin in seinem Ha\u00df verband/", "tokens": ["Vnd", "tritt", "/", "was", "sich", "vor\u00b7hin", "in", "sei\u00b7nem", "Ha\u00df", "ver\u00b7band", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PWS", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor Jhm mu\u00df Himmel/ Erd/ vnd H\u00f6ll vnd Todt sich neig\u1ebd.", "tokens": ["Vor", "Jhm", "mu\u00df", "Him\u00b7mel", "/", "Erd", "/", "vnd", "H\u00f6ll", "vnd", "Todt", "sich", "neig\u1ebd", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "NN", "$(", "NN", "$(", "KON", "NN", "KON", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch bleibt Er auch bey vn\u00df/ weil noch die So\u00f1e wacht", "tokens": ["Doch", "bleibt", "Er", "auch", "bey", "vn\u00df", "/", "weil", "noch", "die", "So\u00f1e", "wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NE", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Weil noch der Stern\u1ebd glantz vmbgibt die schwartze Nacht", "tokens": ["Weil", "noch", "der", "Stern\u1ebd", "glantz", "vmb\u00b7gibt", "die", "schwart\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Bi\u00df Er der Erd jhr End\u2019 vn\u00df wird den Himmel zeigen.", "tokens": ["Bi\u00df", "Er", "der", "Erd", "jhr", "End'", "vn\u00df", "wird", "den", "Him\u00b7mel", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PPOSAT", "NN", "ADJD", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}