{"textgrid.poem.55960": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Fahnentr\u00e4ger", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Andern f\u00fchlen alles an sich rauh", "tokens": ["Die", "An\u00b7dern", "f\u00fch\u00b7len", "al\u00b7les", "an", "sich", "rauh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "APPR", "PRF", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und ohne Anteil: Eisen, Zeug und Leder.", "tokens": ["und", "oh\u00b7ne", "An\u00b7teil", ":", "Ei\u00b7sen", ",", "Zeug", "und", "Le\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$.", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zwar manchmal schmeichelt eine weiche Feder,", "tokens": ["Zwar", "manch\u00b7mal", "schmei\u00b7chelt", "ei\u00b7ne", "wei\u00b7che", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "doch sehr allein und lieb-los ist ein jeder;", "tokens": ["doch", "sehr", "al\u00b7lein", "und", "lie\u00b7blos", "ist", "ein", "je\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "KON", "ADJD", "VAFIN", "ART", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "er aber tr\u00e4gt \u2013 als tr\u00fcg er eine Frau \u2013", "tokens": ["er", "a\u00b7ber", "tr\u00e4gt", "\u2013", "als", "tr\u00fcg", "er", "ei\u00b7ne", "Frau", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$(", "KOKOM", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "die Fahne in dem feierlichen Kleide.", "tokens": ["die", "Fah\u00b7ne", "in", "dem", "fei\u00b7er\u00b7li\u00b7chen", "Klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dicht hinter ihm geht ihre schwere Seide,", "tokens": ["Dicht", "hin\u00b7ter", "ihm", "geht", "ih\u00b7re", "schwe\u00b7re", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "die manchmal \u00fcber seine H\u00e4nde flie\u00dft.", "tokens": ["die", "manch\u00b7mal", "\u00fc\u00b7ber", "sei\u00b7ne", "H\u00e4n\u00b7de", "flie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Er kann allein, wenn er die Augen schlie\u00dft,", "tokens": ["Er", "kann", "al\u00b7lein", ",", "wenn", "er", "die", "Au\u00b7gen", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "ein L\u00e4cheln sehn: er darf sie nicht verlassen. \u2013", "tokens": ["ein", "L\u00e4\u00b7cheln", "sehn", ":", "er", "darf", "sie", "nicht", "ver\u00b7las\u00b7sen", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wenn es kommt in blitzenden K\u00fcrassen", "tokens": ["Und", "wenn", "es", "kommt", "in", "blit\u00b7zen\u00b7den", "K\u00fc\u00b7ras\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und nach ihr greift und ringt und will sie fassen \u2013:", "tokens": ["und", "nach", "ihr", "greift", "und", "ringt", "und", "will", "sie", "fas\u00b7sen", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VMFIN", "PPER", "VVINF", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "dann darf er sie abrei\u00dfen von dem Stocke", "tokens": ["dann", "darf", "er", "sie", "ab\u00b7rei\u00b7\u00dfen", "von", "dem", "Sto\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "als ri\u00df er sie aus ihrem M\u00e4dchentum,", "tokens": ["als", "ri\u00df", "er", "sie", "aus", "ih\u00b7rem", "M\u00e4d\u00b7chen\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "um sie zu halten unterm Waffenrocke.", "tokens": ["um", "sie", "zu", "hal\u00b7ten", "un\u00b7term", "Waf\u00b7fen\u00b7ro\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "w