{"textgrid.poem.51272": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Todt, als ich dich recht, als wie ich sol, erkannt:", "tokens": ["O", "Todt", ",", "als", "ich", "dich", "recht", ",", "als", "wie", "ich", "sol", ",", "er\u00b7kannt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,", "KOUS", "PWAV", "PPER", "VMFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hei\u00df ich dich allezeit mein neugebohrnes Leben:", "tokens": ["Hei\u00df", "ich", "dich", "al\u00b7le\u00b7zeit", "mein", "neu\u00b7ge\u00b7bohr\u00b7nes", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich w\u00fcste nicht, nach was ein Hertze solte streben,", "tokens": ["Ich", "w\u00fcs\u00b7te", "nicht", ",", "nach", "was", "ein", "Hert\u00b7ze", "sol\u00b7te", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "APPR", "PRELS", "ART", "VVFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im fall es nicht sein Thun auff diesen Port gewanndt:", "tokens": ["Im", "fall", "es", "nicht", "sein", "Thun", "auff", "die\u00b7sen", "Port", "ge\u00b7wanndt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wir stehen in Gefahr, wann wir auff diese Th\u00fcr,", "tokens": ["Wir", "ste\u00b7hen", "in", "Ge\u00b7fahr", ",", "wann", "wir", "auff", "die\u00b7se", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PWAV", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die zu dem Leben geht nicht schicken unsre Sinnen,", "tokens": ["Die", "zu", "dem", "Le\u00b7ben", "geht", "nicht", "schi\u00b7cken", "uns\u00b7re", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wol dem, der zu der Post den Schl\u00fc\u00dfel kan beginnen:", "tokens": ["Wol", "dem", ",", "der", "zu", "der", "Post", "den", "Schl\u00fc\u00b7\u00dfel", "kan", "be\u00b7gin\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Ich lebte nicht, lebt' ich, o Tod, ohn dich allhier.", "tokens": ["Ich", "leb\u00b7te", "nicht", ",", "lebt'", "ich", ",", "o", "Tod", ",", "ohn", "dich", "all\u00b7hier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "KOUI", "PPER", "ADV", "$."], "meter": "-+