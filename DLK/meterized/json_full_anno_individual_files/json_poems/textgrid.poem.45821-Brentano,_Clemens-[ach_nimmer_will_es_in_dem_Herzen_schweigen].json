{"textgrid.poem.45821": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ach nimmer will es in dem Herzen schweigen]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach nimmer will es in dem Herzen schweigen", "tokens": ["Ach", "nim\u00b7mer", "will", "es", "in", "dem", "Her\u00b7zen", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es treibt mich fort, fort aus des Lebens Mitte;", "tokens": ["Es", "treibt", "mich", "fort", ",", "fort", "aus", "des", "Le\u00b7bens", "Mit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PTKVZ", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo eil' ich hin, wer leitet meine Schritte,", "tokens": ["Wo", "eil'", "ich", "hin", ",", "wer", "lei\u00b7tet", "mei\u00b7ne", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ferne weite Ziel noch zu erreichen?", "tokens": ["Das", "fer\u00b7ne", "wei\u00b7te", "Ziel", "noch", "zu", "er\u00b7rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "All mein Leben,", "tokens": ["All", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Holder Fr\u00fchling,", "tokens": ["Hol\u00b7der", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Morgenr\u00f6te,", "tokens": ["Mor\u00b7gen\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Abendschimmer,", "tokens": ["A\u00b7bend\u00b7schim\u00b7mer", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Immer weiter abw\u00e4rts schweben.", "tokens": ["Im\u00b7mer", "wei\u00b7ter", "ab\u00b7w\u00e4rts", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O stehe still du eilender Gedanken,", "tokens": ["O", "ste\u00b7he", "still", "du", "ei\u00b7len\u00b7der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df mich in Deine klaren Augen sehen,", "tokens": ["La\u00df", "mich", "in", "Dei\u00b7ne", "kla\u00b7ren", "Au\u00b7gen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df alle meine Leiden dann vergehen.", "tokens": ["Da\u00df", "al\u00b7le", "mei\u00b7ne", "Lei\u00b7den", "dann", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O weile, sieh, ich kann nicht weiter wanken.", "tokens": ["O", "wei\u00b7le", ",", "sieh", ",", "ich", "kann", "nicht", "wei\u00b7ter", "wan\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach, erbarme", "tokens": ["Ach", ",", "er\u00b7bar\u00b7me"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$,", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Dich der Liebe,", "tokens": ["Dich", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Die die Sinne", "tokens": ["Die", "die", "Sin\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Mir gel\u00f6set;", "tokens": ["Mir", "ge\u00b7l\u00f6\u00b7set", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "La\u00df mich ruhn in Deinem Arme.", "tokens": ["La\u00df", "mich", "ruhn", "in", "Dei\u00b7nem", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die F\u00fc\u00dfe hab' ich blutig schon gegangen,", "tokens": ["Die", "F\u00fc\u00b7\u00dfe", "hab'", "ich", "blu\u00b7tig", "schon", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne brennt so hei\u00df auf mich hernieder,", "tokens": ["Die", "Son\u00b7ne", "brennt", "so", "hei\u00df", "auf", "mich", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und immer finde ich den Kummer wieder,", "tokens": ["Und", "im\u00b7mer", "fin\u00b7de", "ich", "den", "Kum\u00b7mer", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bin ewig in dem Leben eingefangen.", "tokens": ["Bin", "e\u00b7wig", "in", "dem", "Le\u00b7ben", "ein\u00b7ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Niemals weilen", "tokens": ["Nie\u00b7mals", "wei\u00b7len"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Seine Schritte,", "tokens": ["Sei\u00b7ne", "Schrit\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Vor mir fliehet", "tokens": ["Vor", "mir", "flie\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Sein Gebilde;", "tokens": ["Sein", "Ge\u00b7bil\u00b7de", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Nimmer kann ich ihn ereilen.", "tokens": ["Nim\u00b7mer", "kann", "ich", "ihn", "er\u00b7ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Denken all in Liebe ist ertrunken,", "tokens": ["Mein", "Den\u00b7ken", "all", "in", "Lie\u00b7be", "ist", "er\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00fcberall seh' ich den M\u00e4cht'gen stehen,", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "seh'", "ich", "den", "M\u00e4cht'\u00b7gen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcberall h\u00f6r' ich ihn mich verschm\u00e4hen;", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "h\u00f6r'", "ich", "ihn", "mich", "ver\u00b7schm\u00e4\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bin stets in tiefen bangen Schmerz versunken.", "tokens": ["Bin", "stets", "in", "tie\u00b7fen", "ban\u00b7gen", "Schmerz", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von dem Gl\u00fccke", "tokens": ["Von", "dem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ihn zu h\u00f6ren,", "tokens": ["Ihn", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Seine Blicke", "tokens": ["Sei\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Aufzufangen,", "tokens": ["Auf\u00b7zu\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Blicket nie ein Strahl zur\u00fccke.", "tokens": ["Bli\u00b7cket", "nie", "ein", "Strahl", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da sitz' ich weinend nun an fremder Schwelle,", "tokens": ["Da", "sitz'", "ich", "wei\u00b7nend", "nun", "an", "frem\u00b7der", "Schwel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und harr' und hoffe, was ich nie erreiche,", "tokens": ["Und", "harr'", "und", "hof\u00b7fe", ",", "was", "ich", "nie", "er\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ich im Herzen tief und still verschweige;", "tokens": ["Was", "ich", "im", "Her\u00b7zen", "tief", "und", "still", "ver\u00b7schwei\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ADJD", "KON", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nimmermehr wird mir die Aussicht helle.", "tokens": ["Und", "nim\u00b7mer\u00b7mehr", "wird", "mir", "die", "Aus\u00b7sicht", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Muntre Lieder,", "tokens": ["Mun\u00b7tre", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Freudig Lachen,", "tokens": ["Freu\u00b7dig", "La\u00b7chen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Frohe Blicke,", "tokens": ["Fro\u00b7he", "Bli\u00b7cke", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Leichte Sinne,", "tokens": ["Leich\u00b7te", "Sin\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Nimmer kehren sie mir wieder.", "tokens": ["Nim\u00b7mer", "keh\u00b7ren", "sie", "mir", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "So m\u00f6ge denn die Blume niedersterben,", "tokens": ["So", "m\u00f6\u00b7ge", "denn", "die", "Blu\u00b7me", "nie\u00b7ders\u00b7ter\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die ohne Nahrung sich hinabgeh\u00e4rmt,", "tokens": ["Die", "oh\u00b7ne", "Nah\u00b7rung", "sich", "hin\u00b7ab\u00b7ge\u00b7h\u00e4rmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die nie ein goldner Sonnenstrahl erw\u00e4rmt;", "tokens": ["Die", "nie", "ein", "gold\u00b7ner", "Son\u00b7nen\u00b7strahl", "er\u00b7w\u00e4rmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich habe nur gelebet zu verderben.", "tokens": ["Ich", "ha\u00b7be", "nur", "ge\u00b7le\u00b7bet", "zu", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Frohes Leben", "tokens": ["Fro\u00b7hes", "Le\u00b7ben"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ihn umschlinge", "tokens": ["Ihn", "um\u00b7schlin\u00b7ge"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "Dicht mit Freude;", "tokens": ["Dicht", "mit", "Freu\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Von der Armen", "tokens": ["Von", "der", "Ar\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.9": {"text": "Sollst Du nie ihm Kundschaft geben.", "tokens": ["Sollst", "Du", "nie", "ihm", "Kund\u00b7schaft", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach nimmer will es in dem Herzen schweigen", "tokens": ["Ach", "nim\u00b7mer", "will", "es", "in", "dem", "Her\u00b7zen", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es treibt mich fort, fort aus des Lebens Mitte;", "tokens": ["Es", "treibt", "mich", "fort", ",", "fort", "aus", "des", "Le\u00b7bens", "Mit\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PTKVZ", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo eil' ich hin, wer leitet meine Schritte,", "tokens": ["Wo", "eil'", "ich", "hin", ",", "wer", "lei\u00b7tet", "mei\u00b7ne", "Schrit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ferne weite Ziel noch zu erreichen?", "tokens": ["Das", "fer\u00b7ne", "wei\u00b7te", "Ziel", "noch", "zu", "er\u00b7rei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "All mein Leben,", "tokens": ["All", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Holder Fr\u00fchling,", "tokens": ["Hol\u00b7der", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "punct"], 