{"dta.poem.14331": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachruf an meinen Bruder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Der Herr wird Dich f\u00fchren.", "tokens": ["Der", "Herr", "wird", "Dich", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Tief kann ich ja sp\u00fcren", "tokens": ["Tief", "kann", "ich", "ja", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Der Sterne still Walten.", "tokens": ["Der", "Ster\u00b7ne", "still", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Erde Gestalten", "tokens": ["Der", "Er\u00b7de", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kaum h\u00f6rbar sich r\u00fchren.", "tokens": ["Kaum", "h\u00f6r\u00b7bar", "sich", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Durch Nacht und durch Graus", "tokens": ["Durch", "Nacht", "und", "durch", "Graus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Gen Morgen, nach Haus \u2014", "tokens": ["Gen", "Mor\u00b7gen", ",", "nach", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "APPR", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Ja, Gott wird mich f\u00fchren.", "tokens": ["Ja", ",", "Gott", "wird", "mich", "f\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}