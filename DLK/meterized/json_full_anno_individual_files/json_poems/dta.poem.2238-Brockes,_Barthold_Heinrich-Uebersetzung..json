{"dta.poem.2238": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wofern der Mensch, zu Gottes Ruhm sich zu vergn\u00fc-", "tokens": ["Wo\u00b7fern", "der", "Mensch", ",", "zu", "Got\u00b7tes", "Ruhm", "sich", "zu", "ver\u00b7gn\u00fc"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "NN", "NN", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gen, nicht erschaffen,", "tokens": ["gen", ",", "nicht", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Und h\u00e4tt\u2019 er, in Bewundrung, Gott zu ehren, keine F\u00e4-", "tokens": ["Und", "h\u00e4tt'", "er", ",", "in", "Be\u00b7wund\u00b7rung", ",", "Gott", "zu", "eh\u00b7ren", ",", "kei\u00b7ne", "F\u00e4"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "$,", "APPR", "NN", "$,", "NN", "PTKZU", "VVINF", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "higkeit;", "tokens": ["hig\u00b7keit", ";"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "++", "measure": "spondeus"}, "line.5": {"text": "So w\u00e4re zwischen einem Menschen und einem Vieh kein", "tokens": ["So", "w\u00e4\u00b7re", "zwi\u00b7schen", "ei\u00b7nem", "Men\u00b7schen", "und", "ei\u00b7nem", "Vieh", "kein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "KON", "ART", "NN", "PIAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Unterscheid,", "tokens": ["Un\u00b7ter\u00b7scheid", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und folgte draus: Die Menschen w\u00e4ren nichts anders,", "tokens": ["Und", "folg\u00b7te", "draus", ":", "Die", "Men\u00b7schen", "w\u00e4\u00b7ren", "nichts", "an\u00b7ders", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$.", "ART", "NN", "VAFIN", "PIS", "ADV", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "als ein' Art von Affen.", "tokens": ["als", "ein'", "Art", "von", "Af\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}