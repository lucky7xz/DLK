{"textgrid.poem.34032": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Natur und Mensch", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft schon kam es mir vor, Natur, als h\u00e4tt'st du zu zeitig", "tokens": ["Oft", "schon", "kam", "es", "mir", "vor", ",", "Na\u00b7tur", ",", "als", "h\u00e4tt'st", "du", "zu", "zei\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "NN", "$,", "KOKOM", "VAFIN", "PPER", "PTKA", "ADJD"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In dein Werk dich verliebt und die Vollendung vers\u00e4umt.", "tokens": ["In", "dein", "Werk", "dich", "ver\u00b7liebt", "und", "die", "Vol\u00b7len\u00b7dung", "ver\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVPP", "KON", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Weil der Mensch dir gefiel, so bleibst du stehen bei'm Menschen,", "tokens": ["Weil", "der", "Mensch", "dir", "ge\u00b7fiel", ",", "so", "bleibst", "du", "ste\u00b7hen", "bei'm", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und erwecktest in ihm nicht noch den schlummernden Gott.", "tokens": ["Und", "er\u00b7weck\u00b7test", "in", "ihm", "nicht", "noch", "den", "schlum\u00b7mern\u00b7den", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Aber nun tr\u00e4umt er von dem, und weil er erwachend sich wieder", "tokens": ["A\u00b7ber", "nun", "tr\u00e4umt", "er", "von", "dem", ",", "und", "weil", "er", "er\u00b7wa\u00b7chend", "sich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "ART", "$,", "KON", "KOUS", "PPER", "VVPP", "PRF", "ADV"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Findet, wie eben vorher, f\u00e4llt er zur\u00fcck in das Thier.", "tokens": ["Fin\u00b7det", ",", "wie", "e\u00b7ben", "vor\u00b7her", ",", "f\u00e4llt", "er", "zu\u00b7r\u00fcck", "in", "das", "Thier", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "ADV", "$,", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Oft schon kam es mir vor, Natur, als h\u00e4tt'st du zu zeitig", "tokens": ["Oft", "schon", "kam", "es", "mir", "vor", ",", "Na\u00b7tur", ",", "als", "h\u00e4tt'st", "du", "zu", "zei\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "NN", "$,", "KOKOM", "VAFIN", "PPER", "PTKA", "ADJD"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In dein Werk dich verliebt und die Vollendung vers\u00e4umt.", "tokens": ["In", "dein", "Werk", "dich", "ver\u00b7liebt", "und", "die", "Vol\u00b7len\u00b7dung", "ver\u00b7s\u00e