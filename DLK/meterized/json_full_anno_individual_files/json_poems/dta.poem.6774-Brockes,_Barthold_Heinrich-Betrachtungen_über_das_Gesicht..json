{"dta.poem.6774": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und, eh\u2019 er durch ein \u00fcbereilt Bewegen", "tokens": ["Und", ",", "eh'", "er", "durch", "ein", "\u00fc\u00b7be\u00b7reilt", "Be\u00b7we\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "ART", "VVPP", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich selbst entf\u00fchren l\u00e4sst, erst selber \u00fcberlegen,", "tokens": ["Sich", "selbst", "ent\u00b7f\u00fch\u00b7ren", "l\u00e4sst", ",", "erst", "sel\u00b7ber", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VVFIN", "$,", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf welche Weis er siehet, was er sieht,", "tokens": ["Auf", "wel\u00b7che", "Weis", "er", "sie\u00b7het", ",", "was", "er", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und was sein Werckzeug sey: wo er sich nicht bem\u00fcht", "tokens": ["Und", "was", "sein", "Wer\u00b7ck\u00b7zeug", "sey", ":", "wo", "er", "sich", "nicht", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VAFIN", "$.", "PWAV", "PPER", "PRF", "PTKNEG", "VVFIN"], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Dergleichen wol zu unterscheiden;", "tokens": ["Derg\u00b7lei\u00b7chen", "wol", "zu", "un\u00b7ter\u00b7schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So ist die Folge schlimm. So, wann uns, nach dem Schein", "tokens": ["So", "ist", "die", "Fol\u00b7ge", "schlimm", ".", "So", ",", "wann", "uns", ",", "nach", "dem", "Schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$.", "ADV", "$,", "PWAV", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Thurm, der viereckt ist, von fern scheint rund zu seyn.", "tokens": ["Ein", "Thurm", ",", "der", "vier\u00b7eckt", "ist", ",", "von", "fern", "scheint", "rund", "zu", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "APPR", "ADJD", "VVFIN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wenn ein gerader Stock, den wir ins Wasser sencken,", "tokens": ["Wenn", "ein", "ge\u00b7ra\u00b7der", "Stock", ",", "den", "wir", "ins", "Was\u00b7ser", "sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gekr\u00fcmmt scheint, ohn da\u00df man durchs Aug\u2019 kan anders", "tokens": ["Ge\u00b7kr\u00fcmmt", "scheint", ",", "ohn", "da\u00df", "man", "durchs", "Aug'", "kan", "an\u00b7ders"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "$,", "KOUI", "KOUS", "PIS", "APPRART", "NN", "VMFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "dencken:", "tokens": ["den\u00b7cken", ":"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "So spricht man zwar, es mu\u00df ein Sinn den andern lehren,", "tokens": ["So", "spricht", "man", "zwar", ",", "es", "mu\u00df", "ein", "Sinn", "den", "an\u00b7dern", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "PPER", "VMFIN", "ART", "NN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die F\u00fcsse d\u00fcrffen uns zum Thurm nur n\u00e4her f\u00fchren,", "tokens": ["Die", "F\u00fcs\u00b7se", "d\u00fcrf\u00b7fen", "uns", "zum", "Thurm", "nur", "n\u00e4\u00b7her", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Man darf ja nur den Stock ber\u00fchren:", "tokens": ["Man", "darf", "ja", "nur", "den", "Stock", "be\u00b7r\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Allein,", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Wo soll denn endlich doch der Wahrheit Richtschnur seyn?", "tokens": ["Wo", "soll", "denn", "end\u00b7lich", "doch", "der", "Wahr\u00b7heit", "Richt\u00b7schnur", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ADV", "ADV", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ist es nicht blo\u00df der Geist, der sich bem\u00fcht und lernet", "tokens": ["Ist", "es", "nicht", "blo\u00df", "der", "Geist", ",", "der", "sich", "be\u00b7m\u00fcht", "und", "ler\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "VVPP", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Ursach, wodurch er vom Wahren sich entfernet?", "tokens": ["Die", "Ur\u00b7sach", ",", "wo\u00b7durch", "er", "vom", "Wah\u00b7ren", "sich", "ent\u00b7fer\u00b7net", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}}}}