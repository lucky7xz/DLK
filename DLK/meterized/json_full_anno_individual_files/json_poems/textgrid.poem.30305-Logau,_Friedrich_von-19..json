{"textgrid.poem.30305": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kan es wol gestehen, da\u00df zierliche Geberden", "tokens": ["Ich", "kan", "es", "wol", "ge\u00b7ste\u00b7hen", ",", "da\u00df", "zier\u00b7li\u00b7che", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und h\u00f6fliches verhalten in Franckreich k\u00fcndig werden;", "tokens": ["Und", "h\u00f6f\u00b7li\u00b7ches", "ver\u00b7hal\u00b7ten", "in", "Fran\u00b7ck\u00b7reich", "k\u00fcn\u00b7dig", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NE", "ADJD", "VAINF", "$."], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Di\u00df aber k\u00fcmt zu wichtig, da\u00df gar nichts sonst soll t\u00fcgen,", "tokens": ["Di\u00df", "a\u00b7ber", "k\u00fcmt", "zu", "wich\u00b7tig", ",", "da\u00df", "gar", "nichts", "sonst", "soll", "t\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PTKA", "ADJD", "$,", "KOUS", "ADV", "PIS", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was Deutsche f\u00fcr sich selbsten an eigner Art verm\u00fcgen.", "tokens": ["Was", "Deut\u00b7sche", "f\u00fcr", "sich", "selbs\u00b7ten", "an", "eig\u00b7ner", "Art", "ver\u00b7m\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", 