{"textgrid.poem.40400": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vergn\u00fcglich fl\u00fcsternd ziehn des Neckar Wogen", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vergn\u00fcglich fl\u00fcsternd ziehn des Neckar Wogen", "tokens": ["Ver\u00b7gn\u00fcg\u00b7lich", "fl\u00fcs\u00b7ternd", "ziehn", "des", "Ne\u00b7ckar", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vorbei dem Ursitz deutscher Wissenschaft,", "tokens": ["Vor\u00b7bei", "dem", "Ur\u00b7sitz", "deut\u00b7scher", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hoch ob der Br\u00fccke schlanken Pfeilerbogen", "tokens": ["Hoch", "ob", "der", "Br\u00fc\u00b7cke", "schlan\u00b7ken", "Pfei\u00b7ler\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hebt sich des Schlosses giebelstolze Kraft.", "tokens": ["Hebt", "sich", "des", "Schlos\u00b7ses", "gie\u00b7bel\u00b7stol\u00b7ze", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Bl\u00fctenschnee von Kirschen, Pfirsich, Flieder", "tokens": ["Ein", "Bl\u00fc\u00b7ten\u00b7schnee", "von", "Kir\u00b7schen", ",", "Pfir\u00b7sich", ",", "Flie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "NE", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Flockt duftverhauchend um das junge Gr\u00fcn,", "tokens": ["Flockt", "duft\u00b7ver\u00b7hau\u00b7chend", "um", "das", "jun\u00b7ge", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und prangt Altheidelberg im Lenzschmuck wieder,", "tokens": ["Und", "prangt", "Al\u00b7thei\u00b7del\u00b7berg", "im", "Lenz\u00b7schmuck", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sorgt niemand viel sich um des Lebens M\u00fchn.", "tokens": ["Sorgt", "nie\u00b7mand", "viel", "sich", "um", "des", "Le\u00b7bens", "M\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In diesem Tal der wei\u00dfen Bl\u00fctenb\u00e4ume", "tokens": ["In", "die\u00b7sem", "Tal", "der", "wei\u00b7\u00dfen", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kam mir des Ortes Genius oft genaht", "tokens": ["Kam", "mir", "des", "Or\u00b7tes", "Ge\u00b7nius", "oft", "ge\u00b7naht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "ADJA", "NN", "ADV", "VVPP"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und f\u00fcgte Scherz, Humor und heitre Tr\u00e4ume", "tokens": ["Und", "f\u00fcg\u00b7te", "Scherz", ",", "Hu\u00b7mor", "und", "heit\u00b7re", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Wissensernst der alten Musenstadt.", "tokens": ["Zum", "Wis\u00b7sen\u00b7sernst", "der", "al\u00b7ten", "Mu\u00b7sen\u00b7stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er ging nicht steif in klassischen Gewanden,", "tokens": ["Er", "ging", "nicht", "steif", "in", "klas\u00b7si\u00b7schen", "Ge\u00b7wan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ging keck und flott und trank wie ein Student", "tokens": ["Ging", "keck", "und", "flott", "und", "trank", "wie", "ein", "Stu\u00b7dent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "VVFIN", "KON", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und glich nicht viel den neun antiken Tanten,", "tokens": ["Und", "glich", "nicht", "viel", "den", "neun", "an\u00b7ti\u00b7ken", "Tan\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ART", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die man im Mythus mit Apollo nennt.", "tokens": ["Die", "man", "im", "My\u00b7thus", "mit", "A\u00b7pol\u00b7lo", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}}, "stanza.3": {"line.1": {"text": "Was er mich lehrte, bracht' ich in den ", "tokens": ["Was", "er", "mich", "lehr\u00b7te", ",", "bracht'", "ich", "in", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PRF", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo eine treubew\u00e4hrte Freundesschar", "tokens": ["Wo", "ei\u00b7ne", "treu\u00b7be\u00b7w\u00e4hr\u00b7te", "Freun\u00b7des\u00b7schar"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Mittwoch in den Donnerstag zu l\u00e4ngern", "tokens": ["Den", "Mitt\u00b7woch", "in", "den", "Don\u00b7ners\u00b7tag", "zu", "l\u00e4n\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei goldnem Rheinwein oft beflissen war.", "tokens": ["Bei", "gold\u00b7nem", "Rhein\u00b7wein", "oft", "be\u00b7flis\u00b7sen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da fiel's nicht schwer, die Saiten hell zu schlagen,", "tokens": ["Da", "fiel's", "nicht", "schwer", ",", "die", "Sai\u00b7ten", "hell", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADJD", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Selbst w\u00fcrdige Pfarrherrn wurden singend laut,", "tokens": ["Selbst", "w\u00fcr\u00b7di\u00b7ge", "Pfar\u00b7rherrn", "wur\u00b7den", "sin\u00b7gend", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wenn uns ein Meister dessen Tod wir klagen,", "tokens": ["Wenn", "uns", "ein", "Meis\u00b7ter", "des\u00b7sen", "Tod", "wir", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PRELAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit kundiger Hand den Maientrank gebraut.", "tokens": ["Mit", "kun\u00b7di\u00b7ger", "Hand", "den", "Mai\u00b7en\u00b7trank", "ge\u00b7braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Zwei Kesselpauken dienten als Orchester", "tokens": ["Zwei", "Kes\u00b7sel\u00b7pau\u00b7ken", "dien\u00b7ten", "als", "Or\u00b7ches\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "KOUS", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und eines Ofenschirms gewalztes Blech,", "tokens": ["Und", "ei\u00b7nes", "O\u00b7fen\u00b7schirms", "ge\u00b7walz\u00b7tes", "Blech", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das dr\u00f6hnte oft zum Rundgesange fester", "tokens": ["Das", "dr\u00f6hn\u00b7te", "oft", "zum", "Rund\u00b7ge\u00b7san\u00b7ge", "fes\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn Meeressturm und wilden Heers Gezech.", "tokens": ["Denn", "Mee\u00b7res\u00b7sturm", "und", "wil\u00b7den", "Heers", "Ge\u00b7zech", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zum lustigen Wort fand sich die lustige Weise", "tokens": ["Zum", "lus\u00b7ti\u00b7gen", "Wort", "fand", "sich", "die", "lus\u00b7ti\u00b7ge", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und oft scholl Beifall unsrer schlichten Art,", "tokens": ["Und", "oft", "scholl", "Bei\u00b7fall", "uns\u00b7rer", "schlich\u00b7ten", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als l\u00e4g' in diesem Maiweinnipekreise", "tokens": ["Als", "l\u00e4g'", "in", "die\u00b7sem", "Mai\u00b7wein\u00b7ni\u00b7pe\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Waldmeisters Wunderhorn als Schatz verwahrt.", "tokens": ["Wald\u00b7meis\u00b7ters", "Wun\u00b7der\u00b7horn", "als", "Schatz", "ver\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Als von der Neckarstadt, der ewig heitern,", "tokens": ["Als", "von", "der", "Ne\u00b7ckar\u00b7stadt", ",", "der", "e\u00b7wig", "hei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zur Ferne sich mein Lebenspfad gewandt,", "tokens": ["Zur", "Fer\u00b7ne", "sich", "mein", "Le\u00b7ben\u00b7spfad", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ward manch ein Schreibebrief noch aus dem ", "tokens": ["Ward", "manch", "ein", "Schrei\u00b7be\u00b7brief", "noch", "aus", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "ART", "NN", "ADV", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Freundesgru\u00df dem Engern zugesandt.", "tokens": ["Mit", "Freun\u00b7des\u00b7gru\u00df", "dem", "En\u00b7gern", "zu\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von welschen wie von deutschen Landschaftsbildern", "tokens": ["Von", "wel\u00b7schen", "wie", "von", "deut\u00b7schen", "Land\u00b7schafts\u00b7bil\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hielt dies und das Erinnerung zur\u00fcck", "tokens": ["Hielt", "dies", "und", "das", "E\u00b7rin\u00b7ne\u00b7rung", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PDS", "KON", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Gleich Bl\u00e4ttern eines Skizzenbuchs: sie schildern", "tokens": ["Gleich", "Bl\u00e4t\u00b7tern", "ei\u00b7nes", "Skiz\u00b7zen\u00b7buchs", ":", "sie", "schil\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "NN", "ART", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Harmloser Wanderlust verfl\u00fcchtigt Gl\u00fcck.", "tokens": ["Harm\u00b7lo\u00b7ser", "Wan\u00b7der\u00b7lust", "ver\u00b7fl\u00fcch\u00b7tigt", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Nun schau' ich aus solidem Schwabenalter", "tokens": ["Nun", "schau'", "ich", "aus", "so\u00b7li\u00b7dem", "Schwa\u00b7ben\u00b7al\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Auf dieser Lyrik jugendtollen Schwung", "tokens": ["Auf", "die\u00b7ser", "Ly\u00b7rik", "ju\u00b7gend\u00b7tol\u00b7len", "Schwung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und reiche l\u00e4chelnd meinen Liederpsalter", "tokens": ["Und", "rei\u00b7che", "l\u00e4\u00b7chelnd", "mei\u00b7nen", "Lie\u00b7der\u00b7psal\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Zechern allen, die im Herzen jung.", "tokens": ["Den", "Ze\u00b7chern", "al\u00b7len", ",", "die", "im", "Her\u00b7zen", "jung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "$,", "PRELS", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wer Spa\u00df versteht, wird manchmal kr\u00e4ftigst lachen,", "tokens": ["Wer", "Spa\u00df", "ver\u00b7steht", ",", "wird", "manch\u00b7mal", "kr\u00e4f\u00b7tigst", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wen manch Lied schier allzu durstig deucht,", "tokens": ["Und", "wen", "manch", "Lied", "schier", "all\u00b7zu", "durs\u00b7tig", "deucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIAT", "NN", "ADJD", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der tr\u00f6ste sich: 's war anders nicht zu machen,", "tokens": ["Der", "tr\u00f6s\u00b7te", "sich", ":", "'s", "war", "an\u00b7ders", "nicht", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der Genius Loci Heidelbergs ist feucht!", "tokens": ["Der", "Ge\u00b7nius", "Lo\u00b7ci", "Hei\u00b7del\u00b7bergs", "ist", "feucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Vergn\u00fcglich fl\u00fcsternd ziehn des Neckar Wogen", "tokens": ["Ver\u00b7gn\u00fcg\u00b7lich", "fl\u00fcs\u00b7ternd", "ziehn", "des", "Ne\u00b7ckar", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "