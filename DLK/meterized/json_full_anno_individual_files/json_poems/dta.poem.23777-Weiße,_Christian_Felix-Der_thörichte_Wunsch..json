{"dta.poem.23777": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der th\u00f6richte Wunsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df ich nicht ein Vogel bin,", "tokens": ["O", "da\u00df", "ich", "nicht", "ein", "Vo\u00b7gel", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schnell und federleicht,", "tokens": ["So", "schnell", "und", "fe\u00b7der\u00b7leicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der \u00fcber Berg und Th\u00e4ler hin", "tokens": ["Der", "\u00fc\u00b7ber", "Berg", "und", "Th\u00e4\u00b7ler", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Augenblicken streicht.", "tokens": ["In", "Au\u00b7gen\u00b7bli\u00b7cken", "streicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann fl\u00f6h ich \u00fcber Land und See,", "tokens": ["Dann", "fl\u00f6h", "ich", "\u00fc\u00b7ber", "Land", "und", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchreiste jeden Ort,", "tokens": ["Durc\u00b7hreis\u00b7te", "je\u00b7den", "Ort", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4r bald im Thal, bald in der H\u00f6h,", "tokens": ["W\u00e4r", "bald", "im", "Thal", ",", "bald", "in", "der", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald hier, bald wieder dort.", "tokens": ["Bald", "hier", ",", "bald", "wie\u00b7der", "dort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann sucht ich stets den Ort mir aus,", "tokens": ["Dann", "sucht", "ich", "stets", "den", "Ort", "mir", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo Lenz und Sommer bl\u00fchn,", "tokens": ["Wo", "Lenz", "und", "Som\u00b7mer", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und baute mir mein fl\u00fcchtig Haus", "tokens": ["Und", "bau\u00b7te", "mir", "mein", "fl\u00fcch\u00b7tig", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An sch\u00f6nsten Oertern hin.", "tokens": ["An", "sch\u00f6ns\u00b7ten", "O\u00b7er\u00b7tern", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Bald schw\u00e4ng ich mit der Lerche Schall", "tokens": ["Bald", "schw\u00e4ng", "ich", "mit", "der", "Ler\u00b7che", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In L\u00fcften mich empor:", "tokens": ["In", "L\u00fcf\u00b7ten", "mich", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald schl\u00fcg ich, wie die Nachtigall,", "tokens": ["Bald", "schl\u00fcg", "ich", ",", "wie", "die", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus dunkeln Str\u00e4uchen vor.", "tokens": ["Aus", "dun\u00b7keln", "Str\u00e4u\u00b7chen", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bald fl\u00f6g ich, wie ein Adler fliegt &#8212;", "tokens": ["Bald", "fl\u00f6g", "ich", ",", "wie", "ein", "Ad\u00b7ler", "fliegt", "&#8212;"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "XML_entity"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch \u2014 welch ein Schu\u00df geschah?", "tokens": ["Doch", "welch", "ein", "Schu\u00df", "ge\u00b7schah", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWAT", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "O weh! ein armer Vogel liegt", "tokens": ["O", "weh", "!", "ein", "ar\u00b7mer", "Vo\u00b7gel", "liegt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "ART", "ADJA", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seinem Blute da.", "tokens": ["In", "sei\u00b7nem", "Blu\u00b7te", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl mir! da\u00df nicht mein Wunsch", "tokens": ["Wohl", "mir", "!", "da\u00df", "nicht", "mein", "Wunsch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$.", "KOUS", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "gelang,", "tokens": ["ge\u00b7lang", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Wie sollt es mich gereun!", "tokens": ["Wie", "sollt", "es", "mich", "ge\u00b7reun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie gro\u00df ist, Gott, Gott sey es Dank!", "tokens": ["Wie", "gro\u00df", "ist", ",", "Gott", ",", "Gott", "sey", "es", "Dank", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "NN", "$,", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Gl\u00fcck ein Mensch zu seyn!", "tokens": ["Das", "Gl\u00fcck", "ein", "Mensch", "zu", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}