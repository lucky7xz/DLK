fo": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Sieh, dein Volk, es z\u00fcrnt nicht l\u00e4nger,", "tokens": ["Sieh", ",", "dein", "Volk", ",", "es", "z\u00fcrnt", "nicht", "l\u00e4n\u00b7ger", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das dich einst so schwer verkannte;", "tokens": ["Das", "dich", "einst", "so", "schwer", "ver\u00b7kann\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber du, verkl\u00e4rter S\u00e4nger,", "tokens": ["A\u00b7ber", "du", ",", "ver\u00b7kl\u00e4r\u00b7ter", "S\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weilst im Paradies des Dante,", "tokens": ["Weilst", "im", "Pa\u00b7ra\u00b7dies", "des", "Dan\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die Sonnen reiner brennen,", "tokens": ["Wo", "die", "Son\u00b7nen", "rei\u00b7ner", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Monde voller schimmern;", "tokens": ["Und", "die", "Mon\u00b7de", "vol\u00b7ler", "schim\u00b7mern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unser Kennen und Verkennen", "tokens": ["Un\u00b7ser", "Ken\u00b7nen", "und", "Ver\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wird dich schwerlich mehr bek\u00fcmmern.", "tokens": ["Wird", "dich", "schwer\u00b7lich", "mehr", "be\u00b7k\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}