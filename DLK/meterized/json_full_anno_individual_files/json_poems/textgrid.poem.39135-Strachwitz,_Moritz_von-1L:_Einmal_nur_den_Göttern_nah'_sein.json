ic.tetra"}, "line.3": {"text": "Eines Martyrs, der wie Keiner", "tokens": ["Ei\u00b7nes", "Mar\u00b7tyrs", ",", "der", "wie", "Kei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr das Reich der Kunst gelitten,", "tokens": ["F\u00fcr", "das", "Reich", "der", "Kunst", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem das Wort, ein Fr\u00fchlingswetter,", "tokens": ["Dem", "das", "Wort", ",", "ein", "Fr\u00fch\u00b7lings\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Von den Lippen rauschte, rollte,", "tokens": ["Von", "den", "Lip\u00b7pen", "rauschte", ",", "roll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Dem der Zorn, wie Zorn der G\u00f6tter,", "tokens": ["Dem", "der", "Zorn", ",", "wie", "Zorn", "der", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "$,", "PWAV", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die olymp'sche Stirn umgrollte.", "tokens": ["Die", "o\u00b7lym\u00b7p'\u00b7sche", "Stirn", "um\u00b7groll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sieh, dein Volk, e