meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Ketten los noch w\u00fcrde", "tokens": ["Die\u00b7ser", "Ket\u00b7ten", "los", "noch", "w\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADJD", "ADV", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Vaterland noch s\u00e4he,", "tokens": ["Und", "mein", "Va\u00b7ter\u00b7land", "noch", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, der Sklave w\u00e4r' ein Konig!", "tokens": ["Ach", ",", "der", "Skla\u00b7ve", "w\u00e4r'", "ein", "Ko\u00b7nig", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}