{"dta.poem.20734": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Milon .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zanken will ich nicht und klagen,               ", "tokens": ["Zan\u00b7ken", "will", "ich", "nicht", "und", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber eins mu\u00df ich dir sagen:", "tokens": ["A\u00b7ber", "eins", "mu\u00df", "ich", "dir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du, der du mein Herz gewannst,", "tokens": ["Du", ",", "der", "du", "mein", "Herz", "ge\u00b7wannst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Milon, der du mich bewirthen", "tokens": ["Mi\u00b7lon", ",", "der", "du", "mich", "be\u00b7wirt\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPER", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch ein freundlich L\u00e4cheln kannst,", "tokens": ["Durch", "ein", "freund\u00b7lich", "L\u00e4\u00b7cheln", "kannst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Du verschm\u00e4htest j\u00fcngst die Myrthen,", "tokens": ["Du", "ver\u00b7schm\u00e4h\u00b7test", "j\u00fcngst", "die", "Myr\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil du dich nicht drauf besannst,", "tokens": ["Weil", "du", "dich", "nicht", "drauf", "be\u00b7sannst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df dein Weigern mich betr\u00fcbte,", "tokens": ["Da\u00df", "dein", "Wei\u00b7gern", "mich", "be\u00b7tr\u00fcb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ach du wustest nicht, da\u00df ich", "tokens": ["Ach", "du", "wus\u00b7test", "nicht", ",", "da\u00df", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "In die Veilchen mich verliebte,", "tokens": ["In", "die", "Veil\u00b7chen", "mich", "ver\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Welche zum Beneiden sich", "tokens": ["Wel\u00b7che", "zum", "Be\u00b7nei\u00b7den", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "APPRART", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Dir ans Herz gelegt befanden,", "tokens": ["Dir", "ans", "Herz", "ge\u00b7legt", "be\u00b7fan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Tauschen wollt ich gern mit dir,", "tokens": ["Tau\u00b7schen", "wollt", "ich", "gern", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und du hast mich nicht verstanden.", "tokens": ["Und", "du", "hast", "mich", "nicht", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Diese Veilchen w\u00e4ren mir", "tokens": ["Die\u00b7se", "Veil\u00b7chen", "w\u00e4\u00b7ren", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Heiliger noch als die andern,", "tokens": ["Hei\u00b7li\u00b7ger", "noch", "als", "die", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Die dein Diener mir gebracht;", "tokens": ["Die", "dein", "Die\u00b7ner", "mir", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Und sie sollten mit mir wandern", "tokens": ["Und", "sie", "soll\u00b7ten", "mit", "mir", "wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "In des finstern Grabes Nacht.", "tokens": ["In", "des", "fins\u00b7tern", "Gra\u00b7bes", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "O wie kannst du das verachten,", "tokens": ["O", "wie", "kannst", "du", "das", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Was dir meine Liebe beut;", "tokens": ["Was", "dir", "mei\u00b7ne", "Lie\u00b7be", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Kannst du nicht mein Herz betrachten", "tokens": ["Kannst", "du", "nicht", "mein", "Herz", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Bei der Blumen Kleinigkeit?", "tokens": ["Bei", "der", "Blu\u00b7men", "Klei\u00b7nig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Pfl\u00fccke du mir auf dem Platze,", "tokens": ["Pfl\u00fc\u00b7cke", "du", "mir", "auf", "dem", "Plat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Wo dein Fu\u00df zu wandeln pflegt,", "tokens": ["Wo", "dein", "Fu\u00df", "zu", "wan\u00b7deln", "pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PTKZU", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Bl\u00fcmchen, die der Grasraum tr\u00e4gt,", "tokens": ["Bl\u00fcm\u00b7chen", ",", "die", "der", "Gras\u00b7raum", "tr\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Und ich mache sie zum Schatze.", "tokens": ["Und", "ich", "ma\u00b7che", "sie", "zum", "Schat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "G\u00e4nsebl\u00fcmchen n\u00e4hm ich an,", "tokens": ["G\u00e4n\u00b7se\u00b7bl\u00fcm\u00b7chen", "n\u00e4hm", "ich", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Und ein Zweigchen von den B\u00e4umen,", "tokens": ["Und", "ein", "Zweig\u00b7chen", "von", "den", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Die ein jeder nutzen kann;", "tokens": ["Die", "ein", "je\u00b7der", "nut\u00b7zen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Wo in l\u00fcgnerischen Tr\u00e4umen", "tokens": ["Wo", "in", "l\u00fcg\u00b7ne\u00b7ri\u00b7schen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Sich der arme Kriegesmann", "tokens": ["Sich", "der", "ar\u00b7me", "Krie\u00b7ges\u00b7mann"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Ausgestreckt am Tische weidet,", "tokens": ["Aus\u00b7ge\u00b7streckt", "am", "Ti\u00b7sche", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und noch hungert, wenn er wacht,", "tokens": ["Und", "noch", "hun\u00b7gert", ",", "wenn", "er", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Und den Reichen noch beneidet,", "tokens": ["Und", "den", "Rei\u00b7chen", "noch", "be\u00b7nei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Der sich Promenaden macht. \u2014", "tokens": ["Der", "sich", "Pro\u00b7me\u00b7na\u00b7den", "macht", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PRF", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Solch ein Zweigchen, du mein Lieber!", "tokens": ["Solch", "ein", "Zweig\u00b7chen", ",", "du", "mein", "Lie\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Brich mir im Begegnen ab,", "tokens": ["Brich", "mir", "im", "Be\u00b7geg\u00b7nen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Und ich freue mich dar\u00fcber,", "tokens": ["Und", "ich", "freu\u00b7e", "mich", "da\u00b7r\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Weil mirs Milon gab. \u2014", "tokens": ["Weil", "mirs", "Mi\u00b7lon", "gab", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NE", "NE", "VVFIN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}