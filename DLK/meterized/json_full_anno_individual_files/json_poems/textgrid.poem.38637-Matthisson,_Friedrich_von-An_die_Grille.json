{"textgrid.poem.38637": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An die Grille", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter des Seethals Ulmen wandl' ich einsam.", "tokens": ["Un\u00b7ter", "des", "See\u00b7thals", "Ul\u00b7men", "wandl'", "ich", "ein\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Feiernd schweigt die Natur, kein L\u00fcftchen athmet;", "tokens": ["Fei\u00b7ernd", "schweigt", "die", "Na\u00b7tur", ",", "kein", "L\u00fcft\u00b7chen", "ath\u00b7met", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nur dein leiser Abendgesang, o Grille,", "tokens": ["Nur", "dein", "lei\u00b7ser", "A\u00b7bend\u00b7ge\u00b7sang", ",", "o", "Gril\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "FM", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "T\u00f6nt in den Bl\u00fcthen.", "tokens": ["T\u00f6nt", "in", "den", "Bl\u00fc\u00b7then", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Als ich ein Knabe war, horcht' ich mit Wonne", "tokens": ["Als", "ich", "ein", "Kna\u00b7be", "war", ",", "horcht'", "ich", "mit", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "wo