{"textgrid.poem.44948": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nie f\u00fchre dich ein Gott in seinem Grimm", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie f\u00fchre dich ein Gott in seinem Grimm", "tokens": ["Nie", "f\u00fch\u00b7re", "dich", "ein", "Gott", "in", "sei\u00b7nem", "Grimm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Menschen, die \u2013 vielleicht nicht eben schlimm \u2013", "tokens": ["Auf", "Men\u00b7schen", ",", "die", "\u2013", "viel\u00b7leicht", "nicht", "e\u00b7ben", "schlimm", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "$(", "ADV", "PTKNEG", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Kreise doch der eignen Tr\u00e4ume stehen,", "tokens": ["Im", "Krei\u00b7se", "doch", "der", "eig\u00b7nen", "Tr\u00e4u\u00b7me", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In sich der Dinge Ma\u00dfstab sehen,", "tokens": ["In", "sich", "der", "Din\u00b7ge", "Ma\u00df\u00b7stab", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr die der Scherz ein Ernst, der Ernst ein Spiel,", "tokens": ["F\u00fcr", "die", "der", "Scherz", "ein", "Ernst", ",", "der", "Ernst", "ein", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein kl\u00fcgelndes Erkennen das Gef\u00fchl,", "tokens": ["Ein", "kl\u00fc\u00b7geln\u00b7des", "Er\u00b7ken\u00b7nen", "das", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--++-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Und die die Welt, sich selbst und andre lieben,", "tokens": ["Und", "die", "die", "Welt", ",", "sich", "selbst", "und", "and\u00b7re", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "$,", "PRF", "ADV", "KON", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht wie sie sind, wie sies gedacht, geschrieben.", "tokens": ["Nicht", "wie", "sie", "sind", ",", "wie", "sies", "ge\u00b7dacht", ",", "ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "PWAV", "PPER", "VAFIN", "$,", "PWAV", "PIS", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nie f\u00fchre dich ein Gott in seinem Grimm", "tokens": ["Nie", "f\u00fch\u00b7re", "dich", "ein", "Gott", "in", "sei\u00b7nem", "Grimm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Menschen, die \u2013 vielleicht nicht eben schlimm \u2013", "tokens": ["Auf", "Men\u00b7schen", ",", "die", "\u2013", "vi