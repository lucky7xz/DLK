{"textgrid.poem.36598": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Amor, ein Vogel", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, wie dort ein kleiner Amor", "tokens": ["Sieh", ",", "wie", "dort", "ein", "klei\u00b7ner", "A\u00b7mor"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADV", "ART", "ADJA", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Myrtenb\u00e4umchen sitzt,", "tokens": ["Auf", "dem", "Myr\u00b7ten\u00b7b\u00e4um\u00b7chen", "sitzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lauschend nach den Sch\u00f6nen siehet,", "tokens": ["Lau\u00b7schend", "nach", "den", "Sch\u00f6\u00b7nen", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Mund zum Pfeifen spitzt!", "tokens": ["Und", "den", "Mund", "zum", "Pfei\u00b7fen", "spitzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denkt er eine, deren Herze", "tokens": ["Denkt", "er", "ei\u00b7ne", ",", "de\u00b7ren", "Her\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht sein sch\u00e4rfster Pfeil durchdrang,", "tokens": ["Nicht", "sein", "sch\u00e4rfs\u00b7ter", "Pfeil", "durch\u00b7drang", ",