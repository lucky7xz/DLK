{"textgrid.poem.61566": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Gesang des Priesters", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du einst in freier Liebe", "tokens": ["Der", "du", "einst", "in", "frei\u00b7er", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich in unsern Staub gebannt,", "tokens": ["Dich", "in", "un\u00b7sern", "Staub", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsrer Brust verworrne Triebe,", "tokens": ["Uns\u00b7rer", "Brust", "ver\u00b7worr\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und all ihr Leid erkannt;", "tokens": ["Ach", ",", "und", "all", "ihr", "Leid", "er\u00b7kannt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der du selbst in jenen Tagen", "tokens": ["Der", "du", "selbst", "in", "je\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schmecktest der Versuchung Pein,", "tokens": ["Schmeck\u00b7test", "der", "Ver\u00b7su\u00b7chung", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denen, die im Kampf erlagen,", "tokens": ["De\u00b7nen", ",", "die", "im", "Kampf", "er\u00b7la\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Reiner, kannst du gn\u00e4dig sein.", "tokens": ["Rei\u00b7ner", ",", "kannst", "du", "gn\u00e4\u00b7dig", "sein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, du wei\u00dft, in Sehnsucht schweifen", "tokens": ["Ach", ",", "du", "wei\u00dft", ",", "in", "Sehn\u00b7sucht", "schwei\u00b7fen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$,", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausend Geister weit und breit;", "tokens": ["Tau\u00b7send", "Geis\u00b7ter", "weit", "und", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, vom Schein bet\u00f6rt, ergreifen", "tokens": ["Doch", ",", "vom", "Schein", "be\u00b7t\u00f6rt", ",", "er\u00b7grei\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "APPRART", "NN", "VVFIN", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr das Wesen sie das Kleid.", "tokens": ["F\u00fcr", "das", "We\u00b7sen", "sie", "das", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was nur geistlich mag gelingen,", "tokens": ["Was", "nur", "geist\u00b7lich", "mag", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was nur g\u00f6ttlich kann erstehn,", "tokens": ["Was", "nur", "g\u00f6tt\u00b7lich", "kann", "er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollen sie im Fleisch vollbringen \u2013", "tokens": ["Wol\u00b7len", "sie", "im", "Fleisch", "voll\u00b7brin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sollen sie verloren gehn?", "tokens": ["Sol\u00b7len", "sie", "ver\u00b7lo\u00b7ren", "gehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die da suchen ohne Steuer", "tokens": ["Die", "da", "su\u00b7chen", "oh\u00b7ne", "Steu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heimwehbang ein Ruhgestad',", "tokens": ["Heim\u00b7weh\u00b7bang", "ein", "Ruh\u00b7ge\u00b7stad'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ein irres Liebesfeuer", "tokens": ["Die", "ein", "ir\u00b7res", "Lie\u00b7bes\u00b7feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hintreibt auf der Sinne Pfad,", "tokens": ["Hin\u00b7treibt", "auf", "der", "Sin\u00b7ne", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die im D\u00e4mmer tauber Schachten", "tokens": ["Die", "im", "D\u00e4m\u00b7mer", "tau\u00b7ber", "Schach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Graben nach der Wahrheit Licht,", "tokens": ["Gra\u00b7ben", "nach", "der", "Wahr\u00b7heit", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle, die nach Freiheit schmachten,", "tokens": ["Al\u00b7le", ",", "die", "nach", "Frei\u00b7heit", "schmach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meinen dich und wissen's nicht.", "tokens": ["Mei\u00b7nen", "dich", "und", "wis\u00b7sen's", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O, beim Worte, das die R\u00e4cher", "tokens": ["O", ",", "beim", "Wor\u00b7te", ",", "das", "die", "R\u00e4\u00b7cher"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPRART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von der S\u00fcnderin verwies,", "tokens": ["Von", "der", "S\u00fcn\u00b7de\u00b7rin", "ver\u00b7wies", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei der Milde, die dem Sch\u00e4cher", "tokens": ["Bei", "der", "Mil\u00b7de", ",", "die", "dem", "Sch\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch am Kreuz das Heil verhie\u00df,", "tokens": ["Noch", "am", "Kreuz", "das", "Heil", "ver\u00b7hie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bei dem Glanz, der himmlisch blendend", "tokens": ["Bei", "dem", "Glanz", ",", "der", "himm\u00b7lisch", "blen\u00b7dend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um Damaskus' Weg geflammt", "tokens": ["Um", "Da\u00b7mas\u00b7kus'", "Weg", "ge\u00b7flammt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, den Sinn des Eifrers wendend,", "tokens": ["Und", ",", "den", "Sinn", "des", "Eif\u00b7rers", "wen\u00b7dend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihn gesalbt zum Botenamt:", "tokens": ["Ihn", "ge\u00b7salbt", "zum", "Bo\u00b7ten\u00b7amt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Zeuch, o Herr, die durst'gen Seelen,", "tokens": ["Zeuch", ",", "o", "Herr", ",", "die", "dur\u00b7st'\u00b7gen", "See\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die in dunkler Trostbegier", "tokens": ["Die", "in", "dunk\u00b7ler", "Trost\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Verg\u00e4nglichen sich qu\u00e4len,", "tokens": ["Im", "Ver\u00b7g\u00e4ng\u00b7li\u00b7chen", "sich", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeuch sie liebend all zu dir!", "tokens": ["Zeuch", "sie", "lie\u00b7bend", "all", "zu", "dir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PIAT", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Statt der Schale, dran sie kleben,", "tokens": ["Statt", "der", "Scha\u00b7le", ",", "dran", "sie", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df sie schaun der Dinge Kern,", "tokens": ["La\u00df", "sie", "schaun", "der", "Din\u00b7ge", "Kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Steig in ihrem dunkeln Leben,", "tokens": ["Steig", "in", "ih\u00b7rem", "dun\u00b7keln", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Steig empor als Morgenstern!", "tokens": ["Steig", "em\u00b7por", "als", "Mor\u00b7gens\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KOKOM", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.6": {"line.1": {"text": "Der du einst in freier Liebe", "tokens": ["Der", "du", "einst", "in", "frei\u00b7er", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich in unsern Staub gebannt,", "tokens": ["Dich", "in", "un\u00b7sern", "Staub", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsrer Brust verworrne Triebe,", "tokens": ["Uns\u00b7rer", "Brust", "ver\u00b7worr\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, und all ihr Leid erkannt;", "tokens": ["Ach", ",", "und", "all", "ihr", "Leid", "er\u00b7kannt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "PIAT", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der du selbst in jenen Tagen", "tokens": ["Der", "du", "selbst", "in", "je\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "PDAT", "NN"], "me