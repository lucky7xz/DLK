{"dta.poem.3815": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Dergleichen/  \n An einen Wittwer, als er sich gl\u00fccklich verheyrathet. Jm  \n Nahmen eines andern.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Grosser Freund! so lobt mein Kiel dein gerecht", "tokens": ["Gros\u00b7ser", "Freund", "!", "so", "lobt", "mein", "Kiel", "dein", "ge\u00b7recht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "PPOSAT", "ADJD"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "begl\u00fcckt Verfahren.", "tokens": ["be\u00b7gl\u00fcckt", "Ver\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "So will Dich des Himmels Schlu\u00df mit der fr\u00f6m\u0303-", "tokens": ["So", "will", "Dich", "des", "Him\u00b7mels", "Schlu\u00df", "mit", "der", "fr\u00f6m\u0303"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "NN", "APPR", "ART", "TRUNC"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "sten Gattin paaren.", "tokens": ["sten", "Gat\u00b7tin", "paa\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Jtzo scheint die Freuden-Sonne, itzo bricht ihr", "tokens": ["Jt\u00b7zo", "scheint", "die", "Freu\u00b7den\u00b7Son\u00b7ne", ",", "it\u00b7zo", "bricht", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Strahl empor.", "tokens": ["Strahl", "em\u00b7por", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Auf! ermuntre deine Sinnen, rei\u00df den bangen", "tokens": ["Auf", "!", "er\u00b7munt\u00b7re", "dei\u00b7ne", "Sin\u00b7nen", ",", "rei\u00df", "den", "ban\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Wittwer-Flohr.", "tokens": ["Witt\u00b7wer\u00b7Fl\u00b7ohr", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Was n\u00fctzt dir die Einsamkeit, was n\u00fctzt die be-", "tokens": ["Was", "n\u00fctzt", "dir", "die", "Ein\u00b7sam\u00b7keit", ",", "was", "n\u00fctzt", "die", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,", "PWS", "VVFIN", "ART", "TRUNC"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "tr\u00fcbte Zelle,", "tokens": ["tr\u00fcb\u00b7te", "Zel\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Jtzo mindert deinen Schmertz der getreuste Schlaf-", "tokens": ["Jt\u00b7zo", "min\u00b7dert", "dei\u00b7nen", "Schmertz", "der", "ge\u00b7treus\u00b7te", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Geselle,", "tokens": ["Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Ey so la\u00df das schn\u00f6de Trauren, und verwirff den", "tokens": ["Ey", "so", "la\u00df", "das", "schn\u00f6\u00b7de", "Trau\u00b7ren", ",", "und", "ver\u00b7wirff", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "VVIMP", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "bangen Schmertz,", "tokens": ["ban\u00b7gen", "Schmertz", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Liebe deine ", "tokens": ["Lie\u00b7be", "dei\u00b7ne"], "token_info": ["word", "word"], "pos": ["NN", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Hertz.", "tokens": ["Hertz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.17": {"text": "Dr\u00fccke sie an deine Brust, diese mehr als sch\u00f6nste", "tokens": ["Dr\u00fc\u00b7cke", "sie", "an", "dei\u00b7ne", "Brust", ",", "die\u00b7se", "mehr", "als", "sch\u00f6ns\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "PDAT", "PIAT", "KOKOM", "VVFIN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.18": {"text": "Freude,", "tokens": ["Freu\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Nimm das angenemste Kind, nimm die frohe Au-", "tokens": ["Nimm", "das", "an\u00b7ge\u00b7nems\u00b7te", "Kind", ",", "nimm", "die", "fro\u00b7he", "Au"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "VVIMP", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "gen-Weide,", "tokens": ["gen\u00b7Wei\u00b7de", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Diesen Leitstern des Vergn\u00fcgens, denn es ist dir", "tokens": ["Die\u00b7sen", "Leits\u00b7tern", "des", "Ver\u00b7gn\u00fc\u00b7gens", ",", "denn", "es", "ist", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "PPER"], "meter": "+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.22": {"text": "ja bekannt", "tokens": ["ja", "be\u00b7kannt"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Jhr so reitzend s\u00fcsses Wesen, ihr besonderer Ver-", "tokens": ["Ihr", "so", "reit\u00b7zend", "s\u00fcs\u00b7ses", "We\u00b7sen", ",", "ihr", "be\u00b7son\u00b7de\u00b7rer", "Ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.24": {"text": "stand.", "tokens": ["stand", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.25": {"text": "Deine Wohlfahrt, ja dein Gl\u00fccke, wird in unsrer", "tokens": ["Dei\u00b7ne", "Wohl\u00b7fahrt", ",", "ja", "dein", "Gl\u00fc\u00b7cke", ",", "wird", "in", "uns\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "$,", "VAFIN", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.26": {"text": "Fluhr und Gr\u00e4ntzen,", "tokens": ["Fluhr", "und", "Gr\u00e4nt\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Da du dir so sch\u00f6n gewehlt, pr\u00e4chtig und ausneh-", "tokens": ["Da", "du", "dir", "so", "sch\u00f6n", "ge\u00b7wehlt", ",", "pr\u00e4ch\u00b7tig", "und", "aus\u00b7neh"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$,", "ADJD", "KON", "TRUNC"], "meter": "-+--+-++-+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "mend gl\u00e4ntzen.", "tokens": ["mend", "gl\u00e4nt\u00b7zen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.29": {"text": "Siehe, wie der Musen-Schaaren dir das treuste", "tokens": ["Sie\u00b7he", ",", "wie", "der", "Mu\u00b7sen\u00b7Schaa\u00b7ren", "dir", "das", "treus\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "PWAV", "ART", "NN", "PPER", "ART", "ADJA"], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.30": {"text": "Opffer bringt,", "tokens": ["Opf\u00b7fer", "bringt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Siehe doch wie deine Liebe, Ehrfurcht, Pflicht", "tokens": ["Sie\u00b7he", "doch", "wie", "dei\u00b7ne", "Lie\u00b7be", ",", "Ehr\u00b7furcht", ",", "Pflicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVIMP", "ADV", "KOKOM", "PPOSAT", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.32": {"text": "und Treu besingt.", "tokens": ["und", "Treu", "be\u00b7singt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Siehe nur das grosse Heer der getreusten Liebes-", "tokens": ["Sie\u00b7he", "nur", "das", "gros\u00b7se", "Heer", "der", "ge\u00b7treus\u00b7ten", "Lie\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "----+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "G\u00f6tter,", "tokens": ["G\u00f6t\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.35": {"text": "Sieh das Musicanten-Chor mit dem fr\u00f6hlichsten", "tokens": ["Sieh", "das", "Mu\u00b7si\u00b7can\u00b7ten\u00b7Chor", "mit", "dem", "fr\u00f6h\u00b7lichs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.36": {"text": "Geschmetter,", "tokens": ["Ge\u00b7schmet\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Siehe doch die grosse Menge, alles, alles k\u00f6m\u0303t herbey,", "tokens": ["Sie\u00b7he", "doch", "die", "gros\u00b7se", "Men\u00b7ge", ",", "al\u00b7les", ",", "al\u00b7les", "k\u00f6m\u0303t", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$,", "PIS", "$,", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "+---+-+-+-+-+-+", "measure": "dactylic.init"}, "line.38": {"text": "Da\u00df es Zeuge der Verm\u00e4hlung und der frommen", "tokens": ["Da\u00df", "es", "Zeu\u00b7ge", "der", "Ver\u00b7m\u00e4h\u00b7lung", "und", "der", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "KON", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.39": {"text": "Liebe sey.", "tokens": ["Lie\u00b7be", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.40": {"text": "Nun so wird dich Gl\u00fcck und Heil mit der auser-", "tokens": ["Nun", "so", "wird", "dich", "Gl\u00fcck", "und", "Heil", "mit", "der", "aus\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "NN", "KON", "NN", "APPR", "ART", "TRUNC"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.41": {"text": "lesnen Sch\u00f6nen,", "tokens": ["les\u00b7nen", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.42": {"text": "Ja die weise Vorsicht selbst mit dem reichsten See-", "tokens": ["Ja", "die", "wei\u00b7se", "Vor\u00b7sicht", "selbst", "mit", "dem", "reichs\u00b7ten", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.43": {"text": "gen cr\u00f6nen.", "tokens": ["gen", "cr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.44": {"text": "Denn so bl\u00fcht die from\u0303e Tugend, ja sie bl\u00fchet gantz", "tokens": ["Denn", "so", "bl\u00fcht", "die", "from\u0303e", "Tu\u00b7gend", ",", "ja", "sie", "bl\u00fc\u00b7het", "gantz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "PPER", "VVFIN", "ADV"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.45": {"text": "gewi\u00df,", "tokens": ["ge\u00b7wi\u00df", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.46": {"text": "So wird euer Eheb\u00fcndnis hier ein irrdisch Paradie\u00df.", "tokens": ["So", "wird", "eu\u00b7er", "E\u00b7he\u00b7b\u00fcnd\u00b7nis", "hier", "ein", "irr\u00b7disch", "Pa\u00b7ra\u00b7die\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.47": {"text": "Letzlich bringt der Jungfer ", "tokens": ["Letz\u00b7lich", "bringt", "der", "Jung\u00b7fer"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.48": {"text": "Hochzeit-Kr\u00e4ntzgen,", "tokens": ["Hoch\u00b7zeit\u00b7Kr\u00e4nt\u00b7zgen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.49": {"text": "Eine kleine ", "tokens": ["Ei\u00b7ne", "klei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}}}}}