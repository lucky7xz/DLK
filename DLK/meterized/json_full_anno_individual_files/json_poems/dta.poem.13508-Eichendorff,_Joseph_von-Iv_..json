{"dta.poem.13508": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Iv .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lerche gr\u00fc\u00dft den ersten Strahl,", "tokens": ["Die", "Ler\u00b7che", "gr\u00fc\u00dft", "den", "ers\u00b7ten", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er die Brust ihr z\u00fcnde,", "tokens": ["Da\u00df", "er", "die", "Brust", "ihr", "z\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn tr\u00e4ge Nacht noch \u00fcberall", "tokens": ["Wenn", "tr\u00e4\u00b7ge", "Nacht", "noch", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchschleicht die tiefen Gr\u00fcnde.", "tokens": ["Durch\u00b7schleicht", "die", "tie\u00b7fen", "Gr\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und du willst, Menschenkind, der Zeit", "tokens": ["Und", "du", "willst", ",", "Men\u00b7schen\u00b7kind", ",", "der", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "$,", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verzagend unterliegen?", "tokens": ["Ver\u00b7za\u00b7gend", "un\u00b7ter\u00b7lie\u00b7gen", "?"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ist dein kleines Erdenleid!", "tokens": ["Was", "ist", "dein", "klei\u00b7nes", "Er\u00b7den\u00b7leid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du mu\u00dft es \u00fcberfliegen!", "tokens": ["Du", "mu\u00dft", "es", "\u00fc\u00b7berf\u00b7lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}