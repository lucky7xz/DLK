{"textgrid.poem.27517": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "E.v.B.", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alles schreibt, es schreibt der Knabe, der Greis, die Matrone.", "tokens": ["Al\u00b7les", "schreibt", ",", "es", "schreibt", "der", "Kna\u00b7be", ",", "der", "Greis", ",", "die", "Mat\u00b7ro\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "G\u00f6tter, erschafft ein Geschlecht, welchem das schreibende schrei", "tokens": ["G\u00f6t\u00b7ter", ",", "er\u00b7schafft", "ein", "Ge\u00b7schlecht", ",", "wel\u00b7chem", "das", "schrei\u00b7ben\u00b7de", "schrei"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Enthusiasmus suchst du bei deutschen Lesern? Du Armer;", "tokens": ["En\u00b7thu\u00b7si\u00b7as\u00b7mus", "suchst", "du", "bei", "deut\u00b7schen", "Le\u00b7sern", "?", "Du", "Ar\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$.", "PPER", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gl\u00fccklich, k\u00f6nntest du auch rechnen auf H\u00f6flichkeit nur.", "tokens": ["Gl\u00fcck\u00b7lich", ",", "k\u00f6nn\u00b7test", "du", "auch", "rech\u00b7nen", "auf", "H\u00f6f\u00b7lich\u00b7keit", "nur", "."], "to