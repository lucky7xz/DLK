{"textgrid.poem.59586": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom Gneisenau", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei Kolberg auf der gr\u00fcnen Au,", "tokens": ["Bei", "Kol\u00b7berg", "auf", "der", "gr\u00fc\u00b7nen", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Juchheididei! Juchheididei!", "tokens": ["Juch\u00b7hei\u00b7di\u00b7dei", "!", "Juch\u00b7hei\u00b7di\u00b7dei", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geht's mit dem Leben nicht zu genau,", "tokens": ["Geht's", "mit", "dem", "Le\u00b7ben", "nicht", "zu", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Juchhei! Juchhei! Juchhei!", "tokens": ["Juch\u00b7hei", "!", "Juch\u00b7hei", "!", "Juch\u00b7hei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da donnert's aus Kanonen,", "tokens": ["Da", "don\u00b7nert's", "aus", "Ka\u00b7no\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da s\u00e4t man blaue Bohnen,", "tokens": ["Da", "s\u00e4t", "man", "blau\u00b7e", "Boh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die nimmer Stengel treiben,", "tokens": ["Die", "nim\u00b7mer", "Sten\u00b7gel", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bei Kolberg auf der Au.", "tokens": ["Bei", "Kol\u00b7berg", "auf", "der", "Au", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bei Kolber hat es flinken Tanz,", "tokens": ["Bei", "Kol\u00b7ber", "hat", "es", "flin\u00b7ken", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "VVINF", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Juchheididei! Juchheididei!", "tokens": ["Juch\u00b7hei\u00b7di\u00b7dei", "!", "Juch\u00b7hei\u00b7di\u00b7dei", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um Mauer und Graben, um Wall und Schanz',", "tokens": ["Um", "Mau\u00b7er", "und", "Gra\u00b7ben", ",", "um", "Wall", "und", "Schanz'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "$,", "KOUI", "NE", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Juchhei! Juchhei! Juchhei!", "tokens": ["Juch\u00b7hei", "!", "Juch\u00b7hei", "!", "Juch\u00b7hei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie tanzen also munter,", "tokens": ["Sie", "tan\u00b7zen", "al\u00b7so", "mun\u0