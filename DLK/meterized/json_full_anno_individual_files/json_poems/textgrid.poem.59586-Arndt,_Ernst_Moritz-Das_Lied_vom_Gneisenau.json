 {"text": "Er ist ein Held von deutschem Stamm,", "tokens": ["Er", "ist", "ein", "Held", "von", "deut\u00b7schem", "Stamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Juchhei! Juchhei! Juchhei!", "tokens": ["Juch\u00b7hei", "!", "Juch\u00b7hei", "!", "Juch\u00b7hei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Held von echten Treuen,", "tokens": ["Ein", "Held", "von", "ech\u00b7ten", "Treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df sich die Deutschen freuen,", "tokens": ["Da\u00df", "sich", "die", "Deut\u00b7schen", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Gneisenau klingt sein Name", "tokens": ["Und", "Gnei\u00b7se\u00b7nau", "klingt", "sein", "Na\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf Kolbergs gr\u00fcner Au.", "tokens": ["Auf", "Kol\u00b7bergs", "gr\u00fc\u00b7ner", "Au", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Bei Kolberg auf der gr\u00fcnen Au,", "tokens": ["Bei", "Kol\u00b7berg", "auf", "der", "gr\u00fc\u00b7nen", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "AR