{"textgrid.poem.65994": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die ihr umstolzt mich mit den vollen Taschen,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr umstolzt mich mit den vollen Taschen,", "tokens": ["Die", "ihr", "um\u00b7stolzt", "mich", "mit", "den", "vol\u00b7len", "Ta\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Krummnasig oder nicht, verfluchte Beter", "tokens": ["Krumm\u00b7na\u00b7sig", "o\u00b7der", "nicht", ",", "ver\u00b7fluch\u00b7te", "Be\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "KON", "PTKNEG", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vorm goldnen Kalb, o w\u00fcrd' mein Wort zum Schwerte,", "tokens": ["Vorm", "gold\u00b7nen", "Kalb", ",", "o", "w\u00fcrd'", "mein", "Wort", "zum", "Schwer\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "FM", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie w\u00e4r' Musik mir euer Furchtgezeter.", "tokens": ["Wie", "w\u00e4r'", "Mu\u00b7sik", "mir", "eu\u00b7er", "Furcht\u00b7ge\u00b7ze\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "NN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Kommt her! legt Rechnung ab von euren Groschen.", "tokens": ["Kommt", "her", "!", "legt", "Rech\u00b7nung", "ab", "von", "eu\u00b7ren", "Gro\u00b7schen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "VVFIN", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie? Stockt so bald im gierigen Hals das Wort euch?", "tokens": ["Wie", "?", "Stockt", "so", "bald", "im", "gie\u00b7ri\u00b7gen", "Hals", "das", "Wort", "euch", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "ADV", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der sonst so freche Blick irrt scheu beiseite,", "tokens": ["Der", "sonst", "so", "fre\u00b7che", "Blick", "irrt", "scheu", "bei\u00b7sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJA", "NN", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie ertappte Buben schleicht ihr fort euch.", "tokens": ["Und", "wie", "er\u00b7tapp\u00b7te", "Bu\u00b7ben", "schleicht", "ihr", "fort", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "NN", "VVFIN", "PPER", "PTKVZ", "PPER", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geht! schachert, tr\u00fcgt und machts \u00bbGesch\u00e4ftchen\u00ab weiter,", "tokens": ["Geht", "!", "scha\u00b7chert", ",", "tr\u00fcgt", "und", "machts", "\u00bb", "Ge\u00b7sch\u00e4ft\u00b7chen", "\u00ab", "wei\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$(", "NN", "$(",