{"dta.poem.3185": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott leitet, wen er will, und l\u00e4sset irre gehn,", "tokens": ["Gott", "lei\u00b7tet", ",", "wen", "er", "will", ",", "und", "l\u00e4s\u00b7set", "ir\u00b7re", "gehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "$,", "KON", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selbst f\u00fcr seinen Weg mu\u00df jeder Rede stehn.", "tokens": ["Und", "selbst", "f\u00fcr", "sei\u00b7nen", "Weg", "mu\u00df", "je\u00b7der", "Re\u00b7de", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was also bleibt dir, als um Leitung ihn zu bitten:", "tokens": ["Was", "al\u00b7so", "bleibt", "dir", ",", "als", "um", "Lei\u00b7tung", "ihn", "zu", "bit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "$,", "KOUS", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Herr, \u00fcberla\u00df mich nicht den eignen irren Tritten!", "tokens": ["Herr", ",", "\u00fc\u00b7berl\u00b7a\u00df", "mich", "nicht", "den", "eig\u00b7nen", "ir\u00b7ren", "Trit\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ja wohl! mein Rath allein kann irre gehn, nicht deiner;", "tokens": ["Ja", "wohl", "!", "mein", "Rath", "al\u00b7lein", "kann", "ir\u00b7re", "gehn", ",", "nicht", "dei\u00b7ner", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$.", "PPOSAT", "NN", "ADV", "VMFIN", "PIS", "VVINF", "$,", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum soll dein Rath allein an mir ergehn, nicht meiner.", "tokens": ["Drum", "soll", "dein", "Rath", "al\u00b7lein", "an", "mir", "er\u00b7gehn", ",", "nicht", "mei\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "VVINF", "$,", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}}}}