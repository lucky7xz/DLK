{"textgrid.poem.48981": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "\u00dcber den Briten", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch ich sch\u00e4tze den Briten, ist er zum Umgang gebildet,", "tokens": ["Auch", "ich", "sch\u00e4t\u00b7ze", "den", "Bri\u00b7ten", ",", "ist", "er", "zum", "Um\u00b7gang", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "F\u00fchlt er das Sch\u00f6ne, und ist offen und edel sein Sinn;", "tokens": ["F\u00fchlt", "er", "das", "Sch\u00f6\u00b7ne", ",", "und", "ist", "of\u00b7fen", "und", "e\u00b7del", "sein", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VAFIN", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "++-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Nur den Bootsknecht vermeid' ich, will er sich br\u00fcsten als Lord, und", "tokens": ["Nur", "den", "Boots\u00b7knecht", "ver\u00b7meid'", "ich", ",", "will", "er", "sich", "br\u00fcs\u00b7ten", "als", "Lord", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "PRF", "VVFIN", "KOUS", "NN", "$,", "KON"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dann den verr\u00fcckten Lord, wie die Seuche der Pest.", "tokens": ["Dann", "den", "ver\u00b7r\u00fcck\u00b7ten", "Lord", ",", "wie", "die", "Seu\u00b7che", "der", "Pest", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN", "$."], "meter": "---+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Auch ich sch\u00e4tze den Briten, ist er zum Umgang gebildet,", "tokens": ["Auch", "ich", "sch\u00e4t\u00b7ze", "den", "Bri\u00b7ten", ",", "ist", "er", "zum", "Um\u00b7gang", "ge\u00b7bil\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "$,", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "F\u00fchlt er das Sch\u00f6ne, und ist offen und edel sein Sinn;", "tokens": ["F\u00fchlt", "er", "das", "Sch\u00f6\u00b7ne", ",", "und", "ist", "of\u00b7fen", "und", "e\u00b7del", "sein", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KON", "VAFIN", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "++-+-+-+--+--+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Nur den Bootsknecht vermeid' ich, will er sich br\u00fcsten als Lord, und", "tokens": ["Nur", "den", "Boots\u00b7knecht", "ver\u00b7meid'", "ich", ",", "will", "er", "sich", "br\u00fcs\u00b7ten", "als", "Lord", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "PRF", "VVFIN", "KOUS", "NN", "$,", "KON"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dann den verr\u00fcckten Lord, wie die Seuche der Pest.", "tokens": ["