{"dta.poem.10478": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich rief den Teufel und er kam,", "tokens": ["Ich", "rief", "den", "Teu\u00b7fel", "und", "er", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich sah ihn mit Verwund'rung an.", "tokens": ["Und", "ich", "sah", "ihn", "mit", "Ver\u00b7wun\u00b7d'\u00b7rung", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Er ist nicht h\u00e4\u00dflich, und ist nicht lahm,", "tokens": ["Er", "ist", "nicht", "h\u00e4\u00df\u00b7lich", ",", "und", "ist", "nicht", "lahm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "KON", "VAFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Er ist ein lieber, scharmanter Mann,", "tokens": ["Er", "ist", "ein", "lie\u00b7ber", ",", "schar\u00b7man\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Ein Mann in seinen besten Jahren,", "tokens": ["Ein", "Mann", "in", "sei\u00b7nen", "bes\u00b7ten", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verbindlich und h\u00f6flich und welterfahren.", "tokens": ["Ver\u00b7bind\u00b7lich", "und", "h\u00f6f\u00b7lich", "und", "wel\u00b7ter\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Er ist ein gescheuter Diplomat,", "tokens": ["Er", "ist", "ein", "ge\u00b7scheu\u00b7ter", "Dip\u00b7lo\u00b7mat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und spricht recht sch\u00f6n \u00fcber Kirch' und Staat.", "tokens": ["Und", "spricht", "recht", "sch\u00f6n", "\u00fc\u00b7ber", "Kirch'", "und", "Staat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Bla\u00df ist er etwas, doch ist es kein Wunder,", "tokens": ["Bla\u00df", "ist", "er", "et\u00b7was", ",", "doch", "ist", "es", "kein", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.10": {"text": "Sanskritt und Hegel studiert er jetzunder.", "tokens": ["Sans\u00b7kritt", "und", "He\u00b7gel", "stu\u00b7diert", "er", "je\u00b7tzun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Sein Lieblingspoet ist noch immer Fouqu", "tokens": ["Sein", "Lieb\u00b7lings\u00b7poet", "ist", "noch", "im\u00b7mer", "Fou\u00b7qu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "NE"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Doch will er nicht mehr mit Kritik sich befassen,", "tokens": ["Doch", "will", "er", "nicht", "mehr", "mit", "Kri\u00b7tik", "sich", "be\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Die hat er jetzt g\u00e4nzlich \u00fcberlassen", "tokens": ["Die", "hat", "er", "jetzt", "g\u00e4nz\u00b7lich", "\u00fc\u00b7ber\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Der theuren Gro\u00dfmutter Hekate.", "tokens": ["Der", "theu\u00b7ren", "Gro\u00df\u00b7mut\u00b7ter", "He\u00b7ka\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$."], "meter": "-+-++-+--", "measure": "unknown.measure.tetra"}, "line.15": {"text": "Er lobte mein juristisches Streben,", "tokens": ["Er", "lob\u00b7te", "mein", "ju\u00b7ris\u00b7ti\u00b7sches", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Hat fr\u00fcher sich auch damit abgegeben.", "tokens": ["Hat", "fr\u00fc\u00b7her", "sich", "auch", "da\u00b7mit", "ab\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PRF", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.17": {"text": "Er sagte, meine Freundschaft sey", "tokens": ["Er", "sag\u00b7te", ",", "mei\u00b7ne", "Freund\u00b7schaft", "sey"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ihm nicht zu theuer, und nickte dabei,", "tokens": ["Ihm", "nicht", "zu", "theu\u00b7er", ",", "und", "nick\u00b7te", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "PTKA", "ADJD", "$,", "KON", "VVFIN", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Und frug: ob wir uns fr\u00fcher nicht", "tokens": ["Und", "frug", ":", "ob", "wir", "uns", "fr\u00fc\u00b7her", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "KOUS", "PPER", "PRF", "ADJD", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Schon einmal gesehn bei'm span'schen Gesandten?", "tokens": ["Schon", "ein\u00b7mal", "ge\u00b7sehn", "bei'm", "span'\u00b7schen", "Ge\u00b7sand\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Und als ich recht besah sein Gesicht,", "tokens": ["Und", "als", "ich", "recht", "be\u00b7sah", "sein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Fand ich in ihm einen alten Bekannten.", "tokens": ["Fand", "ich", "in", "ihm", "ei\u00b7nen", "al\u00b7ten", "Be\u00b7kann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}}}}