{"dta.poem.67": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "An Ebert.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1771", "urn": "urn:nbn:de:kobv:b4-200905199486", "language": ["de:0.99"], "booktitle": "[Klopstock, Friedrich Gottlieb]: Oden. Hamburg, 1771."}, "poem": {"stanza.1": {"line.1": {"text": "Ebert, mich scheucht ein tr\u00fcber Gedanke vom blin-", "tokens": ["E\u00b7bert", ",", "mich", "scheucht", "ein", "tr\u00fc\u00b7ber", "Ge\u00b7dan\u00b7ke", "vom", "blin"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "TRUNC"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Tief in die Melancholey!", "tokens": ["Tief", "in", "die", "Me\u00b7lan\u00b7cho\u00b7ley", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach du redest umsonst, vor dem gewaltiges Kelchglas,", "tokens": ["Ach", "du", "re\u00b7dest", "um\u00b7sonst", ",", "vor", "dem", "ge\u00b7wal\u00b7ti\u00b7ges", "Kelchg\u00b7las", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-----+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Heitre Gedanken mir zu!", "tokens": ["Heit\u00b7re", "Ge\u00b7dan\u00b7ken", "mir", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Weggehn mu\u00df ich, und weinen! vielleicht, da\u00df die", "tokens": ["Weg\u00b7gehn", "mu\u00df", "ich", ",", "und", "wei\u00b7nen", "!", "viel\u00b7leicht", ",", "da\u00df", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVINF", "VMFIN", "PPER", "$,", "KON", "VVINF", "$.", "ADV", "$,", "KOUS", "ART"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Meine Betr\u00fcbni\u00df verweint.", "tokens": ["Mei\u00b7ne", "Be\u00b7tr\u00fcb\u00b7ni\u00df", "ver\u00b7weint", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Lindernde Thr\u00e4nen, euch gab die Natur dem mensch-", "tokens": ["Lin\u00b7dern\u00b7de", "Thr\u00e4\u00b7nen", ",", "euch", "gab", "die", "Na\u00b7tur", "dem", "men\u00b7sch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ART", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.8": {"text": "Weis\u2019 als Gesellinnen zu.", "tokens": ["Weis'", "als", "Ge\u00b7sel\u00b7lin\u00b7nen", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NN", "PTKVZ", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.9": {"text": "W\u00e4ret ihr nicht, und k\u00f6nnten ihr Leiden die Men-", "tokens": ["W\u00e4\u00b7ret", "ihr", "nicht", ",", "und", "k\u00f6nn\u00b7ten", "ihr", "Lei\u00b7den", "die", "Men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "$,", "KON", "VMFIN", "PPOSAT", "NN", "ART", "TRUNC"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ach! wie ertr\u00fcgen sie\u2019s da!", "tokens": ["Ach", "!", "wie", "er\u00b7tr\u00fc\u00b7gen", "sie's", "da", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Weggehn mu\u00df ich, und weinen! Mein schwermuths-", "tokens": ["Weg\u00b7gehn", "mu\u00df", "ich", ",", "und", "wei\u00b7nen", "!", "Mein", "schwer\u00b7muths"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVINF", "VMFIN", "PPER", "$,", "KON", "VVINF", "$.", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Bebt noch gewaltig in mir.", "tokens": ["Bebt", "noch", "ge\u00b7wal\u00b7tig", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.13": {"text": "Ebert! \u2026 sind sie nun \u2026 alle dahin! deckt un-", "tokens": ["E\u00b7bert", "!", "\u2026", "sind", "sie", "nun", "\u2026", "al\u00b7le", "da\u00b7hin", "!", "deckt", "un"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "$(", "VAFIN", "PPER", "ADV", "$(", "PIS", "PAV", "$.", "VVFIN", "TRUNC"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Alle die heilige Gruft;", "tokens": ["Al\u00b7le", "die", "hei\u00b7li\u00b7ge", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.15": {"text": "Und sind wir \u2026 zween Einsame \u2026 dann von", "tokens": ["Und", "sind", "wir", "\u2026", "zween", "Ein\u00b7sa\u00b7me", "\u2026", "dann", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "$(", "VVFIN", "NN", "$(", "ADV", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Ebert! \u2026 verstummst du nicht hier?", "tokens": ["E\u00b7bert", "!", "\u2026", "ver\u00b7stummst", "du", "nicht", "hier", "?"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.17": {"text": "Sieht dein Auge nicht bang um sich her, nicht starr", "tokens": ["Sieht", "dein", "Au\u00b7ge", "nicht", "bang", "um", "sich", "her", ",", "nicht", "starr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "APPR", "PRF", "PTKVZ", "$,", "PTKNEG", "ADJD"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.18": {"text": "So erstarb auch mein Blick!", "tokens": ["So", "er\u00b7starb", "auch", "mein", "Blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "So erbebt\u2019 ich, als mich von allen Gedanken der", "tokens": ["So", "er\u00b7bebt'", "ich", ",", "als", "mich", "von", "al\u00b7len", "Ge\u00b7dan\u00b7ken", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "PIAT", "NN", "ART"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Donnernd das erstemal traf!", "tokens": ["Don\u00b7nernd", "das", "ers\u00b7te\u00b7mal", "traf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.21": {"text": "Wie du einen Wanderer, der, zu eilend der Gattin,", "tokens": ["Wie", "du", "ei\u00b7nen", "Wan\u00b7de\u00b7rer", ",", "der", ",", "zu", "ei\u00b7lend", "der", "Gat\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "PRELS", "$,", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "Und dem gebildeten Sohn,", "tokens": ["Und", "dem", "ge\u00b7bil\u00b7de\u00b7ten", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Und der bl\u00fchenden Tochter, nach ihrer Umarmung", "tokens": ["Und", "der", "bl\u00fc\u00b7hen\u00b7den", "Toch\u00b7ter", ",", "nach", "ih\u00b7rer", "Um\u00b7ar\u00b7mung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Du den, Donner, ereilst,", "tokens": ["Du", "den", ",", "Don\u00b7ner", ",", "er\u00b7eilst", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.25": {"text": "T\u00f6dtend ihn fassest, und seine Gebeine zu fallendem", "tokens": ["T\u00f6d\u00b7tend", "ihn", "fas\u00b7sest", ",", "und", "sei\u00b7ne", "Ge\u00b7bei\u00b7ne", "zu", "fal\u00b7len\u00b7dem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.26": {"text": "Machst, triumphirend alsdann", "tokens": ["Machst", ",", "tri\u00b7um\u00b7phi\u00b7rend", "als\u00b7dann"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADJD", "ADV"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.27": {"text": "Wieder die hohe Wolke durchwandelst; so traf der", "tokens": ["Wie\u00b7der", "die", "ho\u00b7he", "Wol\u00b7ke", "durch\u00b7wan\u00b7delst", ";", "so", "traf", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ART"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Meinen ersch\u00fctterten Geist,", "tokens": ["Mei\u00b7nen", "er\u00b7sch\u00fct\u00b7ter\u00b7ten", "Geist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.29": {"text": "Da\u00df mein Auge sich dunkel verlor, und das bebende", "tokens": ["Da\u00df", "mein", "Au\u00b7ge", "sich", "dun\u00b7kel", "ver\u00b7lor", ",", "und", "das", "be\u00b7ben\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADJD", "VVFIN", "$,", "KON", "ART", "ADJA"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Kraftlos zittert\u2019, und sank.", "tokens": ["Kraft\u00b7los", "zit\u00b7tert'", ",", "und", "sank", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "KON", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.31": {"text": "Ach, in schweigender Nacht, ging mir die Todtener-", "tokens": ["Ach", ",", "in", "schwei\u00b7gen\u00b7der", "Nacht", ",", "ging", "mir", "die", "Tod\u00b7te\u00b7ner"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.32": {"text": "Unsre Freunde, vorbey!", "tokens": ["Uns\u00b7re", "Freun\u00b7de", ",", "vor\u00b7bey", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.33": {"text": "Ach in schweigender Nacht erblickt\u2019 ich die offenen", "tokens": ["Ach", "in", "schwei\u00b7gen\u00b7der", "Nacht", "er\u00b7blickt'", "ich", "die", "of\u00b7fe\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA"], "meter": "--+--+-+--++-", "measure": "anapaest.di.plus"}, "line.34": {"text": "Und der Unsterblichen Schaar!", "tokens": ["Und", "der", "U\u00b7nsterb\u00b7li\u00b7chen", "Schaar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+---+", "measure": "anapaest.init"}, "line.35": {"text": "Wenn nicht mehr des z\u00e4rtlichen Giseken Auge mir", "tokens": ["Wenn", "nicht", "mehr", "des", "z\u00e4rt\u00b7li\u00b7chen", "Gi\u00b7se\u00b7ken", "Au\u00b7ge", "mir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "ART", "ADJA", "NN", "NN", "PPER"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.36": {"text": "Wenn, von der Radikinn fern,", "tokens": ["Wenn", ",", "von", "der", "Ra\u00b7di\u00b7kinn", "fern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.37": {"text": "Unser redlicher Cramer verwest! wenn G\u00e4rtner, wenn", "tokens": ["Un\u00b7ser", "red\u00b7li\u00b7cher", "Cra\u00b7mer", "ver\u00b7west", "!", "wenn", "G\u00e4rt\u00b7ner", ",", "wenn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$.", "KOUS", "NN", "$,", "KOUS"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.38": {"text": "Nicht sokratisch mehr spricht!", "tokens": ["Nicht", "sok\u00b7ra\u00b7tisch", "mehr", "spricht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Wenn in des edelm\u00fcthigen Gellert harmonischem Leben", "tokens": ["Wenn", "in", "des", "e\u00b7del\u00b7m\u00fct\u00b7hi\u00b7gen", "Gel\u00b7lert", "har\u00b7mo\u00b7ni\u00b7schem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.40": {"text": "Jede Saite verstummt!", "tokens": ["Je\u00b7de", "Sai\u00b7te", "ver\u00b7stummt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.41": {"text": "Wenn, nun \u00fcber dem Grabe, der freye gesellige Rothe", "tokens": ["Wenn", ",", "nun", "\u00fc\u00b7ber", "dem", "Gra\u00b7be", ",", "der", "frey\u00b7e", "ge\u00b7sel\u00b7li\u00b7ge", "Ro\u00b7the"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "ADV", "APPR", "ART", "NN", "$,", "ART", "ADJA", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.42": {"text": "Freudegenossen sich w\u00e4hlt!", "tokens": ["Freu\u00b7de\u00b7ge\u00b7nos\u00b7sen", "sich", "w\u00e4hlt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PRF", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.43": {"text": "Wenn der erfindende Schlegel aus einer l\u00e4ngern Ver-", "tokens": ["Wenn", "der", "er\u00b7fin\u00b7den\u00b7de", "Schle\u00b7gel", "aus", "ei\u00b7ner", "l\u00e4n\u00b7gern", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Keinem Freunde mehr schreibt!", "tokens": ["Kei\u00b7nem", "Freun\u00b7de", "mehr", "schreibt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.45": {"text": "Wenn in meines geliebtesten Schmidts Umarmung", "tokens": ["Wenn", "in", "mei\u00b7nes", "ge\u00b7lieb\u00b7tes\u00b7ten", "Schmidts", "Um\u00b7ar\u00b7mung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.46": {"text": "Nicht mehr Z\u00e4rtlichkeit weint!", "tokens": ["Nicht", "mehr", "Z\u00e4rt\u00b7lich\u00b7keit", "weint", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.47": {"text": "Wenn einschlummernd sich Hagedorn unser Vater ent-", "tokens": ["Wenn", "ein\u00b7schlum\u00b7mernd", "sich", "Ha\u00b7ge\u00b7dorn", "un\u00b7ser", "Va\u00b7ter", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "PRF", "NE", "PPOSAT", "NN", "TRUNC"], "meter": "--+--+-+--+--", "measure": "anapaest.di.plus"}, "line.48": {"text": "Ebert, was sind wir alsdann,", "tokens": ["E\u00b7bert", ",", "was", "sind", "wir", "als\u00b7dann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.49": {"text": "Wie Geweihte des Schmerzes, die hier ein tr\u00fcberes", "tokens": ["Wie", "Ge\u00b7weih\u00b7te", "des", "Schmer\u00b7zes", ",", "die", "hier", "ein", "tr\u00fc\u00b7be\u00b7res"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.50": {"text": "L\u00e4nger, als Alle sie lie\u00df.", "tokens": ["L\u00e4n\u00b7ger", ",", "als", "Al\u00b7le", "sie", "lie\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.51": {"text": "Stirbt denn auch einer von uns, mich rei\u00dft mein", "tokens": ["Stirbt", "denn", "auch", "ei\u00b7ner", "von", "uns", ",", "mich", "rei\u00dft", "mein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Immer n\u00e4chtlicher fort!", "tokens": ["Im\u00b7mer", "n\u00e4cht\u00b7li\u00b7cher", "fort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.53": {"text": "Stirbt dann auch Einer von uns, und bleibt nur Ei-", "tokens": ["Stirbt", "dann", "auch", "Ei\u00b7ner", "von", "uns", ",", "und", "bleibt", "nur", "Ei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "PIS", "APPR", "PPER", "$,", "KON", "VVFIN", "ADV", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.54": {"text": "Bin der Eine dann ich;", "tokens": ["Bin", "der", "Ei\u00b7ne", "dann", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ART", "ADV", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.55": {"text": "Hat mich dann auch die schon geliebt, die k\u00fcnftig mich", "tokens": ["Hat", "mich", "dann", "auch", "die", "schon", "ge\u00b7liebt", ",", "die", "k\u00fcnf\u00b7tig", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ART", "ADV", "VVPP", "$,", "PRELS", "ADJD", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Ruht auch Sie in der Gruft;", "tokens": ["Ruht", "auch", "Sie", "in", "der", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "Bin dann ich der Einsame, bin allein auf der Erde:", "tokens": ["Bin", "dann", "ich", "der", "Ein\u00b7sa\u00b7me", ",", "bin", "al\u00b7lein", "auf", "der", "Er\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "NN", "$,", "VAFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.58": {"text": "Wirst du, ewiger Geist,", "tokens": ["Wirst", "du", ",", "e\u00b7wi\u00b7ger", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.59": {"text": "Seele zur Freundschaft erschaffen, du dann die leeren", "tokens": ["See\u00b7le", "zur", "Freund\u00b7schaft", "er\u00b7schaf\u00b7fen", ",", "du", "dann", "die", "lee\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVPP", "$,", "PPER", "ADV", "ART", "ADJA"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.60": {"text": "Sehn, und f\u00fchlend noch seyn?", "tokens": ["Sehn", ",", "und", "f\u00fch\u00b7lend", "noch", "seyn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADJD", "ADV", "VAINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.61": {"text": "Oder wirst du bet\u00e4ubt f\u00fcr N\u00e4chte sie halten, und", "tokens": ["O\u00b7der", "wirst", "du", "be\u00b7t\u00e4ubt", "f\u00fcr", "N\u00e4ch\u00b7te", "sie", "hal\u00b7ten", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "PPER", "VVINF", "$,", "KON"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.62": {"text": "Und gedankenlos ruhn?", "tokens": ["Und", "ge\u00b7dan\u00b7ken\u00b7los", "ruhn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.63": {"text": "Aber wenn du bisweilen erwachtest zu f\u00fchlen dein Elend,", "tokens": ["A\u00b7ber", "wenn", "du", "bis\u00b7wei\u00b7len", "er\u00b7wach\u00b7test", "zu", "f\u00fch\u00b7len", "dein", "E\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "PTKZU", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.64": {"text": "Banger, unsterblicher Geist?", "tokens": ["Ban\u00b7ger", ",", "uns\u00b7terb\u00b7li\u00b7cher", "Geist", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.65": {"text": "Rufe, wenn du erwachst, das Bild vom Grabe der", "tokens": ["Ru\u00b7fe", ",", "wenn", "du", "er\u00b7wachst", ",", "das", "Bild", "vom", "Gra\u00b7be", "der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "ART"], "meter": "+--+-+-+-+--", "measure": "iambic.penta.invert"}, "line.66": {"text": "Das nur rufe zur\u00fcck!", "tokens": ["Das", "nur", "ru\u00b7fe", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.67": {"text": "O ihr Gr\u00e4ber der Todten! ihr Gr\u00e4ber meiner Ent-", "tokens": ["O", "ihr", "Gr\u00e4\u00b7ber", "der", "Tod\u00b7ten", "!", "ihr", "Gr\u00e4\u00b7ber", "mei\u00b7ner", "Ent"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "ART", "NN", "$.", "PPOSAT", "NN", "PPOSAT", "TRUNC"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.68": {"text": "Warum liegt ihr zerstreut?", "tokens": ["Wa\u00b7rum", "liegt", "ihr", "zer\u00b7streut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVPP", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.69": {"text": "Warum liegt ihr nicht in bl\u00fchenden Thalen beysam-", "tokens": ["Wa\u00b7rum", "liegt", "ihr", "nicht", "in", "bl\u00fc\u00b7hen\u00b7den", "Tha\u00b7len", "bey\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.70": {"text": "Oder in Hainen vereint?", "tokens": ["O\u00b7der", "in", "Hai\u00b7nen", "ver\u00b7eint", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.71": {"text": "Leitet den sterbenden Greis! Ich will mit bebendem Fusse", "tokens": ["Lei\u00b7tet", "den", "ster\u00b7ben\u00b7den", "Greis", "!", "Ich", "will", "mit", "be\u00b7ben\u00b7dem", "Fus\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.72": {"text": "Gehn, auf jegliches Grab", "tokens": ["Gehn", ",", "auf", "jeg\u00b7li\u00b7ches", "Grab"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.73": {"text": "Eine Cypresse pflanzen, die noch nicht schattenden B\u00e4ume", "tokens": ["Ei\u00b7ne", "Cyp\u00b7res\u00b7se", "pflan\u00b7zen", ",", "die", "noch", "nicht", "schat\u00b7ten\u00b7den", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "ADV", "PTKNEG", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.74": {"text": "F\u00fcr die Enkel erziehn,", "tokens": ["F\u00fcr", "die", "En\u00b7kel", "er\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.75": {"text": "Oft in der Nacht auf biegsamen Wipfel die himmli-", "tokens": ["Oft", "in", "der", "Nacht", "auf", "bieg\u00b7sa\u00b7men", "Wip\u00b7fel", "die", "himm\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "ART", "TRUNC"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.76": {"text": "Meiner Unsterblichen sehn,", "tokens": ["Mei\u00b7ner", "U\u00b7nsterb\u00b7li\u00b7chen", "sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.77": {"text": "Zitternd mein Haupt gen Himmel erheben, und wei-", "tokens": ["Zit\u00b7ternd", "mein", "Haupt", "gen", "Him\u00b7mel", "er\u00b7he\u00b7ben", ",", "und", "wei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,", "KON", "TRUNC"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.78": {"text": "Grabet den Todten dann ein", "tokens": ["Gra\u00b7bet", "den", "Tod\u00b7ten", "dann", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.79": {"text": "Bey dem Grabe, bey dem er starb! Nimm dann, o", "tokens": ["Bey", "dem", "Gra\u00b7be", ",", "bey", "dem", "er", "starb", "!", "Nimm", "dann", ",", "o"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$.", "NE", "ADV", "$,", "FM"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.80": {"text": "Meine Thr\u00e4nen, und mich! \u2026", "tokens": ["Mei\u00b7ne", "Thr\u00e4\u00b7nen", ",", "und", "mich", "!", "\u2026"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PPER", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.81": {"text": "Finstrer Gedanke, la\u00df ab! la\u00df ab in die Seele zu", "tokens": ["Finst\u00b7rer", "Ge\u00b7dan\u00b7ke", ",", "la\u00df", "ab", "!", "la\u00df", "ab", "in", "die", "See\u00b7le", "zu"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVIMP", "PTKVZ", "$.", "VVIMP", "PTKVZ", "APPR", "ART", "NN", "APPR"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.82": {"text": "Wie die Ewigkeit ernst,", "tokens": ["Wie", "die", "E\u00b7wig\u00b7keit", "ernst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.83": {"text": "Furchtbar, wie das Gericht, la\u00df ab! die verstummen-", "tokens": ["Furcht\u00b7bar", ",", "wie", "das", "Ge\u00b7richt", ",", "la\u00df", "ab", "!", "die", "ver\u00b7stum\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "$,", "VVIMP", "PTKVZ", "$.", "ART", "TRUNC"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.84": {"text": "Fa\u00dft dich, Gedanke, nicht mehr!", "tokens": ["Fa\u00dft", "dich", ",", "Ge\u00b7dan\u00b7ke", ",", "nicht", "mehr", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}