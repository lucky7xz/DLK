{"textgrid.poem.40804": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdas grause K\u00f6nigsspiel will ich nun spielen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdas grause K\u00f6nigsspiel will ich nun spielen", "tokens": ["\u00bb", "das", "grau\u00b7se", "K\u00f6\u00b7nigs\u00b7spiel", "will", "ich", "nun", "spie\u00b7len"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und laden zu Gerichte meine Richter!", "tokens": ["Und", "la\u00b7den", "zu", "Ge\u00b7rich\u00b7te", "mei\u00b7ne", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es dr\u00fcckt das goldne Zepter euch nur Schwielen,", "tokens": ["Es", "dr\u00fcckt", "das", "gold\u00b7ne", "Zep\u00b7ter", "euch", "nur", "Schwie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch hoch empor das seine schwingt der Dichter!", "tokens": ["Doch", "hoch", "em\u00b7por", "das", "sei\u00b7ne", "schwingt", "der", "Dich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "PDS", "PPOSAT", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr k\u00f6nnt die Ebenb\u00fcrdigkeit nicht tadeln", "tokens": ["Ihr", "k\u00f6nnt", "die", "E\u00b7ben\u00b7b\u00fcr\u00b7dig\u00b7keit", "nicht", "ta\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Geists in mir, ihr stolzen Purpurtr\u00e4ger!", "tokens": ["Des", "Geists", "in", "mir", ",", "ihr", "stol\u00b7zen", "Pur\u00b7pur\u00b7tr\u00e4\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er wird zum Throne diesen Schemel adeln", "tokens": ["Er", "wird", "zum", "Thro\u00b7ne", "die\u00b7sen", "Sche\u00b7mel", "a\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und vor die Schranken rufen eure Kl\u00e4ger!", "tokens": ["Und", "vor", "die", "Schran\u00b7ken", "ru\u00b7fen", "eu\u00b7re", "Kl\u00e4\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da sprach die Kette meines Arms: Bei Erzen", "tokens": ["Da", "sprach", "die", "Ket\u00b7te", "mei\u00b7nes", "Arms", ":", "Bei", "Er\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlief einst ich sanft und tief in ew'gen N\u00e4chten!", "tokens": ["Schlief", "einst", "ich", "sanft", "und", "tief", "in", "ew'\u00b7gen", "N\u00e4ch\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ri\u00dft ihr mich dem Berge aus dem Herzen,", "tokens": ["Was", "ri\u00dft", "ihr", "mich", "dem", "Ber\u00b7ge", "aus", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Solch' unbewehrte Arme zu umflechten?", "tokens": ["Solch'", "un\u00b7be\u00b7wehr\u00b7te", "Ar\u00b7me", "zu", "um\u00b7flech\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der W\u00f6lbung Quadern sprachen drauf: Wir trugen", "tokens": ["Der", "W\u00f6l\u00b7bung", "Qua\u00b7dern", "spra\u00b7chen", "drauf", ":", "Wir", "tru\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am Dom des Herrn einst mit als Felsens\u00e4ulen!", "tokens": ["Am", "Dom", "des", "Herrn", "einst", "mit", "als", "Fel\u00b7sen\u00b7s\u00e4u\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "APPR", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was habt ihr uns geschmettert aus den Fugen,", "tokens": ["Was", "habt", "ihr", "uns", "ge\u00b7schmet\u00b7tert", "aus", "den", "Fu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu h\u00f6ren dieses Armen Klagen heulen?", "tokens": ["Zu", "h\u00f6\u00b7ren", "die\u00b7ses", "Ar\u00b7men", "Kla\u00b7gen", "heu\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PDAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Des Bettes Diele sprach: Ich ragt' als Eiche,", "tokens": ["Des", "Bet\u00b7tes", "Die\u00b7le", "sprach", ":", "Ich", "ragt'", "als", "Ei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf gr\u00fcnen H\u00f6h'n zu s\u00e4useln Gottes Ehre!", "tokens": ["Auf", "gr\u00fc\u00b7nen", "H\u00f6h'n", "zu", "s\u00e4u\u00b7seln", "Got\u00b7tes", "Eh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was habt ihr mich gef\u00e4llt mit frechem Streiche,", "tokens": ["Was", "habt", "ihr", "mich", "ge\u00b7f\u00e4llt", "mit", "fre\u00b7chem", "Strei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PRF", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich die\u00df Herz jetzt an mich pochen h\u00f6re?", "tokens": ["Da\u00df", "ich", "die\u00df", "Herz", "jetzt", "an", "mich", "po\u00b7chen", "h\u00f6\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "NN", "ADV", "APPR", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Vor'm Fenster eine Lerche klagte bitter:", "tokens": ["Vor'm", "Fens\u00b7ter", "ei\u00b7ne", "Ler\u00b7che", "klag\u00b7te", "bit\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was zeigt ihr mir, der Freiheitseelen einer,", "tokens": ["Was", "zeigt", "ihr", "mir", ",", "der", "Frei\u00b7heit\u00b7see\u00b7len", "ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$,", "ART", "NN", "ART", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Knechtschaft gelb Gesicht durch schwarzes Gitter", "tokens": ["Der", "Knecht\u00b7schaft", "gelb", "Ge\u00b7sicht", "durch", "schwar\u00b7zes", "Git\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eine Seele, ach, so frei, gleich meiner!", "tokens": ["Und", "ei\u00b7ne", "See\u00b7le", ",", "ach", ",", "so", "frei", ",", "gleich", "mei\u00b7ner", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ITJ", "$,", "ADV", "ADJD", "$,", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Es sprach mein Herz: Euch freut, was mannigfaltig,", "tokens": ["Es", "sprach", "mein", "Herz", ":", "Euch", "freut", ",", "was", "man\u00b7nig\u00b7fal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch Ein Gepr\u00e4g' nur wollt ihr f\u00fcr Gedanken!", "tokens": ["Doch", "Ein", "Ge\u00b7pr\u00e4g'", "nur", "wollt", "ihr", "f\u00fcr", "Ge\u00b7dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VMFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr liebt die Blumen, weil sie vielgestaltig,", "tokens": ["Ihr", "liebt", "die", "Blu\u00b7men", ",", "weil", "sie", "viel\u00b7ge\u00b7stal\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch darf nicht frei das Herz Gef\u00fchle ranken!", "tokens": ["Doch", "darf", "nicht", "frei", "das", "Herz", "Ge\u00b7f\u00fch\u00b7le", "ran\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADJD", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "In plumpe Fesseln wollt den Geist ihr schlagen,", "tokens": ["In", "plum\u00b7pe", "Fes\u00b7seln", "wollt", "den", "Geist", "ihr", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gottgesandt, wie Wolk' und Regenbogen;", "tokens": ["Der", "gott\u00b7ge\u00b7sandt", ",", "wie", "Wol\u00b7k'", "und", "Re\u00b7gen\u00b7bo\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PWAV", "NE", "KON", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Wolke wettert, ihr k\u00f6nnt sie nicht jagen,", "tokens": ["Die", "Wol\u00b7ke", "wet\u00b7tert", ",", "ihr", "k\u00f6nnt", "sie", "nicht", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und knebeln nicht k\u00f6nnt ihr den Regenbogen!", "tokens": ["Und", "kne\u00b7beln", "nicht", "k\u00f6nnt", "ihr", "den", "Re\u00b7gen\u00b7bo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Und nun vernehmt den Urtelspruch des Richters:", "tokens": ["Und", "nun", "ver\u00b7nehmt", "den", "Ur\u00b7tel\u00b7spruch", "des", "Rich\u00b7ters", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr Kett' und Schmach, die ihr ihm lie\u00dft bereiten", "tokens": ["F\u00fcr", "Kett'", "und", "Schmach", ",", "die", "ihr", "ihm", "lie\u00dft", "be\u00b7rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn also richtet mild das Herz des Dichters,", "tokens": ["Denn", "al\u00b7so", "rich\u00b7tet", "mild", "das", "Herz", "des", "Dich\u00b7ters", ","], "token_info": ["word", "word", "word", 