{"textgrid.poem.67741": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Eine Paraphrase des Vaterunser", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott, der im hohen Himmel wohnet", "tokens": ["Gott", ",", "der", "im", "ho\u00b7hen", "Him\u00b7mel", "woh\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und uns, uns Asche, Kinder nennt,", "tokens": ["Und", "uns", ",", "uns", "A\u00b7sche", ",", "Kin\u00b7der", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der \u00fcber Kron' und Scepter thronet", "tokens": ["Der", "\u00fc\u00b7ber", "Kron'", "und", "Scep\u00b7ter", "thro\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch f\u00fcr Staub vor Liebe brennt,", "tokens": ["Und", "doch", "f\u00fcr", "Staub", "vor", "Lie\u00b7be", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich nenne Dich den Herrn und sinke", "tokens": ["Ich", "nen\u00b7ne", "Dich", "den", "Herrn", "und", "sin\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Voll Ehrfurcht auf mein Angesicht", "tokens": ["Voll", "Ehr\u00b7furcht", "auf", "mein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und bete: \u00bbVater, Vater, winke", "tokens": ["Und", "be\u00b7te", ":", "\u00bb", "Va\u00b7ter", ",", "Va\u00b7ter", ",", "win\u00b7ke"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "NN", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Scepter mir zur Zuversicht!\u00ab", "tokens": ["Den", "Scep\u00b7ter", "mir", "zur", "Zu\u00b7ver\u00b7sicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Name, den das Heer der Himmel", "tokens": ["Dein", "Na\u00b7me", ",", "den", "das", "Heer", "der", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Zittern dreimal heilig preist,", "tokens": ["Mit", "Zit\u00b7tern", "drei\u00b7mal", "hei\u00b7lig", "preist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem jauchze aller Welt Get\u00fcmmel:", "tokens": ["Dem", "jauch\u00b7ze", "al\u00b7ler", "Welt", "Ge\u00b7t\u00fcm\u00b7mel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbverehrt sei Vater, Sohn und Geist!\u00ab", "tokens": ["\u00bb", "ver\u00b7ehrt", "sei", "Va\u00b7ter", ",", "Sohn", "und", "Geist", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVPP", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein Reich der Himmel senke nieder,", "tokens": ["Dein", "Reich", "der", "Him\u00b7mel", "sen\u00b7ke", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Segen str\u00f6me Deine Hand!", "tokens": ["Und", "Se\u00b7gen", "str\u00f6\u00b7me", "Dei\u00b7ne", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So kommt uns g\u00fcldne Ruhe wieder,", "tokens": ["So", "kommt", "uns", "g\u00fcld\u00b7ne", "Ru\u00b7he", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und unsre Welt wird Gottes Land.", "tokens": ["Und", "uns\u00b7re", "Welt", "wird", "Got\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein Wink gebeut dort Seraphinen", "tokens": ["Dein", "Wink", "ge\u00b7beut", "dort", "Se\u00b7ra\u00b7phi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor Dir; er will, und es geschicht:", "tokens": ["Vor", "Dir", ";", "er", "will", ",", "und", "es", "ge\u00b7schicht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$.", "PPER", "VMFIN", "$,", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So la\u00df Dir, Gott, die V\u00f6lker dienen!", "tokens": ["So", "la\u00df", "Dir", ",", "Gott", ",", "die", "V\u00f6l\u00b7ker", "die\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "$,", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du willst das Beste stets, wir nicht.", "tokens": ["Du", "willst", "das", "Bes\u00b7te", "stets", ",", "wir", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "$,", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Speis' uns mit Fr\u00fcchten Deiner G\u00fcte,", "tokens": ["Speis'", "uns", "mit", "Fr\u00fcch\u00b7ten", "Dei\u00b7ner", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Gnadenthau sei unser Trank!", "tokens": ["Dein", "Gna\u00b7dent\u00b7hau", "sei", "un\u00b7ser", "Trank", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dem Menschen preis' ihn Eu'r Gem\u00fcthe,", "tokens": ["Dem", "Men\u00b7schen", "preis'", "ihn", "Eu'r", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Himmel h\u00f6ren Euren Dank.", "tokens": ["Und", "Him\u00b7mel", "h\u00f6\u00b7ren", "Eu\u00b7ren", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vergieb die Schuld uns schwachen Armen!", "tokens": ["Ver\u00b7gieb", "die", "Schuld", "uns", "schwa\u00b7chen", "Ar\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir fehlten, s\u00fcndigten nicht gern.", "tokens": ["Wir", "fehl\u00b7ten", ",", "s\u00fcn\u00b7dig\u00b7ten", "nicht", "gern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh, wir vergeben mit Erbarmen", "tokens": ["Sieh", ",", "wir", "ver\u00b7ge\u00b7ben", "mit", "Er\u00b7bar\u00b7men"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVPP", "APPR", "NN"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schulden unsern Schuldigern.", "tokens": ["Die", "Schul\u00b7den", "un\u00b7sern", "Schul\u00b7di\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da lau'rt der Feind; la\u00df ihn nicht siegen!", "tokens": ["Da", "lau'rt", "der", "Feind", ";", "la\u00df", "ihn", "nicht", "sie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VVIMP", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Gieb Kraft uns, wenn er uns versucht!", "tokens": ["Gieb", "Kraft", "uns", ",", "wenn", "er", "uns", "ver\u00b7sucht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "PPER", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Lehr unsre schwachen Arme kriegen,", "tokens": ["Lehr", "uns\u00b7re", "schwa\u00b7chen", "Ar\u00b7me", "krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Lorbeer sei des Sieges Frucht!", "tokens": ["Und", "Lor\u00b7beer", "sei", "des", "Sie\u00b7ges", "Frucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Einst rei\u00df' aus allen Ungl\u00fcckswellen", "tokens": ["Einst", "rei\u00df'", "aus", "al\u00b7len", "Un\u00b7gl\u00fccks\u00b7wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns Deine Rechte! Hoch hervor,", "tokens": ["Uns", "Dei\u00b7ne", "Rech\u00b7te", "!", "Hoch", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo keine Laurer nach uns stellen,", "tokens": ["Wo", "kei\u00b7ne", "Lau\u00b7rer", "nach", "uns", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin steigt unser Wunsch empor.", "tokens": ["Da\u00b7hin", "steigt", "un\u00b7ser", "Wunsch", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sprich Ja! und alle Himmel neigen", "tokens": ["Sprich", "Ja", "!", "und", "al\u00b7le", "Him\u00b7mel", "nei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKANT", "$.", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich Deiner Ehre weit und breit.", "tokens": ["Sich", "Dei\u00b7ner", "Eh\u00b7re", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein, Herr, ist Reich und Kraft! Wir schweigen;", "tokens": ["Dein", ",", "Herr", ",", "ist", "Reich", "und", "Kraft", "!", "Wir", "schwei\u00b7gen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "NN", "$,", "VAFIN", "NN", "KON", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Singt, Engel, Gottes Herrlichkeit!", "tokens": ["Singt", ",", "En\u00b7gel", ",", "Got\u00b7tes", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gott, der im hohen Himmel wohnet", "tokens": ["Gott", ",", "der", "im", "ho\u00b7hen", "Him\u00b7mel", "woh\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und uns, uns Asche, Kinder nennt,", "tokens": ["Und", "uns", ",", "uns", "A\u00b7sche", ",", "Kin\u00b7der", "nennt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der \u00fcber Kron' und Scepter thronet", "tokens": ["Der", "\u00fc\u00b7ber", "Kron'", "und", "Scep\u00b7ter", "thro\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und doch f\u00fcr Staub vor Liebe brennt,", "tokens": ["Und", "doch", "f\u00fcr", "Staub", "vor", "Lie\u00b7be", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich nenne Dich den Herrn und sinke", "tokens": ["Ich", "nen\u00b7ne", "Dich", "den", "Herrn", "und", "sin\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Voll Ehrfurcht auf mein Angesicht", "tokens": ["Voll", "Ehr\u00b7furcht", "auf", "mein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und bete: \u00bbVater, Vater, winke", "tokens": ["Und", "be\u00b7te", ":", "\u00bb", "Va\u00b7ter", ",", "Va\u00b7ter", ",", "win\u00b7ke"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "wor