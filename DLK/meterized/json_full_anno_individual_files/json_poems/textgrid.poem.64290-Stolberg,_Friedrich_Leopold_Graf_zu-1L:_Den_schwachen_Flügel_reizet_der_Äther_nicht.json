{"textgrid.poem.64290": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den schwachen Fl\u00fcgel reizet der \u00c4ther nicht", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den schwachen Fl\u00fcgel reizet der \u00c4ther nicht", "tokens": ["Den", "schwa\u00b7chen", "Fl\u00fc\u00b7gel", "rei\u00b7zet", "der", "\u00c4\u00b7ther", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Felsenneste f\u00fchlt sich der Adler schon", "tokens": ["Im", "Fel\u00b7sen\u00b7nes\u00b7te", "f\u00fchlt", "sich", "der", "Ad\u00b7ler", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Voll seiner Urkraft, hebt den Fittich,", "tokens": ["Voll", "sei\u00b7ner", "Ur\u00b7kraft", ",", "hebt", "den", "Fit\u00b7tich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Senkt sich, und hebt sich, und trinkt die Sonne.", "tokens": ["Senkt", "sich", ",", "und", "hebt", "sich", ",", "und", "trinkt", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KON", "VVFIN", "PRF", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Du gabst, Natur, ihm Flug und den Sonnendurst!", "tokens": ["Du", "g