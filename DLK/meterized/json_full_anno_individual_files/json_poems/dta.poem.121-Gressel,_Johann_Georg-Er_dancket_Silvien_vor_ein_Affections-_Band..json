{"dta.poem.121": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er dancket  Silvien  vor ein  Affections-  \n Band.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich stell mich Silvia, da\u00df ich die Zahlung sey", "tokens": ["Ich", "stell", "mich", "Sil\u00b7via", ",", "da\u00df", "ich", "die", "Zah\u00b7lung", "sey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NE", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil meine Armuth dir sonst nichtes bieten kan/", "tokens": ["Weil", "mei\u00b7ne", "Ar\u00b7muth", "dir", "sonst", "nich\u00b7tes", "bie\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Unverm\u00f6gen z\u00fcndt nur schlechte Kr\u00e4uter an/", "tokens": ["Mein", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", "z\u00fcndt", "nur", "schlech\u00b7te", "Kr\u00e4u\u00b7ter", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es schenckt ", "tokens": ["Es", "schenckt"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Statt rarer Specerey und reinen Weyrauchs-Flammen/", "tokens": ["Statt", "ra\u00b7rer", "Spe\u00b7ce\u00b7rey", "und", "rei\u00b7nen", "Wey\u00b7rauchs\u00b7Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.6": {"text": "Dis Opffer wirst du nicht/ ", "tokens": ["Dis", "Opf\u00b7fer", "wirst", "du", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Weil es ein treuer Knecht auf deinem Altar streut/", "tokens": ["Weil", "es", "ein", "treu\u00b7er", "Knecht", "auf", "dei\u00b7nem", "Al\u00b7tar", "streut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der sich mit Leib und Blut zu deinen Diensten weyht.", "tokens": ["Der", "sich", "mit", "Leib", "und", "Blut", "zu", "dei\u00b7nen", "Diens\u00b7ten", "weyht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der vor erzeigte Gunst sich deinen Sclaven nennt/", "tokens": ["Der", "vor", "er\u00b7zeig\u00b7te", "Gunst", "sich", "dei\u00b7nen", "Scla\u00b7ven", "nennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PRF", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem dein geschencktes Band die Ketten angelegt/", "tokens": ["Dem", "dein", "ge\u00b7schenck\u00b7tes", "Band", "die", "Ket\u00b7ten", "an\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die er als einen Schmuck auf seinen Hertzen tr\u00e4gt/", "tokens": ["Die", "er", "als", "ei\u00b7nen", "Schmuck", "auf", "sei\u00b7nen", "Hert\u00b7zen", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das Band/ da\u00df er sich selbst aus Bl\u00f6digkeit mi\u00dfg\u00f6nnt;", "tokens": ["Das", "Band", "/", "da\u00df", "er", "sich", "selbst", "aus", "Bl\u00f6\u00b7dig\u00b7keit", "mi\u00df\u00b7g\u00f6nnt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KOUS", "PPER", "PRF", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Weil dessen Kostbarkeit auch Cronen \u00fcberwieget/", "tokens": ["Weil", "des\u00b7sen", "Kost\u00b7bar\u00b7keit", "auch", "Cro\u00b7nen", "\u00fc\u00b7berw\u00b7ie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRELAT", "NN", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und mehr als gl\u00e4ntzend Ertz aus ", "tokens": ["Und", "mehr", "als", "gl\u00e4nt\u00b7zend", "Ertz", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "ADJD", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Denn wenn gantz ", "tokens": ["Denn", "wenn", "gantz"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "Und g\u00fcldne Berge zinst/ so ist dein Band doch werth/", "tokens": ["Und", "g\u00fcld\u00b7ne", "Ber\u00b7ge", "zinst", "/", "so", "ist", "dein", "Band", "doch", "werth", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Da\u00df ich den gelben Koth der Erden Uberflu\u00df", "tokens": ["Da\u00df", "ich", "den", "gel\u00b7ben", "Koth", "der", "Er\u00b7den", "U\u00b7ber\u00b7flu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den Ursprung mancher Angst wie leichtes Spreu verschm\u00e4h", "tokens": ["Den", "Ur\u00b7sprung", "man\u00b7cher", "Angst", "wie", "leich\u00b7tes", "Spreu", "ver\u00b7schm\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "NN", "KOKOM", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wenn ich nur die Person/ die mirs geschenckt/ anseh\u2019", "tokens": ["Wenn", "ich", "nur", "die", "Per\u00b7son", "/", "die", "mirs", "ge\u00b7schenckt", "/", "an\u00b7seh'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "$(", "ART", "NE", "VVPP", "$(", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Von deren Hand ich Glas vor Perlen k\u00fcssen mu\u00df/", "tokens": ["Von", "de\u00b7ren", "Hand", "ich", "Glas", "vor", "Per\u00b7len", "k\u00fcs\u00b7sen", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "NN", "APPR", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die ich in allem Thun zu meinen Nord-", "tokens": ["Die", "ich", "in", "al\u00b7lem", "Thun", "zu", "mei\u00b7nen", "Nord"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PIS", "NN", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und eher K\u00f6nige/ als ihr Gebot verletze", "tokens": ["Und", "e\u00b7her", "K\u00f6\u00b7ni\u00b7ge", "/", "als", "ihr", "Ge\u00b7bot", "ver\u00b7let\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "$(", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Weil jener nur den Leib nicht mein Gem\u00fchte/ qu\u00e4lt", "tokens": ["Weil", "je\u00b7ner", "nur", "den", "Leib", "nicht", "mein", "Ge\u00b7m\u00fch\u00b7te", "/", "qu\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PDS", "ADV", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "$(", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sie aber und ihr Ha\u00df so Leib als Geist entseelt.", "tokens": ["Sie", "a\u00b7ber", "und", "ihr", "Ha\u00df", "so", "Leib", "als", "Geist", "ent\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PPOSAT", "NN", "ADV", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}