{"dta.poem.5260": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer GOttes Ordnungen ermi\u00dft,", "tokens": ["Wer", "Got\u00b7tes", "Ord\u00b7nun\u00b7gen", "er\u00b7mi\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Die in der Aenderung unwandelbar", "tokens": ["Die", "in", "der", "A\u00b7en\u00b7de\u00b7rung", "un\u00b7wan\u00b7del\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADJD"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kann schon aus dem, was ehemahls gewesen,", "tokens": ["Kann", "schon", "aus", "dem", ",", "was", "e\u00b7he\u00b7mahls", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "ART", "$,", "PRELS", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das K\u00fcnftige, wie gegenw\u00e4rtig, lesen.", "tokens": ["Das", "K\u00fcnf\u00b7ti\u00b7ge", ",", "wie", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", ",", "le\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach HErr la\u00df mich gesund die Fr\u00fchlings-Zeit erleben!", "tokens": ["Ach", "Herr", "la\u00df", "mich", "ge\u00b7sund", "die", "Fr\u00fch\u00b7lings\u00b7Zeit", "er\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVIMP", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "La\u00df mich, dir Lob und Danck f\u00fcr so viels Guts zu geben,", "tokens": ["La\u00df", "mich", ",", "dir", "Lob", "und", "Danck", "f\u00fcr", "so", "viels", "Guts", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPER", "NN", "KON", "NN", "APPR", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und deine weise Macht und Liebe zu erheben,", "tokens": ["Und", "dei\u00b7ne", "wei\u00b7se", "Macht", "und", "Lie\u00b7be", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In froher Achtsamkeit zum \u00f6ftern mich bestreben!", "tokens": ["In", "fro\u00b7her", "Acht\u00b7sam\u00b7keit", "zum", "\u00f6f\u00b7tern", "mich", "be\u00b7stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}