{"textgrid.poem.54150": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Schnipsel", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Um wie viel stiller ginge es in manchen Familien zu, wenn sich alle Frauen M\u00e4nner kaufen k\u00f6nnten!", "tokens": ["Um", "wie", "viel", "stil\u00b7ler", "gin\u00b7ge", "es", "in", "man\u00b7chen", "Fa\u00b7mi\u00b7li\u00b7en", "zu", ",", "wenn", "sich", "al\u00b7le", "Frau\u00b7en", "M\u00e4n\u00b7ner", "kau\u00b7fen", "k\u00f6nn\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KOKOM", "PIAT", "ADJA", "VVFIN", "PPER", "APPR", "PIAT", "NN", "PTKVZ", "$,", "KOUS", "PRF", "PIAT", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+--+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit dem Tode ist alles aus. Auch der Tod \u2013?", "tokens": ["Mit", "dem", "To\u00b7de", "ist", "al\u00b7les", "aus", ".", "Auch", "der", "Tod", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIS", "PTKVZ", "$.", "ADV", "ART", "NN", "$(", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Kleine Nachricht. Die Ausreisegeb\u00fchr aus den hamburger Zuchth\u00e4usern ist um 40 Mark erh\u00f6ht worden.", "tokens": ["Klei\u00b7ne", "Nach\u00b7richt", ".", "Die", "Aus\u00b7rei\u00b7se\u00b7ge\u00b7b\u00fchr", "aus", "den", "ham\u00b7bur\u00b7ger", "Zucht\u00b7h\u00e4u\u00b7sern", "ist", "um", "40", "Mark", "er\u00b7h\u00f6ht", "wor\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "number", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VAFIN", "APPR", "CARD", "NN", "VVPP", "VAPP", "$."], "meter": "+-+--++--+---+-+--+-+-++-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Max Liebermann w\u00e4re auch ohne H\u00e4nde ein gro\u00dfer Bankier geworden.", "tokens": ["Max", "Lie\u00b7ber\u00b7mann", "w\u00e4\u00b7re", "auch", "oh\u00b7ne", "H\u00e4n\u00b7de", "ein", "gro\u00b7\u00dfer", "Ban\u00b7kier", "ge\u00b7wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "-+--+--+-+--+-+--+-", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Er war hochm\u00fctig wie der Sohn einer zweiten hamburger Familie, aber etwas gebildeter.", "tokens": ["Er", "war", "hoch\u00b7m\u00fc\u00b7tig", "wie", "der", "Sohn", "ei\u00b7ner", "zwei\u00b7ten", "ham\u00b7bur\u00b7ger", "Fa\u00b7mi\u00b7lie", ",", "a\u00b7ber", "et\u00b7was", "ge\u00b7bil\u00b7de\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$,", "ADV", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+--+--+-+--+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Dieses M\u00e4dchen ist h\u00f6flich-sinnlich.", "tokens": ["Die\u00b7ses", "M\u00e4d\u00b7chen", "ist", "h\u00f6f\u00b7lich\u00b7sinn\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Der Schriftsteller F\u00fcl\u00f6p-Miller ist gr\u00fcndlich oberfl\u00e4chlich.", "tokens": ["Der", "Schrift\u00b7stel\u00