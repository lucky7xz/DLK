": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und von solcher Z\u00e4hren Fall", "tokens": ["Und", "von", "sol\u00b7cher", "Z\u00e4h\u00b7ren", "Fall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht man diese Blum' entsprie\u00dfen.", "tokens": ["Sieht", "man", "die\u00b7se", "Blum'", "ent\u00b7sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In dem stolzen Blumengarten", "tokens": ["In", "dem", "stol\u00b7zen", "Blu\u00b7men\u00b7gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Findet man dergleichen nicht,", "tokens": ["Fin\u00b7det", "man", "derg\u00b7lei\u00b7chen", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darum h\u00e4lt dich mein Gedicht", "tokens": ["Da\u00b7rum", "h\u00e4lt", "dich", "mein", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6her, als die andern Arten.", "tokens": ["H\u00f6\u00b7her", ",", "als", "die", "an\u00b7dern", "Ar\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Maienbl\u00fcmlein, deine Glocken,", "tokens": ["Mai\u00b7en\u00b7bl\u00fcm\u00b7lein", ",", "dei\u00b7ne", "Glo\u00b7cken", ","], "token_info": ["word", "punct", "word",