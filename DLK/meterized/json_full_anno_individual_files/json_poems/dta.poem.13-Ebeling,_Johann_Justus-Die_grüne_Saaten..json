{"dta.poem.13": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die gr\u00fcne Saaten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Sch\u00f6pfers weise Wundermacht, die\nallenthalben da zu sehen,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "wei\u00b7se", "Wun\u00b7der\u00b7macht", ",", "die", "al\u00b7len\u00b7thal\u00b7ben", "da", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Wohin wir nur das Augenlicht, auf die-", "tokens": ["Wo\u00b7hin", "wir", "nur", "das", "Au\u00b7gen\u00b7licht", ",", "auf", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "$,", "APPR", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erblikt man mit Verwunderung auf ei-", "tokens": ["Er\u00b7blikt", "man", "mit", "Ver\u00b7wun\u00b7de\u00b7rung", "auf", "ei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da sich der Hofnung Seegensbild, die gr\u00fcne Saat", "tokens": ["Da", "sich", "der", "Hof\u00b7nung", "See\u00b7gens\u00b7bild", ",", "die", "gr\u00fc\u00b7ne", "Saat"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Herze wird darob vergn\u00fcgt, die Augen wer-", "tokens": ["Das", "Her\u00b7ze", "wird", "da\u00b7rob", "ver\u00b7gn\u00fcgt", ",", "die", "Au\u00b7gen", "wer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "VAFIN", "PAV", "VVPP", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn man der Aekker frisches Gr\u00fcn, in einer Ebe-", "tokens": ["Wenn", "man", "der", "A\u00b7ek\u00b7ker", "fri\u00b7sches", "Gr\u00fcn", ",", "in", "ei\u00b7ner", "E\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "ADJA", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Zuerst, wenn jedes Korn gekeimmt, bricht die ge-", "tokens": ["Zu\u00b7erst", ",", "wenn", "je\u00b7des", "Korn", "ge\u00b7keimmt", ",", "bricht", "die", "ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PIAT", "NN", "VVPP", "$,", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und steigt mit seinem gr\u00fcnen Blat, wie eine klei-", "tokens": ["Und", "steigt", "mit", "sei\u00b7nem", "gr\u00fc\u00b7nen", "Blat", ",", "wie", "ei\u00b7ne", "klei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Saat die immer dichter wird, bedekt den schrof-", "tokens": ["Die", "Saat", "die", "im\u00b7mer", "dich\u00b7ter", "wird", ",", "be\u00b7dekt", "den", "schrof"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADV", "ADJD", "VAFIN", "$,", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wir sehn im wachsendem Gedein sie t\u00e4glich immer", "tokens": ["Wir", "sehn", "im", "wach\u00b7sen\u00b7dem", "Ge\u00b7dein", "sie", "t\u00e4g\u00b7lich", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bis sie der Mutter schwarzen Schoos, mit gr\u00fcnen", "tokens": ["Bis", "sie", "der", "Mut\u00b7ter", "schwar\u00b7zen", "Schoos", ",", "mit", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die hin und wieder sind verbr\u00e4mmt, und allent-", "tokens": ["Die", "hin", "und", "wie\u00b7der", "sind", "ver\u00b7br\u00e4mmt", ",", "und", "al\u00b7lent"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PTKVZ", "KON", "ADV", "VAFIN", "VVPP", "$,", "KON", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Mit gelber Blumen hellen Gold. Und wenn dar-", "tokens": ["Mit", "gel\u00b7ber", "Blu\u00b7men", "hel\u00b7len", "Gold", ".", "Und", "wenn", "dar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$.", "KON", "KOUS", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Sieht man ein gr\u00fcn gespanntes Tuch, mit vielen", "tokens": ["Sieht", "man", "ein", "gr\u00fcn", "ge\u00b7spann\u00b7tes", "Tuch", ",", "mit", "vie\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "ART", "ADJD", "ADJA", "NN", "$,", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Jemehr das Auge darauf schaut, jemehr bewundert", "tokens": ["Je\u00b7mehr", "das", "Au\u00b7ge", "da\u00b7rauf", "schaut", ",", "je\u00b7mehr", "be\u00b7wun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "PAV", "VVFIN", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jemehr der Geist es \u00fcberdenkt, jemehr vergn\u00fcget", "tokens": ["Je\u00b7mehr", "der", "Geist", "es", "\u00fc\u00b7ber\u00b7denkt", ",", "je\u00b7mehr", "ver\u00b7gn\u00fc\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "PPER", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Daraus die frohe Hofnung keimmt, da\u00df uns in", "tokens": ["Da\u00b7raus", "die", "fro\u00b7he", "Hof\u00b7nung", "keimmt", ",", "da\u00df", "uns", "in"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.18": {"text": "Des Sch\u00f6pfers weise Macht und G\u00fct, werd un-", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "wei\u00b7se", "Macht", "und", "G\u00fct", ",", "werd", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$,", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die Saat die immer gr\u00f6sser wird, die Sonne, Thau", "tokens": ["Die", "Saat", "die", "im\u00b7mer", "gr\u00f6s\u00b7ser", "wird", ",", "die", "Son\u00b7ne", ",", "Thau"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "ADV", "ADJD", "VAFIN", "$,", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die breitet sich nun weiter aus, und wird ein Zun-", "tokens": ["Die", "brei\u00b7tet", "sich", "nun", "wei\u00b7ter", "aus", ",", "und", "wird", "ein", "Zun"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ADV", "PTKVZ", "$,", "KON", "VAFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und wenn die Luft dieselbe regt; so deucht mir da\u00df", "tokens": ["Und", "wenn", "die", "Luft", "die\u00b7sel\u00b7be", "regt", ";", "so", "deucht", "mir", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PDAT", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "KOUS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mit lauter Zungen angef\u00fcllt, zu ", "tokens": ["Mit", "lau\u00b7ter", "Zun\u00b7gen", "an\u00b7ge\u00b7f\u00fcllt", ",", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Des Sch\u00f6pfers w\u00fcrkend Allmachts-Wort,\ndas Saamenk\u00f6rner fruchtbar macht,\nHat uns nunmehro aus der Erd, als unsern\nSchoo\u00df herf\u00fcrgebracht;\nWir haben nun das Feld bedekt, mit einen\ngr\u00fcnen Hofnungskleide,\nUnd geben dem der uns beschaut, die ange-\nnehmste Augenweide,\nSo machts der Sch\u00f6pfer der Natur, der\nseine Kreaturen schm\u00fckt,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "w\u00fcr\u00b7kend", "All\u00b7machts\u00b7Wort", ",", "das", "Saa\u00b7men\u00b7k\u00f6r\u00b7ner", "frucht\u00b7bar", "macht", ",", "Hat", "uns", "nun\u00b7meh\u00b7ro", "aus", "der", "Erd", ",", "als", "un\u00b7sern", "Schoo\u00df", "her\u00b7f\u00fcr\u00b7ge\u00b7bracht", ";", "Wir", "ha\u00b7ben", "nun", "das", "Feld", "be\u00b7dekt", ",", "mit", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Hof\u00b7nungs\u00b7klei\u00b7de", ",", "Und", "ge\u00b7ben", "dem", "der", "uns", "be\u00b7schaut", ",", "die", "an\u00b7ge", "nehms\u00b7te", "Au\u00b7gen\u00b7wei\u00b7de", ",", "So", "machts", "der", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", ",", "der", "sei\u00b7ne", "Kre\u00b7a\u00b7tu\u00b7ren", "schm\u00fckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "ART", "NN", "ADJD", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$.", "PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "PRELS", "PPER", "VVFIN", "$,", "ART", "TRUNC", "ADJA", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+----+-+-+-+-+-+-+--+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.24": {"text": "Er hat euch seine Wunderg\u00fct, in alle Sin-\nnen eingedr\u00fckt\nAnjezo k\u00f6nt ihr solche sehn, in kurzen k\u00f6n-\nnet ihr sie schmekken,\nO! Menschen last euch nur dadurch, zu eu-\nres Gebers Ruhm erwekken!", "tokens": ["Er", "hat", "euch", "sei\u00b7ne", "Wun\u00b7der\u00b7g\u00fct", ",", "in", "al\u00b7le", "Sin", "nen", "ein\u00b7ge\u00b7dr\u00fckt", "An\u00b7je\u00b7zo", "k\u00f6nt", "ihr", "sol\u00b7che", "sehn", ",", "in", "kur\u00b7zen", "k\u00f6n", "net", "ihr", "sie", "schmek\u00b7ken", ",", "O", "!", "Men\u00b7schen", "last", "euch", "nur", "da\u00b7durch", ",", "zu", "eu", "res", "Ge\u00b7bers", "Ruhm", "er\u00b7wek\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "APPR", "PIAT", "TRUNC", "VVINF", "VVFIN", "NE", "VVFIN", "PPER", "PIS", "VVINF", "$,", "APPR", "ADJA", "TRUNC", "VVFIN", "PPER", "PPER", "VVINF", "$,", "ITJ", "$.", "NN", "VVFIN", "PPER", "ADV", "PAV", "$,", "APPR", "TRUNC", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+-+--+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.25": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.27": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.28": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.29": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.30": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "D", "tokens": ["D"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}}}}}