{"dta.poem.7494": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So redet nur! Ihr sollt mich nicht bekehren.", "tokens": ["So", "re\u00b7det", "nur", "!", "Ihr", "sollt", "mich", "nicht", "be\u00b7keh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er ist in Eurer H\u00fctte nie gestanden,", "tokens": ["Er", "ist", "in", "Eu\u00b7rer", "H\u00fct\u00b7te", "nie", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "War Euch nie weihend, segnend nie zu Handen,", "tokens": ["War", "Euch", "nie", "wei\u00b7hend", ",", "seg\u00b7nend", "nie", "zu", "Han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was man nicht kennt, das mag man leicht entbehren.", "tokens": ["Was", "man", "nicht", "kennt", ",", "das", "mag", "man", "leicht", "ent\u00b7beh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PTKNEG", "VVFIN", "$,", "PDS", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch mir geht ohne ", "tokens": ["Doch", "mir", "geht", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Inde\u00df die N\u00fcchternen in allen Landen,", "tokens": ["In\u00b7de\u00df", "die", "N\u00fcch\u00b7ter\u00b7nen", "in", "al\u00b7len", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Gottentfremdeten, die Sch\u00e4tze mehren.", "tokens": ["Die", "Got\u00b7tent\u00b7frem\u00b7de\u00b7ten", ",", "die", "Sch\u00e4t\u00b7ze", "meh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Behagt Euch wohl im friedlichen Genu\u00df,", "tokens": ["Be\u00b7hagt", "Euch", "wohl", "im", "fried\u00b7li\u00b7chen", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Bischen Witz, es bleib' Euch unbenommen,", "tokens": ["Das", "Bi\u00b7schen", "Witz", ",", "es", "bleib'", "Euch", "un\u00b7be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das auf die Frohne wie ein Sklave mu\u00df.", "tokens": ["Das", "auf", "die", "Froh\u00b7ne", "wie", "ein", "Skla\u00b7ve", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "KOKOM", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Ein heil'ger Sturm mein Herz erst \u00fcberkommen.", "tokens": ["Ein", "heil'\u00b7ger", "Sturm", "mein", "Herz", "erst", "\u00fc\u00b7ber\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}