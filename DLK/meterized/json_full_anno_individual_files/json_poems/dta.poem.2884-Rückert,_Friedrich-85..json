{"dta.poem.2884": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Gl\u00fcck ganz unverhofft dir zugefallen;", "tokens": ["Es", "ist", "ein", "Gl\u00fcck", "ganz", "un\u00b7ver\u00b7hofft", "dir", "zu\u00b7ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun der zufriedenste wirst du wol seyn von allen.", "tokens": ["Nun", "der", "zu\u00b7frie\u00b7dens\u00b7te", "wirst", "du", "wol", "seyn", "von", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VAFIN", "PPER", "ADV", "VAINF", "APPR", "PIAT", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch nein, es hat in dir den Wunsch nur aufgeregt,", "tokens": ["Doch", "nein", ",", "es", "hat", "in", "dir", "den", "Wunsch", "nur", "auf\u00b7ge\u00b7regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Saamen der Begier dir in die Brust gelegt.", "tokens": ["Den", "Saa\u00b7men", "der", "Be\u00b7gier", "dir", "in", "die", "Brust", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du h\u00e4ltst das Gl\u00fcck nur f\u00fcr ein Gl\u00fcckverhei\u00dfungszeichen,", "tokens": ["Du", "h\u00e4ltst", "das", "Gl\u00fcck", "nur", "f\u00fcr", "ein", "Gl\u00fcck\u00b7ver\u00b7hei\u00b7\u00dfungs\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil soviel sei erreicht, sei alles zu erreichen.", "tokens": ["Weil", "so\u00b7viel", "sei", "er\u00b7reicht", ",", "sei", "al\u00b7les", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "VVPP", "$,", "VAFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gib acht, da\u00df \u00fcbernacht es dir nicht komm' abhanden,", "tokens": ["Gib", "acht", ",", "da\u00df", "\u00fc\u00b7bern\u00b7acht", "es", "dir", "nicht", "komm'", "ab\u00b7han\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "KOUS", "VVFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil unser Zeichen du hast schm\u00e4hlich misverstanden.", "tokens": ["Weil", "un\u00b7ser", "Zei\u00b7chen", "du", "hast", "schm\u00e4h\u00b7lich", "mis\u00b7ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}