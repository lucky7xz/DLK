{"dta.poem.6907": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es mache jemand diesen Schlu\u00df,", "tokens": ["Es", "ma\u00b7che", "je\u00b7mand", "die\u00b7sen", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man drey Winckel und drey Seiten,", "tokens": ["Da\u00df", "man", "drey", "Win\u00b7ckel", "und", "drey", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "CARD", "NN", "KON", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um einen Dreyeck zu bereiten,", "tokens": ["Um", "ei\u00b7nen", "Drey\u00b7eck", "zu", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nothwendig nehmen mu\u00df;", "tokens": ["Noth\u00b7wen\u00b7dig", "neh\u00b7men", "mu\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So konnte man ihn schon im Dencken finden:", "tokens": ["So", "konn\u00b7te", "man", "ihn", "schon", "im", "Den\u00b7cken", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ja, um die Eigenschafft desselben zu ergr\u00fcnden,", "tokens": ["Ja", ",", "um", "die", "Ei\u00b7gen\u00b7schafft", "des\u00b7sel\u00b7ben", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUI", "ART", "NN", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Braucht es der W\u00fcrcklichkeit des Drey-Ecks nicht.", "tokens": ["Braucht", "es", "der", "W\u00fcrck\u00b7lich\u00b7keit", "des", "Drey\u00b7Ecks", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Jm Geist allein sind die Jdeen,", "tokens": ["Jm", "Geist", "al\u00b7lein", "sind", "die", "Jdeen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Die alle blo\u00df in ihm bestehen.", "tokens": ["Die", "al\u00b7le", "blo\u00df", "in", "ihm", "be\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Figuren haben stets von hundert tausend Seiten,", "tokens": ["Fi\u00b7gu\u00b7ren", "ha\u00b7ben", "stets", "von", "hun\u00b7dert", "tau\u00b7send", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "CARD", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Merckt es der Blick gleich nicht, doch ihre W\u00fcrcklichkeiten.", "tokens": ["Merckt", "es", "der", "Blick", "gleich", "nicht", ",", "doch", "ih\u00b7re", "W\u00fcrck\u00b7lich\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKNEG", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}}}}}