{"textgrid.poem.63810": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "21. Geisterbeschw\u00f6rung", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jeder, und sei er auch noch so jung, hier lernt er Erinnern;", "tokens": ["Je\u00b7der", ",", "und", "sei", "er", "auch", "noch", "so", "jung", ",", "hier", "lernt", "er", "E\u00b7rin\u00b7nern", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Lernt' er es sonst schon, \u2013 hier wird er ein Meister der Kunst.", "tokens": ["Lernt'", "er", "es", "sonst", "schon", ",", "\u2013", "hier", "wird", "er", "ein", "Meis\u00b7ter", "der", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$,", "$(", "ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Doch hier ist's kein traulich Gesch\u00e4ft. Von herzlicher Treue,", "tokens": ["Doch", "hier", "ist's", "kein", "trau\u00b7lich", "Ge\u00b7sch\u00e4ft", ".", "Von", "herz\u00b7li\u00b7cher", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "ADJD", "NN", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Inniger Sehnsucht wei\u00df hier die Erinnerung nichts.", "tokens": ["In\u00b7ni\u00b7ger", "Sehn\u00b7sucht", "wei\u00df", "hier", "die", "E\u00b7rin\u00b7ne\u00b7rung", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "PIS", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.5": {"text": "Was verschwunden, geh\u00f6rte der Welt. Es rauscht wie ein Sturmwind,", "tokens": ["Was", "ver\u00b7schwun\u00b7den", ",", "ge\u00b7h\u00f6r\u00b7te", "der", "Welt", ".", "Es", "rauscht", "wie", "ein", "Sturm\u00b7wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "$,", "VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Wenn sich ein Folioblatt dieser Annalen bewegt.", "tokens": ["Wenn", "sich", "ein", "Fo\u00b7li\u00b7o\u00b7blatt", "die\u00b7ser", "An\u00b7na\u00b7len", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Nur wer lesen gelernt auch zwischen den Zeilen, erf\u00e4hrt aus", "tokens": ["Nur", "wer", "le\u00b7sen", "ge\u00b7lernt", "auch", "zwi\u00b7schen", "den", "Zei\u00b7len", ",", "er\u00b7f\u00e4hrt", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PWS", "VVINF", "VVPP", "ADV", "APPR", "ART", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Diesem Gedenkbuch auch heimliches Herzensgeschick.", "tokens": ["Die\u00b7sem", "Ge\u00b7denk\u00b7buch", "auch", "heim\u00b7li\u00b7ches", "Her\u00b7zens\u00b7ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.9": {"text": "Dichteraugen erscheint in dem Armband, das in der Villa", "tokens": ["Dich\u00b7ter\u00b7au\u00b7gen", "er\u00b7scheint", "in", "dem", "Arm\u00b7band", ",", "das", "in", "der", "Vil\u00b7la"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+--+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Unter dem Schutte sich fand, mehr als ein goldener Reif.", "tokens": ["Un\u00b7ter", "dem", "Schut\u00b7te", "sich", "fand", ",", "mehr", "als", "ein", "gol\u00b7de\u00b7ner", "Reif", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$,", "PIAT", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Ihnen ersteht aus der Asche der Arm und winkt und bewegt sich,", "tokens": ["Ih\u00b7nen", "er\u00b7steht", "aus", "der", "A\u00b7sche", "der", "Arm", "und", "winkt", "und", "be\u00b7wegt", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "PRF", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Schmiegt sich sch\u00fcchtern und fest um des Erkorenen Hals.", "tokens": ["Schmiegt", "sich", "sch\u00fcch\u00b7tern", "und", "fest", "um", "des", "Er\u00b7ko\u00b7re\u00b7nen", "Hals", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVINF", "KON", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wesenloses gewinnt nun Gehalt, Geringes Bedeutung,", "tokens": ["We\u00b7sen\u00b7lo\u00b7ses", "ge\u00b7winnt", "nun", "Ge\u00b7halt", ",", "Ge\u00b7rin\u00b7ges", "Be\u00b7deu\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Und aus Moder und Staub lodert noch einmal der Geist.", "tokens": ["Und", "aus", "Mo\u00b7der", "und", "Staub", "lo\u00b7dert", "noch", "ein\u00b7mal", "der", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Jeder, und sei er auch noch so jung, hier lernt er Erinnern;", "tokens": ["Je\u00b7der", ",", "und", "sei", "er", "auch", "noch", "so", "jung", ",", "hier", "lernt", "er", "E\u00b7rin\u00b7nern", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KON", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,", "ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Lernt' er es sonst schon, \u2013 hier wird er ein Meister der Kunst.", "tokens": ["Lernt'", "er", "es", "sonst", "schon", ",", "\u2013", "hier", "wird", "er", "ein", "Meis\u00b7ter", "der", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADV", "$,", "$(", "ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Doch hier ist's kein traulich Gesch\u00e4ft. Von h