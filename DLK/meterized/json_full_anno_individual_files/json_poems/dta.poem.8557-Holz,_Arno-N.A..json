{"dta.poem.8557": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Horizont ein Flammenring,", "tokens": ["Der", "Ho\u00b7ri\u00b7zont", "ein", "Flam\u00b7men\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "kein Segel!", "tokens": ["kein", "Se\u00b7gel", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Jammernd,", "tokens": ["Jam\u00b7mernd", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "die weissen H\u00e4nde", "tokens": ["die", "weis\u00b7sen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "ringt", "tokens": ["ringt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "die sch\u00f6nste Frau.", "tokens": ["die", "sch\u00f6ns\u00b7te", "Frau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nur st\u00fcrzende Wellenberge,", "tokens": ["Nur", "st\u00fcr\u00b7zen\u00b7de", "Wel\u00b7len\u00b7ber\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "blinkende Delphine", "tokens": ["blin\u00b7ken\u00b7de", "Del\u00b7phi\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.3": {"text": "und fern ein Meervolk, das auf Muschelh\u00f6rnern bl\u00e4st.", "tokens": ["und", "fern", "ein", "Meer\u00b7volk", ",", "das", "auf", "Mu\u00b7schel\u00b7h\u00f6r\u00b7nern", "bl\u00e4st", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Sonne brennt, die Brandung unten zischt,", "tokens": ["Die", "Son\u00b7ne", "brennt", ",", "die", "Bran\u00b7dung", "un\u00b7ten", "zischt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das Ungeheuer,", "tokens": ["das", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "mit gestreckten Poten,", "tokens": ["mit", "ge\u00b7streck\u00b7ten", "Po\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "die sieben Zungen schlaff aus seinem Maul,", "tokens": ["die", "sie\u00b7ben", "Zun\u00b7gen", "schlaff", "aus", "sei\u00b7nem", "Maul", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "liegt auf dem R\u00fccken und schl\u00e4ft.", "tokens": ["liegt", "auf", "dem", "R\u00fc\u00b7cken", "und", "schl\u00e4ft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Einmal,", "tokens": ["Ein\u00b7mal", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "zweimal noch,", "tokens": ["zwei\u00b7mal", "noch", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "kr\u00fcmmt sich sein Schweif,", "tokens": ["kr\u00fcmmt", "sich", "sein", "Schweif", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "zuckt,", "tokens": ["zuckt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "ringelt sich und rollt dann wieder langsam in die Klippen.", "tokens": ["rin\u00b7gelt", "sich", "und", "rollt", "dann", "wie\u00b7der", "lang\u00b7sam", "in", "die", "Klip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KON", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.6": {"line.1": {"text": "Tang und Quallen kriechen an ihm hoch.", "tokens": ["Tang", "und", "Qual\u00b7len", "krie\u00b7chen", "an", "ihm", "hoch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}