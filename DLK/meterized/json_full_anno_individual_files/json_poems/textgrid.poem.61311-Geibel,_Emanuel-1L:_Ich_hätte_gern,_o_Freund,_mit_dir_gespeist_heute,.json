{"textgrid.poem.61311": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich h\u00e4tte gern, o Freund, mit dir gespeist heute,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich h\u00e4tte gern, o Freund, mit dir gespeist heute,", "tokens": ["Ich", "h\u00e4t\u00b7te", "gern", ",", "o", "Freund", ",", "mit", "dir", "ge\u00b7speist", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "FM", "NN", "$,", "APPR", "PPER", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und frohen Muts bei perlenreichem Schaumweine", "tokens": ["Und", "fro\u00b7hen", "Muts", "bei", "per\u00b7len\u00b7rei\u00b7chem", "Schaum\u00b7wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Der Zeit gedacht, da wir im attischen \u00d6lwalde", "tokens": ["Der", "Zeit", "ge\u00b7dacht", ",", "da", "wir", "im", "at\u00b7ti\u00b7schen", "\u00d6l\u00b7wal\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An herberm Trunk uns labten aus dem Pechschlauche.", "tokens": ["An", "her\u00b7berm", "Trunk", "uns", "lab\u00b7ten", "aus", "dem", "Pech\u00b7schlau\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "Auch h\u00e4tt' ich willig dir von hundert Torheiten", "tokens": ["Auch", "h\u00e4tt'", "ich", "wil\u00b7lig", "dir", "von", "hun\u00b7dert", "Tor\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Erz\u00e4hlt, wie mir im schwangern Haupte buntfarbig", "tokens": ["Er\u00b7z\u00e4hlt", ",", "wie", "mir", "im", "schwan\u00b7gern", "Haup\u00b7te", "bunt\u00b7far\u00b7big"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "APPRART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.7": {"text": "Ein ganzer Rattenk\u00f6nig sitzt von Lustspielen.", "tokens": ["Ein", "gan\u00b7zer", "Rat\u00b7ten\u00b7k\u00f6\u00b7nig", "sitzt", "von", "Lust\u00b7spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.8": {"text": "Du aber w\u00e4rst vielleicht, dafern ich scherzweise", "tokens": ["Du", "a\u00b7ber", "w\u00e4rst", "viel\u00b7leicht", ",", "da\u00b7fern", "ich", "scher\u00b7zwei\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+---+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Mich Zeus vergleichen darf, in ros'ger Weinlaune", "tokens": ["Mich", "Zeus", "ver\u00b7glei\u00b7chen", "darf", ",", "in", "ros'\u00b7ger", "Wein\u00b7lau\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NE", "VVINF", "VMFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.10": {"text": "Heph\u00e4stos worden, meines Kopfes Hebamme.", "tokens": ["He\u00b7ph\u00e4s\u00b7tos", "wor\u00b7den", ",", "mei\u00b7nes", "Kop\u00b7fes", "He\u00b7bam\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAPP", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Doch andres sannest du, und andern Pfad w\u00e4hlet", "tokens": ["Doch", "and\u00b7res", "san\u00b7nest", "du", ",", "und", "an\u00b7dern", "Pfad", "w\u00e4h\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Hore. Denn es lud der malereikund'ge", "tokens": ["Die", "Ho\u00b7re", ".", "Denn", "es", "lud", "der", "ma\u00b7le\u00b7rei\u00b7kun\u00b7d'\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "$.", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Breitstirn'ge Freund mich gestern schon zum Gastmahle;", "tokens": ["Breit\u00b7stirn'\u00b7ge", "Freund", "mich", "ge\u00b7stern", "schon", "zum", "Gast\u00b7mah\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.14": {"text": "Und sicher w\u00e4r' es mi\u00dfgetan, durch Ausbleiben", "tokens": ["Und", "si\u00b7cher", "w\u00e4r'", "es", "mi\u00df\u00b7ge\u00b7tan", ",", "durch", "Aus\u00b7blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PTKVZ", "$,", "APPR", "NN"], "meter": "-+-+-+--+-+-", "measure": "i