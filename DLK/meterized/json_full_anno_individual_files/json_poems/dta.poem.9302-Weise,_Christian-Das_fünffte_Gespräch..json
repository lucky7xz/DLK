{"dta.poem.9302": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Meine mutter wils nicht haben", "tokens": ["Mei\u00b7ne", "mut\u00b7ter", "wils", "nicht", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich zeitlich freyen sol/", "tokens": ["Da\u00df", "ich", "zeit\u00b7lich", "frey\u00b7en", "sol", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sie meinet/ solchen knaben", "tokens": ["Denn", "sie", "mei\u00b7net", "/", "sol\u00b7chen", "kna\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$(", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thuts ein ander h\u00f6ltzgen wol:", "tokens": ["Thuts", "ein", "an\u00b7der", "h\u00f6ltz\u00b7gen", "wol", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJD", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber ich denck auff die freythe", "tokens": ["A\u00b7ber", "ich", "denck", "auff", "die", "frey\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieber morgen w\u00e4rstu heute.", "tokens": ["Lie\u00b7ber", "mor\u00b7gen", "w\u00e4rs\u00b7tu", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Sol ich funfzehn jahr noch warten", "tokens": ["Sol", "ich", "funf\u00b7zehn", "jahr", "noch", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "CARD", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ein alter tantzknecht seyn?", "tokens": ["Und", "ein", "al\u00b7ter", "tantz\u00b7knecht", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach ich kan es besser karten/", "tokens": ["Ach", "ich", "kan", "es", "bes\u00b7ser", "kar\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VMFIN", "PPER", "ADJD", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Willigt nur das m\u00e4dgen drein.", "tokens": ["Wil\u00b7ligt", "nur", "das", "m\u00e4d\u00b7gen", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum gedenck ich an die freythe/", "tokens": ["Drum", "ge\u00b7denck", "ich", "an", "die", "frey\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieber morgen w\u00e4rstu heute.", "tokens": ["Lie\u00b7ber", "mor\u00b7gen", "w\u00e4rs\u00b7tu", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Hab ich doch genug zu wehren/", "tokens": ["Hab", "ich", "doch", "ge\u00b7nug", "zu", "weh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur in einer kurtzen nacht/", "tokens": ["Nur", "in", "ei\u00b7ner", "kurt\u00b7zen", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der ehstand meinen ehren", "tokens": ["Da\u00df", "der", "eh\u00b7stand", "mei\u00b7nen", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine kunckelfusen macht.", "tokens": ["Kei\u00b7ne", "kun\u00b7ckel\u00b7fu\u00b7sen", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum so denck ich an die freythe/", "tokens": ["Drum", "so", "denck", "ich", "an", "die", "frey\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVIMP", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieber morgen w\u00e4rstu heute.", "tokens": ["Lie\u00b7ber", "mor\u00b7gen", "w\u00e4rs\u00b7tu", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "4. Ich mag nicht alleine liegen", "tokens": ["Ich", "mag", "nicht", "al\u00b7lei\u00b7ne", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenns die mutter zehnmahl sagt/", "tokens": ["Wenns", "die", "mut\u00b7ter", "zehn\u00b7mahl", "sagt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kan ich keine jungfer kriegen/", "tokens": ["Kan", "ich", "kei\u00b7ne", "jung\u00b7fer", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun so nehm ich eine magd/", "tokens": ["Nun", "so", "nehm", "ich", "ei\u00b7ne", "magd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also denck ich an die freythe/", "tokens": ["Al\u00b7so", "denck", "ich", "an", "die", "frey\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieber morgen w\u00e4rstu heute.", "tokens": ["Lie\u00b7ber", "mor\u00b7gen", "w\u00e4rs\u00b7tu", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "5. Ich bin witzig und gedencke", "tokens": ["Ich", "bin", "wit\u00b7zig", "und", "ge\u00b7den\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was zu meinen sachen frommt/", "tokens": ["Was", "zu", "mei\u00b7nen", "sa\u00b7chen", "frommt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schickt mir nur das hochzt-geschencke/", "tokens": ["Schickt", "mir", "nur", "das", "hochz\u00b7tge\u00b7schen\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ihr nicht zur hochzeit kommt.", "tokens": ["Wenn", "ihr", "nicht", "zur", "hoch\u00b7zeit", "kommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es geht nun auf die freythe/", "tokens": ["Denn", "es", "geht", "nun", "auf", "die", "frey\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieber morgen w\u00e4rstu heute.", "tokens": ["Lie\u00b7ber", "mor\u00b7gen", "w\u00e4rs\u00b7tu", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}