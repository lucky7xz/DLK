{"dta.poem.23371": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "W\u00e4chterlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die H\u00e4hne kr\u00e4hten durch das Land:", "tokens": ["Die", "H\u00e4h\u00b7ne", "kr\u00e4h\u00b7ten", "durch", "das", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wer in Schlafes Banden ruht,", "tokens": ["Und", "wer", "in", "Schla\u00b7fes", "Ban\u00b7den", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei munter jetzt und wohlgemuth!", "tokens": ["Sei", "mun\u00b7ter", "jetzt", "und", "wohl\u00b7ge\u00b7muth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tag beginnt, die Nacht verschwand.", "tokens": ["Der", "Tag", "be\u00b7ginnt", ",", "die", "Nacht", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der W\u00e4chter auf der Zinne stand", "tokens": ["Der", "W\u00e4ch\u00b7ter", "auf", "der", "Zin\u00b7ne", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rief: ihr sollet munter sein,", "tokens": ["Und", "rief", ":", "ihr", "sol\u00b7let", "mun\u00b7ter", "sein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sehe schon des Tages Schein;", "tokens": ["Ich", "se\u00b7he", "schon", "des", "Ta\u00b7ges", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wacht auf! wacht auf! die Nacht verschwand.", "tokens": ["Wacht", "auf", "!", "wacht", "auf", "!", "die", "Nacht", "ver\u00b7schwand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da stand man auf wohl hie und dort,", "tokens": ["Da", "stand", "man", "auf", "wohl", "hie", "und", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die H\u00e4hne that man in den Topf,", "tokens": ["Die", "H\u00e4h\u00b7ne", "that", "man", "in", "den", "Topf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem W\u00e4chter hieb man ab den Kopf,", "tokens": ["Dem", "W\u00e4ch\u00b7ter", "hieb", "man", "ab", "den", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann aber schlief man weiter fort.", "tokens": ["Dann", "a\u00b7ber", "schlief", "man", "wei\u00b7ter", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wer will noch Hahn und W\u00e4chter sein?", "tokens": ["Wer", "will", "noch", "Hahn", "und", "W\u00e4ch\u00b7ter", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "NE", "KON", "NE", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wecket uns aus Schlafes Noth", "tokens": ["Wer", "we\u00b7cket", "uns", "aus", "Schla\u00b7fes", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald zu der Freiheit Morgenroth?", "tokens": ["Bald", "zu", "der", "Frei\u00b7heit", "Mor\u00b7gen\u00b7roth", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir schlafen in den Tag hinein.", "tokens": ["Wir", "schla\u00b7fen", "in", "den", "Tag", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}