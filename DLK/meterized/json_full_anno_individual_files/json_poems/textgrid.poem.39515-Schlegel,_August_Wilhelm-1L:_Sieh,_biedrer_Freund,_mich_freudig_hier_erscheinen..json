{"textgrid.poem.39515": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sieh, biedrer Freund, mich freudig hier erscheinen.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieh, biedrer Freund, mich freudig hier erscheinen.", "tokens": ["Sieh", ",", "bie\u00b7drer", "Freund", ",", "mich", "freu\u00b7dig", "hier", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die holde Braut in ihrer Myrtenkrone", "tokens": ["Die", "hol\u00b7de", "Braut", "in", "ih\u00b7rer", "Myr\u00b7ten\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird dem bew\u00e4hrten edeln Mann zum Lohne;", "tokens": ["Wird", "dem", "be\u00b7w\u00e4hr\u00b7ten", "e\u00b7deln", "Mann", "zum", "Loh\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zwei treue Herzen wollen sich vereinen.", "tokens": ["Zwei", "treu\u00b7e", "Her\u00b7zen", "wol\u00b7len", "sich", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit Dir sei Gottes Segen, und den Deinen!", "tokens": ["Mit", "Dir", "sei", "Got\u00b7tes", "Se\u00b7gen", ",", "und", "den", "Dei\u00b7nen", "!