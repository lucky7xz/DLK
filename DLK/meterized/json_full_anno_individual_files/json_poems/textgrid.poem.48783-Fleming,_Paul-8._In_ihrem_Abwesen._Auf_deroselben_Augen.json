{"textgrid.poem.48783": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "8. In ihrem Abwesen. Auf deroselben Augen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr irdne Sonn' und Mon, ihr meiner Augen Augen,", "tokens": ["Ihr", "ird\u00b7ne", "Sonn'", "und", "Mon", ",", "ihr", "mei\u00b7ner", "Au\u00b7gen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NE", "$,", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wo la\u00dft ihr euren Mich? Seht ihr mich gar nicht an,", "tokens": ["wo", "la\u00dft", "ihr", "eu\u00b7ren", "Mich", "?", "Seht", "ihr", "mich", "gar", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ach, ach! so ist es ganz und gar um mich getan!", "tokens": ["ach", ",", "ach", "!", "so", "ist", "es", "ganz", "und", "gar", "um", "mich", "ge\u00b7tan", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "ITJ", "$.", "ADV", "VAFIN", "PPER", "ADV", "KON", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich regne f\u00fcr und f\u00fcr mit scharfer Tr\u00e4nen Laugen.", "tokens": ["Ich", "reg\u00b7ne", "f\u00fcr", "und", "f\u00fcr", "mit", "schar\u00b7fer", "Tr\u00e4\u00b7nen", "Lau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "APPR", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "F\u00fcr mich wil ganz kein Licht, als nur das eure, taugen.", "tokens": ["F\u00fcr", "mich", "wil", "ganz", "kein", "Licht", ",", "als", "nur", "das", "eu\u00b7re", ",", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "ADV", "PIAT", "NN", "$,", "KOUS", "ADV", "ART", "PPOSAT", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mittag wird zur Nacht. Ihr, ihr habt Schuld daran,", "tokens": ["Der", "Mit\u00b7tag", "wird", "zur", "Nacht", ".", "Ihr", ",", "ihr", "habt", "Schuld", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "$.", "PPER", "$,", "PPER", "VAFIN", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "da\u00df ich sonst keinen Glanz, denn euren, sehen kan,", "tokens": ["da\u00df", "ich", "sonst", "kei\u00b7nen", "Glanz", ",", "denn", "eu\u00b7ren", ",", "se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "$,", "KON", "PPOSAT", "$,", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und dessen Kraft von euch, als Brunnen, aus mu\u00df saugen.", "tokens": ["und", "des\u00b7sen", "Kraft", "von", "euch", ",", "als", "Brun\u00b7nen", ",", "aus", "mu\u00df", "sau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "APPR", "PPER", "$,", "KOUS", "NN", "$,", "APPR", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich seh' und bin doch blind, ich irre hin und her,", "tokens": ["Ich", "seh'", "und", "bin", "doch", "blind", ",", "ich", "ir\u00b7re", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ich wei\u00df nicht, wo ich bin, in diesem finstern Meer.", "tokens": ["ich", "wei\u00df", "nicht", ",", "wo", "ich", "bin", ",", "in", "die\u00b7sem", "fins\u00b7tern", "Meer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "$,", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erscheint, erscheint mir doch, ihr funkelnden Laternen,", "tokens": ["Er\u00b7scheint", ",", "er\u00b7scheint", "mir", "doch", ",", "ihr", "fun\u00b7keln\u00b7den", "La\u00b7ter\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "ihr Br\u00fcder Helene, und zeigt mir euer Licht!", "tokens": ["ihr", "Br\u00fc\u00b7der", "He\u00b7le\u00b7ne", ",", "und", "zeigt", "mir", "eu\u00b7er", "Licht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-----+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Wo nicht, so hilfet mich ganz keine Flamme nicht.", "tokens": ["Wo", "nicht", ",", "so", "hil\u00b7fet", "mich", "ganz", "kei\u00b7ne", "Flam\u00b7me", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ADV", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bei Tage kein Mittag, bei Nachte keine Sternen.", "tokens": ["bei", "Ta\u00b7ge", "kein", "Mit\u00b7tag", ",", "bei", "Nach\u00b7te", "kei\u00b7ne", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,", "APPR", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihr irdne Sonn' und Mon, ihr meiner Augen Augen,", "tokens": ["Ihr", "ird\u00b7ne", "Sonn'", "und", "Mon", ",", "ihr", "mei\u00b7ner", "Au\u00b7gen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NE", "$,", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wo la\u00dft ihr euren Mich? Seht ihr mich gar nicht an,", "tokens": ["wo", "la\u00dft", "ihr", "eu\u00b7ren", "Mich", "?", "Seht", "ihr", "mich", "gar", "nicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "ach, ach! so ist es ganz