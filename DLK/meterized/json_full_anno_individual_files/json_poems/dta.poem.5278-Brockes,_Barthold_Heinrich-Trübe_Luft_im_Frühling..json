{"dta.poem.5278": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00fcbe Luft im Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man siehet zwar auch oftermahls im Lentzen,", "tokens": ["Man", "sie\u00b7het", "zwar", "auch", "of\u00b7ter\u00b7mahls", "im", "Lent\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, in nicht aufgekl\u00e4rter Luft,", "tokens": ["Da\u00df", ",", "in", "nicht", "auf\u00b7ge\u00b7kl\u00e4r\u00b7ter", "Luft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "APPR", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein allgemeiner Duft", "tokens": ["Ein", "all\u00b7ge\u00b7mei\u00b7ner", "Duft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des Himmels heitres Blau, der Sonnen Gl\u00e4ntzen", "tokens": ["Des", "Him\u00b7mels", "heit\u00b7res", "Blau", ",", "der", "Son\u00b7nen", "Gl\u00e4nt\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bedecket und verh\u00fcllet", "tokens": ["Be\u00b7de\u00b7cket", "und", "ver\u00b7h\u00fcl\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und unsern gantzen Kreis der Luft erf\u00fcllet;", "tokens": ["Und", "un\u00b7sern", "gant\u00b7zen", "Kreis", "der", "Luft", "er\u00b7f\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es f\u00e4rben sich die Wolcken falb\u2019 und grau:", "tokens": ["Es", "f\u00e4r\u00b7ben", "sich", "die", "Wol\u00b7cken", "falb'", "und", "grau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch zeigt ein solches Falbes sich", "tokens": ["Doch", "zeigt", "ein", "sol\u00b7ches", "Fal\u00b7bes", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nicht, wie im Sommer, f\u00fcrchterlich;", "tokens": ["Nicht", ",", "wie", "im", "Som\u00b7mer", ",", "f\u00fcrch\u00b7ter\u00b7lich", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "APPRART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es mischet sich ein klares Blau", "tokens": ["Es", "mi\u00b7schet", "sich", "ein", "kla\u00b7res", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In diese Dunckelheit,", "tokens": ["In", "die\u00b7se", "Dun\u00b7ckel\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Dadurch vergn\u00fcgt so dann ein d\u00e4mmricht Licht", "tokens": ["Da\u00b7durch", "ver\u00b7gn\u00fcgt", "so", "dann", "ein", "d\u00e4mm\u00b7richt", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und tr\u00fcbe Klarheit das Gesicht,", "tokens": ["Und", "tr\u00fc\u00b7be", "Klar\u00b7heit", "das", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Indem die fette Fruchtbarkeit", "tokens": ["In\u00b7dem", "die", "fet\u00b7te", "Frucht\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "In diesem zarten Duft fast sichtbarlich zu sehn;", "tokens": ["In", "die\u00b7sem", "zar\u00b7ten", "Duft", "fast", "sicht\u00b7bar\u00b7lich", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und, weil zugleich die still und glatte Flut,", "tokens": ["Und", ",", "weil", "zu\u00b7gleich", "die", "still", "und", "glat\u00b7te", "Flut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADV", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die bey der stillen Luft, glatt, wie ein Spiegel, ruht,", "tokens": ["Die", "bey", "der", "stil\u00b7len", "Luft", ",", "glatt", ",", "wie", "ein", "Spie\u00b7gel", ",", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "$,", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die sanfte D\u00e4mmerung am Himmel, gleichfals sch\u00f6n,", "tokens": ["Die", "sanf\u00b7te", "D\u00e4m\u00b7me\u00b7rung", "am", "Him\u00b7mel", ",", "gleich\u00b7fals", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "In einem klaren Wiederschein,", "tokens": ["In", "ei\u00b7nem", "kla\u00b7ren", "Wie\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Uns deutlich zeigt; so ist ein stilles duftigs Wesen", "tokens": ["Uns", "deut\u00b7lich", "zeigt", ";", "so", "ist", "ein", "stil\u00b7les", "duf\u00b7tigs", "We\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN", "$.", "ADV", "VAFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So dann fast allgemein,", "tokens": ["So", "dann", "fast", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Und thut den Augen wol,", "tokens": ["Und", "thut", "den", "Au\u00b7gen", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Ja macht zugleich mit einer sanften Lust", "tokens": ["Ja", "macht", "zu\u00b7gleich", "mit", "ei\u00b7ner", "sanf\u00b7ten", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Blut, Hertz und Brust", "tokens": ["Blut", ",", "Hertz", "und", "Brust"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Von einer s\u00fcssen Anmuth voll.", "tokens": ["Von", "ei\u00b7ner", "s\u00fcs\u00b7sen", "An\u00b7muth", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}