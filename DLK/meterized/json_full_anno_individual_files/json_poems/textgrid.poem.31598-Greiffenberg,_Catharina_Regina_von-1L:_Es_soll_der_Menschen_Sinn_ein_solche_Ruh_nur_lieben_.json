{"textgrid.poem.31598": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es soll der Menschen Sinn ein solche Ruh nur lieben /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es soll der Menschen Sinn ein solche Ruh nur lieben /", "tokens": ["Es", "soll", "der", "Men\u00b7schen", "Sinn", "ein", "sol\u00b7che", "Ruh", "nur", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "ART", "PIAT", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Gott zu loben nur nicht ruht in Ewigkeit.", "tokens": ["die", "Gott", "zu", "lo\u00b7ben", "nur", "nicht", "ruht", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ADV", "PTKNEG", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es sey nun solches Thun im Frieden oder Streit /", "tokens": ["Es", "sey", "nun", "sol\u00b7ches", "Thun", "im", "Frie\u00b7den", "o\u00b7der", "Streit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "APPRART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so ist es Ruh genug / in Gottes Lob sich \u00fcben!", "tokens": ["so", "ist", "es", "Ruh", "ge\u00b7nug", "/", "in", "Got\u00b7tes", "Lob", "sich", "\u00fc\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "$(", "APPR", "NN", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das stille Wasser pflegt man schiffend zu betr\u00fcben:", "tokens": ["Das", "stil\u00b7le", "Was\u00b7ser", "pflegt", "man", "schif\u00b7fend", "zu", "be\u00b7tr\u00fc\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "das Ruder / das Gesetz / tr\u00fcbt die still-sicher Zeit;", "tokens": ["das", "Ru\u00b7der", "/", "das", "Ge\u00b7setz", "/", "tr\u00fcbt", "die", "still\u00b7si\u00b7cher", "Zeit", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "die nie-bereute Reu / f\u00fchrt zu des Hafens Freud.", "tokens": ["die", "nie\u00b7be\u00b7reu\u00b7te", "Reu", "/", "f\u00fchrt", "zu", "des", "Ha\u00b7fens", "Freud", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "$(", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "man mu\u00df zu Gottes Ehr' offtmals die Ruh verschieben!", "tokens": ["man", "mu\u00df", "zu", "Got\u00b7tes", "Ehr'", "offt\u00b7mals", "die", "Ruh", "ver\u00b7schie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.9": {"text": "Soll' an Beherrschung / Gott / der Wellen und der Wind", "tokens": ["Soll'", "an", "Be\u00b7herr\u00b7schung", "/", "Gott", "/", "der", "Wel\u00b7len", "und", "der", "Wind"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "$(", "NN", "$(", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "erzeigen seine Macht / mu\u00df er sie erst bewegen:", "tokens": ["er\u00b7zei\u00b7gen", "sei\u00b7ne", "Macht", "/", "mu\u00df", "er", "sie", "erst", "be\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So mustu / durch die Noht gelangen zu dem Ziel.", "tokens": ["So", "mus\u00b7tu", "/", "durch", "die", "Noht", "ge\u00b7lan\u00b7gen", "zu", "dem", "Ziel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$(", "APPR", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "wer keine Rast und Ruh in seinem Sinn empfind /", "tokens": ["wer", "kei\u00b7ne", "Rast", "und", "Ruh", "in", "sei\u00b7nem", "Sinn", "emp\u00b7find", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "bi\u00df sich des Friedens Krafft in ihm beginnt zu regen /", "tokens": ["bi\u00df", "sich", "des", "Frie\u00b7dens", "Krafft", "in", "ihm", "be\u00b7ginnt", "zu", "re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN