{"textgrid.poem.59775": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist die Welt, sei du, o Mensch, auch sch\u00f6n,", "tokens": ["Sch\u00f6n", "ist", "die", "Welt", ",", "sei", "du", ",", "o", "Mensch", ",", "auch", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "VAFIN", "PPER", "$,", "FM", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sei sch\u00f6n und gut, so wird dir's wohl ergehn.", "tokens": ["Sei", "sch\u00f6n", "und", "gut", ",", "so", "wird", "dir's", "wohl", "er\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "$,", "ADV", "VAFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bedenke: Fernst von Worten liegen Taten,", "tokens": ["Be\u00b7den\u00b7ke", ":", "Fernst", "von", "Wor\u00b7ten", "lie\u00b7gen", "Ta\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fern liegt der Ernte Lust vom Streun der Saaten:", "tokens": ["Fern", "liegt", "der", "Ern\u00b7te", "Lust", "vom", "Streun", "der", "Saa\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wer nich