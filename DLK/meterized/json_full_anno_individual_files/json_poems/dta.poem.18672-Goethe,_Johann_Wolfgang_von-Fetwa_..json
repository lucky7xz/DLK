{"dta.poem.18672": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Fetwa .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Hafis Dichterz\u00fcge sie bezeichnen", "tokens": ["Ha\u00b7fis", "Dich\u00b7ter\u00b7z\u00fc\u00b7ge", "sie", "be\u00b7zeich\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ausgemachte Wahrheit unausl\u00f6schlich;", "tokens": ["Aus\u00b7ge\u00b7mach\u00b7te", "Wahr\u00b7heit", "un\u00b7aus\u00b7l\u00f6schlich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber hie und da auch Kleinigkeiten", "tokens": ["A\u00b7ber", "hie", "und", "da", "auch", "Klei\u00b7nig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "ADV", "ADV", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ausserhalb der Gr\u00e4nze des Gesetzes.", "tokens": ["Aus\u00b7ser\u00b7halb", "der", "Gr\u00e4n\u00b7ze", "des", "Ge\u00b7set\u00b7zes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Willst du sicher gehn, so musst du wissen", "tokens": ["Willst", "du", "si\u00b7cher", "gehn", ",", "so", "musst", "du", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schlangengift und Theriak zu sondern \u2014", "tokens": ["Schlan\u00b7gen\u00b7gift", "und", "The\u00b7ri\u00b7ak", "zu", "son\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "KON", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Doch der reinen Wollust edler Handlung", "tokens": ["Doch", "der", "rei\u00b7nen", "Wol\u00b7lust", "ed\u00b7ler", "Hand\u00b7lung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Sich mit frohem Muth zu \u00fcberlassen,", "tokens": ["Sich", "mit", "fro\u00b7hem", "Muth", "zu", "\u00fc\u00b7ber\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Und vor solcher, der nur ew\u2019ge Pein folgt,", "tokens": ["Und", "vor", "sol\u00b7cher", ",", "der", "nur", "ew'\u00b7ge", "Pein", "folgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "$,", "PRELS", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Mit besonnenem Sinn sich zu verwahren,", "tokens": ["Mit", "be\u00b7son\u00b7ne\u00b7nem", "Sinn", "sich", "zu", "ver\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Ist gewiss das beste um nicht zu fehlen.", "tokens": ["Ist", "ge\u00b7wiss", "das", "bes\u00b7te", "um", "nicht", "zu", "feh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "APPR", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Dieses schrieb der arme ", "tokens": ["Die\u00b7ses", "schrieb", "der", "ar\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Gott verzeih ihm seine S\u00fcnden alle.", "tokens": ["Gott", "ver\u00b7zeih", "ihm", "sei\u00b7ne", "S\u00fcn\u00b7den", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "PIAT", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}