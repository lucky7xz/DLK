{"textgrid.poem.28739": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo eure g\u00f6ldne Frucht, Hesperides, ietzt stehet,", "tokens": ["Wo", "eu\u00b7re", "g\u00f6ld\u00b7ne", "Frucht", ",", "Hes\u00b7pe\u00b7ri\u00b7des", ",", "ietzt", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,", "NE", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo wer, Alcinous, in deinen Garten gehet,", "tokens": ["Wo", "wer", ",", "Al\u00b7ci\u00b7nous", ",", "in", "dei\u00b7nen", "Gar\u00b7ten", "ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWS", "$,", "NE", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo Argo und sein Held nach Gold in Colchos schifft,", "tokens": ["Wo", "Ar\u00b7go", "und", "sein", "Held", "nach", "Gold", "in", "Col\u00b7chos", "schifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "PPOSAT", "NN", "APPR", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wei\u00df keinen ich, der ietzt dahin die Wege trifft.", "tokens": ["Wei\u00df", "kei\u00b7nen", "ich", ",", "der", "ietzt", "da\u00b7hin", "die", "We\u00b7ge", "trifft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPER", "$,", "PRELS", "ADV", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was Pelops, Attalus, was Cr\u0153sus schwangre Kasten", "tokens": ["Was", "Pe\u00b7lops", ",", "At\u00b7ta\u00b7lus", ",", "was", "Cr\u0153\u00b7sus", "schwang\u00b7re", "Kas\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NE", "$,", "NE", "$,", "PRELS", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von Golde, Geld und Gut f\u00fcr Zeiten in sich fasten,", "tokens": ["Von", "Gol\u00b7de", ",", "Geld", "und", "Gut", "f\u00fcr", "Zei\u00b7ten", "in", "sich", "fas\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "ADJD", "APPR", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "N\u00fctzt nur so viel, da\u00df der, der gar zu viel drauff denckt,", "tokens": ["N\u00fctzt", "nur", "so", "viel", ",", "da\u00df", "der", ",", "der", "gar", "zu", "viel", "drauff", "denckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "$,", "KOUS", "ART", "$,", "PRELS", "ADV", "PTKA", "PIS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Leib gemein an Baum, die Seel an Nagel henckt.", "tokens": ["Den", "Leib", "ge\u00b7mein", "an", "Baum", ",", "die", "Seel", "an", "Na\u00b7gel", "henckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NE", "$,", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "De\u00df Tagus reicher Sand, Pactolus g\u00f6ldnes flissen", "tokens": ["De\u00df", "Ta\u00b7gus", "rei\u00b7cher", "Sand", ",", "Pac\u00b7to\u00b7lus", "g\u00f6ld\u00b7nes", "flis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "NE", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bringt mehres uns nicht ein, als da\u00df davon wir wissen;", "tokens": ["Bringt", "meh\u00b7res", "uns", "nicht", "ein", ",", "als", "da\u00df", "da\u00b7von", "wir", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "PTKNEG", "PTKVZ", "$,", "KOKOM", "KOUS", "PAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was sonst die reiche Welt in ihrem Busem h\u00e4lt,", "tokens": ["Was", "sonst", "die", "rei\u00b7che", "Welt", "in", "ih\u00b7rem", "Bu\u00b7sem", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist irrdisch-schweres Gut, k\u00fcmmt, bleibt, geht mit der Welt.", "tokens": ["Ist", "irr\u00b7dischschwe\u00b7res", "Gut", ",", "k\u00fcmmt", ",", "bleibt", ",", "geht", "mit", "der", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Ein iedes ist doch Wind, Rauch, Schaten, Schlaf und Tr\u00e4ume;", "tokens": ["Ein", "ie\u00b7des", "ist", "doch", "Wind", ",", "Rauch", ",", "Scha\u00b7ten", ",", "Schlaf", "und", "Tr\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADV", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Zeit reist alles hin, sie leidet keine Z\u00e4ume;", "tokens": ["Die", "Zeit", "reist", "al\u00b7les", "hin", ",", "sie", "lei\u00b7det", "kei\u00b7ne", "Z\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKVZ", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was kummen war, das geht; was ist, das bleibet nicht;", "tokens": ["Was", "kum\u00b7men", "war", ",", "das", "geht", ";", "was", "ist", ",", "das", "blei\u00b7bet", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VAFIN", "$,", "PDS", "VVFIN", "$.", "PWS", "VAFIN", "$,", "PDS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Abend l\u00e4ugnet offt, was vor der Tag verspricht.", "tokens": ["Der", "A\u00b7bend", "l\u00e4ug\u00b7net", "offt", ",", "was", "vor", "der", "Tag", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drum weg, nur immer weg mit diesen Nichtigkeiten!", "tokens": ["Drum", "weg", ",", "nur", "im\u00b7mer", "weg", "mit", "die\u00b7sen", "Nich\u00b7tig\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "ADV", "ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mit diesem armen Gut und diesen runden Zeiten", "tokens": ["Mit", "die\u00b7sem", "ar\u00b7men", "Gut", "und", "die\u00b7sen", "run\u00b7den", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "De\u00df taumelnden Gel\u00fccks! nur weg, du gelber Koth,", "tokens": ["De\u00df", "tau\u00b7meln\u00b7den", "Ge\u00b7l\u00fccks", "!", "nur", "weg", ",", "du", "gel\u00b7ber", "Koth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der alle Welt befleckt, erwecket alle Noth!", "tokens": ["Der", "al\u00b7le", "Welt", "be\u00b7fleckt", ",", "er\u00b7we\u00b7cket", "al\u00b7le", "Noth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was be\u00dfres ist mir kund, war werthers ist zu finden,", "tokens": ["Was", "be\u00df\u00b7res", "ist", "mir", "kund", ",", "war", "wert\u00b7hers", "ist", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAFIN", "PPER", "PTKVZ", "$,", "VAFIN", "ADV", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Darauff sich meine Seel in N\u00f6then starck kan gr\u00fcnden,", "tokens": ["Dar\u00b7auff", "sich", "mei\u00b7ne", "Seel", "in", "N\u00f6\u00b7then", "starck", "kan", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dem alle Welt nicht gleicht, f\u00fcr dem, was gilt, nicht gilt,", "tokens": ["Dem", "al\u00b7le", "Welt", "nicht", "gleicht", ",", "f\u00fcr", "dem", ",", "was", "gilt", ",", "nicht", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VVFIN", "$,", "APPR", "ART", "$,", "PWS", "VVFIN", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df hier die Erd erh\u00e4lt und dort den Himmel v\u00f6llt.", "tokens": ["Da\u00df", "hier", "die", "Erd", "er\u00b7h\u00e4lt", "und", "dort", "den", "Him\u00b7mel", "v\u00f6llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "KON", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich wei\u00df den edlen Grund, ich wei\u00df den theuren Garten,", "tokens": ["Ich", "wei\u00df", "den", "ed\u00b7len", "Grund", ",", "ich", "wei\u00df", "den", "theu\u00b7ren", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich wei\u00df die g\u00f6ldne Frucht, ich wei\u00df die reichen Fahrten,", "tokens": ["Ich", "wei\u00df", "die", "g\u00f6ld\u00b7ne", "Frucht", ",", "ich", "wei\u00df", "die", "rei\u00b7chen", "Fahr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da was man darff, man hollt; ich wei\u00df das sch\u00f6ne Geld,", "tokens": ["Da", "was", "man", "darff", ",", "man", "hollt", ";", "ich", "wei\u00df", "das", "sch\u00f6\u00b7ne", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PIS", "VMFIN", "$,", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das unsren Leib f\u00fcr Noth, f\u00fcr Tod die Seel erh\u00e4lt.", "tokens": ["Das", "un\u00b7sren", "Leib", "f\u00fcr", "Noth", ",", "f\u00fcr", "Tod", "die", "Seel", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich wei\u00df da\u00df frische Quall, drau\u00df g\u00f6ldne Str\u00f6me flissen,", "tokens": ["Ich", "wei\u00df", "da\u00df", "fri\u00b7sche", "Quall", ",", "drau\u00df", "g\u00f6ld\u00b7ne", "Str\u00f6\u00b7me", "flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ADJA", "NN", "$,", "PAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die unsren Sinn und Hertz mit Freud und Trost begissen;", "tokens": ["Die", "un\u00b7sren", "Sinn", "und", "Hertz", "mit", "Freud", "und", "Trost", "be\u00b7gis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wei\u00df das reine Gold, dem Zeit nicht schaden thut,", "tokens": ["Ich", "wei\u00df", "das", "rei\u00b7ne", "Gold", ",", "dem", "Zeit", "nicht", "scha\u00b7den", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df schmeltzen auch nicht wird de\u00df letzten Tages Glut.", "tokens": ["Da\u00df", "schmelt\u00b7zen", "auch", "nicht", "wird", "de\u00df", "letz\u00b7ten", "Ta\u00b7ges", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "PTKNEG", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dein Wort, dein Wort, o HErr! gilt mir f\u00fcr alle Sch\u00e4tze;", "tokens": ["Dein", "Wort", ",", "dein", "Wort", ",", "o", "Herr", "!", "gilt", "mir", "f\u00fcr", "al\u00b7le", "Sch\u00e4t\u00b7ze", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dein Wort, HErr, ist das Gold, damit ich mich ergetze;", "tokens": ["Dein", "Wort", ",", "Herr", ",", "ist", "das", "Gold", ",", "da\u00b7mit", "ich", "mich", "er\u00b7get\u00b7ze", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Dein Wort, HErr, ist mein Gut, drauff meine Seele traut,", "tokens": ["Dein", "Wort", ",", "Herr", ",", "ist", "mein", "Gut", ",", "drauff", "mei\u00b7ne", "See\u00b7le", "traut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Drauff sich mein Mut gesetzt, drauff sich mein Leben baut.", "tokens": ["Drauff", "sich", "mein", "Mut", "ge\u00b7setzt", ",", "drauff", "sich", "mein", "Le\u00b7ben", "baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PPOSAT", "NN", "VVPP", "$,", "PAV", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.37": {"text": "Da\u00df ich, der ich bin, bin, und da\u00df ich nachmals werde", "tokens": ["Da\u00df", "ich", ",", "der", "ich", "bin", ",", "bin", ",", "und", "da\u00df", "ich", "nach\u00b7mals", "wer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "VAFIN", "$,", "VAFIN", "$,", "KON", "KOUS", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "De\u00df Lebens durch den Tod, de\u00df Himmels von der Erde,", "tokens": ["De\u00df", "Le\u00b7bens", "durch", "den", "Tod", ",", "de\u00df", "Him\u00b7mels", "von", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Macht alles, HErr, dein Wort. Dein Wort und deine Hold", "tokens": ["Macht", "al\u00b7les", ",", "Herr", ",", "dein", "Wort", ".", "Dein", "Wort", "und", "dei\u00b7ne", "Hold"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PIS", "$,", "NN", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Deckt meine M\u00e4ngel zu, vertilget meine Schuld;", "tokens": ["Deckt", "mei\u00b7ne", "M\u00e4n\u00b7gel", "zu", ",", "ver\u00b7til\u00b7get", "mei\u00b7ne", "Schuld", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Dein Wort ist meine Krafft: ich darff nicht unter liegen;", "tokens": ["Dein", "Wort", "ist", "mei\u00b7ne", "Krafft", ":", "ich", "darff", "nicht", "un\u00b7ter", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ich darff mich keinem Gl\u00fcck an seine F\u00fcsse biegen.", "tokens": ["Ich", "darff", "mich", "kei\u00b7nem", "Gl\u00fcck", "an", "sei\u00b7ne", "F\u00fcs\u00b7se", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Dein Wort is meine Macht, Helm, Harnisch, Schwerdt und Schild,", "tokens": ["Dein", "Wort", "is", "mei\u00b7ne", "Macht", ",", "Helm", ",", "Har\u00b7nisch", ",", "Schwerdt", "und", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "FM", "PPOSAT", "NN", "$,", "NN", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Darwider Teuffel, Welt, Tod, S\u00fcnde, Fleisch nicht gilt;", "tokens": ["Dar\u00b7wi\u00b7der", "Teuf\u00b7fel", ",", "Welt", ",", "Tod", ",", "S\u00fcn\u00b7de", ",", "Fleisch", "nicht", "gilt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Dein Wort ist meine Freud auch mitten in dem Leiden;", "tokens": ["Dein", "Wort", "ist", "mei\u00b7ne", "Freud", "auch", "mit\u00b7ten", "in", "dem", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Dein Wort ist auch mein Heil, wann Leib und Seele scheiden;", "tokens": ["Dein", "Wort", "ist", "auch", "mein", "Heil", ",", "wann", "Leib", "und", "See\u00b7le", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Dein Wort nimmt mich der Welt und bringt mich au\u00df der Noth,", "tokens": ["Dein", "Wort", "nimmt", "mich", "der", "Welt", "und", "bringt", "mich", "au\u00df", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Schenckt mir die Ewigkeit, gibt mir dich, dir mich, Gott!", "tokens": ["Schenckt", "mir", "die", "E\u00b7wig\u00b7keit", ",", "gibt", "mir", "dich", ",", "dir", "mich", ",", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "PPER", "$,", "PPER", "PPER", "$,", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wo eure g\u00f6ldne Frucht, Hesperides, ietzt stehet,", "tokens": ["Wo", "eu\u00b7re", "g\u00f6ld\u00b7ne", "Frucht", ",", "Hes\u00b7pe\u00b7ri\u00b7des", ",", "ietzt", "ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "$,", "NE", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo wer, Alcinous, in deinen Garten gehet,", "tokens": ["Wo", "wer", ",", "Al\u00b7ci\u00b7nous", ",", "in", "dei\u00b7nen", "Gar\u00b7ten", "ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PWS", "$,", "NE", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo Argo und sein Held nach Gold in Colchos schifft,", "tokens": ["Wo", "Ar\u00b7go", "und", "sein", "Held", "nach", "Gold", "in", "Col\u00b7chos", "schifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "PPOSAT", "NN", "APPR", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wei\u00df keinen ich, der ietzt dahin die Wege trifft.", "tokens": ["Wei\u00df", "kei\u00b7nen", "ich", ",", "der", "ietzt", "da\u00b7hin", "die", "We\u00b7ge", "trifft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "PPER", "$,", "PRELS", "ADV", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was Pelops, Attalus, was Cr\u0153sus schwangre Kasten", "tokens": ["Was", "Pe\u00b7lops", ",", "At\u00b7ta\u00b7lus", ",", "was", "Cr\u0153\u00b7sus", "schwang\u00b7re", "Kas\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "NE", "$,", "NE", "$,", "PRELS", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Von Golde, Geld und Gut f\u00fcr Zeiten in sich fasten,", "tokens": ["Von", "Gol\u00b7de", ",", "Geld", "und", "Gut", "f\u00fcr", "Zei\u00b7ten", "in", "sich", "fas\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "ADJD", "APPR", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "N\u00fctzt nur so viel, da\u00df der, der gar zu viel drauff denckt,", "tokens": ["N\u00fctzt", "nur", "so", "viel", ",", "da\u00df", "der", ",", "der", "gar", "zu", "viel", "drauff", "denckt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADV", "$,", "KOUS", "ART", "$,", "PRELS", "ADV", "PTKA", "PIS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Leib gemein an Baum, die Seel an Nagel henckt.", "tokens": ["Den", "Leib", "ge\u00b7mein", "an", "Baum", ",", "die", "Seel", "an", "Na\u00b7gel", "henckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NE", "$,", "ART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "De\u00df Tagus reicher Sand, Pactolus g\u00f6ldnes flissen", "tokens": ["De\u00df", "Ta\u00b7gus", "rei\u00b7cher", "Sand", ",", "Pac\u00b7to\u00b7lus", "g\u00f6ld\u00b7nes", "flis\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "NE", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bringt mehres uns nicht ein, als da\u00df davon wir wissen;", "tokens": ["Bringt", "meh\u00b7res", "uns", "nicht", "ein", ",", "als", "da\u00df", "da\u00b7von", "wir", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "PTKNEG", "PTKVZ", "$,", "KOKOM", "KOUS", "PAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was sonst die reiche Welt in ihrem Busem h\u00e4lt,", "tokens": ["Was", "sonst", "die", "rei\u00b7che", "Welt", "in", "ih\u00b7rem", "Bu\u00b7sem", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ist irrdisch-schweres Gut, k\u00fcmmt, bleibt, geht mit der Welt.", "tokens": ["Ist", "irr\u00b7dischschwe\u00b7res", "Gut", ",", "k\u00fcmmt", ",", "bleibt", ",", "geht", "mit", "der", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Ein iedes ist doch Wind, Rauch, Schaten, Schlaf und Tr\u00e4ume;", "tokens": ["Ein", "ie\u00b7des", "ist", "doch", "Wind", ",", "Rauch", ",", "Scha\u00b7ten", ",", "Schlaf", "und", "Tr\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADV", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Zeit reist alles hin, sie leidet keine Z\u00e4ume;", "tokens": ["Die", "Zeit", "reist", "al\u00b7les", "hin", ",", "sie", "lei\u00b7det", "kei\u00b7ne", "Z\u00e4u\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKVZ", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Was kummen war, das geht; was ist, das bleibet nicht;", "tokens": ["Was", "kum\u00b7men", "war", ",", "das", "geht", ";", "was", "ist", ",", "das", "blei\u00b7bet", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VAFIN", "$,", "PDS", "VVFIN", "$.", "PWS", "VAFIN", "$,", "PDS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Abend l\u00e4ugnet offt, was vor der Tag verspricht.", "tokens": ["Der", "A\u00b7bend", "l\u00e4ug\u00b7net", "offt", ",", "was", "vor", "der", "Tag", "ver\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drum weg, nur immer weg mit diesen Nichtigkeiten!", "tokens": ["Drum", "weg", ",", "nur", "im\u00b7mer", "weg", "mit", "die\u00b7sen", "Nich\u00b7tig\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "ADV", "ADV", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mit diesem armen Gut und diesen runden Zeiten", "tokens": ["Mit", "die\u00b7sem", "ar\u00b7men", "Gut", "und", "die\u00b7sen", "run\u00b7den", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "De\u00df taumelnden Gel\u00fccks! nur weg, du gelber Koth,", "tokens": ["De\u00df", "tau\u00b7meln\u00b7den", "Ge\u00b7l\u00fccks", "!", "nur", "weg", ",", "du", "gel\u00b7ber", "Koth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der alle Welt befleckt, erwecket alle Noth!", "tokens": ["Der", "al\u00b7le", "Welt", "be\u00b7fleckt", ",", "er\u00b7we\u00b7cket", "al\u00b7le", "Noth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Was be\u00dfres ist mir kund, war werthers ist zu finden,", "tokens": ["Was", "be\u00df\u00b7res", "ist", "mir", "kund", ",", "war", "wert\u00b7hers", "ist", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAFIN", "PPER", "PTKVZ", "$,", "VAFIN", "ADV", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Darauff sich meine Seel in N\u00f6then starck kan gr\u00fcnden,", "tokens": ["Dar\u00b7auff", "sich", "mei\u00b7ne", "Seel", "in", "N\u00f6\u00b7then", "starck", "kan", "gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dem alle Welt nicht gleicht, f\u00fcr dem, was gilt, nicht gilt,", "tokens": ["Dem", "al\u00b7le", "Welt", "nicht", "gleicht", ",", "f\u00fcr", "dem", ",", "was", "gilt", ",", "nicht", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VVFIN", "$,", "APPR", "ART", "$,", "PWS", "VVFIN", "$,", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df hier die Erd erh\u00e4lt und dort den Himmel v\u00f6llt.", "tokens": ["Da\u00df", "hier", "die", "Erd", "er\u00b7h\u00e4lt", "und", "dort", "den", "Him\u00b7mel", "v\u00f6llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "KON", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich wei\u00df den edlen Grund, ich wei\u00df den theuren Garten,", "tokens": ["Ich", "wei\u00df", "den", "ed\u00b7len", "Grund", ",", "ich", "wei\u00df", "den", "theu\u00b7ren", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ich wei\u00df die g\u00f6ldne Frucht, ich wei\u00df die reichen Fahrten,", "tokens": ["Ich", "wei\u00df", "die", "g\u00f6ld\u00b7ne", "Frucht", ",", "ich", "wei\u00df", "die", "rei\u00b7chen", "Fahr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da was man darff, man hollt; ich wei\u00df das sch\u00f6ne Geld,", "tokens": ["Da", "was", "man", "darff", ",", "man", "hollt", ";", "ich", "wei\u00df", "das", "sch\u00f6\u00b7ne", "Geld", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PIS", "VMFIN", "$,", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das unsren Leib f\u00fcr Noth, f\u00fcr Tod die Seel erh\u00e4lt.", "tokens": ["Das", "un\u00b7sren", "Leib", "f\u00fcr", "Noth", ",", "f\u00fcr", "Tod", "die", "Seel", "er\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich wei\u00df da\u00df frische Quall, drau\u00df g\u00f6ldne Str\u00f6me flissen,", "tokens": ["Ich", "wei\u00df", "da\u00df", "fri\u00b7sche", "Quall", ",", "drau\u00df", "g\u00f6ld\u00b7ne", "Str\u00f6\u00b7me", "flis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ADJA", "NN", "$,", "PAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Die unsren Sinn und Hertz mit Freud und Trost begissen;", "tokens": ["Die", "un\u00b7sren", "Sinn", "und", "Hertz", "mit", "Freud", "und", "Trost", "be\u00b7gis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wei\u00df das reine Gold, dem Zeit nicht schaden thut,", "tokens": ["Ich", "wei\u00df", "das", "rei\u00b7ne", "Gold", ",", "dem", "Zeit", "nicht", "scha\u00b7den", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKNEG", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df schmeltzen auch nicht wird de\u00df letzten Tages Glut.", "tokens": ["Da\u00df", "schmelt\u00b7zen", "auch", "nicht", "wird", "de\u00df", "letz\u00b7ten", "Ta\u00b7ges", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADV", "PTKNEG", "VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dein Wort, dein Wort, o HErr! gilt mir f\u00fcr alle Sch\u00e4tze;", "tokens": ["Dein", "Wort", ",", "dein", "Wort", ",", "o", "Herr", "!", "gilt", "mir", "f\u00fcr", "al\u00b7le", "Sch\u00e4t\u00b7ze", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "FM", "NN", "$.", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Dein Wort, HErr, ist das Gold, damit ich mich ergetze;", "tokens": ["Dein", "Wort", ",", "Herr", ",", "ist", "das", "Gold", ",", "da\u00b7mit", "ich", "mich", "er\u00b7get\u00b7ze", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "ART", "NN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Dein Wort, HErr, ist mein Gut, drauff meine Seele traut,", "tokens": ["Dein", "Wort", ",", "Herr", ",", "ist", "mein", "Gut", ",", "drauff", "mei\u00b7ne", "See\u00b7le", "traut", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,", "PAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Drauff sich mein Mut gesetzt, drauff sich mein Leben baut.", "tokens": ["Drauff", "sich", "mein", "Mut", "ge\u00b7setzt", ",", "drauff", "sich", "mein", "Le\u00b7ben", "baut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PPOSAT", "NN", "VVPP", "$,", "PAV", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.37": {"text": "Da\u00df ich, der ich bin, bin, und da\u00df ich nachmals werde", "tokens": ["Da\u00df", "ich", ",", "der", "ich", "bin", ",", "bin", ",", "und", "da\u00df", "ich", "nach\u00b7mals", "wer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "VAFIN", "$,", "VAFIN", "$,", "KON", "KOUS", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "De\u00df Lebens durch den Tod, de\u00df Himmels von der Erde,", "tokens": ["De\u00df", "Le\u00b7bens", "durch", "den", "Tod", ",", "de\u00df", "Him\u00b7mels", "von", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Macht alles, HErr, dein Wort. Dein Wort und deine Hold", "tokens": ["Macht", "al\u00b7les", ",", "Herr", ",", "dein", "Wort", ".", "Dein", "Wort", "und", "dei\u00b7ne", "Hold"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PIS", "$,", "NN", "$,", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Deckt meine M\u00e4ngel zu, vertilget meine Schuld;", "tokens": ["Deckt", "mei\u00b7ne", "M\u00e4n\u00b7gel", "zu", ",", "ver\u00b7til\u00b7get", "mei\u00b7ne", "Schuld", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Dein Wort ist meine Krafft: ich darff nicht unter liegen;", "tokens": ["Dein", "Wort", "ist", "mei\u00b7ne", "Krafft", ":", "ich", "darff", "nicht", "un\u00b7ter", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ich darff mich keinem Gl\u00fcck an seine F\u00fcsse biegen.", "tokens": ["Ich", "darff", "mich", "kei\u00b7nem", "Gl\u00fcck", "an", "sei\u00b7ne", "F\u00fcs\u00b7se", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Dein Wort is meine Macht, Helm, Harnisch, Schwerdt und Schild,", "tokens": ["Dein", "Wort", "is", "mei\u00b7ne", "Macht", ",", "Helm", ",", "Har\u00b7nisch", ",", "Schwerdt", "und", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "FM", "PPOSAT", "NN", "$,", "NN", "$,", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Darwider Teuffel, Welt, Tod, S\u00fcnde, Fleisch nicht gilt;", "tokens": ["Dar\u00b7wi\u00b7der", "Teuf\u00b7fel", ",", "Welt", ",", "Tod", ",", "S\u00fcn\u00b7de", ",", "Fleisch", "nicht", "gilt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Dein Wort ist meine Freud auch mitten in dem Leiden;", "tokens": ["Dein", "Wort", "ist", "mei\u00b7ne", "Freud", "auch", "mit\u00b7ten", "in", "dem", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Dein Wort ist auch mein Heil, wann Leib und Seele scheiden;", "tokens": ["Dein", "Wort", "ist", "auch", "mein", "Heil", ",", "wann", "Leib", "und", "See\u00b7le", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Dein Wort nimmt mich der Welt und bringt mich au\u00df der Noth,", "tokens": ["Dein", "Wort", "nimmt", "mich", "der", "Welt", "und", "bringt", "mich", "au\u00df", "der", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Schenckt mir die Ewigkeit, gibt mir dich, dir mich, Gott!", "tokens": ["Schenckt", "mir", "die", "E\u00b7wig\u00b7keit", ",", "gibt", "mir", "dich", ",", "dir", "mich", ",", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "PPER", "$,", "PPER", "PPER", "$,", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}