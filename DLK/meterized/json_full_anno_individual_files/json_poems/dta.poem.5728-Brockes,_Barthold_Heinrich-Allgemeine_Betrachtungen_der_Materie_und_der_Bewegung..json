{"dta.poem.5728": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Allgemeine Betrachtungen der Materie  \n und der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem sie von dem Volck die Schwachheit angesehen,", "tokens": ["In\u00b7dem", "sie", "von", "dem", "Volck", "die", "Schwach\u00b7heit", "an\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nach derselben sich gerichtet:", "tokens": ["Und", "nach", "der\u00b7sel\u00b7ben", "sich", "ge\u00b7rich\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So haben sie zum Schmuck der Weisheit viel erdichtet,", "tokens": ["So", "ha\u00b7ben", "sie", "zum", "Schmuck", "der", "Weis\u00b7heit", "viel", "er\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit derselben Glanz und ew\u2019ge Sch\u00f6nheit, allen", "tokens": ["Da\u00b7mit", "der\u00b7sel\u00b7ben", "Glanz", "und", "ew'\u00b7ge", "Sch\u00f6n\u00b7heit", ",", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PDAT", "NN", "KON", "ADJA", "NN", "$,", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Noch m\u00f6cht\u2019 um destomehr gefallen.", "tokens": ["Noch", "m\u00f6cht'", "um", "des\u00b7to\u00b7mehr", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}