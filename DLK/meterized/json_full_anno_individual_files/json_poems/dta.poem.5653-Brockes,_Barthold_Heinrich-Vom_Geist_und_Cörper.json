{"dta.poem.5653": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In so weit ich ein Geist; versteh'-bejah-verneine,", "tokens": ["In", "so", "weit", "ich", "ein", "Geist", ";", "ver\u00b7steh'\u00b7be\u00b7jah\u00b7ver\u00b7nei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "PPER", "ART", "NN", "$.", "ADJA", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Lieb\u2019, ha\u00df\u2019, und zweifel\u2019 ich:", "tokens": ["Lieb'", ",", "ha\u00df'", ",", "und", "zwei\u00b7fel'", "ich", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es reuet mich,", "tokens": ["Es", "reu\u00b7et", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Ich f\u00fcrcht\u2019, ich hoff\u2019, und meine.", "tokens": ["Ich", "f\u00fcrcht'", ",", "ich", "hoff'", ",", "und", "mei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "KON", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hier ist nichts c\u00f6rperlichs: auch nicht einmahl die Spur.", "tokens": ["Hier", "ist", "nichts", "c\u00f6r\u00b7per\u00b7lichs", ":", "auch", "nicht", "ein\u00b7mahl", "die", "Spur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PIS", "$.", "ADV", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Geist ist weder spitz, noch warm, noch bunt,", "tokens": ["Der", "Geist", "ist", "we\u00b7der", "spitz", ",", "noch", "warm", ",", "noch", "bunt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KON", "NE", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er ist nicht l\u00e4nglich, und nicht rund;", "tokens": ["Er", "ist", "nicht", "l\u00e4ng\u00b7lich", ",", "und", "nicht", "rund", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch ist mit ihm vereint, ein\u2019 andere Natur:", "tokens": ["Doch", "ist", "mit", "ihm", "ver\u00b7eint", ",", "ein'", "an\u00b7de\u00b7re", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein C\u00f6rper, so sich theilen kan und regen,", "tokens": ["Ein", "C\u00f6r\u00b7per", ",", "so", "sich", "thei\u00b7len", "kan", "und", "re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PRF", "VVINF", "VMFIN", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und dessen sich ver\u00e4ndernde Figur", "tokens": ["Und", "des\u00b7sen", "sich", "ver\u00b7\u00e4n\u00b7dern\u00b7de", "Fi\u00b7gur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "PRF", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wir mercken k\u00f6nnen und erwegen.", "tokens": ["Wir", "mer\u00b7cken", "k\u00f6n\u00b7nen", "und", "er\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}