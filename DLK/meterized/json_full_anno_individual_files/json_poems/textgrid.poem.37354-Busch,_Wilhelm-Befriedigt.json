{"textgrid.poem.37354": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Befriedigt", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gehorchen wird jeder mit Genu\u00df", "tokens": ["Ge\u00b7hor\u00b7chen", "wird", "je\u00b7der", "mit", "Ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIS", "APPR", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Frauen, den hochgesch\u00e4tzten,", "tokens": ["Den", "Frau\u00b7en", ",", "den", "hoch\u00b7ge\u00b7sch\u00e4tz\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hingegen machen uns meist Verdru\u00df", "tokens": ["Hin\u00b7ge\u00b7gen", "ma\u00b7chen", "uns", "meist", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Die sonstigen Vorgesetzten.", "tokens": ["Die", "sons\u00b7ti\u00b7gen", "Vor\u00b7ge\u00b7setz\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Nur wenn ein kleines Mi\u00dfgeschick", "tokens": ["Nur", "wenn", "ein", "klei\u00b7nes", "Mi\u00df\u00b7ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Betrifft den Treiber und Leiter,", "tokens": ["Be\u00b7tr\u00b7ifft", "den", "Trei\u00b7ber", "und", "Lei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Dann f\u00fchlt man f\u00fcr den Augenblick", "tokens": ["Dann", "f\u00fchlt", "man", "f\u00fcr", "den", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich sehr befriedigt und heiter.", "tokens": ["Sich", "sehr", "be\u00b7frie\u00b7digt", "und", "hei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Als neulich am Sonntag der Herr Pastor", "tokens": ["Als", "neu\u00b7lich", "am", "Sonn\u00b7tag", "der", "Herr", "Pas\u00b7tor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ART", "NN", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Eine peinliche Pause machte,", "tokens": ["Ei\u00b7ne", "pein\u00b7li\u00b7che", "Pau\u00b7se", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Weil er den Faden der Rede verlor,", "tokens": ["Weil", "er", "den", "Fa\u00b7den", "der", "Re\u00b7de", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Da duckt sich der K\u00fcster und lachte.", "tokens": ["Da", "duckt", "sich", "der", "K\u00fcs\u00b7ter", "und", "lach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF