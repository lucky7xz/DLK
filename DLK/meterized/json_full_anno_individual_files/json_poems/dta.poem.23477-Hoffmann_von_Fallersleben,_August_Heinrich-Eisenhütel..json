{"dta.poem.23477": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eisenh\u00fctel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nun ist es Zeit zu wachen,", "tokens": ["Nun", "ist", "es", "Zeit", "zu", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eh' Deutschlands Freiheit stirbt", "tokens": ["Eh'", "Deutschlands", "Frei\u00b7heit", "stirbt"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und in dem weiten Rachen", "tokens": ["Und", "in", "dem", "wei\u00b7ten", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Krokodils verdirbt.", "tokens": ["Des", "Kro\u00b7ko\u00b7dils", "ver\u00b7dirbt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Herbei, da\u00df man die Kr\u00f6ten", "tokens": ["Her\u00b7bei", ",", "da\u00df", "man", "die", "Kr\u00f6\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die unsern Rhein betreten", "tokens": ["Die", "un\u00b7sern", "Rhein", "be\u00b7tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit aller Macht zur\u00fccke", "tokens": ["Mit", "al\u00b7ler", "Macht", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zur Son' und Seine schicke!", "tokens": ["Zur", "Son'", "und", "Sei\u00b7ne", "schi\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Feind braucht Stahl und Eisen,", "tokens": ["Der", "Feind", "braucht", "Stahl", "und", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wendt Stahl und Silber an,", "tokens": ["Wendt", "Stahl", "und", "Sil\u00b7ber", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der deutschen Welt zu weisen", "tokens": ["Der", "deut\u00b7schen", "Welt", "zu", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was List und Hochmuth kann.", "tokens": ["Was", "List", "und", "Hoch\u00b7muth", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00dft euch das Gold in H\u00e4nden", "tokens": ["La\u00dft", "euch", "das", "Gold", "in", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Augen nicht verblenden,", "tokens": ["Die", "Au\u00b7gen", "nicht", "ver\u00b7blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Damit euch hinterm R\u00fccken", "tokens": ["Da\u00b7mit", "euch", "hin\u00b7term", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Fessel nicht bestricken.", "tokens": ["Die", "Fes\u00b7sel", "nicht", "be\u00b7stri\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00dft Lerch' und Falken fliegen,", "tokens": ["La\u00dft", "Ler\u00b7ch'", "und", "Fal\u00b7ken", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Setzt alle Kr\u00e4fte bei,", "tokens": ["Setzt", "al\u00b7le", "Kr\u00e4f\u00b7te", "bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihnen zu besiegen", "tokens": ["Mit", "ih\u00b7nen", "zu", "be\u00b7sie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Hahnes Prahlerei!", "tokens": ["Des", "Hah\u00b7nes", "Prah\u00b7le\u00b7rei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er prangt mit euren Federn:", "tokens": ["Er", "prangt", "mit", "eu\u00b7ren", "Fe\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Drum m\u00fc\u00dft ihr ihn ent\u00e4dern,", "tokens": ["Drum", "m\u00fc\u00dft", "ihr", "ihn", "en\u00b7t\u00e4\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Jeder sich bem\u00fchen", "tokens": ["Und", "Je\u00b7der", "sich", "be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Seine wegzuziehen.", "tokens": ["Das", "Sei\u00b7ne", "weg\u00b7zu\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wollt ihr euch unterwinden", "tokens": ["Wollt", "ihr", "euch", "un\u00b7ter\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu thun was sich geb\u00fchrt,", "tokens": ["Zu", "thun", "was", "sich", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Hermann wird sich finden,", "tokens": ["Ein", "Her\u00b7mann", "wird", "sich", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der euch an Reihen f\u00fchrt.", "tokens": ["Der", "euch", "an", "Rei\u00b7hen", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00dft euch verstellten Frieden", "tokens": ["La\u00dft", "euch", "ver\u00b7stell\u00b7ten", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Schlafe nicht erm\u00fcden:", "tokens": ["Zum", "Schla\u00b7fe", "nicht", "er\u00b7m\u00fc\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit ", "tokens": ["Mit"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Mu\u00df man die Ruh erjagen.", "tokens": ["Mu\u00df", "man", "die", "Ruh", "er\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}