{"dta.poem.2190": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der N\u00e4chster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gewi\u00df, es w\u00e4r an einem Vieh die Frechheit minder", "tokens": ["Ge\u00b7wi\u00df", ",", "es", "w\u00e4r", "an", "ei\u00b7nem", "Vieh", "die", "Frech\u00b7heit", "min\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zu ver\u00fcbeln,", "tokens": ["zu", "ver\u00b7\u00fc\u00b7beln", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wenn es dein\u2019 Absicht, deine Schl\u00fcsse bem\u00fchet w\u00e4re zu", "tokens": ["Wenn", "es", "dein'", "Ab\u00b7sicht", ",", "dei\u00b7ne", "Schl\u00fcs\u00b7se", "be\u00b7m\u00fc\u00b7het", "w\u00e4\u00b7re", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "VAFIN", "APPR"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "ergr\u00fcbeln;", "tokens": ["er\u00b7gr\u00fc\u00b7beln", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Als da\u00df dein unversch\u00e4mter Stolz der Gottheit Uner-", "tokens": ["Als", "da\u00df", "dein", "un\u00b7ver\u00b7sch\u00e4m\u00b7ter", "Stolz", "der", "Got\u00b7theit", "Un\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "m\u00e4\u00dflichkeit", "tokens": ["m\u00e4\u00df\u00b7lich\u00b7keit"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Und Seiner Weisheit tiefe Tiefen sich zu ergr\u00fcnden un-", "tokens": ["Und", "Sei\u00b7ner", "Weis\u00b7heit", "tie\u00b7fe", "Tie\u00b7fen", "sich", "zu", "er\u00b7gr\u00fcn\u00b7den", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "PRF", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "terstehet.", "tokens": ["ter\u00b7ste\u00b7het", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Da ja der Abstand deines Geistes zu g\u00f6ttlicher Vollkom-", "tokens": ["Da", "ja", "der", "Ab\u00b7stand", "dei\u00b7nes", "Geis\u00b7tes", "zu", "g\u00f6tt\u00b7li\u00b7cher", "Voll\u00b7kom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PPOSAT", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "menheit,", "tokens": ["men\u00b7heit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "--", "measure": "unknown.measure.zero"}, "line.11": {"text": "Den Abstand zwischen einem Vieh und dir, unendlich", "tokens": ["Den", "Ab\u00b7stand", "zwi\u00b7schen", "ei\u00b7nem", "Vieh", "und", "dir", ",", "un\u00b7end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "PPER", "$,", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u00fcbergehet.", "tokens": ["\u00fc\u00b7berg\u00b7e\u00b7het", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}