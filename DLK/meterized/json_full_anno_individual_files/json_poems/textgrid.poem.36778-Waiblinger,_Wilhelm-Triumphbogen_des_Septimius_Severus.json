{"textgrid.poem.36778": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Triumphbogen des Septimius Severus", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siegesbogen errichtete man den Heroen zum Denkmal,", "tokens": ["Sie\u00b7ges\u00b7bo\u00b7gen", "er\u00b7rich\u00b7te\u00b7te", "man", "den", "He\u00b7roen", "zum", "Denk\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-+---+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Was sie vor andern, so wie was sie f\u00fcr andre gethan.", "tokens": ["Was", "sie", "vor", "an\u00b7dern", ",", "so", "wie", "was", "sie", "f\u00fcr", "and\u00b7re", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PIS", "$,", "ADV", "KOKOM", "PWS", "PPER", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Steig' ich vom Capitol, so steht mir einer vorm Auge,", "tokens": ["Steig'", "ich", "vom", "Ca\u00b7pi\u00b7tol", ",", "so", "steht", "mir", "ei\u00b7ner", "vorm", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Den uns Kronion, doch nein, den uns Barbaren bewahrt.", "tokens": ["Den", "uns", "Kro\u00b7ni\u00b7on", ",", "doch", "nein", ",", "den", "uns", "Bar\u00b7ba\u00b7ren", "be\u00b7wahrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "$,", "ADV", "PTKANT", "$,", "PRELS", "PPER", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Und wie der C\u00e4sar einst mit seinen Heeren hindurchzog,", "tokens": ["Und", "wie", "der", "C\u00e4\u00b7sar", "einst", "mit", "sei\u00b7nen", "Hee\u00b7ren", "hin\u00b7durch\u00b7zog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Treiben da unten auch nun H\u00fchner und G\u00e4nse sich um.", "tokens": ["Trei\u00b7ben", "da", "un\u00b7ten", "auch", "nun", "H\u00fch\u00b7ner", "und", "G\u00e4n\u00b7se", "sich", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADV", "ADV", "NN", "KON", "NN", "PRF", "PTKVZ", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Siegesbogen errichtete man den Heroen zum Denkmal,", "tokens": ["Sie\u00b7ges\u00b7bo\u00b7gen", "er\u00b7rich\u00b7te\u00b7te", "man", "den", "He\u00b7roen", "zum", "Denk\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+--+-+---+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Was sie vor andern, so wie was sie f\u00fcr andre gethan.", "tokens": ["Was", "sie", "vor", "an\u00b7dern", ",", "so", "wie", "was", "sie", "f\u00fcr", "and\u00b7re", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PIS", "$,", "ADV", "KOKOM", "PWS", "PPER", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+--++--+