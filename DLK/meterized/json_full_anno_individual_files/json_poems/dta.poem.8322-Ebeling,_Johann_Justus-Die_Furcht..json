{"dta.poem.8322": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Furcht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Furcht ist unser Plage-Geist,", "tokens": ["Die", "Furcht", "ist", "un\u00b7ser", "Pla\u00b7ge\u00b7Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drum sie des Lebens Kobold heist,", "tokens": ["Drum", "sie", "des", "Le\u00b7bens", "Ko\u00b7bold", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie suchet uns mit eitlen Schatten,", "tokens": ["Sie", "su\u00b7chet", "uns", "mit", "eit\u00b7len", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In dieser Welt stets abzumatten.", "tokens": ["In", "die\u00b7ser", "Welt", "stets", "ab\u00b7zu\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie plagt von aussen unsre Sinnen,", "tokens": ["Sie", "plagt", "von", "aus\u00b7sen", "uns\u00b7re", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie qu\u00e4lt das Herze stets von innen.", "tokens": ["Sie", "qu\u00e4lt", "das", "Her\u00b7ze", "stets", "von", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "VVFIN", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie tr\u00fcget uns mit leeren Schein,", "tokens": ["Sie", "tr\u00fc\u00b7get", "uns", "mit", "lee\u00b7ren", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo nichts ist, soll doch etwas seyn.", "tokens": ["Wo", "nichts", "ist", ",", "soll", "doch", "et\u00b7was", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "$,", "VMFIN", "ADV", "PIS", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie zeigt uns durch der Augen-Fenster,", "tokens": ["Sie", "zeigt", "uns", "durch", "der", "Au\u00b7gen\u00b7Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Offt viele schwarze Nachtgespenster,", "tokens": ["Offt", "vie\u00b7le", "schwar\u00b7ze", "Nacht\u00b7ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wenn man es beim Licht beschauet,", "tokens": ["Doch", "wenn", "man", "es", "beim", "Licht", "be\u00b7schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So schwindet das, wof\u00fcr uns grauet.", "tokens": ["So", "schwin\u00b7det", "das", ",", "wo\u00b7f\u00fcr", "uns", "grau\u00b7et", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDS", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Furcht bet\u00e4ubet das Geh\u00f6r,", "tokens": ["Die", "Furcht", "be\u00b7t\u00e4u\u00b7bet", "das", "Ge\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es d\u00fcnkt uns offt ein grosses Heer,", "tokens": ["Es", "d\u00fcnkt", "uns", "offt", "ein", "gros\u00b7ses", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sei hinter uns auf dunklen Wegen,", "tokens": ["Sei", "hin\u00b7ter", "uns", "auf", "dunk\u00b7len", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sich nur Laubesbl\u00e4tter regen.", "tokens": ["Wenn", "sich", "nur", "Lau\u00b7bes\u00b7bl\u00e4t\u00b7ter", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie macht bei einer schwarzen Stille,", "tokens": ["Sie", "macht", "bei", "ei\u00b7ner", "schwar\u00b7zen", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein klein Geth\u00f6n zum gros Gebr\u00fclle.", "tokens": ["Ein", "klein", "Ge\u00b7th\u00f6n", "zum", "gros", "Ge\u00b7br\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bei einem angestekten Licht,", "tokens": ["Bei", "ei\u00b7nem", "an\u00b7ges\u00b7tek\u00b7ten", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verschwindet gleich ein Schrekgesicht,", "tokens": ["Ver\u00b7schwin\u00b7det", "gleich", "ein", "Schrek\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Poltergeister, die uns plagen,", "tokens": ["Die", "Pol\u00b7ter\u00b7geis\u00b7ter", ",", "die", "uns", "pla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kan Sonn und Tag gar bald verjagen,", "tokens": ["Kan", "Sonn", "und", "Tag", "gar", "bald", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mir deucht es wird die Furcht verrauchen,", "tokens": ["Mir", "deucht", "es", "wird", "die", "Furcht", "ver\u00b7rau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn wir das Licht der Warheit brauchen.", "tokens": ["Wenn", "wir", "das", "Licht", "der", "War\u00b7heit", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}