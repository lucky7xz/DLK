{"textgrid.poem.47849": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Es ist ein Fl\u00fcstern", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Fl\u00fcstern in der Nacht,", "tokens": ["Es", "ist", "ein", "Fl\u00fcs\u00b7tern", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es hat mich ganz um den Schlaf gebracht;", "tokens": ["Es", "hat", "mich", "ganz", "um", "den", "Schlaf", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich f\u00fchl's, es will sich was verk\u00fcnden", "tokens": ["Ich", "f\u00fchl's", ",", "es", "will", "sich", "was", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PRF", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kann den Weg nicht zu mir finden.", "tokens": ["Und", "kann", "den", "Weg", "nicht", "zu", "mir", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind's Liebesworte, vertrauet dem Wind,", "tokens": ["Sin\u00b7d's", "Lie\u00b7bes\u00b7wor\u00b7te", ",", "ver\u00b7trau\u00b7et", "dem", "Wind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die unterwegs verwehet sind?", "tokens": ["Die", "un\u00b7ter\u00b7wegs", "ver\u00b7we\u00b7het", "sind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oder ist's Unheil aus k\u00fcnftigen Tagen,", "tokens": ["O\u00b7der", "ist's", "Un\u00b7heil", "aus", "k\u00fcnf\u00b7ti\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Das emsig dr\u00e4ngt sich anzusagen?", "tokens": ["Das", "em\u00b7sig", "dr\u00e4ngt", "sich", "an\u00b7zu\u00b7sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist ein Fl\u00fcstern in der Nacht,", "tokens": ["Es", "ist", "ein", "Fl\u00fcs\u00b7tern", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es hat mich ganz um den Schlaf gebracht;", "tokens": ["Es", "hat", "mich", "ganz", "um", "den", "Schlaf", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich f\u00fchl's, es will sich was verk\u00fcnden", "tokens": ["Ich", "f\u00fchl's", ",", "es", "will", "sich", "was", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["wo