{"textgrid.poem.34294": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Baum, der mir den Schatten zittert,", "tokens": ["Der", "Baum", ",", "der", "mir", "den", "Schat\u00b7ten", "zit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Quell, der mir sein Mitleid rauscht,", "tokens": ["Der", "Quell", ",", "der", "mir", "sein", "Mit\u00b7leid", "rauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Vogel, der im Baume zwittert,", "tokens": ["Der", "Vo\u00b7gel", ",", "der", "im", "Bau\u00b7me", "zwit\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und, ob ich ihn auch h\u00f6re, lauscht;", "tokens": ["Und", ",", "ob", "ich", "ihn", "auch", "h\u00f6\u00b7re", ",", "lauscht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die ganze freundliche Natur", "tokens": ["Die", "gan\u00b7ze", "freund\u00b7li\u00b7che", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nimmt mich umsonst in ihre Kur.", "tokens": ["Nimmt", "mich", "um\u00b7sonst", "in", "ih\u00b7re", "Kur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Weisheit, strengen Angesichts", "tokens": ["Die", "Weis\u00b7heit", ",", "stren\u00b7gen", "An\u00b7ge\u00b7sichts"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und guten Herzens, aber kalt,", "tokens": ["Und", "gu\u00b7ten", "Her\u00b7zens", ",", "a\u00b7ber", "kalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Lacht meines gl\u00fchenden Gedichtes", "tokens": ["Lacht", "mei\u00b7nes", "gl\u00fc\u00b7hen\u00b7den", "Ge\u00b7dich\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Liebe \u2013 und doch glaubt sie's bald;", "tokens": ["Von", "Lie\u00b7be", "\u2013", "und", "doch", "glaubt", "sie's", "bald", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "KON", "ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Will mich entzaubern, tr\u00f6sten mich,", "tokens": ["Will", "mich", "ent\u00b7zau\u00b7bern", ",", "tr\u00f6s\u00b7ten", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bezaubert und verirre