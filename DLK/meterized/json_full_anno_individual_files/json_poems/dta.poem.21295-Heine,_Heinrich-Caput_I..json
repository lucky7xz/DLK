{"dta.poem.21295": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein kleines Harfenm\u00e4dchen sang.", "tokens": ["Ein", "klei\u00b7nes", "Har\u00b7fen\u00b7m\u00e4d\u00b7chen", "sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sang mit wahrem Gef\u00fchle", "tokens": ["Sie", "sang", "mit", "wah\u00b7rem", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und falscher Stimme, doch ward ich sehr", "tokens": ["Und", "fal\u00b7scher", "Stim\u00b7me", ",", "doch", "ward", "ich", "sehr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ger\u00fchret von ihrem Spiele.", "tokens": ["Ge\u00b7r\u00fch\u00b7ret", "von", "ih\u00b7rem", "Spie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie sang von Liebe und Liebesgram,", "tokens": ["Sie", "sang", "von", "Lie\u00b7be", "und", "Lie\u00b7bes\u00b7gram", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aufopfrung und Wiederfinden", "tokens": ["Auf\u00b7opf\u00b7rung", "und", "Wie\u00b7der\u00b7fin\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort oben, in jener besseren Welt,", "tokens": ["Dort", "o\u00b7ben", ",", "in", "je\u00b7ner", "bes\u00b7se\u00b7ren", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo alle Leiden schwinden.", "tokens": ["Wo", "al\u00b7le", "Lei\u00b7den", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie sang vom irdischen Jammerthal,", "tokens": ["Sie", "sang", "vom", "ir\u00b7di\u00b7schen", "Jam\u00b7mer\u00b7thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von Freuden, die bald zerronnen,", "tokens": ["Von", "Freu\u00b7den", ",", "die", "bald", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Vom Jenseits, wo die Seele schwelgt", "tokens": ["Vom", "Jen\u00b7seits", ",", "wo", "die", "See\u00b7le", "schwelgt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verkl\u00e4rt in ew\u2019gen Wonnen.", "tokens": ["Ver\u00b7kl\u00e4rt", "in", "ew'\u00b7gen", "Won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie sang das alte Entsagungslied,", "tokens": ["Sie", "sang", "das", "al\u00b7te", "Ent\u00b7sa\u00b7gungs\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Das Eyapopeya vom Himmel,", "tokens": ["Das", "Ey\u00b7a\u00b7po\u00b7pe\u00b7ya", "vom", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Womit man einlullt, wenn es greint,", "tokens": ["Wo\u00b7mit", "man", "ein\u00b7lullt", ",", "wenn", "es", "greint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVPP", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Das Volk, den gro\u00dfen L\u00fcmmel.", "tokens": ["Das", "Volk", ",", "den", "gro\u00b7\u00dfen", "L\u00fcm\u00b7mel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}