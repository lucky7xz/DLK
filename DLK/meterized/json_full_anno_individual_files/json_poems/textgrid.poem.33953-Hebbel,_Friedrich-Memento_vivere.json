{"textgrid.poem.33953": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Memento vivere", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ritt einmal im Dunkeln", "tokens": ["Ich", "ritt", "ein\u00b7mal", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sp\u00e4t durch ein enges Thal;", "tokens": ["Sp\u00e4t", "durch", "ein", "en\u00b7ges", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Nacht war still und traurig,", "tokens": ["Die", "Nacht", "war", "still", "und", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich still und traurig zumal.", "tokens": ["Ich", "still", "und", "trau\u00b7rig", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "ADJD", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ich dachte der wenigen Freunde,", "tokens": ["Ich", "dach\u00b7te", "der", "we\u00b7ni\u00b7gen", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die ich auf Erden fand,", "tokens": ["Die", "ich", "auf", "Er\u00b7den", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich dachte derer vor Allen,", "tokens": ["Ich", "dach\u00b7te", "de\u00b7rer", "vor", "Al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die schon bedeckt der Sand.", "tokens": ["Die", "schon", "be\u00b7deckt", "der", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da scholl's, wie Geisterstimme,", "tokens": ["Da", "scholl's", ",", "wie", "Geis\u00b7ters\u00b7tim\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom d\u00fcstern Berg herab:", "tokens": ["Vom", "d\u00fcs\u00b7tern", "Berg", "her\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mensch, freu' dich heut' des Lebens,", "tokens": ["Mensch", ",", "freu'", "dich", "heut'", "des", "Le\u00b7bens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Denn morgen geht's in's Grab.", "tokens": ["Denn", "mor\u00b7gen", "geht's", "in's", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "War es ein Hirtenknabe,", "tokens": ["War", "es", "ein", "Hir\u00b7ten\u00b7kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der jene Worte sang \u2013", "tokens": ["Der", "je\u00b7ne", "Wor\u00b7te", "sang", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df es nicht, sie gingen", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "sie", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir durch die Seele bang.", "tokens": ["Mir", "durch", "die", "See\u00b7le", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Einst hatt' ich sie vernommen", "tokens": ["Einst", "hatt'", "ich", "sie", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus eines Bruders Mund,", "tokens": ["Aus", "ei\u00b7nes", "Bru\u00b7ders", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da trank er meine Gesundheit,", "tokens": ["Da", "trank", "er", "mei\u00b7ne", "Ge\u00b7sund\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jetzt lag er im k\u00fchlen Grund.", "tokens": ["Jetzt", "lag", "er", "im", "k\u00fch\u00b7len", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "p