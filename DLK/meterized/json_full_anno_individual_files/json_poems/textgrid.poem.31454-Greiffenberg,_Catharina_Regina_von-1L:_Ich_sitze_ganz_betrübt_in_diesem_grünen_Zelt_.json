{"textgrid.poem.31454": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sitze ganz betr\u00fcbt in diesem gr\u00fcnen Zelt /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sitze ganz betr\u00fcbt in diesem gr\u00fcnen Zelt /", "tokens": ["Ich", "sit\u00b7ze", "ganz", "be\u00b7tr\u00fcbt", "in", "die\u00b7sem", "gr\u00fc\u00b7nen", "Zelt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mu\u00df in der Hoffnung / mich des hoffens ganz verwegen.", "tokens": ["mu\u00df", "in", "der", "Hoff\u00b7nung", "/", "mich", "des", "hof\u00b7fens", "ganz", "ver\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "$(", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Himmel ist der Erd geneigt / und mir entgegen /", "tokens": ["Der", "Him\u00b7mel", "ist", "der", "Erd", "ge\u00b7neigt", "/", "und", "mir", "ent\u00b7ge\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$(", "KON", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nimt mir das / wo Er mit verehrt die ganze Welt.", "tokens": ["nimt", "mir", "das", "/", "wo", "Er", "mit", "ver\u00b7ehrt", "die", "gan\u00b7ze", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"],