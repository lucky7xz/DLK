{"textgrid.poem.59745": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Trost auf dem Leichenfelde", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber Gr\u00e4bern schaust du in die Welt \u2013", "tokens": ["\u00dc\u00b7ber", "Gr\u00e4\u00b7bern", "schaust", "du", "in", "die", "Welt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Rede, wo sind alle deine Lieben,", "tokens": ["Re\u00b7de", ",", "wo", "sind", "al\u00b7le", "dei\u00b7ne", "Lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PIAT", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Deine Jugendfreunde all geblieben?", "tokens": ["Dei\u00b7ne", "Ju\u00b7gend\u00b7freun\u00b7de", "all", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Suche sie im Leichenfeld.", "tokens": ["Su\u00b7che", "sie", "im", "Lei\u00b7chen\u00b7feld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jeder Erdenfu\u00df tritt hier auf Leichen:", "tokens": ["Je\u00b7der", "Er\u00b7den\u00b7fu\u00df", "tritt", "hier", "auf", "Lei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die Jahrtausende bl\u00fchen und erbleichen,", "tokens": ["Die", "Jahr\u00b7tau\u00b7sen\u00b7de", "bl\u00fc\u00b7hen", "und", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und ein Grabfeld ist die Welt.", "tokens": ["Und", "ein", "Grab\u00b7feld", "ist", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt Gebl\u00e4s' von kleinsten W\u00fcrmerzwergen,", "tokens": ["Jetzt", "Ge\u00b7bl\u00e4s'", "von", "kleins\u00b7ten", "W\u00fcr\u00b7mer\u00b7zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stand einst, Riesenstein, auf Alpenbergen.", "tokens": ["Stand", "einst", ",", "Rie\u00b7sen\u00b7stein", ",", "auf", "Al\u00b7pen\u00b7ber\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dies der Dinge \u00dcbelstand.", "tokens": ["Dies", "der", "Din\u00b7ge", "\u00dc\u00b7bel\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ewigkeit, wie saust und braust dein Meer,", "tokens": ["E\u00b7wig\u00b7keit", ",", "wie", "saust", "und", "braust", "dein", "Meer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Worauf zwischen S\u00e4rgen, zwischen Wiegen", "tokens": ["Wo\u00b7rauf", "zwi\u00b7schen", "S\u00e4r\u00b7gen", ",", "zwi\u00b7schen", "Wie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Jahrtausende sich niederwiegen,", "tokens": ["Die", "Jahr\u00b7tau\u00b7sen\u00b7de", "sich", "nie\u00b7der\u00b7wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Graunvoll rollend hin und her!", "tokens": ["Graun\u00b7voll", "rol\u00b7lend", "hin", "und", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du auch, Erde, du mein Mutterland,", "tokens": ["Du", "auch", ",", "Er\u00b7de", ",", "du", "mein", "Mut\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "S\u00fc\u00dfer Sehnsucht Land und s\u00fc\u00dfer L\u00fcgen,", "tokens": ["S\u00fc\u00b7\u00dfer", "Sehn\u00b7sucht", "Land", "und", "s\u00fc\u00b7\u00dfer", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie mit Millionen Flammenz\u00fcgen", "tokens": ["Wie", "mit", "Mil\u00b7lion\u00b7en", "Flam\u00b7men\u00b7z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ziehst und brennst du mich zu Sand!", "tokens": ["Ziehst", "und", "brennst", "du", "mich", "zu", "Sand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sei's! Mu\u00df alles, was gebar der Staub,", "tokens": ["Sei's", "!", "Mu\u00df", "al\u00b7les", ",", "was", "ge\u00b7bar", "der", "Staub", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PIS", "$,", "PRELS", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wieder hier zu Sand und Staub zerstieben,", "tokens": ["Wie\u00b7der", "hier", "zu", "Sand", "und", "Staub", "zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Lieben all sind mir geblieben;", "tokens": ["Mei\u00b7ne", "Lie\u00b7ben", "all", "sind", "mir", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PIAT", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn kein Staub nimmt solchen Raub.", "tokens": ["Denn", "kein", "Staub", "nimmt", "sol\u00b7chen", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Drum nur immer auf dem Leichensand", "tokens": ["Drum", "nur", "im\u00b7mer", "auf", "dem", "Lei\u00b7chen\u00b7sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Festen Muts und Fu\u00dfes aufgetreten!", "tokens": ["Fes\u00b7ten", "Muts", "und", "Fu\u00b7\u00dfes", "auf\u00b7ge\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf des Herzensnordsterns Lichtmagneten", "tokens": ["Auf", "des", "Her\u00b7zens\u00b7nords\u00b7terns", "Licht\u00b7mag\u00b7ne\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unverr\u00fcckt den Blick gewandt!", "tokens": ["Un\u00b7ver\u00b7r\u00fcckt", "den", "Blick", "ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf! Empor, wohin dein Stern dir weist!", "tokens": ["Auf", "!", "Em\u00b7por", ",", "wo\u00b7hin", "dein", "Stern", "dir", "weist", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NE", "$,", "PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schau', wohin die Sonnenadler schweben!", "tokens": ["Schau'", ",", "wo\u00b7hin", "die", "Son\u00b7ne\u00b7nad\u00b7ler", "schwe\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Traue! Denn er winkt unsterblich Leben,", "tokens": ["Trau\u00b7e", "!", "Denn", "er", "winkt", "uns\u00b7terb\u00b7lich", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KON", "PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Traue deinen Sonnenv\u00f6geln, Geist!", "tokens": ["Trau\u00b7e", "dei\u00b7nen", "Son\u00b7nen\u00b7v\u00f6\u00b7geln", ",", "Geist", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Graun, hinweg! Weg alles, was da bebt!", "tokens": ["Graun", ",", "hin\u00b7weg", "!", "Weg", "al\u00b7les", ",", "was", "da", "bebt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "$.", "NN", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Traue! Glaube! Alle deine Lieben", "tokens": ["Trau\u00b7e", "!", "Glau\u00b7be", "!", "Al\u00b7le", "dei\u00b7ne", "Lie\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "NN", "$.", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Stehn im Himmelsbuche eingeschrieben,", "tokens": ["Stehn", "im", "Him\u00b7mels\u00b7bu\u00b7che", "ein\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo sich's ewig liebt und lebt!", "tokens": ["Wo", "sich's", "e\u00b7wig", "liebt", "und", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00dcber Gr\u00e4bern schaust du in die