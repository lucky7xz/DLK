ter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Sei's! Mu\u00df alles, was gebar der Staub,", "tokens": ["Sei's", "!", "Mu\u00df", "al\u00b7les", ",", "was", "ge\u00b7bar", "der", "Staub", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PIS", "$,", "PRELS", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wieder hier zu Sand und Staub zerstieben,", "tokens": ["Wie\u00b7der", "hier", "zu", "Sand", "und", "Staub", "zer\u00b7stie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Meine Lieben all sind mir geblieben;", "tokens": ["Mei\u00b7ne", "Lie\u00b7ben", "all", "sind", "mir", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PIAT", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn kein Staub nimmt solchen Raub.", "tokens": ["Denn", "kein", "Staub", "nimmt", "sol\u00b7chen", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Drum nur immer auf dem Leichensand", "tokens": ["Drum", "nur", "im\u00b7mer", "auf", "dem", "Lei\u00b7chen\u00b7sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Festen Muts und Fu\u00dfes aufgetreten!", "tokens": ["Fes\u00b7ten", "Muts", "und", "Fu\u00b7\u00dfes", "auf\u00b7ge\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Auf des Herzensnordsterns Lichtmagneten", "tokens": ["Auf", "des", "Her\u00b7zens\u00b7nords\u00b7terns", "Licht\u00b7mag\u00b7ne\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Unverr\u00fcckt den Blick gewandt!", "tokens": ["Un\u00b7ver\u00b7r\u00fcckt", "den", "Blick", "ge\u00b7wandt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Auf! Empor, wohin dein Stern dir weist!", "tokens": ["Auf", "!", "Em\u00b7por", ",", "wo\u00b7hin", "dein", "Stern", "dir", "weist", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "NE", "$,", "PWAV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schau', wohin die Sonnenadler schweben!", "tokens": ["Schau'", ",", "wo\u00b7hin", "die", "Son\u00b7ne\u00b7nad\u00b7ler", "schwe\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Traue! Denn er winkt unsterblich Leben,", "tokens": ["Trau\u00b7e", "!", "Denn", "er", "winkt", "uns\u00b7terb\u00b7lich", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KON", "PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Traue deinen Sonnenv\u00f6geln, Geist!", "tokens": ["Trau\u00b7e", "dei\u00b7nen", "Son\u00b7nen\u00b7v\u00f6\u00b7geln", ",", "Geist", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.18": {"line.1": {"text": "Graun, hinweg! Weg alles, was da bebt!", "tokens": ["Graun", ",", "hin\u00b7weg", "!", "Weg", "al\u00b7les", ",", "was", "da", "bebt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "$.", "NN", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Traue! Glaube! Alle deine Lieben", "tokens": ["Trau\u00b7e", "!", "Glau\u00b7be", "!", "Al\u00b7le", "dei\u00b7ne", "Lie\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "NN", "$.", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Stehn im Himmelsbuche eingeschrieben,", "tokens": ["Stehn", "im", "Him\u00b7mels\u00b7bu\u00b7che", "ein\u00b7ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo sich's ewig liebt und lebt!", "tokens": ["Wo", "sich's", "e\u00b7wig", "liebt", "und", "lebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}