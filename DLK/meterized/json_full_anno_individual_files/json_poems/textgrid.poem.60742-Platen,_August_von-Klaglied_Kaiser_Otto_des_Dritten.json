{"textgrid.poem.60742": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Klaglied Kaiser Otto des Dritten", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Erde, nimm den M\u00fcden,", "tokens": ["O", "Er\u00b7de", ",", "nimm", "den", "M\u00fc\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Lebensm\u00fcden auf,", "tokens": ["Den", "Le\u00b7bens\u00b7m\u00fc\u00b7den", "auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hier im fernen S\u00fcden", "tokens": ["Der", "hier", "im", "fer\u00b7nen", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beschlie\u00dft den Pilgerlauf!", "tokens": ["Be\u00b7schlie\u00dft", "den", "Pil\u00b7ger\u00b7lauf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon steh ich an der Grenze,", "tokens": ["Schon", "steh", "ich", "an", "der", "Gren\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Leib und Seele teilt,", "tokens": ["Die", "Leib", "und", "See\u00b7le", "teilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und meine zwanzig Lenze", "tokens": ["Und", "mei\u00b7ne", "zwan\u00b7zig", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sind rasch dahin geeilt.", "tokens": ["Sind", "rasch", "da\u00b7hin", "ge\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Voll unerf\u00fcllter Tr\u00e4ume,", "tokens": ["Voll", "un\u00b7er\u00b7f\u00fcll\u00b7ter", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwaist, in Gram versenkt,", "tokens": ["Ver\u00b7waist", ",", "in", "Gram", "ver\u00b7senkt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entfallen mir die Z\u00e4ume,", "tokens": ["Ent\u00b7fal\u00b7len", "mir", "die", "Z\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die dieses Reich gelenkt.", "tokens": ["Die", "die\u00b7ses", "Reich", "ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Andrer mag es z\u00fcgeln", "tokens": ["Ein", "A\u00b7ndrer", "mag", "es", "z\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit H\u00e4nden minder schlaff,", "tokens": ["Mit", "H\u00e4n\u00b7den", "min\u00b7der", "schlaff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von diesen sieben H\u00fcgeln", "tokens": ["Von", "die\u00b7sen", "sie\u00b7ben", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis an des Nordens Haff!", "tokens": ["Bis", "an", "des", "Nor\u00b7dens", "Haff", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch selbst im Seelenreiche", "tokens": ["Doch", "selbst", "im", "See\u00b7len\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Harrt meiner noch die Schmach,", "tokens": ["Harrt", "mei\u00b7ner", "noch", "die", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es folgt der blassen Leiche", "tokens": ["Es", "folgt", "der", "blas\u00b7sen", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Begangner Frevel nach:", "tokens": ["Be\u00b7gang\u00b7ner", "Fre\u00b7vel", "nach", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vergebens mit Gebeten", "tokens": ["Ver\u00b7ge\u00b7bens", "mit", "Ge\u00b7be\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Beschw\u00f6r ich diesen Bann,", "tokens": ["Be\u00b7schw\u00f6r", "ich", "die\u00b7sen", "Bann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mir entgegen treten", "tokens": ["Und", "mir", "ent\u00b7ge\u00b7gen", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKVZ", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Crescentius und Johann!", "tokens": ["Cre\u00b7scen\u00b7ti\u00b7us", "und", "Jo\u00b7hann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Doch nein! Die Stolzen beugte", "tokens": ["Doch", "nein", "!", "Die", "Stol\u00b7zen", "beug\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein reuem\u00fctig Flehn;", "tokens": ["Mein", "reu\u00b7e\u00b7m\u00fc\u00b7tig", "Flehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn, welcher mich erzeugte,", "tokens": ["Ihn", ",", "wel\u00b7cher", "mich", "er\u00b7zeug\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn werd ich wiedersehn!", "tokens": ["Ihn", "werd", "ich", "wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach welchem ich als Knabe", "tokens": ["Nach", "wel\u00b7chem", "ich", "als", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So oft vergebens frug:", "tokens": ["So", "oft", "ver\u00b7ge\u00b7bens", "frug", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "An seinem fr\u00fchen Grabe", "tokens": ["An", "sei\u00b7nem", "fr\u00fc\u00b7hen", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hab ich geweint genug.", "tokens": ["Hab", "ich", "ge\u00b7weint", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Des deutschen Volks Berater", "tokens": ["Des", "deut\u00b7schen", "Volks", "Be\u00b7ra\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umwandeln Gottes Thron:", "tokens": ["Um\u00b7wan\u00b7deln", "Got\u00b7tes", "Thron", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir winkt der \u00c4ltervater", "tokens": ["Mir", "winkt", "der", "\u00c4l\u00b7ter\u00b7va\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem gro\u00dfen Sohn.", "tokens": ["Mit", "sei\u00b7nem", "gro\u00b7\u00dfen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und w\u00e4hrend, voll von Milde,", "tokens": ["Und", "w\u00e4h\u00b7rend", ",", "voll", "von", "Mil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die frommen H\u00e4nde legt", "tokens": ["Die", "from\u00b7men", "H\u00e4n\u00b7de", "legt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mir auf das Haupt Mathilde,", "tokens": ["Mir", "auf", "das", "Haupt", "Ma\u00b7thil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Steht Heinrich tiefbewegt.", "tokens": ["Steht", "Hein\u00b7rich", "tief\u00b7be\u00b7wegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun f\u00fchlt ich erst, wie eitel", "tokens": ["Nun", "f\u00fchlt", "ich", "erst", ",", "wie", "ei\u00b7tel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Gl\u00fccks Geschenke sind,", "tokens": ["Des", "Gl\u00fccks", "Ge\u00b7schen\u00b7ke", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wiewohl ich auf dem Scheitel", "tokens": ["Wie\u00b7wohl", "ich", "auf", "dem", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schon Kronen trug als Kind!", "tokens": ["Schon", "Kro\u00b7nen", "trug", "als", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was je mir schien gewichtig,", "tokens": ["Was", "je", "mir", "schien", "ge\u00b7wich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zerstiebt wie ein Atom:", "tokens": ["Zer\u00b7stiebt", "wie", "ein", "A\u00b7tom", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O Welt, du bist so nichtig,", "tokens": ["O", "Welt", ",", "du", "bist", "so", "nich\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du bist so klein, o Rom!", "tokens": ["Du", "bist", "so", "klein", ",", "o", "Rom", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "FM", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O Rom, wo meine Bl\u00fcten", "tokens": ["O", "Rom", ",", "wo", "mei\u00b7ne", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwelkt wie d\u00fcrres Laub,", "tokens": ["Ver\u00b7welkt", "wie", "d\u00fcr\u00b7res", "Laub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir ziemt es nicht, zu h\u00fcten", "tokens": ["Dir", "ziemt", "es", "nicht", ",", "zu", "h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den kaiserlichen Staub!", "tokens": ["Den", "kai\u00b7ser\u00b7li\u00b7chen", "Staub", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die mir die Treue brachen,", "tokens": ["Die", "mir", "die", "Treu\u00b7e", "bra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zerbr\u00e4chen mein Gebein:", "tokens": ["Zer\u00b7br\u00e4\u00b7chen", "mein", "Ge\u00b7bein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Beim gro\u00dfen Karl in Aachen", "tokens": ["Beim", "gro\u00b7\u00dfen", "Karl", "in", "Aa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NE", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Will ich bestattet sein.", "tokens": ["Will", "ich", "be\u00b7stat\u00b7tet", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die echten Palmen wehen", "tokens": ["Die", "ech\u00b7ten", "Pal\u00b7men", "we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur dort um sein Panier:", "tokens": ["Nur", "dort", "um", "sein", "Pa\u00b7nier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn hab ich liegen sehen", "tokens": ["Ihn", "hab", "ich", "lie\u00b7gen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seiner Kaiserzier.", "tokens": ["In", "sei\u00b7ner", "Kai\u00b7ser\u00b7zier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was durfte mich verf\u00fchren,", "tokens": ["Was", "durf\u00b7te", "mich", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu \u00f6ffnen seinen Sarg?", "tokens": ["Zu", "\u00f6ff\u00b7nen", "sei\u00b7nen", "Sarg", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Lorbeer anzur\u00fchren,", "tokens": ["Den", "Lor\u00b7beer", "an\u00b7zu\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der seine Schl\u00e4fe barg?", "tokens": ["Der", "sei\u00b7ne", "Schl\u00e4\u00b7fe", "barg", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "O Freunde, la\u00dft das Klagen,", "tokens": ["O", "Freun\u00b7de", ",", "la\u00dft", "das", "Kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir aber gebt Entsatz,", "tokens": ["Mir", "a\u00b7ber", "gebt", "Ent\u00b7satz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und macht dem Leichenwagen", "tokens": ["Und", "macht", "dem", "Lei\u00b7chen\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit euren Waffen Platz!", "tokens": ["Mit", "eu\u00b7ren", "Waf\u00b7fen", "Platz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bedeckt das Grab mit Rosen,", "tokens": ["Be\u00b7deckt", "das", "Grab", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das ich so fr\u00fch gewann,", "tokens": ["Das", "ich", "so", "fr\u00fch", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und legt den tatenlosen", "tokens": ["Und", "legt", "den", "ta\u00b7ten\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum tatenreichsten Mann!", "tokens": ["Zum", "ta\u00b7ten\u00b7reichs\u00b7ten", "Mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O Erde, nimm den M\u00fcden,", "tokens": ["O", "Er\u00b7de", ",", "nimm", "den", "M\u00fc\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Lebensm\u00fcden auf,", "tokens": ["Den", "Le\u00b7bens\u00b7m\u00fc\u00b7den", "auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hier im fernen S\u00fcden", "tokens": ["Der", "hier", "im", "fer\u00b7nen", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beschlie\u00dft den Pilgerlauf!", "tokens": ["Be\u00b7schlie\u00dft", "den", "Pil\u00b7ger\u00b7lauf", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon steh ich an der Grenze,", "tokens": ["Schon", "steh", "ich", "an", "der", "Gren\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Leib und Seele teilt,", "tokens": ["Die", "Leib", "und", "See\u00b7le", "teilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und meine zwanzig Lenze", "tokens": ["Und", "mei\u00b7ne", "zwan\u00b7zig", "Len\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sind rasch dahin geeilt.", "tokens": ["Sind", "rasch", "da\u00b7hin", "ge\u00b7eilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Voll unerf\u00fcllter Tr\u00e4ume,", "tokens": ["Voll", "un\u00b7er\u00b7f\u00fcll\u00b7ter", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwaist, in Gram versenkt,", "tokens": ["Ver\u00b7waist", ",", "in", "Gram", "ver\u00b7senkt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entfallen mir die Z\u00e4ume,", "tokens": ["Ent\u00b7fal\u00b7len", "mir", "die", "Z\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die dieses Reich gelenkt.", "tokens": ["Die", "die\u00b7ses", "Reich", "ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Andrer mag es z\u00fcgeln", "tokens": ["Ein", "A\u00b7ndrer", "mag", "es", "z\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit H\u00e4nden minder schlaff,", "tokens": ["Mit", "H\u00e4n\u00b7den", "min\u00b7der", "schlaff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von diesen sieben H\u00fcgeln", "tokens": ["Von", "die\u00b7sen", "sie\u00b7ben", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis an des Nordens Haff!", "tokens": ["Bis", "an", "des", "Nor\u00b7dens", "Haff", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Doch selbst im Seelenreiche", "tokens": ["Doch", "selbst", "im", "See\u00b7len\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Harrt meiner noch die Schmach,", "tokens": ["Harrt", "mei\u00b7ner", "noch", "die", "Schmach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es folgt der blassen Leiche", "tokens": ["Es", "folgt", "der", "blas\u00b7sen", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Begangner Frevel nach:", "tokens": ["Be\u00b7gang\u00b7ner", "Fre\u00b7vel", "nach", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vergebens mit Gebeten", "tokens": ["Ver\u00b7ge\u00b7bens", "mit", "Ge\u00b7be\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Beschw\u00f6r ich diesen Bann,", "tokens": ["Be\u00b7schw\u00f6r", "ich", "die\u00b7sen", "Bann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und mir entgegen treten", "tokens": ["Und", "mir", "ent\u00b7ge\u00b7gen", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "PTKVZ", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Crescentius und Johann!", "tokens": ["Cre\u00b7scen\u00b7ti\u00b7us", "und", "Jo\u00b7hann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Doch nein! Die Stolzen beugte", "tokens": ["Doch", "nein", "!", "Die", "Stol\u00b7zen", "beug\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKANT", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein reuem\u00fctig Flehn;", "tokens": ["Mein", "reu\u00b7e\u00b7m\u00fc\u00b7tig", "Flehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn, welcher mich erzeugte,", "tokens": ["Ihn", ",", "wel\u00b7cher", "mich", "er\u00b7zeug\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn werd ich wiedersehn!", "tokens": ["Ihn", "werd", "ich", "wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nach welchem ich als Knabe", "tokens": ["Nach", "wel\u00b7chem", "ich", "als", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "KOUS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So oft vergebens frug:", "tokens": ["So", "oft", "ver\u00b7ge\u00b7bens", "frug", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "An seinem fr\u00fchen Grabe", "tokens": ["An", "sei\u00b7nem", "fr\u00fc\u00b7hen", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hab ich geweint genug.", "tokens": ["Hab", "ich", "ge\u00b7weint", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Des deutschen Volks Berater", "tokens": ["Des", "deut\u00b7schen", "Volks", "Be\u00b7ra\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umwandeln Gottes Thron:", "tokens": ["Um\u00b7wan\u00b7deln", "Got\u00b7tes", "Thron", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir winkt der \u00c4ltervater", "tokens": ["Mir", "winkt", "der", "\u00c4l\u00b7ter\u00b7va\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem gro\u00dfen Sohn.", "tokens": ["Mit", "sei\u00b7nem", "gro\u00b7\u00dfen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und w\u00e4hrend, voll von Milde,", "tokens": ["Und", "w\u00e4h\u00b7rend", ",", "voll", "von", "Mil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die frommen H\u00e4nde legt", "tokens": ["Die", "from\u00b7men", "H\u00e4n\u00b7de", "legt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mir auf das Haupt Mathilde,", "tokens": ["Mir", "auf", "das", "Haupt", "Ma\u00b7thil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Steht Heinrich tiefbewegt.", "tokens": ["Steht", "Hein\u00b7rich", "tief\u00b7be\u00b7wegt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Nun f\u00fchlt ich erst, wie eitel", "tokens": ["Nun", "f\u00fchlt", "ich", "erst", ",", "wie", "ei\u00b7tel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Gl\u00fccks Geschenke sind,", "tokens": ["Des", "Gl\u00fccks", "Ge\u00b7schen\u00b7ke", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wiewohl ich auf dem Scheitel", "tokens": ["Wie\u00b7wohl", "ich", "auf", "dem", "Schei\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schon Kronen trug als Kind!", "tokens": ["Schon", "Kro\u00b7nen", "trug", "als", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was je mir schien gewichtig,", "tokens": ["Was", "je", "mir", "schien", "ge\u00b7wich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zerstiebt wie ein Atom:", "tokens": ["Zer\u00b7stiebt", "wie", "ein", "A\u00b7tom", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O Welt, du bist so nichtig,", "tokens": ["O", "Welt", ",", "du", "bist", "so", "nich\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du bist so klein, o Rom!", "tokens": ["Du", "bist", "so", "klein", ",", "o", "Rom", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "FM", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "O Rom, wo meine Bl\u00fcten", "tokens": ["O", "Rom", ",", "wo", "mei\u00b7ne", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwelkt wie d\u00fcrres Laub,", "tokens": ["Ver\u00b7welkt", "wie", "d\u00fcr\u00b7res", "Laub", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir ziemt es nicht, zu h\u00fcten", "tokens": ["Dir", "ziemt", "es", "nicht", ",", "zu", "h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den kaiserlichen Staub!", "tokens": ["Den", "kai\u00b7ser\u00b7li\u00b7chen", "Staub", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die mir die Treue brachen,", "tokens": ["Die", "mir", "die", "Treu\u00b7e", "bra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zerbr\u00e4chen mein Gebein:", "tokens": ["Zer\u00b7br\u00e4\u00b7chen", "mein", "Ge\u00b7bein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Beim gro\u00dfen Karl in Aachen", "tokens": ["Beim", "gro\u00b7\u00dfen", "Karl", "in", "Aa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NE", "APPR", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Will ich bestattet sein.", "tokens": ["Will", "ich", "be\u00b7stat\u00b7tet", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Die echten Palmen wehen", "tokens": ["Die", "ech\u00b7ten", "Pal\u00b7men", "we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nur dort um sein Panier:", "tokens": ["Nur", "dort", "um", "sein", "Pa\u00b7nier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihn hab ich liegen sehen", "tokens": ["Ihn", "hab", "ich", "lie\u00b7gen", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In seiner Kaiserzier.", "tokens": ["In", "sei\u00b7ner", "Kai\u00b7ser\u00b7zier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was durfte mich verf\u00fchren,", "tokens": ["Was", "durf\u00b7te", "mich", "ver\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu \u00f6ffnen seinen Sarg?", "tokens": ["Zu", "\u00f6ff\u00b7nen", "sei\u00b7nen", "Sarg", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den Lorbeer anzur\u00fchren,", "tokens": ["Den", "Lor\u00b7beer", "an\u00b7zu\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der seine Schl\u00e4fe barg?", "tokens": ["Der", "sei\u00b7ne", "Schl\u00e4\u00b7fe", "barg", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "O Freunde, la\u00dft das Klagen,", "tokens": ["O", "Freun\u00b7de", ",", "la\u00dft", "das", "Kla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir aber gebt Entsatz,", "tokens": ["Mir", "a\u00b7ber", "gebt", "Ent\u00b7satz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und macht dem Leichenwagen", "tokens": ["Und", "macht", "dem", "Lei\u00b7chen\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit euren Waffen Platz!", "tokens": ["Mit", "eu\u00b7ren", "Waf\u00b7fen", "Platz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bedeckt das Grab mit Rosen,", "tokens": ["Be\u00b7deckt", "das", "Grab", "mit", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das ich so fr\u00fch gewann,", "tokens": ["Das", "ich", "so", "fr\u00fch", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und legt den tatenlosen", "tokens": ["Und", "legt", "den", "ta\u00b7ten\u00b7lo\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zum tatenreichsten Mann!", "tokens": ["Zum", "ta\u00b7ten\u00b7reichs\u00b7ten", "Mann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}