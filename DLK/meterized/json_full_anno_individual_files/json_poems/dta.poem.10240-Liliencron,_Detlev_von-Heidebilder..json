{"dta.poem.10240": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Heidebilder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne leiht dem Schnee das Prachtgeschmeide,", "tokens": ["Die", "Son\u00b7ne", "leiht", "dem", "Schnee", "das", "Pracht\u00b7ge\u00b7schmei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch ach! wie kurz ist Schein und Licht.", "tokens": ["Doch", "ach", "!", "wie", "kurz", "ist", "Schein", "und", "Licht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PWAV", "ADJD", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Nebel tropft, und traurig zieht im Leide", "tokens": ["Ein", "Ne\u00b7bel", "tropft", ",", "und", "trau\u00b7rig", "zieht", "im", "Lei\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Landschaft ihren Schleier dicht.", "tokens": ["Die", "Land\u00b7schaft", "ih\u00b7ren", "Schlei\u00b7er", "dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein H\u00e4slein nur f\u00fchlt noch des Lebens W\u00e4rme,", "tokens": ["Ein", "H\u00e4s\u00b7lein", "nur", "f\u00fchlt", "noch", "des", "Le\u00b7bens", "W\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Am Weidenstumpfe hockt es bang.", "tokens": ["Am", "Wei\u00b7den\u00b7stump\u00b7fe", "hockt", "es", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch kreischen hungrig schon die Rabenschw\u00e4rme", "tokens": ["Doch", "krei\u00b7schen", "hung\u00b7rig", "schon", "die", "Ra\u00b7ben\u00b7schw\u00e4r\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hacken auf den sichern Fang.", "tokens": ["Und", "ha\u00b7cken", "auf", "den", "si\u00b7chern", "Fang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bis auf den schwarzen Schlammgrund sind gefroren", "tokens": ["Bis", "auf", "den", "schwar\u00b7zen", "Schlamm\u00b7grund", "sind", "ge\u00b7fro\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Wasserl\u00f6cher und der See.", "tokens": ["Die", "Was\u00b7ser\u00b7l\u00f6\u00b7cher", "und", "der", "See", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zuweilen geht ein Wimmern, wie verloren,", "tokens": ["Zu\u00b7wei\u00b7len", "geht", "ein", "Wim\u00b7mern", ",", "wie", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann stirbt im toten Wald ein Reh.", "tokens": ["Dann", "stirbt", "im", "to\u00b7ten", "Wald", "ein", "Reh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}