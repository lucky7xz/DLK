kens": ["Und", "run\u00b7de", "gr\u00fc\u00b7ne", "Wel\u00b7len", ",", "mei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Zum Spiele nur! am Schiffe schlug.", "tokens": ["Zum", "Spie\u00b7le", "nur", "!", "am", "Schif\u00b7fe", "schlug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$.", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Um dich, der seines Mosts, (entquillt der Palme", "tokens": ["Um", "dich", ",", "der", "sei\u00b7nes", "Mosts", ",", "(", "ent\u00b7quillt", "der", "Pal\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["KOUI", "PPER", "$,", "PRELS", "PPOSAT", "NN", "$,", "$(", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Gleich kaum ein Saft wie dieser, M\u00e4nnern s\u00fc\u00df,)", "tokens": ["Gleich", "kaum", "ein", "Saft", "wie", "die\u00b7ser", ",", "M\u00e4n\u00b7nern", "s\u00fc\u00df", ",", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "KOKOM", "PDS", "$,", "NN", "ADJD", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und seiner Mandeln, seiner fetten Salme,", "tokens": ["Und", "sei\u00b7ner", "Man\u00b7deln", ",", "sei\u00b7ner", "fet\u00b7ten", "Sal\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Mich ganz und gar vergessen lie\u00df.", "tokens": ["Mich", "ganz", "und", "gar", "ver\u00b7ges\u00b7sen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Denn lieblicher als deine Fr\u00fccht' und Reben,", "tokens": ["Denn", "lieb\u00b7li\u00b7cher", "als", "dei\u00b7ne", "Fr\u00fccht'", "und", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Sind meine Freunde, die an deinen Strand", "tokens": ["Sind", "mei\u00b7ne", "Freun\u00b7de", ",", "die", "an", "dei\u00b7nen", "Strand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Der Hebe Finger f\u00fcr die\u00df Erdenleben", "tokens": ["Der", "He\u00b7be", "Fin\u00b7ger", "f\u00fcr", "die\u00df", "Er\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "PDS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Mit Traubenvollen Ranken band.", "tokens": ["Mit", "Trau\u00b7ben\u00b7vol\u00b7len", "Ran\u00b7ken", "band", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und dich, o Rhein! soll ich nicht wiedersehen?", "tokens": ["Und", "dich", ",", "o", "Rhein", "!", "soll", "ich", "nicht", "wie\u00b7der\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NE", "$.", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und sie erst dann, wenn du vertrocknet bist,", "tokens": ["Und", "sie", "erst", "dann", ",", "wenn", "du", "ver\u00b7trock\u00b7net", "bist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Und jeder Stern dort in den blauen H\u00f6hen,", "tokens": ["Und", "je\u00b7der", "Stern", "dort", "in", "den", "blau\u00b7en", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ein kalter Schlackenklumpen ist?", "tokens": ["Ein", "kal\u00b7ter", "Schla\u00b7cken\u00b7klum\u00b7pen", "ist", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Doch warum sollst du sch\u00f6ner Strom, du Erde,", "tokens": ["Doch", "wa\u00b7rum", "sollst", "du", "sch\u00f6\u00b7ner", "Strom", ",", "du", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "ADJA", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Du Sternenhimmel, einstens untergehn?", "tokens": ["Du", "Ster\u00b7nen\u00b7him\u00b7mel", ",", "eins\u00b7tens", "un\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "O nein! des Herzens Hoffnung sagt, ich werde", "tokens": ["O", "nein", "!", "des", "Her\u00b7zens", "Hoff\u00b7nung", "sagt", ",", "ich", "wer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PTKANT", "$.", "ART", "NN", "NN", "VVFIN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Euch alle sch\u00f6ner wiedersehn.", "tokens": ["Euch", "al\u00b7le", "sch\u00f6\u00b7ner", "wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Dann \u00fcberfliegt dich, Rhein! auf leichten Schwingen", "tokens": ["Dann", "\u00fc\u00b7berf\u00b7liegt", "dich", ",", "Rhein", "!", "auf", "leich\u00b7ten", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NE", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Der Schiffer, den dein Strudel jetzt verschlingt;", "tokens": ["Der", "Schif\u00b7fer", ",", "den", "dein", "Stru\u00b7del", "jetzt", "ver\u00b7schlingt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Auch wird er dann allein nach Tugend ringen,", "tokens": ["Auch", "wird", "er", "dann", "al\u00b7lein", "nach", "Tu\u00b7gend", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "So wie er jetzt nach Golde ringt.", "tokens": ["So", "wie", "er", "jetzt", "nach", "Gol\u00b7de", "ringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Dann wird kein F\u00fcrst uns mehr in Fesseln schlagen,", "tokens": ["Dann", "wird", "kein", "F\u00fcrst", "uns", "mehr", "in", "Fes\u00b7seln", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Kein Vaterland und kein Geschlecht mehr seyn,", "tokens": ["Kein", "Va\u00b7ter\u00b7land", "und", "kein", "Ge\u00b7schlecht", "mehr", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Kein Trennen und kein Z\u00e4hlen mehr nach Tagen;", "tokens": ["Kein", "Tren\u00b7nen", "und", "kein", "Z\u00e4h\u00b7len", "mehr", "nach", "Ta\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Die ganze Sch\u00f6pfung ist dann mein!", "tokens": ["Die", "gan\u00b7ze", "Sch\u00f6p\u00b7fung", "ist", "dann", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}