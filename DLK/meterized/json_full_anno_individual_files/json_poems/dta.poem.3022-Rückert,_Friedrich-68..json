{"dta.poem.3022": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Bl\u00fcten aus dem Baum, wir Stralen aus der Sonne,", "tokens": ["Wie", "Bl\u00fc\u00b7ten", "aus", "dem", "Baum", ",", "wir", "Stra\u00b7len", "aus", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,", "PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So tritt aus Gott hervor der Welten lichte Wonne.", "tokens": ["So", "tritt", "aus", "Gott", "her\u00b7vor", "der", "Wel\u00b7ten", "lich\u00b7te", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PTKVZ", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Bl\u00fcten fallen ab, die Stralen sind verglommen,", "tokens": ["Die", "Bl\u00fc\u00b7ten", "fal\u00b7len", "ab", ",", "die", "Stra\u00b7len", "sind", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Niemand sieht, wie sie zur\u00fcck zur Wurzel kommen.", "tokens": ["Und", "Nie\u00b7mand", "sieht", ",", "wie", "sie", "zu\u00b7r\u00fcck", "zur", "Wur\u00b7zel", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PWAV", "PPER", "PTKVZ", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie kommen ungesehn zur Wurzel doch zur\u00fcck,", "tokens": ["Sie", "kom\u00b7men", "un\u00b7ge\u00b7sehn", "zur", "Wur\u00b7zel", "doch", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und treten neu hervor, ein ew'ges Fr\u00fchlingsgl\u00fcck.", "tokens": ["Und", "tre\u00b7ten", "neu", "her\u00b7vor", ",", "ein", "ew'\u00b7ges", "Fr\u00fch\u00b7lings\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}