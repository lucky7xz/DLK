{"textgrid.poem.47359": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Ungeliebte", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00e4nd' ich doch auf ird'scher Flur,", "tokens": ["F\u00e4nd'", "ich", "doch", "auf", "ird'\u00b7scher", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4nd' ich doch die Liebe nur,", "tokens": ["F\u00e4nd'", "ich", "doch", "die", "Lie\u00b7be", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ich liebend denke,", "tokens": ["Die", "ich", "lie\u00b7bend", "den\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df in sie der irre Geist,", "tokens": ["Da\u00df", "in", "sie", "der", "ir\u00b7re", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Der sich wild durch Welten rei\u00dft,", "tokens": ["Der", "sich", "wild", "durch", "Wel\u00b7ten", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebevoll vers\u00e4nke.", "tokens": ["Lie\u00b7be\u00b7voll", "ver\u00b7s\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VV