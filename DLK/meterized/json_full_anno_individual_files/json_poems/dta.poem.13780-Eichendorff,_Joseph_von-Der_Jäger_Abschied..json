{"dta.poem.13780": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der J\u00e4ger Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "nl:0.28"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Tief die Welt verworren schallt,", "tokens": ["Tief", "die", "Welt", "ver\u00b7wor\u00b7ren", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Oben einsam Rehe grasen,", "tokens": ["O\u00b7ben", "ein\u00b7sam", "Re\u00b7he", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wir ziehen fort und blasen,", "tokens": ["Und", "wir", "zie\u00b7hen", "fort", "und", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es tausendfach verhallt:", "tokens": ["Da\u00df", "es", "tau\u00b7send\u00b7fach", "ver\u00b7hallt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebe wohl,", "tokens": ["Le\u00b7be", "wohl", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Lebe wohl, du sch\u00f6ner Wald!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "sch\u00f6\u00b7ner", "Wald", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}