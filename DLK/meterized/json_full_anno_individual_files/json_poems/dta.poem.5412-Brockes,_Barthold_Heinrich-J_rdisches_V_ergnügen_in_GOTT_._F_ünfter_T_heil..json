{"dta.poem.5412": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erwegen, da\u00df die Gegenwart so kurtz, und fast be-", "tokens": ["Er\u00b7we\u00b7gen", ",", "da\u00df", "die", "Ge\u00b7gen\u00b7wart", "so", "kurtz", ",", "und", "fast", "be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "ADV", "ADJD", "$,", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "st\u00e4ndig fliehe,", "tokens": ["st\u00e4n\u00b7dig", "flie\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df sie, best\u00e4ndig fortgezogen, das k\u00fcnft\u2019ge stetig zu", "tokens": ["Da\u00df", "sie", ",", "be\u00b7st\u00e4n\u00b7dig", "fort\u00b7ge\u00b7zo\u00b7gen", ",", "das", "k\u00fcnft'\u00b7ge", "ste\u00b7tig", "zu"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJD", "VVPP", "$,", "PDS", "VVFIN", "ADJD", "PTKZU"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "sich ziehe,", "tokens": ["sich", "zie\u00b7he", ","], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "So da\u00df, in ihrer steten Flucht, sie fast nur scheint,", "tokens": ["So", "da\u00df", ",", "in", "ih\u00b7rer", "ste\u00b7ten", "Flucht", ",", "sie", "fast", "nur", "scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "als wenn sie w\u00e4r;", "tokens": ["als", "wenn", "sie", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Denn halb ist sie noch nicht erschienen, und halb ist", "tokens": ["Denn", "halb", "ist", "sie", "noch", "nicht", "er\u00b7schie\u00b7nen", ",", "und", "halb", "ist"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,", "KON", "ADJD", "VAFIN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "sie bereits nicht mehr:", "tokens": ["sie", "be\u00b7reits", "nicht", "mehr", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "So sag ich, finden wir kein Mittel, uns auf der Erde zu", "tokens": ["So", "sag", "ich", ",", "fin\u00b7den", "wir", "kein", "Mit\u00b7tel", ",", "uns", "auf", "der", "Er\u00b7de", "zu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PIAT", "NN", "$,", "PPER", "APPR", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "vergn\u00fcgen,", "tokens": ["ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Als wenn wir ein vern\u00fcnftges Dencken zum fl\u00fccht\u2019gen Ge-", "tokens": ["Als", "wenn", "wir", "ein", "ver\u00b7n\u00fcnft\u00b7ges", "Den\u00b7cken", "zum", "fl\u00fccht'\u00b7gen", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "genw\u00e4rt'gen f\u00fcgen.", "tokens": ["gen\u00b7w\u00e4rt'\u00b7gen", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Auf diese Weise blos allein h\u00e4lt man der Zeiten schnellen", "tokens": ["Auf", "die\u00b7se", "Wei\u00b7se", "blos", "al\u00b7lein", "h\u00e4lt", "man", "der", "Zei\u00b7ten", "schnel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "ADV", "VVFIN", "PIS", "ART", "NN", "VVINF"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Lauf,", "tokens": ["Lauf", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Wenn man, was man besitzt, erweget, durch frohes Den-", "tokens": ["Wenn", "man", ",", "was", "man", "be\u00b7sitzt", ",", "er\u00b7we\u00b7get", ",", "durch", "fro\u00b7hes", "Den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVFIN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "cken gleichsam auf;", "tokens": ["cken", "gleich\u00b7sam", "auf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "Man macht sie dadurch gleichsam fest, ja eignet sie sich gleich-", "tokens": ["Man", "macht", "sie", "da\u00b7durch", "gleich\u00b7sam", "fest", ",", "ja", "eig\u00b7net", "sie", "sich", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PAV", "ADJD", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.18": {"text": "sam zu,", "tokens": ["sam", "zu", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.19": {"text": "(zumahl da ein stets kommend K\u00fcnftig der Gegenwart", "tokens": ["(", "zu\u00b7mahl", "da", "ein", "stets", "kom\u00b7mend", "K\u00fcnf\u00b7tig", "der", "Ge\u00b7gen\u00b7wart"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "ART", "ADV", "ADJD", "ADJD", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Verlust ersetzt,", "tokens": ["Ver\u00b7lust", "er\u00b7setzt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Da\u00df man es nicht vergangen f\u00fchlet) und alles scheint in", "tokens": ["Da\u00df", "man", "es", "nicht", "ver\u00b7gan\u00b7gen", "f\u00fch\u00b7let", ")", "und", "al\u00b7les", "scheint", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "PTKNEG", "VVPP", "VVFIN", "$(", "KON", "PIS", "VVFIN", "APPR"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "steter Ruh.", "tokens": ["ste\u00b7ter", "Ruh", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Es hat, bey unsrer kurtzen Dauer und Fl\u00fcchtigkeit von un-", "tokens": ["Es", "hat", ",", "bey", "uns\u00b7rer", "kurt\u00b7zen", "Dau\u00b7er", "und", "Fl\u00fcch\u00b7tig\u00b7keit", "von", "un"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.24": {"text": "serm Leben,", "tokens": ["serm", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Der m\u00e4chtig-gut-und weise Sch\u00f6pfer zwey Mittel uns zum", "tokens": ["Der", "m\u00e4ch\u00b7tig\u00b7gut\u00b7\u00b7und", "wei\u00b7se", "Sch\u00f6p\u00b7fer", "zwey", "Mit\u00b7tel", "uns", "zum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "CARD", "NN", "PPER", "APPRART"], "meter": "-+-+-+-+--+-++", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "Trost gegeben,", "tokens": ["Trost", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Die kurtze Lust uns zu verl\u00e4ngern; er hat sie in uns selbst", "tokens": ["Die", "kurt\u00b7ze", "Lust", "uns", "zu", "ver\u00b7l\u00e4n\u00b7gern", ";", "er", "hat", "sie", "in", "uns", "selbst"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "$.", "PPER", "VAFIN", "PPER", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.28": {"text": "gesenckt;", "tokens": ["ge\u00b7senckt", ";"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.29": {"text": "Er legt die F\u00e4higkeit in uns, wenn man nur ordentlich", "tokens": ["Er", "legt", "die", "F\u00e4\u00b7hig\u00b7keit", "in", "uns", ",", "wenn", "man", "nur", "or\u00b7dent\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$,", "KOUS", "PIS", "ADV", "ADJD"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "gedenckt.", "tokens": ["ge\u00b7denckt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}}}}}