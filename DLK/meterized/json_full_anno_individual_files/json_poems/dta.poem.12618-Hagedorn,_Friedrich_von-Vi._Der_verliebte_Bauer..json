{"dta.poem.12618": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  Der verliebte Bauer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "N\u00fchmt mir des Schulzens Tochter nicht.", "tokens": ["N\u00fchmt", "mir", "des", "Schul\u00b7zens", "Toch\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nein! Sagt nur, sie ist reich.", "tokens": ["Nein", "!", "Sagt", "nur", ",", "sie", "ist", "reich", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Jm ganzen Dorf ist kein Gesicht", "tokens": ["Jm", "gan\u00b7zen", "Dorf", "ist", "kein", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der flinken Hanne gleich.", "tokens": ["Der", "flin\u00b7ken", "Han\u00b7ne", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Mensch gef\u00e4llt, auch ungeputzt;", "tokens": ["Das", "Mensch", "ge\u00b7f\u00e4llt", ",", "auch", "un\u00b7ge\u00b7putzt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich sag es ohne Scheu:", "tokens": ["Ich", "sag", "es", "oh\u00b7ne", "Scheu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Trotz mancher, die in Flittern stutzt;", "tokens": ["Trotz", "man\u00b7cher", ",", "die", "in", "Flit\u00b7tern", "stutzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie sey auch wer sie sey.", "tokens": ["Sie", "sey", "auch", "wer", "sie", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie frey und wei\u00df ist ihre Stirn", "tokens": ["Wie", "frey", "und", "wei\u00df", "ist", "ih\u00b7re", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und roth und frisch ihr Mund!", "tokens": ["Und", "roth", "und", "frisch", "ihr", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie glatt der Haarzopf meiner Dirn", "tokens": ["Wie", "glatt", "der", "Haar\u00b7zopf", "mei\u00b7ner", "Dirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Brust, wie rund!", "tokens": ["Und", "ih\u00b7re", "Brust", ",", "wie", "rund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jhr Aug ist schwarz wie reifer Schlee:", "tokens": ["Ihr", "Aug", "ist", "schwarz", "wie", "rei\u00b7fer", "Schlee", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schier komm ich auf den Wahn,", "tokens": ["Schier", "komm", "ich", "auf", "den", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wann ich ihr lang ins Auge seh,", "tokens": ["Wann", "ich", "ihr", "lang", "ins", "Au\u00b7ge", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sie hat mirs angethan.", "tokens": ["Sie", "hat", "mirs", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jhr wi\u00dft, wie wir im Rosenmond", "tokens": ["Ihr", "wi\u00dft", ",", "wie", "wir", "im", "Ro\u00b7sen\u00b7mond"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Meyen hier gepflanzt.", "tokens": ["Die", "Me\u00b7yen", "hier", "ge\u00b7pflanzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ward der F\u00fcsse nicht geschont,", "tokens": ["Da", "ward", "der", "F\u00fcs\u00b7se", "nicht", "ge\u00b7schont", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da hat sichs gnug getanzt.", "tokens": ["Da", "hat", "sichs", "gnug", "ge\u00b7tanzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Schaffers Tenne knarrte recht,", "tokens": ["Des", "Schaf\u00b7fers", "Ten\u00b7ne", "knarr\u00b7te", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir sch\u00e4kerten uns satt:", "tokens": ["Wir", "sch\u00e4\u00b7ker\u00b7ten", "uns", "satt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der H\u00fcfner Heinz und Hanns, der Knecht,", "tokens": ["Der", "H\u00fcf\u00b7ner", "Heinz", "und", "Hanns", ",", "der", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Hartwig aus der Stadt.", "tokens": ["Und", "Hart\u00b7wig", "aus", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Den Vorreihn, Nachbarn, lie\u00df man ihr:", "tokens": ["Den", "Vor\u00b7reihn", ",", "Nach\u00b7barn", ",", "lie\u00df", "man", "ihr", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flugs rief sie mich herbey.", "tokens": ["Flugs", "rief", "sie", "mich", "her\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Beym Element! wie flogen wir", "tokens": ["Beym", "E\u00b7le\u00b7ment", "!", "wie", "flo\u00b7gen", "wir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "PWAV", "VVFIN", "PPER"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Nach Kilians Schallmey.", "tokens": ["Nach", "Ki\u00b7li\u00b7ans", "Schall\u00b7mey", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wann Hanne nur in Schaukeln schwebt,", "tokens": ["Wann", "Han\u00b7ne", "nur", "in", "Schau\u00b7keln", "schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie muthig steigt ihr Schwung!", "tokens": ["Wie", "mut\u00b7hig", "steigt", "ihr", "Schwung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und wann sie sich im Tanzen hebt,", "tokens": ["Und", "wann", "sie", "sich", "im", "Tan\u00b7zen", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie sch\u00f6n ist jeder Sprung!", "tokens": ["Wie", "sch\u00f6n", "ist", "je\u00b7der", "Sprung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Allein, beym Kehraus glitzschte sie;", "tokens": ["Al\u00b7lein", ",", "beym", "Ke\u00b7hraus", "glitz\u00b7schte", "sie", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ich ergriff sie stracks;", "tokens": ["Doch", "ich", "er\u00b7griff", "sie", "stracks", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und daf\u00fcr sah ich auch ein Knie,", "tokens": ["Und", "da\u00b7f\u00fcr", "sah", "ich", "auch", "ein", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das war so wei\u00df als Wachs.", "tokens": ["Das", "war", "so", "wei\u00df", "als", "Wachs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des Pfarrers Muthe schimpft\u2019 aus Neid", "tokens": ["Des", "Pfar\u00b7rers", "Mu\u00b7the", "schimpft'", "aus", "Neid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zwackte mich gar an.", "tokens": ["Und", "zwack\u00b7te", "mich", "gar", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich sprach: Mensch, la\u00dft mich ungeheyt", "tokens": ["Ich", "sprach", ":", "Mensch", ",", "la\u00dft", "mich", "un\u00b7ge\u00b7heyt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "$,", "VVIMP", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und kneipt den Leyermann.", "tokens": ["Und", "kneipt", "den", "Le\u00b7yer\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Mein Liebchen ging mit mir ins Feld:", "tokens": ["Mein", "Lieb\u00b7chen", "ging", "mit", "mir", "ins", "Feld", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich half ihr \u00fcbern Zaun.", "tokens": ["Ich", "half", "ihr", "\u00fc\u00b7bern", "Zaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da hab ich mich nicht mehr verstellt,", "tokens": ["Da", "hab", "ich", "mich", "nicht", "mehr", "ver\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie war bey guter Laun.", "tokens": ["Sie", "war", "bey", "gu\u00b7ter", "Laun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wir lagerten uns drauf ins Gras,", "tokens": ["Wir", "la\u00b7ger\u00b7ten", "uns", "drauf", "ins", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "APPRART", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Wie Nachbars-Kinder thun;", "tokens": ["Wie", "Nach\u00b7bar\u00b7sKin\u00b7der", "thun", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch ich empfand ich wei\u00df nicht was,", "tokens": ["Doch", "ich", "emp\u00b7fand", "ich", "wei\u00df", "nicht", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "PTKNEG", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das lie\u00df mich gar nicht ruhn.", "tokens": ["Das", "lie\u00df", "mich", "gar", "nicht", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gnug, da\u00df sie mich ihr B\u00fcfchen hie\u00df,", "tokens": ["Gnug", ",", "da\u00df", "sie", "mich", "ihr", "B\u00fcfc\u00b7hen", "hie\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir Hand und Guschel reicht\u2019", "tokens": ["Mir", "Hand", "und", "Gu\u00b7schel", "reicht'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NE", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mir ein saftig Schm\u00e4tzchen lie\u00df,", "tokens": ["Und", "mir", "ein", "saf\u00b7tig", "Schm\u00e4tz\u00b7chen", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem auch der Most nicht gleicht.", "tokens": ["Dem", "auch", "der", "Most", "nicht", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Jhr schmutzelt? Denket was ihr wollt.", "tokens": ["Ihr", "schmut\u00b7zelt", "?", "Den\u00b7ket", "was", "ihr", "wollt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Glaubt, da\u00df sie euch nur neckt,", "tokens": ["Glaubt", ",", "da\u00df", "sie", "euch", "nur", "neckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und da\u00df ihr nicht erfahren sollt", "tokens": ["Und", "da\u00df", "ihr", "nicht", "er\u00b7fah\u00b7ren", "sollt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was Hannens Mieder deckt.", "tokens": ["Was", "Han\u00b7nens", "Mie\u00b7der", "deckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Edelfrau ist zart und fein;", "tokens": ["Die", "E\u00b7del\u00b7frau", "ist", "zart", "und", "fein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, 