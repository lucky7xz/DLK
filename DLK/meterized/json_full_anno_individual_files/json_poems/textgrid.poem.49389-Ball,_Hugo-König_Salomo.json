{"textgrid.poem.49389": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Salomo", "genre": "verse", "period": "N.A.", "pub_year": 1923, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als K\u00f6nig Salomo beim Tempelbau", "tokens": ["Als", "K\u00f6\u00b7nig", "Sa\u00b7lo\u00b7mo", "beim", "Tem\u00b7pel\u00b7bau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit den D\u00e4monen stritt, die ihn umsa\u00dfen,", "tokens": ["Mit", "den", "D\u00e4\u00b7mo\u00b7nen", "stritt", ",", "die", "ihn", "um\u00b7sa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lie\u00df er in Mittern\u00e4chten dumpf und grau", "tokens": ["Lie\u00df", "er", "in", "Mit\u00b7ter\u00b7n\u00e4ch\u00b7ten", "dumpf", "und", "grau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Zymbel schlagen und Posaune blasen.", "tokens": ["Die", "Zym\u00b7bel", "schla\u00b7gen", "und", "Po\u00b7sau\u00b7ne", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "An seiner Seite sah man eine Frau,", "tokens": ["An", "sei\u00b7ner", "Sei\u00b7te", "sah", "man", "ei\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die aufgebaut war ganz aus Parabasen,", "tokens": ["Die", "auf\u00b7ge\u00b7baut", "war", "ganz", "aus", "Pa\u00b7ra\u00b7ba\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus Saba kam sie wie ein wei\u00dfer Pfau", "tokens": ["Aus", "Sa\u00b7ba", "kam", "sie", "wie", "ein", "wei\u00b7\u00dfer", "Pfau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und stand wie eine Mumie in Exstasen.", "tokens": ["Und", "stand", "wie", "ei\u00b7ne", "Mu\u00b7mie", "in", "Ex\u00b7sta\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig selber sa\u00df in seinem Zelt,", "tokens": ["Der", "K\u00f6\u00b7nig", "sel\u00b7ber", "sa\u00df", "in", "sei\u00b7nem", "Zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Um dessen \u00d6ffnung Feuer hing in Fransen", "tokens": ["Um", "des\u00b7sen", "\u00d6ff\u00b7nung", "Feu\u00b7er", "hing", "in", "Fran\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wies gebietend in die Unterwelt.", "tokens": ["Und", "wies", "ge\u00b7bie\u00b7tend", "in", "die", "