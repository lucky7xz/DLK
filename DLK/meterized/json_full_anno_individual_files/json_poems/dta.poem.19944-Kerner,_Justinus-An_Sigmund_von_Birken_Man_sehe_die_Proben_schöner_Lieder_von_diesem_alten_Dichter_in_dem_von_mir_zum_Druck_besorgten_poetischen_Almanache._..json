{"dta.poem.19944": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An Sigmund von Birken   Man sehe die Proben sch\u00f6ner Lieder von diesem alten Dichter  \n in dem von mir zum Druck besorgten poetischen Almanache. .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dieses Wort des Danks zu Dir gelangen,", "tokens": ["La\u00df", "die\u00b7ses", "Wort", "des", "Danks", "zu", "Dir", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du sel'ger Meister! f\u00fcr die theuren Lieder!", "tokens": ["Du", "sel'\u00b7ger", "Meis\u00b7ter", "!", "f\u00fcr", "die", "theu\u00b7ren", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schwebtest in Lieb' in unsern Garten nieder,", "tokens": ["Schweb\u00b7test", "in", "Lieb'", "in", "un\u00b7sern", "Gar\u00b7ten", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Wo wir von Rosen, Wald und Sternen sangen.", "tokens": ["Wo", "wir", "von", "Ro\u00b7sen", ",", "Wald", "und", "Ster\u00b7nen", "san\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NE", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bekannte T\u00f6ne Dir entgegen klangen,", "tokens": ["Be\u00b7kann\u00b7te", "T\u00f6\u00b7ne", "Dir", "ent\u00b7ge\u00b7gen", "klan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weckten in Dir die alten Lieder wieder.", "tokens": ["Weck\u00b7ten", "in", "Dir", "die", "al\u00b7ten", "Lie\u00b7der", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Erkanntest uns als treue, deutsche Br\u00fcder,", "tokens": ["Er\u00b7kann\u00b7test", "uns", "als", "treu\u00b7e", ",", "deut\u00b7sche", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die tr\u00f6stend sich in gleichem Leid umfangen.", "tokens": ["Die", "tr\u00f6s\u00b7tend", "sich", "in", "glei\u00b7chem", "Leid", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vom sel'gen B\u00fcndni\u00df gleichgestimmter Geister,", "tokens": ["Vom", "sel'\u00b7gen", "B\u00fcn\u00b7dni\u00df", "gleich\u00b7ges\u00b7timm\u00b7ter", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von des gepre\u00dften Vaterlands Beschwerde,", "tokens": ["Von", "des", "ge\u00b7pre\u00df\u00b7ten", "Va\u00b7ter\u00b7lands", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Kraft durch Hoffnung hat dein Lied gesungen.", "tokens": ["Von", "Kraft", "durch", "Hoff\u00b7nung", "hat", "dein", "Lied", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie bist du uns willkommen, sel'ger Meister!", "tokens": ["Wie", "bist", "du", "uns", "will\u00b7kom\u00b7men", ",", "sel'\u00b7ger", "Meis\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerrissen liegt und kalt die deutsche Erde;", "tokens": ["Zer\u00b7ris\u00b7sen", "liegt", "und", "kalt", "die", "deut\u00b7sche", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Deutscher Gesang nur h\u00e4lt uns treu umschlungen.", "tokens": ["Deut\u00b7scher", "Ge\u00b7sang", "nur", "h\u00e4lt", "uns", "treu", "um\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}