, "von", "der", "Ge\u00b7lieb\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ew'ge Kr\u00e4fte, Zeit und Ferne,", "tokens": ["Ew'\u00b7ge", "Kr\u00e4f\u00b7te", ",", "Zeit", "und", "Fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heimlich wie die Kraft der Sterne,", "tokens": ["Heim\u00b7lich", "wie", "die", "Kraft", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wiegen dieses Blut zur Ruh.", "tokens": ["Wie\u00b7gen", "die\u00b7ses", "Blut", "zur", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein Gef\u00fchl wird stets erweichter;", "tokens": ["Mein", "Ge\u00b7f\u00fchl", "wird", "stets", "er\u00b7weich\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch mein Herz wird t\u00e4glich leichter,", "tokens": ["Doch", "mein", "Herz", "wird", "t\u00e4g\u00b7lich", "leich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mein Gl\u00fcck nimmt immer zu.", "tokens": ["Und", "mein", "Gl\u00fcck", "nimmt", "im\u00b7mer", "zu", "."], "token_info": ["word", "word", "word