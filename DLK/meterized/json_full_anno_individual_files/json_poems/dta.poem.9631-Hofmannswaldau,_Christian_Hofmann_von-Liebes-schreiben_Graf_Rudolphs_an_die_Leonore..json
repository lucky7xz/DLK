{"dta.poem.9631": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Liebes-schreiben Graf Rudolphs an  \n die Leonore.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein blat/ das von der lust der liebe fast geschwollen/", "tokens": ["Ein", "blat", "/", "das", "von", "der", "lust", "der", "lie\u00b7be", "fast", "ge\u00b7schwol\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "ART", "NN", "ART", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An dem ich armer selbst das feste siegel bin/", "tokens": ["An", "dem", "ich", "ar\u00b7mer", "selbst", "das", "fes\u00b7te", "sie\u00b7gel", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "ADV", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und was ich sonsten kan in diesen zedel rollen/", "tokens": ["Und", "was", "ich", "sons\u00b7ten", "kan", "in", "die\u00b7sen", "ze\u00b7del", "rol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VMFIN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "F\u00e4llt/ Leonore/ dir zu demen f\u00fcssen hin.", "tokens": ["F\u00e4llt", "/", "Le\u00b7o\u00b7no\u00b7re", "/", "dir", "zu", "de\u00b7men", "f\u00fcs\u00b7sen", "hin", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NE", "$(", "PPER", "PTKZU", "VVINF", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht frage was es will: mein hertze steht dir offen/", "tokens": ["Nicht", "fra\u00b7ge", "was", "es", "will", ":", "mein", "hert\u00b7ze", "steht", "dir", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PWS", "PPER", "VMFIN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Komm schaue di\u00df nur selbst mit frischen augen an?", "tokens": ["Komm", "schau\u00b7e", "di\u00df", "nur", "selbst", "mit", "fri\u00b7schen", "au\u00b7gen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PDS", "ADV", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drum la\u00df ihm auch ein theil von deiner wehmuth hoffen/", "tokens": ["Drum", "la\u00df", "ihm", "auch", "ein", "theil", "von", "dei\u00b7ner", "weh\u00b7muth", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dencke/ da\u00df ich itzt nicht anders schreiben kan.", "tokens": ["Und", "den\u00b7cke", "/", "da\u00df", "ich", "itzt", "nicht", "an\u00b7ders", "schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich darff nun weiter nicht geschminckte worte f\u00fchren/", "tokens": ["Ich", "darff", "nun", "wei\u00b7ter", "nicht", "ge\u00b7schminck\u00b7te", "wor\u00b7te", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PTKNEG", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie purpur und scarlat von deinen lippen lacht:", "tokens": ["Wie", "pur\u00b7pur", "und", "scar\u00b7lat", "von", "dei\u00b7nen", "lip\u00b7pen", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Wie deiner augen blitz kan alle glieder r\u00fchren/", "tokens": ["Wie", "dei\u00b7ner", "au\u00b7gen", "blitz", "kan", "al\u00b7le", "glie\u00b7der", "r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NE", "VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und sterne selber offt zu blassen fackeln macht.", "tokens": ["Und", "ster\u00b7ne", "sel\u00b7ber", "offt", "zu", "blas\u00b7sen", "fa\u00b7ckeln", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die sch\u00f6nheit l\u00e4st sich nicht in enge federn zwingen/", "tokens": ["Die", "sch\u00f6n\u00b7heit", "l\u00e4st", "sich", "nicht", "in", "en\u00b7ge", "fe\u00b7dern", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein schimmrend antlitz ist vor pinsel auch zu gro\u00df:", "tokens": ["Ein", "schimm\u00b7rend", "ant\u00b7litz", "ist", "vor", "pin\u00b7sel", "auch", "zu", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VAFIN", "APPR", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gnug/ da\u00df die geister noch durch alle nieren dringen/", "tokens": ["Gnug", "/", "da\u00df", "die", "geis\u00b7ter", "noch", "durch", "al\u00b7le", "nie\u00b7ren", "drin\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "ART", "ADJA", "ADV", "APPR", "PIAT", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Durch die der liebe strom in meine lenden scho\u00df.", "tokens": ["Durch", "die", "der", "lie\u00b7be", "strom", "in", "mei\u00b7ne", "len\u00b7den", "scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der schmertz hat der gedult den z\u00fcgel abgerissen/", "tokens": ["Der", "schmertz", "hat", "der", "ge\u00b7dult", "den", "z\u00fc\u00b7gel", "ab\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich schliesse mich nicht mehr in blinde circkel ein:", "tokens": ["Ich", "schlies\u00b7se", "mich", "nicht", "mehr", "in", "blin\u00b7de", "cir\u00b7ckel", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und wilt du noch ein wort von deinem Rudolph wissen;", "tokens": ["Und", "wilt", "du", "noch", "ein", "wort", "von", "dei\u00b7nem", "Ru\u00b7dolph", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er will dein schlaff-geno\u00df und auch dein liebster seyn.", "tokens": ["Er", "will", "dein", "schlaff\u00b7ge\u00b7no\u00df", "und", "auch", "dein", "liebs\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "VVFIN", "KON", "ADV", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nicht t\u00e4dle vor der zeit das l\u00fcstrende beginnen/", "tokens": ["Nicht", "t\u00e4d\u00b7le", "vor", "der", "zeit", "das", "l\u00fcst\u00b7ren\u00b7de", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der kitzel/ der mich sticht/ ist deiner liebe kind.", "tokens": ["Der", "kit\u00b7zel", "/", "der", "mich", "sticht", "/", "ist", "dei\u00b7ner", "lie\u00b7be", "kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "VVFIN", "$(", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wie soll ich armer denn auff fremde mittel sinnen/", "tokens": ["Wie", "soll", "ich", "ar\u00b7mer", "denn", "auff", "frem\u00b7de", "mit\u00b7tel", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJA", "KON", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil man die beste kost doch bey der mutter findt.", "tokens": ["Weil", "man", "die", "bes\u00b7te", "kost", "doch", "bey", "der", "mut\u00b7ter", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "VVFIN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du l\u00e4st ja selber glut zu meinen ampeln schiessen/", "tokens": ["Du", "l\u00e4st", "ja", "sel\u00b7ber", "glut", "zu", "mei\u00b7nen", "am\u00b7peln", "schies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dein brennend \u00f6le flammt auch meine kertzen an:", "tokens": ["Dein", "bren\u00b7nend", "\u00f6\u00b7le", "flammt", "auch", "mei\u00b7ne", "kert\u00b7zen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So la\u00df denn auch den thau um meine glieder fliessen/", "tokens": ["So", "la\u00df", "denn", "auch", "den", "thau", "um", "mei\u00b7ne", "glie\u00b7der", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Der durch beperlte krafft die flammen k\u00fchlen kan.", "tokens": ["Der", "durch", "be\u00b7perl\u00b7te", "krafft", "die", "flam\u00b7men", "k\u00fch\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "VVINF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die liebe l\u00e4st sich nicht durch stumme seuffzer d\u00e4mpffen/", "tokens": ["Die", "lie\u00b7be", "l\u00e4st", "sich", "nicht", "durch", "stum\u00b7me", "seuff\u00b7zer", "d\u00e4mpf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "PTKNEG", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ein abgeschmackter ku\u00df f\u00fchrt schlechte liebligkeit:", "tokens": ["Ein", "ab\u00b7ge\u00b7schmack\u00b7ter", "ku\u00df", "f\u00fchrt", "schlech\u00b7te", "lieb\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und wer sich selber will durch stille glut bek\u00e4mpffen/", "tokens": ["Und", "wer", "sich", "sel\u00b7ber", "will", "durch", "stil\u00b7le", "glut", "be\u00b7k\u00e4mpf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADV", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hat feuer zwar gesch\u00f6pfft/ nicht aber ausgestreut.", "tokens": ["Hat", "feu\u00b7er", "zwar", "ge\u00b7sch\u00f6pfft", "/", "nicht", "a\u00b7ber", "aus\u00b7ge\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$(", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Darffst du doch nicht die bahn durch deinen fehler brechen/", "tokens": ["Darffst", "du", "doch", "nicht", "die", "bahn", "durch", "dei\u00b7nen", "feh\u00b7ler", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die mutter hat es schon vor diesem auch gethan/", "tokens": ["Die", "mut\u00b7ter", "hat", "es", "schon", "vor", "die\u00b7sem", "auch", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und Eva schaute selbst/ ohn alles widersprechen/", "tokens": ["Und", "E\u00b7va", "schau\u00b7te", "selbst", "/", "ohn", "al\u00b7les", "wi\u00b7der\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$(", "APPR", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das kinder-bette vor/ als ihre hochzeit/ an.", "tokens": ["Das", "kin\u00b7der\u00b7bet\u00b7te", "vor", "/", "als", "ih\u00b7re", "hoch\u00b7zeit", "/", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$(", "KOKOM", "PPOSAT", "NN", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die freuden-feste sind nur menschliche gesetze/", "tokens": ["Die", "freu\u00b7den\u00b7fes\u00b7te", "sind", "nur", "menschli\u00b7che", "ge\u00b7set\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.38": {"text": "Da man die g\u00e4ste satt/ den priester reicher macht.", "tokens": ["Da", "man", "die", "g\u00e4s\u00b7te", "satt", "/", "den", "pries\u00b7ter", "rei\u00b7cher", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "$(", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was hat der p\u00f6bel nicht durch flattrendes geschw\u00e4tze", "tokens": ["Was", "hat", "der", "p\u00f6\u00b7bel", "nicht", "durch", "flatt\u00b7ren\u00b7des", "ge\u00b7schw\u00e4t\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Vor missethaten offt aus reiner lust erdacht?", "tokens": ["Vor", "mis\u00b7se\u00b7tha\u00b7ten", "offt", "aus", "rei\u00b7ner", "lust", "er\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "