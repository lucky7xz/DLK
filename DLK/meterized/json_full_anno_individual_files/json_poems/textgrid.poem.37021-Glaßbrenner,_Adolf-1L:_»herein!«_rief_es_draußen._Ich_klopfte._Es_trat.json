{"textgrid.poem.37021": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbherein!\u00ab rief es drau\u00dfen. Ich klopfte. Es trat", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbherein!\u00ab rief es drau\u00dfen. Ich klopfte. Es trat", "tokens": ["\u00bb", "her\u00b7ein", "!", "\u00ab", "rief", "es", "drau\u00b7\u00dfen", ".", "Ich", "klopf\u00b7te", ".", "Es", "trat"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PTKVZ", "$.", "$(", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "In mein Zimmer mit freundlichen Mienen", "tokens": ["In", "mein", "Zim\u00b7mer", "mit", "freund\u00b7li\u00b7chen", "Mie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ein Wesen, halb Mensch und halb Kranich, und bat", "tokens": ["Ein", "We\u00b7sen", ",", "halb", "Mensch", "und", "halb", "Kra\u00b7nich", ",", "und", "bat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "NN", "KON", "ADJD", "ADJD", "$,", "KON", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Mich, mich seines Talents zu bedienen.", "tokens": ["Mich", ",", "mich", "sei\u00b7nes", "Ta\u00b7lents", "zu", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "\u00bbihre Kinder vom Leiden der Kunst zu befrei'n,", "tokens": ["\u00bb", "ih\u00b7re", "Kin\u00b7der", "vom", "Lei\u00b7den", "der", "Kunst", "zu", "be\u00b7frei'n", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "D\u00fcrften Wen'ge wie ich so bef\u00e4higet sein.\u00ab", "tokens": ["D\u00fcrf\u00b7ten", "Wen'\u00b7ge", "wie", "ich", "so", "be\u00b7f\u00e4\u00b7hi\u00b7get", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "KOKOM", "PPER", "ADV", "VVPP", "VAINF", "$.", "$("], "meter": "+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "\u00bbich bin\u00ab, so sprach er, \u00bbein hehrer", "tokens": ["\u00bb", "ich", "bin", "\u00ab", ",", "so", "sprach", "er", ",", "\u00bb", "ein", "heh\u00b7rer"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "$(", "$,", "ADV", "VVFIN", "PPER", "$,", "$(", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Pianoforte-Verlehrer.\u00ab", "tokens": ["Pi\u00b7a\u00b7no\u00b7for\u00b7te\u00b7Ver\u00b7leh\u00b7rer", ".", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Mir von Lotten, der Gr\u00e4fin, erkl\u00e4ren,", "tokens": ["Mir", "von", "Lot\u00b7ten", ",", "der", "Gr\u00e4\u00b7fin", ",", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "ART", "NN", "$,", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da\u00df die M\u00fctter hier allesammt lauter Genie's", "tokens": ["Da\u00df", "die", "M\u00fct\u00b7ter", "hier", "al\u00b7le\u00b7sammt", "lau\u00b7ter", "Ge\u00b7ni\u00b7e's"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "PIAT", "NN"], "meter": "--+--+-++-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "F\u00fcr das Fortepiano geb\u00e4ren!", "tokens": ["F\u00fcr", "das", "For\u00b7te\u00b7pi\u00b7a\u00b7no", "ge\u00b7b\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df der S\u00e4ugling Sonaten, die schwierigsten, schon", "tokens": ["Da\u00df", "der", "S\u00e4ug\u00b7ling", "So\u00b7na\u00b7ten", ",", "die", "schwie\u00b7rigs\u00b7ten", ",", "schon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "PRELS", "VVFIN", "$,", "ADV"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Mit Bravour und mit technischer Perfektion,", "tokens": ["Mit", "Bra\u00b7vour", "und", "mit", "tech\u00b7ni\u00b7scher", "Per\u00b7fek\u00b7ti\u00b7on", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch ehe \u00bbMamachen\u00ab er plappert", "tokens": ["Noch", "e\u00b7he", "\u00bb", "Ma\u00b7ma\u00b7chen", "\u00ab", "er", "plap\u00b7pert"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "KOUS", "$(", "NN", "$(", "PPER", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ja, da\u00df Manche, den Beutel des Lutsch's noch im Mund,", "tokens": ["Ja", ",", "da\u00df", "Man\u00b7che", ",", "den", "Beu\u00b7tel", "des", "Lut\u00b7sch's", "noch", "im", "Mund", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PIS", "$,", "ART", "NN", "ART", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schon sehr geistvoll und tief phantasirten,", "tokens": ["Schon", "sehr", "geist\u00b7voll", "und", "tief", "phan\u00b7ta\u00b7sir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Ja, sogar Generalba\u00df durchgr\u00fcbelten und", "tokens": ["Ja", ",", "so\u00b7gar", "Ge\u00b7ne\u00b7ral\u00b7ba\u00df", "durch\u00b7gr\u00fc\u00b7bel\u00b7ten", "und"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "NN", "VVFIN", "KON"], "meter": "+-+---+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "In den Windeln bereits componirten!", "tokens": ["In", "den", "Win\u00b7deln", "be\u00b7reits", "com\u00b7po\u00b7nir\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Und da\u00df, falls man ihnen entzieht das Klavier,", "tokens": ["Und", "da\u00df", ",", "falls", "man", "ih\u00b7nen", "ent\u00b7zieht", "das", "Kla\u00b7vier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "KOUS", "PIS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die armen, geniekranken W\u00fcrmerchen schier", "tokens": ["Die", "ar\u00b7men", ",", "ge\u00b7nie\u00b7kran\u00b7ken", "W\u00fcr\u00b7mer\u00b7chen", "schier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "ADJD"], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wie Rasende strampeln und trappeln,", "tokens": ["Wie", "Ra\u00b7sen\u00b7de", "stram\u00b7peln", "und", "trap\u00b7peln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und balde zu Tode sich zappeln!", "tokens": ["Und", "bal\u00b7de", "zu", "To\u00b7de", "sich", "zap\u00b7peln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "PRF", "VVINF", "$."], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und so sei denn in keinem Familienkreis", "tokens": ["Und", "so", "sei", "denn", "in", "kei\u00b7nem", "Fa\u00b7mi\u00b7li\u00b7en\u00b7kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Verlehrer allhier zu entbehren,", "tokens": ["Der", "Ver\u00b7leh\u00b7rer", "all\u00b7hier", "zu", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Der, die Kinder zu retten, mit Sorgfalt und Flei\u00df", "tokens": ["Der", ",", "die", "Kin\u00b7der", "zu", "ret\u00b7ten", ",", "mit", "Sorg\u00b7falt", "und", "Flei\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "ART", "NN", "PTKZU", "VVINF", "$,", "APPR", "NN", "KON", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Ihre Kunst sie verlernen mu\u00df lehren,", "tokens": ["Ih\u00b7re", "Kunst", "sie", "ver\u00b7ler\u00b7nen", "mu\u00df", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVINF", "VMFIN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Auf da\u00df sie, anstrebend ein n\u00fctzliches Ziel,", "tokens": ["Auf", "da\u00df", "sie", ",", "an\u00b7stre\u00b7bend", "ein", "n\u00fctz\u00b7li\u00b7ches", "Ziel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Sich selbst nicht verzehren im tobenden Spiel,", "tokens": ["Sich", "selbst", "nicht", "ver\u00b7zeh\u00b7ren", "im", "to\u00b7ben\u00b7den", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "VVINF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und nicht martern die lieben Verwandten", "tokens": ["Und", "nicht", "mar\u00b7tern", "die", "lie\u00b7ben", "Ver\u00b7wand\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Und alle die guten Bekannten.", "tokens": ["Und", "al\u00b7le", "die", "gu\u00b7ten", "Be\u00b7kann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Ich lachte hell auf und erkl\u00e4rte dabei", "tokens": ["Ich", "lach\u00b7te", "hell", "auf", "und", "er\u00b7kl\u00e4r\u00b7te", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN", "PAV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Des Meisters der Umkehr-Kunst Bitte", "tokens": ["Des", "Meis\u00b7ters", "der", "Um\u00b7kehr\u00b7Kunst", "Bit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Als nicht hier am Platz, da ich kinderlos sei", "tokens": ["Als", "nicht", "hier", "am", "Platz", ",", "da", "ich", "kin\u00b7der\u00b7los", "sei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "ADJD", "VAFIN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und Selbst am Piano nicht litte.", "tokens": ["Und", "Selbst", "am", "Pi\u00b7a\u00b7no", "nicht", "lit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NE", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Denn da\u00df ich mit Einem der Finger der Hand", "tokens": ["Denn", "da\u00df", "ich", "mit", "Ei\u00b7nem", "der", "Fin\u00b7ger", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PIS", "ART", "NN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Arndt's deutsches, stets fragliches Vaterland", "tokens": ["Arndt's", "deut\u00b7sches", ",", "stets", "frag\u00b7li\u00b7ches", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "$,", "ADV", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": 