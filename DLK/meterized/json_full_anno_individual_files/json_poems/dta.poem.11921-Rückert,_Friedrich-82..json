{"dta.poem.11921": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was gut ist und was schlecht, ist schwer nicht zu entscheiden;", "tokens": ["Was", "gut", "ist", "und", "was", "schlecht", ",", "ist", "schwer", "nicht", "zu", "ent\u00b7schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "PWS", "VVFIN", "$,", "VAFIN", "ADJD", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch unentschieden schwankt viel andres zwischen beiden.", "tokens": ["Doch", "un\u00b7ent\u00b7schie\u00b7den", "schwankt", "viel", "and\u00b7res", "zwi\u00b7schen", "bei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "PIS", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Gute zieht mich an, das Schlechte widerw\u00e4rtig", "tokens": ["Das", "Gu\u00b7te", "zieht", "mich", "an", ",", "das", "Schlech\u00b7te", "wi\u00b7der\u00b7w\u00e4r\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "St\u00f6\u00dft schnell mich ab, und leicht bin ich mit beiden fertig.", "tokens": ["St\u00f6\u00dft", "schnell", "mich", "ab", ",", "und", "leicht", "bin", "ich", "mit", "bei\u00b7den", "fer\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$,", "KON", "ADJD", "VAFIN", "PPER", "APPR", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Zweifelhafte nur macht langes Kopfzerbrechen,", "tokens": ["Das", "Zwei\u00b7fel\u00b7haf\u00b7te", "nur", "macht", "lan\u00b7ges", "Kopf\u00b7zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis man zu Stande kommt ein Urtheil ihm zu sprechen,", "tokens": ["Bis", "man", "zu", "Stan\u00b7de", "kommt", "ein", "Ur\u00b7theil", "ihm", "zu", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das ich nach meinem Recht am Ende so entscheide:", "tokens": ["Das", "ich", "nach", "mei\u00b7nem", "Recht", "am", "En\u00b7de", "so", "ent\u00b7schei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PPOSAT", "NN", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was weder gut noch schlecht, ist schlechter mir als beide.", "tokens": ["Was", "we\u00b7der", "gut", "noch", "schlecht", ",", "ist", "schlech\u00b7ter", "mir", "als", "bei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KON", "ADJD", "ADV", "ADJD", "$,", "VAFIN", "ADJD", "PPER", "KOUS", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}