{"textgrid.poem.58643": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "75.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott wirkt zwar alles gern in dir, doch wenn dies soll geschehen,", "tokens": ["Gott", "wirkt", "zwar", "al\u00b7les", "gern", "in", "dir", ",", "doch", "wenn", "dies", "soll", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PIS", "ADV", "APPR", "PPER", "$,", "ADV", "KOUS", "PDS", "VMFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Will er auch Willigkeit und Treu von deiner Seite sehen.", "tokens": ["Will", "er", "auch", "Wil\u00b7lig\u00b7keit", "und", "Treu", "von", "dei\u00b7ner", "Sei\u00b7te", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Wenn man zu Gott sich kehren ", "tokens": ["Wenn", "man", "zu", "Gott", "sich", "keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Halt deine Lust nur abgesp\u00e4nt, sei still und warte dann!", "tokens": ["Halt", "dei\u00b7ne", "Lust", "nur", "ab\u00b7ge\u00b7sp\u00e4nt", ",", "sei", "still", "und", "war\u00b7te", "dann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "VVPP", "$,", "VAFIN", "ADJD", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Ich kehr' zu dir, Herr, wie ich kann, doch mu\u00df das rechte Kehren", "tokens": ["Ich", "kehr'", "zu", "dir", ",", "Herr", ",", "wie", "ich", "kann", ",", "doch", "mu\u00df", "das", "rech\u00b7te", "Keh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Dein innrer Zug und Gegenwart selbst wirken und mich lehren.", "tokens": ["Dein", "inn\u00b7rer", "Zug", "und", "Ge\u00b7gen\u00b7wart", "selbst", "wir\u00b7ken", "und", "mich", "leh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "ADV", "VVINF", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Ach, werd im Geist mir offenbar, wenn dieses nur geschicht,", "tokens": ["Ach", ",", "werd", "im", "Geist", "mir", "of\u00b7fen\u00b7bar", ",", "wenn", "die\u00b7ses", "nur", "ge\u00b7schicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "APPRART", "NN", "PPER", "ADJD", "$,", "KOUS", "PDS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "So kann ich eingekehret stehn vor deinem Angesicht!", "tokens": ["So", "kann", "ich", "ein\u00b7ge\u00b7keh\u00b7ret", "stehn", "vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVPP", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "i