{"textgrid.poem.55041": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Bergschlo\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da droben auf jenem Berge,", "tokens": ["Da", "dro\u00b7ben", "auf", "je\u00b7nem", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da steht ein altes Schlo\u00df,", "tokens": ["Da", "steht", "ein", "al\u00b7tes", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo hinter Toren und T\u00fcren", "tokens": ["Wo", "hin\u00b7ter", "To\u00b7ren", "und", "T\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sonst lauerten Ritter und Ro\u00df.", "tokens": ["Sonst", "lau\u00b7er\u00b7ten", "Rit\u00b7ter", "und", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Verbrannt sind T\u00fcren und Tore,", "tokens": ["Ver\u00b7brannt", "sind", "T\u00fc\u00b7ren", "und", "To\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "NN", "KON", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und \u00fcberall ist es so still;", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "ist", "es", "so", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das alte, verfallne Gem\u00e4uer", "tokens": ["Das", "al\u00b7te", ",", "ver\u00b7fall\u00b7ne", "Ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Durchklettr' ich, wie ich nur will.", "tokens": ["Durch\u00b7klett\u00b7r'", "ich", ",", "wie", "ich", "nur", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "PWAV", "PPER", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hierneben lag ein Keller,", "tokens": ["Hier\u00b7ne\u00b7ben", "lag", "ein", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "So voll von k\u00f6stlichem Wein;", "tokens": ["So", "voll", "von", "k\u00f6st\u00b7li\u00b7chem", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nun steiget nicht mehr mit Kr\u00fcgen", "tokens": ["Nun", "stei\u00b7get", "nicht", "mehr", "mit", "Kr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "APPR", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Kellnerin heiter hinein.", "tokens": ["Die", "Kell\u00b7ne\u00b7rin", "hei\u00b7ter", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Sie setzt den G\u00e4sten im Saale", "tokens": ["Sie", "setzt", "den", "G\u00e4s\u00b7ten", "im", "Saa\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht mehr die Becher umher,", "tokens": ["Nicht", "mehr", "die", "Be\u00b7cher", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie f\u00fcllt zum heiligen Mahle", "tokens": ["Sie", "f\u00fcllt", "zum", "hei\u00b7li\u00b7gen", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem Pfaffen das Fl\u00e4schchen nicht mehr.", "tokens": ["Dem", "Pfaf\u00b7fen", "das", "Fl\u00e4\u00b7schchen", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Sie reicht dem l\u00fcsternen Knappen", "tokens": ["Sie", "reicht", "dem", "l\u00fcs\u00b7ter\u00b7nen", "Knap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht mehr auf dem Gange den Trank", "tokens": ["Nicht", "mehr", "auf", "dem", "Gan\u00b7ge", "den", "Trank"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und nimmt f\u00fcr fl\u00fcchtige Gabe", "tokens": ["Und", "nimmt", "f\u00fcr", "fl\u00fcch\u00b7ti\u00b7ge", "Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nicht mehr den fl\u00fcchtigen Dank.", "tokens": ["Nicht", "mehr", "den", "fl\u00fcch\u00b7ti\u00b7gen", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Denn alle Balken und Decken,", "tokens": ["Denn", "al\u00b7le", "Bal\u00b7ken", "und", "De\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Sie sind schon lange verbrannt,", "tokens": ["Sie", "sind", "schon", "lan\u00b7ge", "ver\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und Trepp und Gang und Kapelle", "tokens": ["Und", "Trepp", "und", "Gang", "und", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In Schutt und Tr\u00fcmmer verwandt.", "tokens": ["In", "Schutt", "und", "Tr\u00fcm\u00b7mer", "ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Doch als mit Zither und Flasche", "tokens": ["Doch", "als", "mit", "Zi\u00b7ther", "und", "Fla\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nach diesen felsigen H\u00f6hn", "tokens": ["Nach", "die\u00b7sen", "fel\u00b7si\u00b7gen", "H\u00f6hn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Ich an dem heitersten Tage", "tokens": ["Ich", "an", "dem", "hei\u00b7ters\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mein Liebchen steigen gesehn,", "tokens": ["Mein", "Lieb\u00b7chen", "stei\u00b7gen", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Da dr\u00e4ngte sich frohes Behagen", "tokens": ["Da", "dr\u00e4ng\u00b7te", "sich", "fro\u00b7hes", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Hervor aus ver\u00f6deter Ruh,", "tokens": ["Her\u00b7vor", "aus", "ver\u00b7\u00f6\u00b7de\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da ging's wie in alten Tagen", "tokens": ["Da", "ging's", "wie", "in", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Recht feierlich wieder zu.", "tokens": ["Recht", "fei\u00b7er\u00b7lich", "wie\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Als w\u00e4ren f\u00fcr stattliche G\u00e4ste", "tokens": ["Als", "w\u00e4\u00b7ren", "f\u00fcr", "statt\u00b7li\u00b7che", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die weitesten R\u00e4ume bereit,", "tokens": ["Die", "wei\u00b7tes\u00b7ten", "R\u00e4u\u00b7me", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Als k\u00e4m ein P\u00e4rchen gegangen", "tokens": ["Als", "k\u00e4m", "ein", "P\u00e4r\u00b7chen", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aus jener t\u00fcchtigen Zeit.", "tokens": ["Aus", "je\u00b7ner", "t\u00fcch\u00b7ti\u00b7gen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Als st\u00fcnd in seiner Kapelle", "tokens": ["Als", "st\u00fcnd", "in", "sei\u00b7ner", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der w\u00fcrdige Pfaffe schon da", "tokens": ["Der", "w\u00fcr\u00b7di\u00b7ge", "Pfaf\u00b7fe", "schon", "da"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und fragte: Wollt ihr einander?", "tokens": ["Und", "frag\u00b7te", ":", "Wollt", "ihr", "ein\u00b7an\u00b7der", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VMFIN", "PPER", "PRF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wir aber l\u00e4chelten: Ja!", "tokens": ["Wir", "a\u00b7ber", "l\u00e4\u00b7chel\u00b7ten", ":", "Ja", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$.", "PTKANT", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Und tief bewegten Ges\u00e4nge", "tokens": ["Und", "tief", "be\u00b7weg\u00b7ten", "Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Des Herzens innigsten Grund,", "tokens": ["Des", "Her\u00b7zens", "in\u00b7nigs\u00b7ten", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es zeugte statt der Menge", "tokens": ["Es", "zeug\u00b7te", "statt", "der", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Echo schallender Mund.", "tokens": ["Der", "E\u00b7cho", "schal\u00b7len\u00b7der", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Und als sich gegen den Abend", "tokens": ["Und", "als", "sich", "ge\u00b7gen", "den", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Im stillen alles verlor,", "tokens": ["Im", "stil\u00b7len", "al\u00b7les", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PIS", "VVFIN", "$,