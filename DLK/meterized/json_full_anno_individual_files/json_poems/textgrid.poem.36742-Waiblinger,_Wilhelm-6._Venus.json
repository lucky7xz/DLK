{"textgrid.poem.36742": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "6. Venus", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' es gewi\u00df, und h\u00e4ttest du nur dem entz\u00fcckenden Leibe", "tokens": ["W\u00e4r'", "es", "ge\u00b7wi\u00df", ",", "und", "h\u00e4t\u00b7test", "du", "nur", "dem", "ent\u00b7z\u00fc\u00b7cken\u00b7den", "Lei\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Seine Gew\u00e4nder verlieh'n, weil dir die Schaam es gebot,", "tokens": ["Sei\u00b7ne", "Ge\u00b7w\u00e4n\u00b7der", "ver\u00b7lieh'n", ",", "weil", "dir", "die", "Schaam", "es", "ge\u00b7bot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Dann verehrt ich sie fast als H\u00f6chstes, doch leider bef\u00fcrcht' ich,", "tokens": ["Dann", "ver\u00b7ehrt", "ich", "sie", "fast", "als", "H\u00f6chs\u00b7tes", ",", "doch", "lei\u00b7der", "be\u00b7f\u00fcrcht'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "KOUS", "NN", "$,", "ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da\u00df du die Lust nur nach dem, was sie verbergen, erzielst.", "tokens": ["Da\u00df", "du", "die", "Lust", "nur", "nach", "dem", ",", "was", "sie", "ver\u00b7ber\u