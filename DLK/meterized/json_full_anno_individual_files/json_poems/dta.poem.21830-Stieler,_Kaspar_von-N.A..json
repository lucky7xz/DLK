{"dta.poem.21830": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Weg Kato/ Kurius/ nu habt ihr satt gelesen/", "tokens": ["Weg", "Ka\u00b7to", "/", "Ku\u00b7rius", "/", "nu", "habt", "ihr", "satt", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "NE", "$(", "ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "was hiernegst folget/ ist vor eine muntre Stirn/", "tokens": ["was", "hier\u00b7negst", "fol\u00b7get", "/", "ist", "vor", "ei\u00b7ne", "mun\u00b7tre", "Stirn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$(", "VAFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Spiel und Scherz verstehet", "tokens": ["die", "Spiel", "und", "Scherz", "ver\u00b7ste\u00b7het"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und nicht zu ernstlich gehet.", "tokens": ["und", "nicht", "zu", "ernst\u00b7lich", "ge\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Regul welch\u2019 ersann Fabrizius Gehirn", "tokens": ["Die", "Re\u00b7gul", "welch'", "er\u00b7sann", "Fab\u00b7ri\u00b7zius", "Ge\u00b7hirn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PWAT", "NN", "NE", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "ist memer Jugend Form und Richtschnur nie gewe-", "tokens": ["ist", "me\u00b7mer", "Ju\u00b7gend", "Form", "und", "Richt\u00b7schnur", "nie", "ge\u00b7we"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "NN", "KON", "NN", "ADV", "TRUNC"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "sen.", "tokens": ["sen."], "token_info": ["abbreviation"], "pos": ["XY"]}, "line.8": {"text": "Di\u00df Zehen bleibet mein/", "tokens": ["Di\u00df", "Ze\u00b7hen", "blei\u00b7bet", "mein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "CARD", "VVFIN", "PPOSAT", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "auch schreib\u2019 ichs mir allein.", "tokens": ["auch", "schreib'", "ichs", "mir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}