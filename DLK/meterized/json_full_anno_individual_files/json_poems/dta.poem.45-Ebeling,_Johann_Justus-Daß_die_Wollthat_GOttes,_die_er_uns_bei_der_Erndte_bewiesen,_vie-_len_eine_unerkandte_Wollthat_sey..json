s Gem\u00fct,", "tokens": ["Und", "sein", "ver\u00b7dor\u00b7be\u00b7nes", ",", "un\u00b7ar\u00b7ti\u00b7ges", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.112": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.113": {"text": "Die Erndte wenn sie reich; macht viele gar zu satt,", "tokens": ["Die", "Ernd\u00b7te", "wenn", "sie", "reich", ";", "macht", "vie\u00b7le", "gar", "zu", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOUS", "PPER", "ADJD", "$.", "VVFIN", "PIS", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.114": {"text": "Da\u00df sie dem Geber offt und ihre Pflicht vergessen;", "tokens": ["Da\u00df", "sie", "dem", "Ge\u00b7ber", "offt", "und", "ih\u00b7re", "Pflicht", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.115": {"text": "Da\u00df sie nicht eingedenk, wer es gegeben hat,", "tokens": ["Da\u00df", "sie", "nicht", "ein\u00b7ge\u00b7denk", ",", "wer", "es", "ge\u00b7ge\u00b7ben", "hat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.116": {"text": "Was sie ganz unachtsam ohn Dankbegier auffres-", "tokens": ["Was", "sie", "ganz", "u\u00b7nacht\u00b7sam", "ohn", "Dank\u00b7be\u00b7gier", "auf\u00b7fres"], "tok