{"dta.poem.45": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Da\u00df die Wollthat GOttes, die  \n er uns bei der Erndte bewiesen, vie-  \n len eine unerkandte Wollthat sey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich zur Erndte-Zeit der Fel-\nder reiffe Lust,", "tokens": ["Als", "ich", "zur", "Ernd\u00b7te\u00b7Zeit", "der", "Fel", "der", "reif\u00b7fe", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Weide des Gem\u00fcths, in", "tokens": ["Zur", "Wei\u00b7de", "des", "Ge\u00b7m\u00fcths", ",", "in"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erg\u00f6zte sich das Herz in der", "tokens": ["Er\u00b7g\u00f6z\u00b7te", "sich", "das", "Herz", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich den Landman sah, wie", "tokens": ["Als", "ich", "den", "Land\u00b7man", "sah", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PWAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Halmen dichte Saat, die", "tokens": ["Der", "Hal\u00b7men", "dich\u00b7te", "Saat", ",", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und von der K\u00f6rner Last sich fast zu Boden lenkte,", "tokens": ["Und", "von", "der", "K\u00f6r\u00b7ner", "Last", "sich", "fast", "zu", "Bo\u00b7den", "lenk\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Schien wie ein wallend Meer, nach dem der Luft-", "tokens": ["Schien", "wie", "ein", "wal\u00b7lend", "Meer", ",", "nach", "dem", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "ADJD", "NN", "$,", "APPR", "ART", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der in gar sanften Zug sich durch die Aehren dreng-", "tokens": ["Der", "in", "gar", "sanf\u00b7ten", "Zug", "sich", "durch", "die", "A\u00b7eh\u00b7ren", "dreng"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Der Siecheln blanker Strahl, worauf die Sonne", "tokens": ["Der", "Sie\u00b7ch\u00b7eln", "blan\u00b7ker", "Strahl", ",", "wo\u00b7rauf", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Von starker Schnitter Hand, bald so bald so re-", "tokens": ["Von", "star\u00b7ker", "Schnit\u00b7ter", "Hand", ",", "bald", "so", "bald", "so", "re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "ADV", "ADV", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Erwekte durch den Blik mir manches Sinnenspiel:", "tokens": ["Er\u00b7wek\u00b7te", "durch", "den", "Blik", "mir", "man\u00b7ches", "Sin\u00b7nen\u00b7spiel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch ward ich allemahl dadurch zu ", "tokens": ["Doch", "ward", "ich", "al\u00b7le\u00b7mahl", "da\u00b7durch", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PAV", "PTKZU"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.13": {"text": "Die Andacht regte sich, bei diesem Seegensblik,", "tokens": ["Die", "An\u00b7dacht", "reg\u00b7te", "sich", ",", "bei", "die\u00b7sem", "See\u00b7gens\u00b7blik", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Felder schienen mir gleich aufgeschlagnen Bl\u00e4t-", "tokens": ["Die", "Fel\u00b7der", "schie\u00b7nen", "mir", "gleich", "auf\u00b7ge\u00b7schlag\u00b7nen", "Bl\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich la\u00df des Sch\u00f6pfers Gr\u00f6s auf jedem Akkerst\u00fck;", "tokens": ["Ich", "la\u00df", "des", "Sch\u00f6p\u00b7fers", "Gr\u00f6s", "auf", "je\u00b7dem", "Ak\u00b7kers\u00b7t\u00fck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Halmen dienten mir stat der geschriebnen Let-", "tokens": ["Die", "Hal\u00b7men", "dien\u00b7ten", "mir", "stat", "der", "ge\u00b7schrieb\u00b7nen", "Let"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Ich fand hier \u00fcberall, im Lesen nur allein:", "tokens": ["Ich", "fand", "hier", "\u00fc\u00b7be\u00b7rall", ",", "im", "Le\u00b7sen", "nur", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "APPRART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der dieses Feld gebaut, erf\u00fcllt mit Frucht und Saa-", "tokens": ["Der", "die\u00b7ses", "Feld", "ge\u00b7baut", ",", "er\u00b7f\u00fcllt", "mit", "Frucht", "und", "Saa"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PDAT", "NN", "VVPP", "$,", "VVFIN", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da\u00df mu\u00df ein grosser HErr voll G\u00fct, Macht, Weis-", "tokens": ["Da\u00df", "mu\u00df", "ein", "gros\u00b7ser", "Herr", "voll", "G\u00fct", ",", "Macht", ",", "Weis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "VMFIN", "ART", "ADJA", "NN", "ADJD", "NN", "$,", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Ein grosser Jehovah, ein ", "tokens": ["Ein", "gros\u00b7ser", "Je\u00b7ho\u00b7vah", ",", "ein"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Mein Mensche denke nur, was thut ein Akkers-", "tokens": ["Mein", "Men\u00b7sche", "den\u00b7ke", "nur", ",", "was", "thut", "ein", "Ak\u00b7ker\u00b7s"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "PWS", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Er s\u00e4t den Saamen aus, verscharrt ihn in die Er-", "tokens": ["Er", "s\u00e4t", "den", "Saa\u00b7men", "aus", ",", "ver\u00b7scharrt", "ihn", "in", "die", "Er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer giebt die Treibekraft, die er nicht geben kan,", "tokens": ["Wer", "giebt", "die", "Trei\u00b7be\u00b7kraft", ",", "die", "er", "nicht", "ge\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.24": {"text": "Da\u00df Korn in ihren Schoos ersterbend fruchtbar", "tokens": ["Da\u00df", "Korn", "in", "ih\u00b7ren", "Schoos", "ers\u00b7ter\u00b7bend", "frucht\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVPP", "ADJD"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.25": {"text": "Betrachte dieses nur: So gleich erblikkest du:", "tokens": ["Be\u00b7trach\u00b7te", "die\u00b7ses", "nur", ":", "So", "gleich", "er\u00b7blik\u00b7kest", "du", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADV", "$.", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es mu\u00df ein solcher seyn, von dem das her entsprin-", "tokens": ["Es", "mu\u00df", "ein", "sol\u00b7cher", "seyn", ",", "von", "dem", "das", "her", "ent\u00b7sprin"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "PIAT", "VAINF", "$,", "APPR", "PRELS", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und giebest du mir dies, als eine Warheit zu;", "tokens": ["Und", "gie\u00b7best", "du", "mir", "dies", ",", "als", "ei\u00b7ne", "War\u00b7heit", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PDS", "$,", "KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So frage wer ist es, der uns die Fr\u00fcchte bringet?", "tokens": ["So", "fra\u00b7ge", "wer", "ist", "es", ",", "der", "uns", "die", "Fr\u00fcch\u00b7te", "brin\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PWS", "VAFIN", "PPER", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da zeigt der Sch\u00f6pfer sich an seiner Kreatur,", "tokens": ["Da", "zeigt", "der", "Sch\u00f6p\u00b7fer", "sich", "an", "sei\u00b7ner", "Kre\u00b7a\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mit seiner Herrlichkeit, die man bewundernd mer-", "tokens": ["Mit", "sei\u00b7ner", "Herr\u00b7lich\u00b7keit", ",", "die", "man", "be\u00b7wun\u00b7dernd", "mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PIS", "VVPP", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Man sehe nur ins Feld des Reiches der Natur:", "tokens": ["Man", "se\u00b7he", "nur", "ins", "Feld", "des", "Rei\u00b7ches", "der", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "So wird man davon gleich auf manche Art best\u00e4r-", "tokens": ["So", "wird", "man", "da\u00b7von", "gleich", "auf", "man\u00b7che", "Art", "be\u00b7st\u00e4r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "PAV", "ADV", "APPR", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So wie der Schein bezeugt, der Sonnen g\u00fcldne", "tokens": ["So", "wie", "der", "Schein", "be\u00b7zeugt", ",", "der", "Son\u00b7nen", "g\u00fcld\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Wenn ihre Fenerkraft uns in die Augen strahlet;", "tokens": ["Wenn", "ih\u00b7re", "Fe\u00b7ner\u00b7kraft", "uns", "in", "die", "Au\u00b7gen", "strah\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So strahlt auch ", "tokens": ["So", "strahlt", "auch"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.36": {"text": "Das seine Herrlichkeit uns vor die Augen mahlet.", "tokens": ["Das", "sei\u00b7ne", "Herr\u00b7lich\u00b7keit", "uns", "vor", "die", "Au\u00b7gen", "mah\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Wie wunderbar ist es, wenn man ein K\u00f6rnchen", "tokens": ["Wie", "wun\u00b7der\u00b7bar", "ist", "es", ",", "wenn", "man", "ein", "K\u00f6rn\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "-+----+--+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Da\u00df durch den Nahrungssaft sich auseinander", "tokens": ["Da\u00df", "durch", "den", "Nah\u00b7rungs\u00b7saft", "sich", "aus\u00b7ein\u00b7an\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PRF", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "In einem schlanken Halm durch seinen Boden zieht,", "tokens": ["In", "ei\u00b7nem", "schlan\u00b7ken", "Halm", "durch", "sei\u00b7nen", "Bo\u00b7den", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und in der Aehren-Kopf zu vielen K\u00f6rnern spriesset!", "tokens": ["Und", "in", "der", "A\u00b7eh\u00b7ren\u00b7Kopf", "zu", "vie\u00b7len", "K\u00f6r\u00b7nern", "spries\u00b7set", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+----+-+-+-+-", "measure": "dactylic.init"}, "line.41": {"text": "Mu\u00df man die weise Macht nicht daran sichtbahr", "tokens": ["Mu\u00df", "man", "die", "wei\u00b7se", "Macht", "nicht", "da\u00b7ran", "sicht\u00b7bahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ART", "ADJA", "NN", "PTKNEG", "PAV", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "Wenn man nur aufmerksam dies alles \u00fcberdenket?", "tokens": ["Wenn", "man", "nur", "auf\u00b7merk\u00b7sam", "dies", "al\u00b7les", "\u00fc\u00b7ber\u00b7den\u00b7ket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "PDS", "PIS", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.43": {"text": "Mu\u00df man nicht Ehrfurchtsvoll und \u00fcberzeugt ge-", "tokens": ["Mu\u00df", "man", "nicht", "Ehr\u00b7furchts\u00b7voll", "und", "\u00fc\u00b7berz\u00b7eugt", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "PTKNEG", "NN", "KON", "VVPP", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.45": {"text": "Die ewge G\u00fctigkeit, die das was lebt, ern\u00e4hrt,", "tokens": ["Die", "ew\u00b7ge", "G\u00fc\u00b7tig\u00b7keit", ",", "die", "das", "was", "lebt", ",", "er\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "PIS", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46"