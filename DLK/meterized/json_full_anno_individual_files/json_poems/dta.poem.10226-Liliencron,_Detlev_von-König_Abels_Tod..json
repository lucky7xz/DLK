{"dta.poem.10226": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Abels Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Auf heimlichen Wegen, mit Axt und Beil,", "tokens": ["Auf", "heim\u00b7li\u00b7chen", "We\u00b7gen", ",", "mit", "Axt", "und", "Beil", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Es kommen die freien Friesen in Eil,", "tokens": ["Es", "kom\u00b7men", "die", "frei\u00b7en", "Frie\u00b7sen", "in", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Das Bild des heiligen Christian,", "tokens": ["Das", "Bild", "des", "hei\u00b7li\u00b7gen", "Chris\u00b7ti\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bitt\u2019 f\u00fcr uns, betet der Kapellan,", "tokens": ["Bitt'", "f\u00fcr", "uns", ",", "be\u00b7tet", "der", "Ka\u00b7pel\u00b7lan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Mit Gold schon beschl\u00e4gt ihn der gelbe Mond", "tokens": ["Mit", "Gold", "schon", "be\u00b7schl\u00e4gt", "ihn", "der", "gel\u00b7be", "Mond"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn morgen er wieder am Himmel thront,", "tokens": ["Wenn", "mor\u00b7gen", "er", "wie\u00b7der", "am", "Him\u00b7mel", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}