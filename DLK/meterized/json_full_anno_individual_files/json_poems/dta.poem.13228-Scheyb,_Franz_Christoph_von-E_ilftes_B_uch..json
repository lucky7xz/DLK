{"dta.poem.13228": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nein: Dicht-Kunst! die du dich mit solchem Stolz erh\u00f6hest", "tokens": ["Nein", ":", "Dicht\u00b7Kunst", "!", "die", "du", "dich", "mit", "sol\u00b7chem", "Stolz", "er\u00b7h\u00f6\u00b7hest"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "NN", "$.", "PRELS", "PPER", "PRF", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und alles, wie du sagst, besingest und verstehest,", "tokens": ["Und", "al\u00b7les", ",", "wie", "du", "sagst", ",", "be\u00b7sin\u00b7gest", "und", "ver\u00b7ste\u00b7hest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Stimm deiner Cither Thon; sing dieses F\u00fcrstens Lob,", "tokens": ["Stimm", "dei\u00b7ner", "Cit\u00b7her", "Thon", ";", "sing", "die\u00b7ses", "F\u00fcrs\u00b7tens", "Lob", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$.", "VVFIN", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den GOtt dem Neid zu Troz auf diesen Thron erhob.", "tokens": ["Den", "Gott", "dem", "Neid", "zu", "Troz", "auf", "die\u00b7sen", "Thron", "er\u00b7hob", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "35Sing frey! du siehst an ihm den allerh\u00f6chsten Helden;", "tokens": ["frey", "!", "du", "siehst", "an", "ihm", "den", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Hel\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Du brauchst das Dichten nicht; du kannst die Wahrheit melden.", "tokens": ["Du", "brauchst", "das", "Dich\u00b7ten", "nicht", ";", "du", "kannst", "die", "Wahr\u00b7heit", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}