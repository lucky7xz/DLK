{"dta.poem.6651": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was nun am n\u00f6thigsten von unseren Studieren", "tokens": ["Was", "nun", "am", "n\u00f6\u00b7thigs\u00b7ten", "von", "un\u00b7se\u00b7ren", "Stu\u00b7die\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPRART", "ADJA", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+--+--", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Ist, da\u00df wir uns bem\u00fchen, auszusp\u00fcren", "tokens": ["Ist", ",", "da\u00df", "wir", "uns", "be\u00b7m\u00fc\u00b7hen", ",", "aus\u00b7zu\u00b7sp\u00fc\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,", "VVIZU"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das, was in uns geschicht im Wesen, das gedencket:", "tokens": ["Das", ",", "was", "in", "uns", "ge\u00b7schicht", "im", "We\u00b7sen", ",", "das", "ge\u00b7den\u00b7cket", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "PPER", "VVPP", "APPRART", "NN", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}