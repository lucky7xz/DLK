{"textgrid.poem.42484": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist vielleicht der Herr Professor zu Haus?", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist vielleicht der Herr Professor zu Haus?", "tokens": ["Ist", "viel\u00b7leicht", "der", "Herr", "Pro\u00b7fes\u00b7sor", "zu", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbnein, der Herr Professor ging vorhin aus.\u00ab", "tokens": ["\u00bb", "nein", ",", "der", "Herr", "Pro\u00b7fes\u00b7sor", "ging", "vor\u00b7hin", "aus", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "ART", "NN", "NN", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Ist vielleicht Frau Professor zu Haus?", "tokens": ["Ist", "viel\u00b7leicht", "Frau", "Pro\u00b7fes\u00b7sor", "zu", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbnein, Frau Professor ging eben aus.\u00ab", "tokens": ["\u00bb", "nein", ",", "Frau", "Pro\u00b7fes\u00b7sor", "ging", "e\u00b7ben", "aus", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "NN", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und Fr\u00e4ulein T\u00f6chter, sind sie zu Haus?", "tokens": ["Und", "Fr\u00e4u\u00b7lein", "T\u00f6ch\u00b7ter", ",", "sind", "sie", "zu", "Haus", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$,", "VAFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "\u00bbnein, Fr\u00e4ulein T\u00f6chter gingen auch eben aus.\u00ab", "tokens": ["\u00bb", "nein", ",", "Fr\u00e4u\u00b7lein", "T\u00f6ch\u00b7ter", "gin\u00b7gen", "auch", "e\u00b7ben", "aus", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "NN", "NN", "VVFIN", "ADV", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "So bist du ganz allein, mein Kind?", "tokens": ["So", "bist", "du", "ganz", "al\u00b7lein", ",", "mein", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das pa\u00dft vortrefflich; zeig mir geschwind,", "tokens": ["Das", "pa\u00dft", "vor\u00b7treff\u00b7lich", ";", "zeig", "mir", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "$.", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wo der Herr Professor sitzt,", "tokens": ["Wo", "der", "Herr", "Pro\u00b7fes\u00b7sor", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er bei der Arbeit schwitzt,", "tokens": ["Wenn", "er", "bei", "der", "Ar\u00b7beit", "schwitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn er in tiefer Gelehrsamkeit", "tokens": ["Wenn", "er", "in", "tie\u00b7fer", "Ge\u00b7lehr\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "---+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Vergi\u00dft sogar die Essenszeit.", "tokens": ["Ver\u00b7gi\u00dft", "so\u00b7gar", "die", "Es\u00b7sens\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das also ist sein Schreibtisch, sein Pult;", "tokens": ["Das", "al\u00b7so", "ist", "sein", "Schreib\u00b7tisch", ",", "sein", "Pult", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Von dort aus, in k\u00f6niglicher Huld,", "tokens": ["Von", "dort", "aus", ",", "in", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "Huld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Geruht er seine Kritiken zu kr\u00e4hen", "tokens": ["Ge\u00b7ruht", "er", "sei\u00b7ne", "Kri\u00b7ti\u00b7ken", "zu", "kr\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und auf die jungen Dichter zu schm\u00e4hen,", "tokens": ["Und", "auf", "die", "jun\u00b7gen", "Dich\u00b7ter", "zu", "schm\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Bald mit gerunzelter Stirn zu kritzeln,", "tokens": ["Bald", "mit", "ge\u00b7run\u00b7zel\u00b7ter", "Stirn", "zu", "krit\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Bald mit sardonischem Lachen zu witzeln.", "tokens": ["Bald", "mit", "sar\u00b7do\u00b7ni\u00b7schem", "La\u00b7chen", "zu", "wit\u00b7zeln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-----+--+-", "measure": "trochaic.tri.relaxed"}, "line.13": {"text": "Dann, zur Erholung, nimmt er Horaz", "tokens": ["Dann", ",", "zur", "Er\u00b7ho\u00b7lung", ",", "nimmt", "er", "Ho\u00b7raz"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "APPRART", "NN", "$,", "VVFIN", "PPER", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Oder den langweiligen Trimpetraz.", "tokens": ["O\u00b7der", "den", "lang\u00b7wei\u00b7li\u00b7gen", "Trim\u00b7pe\u00b7traz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "---++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und das ist des G\u00f6ttlichen Kanapee;", "tokens": ["Und", "das", "ist", "des", "G\u00f6tt\u00b7li\u00b7chen", "Ka\u00b7na\u00b7pee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ei, sieh doch. Wie w\u00e4rs, allerliebste Fee,", "tokens": ["Ei", ",", "sieh", "doch", ".", "Wie", "w\u00e4rs", ",", "al\u00b7ler\u00b7liebs\u00b7te", "Fee", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "ADV", "$.", "PWAV", "VAFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort mu\u00df ich mal sitzen, das ist mir erlaubt,", "tokens": ["Dort", "mu\u00df", "ich", "mal", "sit\u00b7zen", ",", "das", "ist", "mir", "er\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,", "PDS", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wo zu Mittag schl\u00e4ft sein klassisches Haupt.", "tokens": ["Wo", "zu", "Mit\u00b7tag", "schl\u00e4ft", "sein", "klas\u00b7si\u00b7sches", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Komm, setz dich neben mich; willst du dich zieren?", "tokens": ["Komm", ",", "setz", "dich", "ne\u00b7ben", "mich", ";", "willst", "du", "dich", "zie\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "APPR", "PPER", "$.", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Herrschaften gingen alle spazieren.", "tokens": ["Die", "Herr\u00b7schaf\u00b7ten", "gin\u00b7gen", "al\u00b7le", "spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "VVINF", "$."], "meter": "-++-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Wahrhaftig, das ist nett von dir;", "tokens": ["Wahr\u00b7haf\u00b7tig", ",", "das", "ist", "nett", "von", "dir", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PDS", "VAFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir sind ja auch nur zu zweien hier.", "tokens": ["Wir", "sind", "ja", "auch", "nur", "zu", "zwei\u00b7en", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPR", "CARD", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Und wo er liest im Chrysostomus,", "tokens": ["Und", "wo", "er", "liest", "im", "Chry\u00b7so\u00b7sto\u00b7mus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kleine, wie w\u00e4r es, rasch einen Ku\u00df.", "tokens": ["Klei\u00b7ne", ",", "wie", "w\u00e4r", "es", ",", "rasch", "ei\u00b7nen", "Ku\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PWAV", "VAFIN", "PPER", "$,", "ADJD", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "\u00bbaber, das geht nicht.\u00ab J, nur im Fluge;", "tokens": ["\u00bb", "a\u00b7ber", ",", "das", "geht", "nicht", ".", "\u00ab", "J", ",", "nur", "im", "Flu\u00b7ge", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PDS", "VVFIN", "PTKNEG", "$.", "$(", "NE", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Glaube mir, bald sind wir im Zuge.", "tokens": ["Glau\u00b7be", "mir", ",", "bald", "sind", "wir", "im", "Zu\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.13": {"text": "\u00bbes klingelt! die T\u00fcr! Nicht doch, bitte.\u00ab", "tokens": ["\u00bb", "es", "klin\u00b7gelt", "!", "die", "T\u00fcr", "!", "Nicht", "doch", ",", "bit\u00b7te", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$.", "ART", "NN", "$.", "PTKNEG", "ADV", "$,", "PTKANT", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.14": {"text": "Schnell noch den einen... \u00bbIch h\u00f6re Schritte.\u00ab", "tokens": ["Schnell", "noch", "den", "ei\u00b7nen", "...", "\u00bb", "Ich", "h\u00f6\u00b7re", "Schrit\u00b7te", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADV", "ART", "ART", "$(", "$(", "PPER", "VVFIN", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Ah, da sind Sie, mein Bester; Sie haben", "tokens": ["Ah", ",", "da", "sind", "Sie", ",", "mein", "Bes\u00b7ter", ";", "Sie", "ha\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ADV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$.", "PPER", "VAFIN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Doch nicht gewartet? Nun wollen wir graben", "tokens": ["Doch", "nicht", "ge\u00b7war\u00b7tet", "?", "Nun", "wol\u00b7len", "wir", "gra\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVPP", "$.", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und t\u00fcchtig die faule Denkschaufel regen.", "tokens": ["Und", "t\u00fcch\u00b7tig", "die", "fau\u00b7le", "Denk\u00b7schau\u00b7fel", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "ADJA", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie kommen des Thukydides wegen.", "tokens": ["Sie", "kom\u00b7men", "des", "Thu\u00b7ky\u00b7di\u00b7des", "we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Die Stelle ist schwierig. Nehmen Sie Platz;", "tokens": ["Die", "Stel\u00b7le", "ist", "schwie\u00b7rig", ".", "Neh\u00b7men", "Sie", "Platz", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich geh sogleich auf Such und Hatz,", "tokens": ["Ich", "geh", "sog\u00b7leich", "auf", "Such", "und", "Hatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich hoffe, wir werden den Racker kriegen", "tokens": ["Ich", "hof\u00b7fe", ",", "wir", "wer\u00b7den", "den", "Ra\u00b7cker", "krie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und ihm den trotzigen Nacken biegen.", "tokens": ["Und", "ihm", "den", "trot\u00b7zi\u00b7gen", "Na\u00b7cken", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Keine Umst\u00e4nde, bitte aufs Kanapee! \u2013", "tokens": ["Kei\u00b7ne", "Um\u00b7st\u00e4n\u00b7de", ",", "bit\u00b7te", "aufs", "Ka\u00b7na\u00b7pee", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Und so geschahs. O Chrysostome!", "tokens": ["Und", "so", "ge\u00b7schahs", ".", "O", "Chry\u00b7so\u00b7sto\u00b7me", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$.", "NE", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ist vielleicht der Herr Professor zu Haus?", "tokens": ["Ist", "viel\u00b7leicht", "der", "Herr", "Pro\u00b7fes\u00b7sor", "zu", "Haus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbnein, der Herr Professor ging vorhin aus.\u00ab", "tokens": ["\u00bb", "nein", ",", "der", "Herr", "Pro\u00b7fes\u00b7sor", "ging", "vor\u00b7hin", "aus", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "ART", "NN", "NN", "VVFIN", "ADV", "PTKVZ", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.8": {"line.1": {"text": "Ist vielleicht Frau Professor zu Haus?", "tokens": ["Ist", "viel\u00b7leicht", "Frau", "Pro\u00b7fes\u00b7sor", "zu", "Haus", "?"], "token_info": ["word", "word", "word", "word", 