{"dta.poem.20722": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Zuruf an den Fremdling  \n beim Marmorsarge  \n  Friedrichs des Gro\u00dfen  \n am 18. August 1786 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wandrer, weile noch und steh,                 ", "tokens": ["Wand\u00b7rer", ",", "wei\u00b7le", "noch", "und", "steh", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich mit unsern Herzen zu betr\u00fcben", "tokens": ["Dich", "mit", "un\u00b7sern", "Her\u00b7zen", "zu", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bei dem wei\u00dfen Marmor, \u00fcberschrieben:", "tokens": ["Bei", "dem", "wei\u00b7\u00dfen", "Mar\u00b7mor", ",", "\u00fc\u00b7bersc\u00b7hrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Friedrich, der Alleinzige. &#8212;               ", "tokens": ["Fried\u00b7rich", ",", "der", "Al\u00b7lein\u00b7zi\u00b7ge", ".", "&#8212;"], "token_info": ["word", "punct", "word", "word", "punct", "XML_entity"], "pos": ["NE", "$,", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Siehe nur, so viel ist hier geblieben", "tokens": ["Sie\u00b7he", "nur", ",", "so", "viel", "ist", "hier", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "ADV", "ADV", "VAFIN", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von dem Ersten aller K\u00f6nige \u2014", "tokens": ["Von", "dem", "Ers\u00b7ten", "al\u00b7ler", "K\u00f6\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nur ein enges Beingeh\u00e4use", "tokens": ["Nur", "ein", "en\u00b7ges", "Bein\u00b7ge\u00b7h\u00e4u\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward die Wohnung eines theuren Haupts,", "tokens": ["Ward", "die", "Woh\u00b7nung", "ei\u00b7nes", "theu\u00b7ren", "Haupts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Voll Gedanken! stark, und hoch, und weise!", "tokens": ["Voll", "Ge\u00b7dan\u00b7ken", "!", "stark", ",", "und", "hoch", ",", "und", "wei\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$.", "ADJD", "$,", "KON", "ADJD", "$,", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Keine Nachwelt glaubts,", "tokens": ["Kei\u00b7ne", "Nach\u00b7welt", "glaubts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Was Ihm unter Seinen Zeitgenossen", "tokens": ["Was", "Ihm", "un\u00b7ter", "Sei\u00b7nen", "Zeit\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Biographen, Redner, Dichter hier,", "tokens": ["Bio\u00b7gra\u00b7phen", ",", "Red\u00b7ner", ",", "Dich\u00b7ter", "hier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als ein Todtenopfer, ausgegossen,", "tokens": ["Als", "ein", "Tod\u00b7ten\u00b7op\u00b7fer", ",", "aus\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Da Sein Geist mit hoher Flugbegier", "tokens": ["Da", "Sein", "Geist", "mit", "ho\u00b7her", "Flug\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Ueber L\u00e4nder, Meere, Gr\u00e4ber, Thronen,", "tokens": ["Ue\u00b7ber", "L\u00e4n\u00b7der", ",", "Mee\u00b7re", ",", "Gr\u00e4\u00b7ber", ",", "Thro\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Sich erhob ins unbekannte Reich", "tokens": ["Sich", "er\u00b7hob", "ins", "un\u00b7be\u00b7kann\u00b7te", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Zu den K\u00f6nigen, die ihre Kronen", "tokens": ["Zu", "den", "K\u00f6\u00b7ni\u00b7gen", ",", "die", "ih\u00b7re", "Kro\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Wohl gesch\u00fctzet, und zugleich", "tokens": ["Wohl", "ge\u00b7sch\u00fct\u00b7zet", ",", "und", "zu\u00b7gleich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "$,", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "S\u00fc\u00dfen Landesvaternamen", "tokens": ["S\u00fc\u00b7\u00dfen", "Lan\u00b7des\u00b7va\u00b7ter\u00b7na\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Lieber h\u00f6rten, als den Titelklang", "tokens": ["Lie\u00b7ber", "h\u00f6r\u00b7ten", ",", "als", "den", "Ti\u00b7tel\u00b7klang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Eines Ueberwinders, wenn sie kamen", "tokens": ["Ei\u00b7nes", "Ue\u00b7ber\u00b7win\u00b7ders", ",", "wenn", "sie", "ka\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Aus dem Siegesthatendrang.", "tokens": ["Aus", "dem", "Sie\u00b7gest\u00b7ha\u00b7ten\u00b7drang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Vaterlandesvater war der Gro\u00dfe,", "tokens": ["Va\u00b7ter\u00b7lan\u00b7des\u00b7va\u00b7ter", "war", "der", "Gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Gepriesne, wenn Er weit", "tokens": ["Der", "Ge\u00b7pri\u00b7es\u00b7ne", ",", "wenn", "Er", "weit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Von des Vaterlandes Schoo\u00dfe", "tokens": ["Von", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter f\u00fcrchterlichem Streit,", "tokens": ["Un\u00b7ter", "f\u00fcrch\u00b7ter\u00b7li\u00b7chem", "Streit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter Kriegesblitz und Donnerschl\u00e4gen,", "tokens": ["Un\u00b7ter", "Krie\u00b7ges\u00b7blitz", "und", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In Gefahr wie Berg und Felsen stand. \u2014", "tokens": ["In", "Ge\u00b7fahr", "wie", "Berg", "und", "Fel\u00b7sen", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KOKOM", "NN", "KON", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "So viel Blicke, so viel Vaterseegen", "tokens": ["So", "viel", "Bli\u00b7cke", ",", "so", "viel", "Va\u00b7ter\u00b7see\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Gab Er Seinem Volke, wenn das Land", "tokens": ["Gab", "Er", "Sei\u00b7nem", "Vol\u00b7ke", ",", "wenn", "das", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Friedensseeligkeit genossen.", "tokens": ["Frie\u00b7dens\u00b7see\u00b7lig\u00b7keit", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, auf Seiner lorbergr\u00fcnen Bahn", "tokens": ["Ach", ",", "auf", "Sei\u00b7ner", "lor\u00b7ber\u00b7gr\u00fc\u00b7nen", "Bahn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ist nie eine Tagesfrist verflossen", "tokens": ["Ist", "nie", "ei\u00b7ne", "Ta\u00b7ges\u00b7frist", "ver\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF"], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ohne da\u00df Er Guts gethan \u2014", "tokens": ["Oh\u00b7ne", "da\u00df", "Er", "Guts", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Niemals kam ein junger Morgen,", "tokens": ["Nie\u00b7mals", "kam", "ein", "jun\u00b7ger", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der in Seiner rechten Hand", "tokens": ["Der", "in", "Sei\u00b7ner", "rech\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Den Regierungsstab nicht fand,", "tokens": ["Den", "Re\u00b7gie\u00b7rungs\u00b7stab", "nicht", "fand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Schwer von K\u00f6niglichen Sorgen,", "tokens": ["Schwer", "von", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Oder gro\u00dfer Feldherrn Pflicht.", "tokens": ["O\u00b7der", "gro\u00b7\u00dfer", "Feld\u00b7herrn", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Immer war Sein Angesicht", "tokens": ["Im\u00b7mer", "war", "Sein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Vor der Morgenr\u00f6the munter,", "tokens": ["Vor", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Bis Sein Augenglanz sich unter", "tokens": ["Bis", "Sein", "Au\u00b7gen\u00b7glanz", "sich", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Todesdunkelheit verlor \u2014", "tokens": ["To\u00b7des\u00b7dun\u00b7kel\u00b7heit", "ver\u00b7lor"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "D\u00f6rfer hie\u00df Er aus der Erde steigen,", "tokens": ["D\u00f6r\u00b7fer", "hie\u00df", "Er", "aus", "der", "Er\u00b7de", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Wenig Tage noch zuvor,", "tokens": ["We\u00b7nig", "Ta\u00b7ge", "noch", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Eh Sein Mund auf immer mu\u00dfte schweigen:", "tokens": ["Eh", "Sein", "Mund", "auf", "im\u00b7mer", "mu\u00df\u00b7te", "schwei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Ihm zum Hymnus bl\u00fchen sie empor!", "tokens": ["Ihm", "zum", "Hym\u00b7nus", "bl\u00fc\u00b7hen", "sie", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}