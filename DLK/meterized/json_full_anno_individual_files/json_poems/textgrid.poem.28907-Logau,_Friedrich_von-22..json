{"textgrid.poem.28907": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Morus klagt, da\u00df seine Frau an der Frauen-Kranckheit liege,", "tokens": ["Mo\u00b7rus", "klagt", ",", "da\u00df", "sei\u00b7ne", "Frau", "an", "der", "Frau\u00b7en\u00b7Kranck\u00b7heit", "lie\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da\u00df daf\u00fcr noch Teuffels-Koth, Biebergeil, noch Feigbohn t\u00fcge;", "tokens": ["Da\u00df", "da\u00b7f\u00fcr", "noch", "Teuf\u00b7fels\u00b7Koth", ",", "Bie\u00b7ber\u00b7geil", ",", "noch", "Feig\u00b7bohn", "t\u00fc\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "ADV", "NN", "$,", "NN", "$,", "ADV", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ob man Mau\u00df-Ohr-, weisser Lilg-, auch Melissen-Wasser name,", "tokens": ["Ob", "man", "Mau\u00df\u00b7Ohr", ",", "weis\u00b7ser", "Lil\u00b7g", ",", "auch", "Me\u00b7lis\u00b7sen\u00b7Was\u00b7ser", "na\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "TRUNC", "$,", "ADJA", "TRUNC", "$,", "ADV", "NE", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Hilffe nichts, auch Anis-, Lein- und auch nicht der Fenchel-Saame;", "tokens": ["Hilf\u00b7fe", "nichts", ",", "auch", "A\u00b7nis", ",", "Lein", "und", "auch", "nicht", "der", "Fen\u00b7chel\u00b7Saa\u00b7me", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ADV", "TRUNC", "$,", "TRUNC", "KON", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Von dem Schwertel gelb und blau, von Rahpontick und dergleichen,", "tokens": ["Von", "dem", "Schwer\u00b7tel", "gelb", "und", "blau", ",", "von", "Rah\u00b7pon\u00b7tick", "und", "derg\u00b7lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "KON", "PIS", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Costus nicht, auch Moly nicht k\u00fcnne diese Noth erweichen;", "tokens": ["Cos\u00b7tus", "nicht", ",", "auch", "Mo\u00b7ly", "nicht", "k\u00fcn\u00b7ne", "die\u00b7se", "Noth", "er\u00b7wei\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "ADV", "NE", "PTKNEG", "VVFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Keine Wurtzel! mich bed\u00fcnckt, da\u00df ich etwa wo gelesen,", "tokens": ["Kei\u00b7ne", "Wurt\u00b7zel", "!", "mich", "be\u00b7d\u00fcnckt", ",", "da\u00df", "ich", "et\u00b7wa", "wo", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "PWAV", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Zapffen-Kraut, so viel genug, macht von dieser Sucht genesen.", "tokens": ["Zapf\u00b7fen\u00b7Kraut", ",", "so", "viel", "ge\u00b7nug", ",", "