{"dta.poem.15703": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol vor dem Sch\u00f6pfer ist, was er geschaffen, klein;", "tokens": ["Wol", "vor", "dem", "Sch\u00f6p\u00b7fer", "ist", ",", "was", "er", "ge\u00b7schaf\u00b7fen", ",", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "$,", "PWS", "PPER", "VVPP", "$,", "ADJD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "K\u00f6nnt' etwas andres gro\u00df vor seiner Gr\u00f6\u00dfe seyn?", "tokens": ["K\u00f6nnt'", "et\u00b7was", "and\u00b7res", "gro\u00df", "vor", "sei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PIS", "ADJD", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mensch, der sich gro\u00df bed\u00fcnkt vor den Geschaffnen allen,", "tokens": ["Mensch", ",", "der", "sich", "gro\u00df", "be\u00b7d\u00fcnkt", "vor", "den", "Ge\u00b7schaff\u00b7nen", "al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "APPR", "ART", "NN", "PIAT", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Du bist nicht gr\u00f6\u00dfer auch; den gro\u00dfen Wahn la\u00df fallen", "tokens": ["Du", "bist", "nicht", "gr\u00f6\u00b7\u00dfer", "auch", ";", "den", "gro\u00b7\u00dfen", "Wahn", "la\u00df", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "$.", "ART", "ADJA", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du bist nicht gr\u00f6\u00dfer auch als alles, was gering", "tokens": ["Du", "bist", "nicht", "gr\u00f6\u00b7\u00dfer", "auch", "als", "al\u00b7les", ",", "was", "ge\u00b7ring"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "KOUS", "PIS", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du achtest; auch dein Geist ist ein geschaffnes Ding.", "tokens": ["Du", "ach\u00b7test", ";", "auch", "dein", "Geist", "ist", "ein", "ge\u00b7schaff\u00b7nes", "Ding", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch, ist dein Sch\u00f6pfer gro\u00df, kann klein seyn was er schuf?", "tokens": ["Doch", ",", "ist", "dein", "Sch\u00f6p\u00b7fer", "gro\u00df", ",", "kann", "klein", "seyn", "was", "er", "schuf", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "$,", "VMFIN", "ADJD", "VAINF", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was, Zeuge seiner Gr\u00f6\u00df', hervorging seinem Ruf?", "tokens": ["Was", ",", "Zeu\u00b7ge", "sei\u00b7ner", "Gr\u00f6\u00df'", ",", "her\u00b7vor\u00b7ging", "sei\u00b7nem", "Ruf", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gro\u00df ist sein kleinstes Werk, nur klein vor seiner Gr\u00f6\u00dfe;", "tokens": ["Gro\u00df", "ist", "sein", "kleins\u00b7tes", "Werk", ",", "nur", "klein", "vor", "sei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was gro\u00df sich selber schm\u00fcckt, erliegt in seiner Bl\u00f6\u00dfe.", "tokens": ["Was", "gro\u00df", "sich", "sel\u00b7ber", "schm\u00fcckt", ",", "er\u00b7liegt", "in", "sei\u00b7ner", "Bl\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PRF", "ADV", "VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Vor seiner Macht f\u00fchlt sich das Gr\u00f6ste nackt und blo\u00df,", "tokens": ["Vor", "sei\u00b7ner", "Macht", "f\u00fchlt", "sich", "das", "Gr\u00f6s\u00b7te", "nackt", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Kleinste aber ist in seiner Liebe gro\u00df.", "tokens": ["Das", "Kleins\u00b7te", "a\u00b7ber", "ist", "in", "sei\u00b7ner", "Lie\u00b7be", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}