{"dta.poem.10084": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fabel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hans war ein feines Kind, und eines K\u00f6nigs Sohn,", "tokens": ["Hans", "war", "ein", "fei\u00b7nes", "Kind", ",", "und", "ei\u00b7nes", "K\u00f6\u00b7nigs", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den man, damit sein Geist des Vaters Cron\u2019 und", "tokens": ["Den", "man", ",", "da\u00b7mit", "sein", "Geist", "des", "Va\u00b7ters", "Cron'", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch einen Gegensatz, noch h\u00f6her achten m\u00f6gte,", "tokens": ["Durch", "ei\u00b7nen", "Ge\u00b7gen\u00b7satz", ",", "noch", "h\u00f6\u00b7her", "ach\u00b7ten", "m\u00f6g\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df er ja sein Gl\u00fcck um desto mehr bed\u00e4chte,", "tokens": ["Und", "da\u00df", "er", "ja", "sein", "Gl\u00fcck", "um", "des\u00b7to", "mehr", "be\u00b7d\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In einem schlechten Dorff\u2019 hatt\u2019 auferziehen lassen.", "tokens": ["In", "ei\u00b7nem", "schlech\u00b7ten", "Dorff'", "hatt'", "auf\u00b7er\u00b7zie\u00b7hen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Damit er alles nun auf einmahl m\u00f6gte fassen,", "tokens": ["Da\u00b7mit", "er", "al\u00b7les", "nun", "auf", "ein\u00b7mahl", "m\u00f6g\u00b7te", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "APPR", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hie\u00df sein Herr Vater einst den Hof sich herrlich schm\u00fccken;", "tokens": ["Hie\u00df", "sein", "Herr", "Va\u00b7ter", "einst", "den", "Hof", "sich", "herr\u00b7lich", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "ADV", "ART", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Lie\u00df alles in geheim \u00fcm Hansens H\u00fctte r\u00fccken.", "tokens": ["Lie\u00df", "al\u00b7les", "in", "ge\u00b7heim", "\u00fcm", "Han\u00b7sens", "H\u00fct\u00b7te", "r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJD", "APPRART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es stand nunmehr der Hof, nach erst vergangner Nacht,", "tokens": ["Es", "stand", "nun\u00b7mehr", "der", "Hof", ",", "nach", "erst", "ver\u00b7gang\u00b7ner", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seiner h\u00f6chsten Pracht:", "tokens": ["In", "sei\u00b7ner", "h\u00f6chs\u00b7ten", "Pracht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Silber, Gold, Rubin und andern Edelsteinen", "tokens": ["Von", "Sil\u00b7ber", ",", "Gold", ",", "Ru\u00b7bin", "und", "an\u00b7dern", "E\u00b7del\u00b7stei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NE", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "War ein gef\u00e4rbter Glantz, und ein fast blendend scheinen", "tokens": ["War", "ein", "ge\u00b7f\u00e4rb\u00b7ter", "Glantz", ",", "und", "ein", "fast", "blen\u00b7dend", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "KON", "ART", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So hell als allgemein. Ein schimmernd blitzend Licht", "tokens": ["So", "hell", "als", "all\u00b7ge\u00b7mein", ".", "Ein", "schim\u00b7mernd", "blit\u00b7zend", "Licht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "$.", "ART", "ADJD", "VVPP", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bestrahlte Hertz und Brust,", "tokens": ["Be\u00b7strahl\u00b7te", "Hertz", "und", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erf\u00fcllte das Gesicht", "tokens": ["Er\u00b7f\u00fcll\u00b7te", "das", "Ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Mit Anmuth und mit Lust.", "tokens": ["Mit", "An\u00b7muth", "und", "mit", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hans \u00f6ffnete sogleich die starren Augen-Lieder,", "tokens": ["Hans", "\u00f6ff\u00b7ne\u00b7te", "sog\u00b7leich", "die", "star\u00b7ren", "Au\u00b7gen\u00b7Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprach: Ey das ist sch\u00f6n! Und damit kehrt\u2019 er wieder,", "tokens": ["Und", "sprach", ":", "Ey", "das", "ist", "sch\u00f6n", "!", "Und", "da\u00b7mit", "kehrt'", "er", "wie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "PDS", "VAFIN", "ADJD", "$.", "KON", "PAV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und setzte ruhig sich in seiner H\u00fctten nieder.", "tokens": ["Und", "setz\u00b7te", "ru\u00b7hig", "sich", "in", "sei\u00b7ner", "H\u00fct\u00b7ten", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Hans ist ieder Mensch, der aller Himmel Pracht,", "tokens": ["Der", "Hans", "ist", "ie\u00b7der", "Mensch", ",", "der", "al\u00b7ler", "Him\u00b7mel", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PIAT", "NN", "$,", "PRELS", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Sonnen Wunder-Licht, und, bey gestirnter Nacht,", "tokens": ["Der", "Son\u00b7nen", "Wun\u00b7der\u00b7Licht", ",", "und", ",", "bey", "ge\u00b7stirn\u00b7ter", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ungezehlte Zahl der Sonnen in den Sternen", "tokens": ["Die", "un\u00b7ge\u00b7zehl\u00b7te", "Zahl", "der", "Son\u00b7nen", "in", "den", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erstaunet nicht beschaut, und Den, der sie gemacht,", "tokens": ["Er\u00b7stau\u00b7net", "nicht", "be\u00b7schaut", ",", "und", "Den", ",", "der", "sie", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADJD", "$,", "KON", "NE", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In seiner Lust nicht will bewundern lernen.", "tokens": ["In", "sei\u00b7ner", "Lust", "nicht", "will", "be\u00b7wun\u00b7dern", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}