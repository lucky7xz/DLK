{"dta.poem.13411": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Die Zufriedenheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Wer mag gern unzufrieden leben?", "tokens": ["Wer", "mag", "gern", "un\u00b7zu\u00b7frie\u00b7den", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem will ich meine Sorgen geben,", "tokens": ["Dem", "will", "ich", "mei\u00b7ne", "Sor\u00b7gen", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die jetzt von mir ins Elend gehn;", "tokens": ["Die", "jetzt", "von", "mir", "ins", "E\u00b7lend", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich habe sie nun ausgetrieben,", "tokens": ["Ich", "ha\u00b7be", "sie", "nun", "aus\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Scheidebrief ist unterschrieben;", "tokens": ["Der", "Schei\u00b7de\u00b7brief", "ist", "un\u00b7ter\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Angesicht nicht mehr zu sehn.", "tokens": ["Mein", "An\u00b7ge\u00b7sicht", "nicht", "mehr", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Was hilft es; sich vergebens qv\u00e4len,", "tokens": ["Was", "hilft", "es", ";", "sich", "ver\u00b7ge\u00b7bens", "qv\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer b\u00f6se Tage z\u00e4hlen,", "tokens": ["Und", "im\u00b7mer", "b\u00f6\u00b7se", "Ta\u00b7ge", "z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch in dem Finstern \u00e4ngstlich thun?", "tokens": ["Auch", "in", "dem", "Fins\u00b7tern", "\u00e4ngst\u00b7lich", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am besten ists, ein Leben f\u00fchren,", "tokens": ["Am", "bes\u00b7ten", "ists", ",", "ein", "Le\u00b7ben", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df man ein Gut mit Lust verliehren", "tokens": ["Da\u00df", "man", "ein", "Gut", "mit", "Lust", "ver\u00b7lieh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dabey kann gelassen ruhn.", "tokens": ["Und", "da\u00b7bey", "kann", "ge\u00b7las\u00b7sen", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dem Himmel will ich mich vertrauen,", "tokens": ["Dem", "Him\u00b7mel", "will", "ich", "mich", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und blo\u00df auf seine H\u00fclfe bauen,", "tokens": ["Und", "blo\u00df", "auf", "sei\u00b7ne", "H\u00fcl\u00b7fe", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ist es sicher \u00fcber mir;", "tokens": ["So", "ist", "es", "si\u00b7cher", "\u00fc\u00b7ber", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn mag die Welt in St\u00fccken fallen,", "tokens": ["Denn", "mag", "die", "Welt", "in", "St\u00fc\u00b7cken", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich lache bey dem letzten Knallen", "tokens": ["Ich", "la\u00b7che", "bey", "dem", "letz\u00b7ten", "Knal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und rufe noch, da bin ich! hier!", "tokens": ["Und", "ru\u00b7fe", "noch", ",", "da", "bin", "ich", "!", "hier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADV", "VAFIN", "PPER", "$.", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Um weiter bin ich nichts besorget,", "tokens": ["Um", "wei\u00b7ter", "bin", "ich", "nichts", "be\u00b7sor\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "VAFIN", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich zahle, wenn ich was geborget,", "tokens": ["Ich", "zah\u00b7le", ",", "wenn", "ich", "was", "ge\u00b7bor\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und esse, wenn mein Tisch gedeckt;", "tokens": ["Und", "es\u00b7se", ",", "wenn", "mein", "Tisch", "ge\u00b7deckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hab ich hiern\u00e4chst ein rein Gewissen,", "tokens": ["Hab", "ich", "hier\u00b7n\u00e4chst", "ein", "rein", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ART", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So wei\u00df ich, da\u00df ein magrer Bissen", "tokens": ["So", "wei\u00df", "ich", ",", "da\u00df", "ein", "mag\u00b7rer", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch besser als Melonen schmeckt.", "tokens": ["Noch", "bes\u00b7ser", "als", "Me\u00b7lo\u00b7nen", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kann ich dabey beqvemlich wohnen", "tokens": ["Kann", "ich", "da\u00b7bey", "be\u00b7qvem\u00b7lich", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PAV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In diesen auserw\u00e4hlten Zonen,", "tokens": ["In", "die\u00b7sen", "au\u00b7ser\u00b7w\u00e4hl\u00b7ten", "Zo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenigen verpflichtet seyn;", "tokens": ["Und", "we\u00b7ni\u00b7gen", "ver\u00b7pflich\u00b7tet", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So theil ich meiner Tage L\u00e4nge", "tokens": ["So", "theil", "ich", "mei\u00b7ner", "Ta\u00b7ge", "L\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In guter B\u00fccher kleine Menge,", "tokens": ["In", "gu\u00b7ter", "B\u00fc\u00b7cher", "klei\u00b7ne", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich aber bin und bleibe mein.", "tokens": ["Ich", "a\u00b7ber", "bin", "und", "blei\u00b7be", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "KON", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Darf ich nicht viele Nullen malen,", "tokens": ["Darf", "ich", "nicht", "vie\u00b7le", "Nul\u00b7len", "ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht kein tausendfaches Pralen", "tokens": ["Und", "macht", "kein", "tau\u00b7send\u00b7fa\u00b7ches", "Pra\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein hellerhaft Verm\u00f6gen aus,", "tokens": ["Mein", "hel\u00b7ler\u00b7haft", "Ver\u00b7m\u00f6\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird kein diebisches Gel\u00fcsten", "tokens": ["So", "wird", "kein", "die\u00b7bi\u00b7sches", "Ge\u00b7l\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sich wider mich mit Werkzeug r\u00fcsten,", "tokens": ["Sich", "wi\u00b7der", "mich", "mit", "Werk\u00b7zeug", "r\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kein wilder Brand fri\u00dft Hof und Haus.", "tokens": ["Kein", "wil\u00b7der", "Brand", "fri\u00dft", "Hof", "und", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Darf ich mich nicht vor vielen b\u00fccken,", "tokens": ["Darf", "ich", "mich", "nicht", "vor", "vie\u00b7len", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wei\u00df ich auch, da\u00df mir der R\u00fccken", "tokens": ["So", "wei\u00df", "ich", "auch", ",", "da\u00df", "mir", "der", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht oftmahls ungew\u00f6hnlich schmerzt.", "tokens": ["Nicht", "oft\u00b7mahls", "un\u00b7ge\u00b7w\u00f6hn\u00b7lich", "schmerzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird mir ein klein, klein Amt vertrauet,", "tokens": ["Wird", "mir", "ein", "klein", ",", "klein", "Amt", "ver\u00b7trau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJD", "$,", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O wohl mir! da mir niemahls grauet,", "tokens": ["O", "wohl", "mir", "!", "da", "mir", "nie\u00b7mahls", "grau\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PPER", "$.", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df mich ein Neider angeschw\u00e4rzt.", "tokens": ["Da\u00df", "mich", "ein", "Nei\u00b7der", "an\u00b7ge\u00b7schw\u00e4rzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein ganzes Kleid, ein treu Gem\u00fcthe,", "tokens": ["Ein", "gan\u00b7zes", "Kleid", ",", "ein", "treu", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ein M\u00e4cen voll Gnad und G\u00fcte,", "tokens": ["Und", "ein", "M\u00e4\u00b7cen", "voll", "Gnad", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Freund nach alter deutscher Art,", "tokens": ["Ein", "Freund", "nach", "al\u00b7ter", "deut\u00b7scher", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zwey Wangen, die sich niemahls f\u00e4rben,", "tokens": ["Zwey", "Wan\u00b7gen", ",", "die", "sich", "nie\u00b7mahls", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sind Dinge, welche zu erwerben", "tokens": ["Sind", "Din\u00b7ge", ",", "wel\u00b7che", "zu", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "PRELS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich niemahls Flei\u00df noch M\u00fch gespart.", "tokens": ["Ich", "nie\u00b7mahls", "Flei\u00df", "noch", "M\u00fch", "ge\u00b7spart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Hiern\u00e4chst ist meine Lust gewesen,", "tokens": ["Hier\u00b7n\u00e4chst", "ist", "mei\u00b7ne", "Lust", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein wohlgeschrieben Buch zu lesen,", "tokens": ["Ein", "wohl\u00b7ge\u00b7schrie\u00b7ben", "Buch", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Willen und Verstande n\u00fctzt;", "tokens": ["Das", "Wil\u00b7len", "und", "Ver\u00b7stan\u00b7de", "n\u00fctzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es kann auch auf der ganzen Erden", "tokens": ["Es", "kann", "auch", "auf", "der", "gan\u00b7zen", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts reizenders gefunden werden,", "tokens": ["Nichts", "rei\u00b7zen\u00b7ders", "ge\u00b7fun\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als wer so bey den Todten sitzt.", "tokens": ["Als", "wer", "so", "bey", "den", "Tod\u00b7ten", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "So bin ich nun, so will ich bleiben,", "tokens": ["So", "bin", "ich", "nun", ",", "so", "will", "ich", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und meine Tage so vertreiben,", "tokens": ["Und", "mei\u00b7ne", "Ta\u00b7ge", "so", "ver\u00b7trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis zu der Feyerabendzeit;", "tokens": ["Bis", "zu", "der", "Fe\u00b7ye\u00b7ra\u00b7bend\u00b7zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hab ich nichts mehr, und ich soll sterben,", "tokens": ["Hab", "ich", "nichts", "mehr", ",", "und", "ich", "soll", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIS", "ADV", "$,", "KON", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O gut! ich hinterla\u00df euch Erben", "tokens": ["O", "gut", "!", "ich", "hin\u00b7ter\u00b7la\u00df", "euch", "Er\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "$.", "PPER", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den reichen Schatz: Zufriedenheit.", "tokens": ["Den", "rei\u00b7chen", "Schatz", ":", "Zu\u00b7frie\u00b7den\u00b7heit", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}